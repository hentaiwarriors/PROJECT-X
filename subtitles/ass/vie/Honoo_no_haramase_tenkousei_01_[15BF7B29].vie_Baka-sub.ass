Dialogue: 0,0:09:04.55,0:09:09.57,den,,0000,0000,0320,,{\fnUVN Nguyen Du\fs40\c&H09140E&\pos(323,254)\frx10\fry358}Kho Thể Dục
Dialogue: 0,0:09:05.72,0:09:07.72,Default,,0000,0000,0000,,Sướng không hả, Tatsuya?
Dialogue: 0,0:09:07.97,0:09:09.51,Default,,0000,0000,0000,,Đây chính là món quà của mình dành cho cậu.
Dialogue: 0,0:09:11.05,0:09:14.21,Default,,0000,0000,0000,,Thằng nhỏ của cậu bự quá.
Dialogue: 0,0:09:16.33,0:09:19.32,Default,,0000,0000,0000,,Ngực cậu thật mềm mại.
Dialogue: 0,0:09:20.23,0:09:21.66,Default,,0000,0000,0000,,Tuyệt quá đi mất.
Dialogue: 0,0:09:22.68,0:09:26.23,Default,,0000,0000,0000,,Thằng nhỏ của cậu đang co giật liên hồi kìa.
Dialogue: 0,0:09:27.63,0:09:29.93,Default,,0000,0000,0000,,Hình như nó sắp bắn ra rồi.
Dialogue: 0,0:09:34.56,0:09:36.41,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:09:38.04,0:09:40.67,Default,,0000,0000,0000,,Chỉ mình mình được vui vẻ thế này thì chán chết.
Dialogue: 0,0:09:45.33,0:09:48.92,Default,,0000,0000,0000,,Hừ! Đừng véo ngực mình nữa!
Dialogue: 0,0:09:50.46,0:09:53.82,Default,,0000,0000,0000,,Dừng lại... Nó phụt ra mất.
Dialogue: 0,0:09:54.49,0:09:56.81,Default,,0000,0000,0000,,Mình biết. Cậu dễ thương quá.
Dialogue: 0,0:09:59.35,0:10:01.20,Default,,0000,0000,0000,,Cảm giác tệ quá...
Dialogue: 0,0:10:14.38,0:10:16.56,Default,,0000,0000,0000,,T-Tatsuya-san...
Dialogue: 0,0:10:16.83,0:10:19.25,Default,,0000,0000,0000,,Cậu muốn tấn công xuống dưới đó sao?
Dialogue: 0,0:10:24.47,0:10:26.35,Default,,0000,0000,0000,,Mình sắp ra rồi.
Dialogue: 0,0:10:51.13,0:10:52.87,Default,,0000,0000,0000,,Ruri, đi nào.
Dialogue: 0,0:10:52.97,0:10:55.66,Default,,0000,0000,0000,,Tới giờ rồi sao?
Dialogue: 0,0:10:59.37,0:11:01.78,Default,,0000,0000,0000,,Bệ nhảy ngựa nằm sâu trong kho thể dục.
Dialogue: 0,0:11:01.86,0:11:04.16,Default,,0000,0000,0000,,Chắc chắn nó sẽ nằm ở đó.
Dialogue: 0,0:11:03.89,0:11:08.42,den,,0000,0000,0320,,{\p1\c&H436859&\pos(535,312)\fscx1075\fscy538.75}m 15 -11 l -20 -11 l -20 3 l 16 3
Dialogue: 0,0:11:03.89,0:11:08.42,den,,0000,0000,0320,,{\fnUVN Nguyen Du\fs40\c&H09140E&\pos(323,254)\frx10\fry358}Kho Thể Dục
Dialogue: 0,0:11:09.47,0:11:11.59,Default,,0000,0000,0000,,Nghe thấy gì không?
Dialogue: 0,0:11:13.85,0:11:15.83,Default,,0000,0000,0000,,Không, đợi đã...
Dialogue: 0,0:11:18.75,0:11:23.09,Default,,0000,0000,0000,,Mình không ngờ chúng ta lại đi xa đến vậy.
Dialogue: 0,0:11:26.85,0:11:29.62,Default,,0000,0000,0000,,Tatsuya, đừng mà.
Dialogue: 0,0:11:37.88,0:11:40.47,Default,,0000,0000,0000,,Hừ! Mình đã bảo cậu dừng lại mà!
Dialogue: 0,0:11:40.55,0:11:43.20,Default,,0000,0000,0000,,Sau khi chiêm ngưỡng cơ thể cậu, mình đã không kiềm chế nổi.
Dialogue: 0,0:11:49.48,0:11:51.42,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:12:05.37,0:12:07.19,Default,,0000,0000,0000,,Đau lắm hả?
Dialogue: 0,0:12:07.41,0:12:09.33,Default,,0000,0000,0000,,Cậu có thể làm mạnh chút nữa.
Dialogue: 0,0:12:09.43,0:12:12.98,Default,,0000,0000,0000,,Mình muốn cậu được sung sướng.
Dialogue: 0,0:12:25.75,0:12:27.14,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:12:27.73,0:12:29.85,Default,,0000,0000,0000,,Bên trong mình tuyệt quá đi mất.
Dialogue: 0,0:12:30.77,0:12:32.19,Default,,0000,0000,0000,,Tatsuya!
Dialogue: 0,0:12:32.44,0:12:34.92,Default,,0000,0000,0000,,Tiếng rên của cậu thật dâm đãng.
Dialogue: 0,0:12:34.94,0:12:35.62,Default,,0000,0000,0000,,Cậu không cảm thấy vậy sao?
Dialogue: 0,0:12:37.56,0:12:39.31,Default,,0000,0000,0000,,Nhưng mà... Nhưng mà...
Dialogue: 0,0:12:39.64,0:12:44.23,Default,,0000,0000,0000,,Nhưng thằng nhỏ của cậu lớn quá!
Dialogue: 0,0:12:47.49,0:12:49.29,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:12:54.58,0:12:57.07,Default,,0000,0000,0000,,Bên trong mình tuyệt quá đi mất!
Dialogue: 0,0:12:59.04,0:13:01.94,Default,,0000,0000,0000,,Tất cả dường như bị khuấy tung lên...
Dialogue: 0,0:13:04.55,0:13:06.52,Default,,0000,0000,0000,,Đừng mà... Sướng quá đi mất...
Dialogue: 0,0:13:06.55,0:13:09.12,Default,,0000,0000,0000,,Mình sắp ra rồi.
Dialogue: 0,0:13:10.71,0:13:13.52,Default,,0000,0000,0000,,Mình sắp ra rồi...
Dialogue: 0,0:13:14.25,0:13:16.10,Default,,0000,0000,0000,,Mình sẽ bắn vào bên trong cậu.
Dialogue: 0,0:13:16.25,0:13:18.80,Default,,0000,0000,0000,,Không! Mình sẽ mang bầu mất!
Dialogue: 0,0:13:18.86,0:13:20.96,Default,,0000,0000,0000,,Mình ra đây, Wakana!
Dialogue: 0,0:13:21.59,0:13:22.88,Default,,0000,0000,0000,,Đừng mà...
Dialogue: 0,0:13:27.84,0:13:31.12,Default,,0000,0000,0000,,Tinh dịch của cậu đang tràn vào bên trong mình...
Dialogue: 0,0:13:32.09,0:13:34.34,Default,,0000,0000,0000,,Mình ra nữa rồi...
Dialogue: 0,0:13:47.48,0:13:48.81,Default,,0000,0000,0000,,Bên trong...
Dialogue: 0,0:13:49.76,0:13:51.30,Default,,0000,0000,0000,,Bên trong mình...
Dialogue: 0,0:13:51.41,0:13:55.19,Default,,0000,0000,0000,,Bên trong mình đang ngập tràn tinh dịch.
Dialogue: 0,0:14:11.81,0:14:15.32,Default,,0000,0000,0000,,Hai người làm trò gì ở đây vậy!?
Dialogue: 0,0:14:16.18,0:14:17.99,Default,,0000,0000,0000,,Nặng như heo ấy.
Dialogue: 0,0:14:19.93,0:14:21.73,Default,,0000,0000,0000,,Chuyện quái gì vậy!
Dialogue: 0,0:14:21.78,0:14:23.77,Default,,0000,0000,0000,,Wakana, đừng lo.
Dialogue: 0,0:14:23.91,0:14:24.83,Default,,0000,0000,0000,,Họ là bạn cậu mà, phải không?
Dialogue: 0,0:14:24.91,0:14:27.53,Default,,0000,0000,0000,,Nhưng còn Tatsuya...
Dialogue: 0,0:14:27.58,0:14:29.07,Default,,0000,0000,0000,,Không việc gì phải khóc hết.
Dialogue: 0,0:14:29.19,0:14:33.26,Default,,0000,0000,0000,,Hình như họ còn đứng xem rất thích thú nữa kìa.
Dialogue: 0,0:14:34.92,0:14:36.87,Default,,0000,0000,0000,,Không, Tatsuya...
Dialogue: 0,0:14:36.96,0:14:38.84,Default,,0000,0000,0000,,Chẳng có gì đáng xấu hổ hết.
Dialogue: 0,0:14:39.82,0:14:41.82,Default,,0000,0000,0000,,Dù cậu nói vậy...
Dialogue: 0,0:14:45.14,0:14:46.49,Default,,0000,0000,0000,,Wakana...
Dialogue: 0,0:14:48.39,0:14:50.34,Default,,0000,0000,0000,,M-Mình cũng muốn làm điều đó.
Dialogue: 0,0:14:50.48,0:14:53.34,Default,,0000,0000,0000,,Tatsuya-kun, mình muốn cậu làm chuyện đó với mình.
Dialogue: 0,0:14:55.17,0:14:59.72,Default,,0000,0000,0000,,Cả mình nữa, mình cũng muốn.
Dialogue: 0,0:15:00.81,0:15:02.29,Default,,0000,0000,0000,,Hai cậu này!
Dialogue: 0,0:15:02.42,0:15:03.90,Default,,0000,0000,0000,,Thôi nào.
Dialogue: 0,0:15:06.32,0:15:07.93,Default,,0000,0000,0000,,Cả hai cậu...
Dialogue: 0,0:15:08.54,0:15:09.78,Default,,0000,0000,0000,,Tới đây nào.
Dialogue: 0,0:15:32.38,0:15:35.94,Default,,0000,0000,0000,,Nước đang trào ra nè.
Dialogue: 0,0:15:32.38,0:15:34.04,Default,,0000,0000,0000,,Nhìn xem này
Dialogue: 0,0:15:34.06,0:15:41.18,Default,,0000,0000,0000,,Xấu hổ chết đi được ấy...
Dialogue: 0,0:15:55.99,0:15:57.38,Default,,0000,0000,0000,,Không, chỗ đó...
Dialogue: 0,0:15:57.39,0:15:59.78,Default,,0000,0000,0000,,Dừng lại đi mà...
Dialogue: 0,0:16:03.02,0:16:05.10,Default,,0000,0000,0000,,Có vẻ như cậu không chịu nổi nữa rồi.
Dialogue: 0,0:16:16.66,0:16:20.20,Default,,0000,0000,0000,,Sao hả, thấy sướng không?
Dialogue: 0,0:16:22.72,0:16:25.85,Default,,0000,0000,0000,,Phải! Thằng nhỏ của cậu tuyệt quá!
Dialogue: 0,0:16:25.99,0:16:28.81,Default,,0000,0000,0000,,Đừng dừng lại mà! Không!
Dialogue: 0,0:16:44.50,0:16:46.62,Default,,0000,0000,0000,,Cảm giác tệ quá!
Dialogue: 0,0:16:46.75,0:16:49.79,Default,,0000,0000,0000,,Được rồi, mình sẽ ra bên trong.
Dialogue: 0,0:16:49.90,0:16:53.15,Default,,0000,0000,0000,,Làm ơn hãy lấp đầy bên trong mình.
Dialogue: 0,0:16:55.43,0:16:56.70,Default,,0000,0000,0000,,Mình ra đây!
Dialogue: 0,0:17:06.25,0:17:09.85,Default,,0000,0000,0000,,Nhiều quá... Nó đang tràn ngập bên trong.
Dialogue: 0,0:17:14.18,0:17:16.40,Default,,0000,0000,0000,,Ruri, cậu tè rồi ư!
Dialogue: 0,0:17:16.73,0:17:19.33,Default,,0000,0000,0000,,Mình xin lỗi...
Dialogue: 0,0:17:20.83,0:17:22.96,Default,,0000,0000,0000,,Mình biết làm sao bây giờ?
Dialogue: 0,0:17:23.30,0:17:25.18,Default,,0000,0000,0000,,Đừng buồn.
Dialogue: 0,0:17:25.49,0:17:27.40,Default,,0000,0000,0000,,Buồn?
Dialogue: 0,0:17:28.18,0:17:29.17,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:17:29.32,0:17:33.04,Default,,0000,0000,0000,,Dù mình đang phê nên không giúp được gì nhiều.
Dialogue: 0,0:17:46.15,0:17:48.46,Default,,0000,0000,0000,,Chết này!
Dialogue: 0,0:17:51.39,0:17:53.14,Default,,0000,0000,0000,,Bóp bóp.
Dialogue: 0,0:17:56.38,0:17:58.39,Default,,0000,0000,0000,,Phụ mình một tay nào.
Dialogue: 0,0:17:58.48,0:17:59.19,Default,,0000,0000,0000,,Eh!!
Dialogue: 0,0:18:10.21,0:18:11.76,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:18:12.09,0:18:16.33,Default,,0000,0000,0000,,Đừng bú cùng lúc như vậy! Không!
Dialogue: 0,0:18:16.68,0:18:19.68,Default,,0000,0000,0000,,Vậy thế này là sao?
Dialogue: 0,0:18:21.07,0:18:23.90,Default,,0000,0000,0000,,Ruri, nhìn nè.
Dialogue: 0,0:18:36.29,0:18:38.08,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:18:39.13,0:18:40.59,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:18:55.08,0:18:58.93,Default,,0000,0000,0000,,Cậu chơi mạnh quá!
Dialogue: 0,0:19:06.22,0:19:08.39,Default,,0000,0000,0000,,Sướng quá đi mất!
Dialogue: 0,0:19:10.52,0:19:12.65,Default,,0000,0000,0000,,Mình ra đây!
Dialogue: 0,0:19:12.85,0:19:16.46,Default,,0000,0000,0000,,Mình sẽ làm cho tất cả mang bầu!
Dialogue: 0,0:19:44.26,0:19:46.99,Default,,0000,0000,0000,,Onii-chan, không sao chứ?
Dialogue: 0,0:19:47.62,0:19:51.38,Default,,0000,0000,0000,,Uhm, em sẽ có quà sớm thôi.
Dialogue: 0,0:19:51.60,0:19:52.93,Default,,0000,0000,0000,,Wakana.
Dialogue: 0,0:19:58.82,0:20:00.39,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:20:02.42,0:20:04.07,Default,,0000,0000,0000,,Ngực em tuyệt thật đó.
Dialogue: 0,0:20:04.12,0:20:05.07,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:20:06.95,0:20:12.12,Default,,0000,0000,0000,,Thằng nhỏ của anh đang lấp đầy bên trong em.
Dialogue: 0,0:20:22.79,0:20:26.60,Default,,0000,0000,0000,,Không! Không phải ở đó!
Dialogue: 0,0:20:31.77,0:20:35.51,Default,,0000,0000,0000,,Không! Em ra mất!
Dialogue: 0,0:20:41.05,0:20:44.72,Default,,0000,0000,0000,,Onii-chan, anh làm thô bạo quá...
Dialogue: 0,0:20:45.42,0:20:50.01,Default,,0000,0000,0000,,Em muốn tinh dich của anh.
Dialogue: 0,0:20:51.04,0:20:53.64,Default,,0000,0000,0000,,Vậy thì anh sẽ làm em cấn thai luôn!
Dialogue: 0,0:20:56.64,0:20:58.86,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:21:08.96,0:21:12.81,Default,,0000,0000,0000,,Thứ gì đó rất ấm đang tràn vào bên trong em...
Dialogue: 0,0:21:26.77,0:21:28.90,Default,,0000,0000,0000,,Này, Tatsuya.
Dialogue: 0,0:21:29.22,0:21:31.90,Default,,0000,0000,0000,,Cậu đang ấp ủ dự định gì vậy?
Dialogue: 0,0:21:31.99,0:21:35.37,Default,,0000,0000,0000,,Tatsuya muốn tạo nên một thế giới tươi đẹp hơn.
Dialogue: 0,0:21:35.58,0:21:38.16,Default,,0000,0000,0000,,Manaka! Yuko!
Dialogue: 0,0:21:39.01,0:21:41.99,Default,,0000,0000,0000,,Thế giới tươi đẹp là sao cơ?
Dialogue: 0,0:21:42.41,0:21:44.99,Default,,0000,0000,0000,,Cậu ta sẽ sớm cho cô biết thôi.
Dialogue: 0,0:21:45.16,0:21:48.89,Default,,0000,0000,0000,,Xem như đây là quà tặng của tôi...
Dialogue: 0,0:21:49.07,0:21:51.01,Default,,0000,0000,0000,,Crocus.
Dialogue: 0,0:22:03.11,0:22:07.82,Default,,0000,0000,0000,,Ma...Manaka, cô không định để tôi giải lao một chút sao?
Dialogue: 0,0:22:08.22,0:22:11.91,Default,,0000,0000,0000,,Chính anh đã làm hỏng phát minh mới nhất của tôi sáng nay.
Dialogue: 0,0:22:12.18,0:22:13.91,Default,,0000,0000,0000,,Đừng so đo thế chứ.
Dialogue: 0,0:22:14.06,0:22:15.57,Default,,0000,0000,0000,,Sáng nay ư?
Dialogue: 0,0:22:15.83,0:22:17.60,Default,,0000,0000,0000,,Đừng nói là...
Dialogue: 0,0:22:18.03,0:22:22.15,Default,,0000,0000,0000,,Wakana, cậu là vật hi sinh cho kế hoạch vĩ đại của mình.
Dialogue: 0,0:22:22.35,0:22:23.97,Default,,0000,0000,0000,,Kế hoạch?
Dialogue: 0,0:22:25.63,0:22:27.28,Default,,0000,0000,0000,,Crocus!
Dialogue: 0,0:22:43.53,0:22:45.56,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:23:12.05,0:23:13.53,Default,,0000,0000,0000,,Wakana.
Dialogue: 0,0:23:19.75,0:23:21.37,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:23:26.23,0:23:29.23,Default,,0000,0000,0000,,Đây là kế hoạch của cậu sao?
Dialogue: 0,0:23:37.11,0:23:39.11,Default,,0000,0000,0000,,Tới lượt tôi.
Dialogue: 0,0:23:39.29,0:23:40.94,Default,,0000,0000,0000,,Phải không nào, Tagami-sama?
Dialogue: 0,0:23:41.03,0:23:43.92,Default,,0000,0000,0000,,Hãy để tôi làm điều đó.
Dialogue: 0,0:23:49.32,0:23:50.80,Default,,0000,0000,0000,,Yuko!
Dialogue: 0,0:23:52.22,0:23:54.59,Default,,0000,0000,0000,,Tuyệt quá đi mất.
Dialogue: 0,0:23:54.74,0:23:56.38,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:23:56.47,0:23:57.81,Default,,0000,0000,0000,,Tagami-sama.
Dialogue: 0,0:23:57.93,0:24:01.58,Default,,0000,0000,0000,,Làm chuyện đó ngay lúc này.
Dialogue: 0,0:24:04.67,0:24:06.67,Default,,0000,0000,0000,,Không còn cách nào khác.
Dialogue: 0,0:24:11.24,0:24:12.49,Default,,0000,0000,0000,,Khít quá!
Dialogue: 0,0:24:12.62,0:24:13.94,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:24:13.94,0:24:17.28,Default,,0000,0000,0000,,Haruka, chăm sóc hậu môn của Makana nhé.
Dialogue: 0,0:24:22.97,0:24:25.80,Default,,0000,0000,0000,,Nó khít thật đấy.
Dialogue: 0,0:24:34.11,0:24:35.74,Default,,0000,0000,0000,,Tatsuya!
Dialogue: 0,0:24:38.84,0:24:40.33,Default,,0000,0000,0000,,Tagami-sama!
Dialogue: 0,0:24:40.76,0:24:42.66,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:24:42.73,0:24:45.27,Default,,0000,0000,0000,,Chốt sổ hôm nay tôi sẽ ra bên trong tôi.
Dialogue: 0,0:24:45.27,0:24:47.69,Default,,0000,0000,0000,,Tùy cậu thôi!
Dialogue: 0,0:24:49.10,0:24:50.87,Default,,0000,0000,0000,,Không, chưa được!
Dialogue: 0,0:24:51.67,0:24:53.94,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:24:57.54,0:24:59.89,Default,,0000,0000,0000,,Anh sẽ ra sớm thôi.
Dialogue: 0,0:25:00.10,0:25:03.81,Default,,0000,0000,0000,,Em muốn sinh con cho anh...
Dialogue: 0,0:25:04.31,0:25:07.36,Default,,0000,0000,0000,,Nhất trí thôi, Haruka!
Dialogue: 0,0:25:07.44,0:25:09.42,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:25:10.85,0:25:13.05,Default,,0000,0000,0000,,Tagami-sama!
Dialogue: 0,0:25:13.72,0:25:16.76,Default,,0000,0000,0000,,Hãy làm chúng ta... Làm chúng ta bên nhau mãi mãi.
Dialogue: 0,0:25:16.87,0:25:19.41,Default,,0000,0000,0000,,Uhm, được thôi.
Dialogue: 0,0:25:19.63,0:25:21.65,Default,,0000,0000,0000,,Hãy sinh con cho mình, Ruri!
Dialogue: 0,0:25:21.71,0:25:23.64,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:25:24.24,0:25:26.31,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:25:26.44,0:25:28.75,Default,,0000,0000,0000,,Yuko, tôi ra đây.
Dialogue: 0,0:25:28.84,0:25:31.84,Default,,0000,0000,0000,,Vâng, Tatsuya.
Dialogue: 0,0:25:31.91,0:25:34.12,Default,,0000,0000,0000,,Không, tôi ra rồi!
Dialogue: 0,0:25:34.73,0:25:36.95,Default,,0000,0000,0000,,Tôi cũng vậy!
Dialogue: 0,0:25:38.29,0:25:41.21,Default,,0000,0000,0000,,Cùng nhau xây dựng thế giới của cậu.
Dialogue: 0,0:25:41.32,0:25:43.38,Default,,0000,0000,0000,,Phải, cùng nhau!
Dialogue: 0,0:25:43.47,0:25:45.01,Default,,0000,0000,0000,,Cùng nhau.
Dialogue: 0,0:25:47.25,0:25:49.36,Default,,0000,0000,0000,,Sinh con cho mình nhé!
Dialogue: 0,0:25:49.36,0:25:50.97,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:25:51.87,0:25:54.33,Default,,0000,0000,0000,,Vậy ra đó là mục đích của cậu?
Dialogue: 0,0:25:54.82,0:25:57.25,Default,,0000,0000,0000,,Mình cũng muốn sinh con cho cậu...
Dialogue: 0,0:25:57.54,0:25:58.93,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:26:00.40,0:26:03.20,Default,,0000,0000,0000,,Mình ra đây, Wakana!
Dialogue: 0,0:26:04.01,0:26:05.24,Default,,0000,0000,0000,,Mình ra rồi.
Dialogue: 0,0:26:05.71,0:26:08.18,Default,,0000,0000,0000,,Tất cả các cậu, hãy sinh con cho mình!
Dialogue: 0,0:26:16.78,0:26:21.78,Default,,0000,0000,0000,,Nào, mọi người lắng nghe tôi phân công nhiệm vụ đây.
Dialogue: 0,0:26:22.07,0:26:24.96,Default,,0000,0000,0000,,Làm ơn hãy hợp tác cùng nhau.
Dialogue: 0,0:26:26.50,0:26:30.36,Default,,0000,0000,0000,,Ah, dù sao anh cũng khiến cả đám mê tơi.
Dialogue: 0,0:26:32.98,0:26:35.18,Default,,0000,0000,0000,,Chúng ta sẽ chinh phục thế giới!
Dialogue: 0,0:26:35.28,0:26:37.06,Default,,0000,0000,0000,,Chinh phục thế giới!
Dialogue: 0,0:26:37.18,0:26:39.28,Default,,0000,0000,0000,,Không có gì là không thể!
Dialogue: 0,0:26:39.48,0:26:42.43,Default,,0000,0000,0000,,Nhắc đi nhắc lại điều này, ước mơ của chúng ta sẽ sớm thành hiện thực!
Dialogue: 0,0:26:49.18,0:26:53.77,Default,,0000,0000,0000,,Minase, chuyện này sẽ mất khá nhiều thời gian đấy.
Dialogue: 0,0:26:58.40,0:26:59.75,Default,,0000,0000,0000,,Minase-san.
Dialogue: 0,0:27:00.06,0:27:01.12,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:27:01.38,0:27:07.07,Default,,0000,0000,0000,,Về buổi lễ tốt nghiệp do em đề xuất trước đó...
Dialogue: 0,0:27:07.34,0:27:09.77,Default,,0000,0000,0000,,Nhà trường đã quyết định tạm hoãn tới lễ hội văn hóa.
Dialogue: 0,0:27:10.32,0:27:12.07,Default,,0000,0000,0000,,Thật ạ?
Dialogue: 0,0:27:12.14,0:27:13.19,Default,,0000,0000,0000,,Uh.
Dialogue: 0,0:27:13.35,0:27:16.58,Default,,0000,0000,0000,,Chúng ta có những thành viên rất năng nổ tham gia hoạt động này.
Dialogue: 0,0:27:16.73,0:27:19.42,Default,,0000,0000,0000,,Cô rất mong chờ bản vẽ mới của em đó.
Dialogue: 0,0:27:19.56,0:27:21.79,Default,,0000,0000,0000,,Vâng, Sensei!
Dialogue: 0,0:27:22.74,0:27:25.13,Default,,0000,0000,0000,,Hẹn gặp lại em ngày mai.
Dialogue: 0,0:27:26.43,0:27:28.54,Default,,0000,0000,0000,,Sensei,tạm biệt!
Dialogue: 0,0:27:30.55,0:27:33.47,Default,,0000,0000,0000,,Mình muốn vẽ hình Tacchan!
Dialogue: 0,0:27:42.00,0:27:47.71,dacbiet,,0000,0000,0180,,{\fn UVN Nguyen Du\bord0\shad0\fs25\c&HD6D6D6&}Phần 1   Đón nhận nòi giống của tôi!!!
