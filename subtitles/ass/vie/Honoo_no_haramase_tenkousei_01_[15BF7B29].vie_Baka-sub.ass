Dialogue: 0,0:04:50.62,0:04:54.59,Default,,0000,0000,0000,,Ngôi trường này sẽ trở thành bàn đạp giúp mình thực hiện kế hoạch đó.
Dialogue: 0,0:04:55.17,0:04:57.61,Default,,0000,0000,0000,,Nơi này sắp biến thành trường nữ sinh,
Dialogue: 0,0:04:57.68,0:04:59.35,Default,,0000,0000,0000,,vậy nên chẳng có mấy mống con trai.
Dialogue: 0,0:04:59.38,0:05:01.95,Default,,0000,0000,0000,,Và tôi chính là thằng con trai duy nhất trong lớp.
Dialogue: 0,0:05:02.69,0:05:06.06,Default,,0000,0000,0000,,Hơn nữa, bạn cùng lớp lại toàn các em xinh tươi.
Dialogue: 0,0:05:06.57,0:05:10.18,Default,,0000,0000,0000,,Đặc biệt là Minase Sayaka, cô bạn thơ ấu.
Dialogue: 0,0:05:10.34,0:05:11.57,Default,,0000,0000,0000,,Cô ấy thật trẻ trung và xinh xắn.
Dialogue: 0,0:05:11.64,0:05:14.17,Default,,0000,0000,0000,,Tạ ơn Trời đã cho mình học cùng lớp với cô ấy
Dialogue: 0,0:05:15.62,0:05:18.16,Default,,0000,0000,0000,,Cô nàng Kanae cũng rất nổi tiếng.
Dialogue: 0,0:05:18.32,0:05:20.28,Default,,0000,0000,0000,,Cô nàng này bạo lực còn hơn cả đàn ông.
Dialogue: 0,0:05:20.32,0:05:22.98,Default,,0000,0000,0000,,Mà thôi, mặc kệ cô ta đi.
Dialogue: 0,0:05:23.25,0:05:28.06,Default,,0000,0000,0000,,Bước đầu tiên trong kế hoạch là phải làm cho cả lớp dính bầu.
Dialogue: 0,0:05:28.62,0:05:31.92,Default,,0000,0000,0000,,Cứ từ từ tiến hành.
Dialogue: 0,0:05:37.18,0:05:39.63,Default,,0000,0000,0000,,Tacchan! Đi ăn trưa nào!
Dialogue: 0,0:05:39.79,0:05:40.21,Default,,0000,0000,0000,,Uh.
Dialogue: 0,0:05:40.29,0:05:42.31,Default,,0000,0000,0000,,Mình đi cùng nhé.
Dialogue: 0,0:05:42.93,0:05:44.20,Default,,0000,0000,0000,,Ờ.
Dialogue: 0,0:05:46.65,0:05:48.95,Default,,0000,0000,0000,,Cậu lại mua bữa trưa làm sẵn nữa hả?
Dialogue: 0,0:05:49.19,0:05:53.67,Default,,0000,0000,0000,,Mẹ luôn dặn phải tự chuẩn bị bữa trưa nhưng mình không thích việc đó lắm.
Dialogue: 0,0:05:59.36,0:06:01.74,Default,,0000,0000,0000,,Tuyệt ghê!
Dialogue: 0,0:06:02.32,0:06:04.71,Default,,0000,0000,0000,,Cậu tự làm à?
Dialogue: 0,0:06:05.15,0:06:07.56,Default,,0000,0000,0000,,Mình đoán cậu sẽ nói thế mà.
Dialogue: 0,0:06:09.28,0:06:10.58,Default,,0000,0000,0000,,Còn cậu thì sao, Minase?
Dialogue: 0,0:06:10.64,0:06:12.58,Default,,0000,0000,0000,,Uhm, mình tự làm.
Dialogue: 0,0:06:13.27,0:06:15.65,Default,,0000,0000,0000,,Trông ngon quá.
Dialogue: 0,0:06:15.92,0:06:18.08,Default,,0000,0000,0000,,Cậu tài ghê ha.
Dialogue: 0,0:06:19.40,0:06:21.00,Default,,0000,0000,0000,,Tất nhiên rồi.
Dialogue: 0,0:06:21.01,0:06:22.36,Default,,0000,0000,0000,,Đùa thôi mà.
Dialogue: 0,0:06:22.50,0:06:25.02,Default,,0000,0000,0000,,Cậu đâu nấu nướng siêu đến mức đó.
Dialogue: 0,0:06:25.13,0:06:26.80,Default,,0000,0000,0000,,Mình hơi bị siêu đấy.
Dialogue: 0,0:06:26.96,0:06:29.72,Default,,0000,0000,0000,,Hai người lúc nào cũng thế.
Dialogue: 0,0:06:29.97,0:06:33.18,Default,,0000,0000,0000,,Còn cả đống bài tập Tư tưởng đấy nhé.
Dialogue: 0,0:06:33.87,0:06:34.71,Default,,0000,0000,0000,,Rồi, rồi.
Dialogue: 0,0:06:34.83,0:06:38.84,Default,,0000,0000,0000,,Chúng mình sinh ra là để dành cho nhau mà.
Dialogue: 0,0:06:42.58,0:06:45.41,Default,,0000,0000,0000,,Mình quên mang đũa rồi!
Dialogue: 0,0:06:45.99,0:06:49.09,Default,,0000,0000,0000,,Vậy à... Dùng tạm...
Dialogue: 0,0:06:49.63,0:06:51.54,Default,,0000,0000,0000,,Mình cũng quên đũa luôn rồi!
Dialogue: 0,0:06:51.77,0:06:52.95,Default,,0000,0000,0000,,Hừ,
Dialogue: 0,0:06:53.08,0:06:56.74,Default,,0000,0000,0000,,Đến cả quên cũng giống nhau.
Dialogue: 0,0:06:57.12,0:06:58.74,Default,,0000,0000,0000,,Được rồi.
Dialogue: 0,0:06:59.88,0:07:02.07,Default,,0000,0000,0000,,Xài của mình đi.
Dialogue: 0,0:07:02.33,0:07:04.07,Default,,0000,0000,0000,,Cám ơn!
Dialogue: 0,0:07:05.28,0:07:06.07,Default,,0000,0000,0000,,Còn mình thì sao?
Dialogue: 0,0:07:06.08,0:07:07.17,Default,,0000,0000,0000,,Ăn thôi.
Dialogue: 0,0:07:08.42,0:07:09.18,Default,,0000,0000,0000,,Thế còn mình?
Dialogue: 0,0:07:09.22,0:07:10.78,Default,,0000,0000,0000,,Miễn đi.
Dialogue: 0,0:07:11.49,0:07:15.08,Default,,0000,0000,0000,,Cậu xài của mình nhé.
Dialogue: 0,0:07:18.85,0:07:20.72,Default,,0000,0000,0000,,Cậu chắc chứ?
Dialogue: 0,0:07:22.31,0:07:23.64,Default,,0000,0000,0000,,Oh...
Dialogue: 0,0:07:24.38,0:07:26.21,Default,,0000,0000,0000,,Đừng ngại.
Dialogue: 0,0:07:28.04,0:07:29.26,Default,,0000,0000,0000,,Thôi được.
Dialogue: 0,0:07:30.29,0:07:31.82,Default,,0000,0000,0000,,Tạm biệt...
Dialogue: 0,0:07:33.43,0:07:35.35,Default,,0000,0000,0000,,Cám ơn, Wakana!
Dialogue: 0,0:07:37.46,0:07:39.26,Default,,0000,0000,0000,,Được rồi, chén thôi...
Dialogue: 0,0:07:39.27,0:07:39.96,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:07:40.41,0:07:41.86,Default,,0000,0000,0000,,Mình muốn cám ơn cậu.
Dialogue: 0,0:07:41.97,0:07:44.55,Default,,0000,0000,0000,,Mình sẽ đợi cậu trong kho thể dục vào lúc chiều muộn.
Dialogue: 0,0:07:44.80,0:07:45.78,Default,,0000,0000,0000,,-Wakana.
Dialogue: 0,0:07:46.67,0:07:47.99,Default,,0000,0000,0000,,Gì vậy?
Dialogue: 0,0:07:48.10,0:07:49.90,Default,,0000,0000,0000,,Không có gì.
Dialogue: 0,0:07:49.97,0:07:52.13,Default,,0000,0000,0000,,Ăn thôi nào.
Dialogue: 0,0:07:53.67,0:07:55.70,Default,,0000,0000,0000,,Mời mọi người!
Dialogue: 0,0:07:55.97,0:07:58.60,Default,,0000,0000,0000,,Đũa mình gãy mất rồi!
Dialogue: 0,0:07:58.75,0:07:59.96,Default,,0000,0000,0000,,Tuyệt ghê!
Dialogue: 0,0:07:59.96,0:08:01.70,Default,,0000,0000,0000,,Tacchan!
Dialogue: 0,0:08:01.78,0:08:03.68,Default,,0000,0000,0000,,Tatsuya.
Dialogue: 0,0:08:04.68,0:08:06.89,Default,,0000,0000,0000,,Lại nữa à.
Dialogue: 0,0:08:07.11,0:08:09.05,Default,,0000,0000,0000,,Bởi vì cậu là tên đầu đất.
Dialogue: 0,0:08:09.10,0:08:10.52,Default,,0000,0000,0000,,Thôi nào, hai người.
Dialogue: 0,0:08:10.55,0:08:12.37,Default,,0000,0000,0000,,Đừng hòng chạy thoát.
Dialogue: 0,0:08:22.09,0:08:24.50,Default,,0000,0000,0000,,Đó là ý muốn của Thượng đế.
Dialogue: 0,0:08:25.73,0:08:28.33,Default,,0000,0000,0000,,Mọi người sẽ trông thấy con đường sáng lạn phía trước.
Dialogue: 0,0:08:29.20,0:08:33.42,Default,,0000,0000,0000,,Một thế giới tươi đẹp đang rộng mở.
Dialogue: 0,0:08:34.00,0:08:37.43,Default,,0000,0000,0000,,Giờ lành là 4:44.
Dialogue: 0,0:08:38.52,0:08:41.98,Default,,0000,0000,0000,,Vậy là chúng là phải tới phòng nhảy ngựa vào lúc 4:44?
Dialogue: 0,0:08:42.18,0:08:43.58,Default,,0000,0000,0000,,Phải đó.
Dialogue: 0,0:08:46.03,0:08:47.55,Default,,0000,0000,0000,,Cùng với Onii-chan...
Dialogue: 0,0:08:49.31,0:08:51.36,Default,,0000,0000,0000,,Thôi nào, ta tới đó kiểm chứng đi..
Dialogue: 0,0:08:51.50,0:08:52.57,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:08:52.72,0:08:54.04,Default,,0000,0000,0000,,Cám ơn, Yuko-chan.
Dialogue: 0,0:08:54.08,0:08:55.42,Default,,0000,0000,0000,,Đi nào.
Dialogue: 0,0:08:55.75,0:08:57.38,Default,,0000,0000,0000,,Vâng.
