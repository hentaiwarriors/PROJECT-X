Dialogue: 0,0:00:13.88,0:00:14.84,Default,,0000,0000,0000,,Đừng mà...
Dialogue: 0,0:00:14.94,0:00:17.40,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:00:18.15,0:00:19.55,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:00:26.79,0:00:29.00,Default,,0000,0000,0000,,Làm ơn dừng lại đi!
Dialogue: 0,0:00:30.34,0:00:31.82,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:00:43.83,0:00:45.78,Default,,0000,0000,0000,,Không... Không...
Dialogue: 0,0:00:46.93,0:00:49.54,Default,,0000,0000,0000,,Xấu hổ chết đi được...
Dialogue: 0,0:00:50.02,0:00:52.23,Default,,0000,0000,0000,,Buông tôi ra...
Dialogue: 0,0:00:53.83,0:00:55.97,Default,,0000,0000,0000,,Không, dừng lại...
Dialogue: 0,0:00:58.30,0:01:00.46,Default,,0000,0000,0000,,Tha cho tôi!
Dialogue: 0,0:01:01.53,0:01:04.25,Default,,0000,0000,0000,,Đừng mà, xin anh đó!
Dialogue: 0,0:01:05.16,0:01:08.41,Default,,0000,0000,0000,,Ai đó làm ơn cứu tôi với!
Dialogue: 0,0:01:10.54,0:01:12.14,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:01:14.56,0:01:17.66,Default,,0000,0000,0000,,Mi đang làm trò quỷ gì vậy, tên quấy rối khốn nạn kia!?
Dialogue: 0,0:01:18.75,0:01:20.58,Default,,0000,0000,0000,,Khốn kiếp.
Dialogue: 0,0:01:25.48,0:01:27.74,Default,,0000,0000,0000,,Vô cùng cám ơn cậu.
Dialogue: 0,0:01:28.02,0:01:29.66,Default,,0000,0000,0000,,Không vấn đề gì.
Dialogue: 0,0:01:29.74,0:01:30.26,Default,,0000,0000,0000,,Tạm biệt.
Dialogue: 0,0:01:45.29,0:01:46.78,Default,,0000,0000,0000,,Tatsuya.
Dialogue: 0,0:01:47.95,0:01:50.66,Default,,0000,0000,0000,,Cậu là người vừa ra tay hả?
Dialogue: 0,0:01:50.75,0:01:51.76,Default,,0000,0000,0000,,Cậu giỏi ghê, Tacchan!
Dialogue: 0,0:01:53.59,0:01:55.69,Default,,0000,0000,0000,,Cậu đúng là một chàng trai đáng tin cậy!
Dialogue: 0,0:01:55.70,0:01:58.19,Default,,0000,0000,0000,,Đừng nói vậy!
Dialogue: 0,0:01:59.28,0:02:00.93,Default,,0000,0000,0000,,Cám ơn cậu.
Dialogue: 0,0:02:01.08,0:02:04.71,Default,,0000,0000,0000,,May mà cậu ra tay kịp lúc.
Dialogue: 0,0:02:05.63,0:02:07.28,Default,,0000,0000,0000,,Cũng chẳng có gì to tát đâu.
Dialogue: 0,0:02:08.14,0:02:09.92,Default,,0000,0000,0000,,Thật nam tính làm sao!
Dialogue: 0,0:02:10.99,0:02:15.02,Default,,0000,0000,0000,,Đó mới là lời nói của người đàn ông đích thực,
Dialogue: 0,0:02:16.52,0:02:18.83,Default,,0000,0000,0000,,Mình kết cậu rồi đó...
Dialogue: 0,0:02:19.04,0:02:20.89,Default,,0000,0000,0000,,À thì...
Dialogue: 0,0:02:21.17,0:02:23.00,Default,,0000,0000,0000,,Cả mình nữa!
Dialogue: 0,0:02:27.01,0:02:30.85,Default,,0000,0000,0000,,Không ngờ cậu lại phát hiện ra cô ấy bị quấy rối, Tatsuya.
Dialogue: 0,0:02:31.61,0:02:34.93,Default,,0000,0000,0000,,Toàn các em trẻ và xinh tươi, nhể?
Dialogue: 0,0:02:35.70,0:02:37.47,Default,,0000,0000,0000,,Dĩ nhiên rồi!
Dialogue: 0,0:02:37.65,0:02:41.54,Default,,0000,0000,0000,,Một em trẻ măng với bộ mặt đỏ bừng vì bị quấy rối.
Dialogue: 0,0:02:42.68,0:02:47.23,Default,,0000,0000,0000,,Và cả bộ ngực bự hấp dẫn đó nữa.
Dialogue: 0,0:02:53.22,0:02:56.80,Default,,0000,0000,0000,,Hóa ra cậu đã trông thấy hết à?
Dialogue: 0,0:02:57.97,0:02:59.70,Default,,0000,0000,0000,,Đồ tồi!
Dialogue: 0,0:02:59.85,0:03:02.90,Default,,0000,0000,0000,,Ấy, cái đó...
Dialogue: 0,0:03:03.10,0:03:04.62,Default,,0000,0000,0000,,Tên dâm tặc.
Dialogue: 0,0:03:04.95,0:03:07.34,Default,,0000,0000,0000,,Không thanh minh thanh nga gì hết.
Dialogue: 0,0:03:08.99,0:03:10.62,Default,,0000,0000,0000,,Đỡ này!
Dialogue: 0,0:03:14.60,0:03:17.30,Default,,0000,0000,0000,,Thế nào hả, Tatsuya?
Dialogue: 0,0:03:17.37,0:03:20.22,Default,,0000,0000,0000,,Ahh, tôi đầu hàng.
Dialogue: 0,0:03:20.32,0:03:24.33,Default,,0000,0000,0000,,Tôi hỏi lần nữa: Thế nào hả!?
Dialogue: 0,0:03:26.49,0:03:28.04,Default,,0000,0000,0000,,Kanae...
Dialogue: 0,0:03:28.24,0:03:30.02,Default,,0000,0000,0000,,Làm ơn tha cho tôi.
Dialogue: 0,0:03:30.09,0:03:32.66,Default,,0000,0000,0000,,Nói lại lần nữa.
Dialogue: 0,0:03:38.98,0:03:42.72,Default,,0000,0000,0000,,Sao cậu phải quan trọng hóa vấn đề lên thế nhỉ.
Dialogue: 0,0:03:52.80,0:03:55.49,Default,,0000,0000,0000,,Có vẻ như cậu ta sẽ là người kế tiếp.
Dialogue: 0,0:04:00.29,0:04:02.70,Default,,0000,0000,0000,,Cậu ta rất có triển vọng đấy.
Dialogue: 0,0:04:02.96,0:04:06.18,Default,,0000,0000,0000,,Một sản phẩm tuyệt đẹp.
Dialogue: 0,0:04:09.18,0:04:10.75,Default,,0000,0000,0000,,Coi nào.
Dialogue: 0,0:04:10.91,0:04:13.39,Default,,0000,0000,0000,,Mau khai hết đi.
Dialogue: 0,0:04:25.69,0:04:32.16,Default,,0000,0000,0050,,{\3c&HBF9668&\fs50\c&HFCF9F3&}Honoo no Haramase Tenkousei
