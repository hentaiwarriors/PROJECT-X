Comment: 0,0:05:00.70,0:05:01.16,Default,,0000,0000,0000,,Hơ?

Dialogue: 0,0:00:18.18,0:00:20.02,Default,,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:00:24.79,0:00:25.18,Default,,0000,0000,0000,,Gì thế này?
Dialogue: 0,0:00:27.66,0:00:29.62,Default,,0000,0000,0000,,Thật đáng kinh ngạc.
Dialogue: 0,0:00:30.33,0:00:33.76,Default,,0000,0000,0000,,Cái của cậu ta phồng ra bên trong miệng tôi.
Dialogue: 0,0:00:33.83,0:00:38.03,Default,,0000,0000,0000,,Nó dài ra chạm đến tận cổ họng.
Dialogue: 0,0:00:38.06,0:00:40.80,Default,,0000,0000,0000,,Như một thực thể sống.
Dialogue: 0,0:00:42.34,0:00:46.17,Default,,0000,0000,0000,,Chỉ mới tưởng tượng cái thứ đó vào bên trong, mà tôi đã...
Dialogue: 0,0:00:47.51,0:00:48.50,Default,,0000,0000,0000,,Tôi...
Dialogue: 0,0:00:50.01,0:00:50.60,Default,,0000,0000,0000,,Matsuno!
Dialogue: 0,0:00:51.38,0:00:52.28,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:00:54.02,0:00:56.11,Default,,0000,0000,0000,,Bài kiểm tra vẫn chưa xong đâu.
Dialogue: 0,0:00:56.62,0:00:57.78,Default,,0000,0000,0000,,Tôi xin lỗi.
Dialogue: 0,0:01:05.03,0:01:07.53,Default,,0000,0000,0000,,Đầu ngực cô cứng rồi này.
Dialogue: 0,0:01:08.96,0:01:09.96,Default,,0000,0000,0000,,Nghe cho rõ đây, Matsuno.
Dialogue: 0,0:01:15.87,0:01:18.13,Default,,0000,0000,0000,,Cô không được phép đụng đến cậu ta.
Dialogue: 0,0:01:18.21,0:01:20.74,Default,,0000,0000,0000,,Cậu ta là để thỏa mãn em gái tôi, cô biết mà.
Dialogue: 0,0:01:21.64,0:01:23.08,Default,,0000,0000,0000,,Vâng, Reina-sama.
Dialogue: 0,0:01:23.55,0:01:26.67,Default,,0000,0000,0000,,Rõ rồi thì... tiếp tục... kiểm tra đi.
Dialogue: 0,0:01:37.19,0:01:41.19,Default,,0000,0000,0000,,Cậu đã qua kỳ kiểm tra. Tôi cho cậu điểm A.
Dialogue: 0,0:01:49.48,0:01:57.18,Default,,0000,0000,0000,,{\fad(500,0)\bord0\3c&HFFFFFF&\c&H000000&\4c&HFFFFFF&\pos(56,458)}baka-sub
