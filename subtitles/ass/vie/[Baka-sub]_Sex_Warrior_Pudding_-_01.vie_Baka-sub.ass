Dialogue: 0,0:03:17.99,0:03:19.15,Default,,0000,0000,0000,,Flash!!
Dialogue: 0,0:03:25.60,0:03:29.90,Default,,0000,0000,0000,,Vậy, hình dạng thật của Lividoll chỉ là những con búp bê đồ chơi.
Dialogue: 0,0:03:31.11,0:03:32.83,Default,,0000,0000,0000,,Hmm, tụi Sex Warrior...
Dialogue: 0,0:03:36.41,0:03:37.81,Default,,0000,0000,0000,,Ta sẽ ko bỏ qua chuyện này đâu.
Dialogue: 0,0:03:40.38,0:03:42.94,Default,,0000,0000,0000,,Uhm, chuyện đó quả thật rất khó khăn với cậu.
Dialogue: 0,0:03:44.39,0:03:46.65,Default,,0000,0000,0000,,Nhưng mình mừng vị cậu vẫn an toàn.
Dialogue: 0,0:03:46.99,0:03:53.26,Default,,0000,0000,0000,,Đúng vậy, nhưng những người đã cứu tớ, Hình như tớ đã gặp họ ở đâu đó thì phải...?
Dialogue: 0,0:03:53.39,0:03:55.79,Default,,0000,0000,0000,,Haha, cậu nghĩ ngợi nhiều quá rồi.
Dialogue: 0,0:03:56.23,0:03:59.43,Default,,0000,0000,0000,,Nhưng mình vô cùng ngạc nhiên khi cậu trở thành quản lí của Alamode đấy,
Dialogue: 0,0:03:59.43,0:04:02.93,Default,,0000,0000,0000,,Nhà hàng mà tình yêu của cậu, Pudding, làm việc. Huh...
Dialogue: 0,0:04:04.41,0:04:06.27,Default,,0000,0000,0000,,Uhm, tớ...
Dialogue: 0,0:04:07.34,0:04:09.64,Default,,0000,0000,0000,,Chuyện này thật khó giải thích.
Dialogue: 0,0:04:11.21,0:04:13.11,Default,,0000,0000,0000,,Nơi này là đâu vậy?
Dialogue: 0,0:04:13.95,0:04:14.97,Default,,0000,0000,0000,,Chào mừng cậu.
Dialogue: 0,0:04:15.25,0:04:18.74,Default,,0000,0000,0000,,Tới Alamode, Shuta...ko.
Dialogue: 0,0:04:19.29,0:04:20.15,Default,,0000,0000,0000,,Ngài quản lý.
Dialogue: 0,0:04:20.26,0:04:21.81,Default,,0000,0000,0000,,Hả? Quản lý á?
Dialogue: 0,0:04:24.33,0:04:26.82,Default,,0000,0000,0000,,Oái mẹ ơi!
Dialogue: 0,0:04:29.53,0:04:32.90,Default,,0000,0000,0000,,Đợi đã, chuyện này là sao!?
Dialogue: 0,0:04:33.17,0:04:36.14,Default,,0000,0000,0000,,Hãy coi đó là món quà ra mắt cũng như sự tôn trọng của chúng tôi dành cho cậu.
Dialogue: 0,0:04:36.40,0:04:37.81,Default,,0000,0000,0000,,Xin cậu thứ lỗi về sự giới thiệu chậm trễ này.
Dialogue: 0,0:04:37.81,0:04:41.90,Default,,0000,0000,0000,,Tôi là phó quản lý của Alamode,\NChiyoko.
Dialogue: 0,0:04:42.84,0:04:43.64,Default,,0000,0000,0000,,Đây...
Dialogue: 0,0:04:44.21,0:04:46.15,Default,,0000,0000,0000,,Cha cậu đã để lại vật này.
Dialogue: 0,0:04:46.88,0:04:47.78,Default,,0000,0000,0000,,Cha tôi ấy hả?
Dialogue: 0,0:04:48.85,0:04:49.98,Default,,0000,0000,0000,,Cục cưng của cha!!
Dialogue: 0,0:04:49.98,0:04:52.18,Default,,0000,0000,0000,,Con có còn hay làm mấy trò ngốc nghếch nữa ko?
Dialogue: 0,0:04:52.82,0:04:54.79,Default,,0000,0000,0000,,Ta đã cải tiến nơi này thành một cỗ máy tốc độ cao và dễ sử dụng.
Dialogue: 0,0:04:54.79,0:04:58.78,Default,,0000,0000,0000,,Nhà hàng này được thành lập khi ta đang tiến hành thử nghiệm phát minh mới của mình.
Dialogue: 0,0:04:58.89,0:05:02.50,Default,,0000,0000,0000,,Còn bây giờ khi ta đã ra đi, con sẽ thay ta điều hành nơi này!
Dialogue: 0,0:05:02.50,0:05:03.56,Default,,0000,0000,0000,,Cám ơn con trai cưng nhé!
Dialogue: 0,0:05:04.10,0:05:05.79,Default,,0000,0000,0000,,Cục cưng?
Dialogue: 0,0:05:06.27,0:05:09.76,Default,,0000,0000,0000,,Nếu con muốn 411, hãy hỏi Chiyoko.\NTa sẽ luôn theo dõi công việc của con đấy!
Dialogue: 0,0:05:10.07,0:05:12.30,Default,,0000,0000,0000,,Cha ơi, đợi đã! Đợi đã nào!
