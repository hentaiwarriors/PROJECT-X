Dialogue: 0,0:20:00.79,0:20:02.92,Default,,0000,0000,0000,,Nếu chúng ta ko kết hợp cùng với nhau,
Dialogue: 0,0:20:03.50,0:20:07.40,Default,,0000,0000,0000,,sẽ ko có cách nào có thể đánh bại được con quái vật đó.
Dialogue: 0,0:20:08.20,0:20:12.26,Default,,0000,0000,0000,,Tôi muốn cô hãy chấp nhận tôi và cùng tôi tới giải cứu bọn họ.
Dialogue: 0,0:20:12.97,0:20:18.41,Default,,0000,0000,0000,,Tôi ko thể...tôi ko thể tin tưởng cậu được...
Dialogue: 0,0:20:18.91,0:20:22.01,Default,,0000,0000,0000,,Nếu làm như trong trò chơi....
Dialogue: 0,0:20:24.25,0:20:26.15,Default,,0000,0000,0000,,Được rồi, mình sẽ làm giống như vậy.
Dialogue: 0,0:20:32.43,0:20:35.26,Default,,0000,0000,0000,,Mình ko thể, Mình ko thể làm điều đó...
Dialogue: 0,0:20:36.00,0:20:39.66,Default,,0000,0000,0000,,Anh ấy đã bị thương khi cô cứu mình.
Dialogue: 0,0:20:40.97,0:20:43.96,Default,,0000,0000,0000,,Pudding, tôi sẽ thành thực với em.
Dialogue: 0,0:20:45.21,0:20:48.04,Default,,0000,0000,0000,,Ngươi mà tôi thực sự yêu là em!!
Dialogue: 0,0:20:48.81,0:20:52.51,Default,,0000,0000,0000,,Nếu em ko yêu tôi, tôi cũng chẳng cần quan tâm tới hành tinh này hay bất cứ thứ gì khác.
Dialogue: 0,0:20:52.88,0:20:54.97,Default,,0000,0000,0000,,Tôi muốn em hãy mởi rộng trái tim đón nhận tôi.
Dialogue: 0,0:20:56.85,0:20:57.71,Default,,0000,0000,0000,,Ôi...
Dialogue: 0,0:21:04.19,0:21:08.00,Default,,0000,0000,0000,,Em đúng là một cô gái xấu tính, tha lỗi cho em...
Dialogue: 0,0:21:08.00,0:21:10.36,Default,,0000,0000,0000,,Đừng nói như thế.
Dialogue: 0,0:21:11.16,0:21:16.43,Default,,0000,0000,0000,,Vậy làm ơn...hãy trừng phạt em đi.
Dialogue: 0,0:21:16.87,0:21:18.20,Default,,0000,0000,0000,,Trừng phạt...ối mẹ ơi...
Dialogue: 0,0:21:19.17,0:21:22.01,Default,,0000,0000,0000,,Làm ơn đi mà...
Dialogue: 0,0:21:22.44,0:21:24.10,Default,,0000,0000,0000,,Vậy thì...
Dialogue: 0,0:21:30.32,0:21:32.58,Default,,0000,0000,0000,,Anh vẫn ko đoái hoài gì tới em ngay cả khi em như thế này ư?
Dialogue: 0,0:21:33.89,0:21:34.89,Default,,0000,0000,0000,,Được rồi!
Dialogue: 0,0:21:34.89,0:21:37.65,Default,,0000,0000,0000,,Anh sẽ trừng phạt em!
Dialogue: 0,0:21:52.87,0:21:54.67,Default,,0000,0000,0000,,Này? Anh sé nhét nó vào trong đây...
Dialogue: 0,0:21:59.65,0:22:02.01,Default,,0000,0000,0000,,Đau quá...!
Dialogue: 0,0:22:03.42,0:22:05.01,Default,,0000,0000,0000,,Em ko sao chứ?
Dialogue: 0,0:22:05.49,0:22:08.98,Default,,0000,0000,0000,,Vâng, em ổn mà...
Dialogue: 0,0:22:33.75,0:22:35.74,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:22:39.65,0:22:40.52,Default,,0000,0000,0000,,Rát quá...
Dialogue: 0,0:22:44.36,0:22:46.02,Default,,0000,0000,0000,,Nó thật là lớn...
Dialogue: 0,0:22:59.81,0:23:02.24,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:23:09.68,0:23:14.12,Default,,0000,0000,0000,,Ko, em sắp ra rồi!
Dialogue: 0,0:23:16.56,0:23:18.99,Default,,0000,0000,0000,,Trạng thái cảm xúc của Pudding đã đạt cực đại!
Dialogue: 0,0:23:19.63,0:23:20.32,Default,,0000,0000,0000,,Sao cơ!?
Dialogue: 0,0:23:20.56,0:23:23.59,Default,,0000,0000,0000,,Tôi ko cần biết chuyện gì sẽ xảy ra... đưa họ tới đó, ngay lập tức!
Dialogue: 0,0:23:28.64,0:23:32.63,Default,,0000,0000,0000,,Hehehe, đây sẽ là kết cục của bọn Sex Warriors.
Dialogue: 0,0:23:33.37,0:23:34.17,Default,,0000,0000,0000,,Cái gì thế?
Dialogue: 0,0:23:37.11,0:23:37.87,Default,,0000,0000,0000,,Cái gì!?
Dialogue: 0,0:23:39.08,0:23:44.52,Default,,0000,0000,0000,,Huh, gì thế này? Mình bay được ư?
Dialogue: 0,0:23:44.92,0:23:46.91,Default,,0000,0000,0000,,Oh, xấu hổ chết đi được!!
Dialogue: 0,0:23:47.09,0:23:51.05,Default,,0000,0000,0000,,Wow! Khi hai trái tim hòa chung nhịp đập, một nguồn sức mạnh vĩ đại đã xuất hiện.
Dialogue: 0,0:23:51.32,0:23:54.33,Default,,0000,0000,0000,,Đây chắc hẳn là Super Pudding trong truyền thuyết!
Dialogue: 0,0:23:54.33,0:23:55.23,Default,,0000,0000,0000,,Truyền thuyết?
Dialogue: 0,0:23:55.63,0:23:58.33,Default,,0000,0000,0000,,Mình ko thể như thế này mãi được!
Dialogue: 0,0:23:59.50,0:24:00.33,Default,,0000,0000,0000,,Phải tấn công thôi!
Dialogue: 0,0:24:00.50,0:24:03.49,Default,,0000,0000,0000,,Electrifying Pudding Kick!
Dialogue: 0,0:24:06.11,0:24:07.66,Default,,0000,0000,0000,,Em hạ được nó rồi, Pudding!
Dialogue: 0,0:24:07.97,0:24:13.25,Default,,0000,0000,0000,,Sức mạnh của tình yêu... thật là lớn lao!
Dialogue: 0,0:24:13.25,0:24:14.04,Default,,0000,0000,0000,,Tình yêu?
Dialogue: 0,0:24:14.85,0:24:18.94,Default,,0000,0000,0000,,Tất cả đều nhờ vào anh. Thật đấy...
Dialogue: 0,0:24:19.09,0:24:20.28,Default,,0000,0000,0000,,Pudding...
Dialogue: 0,0:24:20.79,0:24:22.72,Default,,0000,0000,0000,,Đừng có chạm vào cô ấy!
Dialogue: 0,0:24:23.26,0:24:25.73,Default,,0000,0000,0000,,Pudding là! Pudding là!
Dialogue: 0,0:24:25.73,0:24:28.56,Default,,0000,0000,0000,,Đúng rồi, Misuzu thích ngài quản lý.
Dialogue: 0,0:24:29.16,0:24:30.53,Default,,0000,0000,0000,,Cô ấy là của tôi!
Dialogue: 0,0:24:30.86,0:24:33.60,Default,,0000,0000,0000,,Đừng có nói với tôi người mà cô nói tới là...
Dialogue: 0,0:24:33.60,0:24:34.63,Default,,0000,0000,0000,,Đó là Pudding.
Dialogue: 0,0:24:34.63,0:24:35.69,Default,,0000,0000,0000,,Cái quái gì vậy...
Dialogue: 0,0:24:36.54,0:24:38.97,Default,,0000,0000,0000,,Lũ Sex Warriors khốn kiếp!
Dialogue: 0,0:24:39.67,0:24:40.54,Default,,0000,0000,0000,,Bỏ mẹ rồi.
Dialogue: 0,0:24:45.55,0:24:47.51,Default,,0000,0000,0000,,Ko công bằng... tôi nữa.
Dialogue: 0,0:24:47.51,0:24:49.88,Default,,0000,0000,0000,,Dừng lại ngay!
Dialogue: 0,0:24:49.95,0:24:54.39,Default,,0000,0000,0000,,Cuộc chiến vẫn chưa kết thúc. Trận chiến của tình yêu thương mới chỉ bắt đầu.
Dialogue: 0,0:24:55.32,0:24:58.05,Default,,0000,0000,0000,,Cô chắc chắn sẽ làm được, Pudding! Can đảm lên, Pudding!
Dialogue: 0,0:24:58.33,0:25:00.29,Default,,0000,0000,0000,,Oh, và cả con nữa, Shuta!
