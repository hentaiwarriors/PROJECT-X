Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:14.28,0:00:18.51,Default,,0000,0000,0000,,Tại đây trước sự chứng kiến của Chúa trời và những người làm chứng, dù đau ốm hay khỏe mạnh...
Dialogue: 0,0:00:19.08,0:00:24.45,Default,,0000,0000,0000,,Hai người này đã nguyện sẽ yêu thương nhau trọn đời.
Dialogue: 0,0:00:26.29,0:00:28.12,Default,,0000,0000,0000,,Tên tôi là Takega Satsu.
Dialogue: 0,0:00:28.56,0:00:29.45,Default,,0000,0000,0000,,Tôi hiện giờ 18 tuổi.
Dialogue: 0,0:00:30.29,0:00:34.46,Default,,0000,0000,0000,,Tuân theo yêu cầu của ông ngoại Akuji, tôi sẽ kết hôm vào ngày hôm nay.
Dialogue: 0,0:00:36.83,0:00:41.06,Default,,0000,0000,0000,,Người sẽ trở thành chồng tôi, Yamamoto Akuji.
Dialogue: 0,0:00:42.64,0:00:45.80,Default,,0000,0000,0000,,Từ hôm nay trở đi, tôi sẽ có tên là Yamamoto Satsu.
Dialogue: 0,0:00:45.97,0:00:50.88,Default,,0000,0000,0000,,DAIAKUJI 7
Dialogue: 0,0:00:53.18,0:00:54.94,Default,,0000,0000,0000,,Ngay bây giờ, con có thể hôn cô dâu của mình.
Dialogue: 0,0:01:12.27,0:01:14.43,Default,,0000,0000,0000,,Akuji! Mạng sống của anh phải thuộc về tôi!
Dialogue: 0,0:01:14.80,0:01:16.03,Default,,0000,0000,0000,,Ko tha một ai cả!
Dialogue: 0,0:01:17.21,0:01:19.64,Default,,0000,0000,0000,,Chủ nhân! Chủ nhân!
Dialogue: 0,0:01:34.89,0:01:36.05,Default,,0000,0000,0000,,Hạ được hắn chưa?
Dialogue: 0,0:01:38.36,0:01:42.06,Default,,0000,0000,0000,,Làm điều này ngay trong ngày cưới của ta hả! To gan đấy!
Dialogue: 0,0:01:42.90,0:01:45.53,Default,,0000,0000,0000,,Akuji!! lan, Kilika, xông lên!
Dialogue: 0,0:01:45.90,0:01:46.46,Default,,0000,0000,0000,,Mạng sống của anh...
Dialogue: 0,0:01:46.77,0:01:47.39,Default,,0000,0000,0000,,là của tôi!
Dialogue: 0,0:02:10.39,0:02:11.69,Default,,0000,0000,0000,,Ha, ha, ha...
Dialogue: 0,0:02:19.27,0:02:21.24,Default,,0000,0000,0000,,Rút lui! Các người hãy cứ chờ xem !
Dialogue: 0,0:02:21.60,0:02:22.57,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:02:28.34,0:02:32.71,Default,,0000,0000,0000,,Whew...cô nhìn vẫn chẳng khác một đứa trẻ là mấy.
Dialogue: 0,0:02:33.21,0:02:34.51,Default,,0000,0000,0000,,Tôi đã đến tuổi trưởng thành rồi
Dialogue: 0,0:02:35.02,0:02:35.64,Default,,0000,0000,0000,,Thật hả?
Dialogue: 0,0:02:36.02,0:02:36.31,Default,,0000,0000,0000,,Đây.
Dialogue: 0,0:02:36.75,0:02:37.65,Default,,0000,0000,0000,,Bằng lái xe của cô hả...
Dialogue: 0,0:02:38.29,0:02:38.65,Default,,0000,0000,0000,,Đâu xem nào...
Dialogue: 0,0:02:39.95,0:02:43.32,Default,,0000,0000,0000,,Hả, bằng lái cấp độ A và C á?
Dialogue: 0,0:02:43.63,0:02:47.32,Default,,0000,0000,0000,,Anh có muốn xem thêm bằng lái tàu thủy và máy bay của tôi ko?
Dialogue: 0,0:02:47.63,0:02:48.12,Default,,0000,0000,0000,,Thôi khỏi...
Dialogue: 0,0:02:48.90,0:02:50.52,Default,,0000,0000,0000,,Tôi cũng đã được cấp chứng chỉ chuyên viên thuốc nổ loại ưu.
Dialogue: 0,0:02:54.10,0:02:57.20,Default,,0000,0000,0000,,Vậy bây giờ tôi đã chứng minh được là mình đủ tuổi chưa?
Dialogue: 0,0:02:57.71,0:02:58.87,Default,,0000,0000,0000,,Rồi, tôi biết rồi.
Dialogue: 0,0:02:59.11,0:03:01.74,Default,,0000,0000,0000,,Cô đã đủ tuổi, đã là một người trưởng thành.
Dialogue: 0,0:03:02.44,0:03:04.81,Default,,0000,0000,0000,,Nhưng tại sao cô lại đột ngột muốn kết hôn với tôi thế?
Dialogue: 0,0:03:05.18,0:03:07.48,Default,,0000,0000,0000,,Đó là yêu cầu của ông ngoại anh.
Dialogue: 0,0:03:13.45,0:03:14.89,Default,,0000,0000,0000,,Và cô chấp nhận điều đó à?
Dialogue: 0,0:03:20.53,0:03:21.83,Default,,0000,0000,0000,,Sao? Sao thế?
Dialogue: 0,0:03:22.80,0:03:24.77,Default,,0000,0000,0000,,Tôi sẽ ko phiền nếu anh gọi tôi là "Sat-chan."
Dialogue: 0,0:03:25.33,0:03:26.10,Default,,0000,0000,0000,,Umm...
Dialogue: 0,0:03:28.20,0:03:29.97,Default,,0000,0000,0000,,À đúng rồi, anh có muốn uống trà ko?
Dialogue: 0,0:03:30.47,0:03:31.84,Default,,0000,0000,0000,,Uhm, cô cứ pha đi.
Dialogue: 0,0:03:39.41,0:03:41.51,Default,,0000,0000,0000,,Này này, chẳng phải cô nói sẽ pha trà sao?
Dialogue: 0,0:03:42.15,0:03:42.78,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:03:43.55,0:03:47.39,Default,,0000,0000,0000,,Tôi tưởng một bà nội trợ thì phải mặc thứ này khi pha trà.
Dialogue: 0,0:03:49.16,0:03:49.85,Default,,0000,0000,0000,,Ờ thì...
Dialogue: 0,0:03:50.09,0:03:50.79,Default,,0000,0000,0000,,Ko đúng à?
Dialogue: 0,0:03:52.29,0:03:55.66,Default,,0000,0000,0000,,Uhm, cái này còn tùy xem người chồng có yêu cầu hay ko.
Dialogue: 0,0:03:56.23,0:03:58.13,Default,,0000,0000,0000,,Anh ko muốn thế à?
Dialogue: 0,0:03:58.83,0:04:01.27,Default,,0000,0000,0000,,Thành thật mà nói, tôi ko khoái trò này lắm.
Dialogue: 0,0:04:03.30,0:04:08.00,Default,,0000,0000,0000,,Xin lỗi, tôi ko hề biết rằng anh ko có hứng thú với phụ nữ.
Dialogue: 0,0:04:10.85,0:04:12.75,Default,,0000,0000,0000,,Ko phải! Ko phải như thế đâu mẹ trẻ ạ!
Dialogue: 0,0:04:13.98,0:04:16.35,Default,,0000,0000,0000,,Cô trông vẫn chỉ giống như...
Dialogue: 0,0:04:17.59,0:04:18.88,Default,,0000,0000,0000,,Giống như gì?
Dialogue: 0,0:04:19.85,0:04:26.76,Default,,0000,0000,0000,,Thì vẫn còn một chút gì đó... chưa trưởng thành hết.
Dialogue: 0,0:04:28.20,0:04:32.16,Default,,0000,0000,0000,,Ý anh là tôi chưa phát triển hết chứ gì? Anh thích ngực bự hay gì khác phải ko?
Dialogue: 0,0:04:32.87,0:04:35.56,Default,,0000,0000,0000,,Hay là anh ko thích hình dạng của tôi?
Dialogue: 0,0:04:37.54,0:04:40.97,Default,,0000,0000,0000,,Mà thôi, tôi sẽ ko cãi nhau với anh.
Dialogue: 0,0:04:51.89,0:04:52.32,Default,,0000,0000,0000,,Này.
Dialogue: 0,0:04:52.69,0:04:56.05,Default,,0000,0000,0000,,Đừng lo. Tôi sẽ ko làm cuộc sống của chúng ta trở nên ngột ngạt đâu.
Dialogue: 0,0:04:57.89,0:05:00.12,Default,,0000,0000,0000,,Nâng váy lên nào.
Dialogue: 0,0:05:01.10,0:05:01.53,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:05:02.23,0:05:04.79,Default,,0000,0000,0000,,Cô ko phải vợ tôi sao, Sat-chan?
Dialogue: 0,0:05:09.97,0:05:11.27,Default,,0000,0000,0000,,Chiếc quần lót dễ thương đấy.
Dialogue: 0,0:05:11.77,0:05:15.27,Default,,0000,0000,0000,,Đó là thứ duy nhất ko hợp với đồng phục của tôi.
Dialogue: 0,0:05:16.78,0:05:18.61,Default,,0000,0000,0000,,Lúc xấu hổ trông cô thật dễ thương.
Dialogue: 0,0:05:21.12,0:05:22.08,Default,,0000,0000,0000,,Từ lúc nào mà anh...
Dialogue: 0,0:05:27.92,0:05:29.55,Default,,0000,0000,0000,,Chúng ta chưa hôn nhau lần nào đúng ko.
Dialogue: 0,0:05:29.99,0:05:31.75,Default,,0000,0000,0000,,Nhưng, sao lại là ở đây?
Dialogue: 0,0:05:32.06,0:05:35.69,Default,,0000,0000,0000,,Ở trong tình huống này, người vợ sẽ ko bao giờ thắc mắc cả.
Dialogue: 0,0:05:46.54,0:05:47.57,Default,,0000,0000,0000,,Bộ ngực dễ thương thật.
Dialogue: 0,0:05:48.68,0:05:50.30,Default,,0000,0000,0000,,Ý anh là chúng quá nhỏ à?
Dialogue: 0,0:05:51.08,0:05:53.17,Default,,0000,0000,0000,,Vẫn còn giận à? Cô dâu của tôi.
Dialogue: 0,0:05:53.88,0:05:56.37,Default,,0000,0000,0000,,Nhưng kích cỡ và độ nhạy cảm lại khác nhau hoàn toàn.
Dialogue: 0,0:05:59.29,0:06:02.26,Default,,0000,0000,0000,,Lần đầu tiên được một người đàn ông chạm vào hả?
Dialogue: 0,0:06:03.09,0:06:03.79,Default,,0000,0000,0000,,Như thế này .
Dialogue: 0,0:06:05.96,0:06:07.72,Default,,0000,0000,0000,,Anh làm gì thế?
Dialogue: 0,0:06:08.56,0:06:09.33,Default,,0000,0000,0000,,Yên lặng nào.
Dialogue: 0,0:06:09.76,0:06:15.46,Default,,0000,0000,0000,,Đợi đã. Tôi ko ngại làm chuyện này, nhưng tôi vẫn còn là trinh nữ đấy.
Dialogue: 0,0:06:16.97,0:06:17.87,Default,,0000,0000,0000,,Tôi biết rồi.
Dialogue: 0,0:06:18.51,0:06:21.00,Default,,0000,0000,0000,,Tôi tin là mình biết cách chăm sóc phụ nữ.
Dialogue: 0,0:06:21.58,0:06:22.74,Default,,0000,0000,0000,,Hiểu rồi.
Dialogue: 0,0:06:24.71,0:06:26.41,Default,,0000,0000,0000,,Cô có làn da thật đẹp.
Dialogue: 0,0:06:27.45,0:06:29.28,Default,,0000,0000,0000,,Dang ra một chút nữa nào.
Dialogue: 0,0:06:30.45,0:06:31.94,Default,,0000,0000,0000,,Như thế này hả?
Dialogue: 0,0:06:39.86,0:06:40.49,Default,,0000,0000,0000,,Để xem nào.
Dialogue: 0,0:06:43.66,0:06:44.76,Default,,0000,0000,0000,,Đặt hai chân của cô như thế này
Dialogue: 0,0:06:45.47,0:06:46.96,Default,,0000,0000,0000,,Cô nhạy cảm hơn tôi nghĩ nhiều đấy.
Dialogue: 0,0:06:47.74,0:06:49.36,Default,,0000,0000,0000,,Ko thể tin là cô hãy còn trinh.
Dialogue: 0,0:06:50.47,0:06:52.77,Default,,0000,0000,0000,,Ở nguyên đó. Tôi sẽ làm cho cô sung sướng.
Dialogue: 0,0:06:56.08,0:06:57.31,Default,,0000,0000,0000,,Thư giãn cở thể đi nào
Dialogue: 0,0:06:58.21,0:07:00.51,Default,,0000,0000,0000,,Đúng thế. Hãy tận hưởng cảm giác này.
Dialogue: 0,0:07:04.89,0:07:05.85,Default,,0000,0000,0000,,Sao...sao cơ.
Dialogue: 0,0:07:06.69,0:07:09.71,Default,,0000,0000,0000,,Đợi đã, toàn bộ sức lực của tôi...
Dialogue: 0,0:07:15.83,0:07:17.66,Default,,0000,0000,0000,,Tốt. Tôi sẽ đặt cô ở đây.
Dialogue: 0,0:07:20.84,0:07:23.80,Default,,0000,0000,0000,,Anh định làm gì thế? Tôi cảm thấy thật tệ.
Dialogue: 0,0:07:24.31,0:07:27.07,Default,,0000,0000,0000,,Điều đó ko hề tệ chút nào, chứng tỏ cô đang trở nên nữ tính hơn.
Dialogue: 0,0:07:27.44,0:07:29.27,Default,,0000,0000,0000,,Trở nên nữ tính hơn?
Dialogue: 0,0:07:29.51,0:07:34.00,Default,,0000,0000,0000,,Đúng thế. Hãy để cở thể cô dần dần cảm nhận nó...
Dialogue: 0,0:07:34.65,0:07:38.21,Default,,0000,0000,0000,,Những khát khao thầm kín của người phụ nữ.
Dialogue: 0,0:07:38.32,0:07:38.94,Default,,0000,0000,0000,,Phải rồi.
Dialogue: 0,0:07:39.45,0:07:42.95,Default,,0000,0000,0000,,Nếu cô học được điều đó, cô sẽ trở thành một người phụ nữ cực kì quyến rũ.
Dialogue: 0,0:07:43.66,0:07:45.82,Default,,0000,0000,0000,,Đó, hãy cố cảm nhận nó.
Dialogue: 0,0:07:50.33,0:07:52.16,Default,,0000,0000,0000,,Thấy thế nào? Sướng chứ?
Dialogue: 0,0:07:53.87,0:07:55.63,Default,,0000,0000,0000,,Thôi nào, cứ nói ra đi.
Dialogue: 0,0:07:56.20,0:07:57.17,Default,,0000,0000,0000,,Tôi sẽ làm cô sung sướng.
Dialogue: 0,0:07:58.34,0:08:01.04,Default,,0000,0000,0000,,Sướng... lắm... nhưng mà...
Dialogue: 0,0:08:02.28,0:08:04.24,Default,,0000,0000,0000,,Có gì đó... có gì đó đang ra!
Dialogue: 0,0:08:04.75,0:08:06.77,Default,,0000,0000,0000,,Ko, hãy thật tập trung trong khoảnh khắc cuối này.
Dialogue: 0,0:08:07.15,0:08:08.17,Default,,0000,0000,0000,,Như...Nhưng...
Dialogue: 0,0:08:10.82,0:08:11.65,Default,,0000,0000,0000,,Ko, ko.
Dialogue: 0,0:08:12.09,0:08:13.52,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:08:15.89,0:08:19.26,Default,,0000,0000,0000,,Tôi ko nhịn được nữa rồi...
Dialogue: 0,0:08:19.56,0:08:21.39,Default,,0000,0000,0000,,Ko sao đâu. Hãy ra hết đi.
Dialogue: 0,0:08:25.57,0:08:26.46,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:08:28.24,0:08:30.40,Default,,0000,0000,0000,,Có gì đó đang ra... đang ra...
Dialogue: 0,0:08:31.24,0:08:33.87,Default,,0000,0000,0000,,Tôi đang ra... đang ra... đang ra...
Dialogue: 0,0:08:34.64,0:08:35.40,Default,,0000,0000,0000,,Akuji...
Dialogue: 0,0:09:02.80,0:09:05.03,Default,,0000,0000,0000,,Giờ đến lượt em rồi đấy.
Dialogue: 0,0:09:05.47,0:09:06.17,Default,,0000,0000,0000,,Đến lượt em hả...
Dialogue: 0,0:09:12.61,0:09:15.64,Default,,0000,0000,0000,,Anh đoán là em đã biết phải làm thế nào, đúng ko?
Dialogue: 0,0:09:15.82,0:09:16.98,Default,,0000,0000,0000,,Vâ, vâng...
Dialogue: 0,0:09:23.36,0:09:24.19,Default,,0000,0000,0000,,Được rồi đó.
Dialogue: 0,0:09:24.76,0:09:27.99,Default,,0000,0000,0000,,Khi em làm điều này, hãy nhìn vào mặt anh.
Dialogue: 0,0:09:28.63,0:09:29.25,Default,,0000,0000,0000,,Thế này hả?
Dialogue: 0,0:09:31.63,0:09:35.19,Default,,0000,0000,0000,,Giờ thì xòe tay ra, và vuốt ve chỗ đó của anh đi.
Dialogue: 0,0:09:38.97,0:09:41.13,Default,,0000,0000,0000,,Tốt lắm. Em làm rất tuyệt.
Dialogue: 0,0:09:47.98,0:09:52.21,Default,,0000,0000,0000,,À phải rồi, miệng của em hơi nhỏ, nên thật khó để ngậm thứ to như vậy.
Dialogue: 0,0:09:52.79,0:09:54.35,Default,,0000,0000,0000,,Em ỏn mà.
Dialogue: 0,0:09:55.99,0:09:56.75,Default,,0000,0000,0000,,Đững gượng ép mình quá.
Dialogue: 0,0:09:56.92,0:09:58.48,Default,,0000,0000,0000,,Đừng lo cho em.
Dialogue: 0,0:09:59.86,0:10:00.42,Default,,0000,0000,0000,,Cô gái này...
Dialogue: 0,0:10:44.24,0:10:44.67,Default,,0000,0000,0000,,Tuyệt lắm...
Dialogue: 0,0:10:47.24,0:10:47.61,Default,,0000,0000,0000,,Anh ra đây!
Dialogue: 0,0:11:10.87,0:11:11.30,Default,,0000,0000,0000,,Sẵn sàng chưa?
Dialogue: 0,0:11:12.27,0:11:12.76,Default,,0000,0000,0000,,Làm đi anh.
Dialogue: 0,0:11:17.87,0:11:19.43,Default,,0000,0000,0000,,Đừng quá căng thẳng.
Dialogue: 0,0:11:20.21,0:11:21.90,Default,,0000,0000,0000,,Cứ để anh lo mọi chuyện.
Dialogue: 0,0:11:22.28,0:11:23.37,Default,,0000,0000,0000,,Em biết rồi.
Dialogue: 0,0:11:26.35,0:11:28.58,Default,,0000,0000,0000,,Người ta nói những người ngực nhỏ thường rất nhạy cảm.
Dialogue: 0,0:11:36.09,0:11:36.65,Default,,0000,0000,0000,,Anh nhét nó vào đây...
Dialogue: 0,0:11:39.69,0:11:42.39,Default,,0000,0000,0000,,Của em khít quá. Thư giãn đi, Sat-chan.
Dialogue: 0,0:11:43.70,0:11:45.79,Default,,0000,0000,0000,,Em đang cố thư giãn đây.
Dialogue: 0,0:11:46.50,0:11:47.93,Default,,0000,0000,0000,,Vậy hả?
Dialogue: 0,0:11:49.84,0:11:50.86,Default,,0000,0000,0000,,Ouch, ouch...
Dialogue: 0,0:11:50.97,0:11:51.46,Default,,0000,0000,0000,,Em ko sao chứ?
Dialogue: 0,0:11:52.04,0:11:54.34,Default,,0000,0000,0000,,Vâng, anh cứ tiếp tục đi.
Dialogue: 0,0:11:55.04,0:11:56.20,Default,,0000,0000,0000,,Nếu em đã muốn như vậy...
Dialogue: 0,0:11:56.71,0:11:57.20,Default,,0000,0000,0000,,Làm ơn đi mà!
Dialogue: 0,0:12:00.65,0:12:01.55,Default,,0000,0000,0000,,Oh tệ thật...
Dialogue: 0,0:12:04.59,0:12:05.28,Default,,0000,0000,0000,,Ouch...
Dialogue: 0,0:12:05.99,0:12:06.48,Default,,0000,0000,0000,,Khỉ thật...
Dialogue: 0,0:12:07.19,0:12:07.81,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:12:09.06,0:12:10.35,Default,,0000,0000,0000,,Một chút nữa thôi... một chút nữa...
Dialogue: 0,0:12:17.73,0:12:18.96,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:12:19.80,0:12:20.49,Default,,0000,0000,0000,,Em...
Dialogue: 0,0:12:21.14,0:12:22.69,Default,,0000,0000,0000,,Em ko sao thật mà. Chúng ta hãy...
Dialogue: 0,0:12:23.07,0:12:23.76,Default,,0000,0000,0000,,Ngừng lại thôi.
Dialogue: 0,0:12:24.00,0:12:24.30,Default,,0000,0000,0000,,Sao thế?
Dialogue: 0,0:12:24.81,0:12:26.50,Default,,0000,0000,0000,,Ngày hôm nay như vậy là đủ rồi
Dialogue: 0,0:12:26.87,0:12:27.17,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:12:27.74,0:12:30.30,Default,,0000,0000,0000,,Nếu em thấy đau đớn như vậy, anh cũng chả thích thù gì.
Dialogue: 0,0:12:30.68,0:12:31.51,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:12:35.48,0:12:37.97,Default,,0000,0000,0000,,Thôi nào, hãy sống vui vẻ cùng nhau nào, được ko?
Dialogue: 0,0:13:15.26,0:13:16.95,Default,,0000,0000,0000,,Oh ko...
Dialogue: 0,0:13:19.39,0:13:20.16,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:13:24.80,0:13:29.50,Default,,0000,0000,0000,,Tôi hiểu rồi, vậy đó là lí do cô tới tìm tôi hả.
Dialogue: 0,0:13:30.20,0:13:33.83,Default,,0000,0000,0000,,Đúng vậy, có phải vì chỗ đó của tôi nhỏ quá ko?
Dialogue: 0,0:13:34.41,0:13:37.24,Default,,0000,0000,0000,,Điều đó ko hề đúng. Vấn đề chỉ là thời điểm thích hợp...
Dialogue: 0,0:13:37.81,0:13:40.58,Default,,0000,0000,0000,,Chú ý nhé...đây là một đặc điểm của phụ nữ.
Dialogue: 0,0:13:41.88,0:13:44.44,Default,,0000,0000,0000,,Một khi được tập luyện, cô cũng có thể chịu được một vật lớn như thế này.
Dialogue: 0,0:13:44.82,0:13:48.19,Default,,0000,0000,0000,,Ko, ko. Nó lớn quá.
Dialogue: 0,0:13:48.36,0:13:48.98,Default,,0000,0000,0000,,Nó đang vào trong.
Dialogue: 0,0:13:58.10,0:13:59.07,Default,,0000,0000,0000,,Chú ý nhé.
Dialogue: 0,0:13:59.43,0:14:03.39,Default,,0000,0000,0000,,Cái này có giống như gương mặt đau đớn của cô?
Dialogue: 0,0:14:05.31,0:14:07.21,Default,,0000,0000,0000,,Thế nào? Có đau ko?
Dialogue: 0,0:14:07.71,0:14:08.14,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:14:08.78,0:14:10.47,Default,,0000,0000,0000,,Nói xem cô cảm thấy thế nào.
Dialogue: 0,0:14:10.91,0:14:11.88,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:14:12.25,0:14:13.21,Default,,0000,0000,0000,,Cô nói thật đấy chứ.
Dialogue: 0,0:14:14.11,0:14:16.01,Default,,0000,0000,0000,,Sướng lắm ạ, thưa chủ nhân.
Dialogue: 0,0:14:16.38,0:14:18.35,Default,,0000,0000,0000,,Tốt lắm. Kết quả rất tuyệt.
Dialogue: 0,0:14:19.65,0:14:21.35,Default,,0000,0000,0000,,Nào, giờ là bước tiếp theo.
Dialogue: 0,0:14:34.94,0:14:35.49,Default,,0000,0000,0000,,Hãy nhìn xem...
Dialogue: 0,0:14:42.14,0:14:46.31,Default,,0000,0000,0000,,Ohh, đây là lúc thích hợp đấy. Coi kìa.
Dialogue: 0,0:14:46.75,0:14:50.65,Default,,0000,0000,0000,,Đây. Âm hộ của cô ta đã nới rộng hơn rồi.
Dialogue: 0,0:14:50.89,0:14:53.12,Default,,0000,0000,0000,,Ko, đừng banh nó ra.
Dialogue: 0,0:14:53.82,0:14:57.52,Default,,0000,0000,0000,,Nó giờ rất mềm. Thử đụng vào xem.
Dialogue: 0,0:15:01.63,0:15:02.72,Default,,0000,0000,0000,,Cú chạm thật tinh tế...
Dialogue: 0,0:15:03.50,0:15:04.40,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:15:05.63,0:15:10.13,Default,,0000,0000,0000,,Giờ cô đã hiễu rõ rồi chứ? Với sự rèn luyện cao độ, chỗ đó có thể nới rộng ra rất nhiều.
Dialogue: 0,0:15:10.70,0:15:11.14,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:15:11.71,0:15:15.33,Default,,0000,0000,0000,,Hi vọng cô sẽ làm vừa lòng chủ nhân!
Dialogue: 0,0:15:17.24,0:15:21.01,Default,,0000,0000,0000,,Này, đừng dừng lại lúc này. Hãy cho tôi thêm nữa...
Dialogue: 0,0:15:33.06,0:15:34.96,Default,,0000,0000,0000,,Akuji, tới suối nước nóng nào.
Dialogue: 0,0:15:35.20,0:15:35.56,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:15:36.26,0:15:38.03,Default,,0000,0000,0000,,Chúng ta sẽ tới đó vào sáng mai, rõ chưa?
Dialogue: 0,0:15:38.27,0:15:39.96,Default,,0000,0000,0000,,Này, ý em là sao, "rõ chưa" là thế nào hả?
Dialogue: 0,0:15:40.60,0:15:41.63,Default,,0000,0000,0000,,Nếu anh dậy muộn, em sẽ ko tha cho anh đâu đấy.
Dialogue: 0,0:15:42.00,0:15:44.49,Default,,0000,0000,0000,,Này đợi đã! Sao lại là suối nước nóng?
Dialogue: 0,0:15:44.74,0:15:45.90,Default,,0000,0000,0000,,Cho tuần trăng mật của chúng ta.
Dialogue: 0,0:15:46.41,0:15:46.84,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:15:47.74,0:15:49.50,Default,,0000,0000,0000,,Tuần trăng mật á!
Dialogue: 0,0:15:58.22,0:15:58.91,Default,,0000,0000,0000,,Chúng ta tới rồi.
Dialogue: 0,0:16:03.69,0:16:04.92,Default,,0000,0000,0000,,Chào mừng quý khách!
Dialogue: 0,0:16:05.16,0:16:06.18,Default,,0000,0000,0000,,He, hey...
Dialogue: 0,0:16:06.63,0:16:08.25,Default,,0000,0000,0000,,Chúng tôi đã đặt riêng một phòng tắm với tên Yamamoto.
Dialogue: 0,0:16:08.56,0:16:10.53,Default,,0000,0000,0000,,Oh, vâng, vâng, chúng tôi đang đợi hai người đây.
Dialogue: 0,0:16:10.83,0:16:11.13,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:16:11.90,0:16:12.52,Default,,0000,0000,0000,,Tôi có thể giúp gì anh?
Dialogue: 0,0:16:13.03,0:16:16.66,Default,,0000,0000,0000,,À, chỉ là tôi thấy cô trông quen quen.
Dialogue: 0,0:16:18.11,0:16:22.67,Default,,0000,0000,0000,,Thôi nào! Đừng có tán tỉnh tôi ngay trước mặt em gái anh vậy chứ!
Dialogue: 0,0:16:23.18,0:16:25.15,Default,,0000,0000,0000,,Tôi ko phải em gái anh ấy. Tôi là vợ anh ấy!
Dialogue: 0,0:16:25.31,0:16:25.61,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:16:28.12,0:16:28.48,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:16:29.25,0:16:30.94,Default,,0000,0000,0000,,Cái gì!
Dialogue: 0,0:16:37.59,0:16:39.15,Default,,0000,0000,0000,,Nơi này đúng là thiên đường.
Dialogue: 0,0:16:39.46,0:16:40.48,Default,,0000,0000,0000,,Anh nói cứ như một ông già vậy.
Dialogue: 0,0:16:40.79,0:16:45.82,Default,,0000,0000,0000,,Uhm, ban đầu anh ko thích nơi này lắm, nhưng suối nước nóng uqar thật rất tuyệt.
Dialogue: 0,0:16:47.20,0:16:49.23,Default,,0000,0000,0000,,Em đang mặc gì thế?
Dialogue: 0,0:16:50.07,0:16:51.50,Default,,0000,0000,0000,,Anh có muốn uống chút rượu ko?
Dialogue: 0,0:16:51.61,0:16:53.04,Default,,0000,0000,0000,,À có.
Dialogue: 0,0:16:58.15,0:16:59.58,Default,,0000,0000,0000,,Thế này thích thật!
Dialogue: 0,0:17:00.28,0:17:03.37,Default,,0000,0000,0000,,Ngắm phong cảnh tuyệt đẹp và ngâm mình trong suối nước nóng.
Dialogue: 0,0:17:06.62,0:17:06.98,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:17:07.49,0:17:08.39,Default,,0000,0000,0000,,Em đang làm gì vậy?
Dialogue: 0,0:17:08.96,0:17:13.12,Default,,0000,0000,0000,,Nước nóng có thể làm cho chỗ đó mềm hơn.
Dialogue: 0,0:17:14.36,0:17:17.92,Default,,0000,0000,0000,,Anh hiểu rồi. Vậy đó là lí do em khăng khăng bắt anh tới đây.
Dialogue: 0,0:17:21.57,0:17:22.47,Default,,0000,0000,0000,,Để đó cho anh nào.
Dialogue: 0,0:17:30.11,0:17:32.48,Default,,0000,0000,0000,,Đúng là nó mềm hơn trước nhiều.
Dialogue: 0,0:17:32.98,0:17:37.08,Default,,0000,0000,0000,,Thật chứ? Em biết chắc là thế mà.
Dialogue: 0,0:17:37.65,0:17:38.41,Default,,0000,0000,0000,,Dễ thương lắm.
Dialogue: 0,0:17:40.25,0:17:41.62,Default,,0000,0000,0000,,Anh bắt đầu thấy hứng lên rồi,
Dialogue: 0,0:17:42.12,0:17:42.75,Default,,0000,0000,0000,,Hãy để việc này cho em.
Dialogue: 0,0:17:46.06,0:17:46.62,Default,,0000,0000,0000,,Thế nào anh?
Dialogue: 0,0:17:48.46,0:17:49.95,Default,,0000,0000,0000,,Em làm tốt hơn trước nhiều, phải ko?
Dialogue: 0,0:17:53.20,0:17:54.63,Default,,0000,0000,0000,,Em muốn chuyện này đến thế sao?
Dialogue: 0,0:17:55.80,0:17:58.29,Default,,0000,0000,0000,,Em muốn khiến anh được sung sướng.
Dialogue: 0,0:17:58.61,0:18:01.23,Default,,0000,0000,0000,,Nếu em lo răng anh có thể ngoại tình, anh sẽ ko...
Dialogue: 0,0:18:02.01,0:18:02.44,Default,,0000,0000,0000,,Một ngày nào đó...
Dialogue: 0,0:18:02.61,0:18:07.51,Default,,0000,0000,0000,,Ko, em chỉ muốn anh được sung sướng, Akuji.
Dialogue: 0,0:18:08.28,0:18:09.84,Default,,0000,0000,0000,,Chỉ vậy thôi.
Dialogue: 0,0:18:10.28,0:18:10.98,Default,,0000,0000,0000,,Sat-chan.
Dialogue: 0,0:18:12.55,0:18:15.04,Default,,0000,0000,0000,,Em muốn anh là người đầu tiên làm chuyện này với em.
Dialogue: 0,0:18:22.70,0:18:23.19,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:18:25.63,0:18:26.26,Default,,0000,0000,0000,,Sẵn sàng chưa?
Dialogue: 0,0:18:33.91,0:18:34.67,Default,,0000,0000,0000,,Wow...
Dialogue: 0,0:18:35.38,0:18:37.07,Default,,0000,0000,0000,,Em có cơ thể thật đẹp
Dialogue: 0,0:19:24.83,0:19:25.79,Default,,0000,0000,0000,,Hay đấy nhỉ.
Dialogue: 0,0:19:33.17,0:19:34.53,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:19:35.90,0:19:37.34,Default,,0000,0000,0000,,Anh có thể ra ngay bây giờ cũng được.
Dialogue: 0,0:19:39.57,0:19:41.47,Default,,0000,0000,0000,,Anh sẽ chơi em ở chỗ này. Rõ rồi chứ ?
Dialogue: 0,0:19:42.58,0:19:43.27,Default,,0000,0000,0000,,Đây...
Dialogue: 0,0:19:43.98,0:19:45.95,Default,,0000,0000,0000,,Được rồi, anh sẽ nhét nó vào đây.
Dialogue: 0,0:19:48.65,0:19:51.55,Default,,0000,0000,0000,,Liệu có vào được ko nhỉ?
Dialogue: 0,0:19:51.92,0:19:52.75,Default,,0000,0000,0000,,Em ko muốn à?
Dialogue: 0,0:19:55.46,0:19:56.15,Default,,0000,0000,0000,,Nhét nó vào đi anh!
Dialogue: 0,0:19:56.86,0:19:59.62,Default,,0000,0000,0000,,Em muốn được làm điều này với anh, Akuji.
Dialogue: 0,0:19:59.99,0:20:00.43,Default,,0000,0000,0000,,Anh vào đây.
Dialogue: 0,0:20:05.13,0:20:05.56,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:20:06.00,0:20:06.69,Default,,0000,0000,0000,,Một chút nữa thôi...
Dialogue: 0,0:20:10.34,0:20:11.03,Default,,0000,0000,0000,,Chút nữa...
Dialogue: 0,0:20:11.41,0:20:12.37,Default,,0000,0000,0000,,Ouch...
Dialogue: 0,0:20:13.07,0:20:14.17,Default,,0000,0000,0000,,Thư giãn đi.
Dialogue: 0,0:20:16.41,0:20:16.97,Default,,0000,0000,0000,,Cố tí nữa nào...
Dialogue: 0,0:20:21.48,0:20:22.11,Default,,0000,0000,0000,,Anh vào được rồi!
Dialogue: 0,0:20:40.17,0:20:40.79,Default,,0000,0000,0000,,Anh bắt đầu di chuyển đây.
Dialogue: 0,0:20:41.84,0:20:43.80,Default,,0000,0000,0000,,Đợi đã. Nếu anh làm nó ngay bây giờ...
Dialogue: 0,0:20:45.31,0:20:48.00,Default,,0000,0000,0000,,Đừng lo. Một khi nó đã vào được bên trong, thì mọi chuyện sẽ ổn.
Dialogue: 0,0:20:50.84,0:20:51.54,Default,,0000,0000,0000,,Ouch...
Dialogue: 0,0:20:59.85,0:21:01.01,Default,,0000,0000,0000,,Bắt đầu thấy khá hơn chưa...
Dialogue: 0,0:21:02.86,0:21:04.82,Default,,0000,0000,0000,,Chỗ đó rát quá...
Dialogue: 0,0:21:08.00,0:21:10.02,Default,,0000,0000,0000,,Akuji...Akuji...
Dialogue: 0,0:21:11.73,0:21:12.43,Default,,0000,0000,0000,,Akuji...
Dialogue: 0,0:21:12.73,0:21:13.36,Default,,0000,0000,0000,,Đừng lo gì hết.
Dialogue: 0,0:21:14.47,0:21:16.16,Default,,0000,0000,0000,,Em là vợ của anh mà.
Dialogue: 0,0:21:18.87,0:21:19.50,Default,,0000,0000,0000,,Akuji...
Dialogue: 0,0:21:20.94,0:21:21.70,Default,,0000,0000,0000,,Akuji!
Dialogue: 0,0:21:22.74,0:21:23.64,Default,,0000,0000,0000,,Anh sắp ra rồi.
Dialogue: 0,0:21:26.28,0:21:29.04,Default,,0000,0000,0000,,Em đang ra, đang ra. Em cảm thấy điều ấy rất rõ...
Dialogue: 0,0:21:29.82,0:21:30.37,Default,,0000,0000,0000,,Ra rồi...
Dialogue: 0,0:21:31.22,0:21:31.84,Default,,0000,0000,0000,,Em cũng thế...
Dialogue: 0,0:21:33.22,0:21:34.71,Default,,0000,0000,0000,,Em ra rồi, ra rồi!
Dialogue: 0,0:22:14.06,0:22:15.43,Default,,0000,0000,0000,,Gió mát thật.
Dialogue: 0,0:22:15.93,0:22:16.42,Default,,0000,0000,0000,,Uhm...
Dialogue: 0,0:22:17.06,0:22:19.36,Default,,0000,0000,0000,,Em cảm thấy thế nào sau khi làm chuyện này?
Dialogue: 0,0:22:20.60,0:22:25.70,Default,,0000,0000,0000,,Em ko rõ lắm. Nhưng em biết chắc rằng mình đã thuộc về anh.
Dialogue: 0,0:22:30.94,0:22:32.57,Default,,0000,0000,0000,,Có vẻ như em còn muốn nữa.
Dialogue: 0,0:22:33.55,0:22:34.31,Default,,0000,0000,0000,,Đợi em chút.
Dialogue: 0,0:22:44.76,0:22:45.78,Default,,0000,0000,0000,,Cái gì thế này?
Dialogue: 0,0:22:46.09,0:22:50.39,Default,,0000,0000,0000,,Anh ko thích à? Em nghe nói đàn ông rất thích phụ nữ mặc những thứ này.
Dialogue: 0,0:22:52.97,0:22:53.40,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:22:54.10,0:22:56.07,Default,,0000,0000,0000,,Ko có vấn đề gì với anh cả. Lại đây nào.
Dialogue: 0,0:23:05.31,0:23:05.94,Default,,0000,0000,0000,,Akuji...
Dialogue: 0,0:23:17.92,0:23:18.89,Default,,0000,0000,0000,,Nó vào trong rồi...
Dialogue: 0,0:23:19.19,0:23:19.62,Default,,0000,0000,0000,,Khít quá!
Dialogue: 0,0:23:20.13,0:23:21.62,Default,,0000,0000,0000,,Xin lỗi anh, nhưng...
Dialogue: 0,0:23:29.00,0:23:30.77,Default,,0000,0000,0000,,Ko sao đâu, hãy cảm nhận nó!
Dialogue: 0,0:23:34.88,0:23:37.11,Default,,0000,0000,0000,,Thế nào rồi? Em còn thấy đau nữa ko?
Dialogue: 0,0:23:50.02,0:23:51.39,Default,,0000,0000,0000,,Anh sẽ đâm.vào sâu hơn nữa
Dialogue: 0,0:23:54.03,0:23:55.19,Default,,0000,0000,0000,,Nó đang mở rộng ra... Mọi thứ...
Dialogue: 0,0:23:56.03,0:23:57.33,Default,,0000,0000,0000,,Ko, nó rách ra mất!
Dialogue: 0,0:24:02.90,0:24:04.67,Default,,0000,0000,0000,,Cái của anh vào hết bên trong em rồi
Dialogue: 0,0:24:04.91,0:24:07.47,Default,,0000,0000,0000,,Ko, ko phải thế này... ko phải thế này...
Dialogue: 0,0:24:08.38,0:24:11.81,Default,,0000,0000,0000,,Hãy ra đi. Quên đi tất cả mọi chuyện, và trở thành một người phụ nữ thực thụ.
Dialogue: 0,0:24:12.45,0:24:13.41,Default,,0000,0000,0000,,Hãy cho anh thấy tất cả những gì em có.
Dialogue: 0,0:24:14.92,0:24:17.68,Default,,0000,0000,0000,,Sâu quá...nó vào sâu quá...
Dialogue: 0,0:24:19.25,0:24:20.01,Default,,0000,0000,0000,,Sao, sao cơ?
Dialogue: 0,0:24:20.59,0:24:21.88,Default,,0000,0000,0000,,Mọi người sẽ nhìn thất mất...
Dialogue: 0,0:24:36.94,0:24:38.03,Default,,0000,0000,0000,,Thứ gì đó đang ra.
Dialogue: 0,0:25:04.16,0:25:05.26,Default,,0000,0000,0000,,Chỉ là một giấc mơ.
Dialogue: 0,0:25:07.90,0:25:10.46,Default,,0000,0000,0000,,Mẹ ơi! Cha bảo cần mẹ giúp chuyện gì kìa!
Dialogue: 0,0:25:10.84,0:25:13.40,Default,,0000,0000,0000,,Cha nói rằng sẽ đi tiêu diệt những gã khốn ở Shigi-kai!
Dialogue: 0,0:25:13.51,0:25:14.41,Default,,0000,0000,0000,,Em làm cái gì thế?
Dialogue: 0,0:25:15.91,0:25:17.14,Default,,0000,0000,0000,,Đồ điên!
Dialogue: 0,0:25:18.51,0:25:19.07,Default,,0000,0000,0000,,Mẹ biết rồi.
Dialogue: 0,0:25:22.12,0:25:22.74,Default,,0000,0000,0000,,Mẹ ra ngay bây giờ đây.
Dialogue: 0,0:25:30.72,0:25:32.62,Default,,0000,0000,0000,,Oh, phu nhân, họ đang đợi ngoài kia.
Dialogue: 0,0:25:33.19,0:25:35.49,Default,,0000,0000,0000,,Này, Sat-chan. Xin lỗi em về chuyện này nhé.
Dialogue: 0,0:25:35.80,0:25:36.16,Default,,0000,0000,0000,,Ko có gì đâu...
Dialogue: 0,0:25:36.60,0:25:37.89,Default,,0000,0000,0000,,Cha mẹ cẩn trọng nhé!
Dialogue: 0,0:25:38.20,0:25:40.10,Default,,0000,0000,0000,,Đánh cho chúng ko còn manh giáp nhé!
