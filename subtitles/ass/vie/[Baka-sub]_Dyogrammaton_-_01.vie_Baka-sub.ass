Comment: 0,0:03:15.89,0:03:19.72,Note,,0000,0000,0000,,PHÒNG TẮM ĐÃ ĐẶT TRƯỚC
Comment: 0,0:05:17.88,0:05:19.68,Default,,0000,0000,0000,,Ôi
Comment: 0,0:15:18.88,0:15:21.71,Default,,0000,0000,0000,,Vâng thưa ngài!

Dialogue: 0,0:00:06.94,0:00:13.90,Note,,0000,0000,0000,,{\pos(352,366)\fad(500,1000)}ODAlBA, TOKYO
Dialogue: 0,0:02:03.99,0:02:06.95,Note,,0000,0000,0000,,{\pos(352,366)\fad(500,1000)}GlNZA, TOKYO
Dialogue: 0,0:02:10.63,0:02:12.62,Default,,0000,0000,0000,,Ở Odaiba xuất hiện một Quái vật Tan biến.
Dialogue: 0,0:02:14.90,0:02:18.33,Default,,0000,0000,0000,,Theo tín hiệu nhiệt, thì đó là một Siêu Quái vật Tan biến.
Dialogue: 0,0:02:18.43,0:02:21.30,Default,,0000,0000,0000,,Cái gì? Thông tin chính xác chứ?
Dialogue: 0,0:02:21.37,0:02:27.10,Default,,0000,0000,0000,,Vâng, có 98.25% khả năng đó là một Siêu Quái vật Tan biến.
Dialogue: 0,0:02:27.21,0:02:31.04,Default,,0000,0000,0000,,Có máy theo dõi ở đó, hãy chuyển sang màn hình chính.
Dialogue: 0,0:02:33.48,0:02:37.65,Default,,0000,0000,0000,,Siêu Quái vật Tan biến xuất hiện bất ngờ thật sự là tai hoạ.
Dialogue: 0,0:02:37.72,0:02:41.28,Default,,0000,0000,0000,,lsvelga-kun, phái đơn vị Zeigfried đi ngay lập tức.
Dialogue: 0,0:02:41.39,0:02:45.59,Default,,0000,0000,0000,,Thưa chỉ huy, chỉ có đơn vị 1 là có thể điều động.
Dialogue: 0,0:02:45.66,0:02:49.76,Default,,0000,0000,0000,,Các phi công khác hiện giờ đều không có ở sở chỉ huy.
Dialogue: 0,0:02:49.83,0:02:51.46,Default,,0000,0000,0000,,Ta thật ngu ngốc.
Dialogue: 0,0:02:51.53,0:02:54.33,Default,,0000,0000,0000,,Hãy ra lệnh các phi công trở lại căn cứ ngay.
Dialogue: 0,0:02:54.40,0:02:56.67,Default,,0000,0000,0000,,Rõ. Riska...
Dialogue: 0,0:02:56.77,0:02:59.37,Default,,0000,0000,0000,,...những ai có thể trở về đây sớm nhất?
Dialogue: 0,0:02:59.44,0:03:05.14,Default,,0000,0000,0000,,Các phi công ở gần đây nhất là Mido-san và Matsui-san.
Dialogue: 0,0:03:05.25,0:03:08.41,Default,,0000,0000,0000,,Đơn vị 5 và đơn vị 6. Còn những người khác?
Dialogue: 0,0:03:08.52,0:03:11.85,Default,,0000,0000,0000,,Các phi công khác đều không ở Tokyo.
Dialogue: 0,0:03:11.95,0:03:15.56,Note,,0000,0000,0000,,{\pos(352,366)\fad(0,1000)}QUẬN KANAGAWA
Dialogue: 0,0:03:20.43,0:03:22.76,Default,,0000,0000,0000,,Cảm giác thật là tuyệt.
Dialogue: 0,0:03:22.83,0:03:24.92,Default,,0000,0000,0000,,Ừ, phải đó.
Dialogue: 0,0:03:28.80,0:03:32.36,Default,,0000,0000,0000,,Chúng không xuất hiện trong hai tháng làm ta lơ là.
Dialogue: 0,0:03:32.44,0:03:33.57,Default,,0000,0000,0000,,Hừ.
Dialogue: 0,0:03:33.68,0:03:37.20,Default,,0000,0000,0000,,Gori-kun, cử tất cả các đơn vị đánh trả ngay lập tức.
Dialogue: 0,0:03:37.31,0:03:39.11,Default,,0000,0000,0000,,Vâng thưa ngài.
Dialogue: 0,0:03:39.21,0:03:40.94,Default,,0000,0000,0000,,Có một thông điệp...
Dialogue: 0,0:03:41.05,0:03:42.14,Default,,0000,0000,0000,,...từ cảng Yokohama,
Dialogue: 0,0:03:42.25,0:03:45.52,Default,,0000,0000,0000,,...một chiến hạm và hai tàu hộ tống đang đánh trả.
Dialogue: 0,0:03:52.06,0:03:56.00,Note,,0000,0000,0000,,{\pos(352,366)\fad(500,1000)}NHÀ GA KlCHlJOJl, TOKYO
Dialogue: 0,0:03:56.30,0:03:59.97,Note,,0000,0000,0000,,{\pos(352,366)\fad(500,1000)}KHU VỰC BUÔN BÁN
Dialogue: 0,0:04:07.18,0:04:11.48,Default,,0000,0000,0000,,Đã một năm rồi, chúng ta mới lại được ở bên nhau.
Dialogue: 0,0:04:11.58,0:04:13.84,Default,,0000,0000,0000,,Ừ, đúng vậy.
Dialogue: 0,0:04:42.58,0:04:46.14,Default,,0000,0000,0000,,Đừng, chúng ta không nên làm thế này.
Dialogue: 0,0:04:47.15,0:04:50.74,Default,,0000,0000,0000,,Mới bắt đầu thôi, Hikari, vợ chưa cưới của anh.
Dialogue: 0,0:04:56.52,0:04:59.89,Default,,0000,0000,0000,,Nhưng mà.... làm thế này xấu hổ lắm.
Dialogue: 0,0:05:01.96,0:05:07.56,Default,,0000,0000,0000,,Ở sở chỉ huy DAKT, chúng ta đâu có cơ hội để làm chuyện này.
Dialogue: 0,0:05:11.34,0:05:12.33,Default,,0000,0000,0000,,Anh nói dối!
Dialogue: 0,0:05:12.44,0:05:16.67,Default,,0000,0000,0000,,Em đã biết chuyện của anh với Reiho-san.
Dialogue: 0,0:05:16.78,0:05:17.77,Default,,0000,0000,0000,,Gì?
Dialogue: 0,0:05:24.22,0:05:28.18,Default,,0000,0000,0000,,Đừng nói với em... anh đã làm chuyện đó... với Reiho-san.
Dialogue: 0,0:05:28.26,0:05:29.25,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:05:29.36,0:05:32.49,Default,,0000,0000,0000,,Đừng xấu hổ, em là người duy nhất của anh.
Dialogue: 0,0:05:35.56,0:05:37.36,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:05:44.67,0:05:46.00,Default,,0000,0000,0000,,Không, ở đó...
Dialogue: 0,0:05:50.48,0:05:52.97,Default,,0000,0000,0000,,Đừng, em ướt rồi.
Dialogue: 0,0:05:57.05,0:05:59.65,Default,,0000,0000,0000,,Cảm giác thật... Đừng, không.
Dialogue: 0,0:06:03.09,0:06:07.22,Default,,0000,0000,0000,,Đợi đã, để em đi tắm rửa trước đã.
Dialogue: 0,0:06:07.33,0:06:09.30,Default,,0000,0000,0000,,Anh hiểu rồi.
Dialogue: 0,0:06:09.40,0:06:11.52,Default,,0000,0000,0000,,Vậy, chúng ta tắm cùng nhau nhé.
Dialogue: 0,0:06:12.47,0:06:14.13,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:06:52.47,0:06:57.43,Default,,0000,0000,0000,,Không, đừng mà. Anh cứ làm thế... thì em sẽ...
Dialogue: 0,0:07:03.35,0:07:07.98,Default,,0000,0000,0000,,Hikari, anh cũng không kìm chế được. Ta làm nhé?
Dialogue: 0,0:07:08.06,0:07:11.79,Default,,0000,0000,0000,,Sao! Làm chuyện đó ở đây à?
Dialogue: 0,0:07:11.86,0:07:14.76,Default,,0000,0000,0000,,Không. Anh chịu hết nổi rồi, Hikari.
Dialogue: 0,0:07:14.83,0:07:17.86,Default,,0000,0000,0000,,Đừng, em xấu hổ lắm.
Dialogue: 0,0:07:17.97,0:07:19.69,Default,,0000,0000,0000,,Đừng làm thế.
Dialogue: 0,0:07:20.97,0:07:22.87,Default,,0000,0000,0000,,Đừng nhìn em như thế.
Dialogue: 0,0:07:22.94,0:07:26.50,Default,,0000,0000,0000,,Sao phải xấu hổ? Của em đẹp lắm.
Dialogue: 0,0:07:26.61,0:07:29.87,Default,,0000,0000,0000,,Không đâu, xấu hổ lắm.
Dialogue: 0,0:07:31.48,0:07:35.71,Default,,0000,0000,0000,,Không! Kyosuke. Đừng.
Dialogue: 0,0:07:35.82,0:07:38.75,Default,,0000,0000,0000,,Đừng, em... em không giữ được...
Dialogue: 0,0:07:46.46,0:07:50.30,Default,,0000,0000,0000,,Không, em không giữ được
Dialogue: 0,0:07:51.40,0:07:53.42,Default,,0000,0000,0000,,Em không...
Dialogue: 0,0:07:55.07,0:07:56.83,Default,,0000,0000,0000,,Anh làm đây, Hikari.
Dialogue: 0,0:07:56.90,0:08:00.07,Default,,0000,0000,0000,,Đây là lần đầu tiên, làm ơn nhẹ nhàng với em.
Dialogue: 0,0:08:03.21,0:08:05.01,Default,,0000,0000,0000,,Đau em! Đau em!
Dialogue: 0,0:08:07.05,0:08:09.52,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:08:09.58,0:08:13.38,Default,,0000,0000,0000,,Đau, đau lắm! Sâu quá!
Dialogue: 0,0:08:13.99,0:08:18.55,Default,,0000,0000,0000,,Không, đau em!
Dialogue: 0,0:08:20.29,0:08:22.63,Default,,0000,0000,0000,,Đau em! Em không chịu nổi nữa.
Dialogue: 0,0:08:28.10,0:08:30.54,Default,,0000,0000,0000,,Anh thật xấu xa.
Dialogue: 0,0:08:30.61,0:08:32.40,Default,,0000,0000,0000,,Đáng lẽ anh phải nhẹ nhàng hơn.
Dialogue: 0,0:08:32.51,0:08:33.70,Default,,0000,0000,0000,,Anh xin lỗi.
Dialogue: 0,0:08:33.77,0:08:35.47,Default,,0000,0000,0000,,Anh không kìm chế được.
Dialogue: 0,0:08:37.25,0:08:39.27,Default,,0000,0000,0000,,Lần sau anh sẽ làm tốt hơn.
Dialogue: 0,0:08:39.35,0:08:42.94,Default,,0000,0000,0000,,Lần sau đừng thô bạo với em.
Dialogue: 0,0:08:43.05,0:08:45.71,Default,,0000,0000,0000,,Anh hứa mà. Chúng ta lên gường đi.
Dialogue: 0,0:09:04.44,0:09:07.64,Default,,0000,0000,0000,,Xin hãy nhẹ nhàng với em.
Dialogue: 0,0:09:09.11,0:09:10.17,Default,,0000,0000,0000,,Nữa đi anh.
Dialogue: 0,0:09:29.33,0:09:31.06,Default,,0000,0000,0000,,Em ướt lắm rồi.
Dialogue: 0,0:09:49.72,0:09:52.21,Default,,0000,0000,0000,,Nữa đi, nữa đi.
Dialogue: 0,0:09:54.16,0:09:56.75,Default,,0000,0000,0000,,Em muốn lắm rồi.
Dialogue: 0,0:10:03.00,0:10:06.36,Default,,0000,0000,0000,,Em không giữ lâu hơn được nữa đâu!
Dialogue: 0,0:10:09.40,0:10:14.10,Default,,0000,0000,0000,,Làm đi anh, nhét nó vào trong đi, Kyosuke.
Dialogue: 0,0:10:15.51,0:10:16.74,Default,,0000,0000,0000,,Hơ?
Dialogue: 0,0:10:16.84,0:10:20.91,Default,,0000,0000,0000,,Sao vậy? Anh không muốn à, Kyosuke?
Dialogue: 0,0:10:21.02,0:10:26.35,Default,,0000,0000,0000,,Anh vừa mới ra lúc nãy, bây giờ nó vẫn chưa lên lại.
Dialogue: 0,0:10:36.73,0:10:39.26,Default,,0000,0000,0000,,Hikari, sao em...
Dialogue: 0,0:10:42.70,0:10:46.97,Default,,0000,0000,0000,,Em thấy trong mấy cuốn tạp chí khiêu dâm của anh đấy.
Dialogue: 0,0:10:47.04,0:10:50.74,Default,,0000,0000,0000,,Bú nó thế này sẽ làm nó cứng trở lại.
Dialogue: 0,0:11:06.66,0:11:08.53,Default,,0000,0000,0000,,Hikari.
Dialogue: 0,0:11:44.00,0:11:46.16,Default,,0000,0000,0000,,Để anh nhét nó vào.
Dialogue: 0,0:11:46.27,0:11:49.93,Default,,0000,0000,0000,,Vâng, làm đi anh, Kyosuke.
Dialogue: 0,0:11:53.27,0:11:54.67,Default,,0000,0000,0000,,Đ-Đau...
Dialogue: 0,0:11:55.88,0:11:57.78,Default,,0000,0000,0000,,Đau em!
Dialogue: 0,0:11:58.78,0:11:59.80,Default,,0000,0000,0000,,Em cảm thấy...
Dialogue: 0,0:11:59.91,0:12:04.18,Default,,0000,0000,0000,,...anh đang cọ xát bên trong em.
Dialogue: 0,0:12:10.06,0:12:13.32,Default,,0000,0000,0000,,Em cảm thấy nó chứ, Hikari?
Dialogue: 0,0:13:01.51,0:13:02.67,Default,,0000,0000,0000,,Anh sắp ra...
Dialogue: 0,0:13:03.81,0:13:05.14,Default,,0000,0000,0000,,Anh sắp ra rồi, Hikari.
Dialogue: 0,0:13:05.25,0:13:06.98,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:13:08.18,0:13:10.01,Default,,0000,0000,0000,,Kyosuke!
Dialogue: 0,0:13:13.29,0:13:14.52,Default,,0000,0000,0000,,Anh ra đây!
Dialogue: 0,0:13:32.64,0:13:35.07,Default,,0000,0000,0000,,Giờ thì em đã thuộc về anh.
Dialogue: 0,0:13:35.18,0:13:38.48,Default,,0000,0000,0000,,Tốt hơn là anh đừng có lăng nhăng đấy.
Dialogue: 0,0:13:38.58,0:13:43.04,Default,,0000,0000,0000,,Không bao giờ. Em là người duy nhất của đời anh.
Dialogue: 0,0:13:44.45,0:13:47.94,Default,,0000,0000,0000,,Anh đã làm tình với Reiho-san phải vậy không?
Dialogue: 0,0:13:48.06,0:13:50.42,Default,,0000,0000,0000,,Không. Anh chưa từng làm thế.
Dialogue: 0,0:13:50.52,0:13:53.93,Default,,0000,0000,0000,,Bất cứ khi nào anh muốn, hãy nói với em.
Dialogue: 0,0:13:53.99,0:13:56.33,Default,,0000,0000,0000,,Anh mà làm với người khác, em sẽ giết anh.
Dialogue: 0,0:13:56.43,0:14:01.23,Default,,0000,0000,0000,,Đừng lo mà, em là tình yêu duy nhất của anh.
Dialogue: 0,0:14:05.84,0:14:08.57,Default,,0000,0000,0000,,Hikari, muốn làm nữa không?
Dialogue: 0,0:14:08.64,0:14:09.80,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:14:32.50,0:14:33.86,Default,,0000,0000,0000,,Để em xem anh làm nó đi.
Dialogue: 0,0:14:33.97,0:14:36.06,Default,,0000,0000,0000,,Được.
Dialogue: 0,0:14:39.91,0:14:40.17,Default,,0000,0000,0000,,Tấn công!
Dialogue: 0,0:14:40.17,0:14:41.73,Default,,0000,0000,0000,,Tấn công!
Dialogue: 0,0:14:41.84,0:14:43.07,Default,,0000,0000,0000,,Cả hai đã bị bắt!
Dialogue: 0,0:14:47.15,0:14:49.27,Default,,0000,0000,0000,,Một Quái vật Tan biến đã xuất hiện.
Dialogue: 0,0:14:49.38,0:14:52.22,Default,,0000,0000,0000,,Cả hai người đang làm gì vậy.
Dialogue: 0,0:14:52.32,0:14:54.18,Default,,0000,0000,0000,,Hở?
Dialogue: 0,0:15:03.87,0:15:08.86,Note,,0000,0000,0000,,{\pos(352,366)\fad(500,1000)}CẦU VỒNG, TOKYO
Dialogue: 0,0:15:18.88,0:15:21.71,Default,,0000,0000,0000,,Câu giờ cho đến khi chúng tôi vào vị trí.
Dialogue: 0,0:15:48.54,0:15:51.24,Default,,0000,0000,0000,,Cứ thế này, chúng ta bị tiêu diệt mất.
Dialogue: 0,0:15:51.35,0:15:54.44,Default,,0000,0000,0000,,Kẻ thù dự định phá huỷ sở chỉ huy DAKT.
Dialogue: 0,0:15:54.55,0:15:56.41,Default,,0000,0000,0000,,Này.
Dialogue: 0,0:15:56.52,0:15:58.71,Default,,0000,0000,0000,,Đã tìm thấy Kyosuke và Hikari-chan.
Dialogue: 0,0:15:58.79,0:16:01.28,Default,,0000,0000,0000,,Họ sẽ ở đây sớm thôi.
Dialogue: 0,0:16:01.39,0:16:06.72,Default,,0000,0000,0000,,Lạ thật, cứ như thể bọn chúng biết phải tấn công lúc nào.
Dialogue: 0,0:16:07.73,0:16:09.75,Default,,0000,0000,0000,,Trong chúng ta có gián điệp sao?
Dialogue: 0,0:16:27.01,0:16:30.25,Default,,0000,0000,0000,,Thả tôi ra! Dừng lại đi.
Dialogue: 0,0:16:30.35,0:16:33.08,Default,,0000,0000,0000,,Chưa được đâu, Yukiko-san.
Dialogue: 0,0:16:33.15,0:16:36.45,Default,,0000,0000,0000,,Cô phải nói với tôi mọi thứ.
Dialogue: 0,0:16:36.56,0:16:39.29,Default,,0000,0000,0000,,Tôi không biết gì cả.
Dialogue: 0,0:16:39.36,0:16:43.69,Default,,0000,0000,0000,,Ông là đặc vụ Liên Hiệp Quốc mà. Tự đi mà tìm hiểu.
Dialogue: 0,0:16:43.80,0:16:48.63,Default,,0000,0000,0000,,Được thôi. Ta sẽ hỏi cơ thể cô.
Dialogue: 0,0:16:48.74,0:16:51.40,Default,,0000,0000,0000,,Làm tình kiểu bình thường hoài cũng chán nhỉ.
Dialogue: 0,0:16:51.47,0:16:56.57,Default,,0000,0000,0000,,Để ta cho cô thấy cảm giác của một nô lệ.
Dialogue: 0,0:16:56.68,0:17:00.01,Default,,0000,0000,0000,,Lúc đó cô sẽ nói với ta mọi thứ.
Dialogue: 0,0:17:00.11,0:17:03.71,Default,,0000,0000,0000,,Không, dừng lại đi! Thả tôi ra.
Dialogue: 0,0:17:03.82,0:17:05.31,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:17:06.29,0:17:07.84,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:17:09.19,0:17:12.02,Default,,0000,0000,0000,,Không! Không phải ở đó!
Dialogue: 0,0:17:13.13,0:17:14.79,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:17:20.10,0:17:21.86,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:17:23.67,0:17:27.61,Default,,0000,0000,0000,,Nhìn xem cái chất nhớt ẩm ướt của cô này.
Dialogue: 0,0:17:27.71,0:17:30.11,Default,,0000,0000,0000,,Không, đừng
Dialogue: 0,0:18:40.08,0:18:42.05,Default,,0000,0000,0000,,Chiến hạm Bảo hộ đã bị hạ.
Dialogue: 0,0:18:42.12,0:18:45.38,Default,,0000,0000,0000,,Thông địêp từ Lực lượng Trên bộ: đã bắt đầu phản công.
Dialogue: 0,0:18:45.49,0:18:48.55,Default,,0000,0000,0000,,Tệ thật. Chúng ta phải dừng chuyện này lại.
Dialogue: 0,0:18:48.66,0:18:52.18,Default,,0000,0000,0000,,Vũ khí thường không có tác dụng. Đơn vị Zeigfried đâu rồi?
Dialogue: 0,0:18:52.29,0:18:56.66,Default,,0000,0000,0000,,Đơn vị 5 và 6 sẽ sớm tham dự vào chiến trường.
Dialogue: 0,0:19:22.49,0:19:26.79,Default,,0000,0000,0000,,Ta sẽ tấn công ngay khi các đơn vị Zeigfried còn lại đến.
Dialogue: 0,0:19:26.89,0:19:28.76,Default,,0000,0000,0000,,Ra lệnh rút lui tạm thời.
Dialogue: 0,0:19:28.83,0:19:29.85,Default,,0000,0000,0000,,Rõ!
Dialogue: 0,0:19:30.63,0:19:32.36,Default,,0000,0000,0000,,Chúng tôi tới rồi!
Dialogue: 0,0:19:32.47,0:19:33.99,Default,,0000,0000,0000,,Các người trễ qua đấy!
Dialogue: 0,0:19:34.10,0:19:36.30,Default,,0000,0000,0000,,Xin lỗi, có một số chuyện.
Dialogue: 0,0:19:36.40,0:19:39.13,Default,,0000,0000,0000,,Tôi chắc là các cậu đang chơi bời.
Dialogue: 0,0:19:39.24,0:19:40.23,Default,,0000,0000,0000,,Sao?
Dialogue: 0,0:19:40.34,0:19:44.40,Default,,0000,0000,0000,,Cậu có thể chơi sau khi chúng ta hạ tên này.
Dialogue: 0,0:19:44.51,0:19:48.11,Default,,0000,0000,0000,,Đã rõ. Hikari, chúng ta sẽ tiếp tục sau nhé.
Dialogue: 0,0:19:48.22,0:19:49.68,Default,,0000,0000,0000,,Thôi nào.
Dialogue: 0,0:19:54.76,0:19:56.19,Default,,0000,0000,0000,,Tấn công!
Dialogue: 0,0:19:57.79,0:19:59.45,Default,,0000,0000,0000,,Rõ!
Dialogue: 0,0:19:57.79,0:19:59.45,Default,,0000,0000,0000,,Rõ!
