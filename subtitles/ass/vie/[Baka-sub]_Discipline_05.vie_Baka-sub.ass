Dialogue: 0,0:01:49.34,0:01:50.36,Default,,0000,0000,0000,,Cậu ướt rồi này...
Dialogue: 0,0:01:50.94,0:01:51.67,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:02:22.10,0:02:22.90,Default,,0000,0000,0000,,Ôi không!
Dialogue: 0,0:02:32.81,0:02:34.94,Default,,0000,0000,0000,,Ôi không, nhanh quá!
Dialogue: 0,0:02:35.60,0:02:37.24,Default,,0000,0000,0000,,Tôi xin lỗi!
Dialogue: 0,0:02:38.15,0:02:40.55,Default,,0000,0000,0000,,Đừng xin lỗi, mình làm lại đi.
Dialogue: 0,0:03:08.62,0:03:09.88,Default,,0000,0000,0000,,Cậu thật xấu xa.
Dialogue: 0,0:03:11.29,0:03:13.12,Default,,0000,0000,0000,,Tôi thật... xin lỗi!
Dialogue: 0,0:03:13.69,0:03:15.36,Default,,0000,0000,0000,,Nè, cậu làm sao vậy?
Dialogue: 0,0:03:15.52,0:03:17.35,Default,,0000,0000,0000,,Thường đâu có như thế...
Dialogue: 0,0:03:17.53,0:03:19.12,Default,,0000,0000,0000,,Tớ hơi thất vọng một chút.
Dialogue: 0,0:03:38.18,0:03:40.67,Default,,0000,0000,0000,,Thật sự là... có chuyện gì vậy?
Dialogue: 0,0:03:41.22,0:03:44.18,Default,,0000,0000,0000,,Hình như tôi trở nên nhạy cảm quá rồi.
Dialogue: 0,0:04:09.88,0:04:11.44,Default,,0000,0000,0000,,Xin chào, mọi người.
Dialogue: 0,0:04:11.91,0:04:18.68,Default,,0000,0000,0000,,Xin gởi lời chào nồng nhiệt nhất đến các khách hàng của tập đoàn Morimoto.
Dialogue: 0,0:04:19.42,0:04:20.18,Default,,0000,0000,0000,,Được rồi.
Dialogue: 0,0:04:41.21,0:04:43.47,Default,,0000,0000,0000,,Các cô ở đây chắc mọi người cũng đã quen thuộc.
Dialogue: 0,0:04:43.95,0:04:51.01,Default,,0000,0000,0000,,Họ được đào tạo để phù hợp với sở thích của từng người trong số các bạn.
Dialogue: 0,0:04:52.72,0:04:54.12,Default,,0000,0000,0000,,Xin mời thưởng thức.
Dialogue: 0,0:05:35.56,0:05:39.16,Default,,0000,0000,0000,,Tôi muốn các cô nhìn kỹ từng người ở đây.
Dialogue: 0,0:05:40.14,0:05:42.06,Default,,0000,0000,0000,,Tất cả các cô sẽ có cơ hội...
Dialogue: 0,0:05:42.31,0:05:46.17,Default,,0000,0000,0000,,...được vinh dự phục vụ một trong các quý ông quyền lực đó.
Dialogue: 0,0:05:47.41,0:05:49.60,Default,,0000,0000,0000,,Chỉ những ai hứa trung thành với Morimoto...
Dialogue: 0,0:05:50.09,0:05:53.90,Default,,0000,0000,0000,,...và mài giũa cơ thể mình lên đến đỉnh điểm...
Dialogue: 0,0:05:54.95,0:05:57.65,Default,,0000,0000,0000,,...sẽ có vinh dự tham gia một bữa tiệc thế này!
Dialogue: 0,0:05:58.02,0:05:58.71,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:06:01.62,0:06:03.15,Default,,0000,0000,0000,,Cuối cùng cũng có được nó!
Dialogue: 0,0:06:03.53,0:06:06.69,Default,,0000,0000,0000,,Giờ mình chỉ cần xem qua danh sách những cái tên.
Dialogue: 0,0:06:07.86,0:06:09.59,Default,,0000,0000,0000,,À, gửi nó đi trước đã.
Dialogue: 0,0:06:16.04,0:06:17.44,Default,,0000,0000,0000,,Không có tín hiệu?!
Dialogue: 0,0:06:17.87,0:06:21.96,Default,,0000,0000,0000,,Gì nữa đây? Hừ? Tệ thật!
Dialogue: 0,0:06:22.54,0:06:25.48,Default,,0000,0000,0000,,Sao có thể thế chứ? Chẳng còn cách nào khác...
Dialogue: 0,0:06:29.08,0:06:32.38,Default,,0000,0000,0000,,Máng đổ rác!
Dialogue: 0,0:06:47.07,0:06:48.97,Default,,0000,0000,0000,,Morimoto, hãy coi chừng.
Dialogue: 0,0:07:21.60,0:07:22.87,Default,,0000,0000,0000,,An toàn rồi.
Dialogue: 0,0:07:23.61,0:07:25.40,Default,,0000,0000,0000,,Này cô. Sao vậy?
Dialogue: 0,0:07:25.77,0:07:28.30,Default,,0000,0000,0000,,À, chỉ là... tôi bị lạc đường.
Dialogue: 0,0:07:28.84,0:07:33.97,Default,,0000,0000,0000,,Vậy à! Cô là một trong các cô gái của Leona-ojousama phải không?
Dialogue: 0,0:07:34.28,0:07:37.12,Default,,0000,0000,0000,,Vâng! Đúng vậy! Thật là ngượng quá.
Dialogue: 0,0:07:37.45,0:07:40.48,Default,,0000,0000,0000,,Tôi có ý này! Sao mình không vui vẻ một lát?
Dialogue: 0,0:07:40.96,0:07:43.46,Default,,0000,0000,0000,,Những người mới chẳng bao giờ xuống đến đây.
Dialogue: 0,0:07:49.70,0:07:50.49,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:08:04.98,0:08:06.19,Default,,0000,0000,0000,,Nào, mọi người.
Dialogue: 0,0:08:06.85,0:08:12.08,Default,,0000,0000,0000,,Hôm nay, người anh hùng khiến chúng ta tự hào, Araki Madoka-san, sẽ báo cáo.
Dialogue: 0,0:08:17.16,0:08:18.25,Default,,0000,0000,0000,,Hỡi các đồng chí!
Dialogue: 0,0:08:18.66,0:08:22.60,Default,,0000,0000,0000,,Tôi, Araki Madoka, đã trở về an toàn...
Dialogue: 0,0:08:23.13,0:08:25.08,Default,,0000,0000,0000,,...sau khi do thám lâu đài nhà Morimoto.
Dialogue: 0,0:08:28.67,0:08:30.22,Default,,0000,0000,0000,,Đúng như những thông tin ban đầu...
Dialogue: 0,0:08:30.58,0:08:33.80,Default,,0000,0000,0000,,...nhà Morimoto cung cấp dịch vụ harem...
Dialogue: 0,0:08:34.01,0:08:38.17,Default,,0000,0000,0000,,...cho người trong giới chính trị và tài chính xã hội.
Dialogue: 0,0:08:38.48,0:08:41.45,Default,,0000,0000,0000,,Tôi đã thấy tận mắt học sinh trường chúng ta làm việc ở đó!
Dialogue: 0,0:08:43.48,0:08:48.29,Default,,0000,0000,0000,,Tiếc là ở đó tối quá, nên tôi không thấy được chi tiết việc đang diễn ra.
Dialogue: 0,0:08:50.66,0:08:56.96,Default,,0000,0000,0000,,Tuy nhiên, thật sự là chúng ta đang ở trong một tình thế khó khăn.
Dialogue: 0,0:08:57.04,0:09:03.64,Default,,0000,0000,0000,,Dị giáo kinh tởm nhà Morimoto\N đang nắm được điểm yếu của chúng ta...
Dialogue: 0,0:09:03.91,0:09:08.34,Default,,0000,0000,0000,,...cả về mặt xã hội lẫn tài chính, và lấy đó làm lợi thế.
Dialogue: 0,0:09:10.04,0:09:14.00,Default,,0000,0000,0000,,Tôi cũng đã khám phá ra danh sách học sinh bị hăm dọa.
Dialogue: 0,0:09:14.88,0:09:18.25,Default,,0000,0000,0000,,Danh sách bí mật này được giữ kín, trong đó chứa...
Dialogue: 0,0:09:18.89,0:09:22.75,Default,,0000,0000,0000,,...đặc điểm cơ thể và tinh thần của những người bạn yêu quý.
Dialogue: 0,0:09:23.22,0:09:25.28,Default,,0000,0000,0000,,Và tôi đã biết được vị trí cất giấu nó.
Dialogue: 0,0:09:26.96,0:09:33.13,Default,,0000,0000,0000,,Việc trước mắt, là phải tịch thu\N và công khai cái danh sách kinh hoàng đó.
Dialogue: 0,0:09:33.84,0:09:38.47,Default,,0000,0000,0000,,Vì vậy, tôi muốn lập một đội và tìm một kế để lấy được nó.
Dialogue: 0,0:09:40.78,0:09:41.87,Default,,0000,0000,0000,,Ai tình nguyện nào?
Dialogue: 0,0:09:43.28,0:09:44.47,Default,,0000,0000,0000,,Có ai không?
Dialogue: 0,0:09:50.89,0:09:52.95,Default,,0000,0000,0000,,Thật tình, may mà hôm nay là cuối tuần.
Dialogue: 0,0:09:53.17,0:09:55.29,Default,,0000,0000,0000,,Không có ai trong hội đến đây.
Dialogue: 0,0:09:58.19,0:09:59.52,Default,,0000,0000,0000,,Đừng lo lắng gì cả...
Dialogue: 0,0:09:59.73,0:10:02.82,Default,,0000,0000,0000,,...tớ sẽ nói với mọi người là cậu đã học cả đêm.
Dialogue: 0,0:10:04.03,0:10:04.66,Default,,0000,0000,0000,,Ừ.
Dialogue: 0,0:10:05.23,0:10:08.13,Default,,0000,0000,0000,,May mắn là có nhiều giấy vệ sinh ở đây.
Dialogue: 0,0:10:08.64,0:10:10.23,Default,,0000,0000,0000,,Không thì chả biết làm sao!
Dialogue: 0,0:10:11.37,0:10:13.19,Default,,0000,0000,0000,,Xin lỗi cậu... về tất cả.
Dialogue: 0,0:10:13.78,0:10:15.83,Default,,0000,0000,0000,,Không sao. Nhanh bình ổn lại đi nhé.
Dialogue: 0,0:10:21.52,0:10:22.98,Default,,0000,0000,0000,,Ôi không!
Dialogue: 0,0:10:29.09,0:10:31.22,Default,,0000,0000,0000,,Cậu có chắc gượng ép như thế là ý hay?
Dialogue: 0,0:10:32.46,0:10:33.95,Default,,0000,0000,0000,,Chẳng có gì gượng ép!
Dialogue: 0,0:10:34.13,0:10:35.99,Default,,0000,0000,0000,,Việc chuẩn bị đang diễn ra. Sau đó...
Dialogue: 0,0:10:37.93,0:10:40.40,Default,,0000,0000,0000,,Takigawa-kun? Vậy là sao?
Dialogue: 0,0:10:41.17,0:10:42.53,Default,,0000,0000,0000,,X-Xin chào.
Dialogue: 0,0:10:44.47,0:10:47.39,Default,,0000,0000,0000,,Cậu nói là... cậu đang bắt đầu một cuộc cách mạng?
Dialogue: 0,0:10:48.08,0:10:51.81,Default,,0000,0000,0000,,Một cuộc cách mạng nhằm lập lại công lý.
Dialogue: 0,0:10:51.98,0:10:53.27,Default,,0000,0000,0000,,Chúng ta sẽ đánh bại quỷ dữ.
Dialogue: 0,0:10:55.45,0:10:59.68,Default,,0000,0000,0000,,Hujiwara và Endou cũng tham gia nhóm này à?
Dialogue: 0,0:11:00.32,0:11:02.56,Default,,0000,0000,0000,,Ừ! Vấn đề là...
Dialogue: 0,0:11:02.67,0:11:07.02,Default,,0000,0000,0000,,...chúng ta luôn theo đuổi\N những vụ không thể giải quyết...
Dialogue: 0,0:11:07.36,0:11:10.99,Default,,0000,0000,0000,,Nhưng lần này, có cảm giác là sẽ thành công!
Dialogue: 0,0:11:11.33,0:11:12.39,Default,,0000,0000,0000,,Tớ cũng nghĩ thế!
Dialogue: 0,0:11:13.33,0:11:15.77,Default,,0000,0000,0000,,Vậy cho nên, Otokawa-san tham gia với bọn tớ đi!
Dialogue: 0,0:11:16.20,0:11:19.54,Default,,0000,0000,0000,,Chúng ta sẽ không gặp được ma cà rồng hay gì đó...
Dialogue: 0,0:11:19.67,0:11:22.61,Default,,0000,0000,0000,,...nhưng vụ này liên quan đến cuộc sống hằng ngày!
Dialogue: 0,0:11:24.01,0:11:24.91,Default,,0000,0000,0000,,Otokawa-san.
Dialogue: 0,0:11:25.51,0:11:30.42,Default,,0000,0000,0000,,Tôi đã từng lần thi rớt trong một kỳ thi.
Dialogue: 0,0:11:33.65,0:11:34.97,Default,,0000,0000,0000,,Tôi chưa từng biết rằng...
Dialogue: 0,0:11:35.03,0:11:39.58,Default,,0000,0000,0000,,...lớp phụ đạo cho học sinh rớt lại khủng khiếp đến thế.
Dialogue: 0,0:11:40.96,0:11:42.73,Default,,0000,0000,0000,,Họ không cần sự hối cải của chúng ta.
Dialogue: 0,0:11:43.14,0:11:45.09,Default,,0000,0000,0000,,Để học về quy luật của cơ thể...
Dialogue: 0,0:11:46.37,0:11:47.86,Default,,0000,0000,0000,,...họ bắt tôi làm nhiều thứ...
Dialogue: 0,0:11:49.84,0:11:51.83,Default,,0000,0000,0000,,...và nhét vào đó nhiều thứ...
Dialogue: 0,0:11:53.91,0:11:55.90,Default,,0000,0000,0000,,...và khiến nhiều thứ bắn ra.
Dialogue: 0,0:11:59.85,0:12:01.13,Default,,0000,0000,0000,,Ngay lúc đó, tớ quyết định...
Dialogue: 0,0:12:01.78,0:12:04.55,Default,,0000,0000,0000,,...bi kịch này phải được ai đó chấm dứt.
Dialogue: 0,0:12:06.22,0:12:10.25,Default,,0000,0000,0000,,Ngay hôm qua, khi điều tra lâu đài nhà Morimoto...
Dialogue: 0,0:12:12.73,0:12:16.56,Default,,0000,0000,0000,,...tớ đã mắc sai lầm và bị mấy gã đàn ông bắt được.
Dialogue: 0,0:12:18.37,0:12:19.13,Default,,0000,0000,0000,,Sau đó...
Dialogue: 0,0:12:29.98,0:12:31.13,Default,,0000,0000,0000,,Đến mười phát!
Dialogue: 0,0:12:27.24,0:12:29.79,Default,,0000,0000,0000,,Tớ đã phải nhận đến mười phát từ họ!
Dialogue: 0,0:12:31.75,0:12:33.67,Default,,0000,0000,0000,,Học cách kiềm chế chút đi!
Dialogue: 0,0:12:34.75,0:12:40.85,Default,,0000,0000,0000,,Nhưng, tớ không thua! Tớ đã thề...\N chắc chắn sẽ đẩy họ xuống địa ngục.
Dialogue: 0,0:12:41.29,0:12:46.99,Default,,0000,0000,0000,,Do vậy tớ cần cậu và sức mạnh của những người ở ký túc xá!
Dialogue: 0,0:12:47.99,0:12:50.97,Default,,0000,0000,0000,,Làm ơn! Vì lợi ích của tất cả mọi người.
Dialogue: 0,0:12:51.77,0:12:54.00,Default,,0000,0000,0000,,Cho dù cậu có nói vậy thì...
Dialogue: 0,0:13:00.41,0:13:05.37,Default,,0000,0000,0000,,Tớ biết các cậu có quan hệ\N không tốt với nhóm của Leona.
Dialogue: 0,0:13:13.86,0:13:14.72,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:13:15.46,0:13:18.54,Default,,0000,0000,0000,,Nếu chiến đấu thật sự, chúng ta có thể thay đổi tương lai.
Dialogue: 0,0:13:19.29,0:13:20.16,Default,,0000,0000,0000,,Otokawa-san!
Dialogue: 0,0:13:21.63,0:13:24.39,Default,,0000,0000,0000,,Có một chuyện làm tớ lo lắng.
Dialogue: 0,0:13:29.14,0:13:30.18,Default,,0000,0000,0000,,Cậu biết không...
Dialogue: 0,0:13:32.41,0:13:33.70,Default,,0000,0000,0000,,Sao? Nó rỉ ra à?
Dialogue: 0,0:13:33.81,0:13:35.47,Default,,0000,0000,0000,,Suỵt! Đừng nói gì!
Dialogue: 0,0:13:37.40,0:13:40.64,Default,,0000,0000,0000,,Nè, tôi nghe đồn về khả năng thần kỳ của cậu...
Dialogue: 0,0:13:41.10,0:13:42.47,Default,,0000,0000,0000,,Xem ra đó là sự thật.
Dialogue: 0,0:13:42.82,0:13:43.86,Default,,0000,0000,0000,,Làm ơn thôi đi mà.
Dialogue: 0,0:13:44.42,0:13:47.95,Default,,0000,0000,0000,,Nè, có phải lần nào cũng sướng không?
Dialogue: 0,0:13:48.52,0:13:50.58,Default,,0000,0000,0000,,Tha cho tôi đi, Momoe-chan!
Dialogue: 0,0:13:51.13,0:13:55.76,Default,,0000,0000,0000,,Hayami-kun, vì cậu như thế mà Morimoto Leona lấn tới.
Dialogue: 0,0:13:55.93,0:13:57.29,Default,,0000,0000,0000,,Đứng đắn một chút đi!
Dialogue: 0,0:13:57.90,0:14:00.23,Default,,0000,0000,0000,,Đứng đắn là sao chứ?
Dialogue: 0,0:14:00.64,0:14:04.26,Default,,0000,0000,0000,,Nè, sao cậu không để Kawahara-sensei, xem qua?
Dialogue: 0,0:14:04.87,0:14:06.17,Default,,0000,0000,0000,,Kawahara-sensei?
Dialogue: 0,0:14:06.84,0:14:10.30,Default,,0000,0000,0000,,Phải, có khi cô ấy biết một cách chữa thần kỳ nào đó...
Dialogue: 0,0:14:10.68,0:14:13.72,Default,,0000,0000,0000,,...mà hơn nữa, Kawahara-sensei là cố vấn của chúng ta.
Dialogue: 0,0:14:14.52,0:14:15.24,Default,,0000,0000,0000,,Ồ, phải.
Dialogue: 0,0:14:15.85,0:14:17.28,Default,,0000,0000,0000,,Nhắc đến Kawahara-sensei...
Dialogue: 0,0:14:18.19,0:14:20.59,Default,,0000,0000,0000,,...tớ nghe đồn cô ấy đang chống đối lại ủy ban trường.
Dialogue: 0,0:14:20.72,0:14:23.02,Default,,0000,0000,0000,,Đó chính là người chúng ta cần.
Dialogue: 0,0:14:23.79,0:14:24.59,Default,,0000,0000,0000,,Tớ hiểu rồi.
Dialogue: 0,0:14:25.39,0:14:30.63,Default,,0000,0000,0000,,Chúng ta đừng làm thế. Tớ có cảm giác không hay.
Dialogue: 0,0:14:32.20,0:14:33.46,Default,,0000,0000,0000,,Cậu nói gì thế?
Dialogue: 0,0:14:33.60,0:14:35.00,Default,,0000,0000,0000,,Tôi nghiêm túc đấy.
Dialogue: 0,0:14:35.26,0:14:38.04,Default,,0000,0000,0000,,Con thú bên trong tôi rất cảnh giác.
Dialogue: 0,0:14:38.44,0:14:40.74,Default,,0000,0000,0000,,Con thú đó chính là vấn đề đấy.
Dialogue: 0,0:14:47.21,0:14:49.55,Default,,0000,0000,0000,,Không có sự kích động khác thường nào.
Dialogue: 0,0:14:50.89,0:14:53.29,Default,,0000,0000,0000,,À, mọi thứ đều ổn.
Dialogue: 0,0:14:55.22,0:14:57.25,Default,,0000,0000,0000,,Dù sao thì, chúng ta nên thử nghiệm chính xác hơn.
Dialogue: 0,0:14:57.73,0:15:00.72,Default,,0000,0000,0000,,Với dụng cụ trong trường, cô có thể kiểm tra vài thứ.
Dialogue: 0,0:15:04.90,0:15:05.96,Default,,0000,0000,0000,,Có vấn đề gì không?
Dialogue: 0,0:15:06.20,0:15:07.76,Default,,0000,0000,0000,,À, không ạ.
Dialogue: 0,0:15:08.04,0:15:11.16,Default,,0000,0000,0000,,Sensei, cô có thể giữ bí mật được không?
Dialogue: 0,0:15:12.04,0:15:13.37,Default,,0000,0000,0000,,Cô hiểu rồi.
Dialogue: 0,0:15:14.54,0:15:17.08,Default,,0000,0000,0000,,Em rất thành thật, Otokawa-san.
Dialogue: 0,0:15:21.58,0:15:23.38,Default,,0000,0000,0000,,À, có lẽ em phải đi bây giờ.
Dialogue: 0,0:15:24.02,0:15:25.78,Default,,0000,0000,0000,,Vậy, chào mọi người giúp cô nhé.
Dialogue: 0,0:15:26.05,0:15:26.75,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:15:31.93,0:15:33.42,Default,,0000,0000,0000,,Vậy, thế nào?
Dialogue: 0,0:15:33.86,0:15:36.33,Default,,0000,0000,0000,,Em muốn uống thuốc? Châm cứu? Hay cắt ống dẫn tinh?
Dialogue: 0,0:15:36.53,0:15:38.31,Default,,0000,0000,0000,,Ngày nay có rất nhiều sự lựa chọn.
Dialogue: 0,0:15:39.27,0:15:41.52,Default,,0000,0000,0000,,Ừm, cứ xét nghiệm trước đã.
Dialogue: 0,0:15:43.77,0:15:45.90,Default,,0000,0000,0000,,Vây cô sẽ cho em vài viên thuốc ngủ.
Dialogue: 0,0:15:46.27,0:15:48.64,Default,,0000,0000,0000,,Em cứ ngủ lại đây đến khi mọi thứ sẵn sàng.
Dialogue: 0,0:15:49.08,0:15:49.91,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:15:58.92,0:16:01.29,Default,,0000,0000,0000,,Vậy là cậu muốn trộm cái danh sách đó?
Dialogue: 0,0:16:02.92,0:16:06.09,Default,,0000,0000,0000,,Phải, cái danh sách nắm giữ điểm yếu của chúng ta.
Dialogue: 0,0:16:06.36,0:16:07.92,Default,,0000,0000,0000,,Các cậu nên hiểu tầm quan trọng của nó.
Dialogue: 0,0:16:08.90,0:16:09.86,Default,,0000,0000,0000,,Thật sao?
Dialogue: 0,0:16:10.13,0:16:12.93,Default,,0000,0000,0000,,Vậy, những thông tin cần thiết đã có?
Dialogue: 0,0:16:13.30,0:16:17.96,Default,,0000,0000,0000,,Phải! Ngay lúc này, những người khác đang lo việc chuẩn bị!
Dialogue: 0,0:16:18.27,0:16:20.56,Default,,0000,0000,0000,,Chỉ cần có thêm một nhóm để tiến hành kế hoạch.
Dialogue: 0,0:16:21.31,0:16:24.00,Default,,0000,0000,0000,,Chúng ta đã trải qua nhiều chuyện kinh khủng.
Dialogue: 0,0:16:24.65,0:16:26.17,Default,,0000,0000,0000,,Giờ không phải lúc để nghĩ!
Dialogue: 0,0:16:26.78,0:16:30.40,Default,,0000,0000,0000,,Không có nhiều cơ hội đánh bại Hội Thân hữu!
Dialogue: 0,0:16:30.99,0:16:32.58,Default,,0000,0000,0000,,Chúng tôi vừa thất bại hoàn toàn!
Dialogue: 0,0:16:32.95,0:16:34.82,Default,,0000,0000,0000,,Vậy mà ngay khi sắp quên được nó...
Dialogue: 0,0:16:35.32,0:16:37.62,Default,,0000,0000,0000,,Tốt hơn là không nên nhắc lại những chuyện đó.
Dialogue: 0,0:16:37.96,0:16:39.26,Default,,0000,0000,0000,,X-Xin lỗi!
Dialogue: 0,0:16:39.73,0:16:42.82,Default,,0000,0000,0000,,Vậy, các cậu vẫn tham gia phải không?
Dialogue: 0,0:16:43.46,0:16:43.96,Default,,0000,0000,0000,,Tất nhiên.
Dialogue: 0,0:16:45.50,0:16:46.25,Default,,0000,0000,0000,,Được thôi!
Dialogue: 0,0:16:48.67,0:16:50.30,Default,,0000,0000,0000,,Nào, mọi người tập trung lại!
Dialogue: 0,0:16:50.81,0:16:51.64,Default,,0000,0000,0000,,Ừ!
Dialogue: 0,0:16:55.08,0:16:55.80,Default,,0000,0000,0000,,Được.
Dialogue: 0,0:16:55.88,0:16:57.74,Default,,0000,0000,0000,,Mà này, Ta-kun đâu?
Dialogue: 0,0:16:57.95,0:16:59.89,Default,,0000,0000,0000,,Phải, có chuyện gì với Ta-kun vậy?
Dialogue: 0,0:17:00.58,0:17:02.67,Default,,0000,0000,0000,,Cậu ấy có nhiều việc phải làm.
Dialogue: 0,0:17:02.76,0:17:05.54,Default,,0000,0000,0000,,Tớ chắc là cậu ấy sẽ bận cả đêm.
Dialogue: 0,0:17:05.85,0:17:08.48,Default,,0000,0000,0000,,À, phải, phải. Cậu ấy đang giúp bọn tớ.
Dialogue: 0,0:17:16.20,0:17:17.49,Default,,0000,0000,0000,,Hơ?! Sao lại...?
Dialogue: 0,0:17:19.10,0:17:21.99,Default,,0000,0000,0000,,Sensei? Chuyện gì vậy?
Dialogue: 0,0:17:22.94,0:17:24.21,Default,,0000,0000,0000,,Xin lỗi, Hayami-kun.
Dialogue: 0,0:17:26.11,0:17:26.75,Default,,0000,0000,0000,,Sensei.
Dialogue: 0,0:17:27.37,0:17:29.57,Default,,0000,0000,0000,,Cậu đã nghe về tôi phải không?
Dialogue: 0,0:17:30.01,0:17:32.10,Default,,0000,0000,0000,,Việc trường lớp diễn ra không được tốt lắm.
Dialogue: 0,0:17:33.11,0:17:33.82,Default,,0000,0000,0000,,Lý do...
Dialogue: 0,0:17:34.25,0:17:39.25,Default,,0000,0000,0000,,...tôi tốt nghiệp trường này, tôi biết rất rõ.
Dialogue: 0,0:17:40.49,0:17:44.65,Default,,0000,0000,0000,,Ngôi trường này, à không, tập đoàn Morimoto, thật sự tàn nhẫn.
Dialogue: 0,0:17:45.03,0:17:46.79,Default,,0000,0000,0000,,Tôi cũng đã liều lĩnh chống lại.
Dialogue: 0,0:17:47.39,0:17:49.65,Default,,0000,0000,0000,,Giờ tôi vẫn nghĩ đến điều đó.
Dialogue: 0,0:17:50.33,0:17:52.16,Default,,0000,0000,0000,,Nhưng mà, tôi không thể làm gì cả.
Dialogue: 0,0:17:52.66,0:17:56.46,Default,,0000,0000,0000,,Cơ thể tôi đã được huấn luyện để không thể kìm nén.
Dialogue: 0,0:18:03.54,0:18:04.50,Default,,0000,0000,0000,,Mình biết mà.
Dialogue: 0,0:18:04.71,0:18:06.74,Default,,0000,0000,0000,,Tôi không thể tự kiềm chế.
Dialogue: 0,0:18:41.08,0:18:43.68,Default,,0000,0000,0000,,Hayami-kun, hãy để đó cho tôi!
Dialogue: 0,0:19:49.25,0:19:51.12,Default,,0000,0000,0000,,Hayami-kun, hãy cố chịu.
Dialogue: 0,0:20:01.53,0:20:02.76,Default,,0000,0000,0000,,Khốn thật!
Dialogue: 0,0:20:49.18,0:20:51.30,Default,,0000,0000,0000,,Hayami-kun! Mới chỉ là bắt đầu thôi!
Dialogue: 0,0:21:43.26,0:21:44.29,Default,,0000,0000,0000,,Hayami-kun!
Dialogue: 0,0:21:47.77,0:21:52.07,Default,,0000,0000,0000,,Thật tuyệt! Cái gì thế này? Không thể tin nổi!
Dialogue: 0,0:22:21.97,0:22:23.40,Default,,0000,0000,0000,,Việc chuẩn bị đã hoàn tất!
Dialogue: 0,0:22:28.18,0:22:29.70,Default,,0000,0000,0000,,Mọi người, đi nào!
Dialogue: 0,0:22:30.28,0:22:30.90,Default,,0000,0000,0000,,Được!
Dialogue: 0,0:22:31.01,0:22:32.41,Default,,0000,0000,0000,,Cứ tin ở tớ!
Dialogue: 0,0:22:32.41,0:22:37.41,Default,,0000,0000,0000,,
