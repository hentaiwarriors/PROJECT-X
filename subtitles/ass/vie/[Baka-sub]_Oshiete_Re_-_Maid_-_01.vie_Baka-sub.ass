Dialogue: 0,0:00:12.87,0:00:13.61,handwriting 4,,0000,0000,0000,,Akari này.
Dialogue: 0,0:00:14.41,0:00:15.37,handwriting 4,,0000,0000,0000,,Vâng!
Dialogue: 0,0:00:24.90,0:00:26.39,handwriting 4,,0000,0000,0000,,Chủ nhân..!
Dialogue: 0,0:00:26.39,0:00:31.08,handwriting 4,,0000,0000,0000,,Món tráng miệng hôm nay là bánh ga tô cùng với kem lạnh hảo hạng.
Dialogue: 0,0:00:31.20,0:00:34.50,handwriting 4,,0000,0000,0000,,Ái chà. Một loại bánh kem mới hả.
Dialogue: 0,0:00:37.78,0:00:38.90,handwriting 4,,0000,0000,0000,,Gì vậy..?
Dialogue: 0,0:00:42.05,0:00:42.93,handwriting 4,,0000,0000,0000,,Akari!
Dialogue: 0,0:00:43.37,0:00:44.01,handwriting 4,,0000,0000,0000,,Cậu ko sao chứ?
Dialogue: 0,0:00:44.01,0:00:46.01,handwriting 4,,0000,0000,0000,,{\pos(324,474)}Chủ nhân, coi chừng!
Dialogue: 0,0:00:52.39,0:00:55.15,handwriting 4,,0000,0000,0000,,Có vẻ như tôi đã chém hụt mục tiêu.
Dialogue: 0,0:00:55.88,0:00:58.25,handwriting 4,,0000,0000,0000,,Đừng lo, tôi vẫn an toàn.
Dialogue: 0,0:01:03.19,0:01:04.19,handwriting 4,,0000,0000,0000,,Ko vấn đề gì..
Dialogue: 0,0:01:05.24,0:01:06.47,handwriting 4,,0000,0000,0000,,Im lặng nào.
Dialogue: 0,0:01:06.47,0:01:09.06,handwriting 4,,0000,0000,0000,,Bánh tráng miệng của chủ nhân..
Dialogue: 0,0:01:09.06,0:01:10.35,handwriting 4,,0000,0000,0000,,Đó ko phải lỗi của cậu mà.
Dialogue: 0,0:01:10.35,0:01:11.23,handwriting 4,,0000,0000,0000,,Tôi đã bảo tất cả im lặng mà.
Dialogue: 0,0:01:11.77,0:01:13.65,handwriting 4,,0000,0000,0000,,Cô ấy thực sự ko cố tình mà?
Dialogue: 0,0:01:14.46,0:01:15.12,handwriting 4,,0000,0000,0000,,Philine.
Dialogue: 0,0:01:17.32,0:01:18.05,handwriting 4,,0000,0000,0000,,Cho phép tôi.
Dialogue: 0,0:01:19.29,0:01:21.49,handwriting 4,,0000,0000,0000,,Shiraha, dọn bàn đi đã nào .
Dialogue: 0,0:01:22.03,0:01:24.38,handwriting 4,,0000,0000,0000,,Akito, cậu ko làm gì thì phụ bọn tôi một tay đi chứ.
Dialogue: 0,0:01:22.76,0:01:24.35,handwriting 4,,0000,0000,0000,,{\pos(318,51)}Tha cho tôi!
Dialogue: 0,0:01:24.87,0:01:29.88,handwriting 4,,0000,0000,0000,,Điều này luôn lặp đi lặp lại ngày này qua ngày khác, ôi các cô hầu gái của tôi.
Dialogue: 0,0:01:29.88,0:01:31.34,handwriting 4,,0000,0000,0000,,Làm ơn dừng lại đi mà!
Dialogue: 0,0:01:33.03,0:01:35.55,cursive,,0000,0000,0000,,{\pos(248,130)}Oshiete
Dialogue: 0,0:01:33.03,0:01:35.55,cursive,,0000,0000,0000,,{\pos(396,377)}Maid
Dialogue: 0,0:01:33.03,0:01:35.55,cursive,,0000,0000,0000,,{\pos(336,481)}Bài học đầu tiên
Dialogue: 0,0:01:33.00,0:01:35.55,handwriting 4,,0000,0000,0000,,{\pos(41,37)}
Dialogue: 0,0:01:33.00,0:01:35.55,handwriting 4,,0000,0000,0000,,{\pos(176,33)}Baka-sub.net
Dialogue: 0,0:01:36.43,0:01:38.80,handwriting 4,,0000,0000,0000,,Tên tôi là Ayabe Yukari.
Dialogue: 0,0:01:39.21,0:01:43.05,handwriting 4,,0000,0000,0000,,Công việc của tôi là dạy dỗ và huấn luyện các hầu gái chuyên nghiệp.
Dialogue: 0,0:01:43.54,0:01:46.37,handwriting 4,,0000,0000,0000,,Vậy, tất cả chuyện này...
Dialogue: 0,0:01:47.67,0:01:48.62,handwriting 4,,0000,0000,0000,,Có liên quan gì tới tôi vậy?
Dialogue: 0,0:01:48.89,0:01:51.09,handwriting 4,,0000,0000,0000,,Tôi sẽ giải thích cho cậu tất cả mọi chuyện.
Dialogue: 0,0:01:51.28,0:01:54.39,handwriting 4,,0000,0000,0000,,Xin phép tự giới thiệu: tôi là hiệu trưởng của học viện đào tạo hầu gái chuyên nghiệp.
Dialogue: 0,0:01:55.78,0:01:57.93,handwriting 4,,0000,0000,0000,,Công việc của cậu là...
Dialogue: 0,0:01:58.42,0:02:00.28,handwriting 4,,0000,0000,0000,,Cậu sẽ trở thành chủ nhân của bọn họ.
Dialogue: 0,0:02:00.47,0:02:02.01,handwriting 4,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:02:02.52,0:02:03.18,handwriting 4,,0000,0000,0000,,Đó là Mochi.
Dialogue: 0,0:02:03.80,0:02:04.31,handwriting 4,,0000,0000,0000,,Mochi?
Dialogue: 0,0:02:05.19,0:02:07.68,handwriting 4,,0000,0000,0000,,Nó có giọng nói dễ thương nhỉ.
Dialogue: 0,0:02:08.32,0:02:10.96,handwriting 4,,0000,0000,0000,,Cậu vừa khen tôi dễ thương đấy ư, ôi xấu hổ quá.
Dialogue: 0,0:02:10.98,0:02:11.98,handwriting 4,,0000,0000,0000,,Nó biết nói sao.
Dialogue: 0,0:02:12.54,0:02:13.84,handwriting 4,,0000,0000,0000,,Xin hãy bình tĩnh.
Dialogue: 0,0:02:14.11,0:02:15.79,handwriting 4,,0000,0000,0000,,Người đang nói đích thực là ngài hiệu trưởng.
Dialogue: 0,0:02:16.11,0:02:16.97,handwriting 4,,0000,0000,0000,,Gì cơ?!
Dialogue: 0,0:02:17.70,0:02:19.58,handwriting 4,,0000,0000,0000,,Vật quái dị này là ông hiệu trưởng sao?
Dialogue: 0,0:02:19.80,0:02:20.93,handwriting 4,,0000,0000,0000,,Ko, cậu nhầm rồi.
Dialogue: 0,0:02:21.10,0:02:22.86,handwriting 4,,0000,0000,0000,,Đây là Mo-chan.
Dialogue: 0,0:02:23.57,0:02:24.86,handwriting 4,,0000,0000,0000,,Thế hả..
Dialogue: 0,0:02:25.74,0:02:27.50,handwriting 4,,0000,0000,0000,,Nghĩa là mày ko phải Mochi hả?
Dialogue: 0,0:02:28.60,0:02:32.85,handwriting 4,,0000,0000,0000,,Ái chà, cứ như khối bông gòn vậy.
Dialogue: 0,0:02:33.39,0:02:34.46,handwriting 4,,0000,0000,0000,,Cậu làm Mochi đau đấy...
Dialogue: 0,0:02:35.49,0:02:40.52,handwriting 4,,0000,0000,0000,,Hiểu rồi, vậy là Mochi được sử dụng như một công cụ liên lạc.
Dialogue: 0,0:02:40.62,0:02:41.21,handwriting 4,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:02:41.77,0:02:44.12,handwriting 4,,0000,0000,0000,,Ngài hiệu trưởng là người vô cùng bận rộn.
Dialogue: 0,0:02:45.19,0:02:50.15,handwriting 4,,0000,0000,0000,,Đó chính là lí do khiến Yukari-chan và những người khác được cử đi giải quyết công việc thay tôi.
Dialogue: 0,0:02:50.15,0:02:51.11,handwriting 4,,0000,0000,0000,,Tôi xin lỗi.
Dialogue: 0,0:02:51.23,0:02:52.01,handwriting 4,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:02:52.45,0:02:55.77,handwriting 4,,0000,0000,0000,,Vậy? Tôi giúp gì được cho các vị đây? Yukari-san?
Dialogue: 0,0:02:56.07,0:02:58.51,handwriting 4,,0000,0000,0000,,Làm ơn gọi tôi là Wistoru.
Dialogue: 0,0:02:59.37,0:03:01.57,handwriting 4,,0000,0000,0000,,Cậu xem kĩ đi.
Dialogue: 0,0:03:01.74,0:03:04.13,handwriting 4,,0000,0000,0000,,Đó là bốn hầu gái cậu sẽ tiếp quản.
Dialogue: 0,0:03:08.95,0:03:12.07,handwriting 4,,0000,0000,0000,,Lúc đó mình đâu ngờ họ lại gây ra nhiều rắc rối như vậy.
Dialogue: 0,0:03:13.30,0:03:13.78,handwriting 4,,0000,0000,0000,,Vào đi.
Dialogue: 0,0:03:14.86,0:03:17.67,handwriting 4,,0000,0000,0000,,Em mang trà đến cho cậu đây, Chủ nhân.
Dialogue: 0,0:03:18.72,0:03:19.80,handwriting 4,,0000,0000,0000,,Đặt nó ở đây nhé.
Dialogue: 0,0:03:19.87,0:03:20.50,handwriting 4,,0000,0000,0000,,Vâng ạ!
Dialogue: 0,0:03:22.17,0:03:23.14,handwriting 4,,0000,0000,0000,,Đừng chạy khi bưng trà chứ.
Dialogue: 0,0:03:23.14,0:03:23.73,handwriting 4,,0000,0000,0000,,Vâng!
Dialogue: 0,0:03:24.27,0:03:26.30,handwriting 4,,0000,0000,0000,,Đừng quên đóng cửa sau khi vào nhé.
Dialogue: 0,0:03:26.66,0:03:28.01,handwriting 4,,0000,0000,0000,,Em xin lỗi!
Dialogue: 0,0:03:33.33,0:03:34.63,handwriting 4,,0000,0000,0000,,Chủ nhân...
Dialogue: 0,0:03:36.14,0:03:39.96,handwriting 4,,0000,0000,0000,,Chà, cô bất cẩn quá đấy.
Dialogue: 0,0:03:40.01,0:03:41.96,handwriting 4,,0000,0000,0000,,Đừng có chạy vòng vòng như vậy chứ!
Dialogue: 0,0:03:46.80,0:03:47.14,handwriting 4,,0000,0000,0000,,Akari?
Dialogue: 0,0:03:49.88,0:03:50.90,handwriting 4,,0000,0000,0000,,Này Akito!
Dialogue: 0,0:03:51.05,0:03:53.35,handwriting 4,,0000,0000,0000,,Cậu làm gì khiến Akari khóc vậy.
Dialogue: 0,0:03:53.40,0:03:55.35,handwriting 4,,0000,0000,0000,,Nhìn xem cô ấy vừa làm gì tôi này.
Dialogue: 0,0:03:57.50,0:03:59.70,handwriting 4,,0000,0000,0000,,A..Akari lại thế nữa rồi.
Dialogue: 0,0:04:01.05,0:04:04.03,handwriting 4,,0000,0000,0000,,Cậu phải thay áo ngay đi.
Dialogue: 0,0:04:04.64,0:04:07.45,handwriting 4,,0000,0000,0000,,Chẳng phải cậu là hầu gái đó sao, giúp tôi thay nó ra nào..
Dialogue: 0,0:04:07.99,0:04:10.77,handwriting 4,,0000,0000,0000,,Tôi vẫn đang là chủ nhân của cậu mà.
Dialogue: 0,0:04:12.12,0:04:14.76,handwriting 4,,0000,0000,0000,,Chà, Akito luôn như thế này sao ?
Dialogue: 0,0:04:16.91,0:04:22.53,handwriting 4,,0000,0000,0000,,Chủ nhân cũ của tôi là một gã vô dụng, hắn ta ko biết làm bất cứ một công việc gì cả.
Dialogue: 0,0:04:22.82,0:04:24.09,handwriting 4,,0000,0000,0000,,Ah này...
Dialogue: 0,0:04:24.34,0:04:28.25,handwriting 4,,0000,0000,0000,,Nếu cậu cứ tiếp tục như vậy, sẽ chỉ khiến họ ghét cậu hơn mà thôi.
Dialogue: 0,0:04:28.47,0:04:29.32,handwriting 4,,0000,0000,0000,,Thôi vậy...
Dialogue: 0,0:04:29.39,0:04:31.32,handwriting 4,,0000,0000,0000,,Để tôi gọi Philine mang áo đến cho cậu.
Dialogue: 0,0:04:31.45,0:04:32.74,handwriting 4,,0000,0000,0000,,Cậu nói gì lạ vậy?
Dialogue: 0,0:04:33.01,0:04:34.89,handwriting 4,,0000,0000,0000,,Quần áo lúc nào chả ở trong tủ.
Dialogue: 0,0:04:35.58,0:04:36.89,handwriting 4,,0000,0000,0000,,Ko có ở đó đâu.
Dialogue: 0,0:04:36.90,0:04:39.27,handwriting 4,,0000,0000,0000,,Ngày hôm nay nắng rất đẹp, vì vậy tất cả quần áo đã được mang đi giặt và phơi ở ngoài.
Dialogue: 0,0:04:43.32,0:04:43.74,handwriting 4,,0000,0000,0000,,Philine.
Dialogue: 0,0:04:45.91,0:04:46.26,handwriting 4,,0000,0000,0000,,Chủ...
Dialogue: 0,0:04:48.55,0:04:53.54,handwriting 4,,0000,0000,0000,,Sao cậu lại cởi trần thế này, Chủ nhân..?
Dialogue: 0,0:04:54.15,0:04:57.52,handwriting 4,,0000,0000,0000,,À thì, tất cả quần áo của tôi đều đang phơi ngoài đây mà.
Dialogue: 0,0:04:59.94,0:05:04.98,handwriting 4,,0000,0000,0000,,Ah.. chúng sắp khô cả rồi, cậu có thể chọn một chiếc để mặc.
Dialogue: 0,0:05:15.46,0:05:16.29,handwriting 4,,0000,0000,0000,,Thật phi thường.
Dialogue: 0,0:05:28.56,0:05:29.63,handwriting 4,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:05:30.00,0:05:32.61,handwriting 4,,0000,0000,0000,,Quần áo của cậu lại ướt hết rồi.
Dialogue: 0,0:05:33.98,0:05:35.25,handwriting 4,,0000,0000,0000,,Thời tiết thay đổi nhanh khiếp thật.
Dialogue: 0,0:05:35.67,0:05:36.77,handwriting 4,,0000,0000,0000,,Có lẽ tự nhiên nó thế?
Dialogue: 0,0:05:37.18,0:05:38.80,handwriting 4,,0000,0000,0000,,Này, Shiraha.
Dialogue: 0,0:05:44.98,0:05:46.35,handwriting 4,,0000,0000,0000,,Cô ko thể đi xuống như một người bình thường được sao.
Dialogue: 0,0:05:50.53,0:05:52.87,handwriting 4,,0000,0000,0000,,Yêu cầu của cậu là gì? Chủ nhân...
Dialogue: 0,0:05:54.09,0:05:57.00,handwriting 4,,0000,0000,0000,,Cậu muốn lấy mạng kẻ nào?
Dialogue: 0,0:05:57.03,0:05:57.37,handwriting 4,,0000,0000,0000,,Ko.
Dialogue: 0,0:05:58.05,0:05:59.37,handwriting 4,,0000,0000,0000,,Cậu muốn bắt cóc ai?
Dialogue: 0,0:06:00.33,0:06:01.01,handwriting 4,,0000,0000,0000,,Ko nốt.
Dialogue: 0,0:06:01.11,0:06:01.84,handwriting 4,,0000,0000,0000,,Vậy thì...
Dialogue: 0,0:06:03.72,0:06:04.06,handwriting 4,,0000,0000,0000,,Cậu muốn gì?
Dialogue: 0,0:06:04.87,0:06:07.17,handwriting 4,,0000,0000,0000,,Giặt lại chỗ quần áo này dùm tôi.
Dialogue: 0,0:06:09.76,0:06:11.40,handwriting 4,,0000,0000,0000,,Đó là công việc của một hầu gái.
Dialogue: 0,0:06:11.71,0:06:13.40,handwriting 4,,0000,0000,0000,,Thế cô ko phải hầu gái à?
Dialogue: 0,0:06:14.57,0:06:15.50,handwriting 4,,0000,0000,0000,,À, đúng rồi.
Dialogue: 0,0:06:16.09,0:06:17.87,handwriting 4,,0000,0000,0000,,Tôi ko còn làm vệ sĩ nữa ..
Dialogue: 0,0:06:18.21,0:06:21.22,handwriting 4,,0000,0000,0000,,Vậy nhé, làm ơn phụ mình một tay, Shiraha-chan.
Dialogue: 0,0:06:21.88,0:06:22.59,handwriting 4,,0000,0000,0000,,Được rồi.
Dialogue: 0,0:06:31.19,0:06:32.27,handwriting 4,,0000,0000,0000,,Cô sao thế, Shiraha?
Dialogue: 0,0:06:32.27,0:06:34.49,handwriting 4,,0000,0000,0000,,Chủ nhân, chạy mau!
Dialogue: 0,0:06:43.87,0:06:45.00,handwriting 4,,0000,0000,0000,,Bình tĩnh nào, Shiraha!
Dialogue: 0,0:06:46.22,0:06:49.71,handwriting 4,,0000,0000,0000,,Chết đi! Ta sẽ giết mày, tao phải giết mày!
Dialogue: 0,0:06:50.72,0:06:51.69,handwriting 4,,0000,0000,0000,,Coi chừng đấy, Shiraha.
Dialogue: 0,0:06:54.43,0:07:00.47,handwriting 4,,0000,0000,0000,,Sát! Sát! Sát!
Dialogue: 0,0:07:01.74,0:07:02.47,handwriting 4,,0000,0000,0000,,Là vậy đó...
Dialogue: 0,0:07:02.89,0:07:04.87,handwriting 4,,0000,0000,0000,,Chuyện này đã vượt ra ngoài tầm kiểm soát của tôi.
Dialogue: 0,0:07:06.14,0:07:06.89,handwriting 4,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:07:07.80,0:07:08.56,handwriting 4,,0000,0000,0000,,Nhân tiện...
Dialogue: 0,0:07:09.02,0:07:10.56,handwriting 4,,0000,0000,0000,,Sao cậu lại ăn mặc thế này?
Dialogue: 0,0:07:10.95,0:07:13.64,handwriting 4,,0000,0000,0000,,Đó là tác phẩm của "nữ chiến binh" hầu gái đó.
Dialogue: 0,0:07:14.10,0:07:14.96,handwriting 4,,0000,0000,0000,,Ra là vậy.
Dialogue: 0,0:07:15.59,0:07:18.50,handwriting 4,,0000,0000,0000,,Có vẻ như các hầu gái vẫn chưa hoàn thành công việc thực sự của họ ở đây.
Dialogue: 0,0:07:18.55,0:07:19.80,handwriting 4,,0000,0000,0000,,Ý cô là sao?
Dialogue: 0,0:07:21.19,0:07:24.86,handwriting 4,,0000,0000,0000,,Tôi đang nói đến công việc mà bọn họ phải làm vào ban đêm.
Dialogue: 0,0:07:25.05,0:07:27.49,handwriting 4,,0000,0000,0000,,Có quá sớm để nói đến chuyện đó ko.
Dialogue: 0,0:07:27.91,0:07:30.45,handwriting 4,,0000,0000,0000,,Họ thậm chí còn chưa làm nổi việc gì ra hồn vào ban ngày.
Dialogue: 0,0:07:31.40,0:07:34.02,handwriting 4,,0000,0000,0000,,Có lẽ cậu vẫn chưa hiểu tôi muốn nói gì.
Dialogue: 0,0:07:34.43,0:07:36.93,handwriting 4,,0000,0000,0000,,Công việc bọn họ phải làm vào ban đêm thực sự là...
Dialogue: 0,0:07:37.47,0:07:41.13,handwriting 4,,0000,0000,0000,,Tôi sẽ chỉ cho cậu thấy.
Dialogue: 0,0:07:41.64,0:07:42.57,handwriting 4,,0000,0000,0000,,Công việc thực sự ư?
Dialogue: 0,0:07:46.60,0:07:47.61,handwriting 4,,0000,0000,0000,,Khoan đã, Wistoru...
Dialogue: 0,0:07:48.24,0:07:52.64,handwriting 4,,0000,0000,0000,,Cậu cứ ngồi yên ở đó là được rồi, Chủ nhân.
Dialogue: 0,0:08:04.96,0:08:07.13,handwriting 4,,0000,0000,0000,,Có vẻ như nó đã thức dậy rồi kìa...
Dialogue: 0,0:08:07.18,0:08:09.06,handwriting 4,,0000,0000,0000,,Hình như nó cũng thích điều đó...
Dialogue: 0,0:08:09.75,0:08:13.17,handwriting 4,,0000,0000,0000,,Là vậy đó, các hầu gái phải hoàn thành công việc của mình vào ban đêm.
Dialogue: 0,0:08:13.29,0:08:16.51,handwriting 4,,0000,0000,0000,,Cậu có thể yêu cầu một, hoặc tất cả bọn họ.
Dialogue: 0,0:08:17.76,0:08:20.89,handwriting 4,,0000,0000,0000,,Bởi vì đó là yêu cầu của chủ nhân...
Dialogue: 0,0:08:21.38,0:08:24.78,handwriting 4,,0000,0000,0000,,Các hầu gái có nghĩa vụ lắng nghe và đáp ứng tất cả.
Dialogue: 0,0:08:26.83,0:08:27.49,handwriting 4,,0000,0000,0000,,Wistoru...
Dialogue: 0,0:08:27.71,0:08:30.84,handwriting 4,,0000,0000,0000,,Chỉ khi ở trong vòng tay của chủ nhân...
Dialogue: 0,0:08:30.84,0:08:35.26,handwriting 4,,0000,0000,0000,,Họ mới có thể cảm nhận được hơi ấm từ cơ thể cậu và trung thành với cậu.
Dialogue: 0,0:08:36.85,0:08:39.27,handwriting 4,,0000,0000,0000,,Niềm tự hào của tất cả các hầu gái.
Dialogue: 0,0:08:39.78,0:08:42.35,handwriting 4,,0000,0000,0000,,Họ sẽ làm bất cứ điều gì...
Dialogue: 0,0:08:43.06,0:08:45.62,handwriting 4,,0000,0000,0000,,Người duy nhất họ nghe lời chính là cậu, Chủ nhân.
Dialogue: 0,0:08:48.24,0:08:48.77,handwriting 4,,0000,0000,0000,,Wistoru...
Dialogue: 0,0:09:05.00,0:09:09.28,handwriting 4,,0000,0000,0000,,Bên trong Wistoru thật là ấm...
Dialogue: 0,0:09:10.79,0:09:12.16,handwriting 4,,0000,0000,0000,,Đây là lần đầu tiên mình cảm thấy thế này...
Dialogue: 0,0:09:12.67,0:09:15.04,handwriting 4,,0000,0000,0000,,Thằng nhỏ của cậu ấm thật đấy, Chủ nhân.
Dialogue: 0,0:09:15.61,0:09:16.71,handwriting 4,,0000,0000,0000,,Và còn rất bự nữa.
Dialogue: 0,0:09:27.95,0:09:33.03,handwriting 4,,0000,0000,0000,,Vào lúc này, thân thể tôi hoàn toàn thuộc về cậu, Chủ nhân.
Dialogue: 0,0:09:33.35,0:09:35.91,handwriting 4,,0000,0000,0000,,Hãy làm bất cứ điều gì cậu muốn.
Dialogue: 0,0:09:43.32,0:09:47.35,handwriting 4,,0000,0000,0000,,Tất cả quần áo đêì đã khô, phải nhanh đem đến cho chủ nhân.
Dialogue: 0,0:09:47.35,0:09:49.65,handwriting 4,,0000,0000,0000,,Akari, ko được chạy khi bê đồ.
Dialogue: 0,0:09:53.00,0:09:54.32,handwriting 4,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:09:55.73,0:09:58.03,handwriting 4,,0000,0000,0000,,Cô xấu tính vậy, Shiraha-san!
Dialogue: 0,0:09:58.42,0:10:03.67,handwriting 4,,0000,0000,0000,,Tôi muốn tự mình mang áo tới cho chủ nhân!
Dialogue: 0,0:10:25.74,0:10:26.74,handwriting 4,,0000,0000,0000,,Onee-chan...
Dialogue: 0,0:10:27.67,0:10:30.26,handwriting 4,,0000,0000,0000,,Tuyệt quá đi mất, Chủ nhân.
Dialogue: 0,0:10:42.73,0:10:43.53,handwriting 4,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:10:44.17,0:10:45.44,handwriting 4,,0000,0000,0000,,Chủ nhân..!
Dialogue: 0,0:10:46.83,0:10:52.94,handwriting 4,,0000,0000,0000,,Hãy cho tôi toàn bộ tinh dịch của cậu, Chủ nhân.
Dialogue: 0,0:10:56.51,0:10:57.78,handwriting 4,,0000,0000,0000,,Cho tôi tất cả đi nào.
Dialogue: 0,0:10:57.85,0:10:58.41,handwriting 4,,0000,0000,0000,,Wistoru...
Dialogue: 0,0:11:05.16,0:11:07.75,handwriting 4,,0000,0000,0000,,Chủ... nhân
Dialogue: 0,0:11:13.59,0:11:14.20,handwriting 4,,0000,0000,0000,,Akari...
Dialogue: 0,0:11:15.25,0:11:16.79,handwriting 4,,0000,0000,0000,,Đó là một phần công việc của chúng ta.
Dialogue: 0,0:11:19.14,0:11:19.85,handwriting 4,,0000,0000,0000,,Um..
Dialogue: 0,0:11:23.78,0:11:27.62,handwriting 4,,0000,0000,0000,,Chủ nhân và chị ấy đã làm chuyện đó...
Dialogue: 0,0:11:27.86,0:11:31.41,handwriting 4,,0000,0000,0000,,Akari chỉ là một hầu gái, và chủ nhân thì...
Dialogue: 0,0:11:31.55,0:11:33.41,handwriting 4,,0000,0000,0000,,chủ nhân thì...
Dialogue: 0,0:11:49.07,0:11:52.62,handwriting 4,,0000,0000,0000,,Đang làm gì thế? Con hầu kia.
Dialogue: 0,0:11:53.30,0:11:54.28,handwriting 4,,0000,0000,0000,,Yonis-chan...
Dialogue: 0,0:11:54.55,0:11:58.46,handwriting 4,,0000,0000,0000,,Có vẻ như cô vừa làm chủ nhân của mình phật lòng hả?
Dialogue: 0,0:11:59.07,0:12:01.27,handwriting 4,,0000,0000,0000,,Cô ít tuổi hơn bọn này, phải ko nhỉ?
Dialogue: 0,0:12:01.27,0:12:03.27,handwriting 4,,0000,0000,0000,,Chị của cô là Wistoru, đúng ko?
Dialogue: 0,0:12:07.65,0:12:09.45,handwriting 4,,0000,0000,0000,,Các cô.. Các cô nhầm rồi!
Dialogue: 0,0:12:09.87,0:12:14.42,handwriting 4,,0000,0000,0000,,Akari.. là do chủ nhân...
Dialogue: 0,0:12:14.54,0:12:16.76,handwriting 4,,0000,0000,0000,,Chủ nhân của cô làm sao?
Dialogue: 0,0:12:17.10,0:12:19.79,handwriting 4,,0000,0000,0000,,À, là công việc mà chúng tôi phải làm vào ban đêm...
Dialogue: 0,0:12:19.82,0:12:22.92,handwriting 4,,0000,0000,0000,,Cái gì! Đừng bảo gã dâm tặc đó và chị của tôi đã...
Dialogue: 0,0:12:22.92,0:12:24.92,handwriting 4,,0000,0000,0000,,Ko, ko phải như vậy.
Dialogue: 0,0:12:24.92,0:12:25.85,handwriting 4,,0000,0000,0000,,Onee-sama!
Dialogue: 0,0:12:26.37,0:12:28.03,handwriting 4,,0000,0000,0000,,Ah.. Yonis-chan...
Dialogue: 0,0:12:28.71,0:12:30.20,handwriting 4,,0000,0000,0000,,Miko-chan...Mako-chan...
Dialogue: 0,0:12:30.28,0:12:31.52,handwriting 4,,0000,0000,0000,,Onee-sama!
Dialogue: 0,0:12:32.57,0:12:34.43,handwriting 4,,0000,0000,0000,,Khoan..khoan đã Yonis-chan.
Dialogue: 0,0:12:34.58,0:12:38.32,handwriting 4,,0000,0000,0000,,Em biết mà, dù chị mặc trên người bộ đồ hầu gái, cơ thể chị vẫn ko thay đổi chút nào.
Dialogue: 0,0:12:39.46,0:12:42.23,handwriting 4,,0000,0000,0000,,Ai cho phép cô mặc bộ đồ này ở bên ngoài trường học, Yonis?
Dialogue: 0,0:12:42.96,0:12:43.81,handwriting 4,,0000,0000,0000,,Wistoru...
Dialogue: 0,0:12:44.11,0:12:46.48,handwriting 4,,0000,0000,0000,,Đây là nơi huấn luyện các hầu gái.
Dialogue: 0,0:12:47.48,0:12:49.65,handwriting 4,,0000,0000,0000,,Nếu cô muốn tới đây gây chuyện, thì tôi khuyên cô tốt nhất nên rời khỏi đây ngay.
Dialogue: 0,0:12:51.34,0:12:54.05,handwriting 4,,0000,0000,0000,,Đừng nặng lời như vậy chứ.
Dialogue: 0,0:12:54.25,0:12:57.52,handwriting 4,,0000,0000,0000,,Tôi chỉ ghé thăm nơi này chút thôi mà.
Dialogue: 0,0:12:57.65,0:12:59.99,handwriting 4,,0000,0000,0000,,Thôi vậy, xin lỗi vì đã làm phiền.
Dialogue: 0,0:13:00.31,0:13:02.85,handwriting 4,,0000,0000,0000,,Ah! Yonis-chan, đợi đã nào!
Dialogue: 0,0:13:03.27,0:13:05.59,handwriting 4,,0000,0000,0000,,Wistoru.. chị về đấy à?
Dialogue: 0,0:13:06.15,0:13:06.47,handwriting 4,,0000,0000,0000,,Phải...
Dialogue: 0,0:13:07.30,0:13:10.74,handwriting 4,,0000,0000,0000,,Công việc của tôi ở đây đã xong, dù hơi tốn thời gian.
Dialogue: 0,0:13:22.18,0:13:23.13,handwriting 4,,0000,0000,0000,,Em sao vậy?
Dialogue: 0,0:13:23.38,0:13:24.06,handwriting 4,,0000,0000,0000,,Akari...
Dialogue: 0,0:13:24.65,0:13:29.63,handwriting 4,,0000,0000,0000,,Ko, ko có gì. Ko có gì đâu, Onee-ch.. Wistoru.
Dialogue: 0,0:13:29.83,0:13:30.81,handwriting 4,,0000,0000,0000,,Vậy hả?
Dialogue: 0,0:13:31.61,0:13:35.11,handwriting 4,,0000,0000,0000,,Hi vọng em sẽ làm Chủ nhân vừa lòng.
Dialogue: 0,0:13:35.52,0:13:36.57,handwriting 4,,0000,0000,0000,,Cậu ta vui tính lắm đấy.
Dialogue: 0,0:13:37.33,0:13:38.28,handwriting 4,,0000,0000,0000,,Vâng ạ!
Dialogue: 0,0:13:38.70,0:13:40.73,handwriting 4,,0000,0000,0000,,Vậy nhé, công việc ở đây giao cho mọi người hết đấy.
Dialogue: 0,0:13:41.24,0:13:41.95,handwriting 4,,0000,0000,0000,,Vâng!
Dialogue: 0,0:13:43.83,0:13:44.64,handwriting 4,,0000,0000,0000,,Akari-chan.
Dialogue: 0,0:13:45.10,0:13:46.64,handwriting 4,,0000,0000,0000,,Tới đây phụ tôi nào.
Dialogue: 0,0:13:56.74,0:13:59.11,handwriting 4,,0000,0000,0000,,Huấn luyện ban đêm ư?
Dialogue: 0,0:14:00.62,0:14:01.04,handwriting 4,,0000,0000,0000,,Vào đi!
Dialogue: 0,0:14:02.09,0:14:04.14,handwriting 4,,0000,0000,0000,,Chuyện gì vậy, Shiraha? Khuya lắm rồi.
Dialogue: 0,0:14:05.70,0:14:07.12,handwriting 4,,0000,0000,0000,,Tôi tới đây để tham gia việc huấn luyện ban đêm.
Dialogue: 0,0:14:07.22,0:14:07.83,handwriting 4,,0000,0000,0000,,Vậy hả.
Dialogue: 0,0:14:10.40,0:14:14.50,handwriting 4,,0000,0000,0000,,Này! Shiraha.. Cô biết mình đang nói gì ko đó?
Dialogue: 0,0:14:16.14,0:14:17.19,handwriting 4,,0000,0000,0000,,Tôi bắt đầu nhé.
Dialogue: 0,0:14:18.85,0:14:20.27,handwriting 4,,0000,0000,0000,,Shiraha..
Dialogue: 0,0:14:20.56,0:14:21.49,handwriting 4,,0000,0000,0000,,Cô làm gì vậy?
Dialogue: 0,0:14:21.98,0:14:26.94,handwriting 4,,0000,0000,0000,,Hầu gái có nghĩa vụ tuân theo mọi ý muốn của chủ nhân.
Dialogue: 0,0:14:27.14,0:14:28.94,handwriting 4,,0000,0000,0000,,Cô hiểu sai chuyện này rồi.
Dialogue: 0,0:14:30.07,0:14:31.51,handwriting 4,,0000,0000,0000,,Cô.. làm gì vậy?
Dialogue: 0,0:14:43.34,0:14:45.15,handwriting 4,,0000,0000,0000,,Đây là điều cô muốn làm sao?
Dialogue: 0,0:14:45.37,0:14:47.98,handwriting 4,,0000,0000,0000,,Đó mới chỉ là bắt đầu thôi.
Dialogue: 0,0:14:48.91,0:14:52.92,handwriting 4,,0000,0000,0000,,Cứ giao mọi chuyện cho tôi.
Dialogue: 0,0:14:53.33,0:14:54.87,handwriting 4,,0000,0000,0000,,Tôi đã học được rất nhiều điều về chuyện này.
Dialogue: 0,0:14:54.87,0:14:57.76,handwriting 4,,0000,0000,0000,,Này, chờ đã, tôi nhớ là mình vẫn chưa hề dạy cô điều gì mà.
Dialogue: 0,0:14:58.17,0:14:58.88,handwriting 4,,0000,0000,0000,,Ko cần đâu.
Dialogue: 0,0:15:00.35,0:15:05.82,handwriting 4,,0000,0000,0000,,Tôi đã nghiên cứu rất nhiều tạp chí người lớn và các "Ero game" trong máy của cậu.
Dialogue: 0,0:15:07.26,0:15:08.19,handwriting 4,,0000,0000,0000,,Ko vấn đề gì!
Dialogue: 0,0:15:08.97,0:15:10.24,handwriting 4,,0000,0000,0000,,Có vấn đề đấy.
Dialogue: 0,0:15:11.17,0:15:17.04,handwriting 4,,0000,0000,0000,,Trước tiên, phải làm cho thằng nhỏ của chủ nhân bự lên.
Dialogue: 0,0:15:17.92,0:15:20.43,handwriting 4,,0000,0000,0000,,Để làm được điều đó, cần phải dùng miệng của tôi.
Dialogue: 0,0:15:45.43,0:15:47.02,handwriting 4,,0000,0000,0000,,Nó đang bự lên...
Dialogue: 0,0:15:54.25,0:15:58.16,handwriting 4,,0000,0000,0000,,Mới lần đầu tiên mà cô ấy đã làm chuyện đó tuyệt như vậy ư.
Dialogue: 0,0:16:04.86,0:16:06.30,handwriting 4,,0000,0000,0000,,Bắt đầu nào...
Dialogue: 0,0:16:06.69,0:16:08.58,handwriting 4,,0000,0000,0000,,Shiraha, chúng ta tạm ngừng một chút được ko?
Dialogue: 0,0:16:09.02,0:16:13.17,handwriting 4,,0000,0000,0000,,Hầu gái ko cần phải xoa dịu tâm hồn của chủ nhân mình, nhưng với cơ thể thì đó là điều bắt buộc.
Dialogue: 0,0:16:13.78,0:16:15.42,handwriting 4,,0000,0000,0000,,Cô có thể làm chuyện này theo cách khác mà.
Dialogue: 0,0:16:25.39,0:16:30.45,handwriting 4,,0000,0000,0000,,Buổi huấn luyện đêm nay là thời điểm thích hợp nhất để chủ nhân và em hòa làm một.
Dialogue: 0,0:16:31.16,0:16:32.08,handwriting 4,,0000,0000,0000,,Xin đừng ngăn cản em.
Dialogue: 0,0:16:32.08,0:16:33.97,handwriting 4,,0000,0000,0000,,Đây đâu phải là trò chơi "Chủ nhân- Hầu gái" chứ.
Dialogue: 0,0:16:33.97,0:16:34.48,handwriting 4,,0000,0000,0000,,Im lặng nào!
Dialogue: 0,0:16:36.26,0:16:40.59,handwriting 4,,0000,0000,0000,,Em có thể cảm thấy thằng nhỏ của chủ nhân đang đâm vào màng trinh của em.
Dialogue: 0,0:16:43.67,0:16:45.82,handwriting 4,,0000,0000,0000,,Chuyện này đau hơn là em tưởng nhiều.
Dialogue: 0,0:16:46.50,0:16:47.68,handwriting 4,,0000,0000,0000,,Thôi bỏ đi, cô đâu cần phải.
Dialogue: 0,0:16:47.80,0:16:52.61,handwriting 4,,0000,0000,0000,,Ko, với tư cách là hầu gái, em phải khiến chủ nhân thỏa mãn.
Dialogue: 0,0:16:54.32,0:16:56.28,handwriting 4,,0000,0000,0000,,Vậy trong mấy trò đó, hầu gái và chủ nhân đều làm thế này hả?
Dialogue: 0,0:16:56.28,0:17:01.95,handwriting 4,,0000,0000,0000,,Nhiệm vụ của em chỉ hoàn thành khi chủ nhân thực sự ra.
Dialogue: 0,0:17:11.82,0:17:12.65,handwriting 4,,0000,0000,0000,,Shiraha...
Dialogue: 0,0:17:14.58,0:17:17.98,handwriting 4,,0000,0000,0000,,Cậu thấy thế nào? Cậu thích âm hộ của em chứ?
Dialogue: 0,0:17:18.98,0:17:20.79,handwriting 4,,0000,0000,0000,,Nếu chủ nhân ko cảm thấy thỏa mãn...
Dialogue: 0,0:17:20.79,0:17:21.42,handwriting 4,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:17:25.94,0:17:28.09,handwriting 4,,0000,0000,0000,,Em chỉ muốn khiến cậu sung sướng...
Dialogue: 0,0:17:29.41,0:17:31.96,handwriting 4,,0000,0000,0000,,Bên trong Shiraha thật ấm...
Dialogue: 0,0:17:32.30,0:17:34.40,handwriting 4,,0000,0000,0000,,Ko ngờ nó lại khít đến vậy...
Dialogue: 0,0:17:38.07,0:17:39.14,handwriting 4,,0000,0000,0000,,Chủ nhân sao vậy?
Dialogue: 0,0:17:39.61,0:17:44.66,handwriting 4,,0000,0000,0000,,Hãy ra hết vào bên trong em, Chủ nhân...
Dialogue: 0,0:17:47.47,0:17:48.43,handwriting 4,,0000,0000,0000,,Nếu cô tiếp tục làm vậy...
Dialogue: 0,0:17:59.62,0:18:06.90,handwriting 4,,0000,0000,0000,,Ở bên trong... cậu ra nhiều quá, Chủ nhân..
Dialogue: 0,0:18:18.66,0:18:20.81,handwriting 4,,0000,0000,0000,,Thứ này là.. tinh dịch...
Dialogue: 0,0:18:32.07,0:18:34.17,handwriting 4,,0000,0000,0000,,Đây là.. tinh dịch của chủ nhân...
Dialogue: 0,0:18:36.20,0:18:37.28,handwriting 4,,0000,0000,0000,,Nhiệm vụ đã hoàn thành.
Dialogue: 0,0:18:39.89,0:18:40.21,handwriting 4,,0000,0000,0000,,Này...
Dialogue: 0,0:18:41.85,0:18:42.48,handwriting 4,,0000,0000,0000,,Shiraha...
Dialogue: 0,0:18:44.93,0:18:46.10,handwriting 4,,0000,0000,0000,,Cởi trói cho tôi...
Dialogue: 0,0:18:50.69,0:18:52.67,handwriting 4,,0000,0000,0000,,Cởi trói cho tôi đã rồi hãy mặc quần áo nào.
Dialogue: 0,0:19:01.18,0:19:03.47,handwriting 4,,0000,0000,0000,,Shiraha! Đợi đã...
Dialogue: 0,0:19:04.18,0:19:05.84,handwriting 4,,0000,0000,0000,,Đừng có tắt đèn rồi bỏ đi như vậy chứ..
Dialogue: 0,0:19:05.94,0:19:07.51,handwriting 4,,0000,0000,0000,,Shiraha..!
Dialogue: 0,0:19:13.13,0:19:13.88,handwriting 4,,0000,0000,0000,,Gì vậy?
Dialogue: 0,0:19:13.88,0:19:16.45,handwriting 4,,0000,0000,0000,,Laila-san! Cứu mình với!
Dialogue: 0,0:19:17.67,0:19:18.31,handwriting 4,,0000,0000,0000,,Philine!
Dialogue: 0,0:19:20.02,0:19:22.27,handwriting 4,,0000,0000,0000,,Cô đang làm gì vậy.. Yonis?!
Dialogue: 0,0:19:22.73,0:19:25.03,handwriting 4,,0000,0000,0000,,Cô ko thấy sao? Tôi đang kích thích chị ấy.
Dialogue: 0,0:19:25.20,0:19:30.11,handwriting 4,,0000,0000,0000,,Tôi phải làm chuyện này trước khi gã chủ nhân ngu ngốc của các người đụng tới chị ấy.
Dialogue: 0,0:19:31.75,0:19:35.22,handwriting 4,,0000,0000,0000,,Yonis-san, nghe lời chị quay về trường đi mà..
Dialogue: 0,0:19:35.63,0:19:39.18,handwriting 4,,0000,0000,0000,,Em sẽ ko làm như vậy đâu, Onee-sama!
Dialogue: 0,0:19:40.23,0:19:44.07,handwriting 4,,0000,0000,0000,,Hãy cho em trinh tiết của chị, Onee-sama..
Dialogue: 0,0:19:44.55,0:19:45.73,handwriting 4,,0000,0000,0000,,Ko..!
Dialogue: 0,0:19:56.89,0:19:59.36,handwriting 4,,0000,0000,0000,,Gì thế? Gì thế? Súng ư? Sao lại như vậy?
Dialogue: 0,0:19:59.36,0:20:00.05,handwriting 4,,0000,0000,0000,,Xem đây..
Dialogue: 0,0:20:01.61,0:20:02.39,handwriting 4,,0000,0000,0000,,Đồ rác rưởi...
Dialogue: 0,0:20:02.64,0:20:05.96,handwriting 4,,0000,0000,0000,,Đợi đã, tôi là.. Yonis đây mà!
Dialogue: 0,0:20:06.74,0:20:07.96,handwriting 4,,0000,0000,0000,,Tôi biết.
Dialogue: 0,0:20:38.34,0:20:39.88,handwriting 4,,0000,0000,0000,,Chuyện gì vậy? Sao ở đây ầm ĩ vậy?
Dialogue: 0,0:20:40.98,0:20:42.72,handwriting 4,,0000,0000,0000,,Chuyện gì đã xảy ra vậy?
Dialogue: 0,0:20:42.89,0:20:44.23,handwriting 4,,0000,0000,0000,,Sao cô ta lại ở đây trong lúc đêm khuya thế này?
Dialogue: 0,0:20:44.84,0:20:45.33,handwriting 4,,0000,0000,0000,,Akit..
Dialogue: 0,0:20:45.40,0:20:45.89,handwriting 4,,0000,0000,0000,,Philine.
Dialogue: 0,0:20:46.58,0:20:47.89,handwriting 4,,0000,0000,0000,,Philine.. Cô ko sao chứ?
Dialogue: 0,0:20:48.12,0:20:49.00,handwriting 4,,0000,0000,0000,,Chủ nhân!
Dialogue: 0,0:20:49.07,0:20:49.80,handwriting 4,,0000,0000,0000,,Bình tĩnh nào.
Dialogue: 0,0:20:50.73,0:20:51.78,handwriting 4,,0000,0000,0000,,Mọi chuyện ổn rồi.
Dialogue: 0,0:20:52.34,0:20:53.30,handwriting 4,,0000,0000,0000,,Akito..
Dialogue: 0,0:20:56.47,0:20:58.26,handwriting 4,,0000,0000,0000,,Akito.. gã ngốc!
Dialogue: 0,0:21:02.24,0:21:03.85,handwriting 4,,0000,0000,0000,,Cô ấy sao thế nhỉ?
Dialogue: 0,0:21:04.42,0:21:06.27,handwriting 4,,0000,0000,0000,,Chủ nhân, đuổi theo cô ấy mau.
Dialogue: 0,0:21:06.86,0:21:07.25,handwriting 4,,0000,0000,0000,,Nhưng mà..
Dialogue: 0,0:21:07.35,0:21:10.38,handwriting 4,,0000,0000,0000,,Laila-san luôn mong muốn được làm chuyện đó cùng cậu.
Dialogue: 0,0:21:13.04,0:21:13.60,handwriting 4,,0000,0000,0000,,Hiểu rồi.
Dialogue: 0,0:21:24.48,0:21:24.99,handwriting 4,,0000,0000,0000,,Laila..
Dialogue: 0,0:21:25.31,0:21:26.99,handwriting 4,,0000,0000,0000,,Cậu có trong đây ko?
Dialogue: 0,0:21:34.89,0:21:36.28,handwriting 4,,0000,0000,0000,,X..Xin..Xin lỗi!
Dialogue: 0,0:21:38.68,0:21:43.52,handwriting 4,,0000,0000,0000,,Lại thế rồi, mình chỉ toàn đưa Laila-san và tình thế khó xử.
Dialogue: 0,0:21:51.46,0:21:53.46,handwriting 4,,0000,0000,0000,,Akito.. cậu vào được rồi đó.
Dialogue: 0,0:21:54.49,0:21:55.05,handwriting 4,,0000,0000,0000,,Cậu chắc chứ?
Dialogue: 0,0:21:55.29,0:21:57.05,handwriting 4,,0000,0000,0000,,Ko nghe tôi nói sao, xong cả rồi?
Dialogue: 0,0:21:57.49,0:21:58.08,handwriting 4,,0000,0000,0000,,Uhm..
Dialogue: 0,0:21:59.20,0:21:59.96,handwriting 4,,0000,0000,0000,,Tôi vào đây.
Dialogue: 0,0:22:03.09,0:22:04.58,handwriting 4,,0000,0000,0000,,Laila! Bộ quần áo này..
Dialogue: 0,0:22:05.05,0:22:08.44,handwriting 4,,0000,0000,0000,,Tôi nghĩ mặc nó lúc này là hợp nhất.
Dialogue: 0,0:22:10.13,0:22:12.65,handwriting 4,,0000,0000,0000,,Đó là bộ quần áo mà chúng ta cùng nhau ...
Dialogue: 0,0:22:12.91,0:22:15.60,handwriting 4,,0000,0000,0000,,Phải, mặc dù nó chưa bao giờ được hoàn thành ...
Dialogue: 0,0:22:18.31,0:22:19.15,handwriting 4,,0000,0000,0000,,Nhìn tôi nè!
Dialogue: 0,0:22:25.01,0:22:26.67,handwriting 4,,0000,0000,0000,,Cậu hết giận rồi sao?
Dialogue: 0,0:22:26.67,0:22:27.94,handwriting 4,,0000,0000,0000,,Tất nhiên là tôi vẫn còn giận..
Dialogue: 0,0:22:30.02,0:22:34.49,handwriting 4,,0000,0000,0000,,Nhưng mà, nói thật, đâu có lí do gì khiến tôi phải giận chứ.
Dialogue: 0,0:22:34.57,0:22:35.84,handwriting 4,,0000,0000,0000,,Vậy à..
Dialogue: 0,0:22:35.93,0:22:37.86,handwriting 4,,0000,0000,0000,,Phải, bởi vì tôi là một hầu gái..
Dialogue: 0,0:22:38.89,0:22:40.80,handwriting 4,,0000,0000,0000,,Và cậu thì lại là chủ nhân của tôi.
Dialogue: 0,0:22:41.97,0:22:44.95,handwriting 4,,0000,0000,0000,,Dù vậy, cậu vẫn đến đây tìm tôi.
Dialogue: 0,0:22:46.59,0:22:47.20,handwriting 4,,0000,0000,0000,,Tôi thật khiếm nhã.
Dialogue: 0,0:22:47.35,0:22:47.86,handwriting 4,,0000,0000,0000,,Laila..
Dialogue: 0,0:22:48.54,0:22:49.86,handwriting 4,,0000,0000,0000,,Đó là bởi vì.. tôi..
Dialogue: 0,0:23:00.10,0:23:03.57,handwriting 4,,0000,0000,0000,,Tôi ko quan tâm mối quan hệ của chúng ta sẽ trở nên ra sao, hầu gái và chủ nhân .
Dialogue: 0,0:23:04.60,0:23:05.57,handwriting 4,,0000,0000,0000,,Vì thế mà tôi..
Dialogue: 0,0:23:14.69,0:23:17.70,handwriting 4,,0000,0000,0000,,Ko bự bằng Philine, nhưng vẫn rất bự.
Dialogue: 0,0:23:18.75,0:23:20.51,handwriting 4,,0000,0000,0000,,Đừng có so sánh tôi với Philine.
Dialogue: 0,0:23:21.56,0:23:24.03,handwriting 4,,0000,0000,0000,,Bự hơn Akari và có lẽ là cả Shiraha.
Dialogue: 0,0:23:25.15,0:23:26.23,handwriting 4,,0000,0000,0000,,Ko..
Dialogue: 0,0:23:26.74,0:23:28.21,handwriting 4,,0000,0000,0000,,Đừng nhắc tới bọn họ ở đây..
Dialogue: 0,0:23:30.38,0:23:30.92,handwriting 4,,0000,0000,0000,,Tên ngốc..
Dialogue: 0,0:23:32.48,0:23:33.92,handwriting 4,,0000,0000,0000,,Cậu vẫn chẳng hề thay đổi chút nào cả!
Dialogue: 0,0:23:36.61,0:23:38.76,handwriting 4,,0000,0000,0000,,Akito.. nơi này..
Dialogue: 0,0:23:39.42,0:23:41.28,handwriting 4,,0000,0000,0000,,Đừng lo, tôi sẽ đối xử nhẹ nhàng với cậu..
Dialogue: 0,0:23:42.28,0:23:43.28,handwriting 4,,0000,0000,0000,,Nhưng mà..
Dialogue: 0,0:23:46.75,0:23:50.79,handwriting 4,,0000,0000,0000,,Tay của Akito đang nắm trọn ngực của mình...
Dialogue: 0,0:23:58.07,0:24:01.05,handwriting 4,,0000,0000,0000,,Ngay cả trong mơ tôi cũng ko nghĩ mình có thể làm chuyện này với cậu, Laila..
Dialogue: 0,0:24:04.15,0:24:08.04,handwriting 4,,0000,0000,0000,,Tôi cũng vậy, tôi chưa bao giờ nghĩ mình lại làm chuyện này cùng cậu, Akito...
Dialogue: 0,0:24:10.21,0:24:11.17,handwriting 4,,0000,0000,0000,,Cậu ướt lắm rồi này..
Dialogue: 0,0:24:11.46,0:24:13.95,handwriting 4,,0000,0000,0000,,Ước gì cậu luôn như thế này, lúc này trông cậu thật dễ thương..
Dialogue: 0,0:24:14.42,0:24:15.93,handwriting 4,,0000,0000,0000,,Xin lỗi vì suốt thời gian qua tôi đã ko khiến cậu vừa lòng!
Dialogue: 0,0:24:21.09,0:24:27.98,handwriting 4,,0000,0000,0000,,A..Akito..đừng.. Tôi sẽ.. ra mất...
Dialogue: 0,0:24:28.86,0:24:31.01,handwriting 4,,0000,0000,0000,,Nếu cậu tiếp tục làm vậy, tôi sẽ ra mất..!
Dialogue: 0,0:24:34.80,0:24:35.68,handwriting 4,,0000,0000,0000,,Giờ thì..
Dialogue: 0,0:24:40.66,0:24:41.23,handwriting 4,,0000,0000,0000,,Tôi vào đây.
Dialogue: 0,0:24:46.36,0:24:49.05,handwriting 4,,0000,0000,0000,,Laila! Đây là lần đầu tiên của cậu sao?
Dialogue: 0,0:24:49.09,0:24:50.51,handwriting 4,,0000,0000,0000,,Tất nhiên rồi!
Dialogue: 0,0:24:52.32,0:24:54.71,handwriting 4,,0000,0000,0000,,Cậu sao vậy?
Dialogue: 0,0:24:55.01,0:24:57.74,handwriting 4,,0000,0000,0000,,Cậu vừa cướp đi trinh tiết của tôi..
Dialogue: 0,0:24:58.89,0:25:01.61,handwriting 4,,0000,0000,0000,,Và chúng ta cũng chỉ vừa mới bắt đầu, cậu ko định dừng lại đấy chứ.
Dialogue: 0,0:25:01.61,0:25:02.78,handwriting 4,,0000,0000,0000,,Vậy thì cậu quay người lại nào..
Dialogue: 0,0:25:02.97,0:25:05.71,handwriting 4,,0000,0000,0000,,Nhẹ nhàng thôi nhé..
Dialogue: 0,0:25:06.62,0:25:07.81,handwriting 4,,0000,0000,0000,,Nào..
Dialogue: 0,0:25:08.25,0:25:09.57,handwriting 4,,0000,0000,0000,,Bắt đầu đi nào.
Dialogue: 0,0:25:09.77,0:25:11.28,handwriting 4,,0000,0000,0000,,Tôi làm đây.
Dialogue: 0,0:25:35.57,0:25:36.53,handwriting 4,,0000,0000,0000,,Akito!
Dialogue: 0,0:25:41.10,0:25:41.73,handwriting 4,,0000,0000,0000,,Điều này..
Dialogue: 0,0:25:41.98,0:25:44.03,handwriting 4,,0000,0000,0000,,Tuyệt quá đi mất..
Dialogue: 0,0:25:44.66,0:25:46.96,handwriting 4,,0000,0000,0000,,Cậu thấy sao, Akito?
Dialogue: 0,0:25:48.57,0:25:51.95,handwriting 4,,0000,0000,0000,,Tôi đã rất sung sướng khi biết đó là lần đầu tiên của cậu..
Dialogue: 0,0:25:52.14,0:25:54.07,handwriting 4,,0000,0000,0000,,có nghĩa tôi chính là người đầu tiên của cậu.
Dialogue: 0,0:25:57.20,0:25:59.57,handwriting 4,,0000,0000,0000,,Tôi ko phiền đâu, bởi vì người đó là Akito..
Dialogue: 0,0:26:01.01,0:26:02.11,handwriting 4,,0000,0000,0000,,Akito..
Dialogue: 0,0:26:02.80,0:26:03.53,handwriting 4,,0000,0000,0000,,Akito!
Dialogue: 0,0:26:03.82,0:26:04.85,handwriting 4,,0000,0000,0000,,Akito!
Dialogue: 0,0:26:16.43,0:26:17.12,handwriting 4,,0000,0000,0000,,Akito!
Dialogue: 0,0:26:18.32,0:26:19.37,handwriting 4,,0000,0000,0000,,Tôi ra đây, tôi ra đây!
Dialogue: 0,0:26:19.37,0:26:22.57,handwriting 4,,0000,0000,0000,,Tôi ra đây...!
Dialogue: 0,0:26:36.50,0:26:38.06,handwriting 4,,0000,0000,0000,,Thật sự rất tuyệt..
Dialogue: 0,0:26:38.50,0:26:40.68,handwriting 4,,0000,0000,0000,,Tôi vẫn chưa thể tin đây là lần đầu tiên của cậu.
Dialogue: 0,0:26:41.38,0:26:46.71,handwriting 4,,0000,0000,0000,,Nếu tiếp tục như vậy, cậu nhất định sẽ được đánh giá cao hơn hẳn những người khác.
Dialogue: 0,0:26:48.35,0:26:52.33,handwriting 4,,0000,0000,0000,,Cậu sẽ có cơ hội lớn để trở thành hầu gái phục vụ ban đêm cừ nhất.
Dialogue: 0,0:26:53.04,0:26:54.19,handwriting 4,,0000,0000,0000,,Akito...!
Dialogue: 0,0:26:55.75,0:26:57.10,handwriting 4,,0000,0000,0000,,Đồ ngốc..!
Dialogue: 0,0:27:05.21,0:27:05.70,handwriting 4,,0000,0000,0000,,S..Sao vậy..?
Dialogue: 0,0:27:09.51,0:27:13.57,handwriting 4,,0000,0000,0000,,Làm cách nào để huấn luyện họ thành thạo chỉ trong vòng một năm đây hả trời ?!
Dialogue: 0,0:27:19.58,0:27:31.68,handwriting 4,,0000,0000,0000,,{\pos(238,75)} Translator : Invincible Dark Thunder!
