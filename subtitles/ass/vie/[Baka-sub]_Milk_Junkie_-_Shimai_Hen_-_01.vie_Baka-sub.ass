Dialogue: 0,0:00:02.33,0:00:12.82,Default,,0000,0000,0000,,Project thuộc về website : Baka-sub.net
Dialogue: 0,0:00:39.17,0:00:39.79,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:00:43.67,0:00:44.93,Default,,0000,0000,0000,,Cùng chơi nào, Wataru.
Dialogue: 0,0:00:53.68,0:00:56.34,Default,,0000,0000,0000,,Ông xã, mình ra đó luôn nhé.
Dialogue: 0,0:00:56.52,0:00:58.42,Default,,0000,0000,0000,,Uhm, ra chứ.
Dialogue: 0,0:00:59.79,0:01:02.05,Default,,0000,0000,0000,,Wataru, lùi lại một chút nữa nào.
Dialogue: 0,0:01:02.12,0:01:03.32,Default,,0000,0000,0000,,Oh, OK!
Dialogue: 0,0:01:06.49,0:01:07.30,Default,,0000,0000,0000,,Đứng ở đây được chưa?
Dialogue: 0,0:01:07.30,0:01:09.72,Default,,0000,0000,0000,,Cho em xem với nào.
Dialogue: 0,0:01:10.83,0:01:11.80,Default,,0000,0000,0000,,Mình bắt đầu nhé!
Dialogue: 0,0:01:16.24,0:01:17.90,Default,,0000,0000,0000,,Đánh hay lắm, Wataru.
Dialogue: 0,0:01:18.79,0:01:19.76,Default,,0000,0000,0000,,{\pos(320,435)}May mắn thôi mà.
Dialogue: 0,0:01:19.33,0:01:24.21,Default,,0000,0000,0000,,Wataru-chan...mất trí nhớ thật sao?
Dialogue: 0,0:01:26.35,0:01:29.11,Default,,0000,0000,0000,,Cha cậu ấy đã khẳng định điều đó với chị.
Dialogue: 0,0:01:29.45,0:01:32.68,Default,,0000,0000,0000,,Nhưng lúc này trông cậu ấy ko giống người mất trí nhớ chút nào cả.
Dialogue: 0,0:01:33.42,0:01:35.91,Default,,0000,0000,0000,,Rõ ràng cậu ấy ko hề có biểu hiện gì khác thường.
Dialogue: 0,0:01:36.12,0:01:37.89,Default,,0000,0000,0000,,Mình thấy nóng người rồi đấy.
Dialogue: 0,0:01:39.16,0:01:40.15,Default,,0000,0000,0000,,Đỡ này!
Dialogue: 0,0:01:42.80,0:01:45.89,Default,,0000,0000,0000,,Lúc này cậu ấy sôi nổi hơn hẳn trước kia.
Dialogue: 0,0:01:48.70,0:01:49.83,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:01:50.94,0:01:52.63,Default,,0000,0000,0000,,Ngực cô ta phẳng lì!
Dialogue: 0,0:01:52.71,0:01:54.70,Default,,0000,0000,0000,,Huh, ý chị là sao?
Dialogue: 0,0:01:55.24,0:01:56.30,Default,,0000,0000,0000,,Thấy ko?
Dialogue: 0,0:01:56.61,0:01:58.64,Default,,0000,0000,0000,,Mình sẽ ko nương tay với cậu nữa đâu.
Dialogue: 0,0:02:00.92,0:02:02.51,Default,,0000,0000,0000,,A-cup!?
Dialogue: 0,0:02:02.98,0:02:06.55,Default,,0000,0000,0000,,Sao kì vậy, chẳng phải Wataru là một gã mê ngực sao!?
Dialogue: 0,0:02:11.03,0:02:12.86,Default,,0000,0000,0000,,Vậy có nghĩa là...
Dialogue: 0,0:02:13.56,0:02:15.26,Default,,0000,0000,0000,,Mất trí nhớ tạm thời!
Dialogue: 0,0:02:17.03,0:02:18.50,Default,,0000,0000,0000,,Wataru?
Dialogue: 0,0:02:19.40,0:02:22.60,Default,,0000,0000,0000,,Được thôi, chúng ta sẽ phải dùng đến bộ ngực của mình!
Dialogue: 0,0:02:23.77,0:02:26.57,Default,,0000,0000,0000,,Để giúp Wataru-kun hồi phục trí nhớ!
Dialogue: 0,0:02:34.18,0:02:37.55,Default,,0000,0000,0000,,Uhm, cần phải bàn bạc tỉ mỉ kế hoạch hành động vào ngày mai.
Dialogue: 0,0:02:37.85,0:02:40.84,Default,,0000,0000,0000,,Đừng quên chúng ta đang ở bờ biển California...
Dialogue: 0,0:02:42.19,0:02:43.21,Default,,0000,0000,0000,,Đây!
Dialogue: 0,0:02:43.29,0:02:44.55,Default,,0000,0000,0000,,Và đây nữa!
Dialogue: 0,0:03:56.73,0:03:59.96,Default,,0000,0000,0000,,Rena, con hướng dẫn Wataru cách vắt sữa bò nhé.
Dialogue: 0,0:04:00.03,0:04:01.52,Default,,0000,0000,0000,,Vâng, cha cứ yên tâm.
Dialogue: 0,0:04:06.04,0:04:06.84,Default,,0000,0000,0000,,Wataru.
Dialogue: 0,0:04:07.34,0:04:08.31,Default,,0000,0000,0000,,Đến giờ làm việc rồi.
Dialogue: 0,0:04:08.84,0:04:10.07,Default,,0000,0000,0000,,R, Rõ!
Dialogue: 0,0:04:18.85,0:04:20.95,Default,,0000,0000,0000,,Nào, cậu thử bắt đầu với "ả" này xem.
Dialogue: 0,0:04:21.02,0:04:21.78,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:04:27.10,0:04:28.72,Default,,0000,0000,0000,,Xin lỗi vì đã kêu cậu hơi sớm.
Dialogue: 0,0:04:29.60,0:04:31.36,Default,,0000,0000,0000,,Mình ko cố ý làm cậu giật mình đâu.
Dialogue: 0,0:04:32.83,0:04:35.46,Default,,0000,0000,0000,,Ko sao đâu mà, đừng bận tâm.
Dialogue: 0,0:04:35.64,0:04:37.00,Default,,0000,0000,0000,,Ồ, vậy à...
Dialogue: 0,0:04:44.31,0:04:47.01,Default,,0000,0000,0000,,Cháu sẽ...Cháu sẽ chịu trách nhiệm về chuyện này.
Dialogue: 0,0:04:47.95,0:04:52.41,Default,,0000,0000,0000,,Vả lại, biết đâu khi ở lại đây, trí nhớ cậu ấy lại có cơ may hồi phục thì sao!
Dialogue: 0,0:04:53.76,0:04:56.32,Default,,0000,0000,0000,,Làm ơn đi bác, hãy để cháu chăm sóc cậu ấy!
Dialogue: 0,0:05:07.67,0:05:09.64,Default,,0000,0000,0000,,Ko phải như vậy đâu, Wataru.
Dialogue: 0,0:05:09.70,0:05:11.67,Default,,0000,0000,0000,,Cậu bóp mạnh tay quá.
Dialogue: 0,0:05:11.91,0:05:13.00,Default,,0000,0000,0000,,Ờ, ờ.
Dialogue: 0,0:05:15.74,0:05:20.04,Default,,0000,0000,0000,,Thả lỏng tay và bóp nhẹ hơn chút nữa...vậy nhé?
Dialogue: 0,0:05:22.12,0:05:23.52,Default,,0000,0000,0000,,Ờ...
Dialogue: 0,0:05:28.56,0:05:31.12,Default,,0000,0000,0000,,Đúng rồi, là như vậy đó, cậu làm tốt lắm.
Dialogue: 0,0:05:33.93,0:05:35.26,Default,,0000,0000,0000,,Cậu cừ thật, Wataru.
Dialogue: 0,0:05:36.26,0:05:39.20,Default,,0000,0000,0000,,Chắc là cậu đã làm điều này với em nào rồi phải ko?!
Dialogue: 0,0:05:39.73,0:05:40.36,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:05:44.17,0:05:45.94,Default,,0000,0000,0000,,Đùa chút thôi, là đùa thôi mà.
Dialogue: 0,0:05:52.61,0:05:54.74,Default,,0000,0000,0000,,Uhm, cậu cứ tiếp tục cho đến khi đầy xô nhé.
Dialogue: 0,0:05:54.88,0:05:57.41,Default,,0000,0000,0000,,Mình phải đi lấy thêm cỏ cho Ed.
Dialogue: 0,0:05:59.92,0:06:02.55,Default,,0000,0000,0000,,Rena đúng là một cô gái tốt.
Dialogue: 0,0:06:10.16,0:06:12.39,Default,,0000,0000,0000,,Ấy chết, phải tập trung vào công việc mới được.
Dialogue: 0,0:06:15.34,0:06:17.30,Default,,0000,0000,0000,,Oái, đây mà là vú bò sao...?
Dialogue: 0,0:06:22.11,0:06:24.08,Default,,0000,0000,0000,,Vắt sữa em đi nào, cưng.
Dialogue: 0,0:06:27.01,0:06:28.48,Default,,0000,0000,0000,,Mạnh tay vào nhé...
Dialogue: 0,0:06:30.05,0:06:31.02,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:06:34.46,0:06:37.52,Default,,0000,0000,0000,,Tốt hơn cả là cậu nên ngoan ngoãn nghe lời chị ấy đi.
Dialogue: 0,0:06:39.76,0:06:40.96,Default,,0000,0000,0000,,Nào...
Dialogue: 0,0:06:41.30,0:06:42.42,Default,,0000,0000,0000,,Quyết định vậy nhé!
Dialogue: 0,0:06:46.07,0:06:48.54,Default,,0000,0000,0000,,Lại đây nào Wataru-kun, vắt sữa bọn em đi cưng.
Dialogue: 0,0:06:48.60,0:06:51.40,Default,,0000,0000,0000,,Phải đó, Wataru-chan, mau lên nào.
Dialogue: 0,0:06:51.51,0:06:53.13,Default,,0000,0000,0000,,Nào, Wataru-kun.
Dialogue: 0,0:06:53.24,0:06:54.57,Default,,0000,0000,0000,,Wataru-chan.
Dialogue: 0,0:06:54.64,0:06:55.84,Default,,0000,0000,0000,,Wataru-kun.
Dialogue: 0,0:06:55.91,0:06:57.47,Default,,0000,0000,0000,,Wataru-chan.
Dialogue: 0,0:06:57.55,0:06:58.91,Default,,0000,0000,0000,,Wataru-kun.
Dialogue: 0,0:06:58.98,0:07:00.50,Default,,0000,0000,0000,,Wataru-chan.
Dialogue: 0,0:07:00.58,0:07:01.91,Default,,0000,0000,0000,,Wataru-kun.
Dialogue: 0,0:07:01.98,0:07:03.21,Default,,0000,0000,0000,,Wataru-chan.
Dialogue: 0,0:07:03.28,0:07:04.44,Default,,0000,0000,0000,,Wataru-kun.
Dialogue: 0,0:07:04.85,0:07:06.15,Default,,0000,0000,0000,,Wataru-chan.
Dialogue: 0,0:07:06.16,0:07:08.69,Default,,0000,0000,0000,,Hơ, như này là sao....?
Dialogue: 0,0:07:25.31,0:07:28.07,Default,,0000,0000,0000,,Phải rồi, làm tốt lắm, Wataru-chan.
Dialogue: 0,0:07:28.14,0:07:32.27,Default,,0000,0000,0000,,Wataru-kun...cừ quá .
Dialogue: 0,0:07:32.45,0:07:33.61,Default,,0000,0000,0000,,Wataru-chan.
Dialogue: 0,0:07:40.25,0:07:41.48,Default,,0000,0000,0000,,Hai cô làm trò gì vậy hả!?
Dialogue: 0,0:07:41.59,0:07:43.11,Default,,0000,0000,0000,,Tránh xa Wataru mau!
Dialogue: 0,0:07:49.13,0:07:53.16,Default,,0000,0000,0000,,Hai cô là ai?
Dialogue: 0,0:07:53.97,0:07:56.96,Default,,0000,0000,0000,,Hình như các cô quen...Wataru?
Dialogue: 0,0:07:59.34,0:08:02.80,Default,,0000,0000,0000,,Người Nhật Bản...và cả bộ ngực kia nữa!?
Dialogue: 0,0:08:06.91,0:08:09.58,Default,,0000,0000,0000,,Hai cô là bạn của Wataru...?
Dialogue: 0,0:08:13.75,0:08:15.35,Default,,0000,0000,0000,,Hóa ra hai cô là...
Dialogue: 0,0:08:25.97,0:08:31.27,Default,,0000,0000,0000,,Wataru bị như vậy là do lỗi của tôi...
Dialogue: 0,0:08:36.41,0:08:39.04,Default,,0000,0000,0000,,Hiểu rồi, nói thật là tôi cũng chưa rõ chuyện gì đã xảy ra.
Dialogue: 0,0:08:39.65,0:08:43.91,Default,,0000,0000,0000,,Nhưng tại sao cô ko để chúng tôi nói chuyện với Wataru-kun xem sao?
Dialogue: 0,0:08:46.05,0:08:49.25,Default,,0000,0000,0000,,Thế nhé, cô tạm rời khỏi đây một lúc được ko?
Dialogue: 0,0:08:50.42,0:08:52.95,Default,,0000,0000,0000,,Đến lúc nhường lại việc này cho "người lớn" rồi.
Dialogue: 0,0:08:55.70,0:09:00.03,Default,,0000,0000,0000,,Người ngây thơ như cô ko nên chứng kiến chuyện này.
Dialogue: 0,0:09:03.91,0:09:04.96,Default,,0000,0000,0000,,Hẹn gặp lại cô sau nhé.
Dialogue: 0,0:09:05.54,0:09:06.67,Default,,0000,0000,0000,,Này, đợi đã!
Dialogue: 0,0:09:07.71,0:09:09.01,Default,,0000,0000,0000,,Đừng làm phiền chúng tôi đấy.
Dialogue: 0,0:09:11.08,0:09:11.91,Default,,0000,0000,0000,,Rena...
Dialogue: 0,0:09:13.08,0:09:14.51,Default,,0000,0000,0000,,Nào, Wataru-kun...
Dialogue: 0,0:09:14.65,0:09:16.71,Default,,0000,0000,0000,,Đến giờ "chữa bệnh" rồi.
Dialogue: 0,0:09:17.59,0:09:18.45,Default,,0000,0000,0000,,Chữa bệnh ư?
Dialogue: 0,0:09:19.85,0:09:21.48,Default,,0000,0000,0000,,Phải, chữa bệnh.
Dialogue: 0,0:09:21.59,0:09:23.82,Default,,0000,0000,0000,,Hay nói cách khác: hàn gắn tổn thương.
Dialogue: 0,0:09:24.59,0:09:27.36,Default,,0000,0000,0000,,Và bây giờ sẽ là phương pháp trị liệu đặc biệt dành cho cưng.
Dialogue: 0,0:09:27.50,0:09:29.96,Default,,0000,0000,0000,,"Liệu pháp kích thích".
Dialogue: 0,0:09:34.40,0:09:36.27,Default,,0000,0000,0000,,Này, còn nhớ tấm hình này ko?
Dialogue: 0,0:09:36.37,0:09:39.07,Default,,0000,0000,0000,,Phải rồi, khi đó chúng ta đã....
Dialogue: 0,0:09:39.87,0:09:42.74,Default,,0000,0000,0000,,Uhm, đúng rồi...
Dialogue: 0,0:09:44.78,0:09:50.18,Default,,0000,0000,0000,,Bọn em sẽ giúp cưng nhớ lại khoảnh khắc đó.
Dialogue: 0,0:09:50.25,0:09:51.95,Default,,0000,0000,0000,,Nào Wataru-kun....
Dialogue: 0,0:09:53.45,0:09:54.82,Default,,0000,0000,0000,,N, nhưng mà...
Dialogue: 0,0:09:55.19,0:09:57.12,Default,,0000,0000,0000,,Cưng biết phải làm gì mà, Wataru-chan.
Dialogue: 0,0:09:57.19,0:09:58.78,Default,,0000,0000,0000,,Giờ ta trị liệu tiếp nhé.
Dialogue: 0,0:10:02.03,0:10:02.96,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:10:04.47,0:10:06.99,Default,,0000,0000,0000,,Thằng nhỏ của cưng bự lên ghê quá.
Dialogue: 0,0:10:12.87,0:10:14.10,Default,,0000,0000,0000,,Em bắt đầu nhé.
Dialogue: 0,0:10:15.41,0:10:17.00,Default,,0000,0000,0000,,Cả em nữa.
Dialogue: 0,0:10:22.55,0:10:25.68,Default,,0000,0000,0000,,Thế nào, cưng đã nhớ ra mình từng rất thích điều này chưa?
Dialogue: 0,0:10:26.95,0:10:28.98,Default,,0000,0000,0000,,Bự quá đi mất.
Dialogue: 0,0:10:39.50,0:10:42.59,Default,,0000,0000,0000,,Sao hả Wataru-kun, nó có tác dụng chứ?
Dialogue: 0,0:10:43.30,0:10:45.60,Default,,0000,0000,0000,,Có lẽ cậu ấy được trị liệu nhiều hơn nữa.
Dialogue: 0,0:10:48.74,0:10:50.68,Default,,0000,0000,0000,,Chà, vậy thì sao chúng ta ko thưởng thức ''nó'' một chút nhỉ?
Dialogue: 0,0:10:50.75,0:10:51.80,Default,,0000,0000,0000,,Phải đó.
Dialogue: 0,0:10:58.35,0:11:00.29,Default,,0000,0000,0000,,T, tuyệt quá....
Dialogue: 0,0:11:06.69,0:11:08.72,Default,,0000,0000,0000,,Thế này được chưa nhỉ?
Dialogue: 0,0:11:10.60,0:11:13.40,Default,,0000,0000,0000,,Ôi, em ướt hết rồi...
Dialogue: 0,0:11:22.41,0:11:26.31,Default,,0000,0000,0000,,Nếu đã vậy, chúng ta buộc phải chuyển sang bước trị liệu cuối cùng thôi.
Dialogue: 0,0:11:26.91,0:11:28.28,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:11:30.08,0:11:33.14,Default,,0000,0000,0000,,Này Tomoka, em trước đi.
Dialogue: 0,0:11:33.49,0:11:36.01,Default,,0000,0000,0000,,Rõ, thưa bác sĩ Tình ái.
Dialogue: 0,0:11:37.46,0:11:40.43,Default,,0000,0000,0000,,Đừng đùa nữa, tập trung vào chuyên môn đi nào.
Dialogue: 0,0:11:40.70,0:11:41.56,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:11:43.36,0:11:44.59,Default,,0000,0000,0000,,Cứng quá...
Dialogue: 0,0:12:02.05,0:12:05.42,Default,,0000,0000,0000,,Tuyệt quá, ''nó'' vẫn chẳng hề thay đổi chút nào.
Dialogue: 0,0:12:09.32,0:12:10.76,Default,,0000,0000,0000,,Wataru-kun...
Dialogue: 0,0:12:12.19,0:12:14.53,Default,,0000,0000,0000,,Bú đi, bú chúng đi nào.
Dialogue: 0,0:12:21.90,0:12:24.30,Default,,0000,0000,0000,,Wataru-chan, cưng thật tuyệt vời!
Dialogue: 0,0:12:25.47,0:12:27.87,Default,,0000,0000,0000,,Nữa đi, mạnh nữa lên!
Dialogue: 0,0:12:28.48,0:12:32.54,Default,,0000,0000,0000,,Wataru-kun, chỗ đó, ngay chỗ đó!
Dialogue: 0,0:12:33.85,0:12:36.98,Default,,0000,0000,0000,,Của cưng đang vào sâu bên trong em, tuyệt quá!
Dialogue: 0,0:12:44.29,0:12:47.23,Default,,0000,0000,0000,,Wataru, tuyệt quá đi mất...
Dialogue: 0,0:12:51.67,0:12:54.10,Default,,0000,0000,0000,,Đúng là Wataru-chan mạnh mẽ của em rồi!
Dialogue: 0,0:12:54.17,0:12:55.97,Default,,0000,0000,0000,,E, Em sắp!!
Dialogue: 0,0:12:57.84,0:13:01.17,Default,,0000,0000,0000,,Wataru-kun, em cũng vậy, em cũng vậy!
Dialogue: 0,0:13:26.47,0:13:29.20,Default,,0000,0000,0000,,Cưng đúng là người đàn ông tuyệt vời nhất, Wataru-chan.
Dialogue: 0,0:13:29.27,0:13:31.57,Default,,0000,0000,0000,,Cưng đã nhớ ra điều gì chưa?
Dialogue: 0,0:13:32.01,0:13:32.90,Default,,0000,0000,0000,,C, chưa...
Dialogue: 0,0:13:33.57,0:13:35.60,Default,,0000,0000,0000,,Bây giờ sẽ đến lượt em trị liệu cho cưng.
Dialogue: 0,0:13:35.71,0:13:37.77,Default,,0000,0000,0000,,Lần này, chắc chắn cưng sẽ...
Dialogue: 0,0:13:55.66,0:13:56.69,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:13:57.63,0:13:58.36,Default,,0000,0000,0000,,Rena!
Dialogue: 0,0:14:24.89,0:14:26.22,Default,,0000,0000,0000,,E, Ed?
Dialogue: 0,0:14:29.63,0:14:31.43,Default,,0000,0000,0000,,Nhột quá.
Dialogue: 0,0:14:33.03,0:14:34.86,Default,,0000,0000,0000,,Được rồi, tao ko buồn nữa đâu.
Dialogue: 0,0:14:35.17,0:14:36.50,Default,,0000,0000,0000,,Yên nào, yên nào.
Dialogue: 0,0:14:38.24,0:14:39.71,Default,,0000,0000,0000,,Cảm ơn mày, Ed.
Dialogue: 0,0:14:39.77,0:14:42.64,Default,,0000,0000,0000,,Cảm ơn vì đã an ủi tao.
Dialogue: 0,0:14:43.64,0:14:44.34,Default,,0000,0000,0000,,Rena.
Dialogue: 0,0:14:46.35,0:14:48.18,Default,,0000,0000,0000,,Wa, Wataru!?
Dialogue: 0,0:14:52.25,0:14:53.22,Default,,0000,0000,0000,,C, Chuyện gì vậy?
Dialogue: 0,0:14:53.85,0:14:54.68,Default,,0000,0000,0000,,Mình xin lỗi...
Dialogue: 0,0:14:54.82,0:14:57.12,Default,,0000,0000,0000,,Vì mình mà cậu bị lôi vào chuyện này.
Dialogue: 0,0:14:57.89,0:14:59.02,Default,,0000,0000,0000,,Ko sao đâu.
Dialogue: 0,0:14:59.36,0:15:02.82,Default,,0000,0000,0000,,Vậy ra cậu đúng là một gã mê ngực...
Dialogue: 0,0:15:05.17,0:15:06.63,Default,,0000,0000,0000,,Có lẽ là vậy...
Dialogue: 0,0:15:07.77,0:15:08.67,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:15:10.87,0:15:13.77,Default,,0000,0000,0000,,Phải chăng chuyện đó khiến cậu ko vui...
Dialogue: 0,0:15:15.68,0:15:16.87,Default,,0000,0000,0000,,Ko phải vậy...
Dialogue: 0,0:15:18.35,0:15:19.81,Default,,0000,0000,0000,,Đó là lỗi của mình...
Dialogue: 0,0:15:22.25,0:15:25.24,Default,,0000,0000,0000,,Là do...bộ ngực của mình.
Dialogue: 0,0:15:25.59,0:15:26.45,Default,,0000,0000,0000,,Ngực ư?
Dialogue: 0,0:15:27.76,0:15:34.46,Default,,0000,0000,0000,,Cậu cho rằng việc va phải mình và ngã xuống sân chính là nguyên nhân khiến cậu mất trí nhớ sao?
Dialogue: 0,0:15:34.63,0:15:35.72,Default,,0000,0000,0000,,Đó là mình nói dối cậu thôi...
Dialogue: 0,0:15:39.67,0:15:42.40,Default,,0000,0000,0000,,Xin lỗi...vì đã ko cho cậu biết sự thật...
Dialogue: 0,0:15:43.40,0:15:47.93,Default,,0000,0000,0000,,Sự thật là, ngực của mình đã va vào đầu cậu.
Dialogue: 0,0:15:50.18,0:15:51.17,Default,,0000,0000,0000,,R, Rena?
Dialogue: 0,0:16:01.92,0:16:03.29,Default,,0000,0000,0000,,Đ, Đó là...
Dialogue: 0,0:16:15.24,0:16:20.90,Default,,0000,0000,0000,,Khi chuyện đó xảy ra, mình rất sợ khi tỉnh lại cậu sẽ căm ghét mình.
Dialogue: 0,0:16:21.78,0:16:28.08,Default,,0000,0000,0000,,Cũng chính vì chuyện đó, mình đã nịt ngực để làm chúng nhỏ đi.
Dialogue: 0,0:16:33.19,0:16:37.35,Default,,0000,0000,0000,,Nhưng nếu chuyện này giúp được cậu, mình thực sự muốn...
Dialogue: 0,0:16:45.57,0:16:46.50,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:16:47.80,0:16:48.56,Default,,0000,0000,0000,,Lại đây nào...
Dialogue: 0,0:16:50.67,0:16:51.60,Default,,0000,0000,0000,,Rena!
Dialogue: 0,0:16:52.61,0:16:53.90,Default,,0000,0000,0000,,Wataru!
Dialogue: 0,0:17:04.25,0:17:06.98,Default,,0000,0000,0000,,Cậu mê ngực đến vậy sao?!
Dialogue: 0,0:17:08.16,0:17:09.75,Default,,0000,0000,0000,,Ko hẳn là vậy.
Dialogue: 0,0:17:11.23,0:17:12.95,Default,,0000,0000,0000,,Rena, mình yêu cậu.
Dialogue: 0,0:17:13.59,0:17:15.96,Default,,0000,0000,0000,,Mình cũng yêu cậu, Wataru.
Dialogue: 0,0:17:31.45,0:17:33.57,Default,,0000,0000,0000,,Này, Wataru...
Dialogue: 0,0:17:35.55,0:17:37.92,Default,,0000,0000,0000,,Ở dưới đây nữa, làm ơn...
Dialogue: 0,0:17:38.39,0:17:40.35,Default,,0000,0000,0000,,Uhm, tất nhiên rồi...
Dialogue: 0,0:17:51.20,0:17:56.07,Default,,0000,0000,0000,,Wataru, tuyệt quá...mình phát điên lên mất!
Dialogue: 0,0:17:57.27,0:18:00.87,Default,,0000,0000,0000,,Rena, ngực của cậu thật ngon và mềm mại.
Dialogue: 0,0:18:01.58,0:18:04.98,Default,,0000,0000,0000,,Cậu có thể làm mạnh hơn nếu muốn...
Dialogue: 0,0:18:05.08,0:18:06.01,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:18:13.45,0:18:16.36,Default,,0000,0000,0000,,Wataru, tuyệt quá đi mất...
Dialogue: 0,0:18:20.66,0:18:21.46,Default,,0000,0000,0000,,Rena.
Dialogue: 0,0:18:22.96,0:18:25.33,Default,,0000,0000,0000,,Cậu làm mình sung sướng như vậy được ko?
Dialogue: 0,0:18:25.75,0:18:26.91,Default,,0000,0000,0000,,Được chứ.
Dialogue: 0,0:18:43.32,0:18:45.48,Default,,0000,0000,0000,,Sướng ko nào?
Dialogue: 0,0:18:46.29,0:18:48.62,Default,,0000,0000,0000,,Phải, thật sự...rất....
Dialogue: 0,0:18:53.59,0:18:56.29,Default,,0000,0000,0000,,Rena, dùng miệng của cậu đi nào.
Dialogue: 0,0:19:02.80,0:19:05.03,Default,,0000,0000,0000,,Tuyệt quá, Rena.
Dialogue: 0,0:19:14.68,0:19:16.88,Default,,0000,0000,0000,,Hãy dùng cả ngực của cậu nữa.
Dialogue: 0,0:19:22.26,0:19:23.59,Default,,0000,0000,0000,,M, mềm mại quá.
Dialogue: 0,0:19:24.76,0:19:26.59,Default,,0000,0000,0000,,Sướng lắm hả, Wataru?
Dialogue: 0,0:19:27.09,0:19:28.89,Default,,0000,0000,0000,,Phải, sướng lắm...
Dialogue: 0,0:19:29.10,0:19:30.79,Default,,0000,0000,0000,,Nhanh nữa lên nào, làm ơn.
Dialogue: 0,0:19:35.64,0:19:36.60,Default,,0000,0000,0000,,Như vậy hả?
Dialogue: 0,0:19:37.37,0:19:38.57,Default,,0000,0000,0000,,Đúng vậy, Rena.
Dialogue: 0,0:19:38.91,0:19:39.84,Default,,0000,0000,0000,,Mình...
Dialogue: 0,0:19:49.58,0:19:52.21,Default,,0000,0000,0000,,Ôi ko, mình làm mặt cậu bẩn hết rồi.
Dialogue: 0,0:19:53.55,0:19:56.75,Default,,0000,0000,0000,,Ko sao hết, đó là tinh dịch của cậu.
Dialogue: 0,0:20:09.74,0:20:11.47,Default,,0000,0000,0000,,Wataru, lại đây nào...
Dialogue: 0,0:20:26.65,0:20:27.64,Default,,0000,0000,0000,,Rena...
Dialogue: 0,0:20:31.83,0:20:34.66,Default,,0000,0000,0000,,Wataru, mau lên nào, nhét nó vào bên trong đi!
Dialogue: 0,0:20:34.90,0:20:36.42,Default,,0000,0000,0000,,M, mình vào đây.
Dialogue: 0,0:20:40.67,0:20:42.26,Default,,0000,0000,0000,,Đ, đau lắm hả?
Dialogue: 0,0:20:42.34,0:20:45.79,Default,,0000,0000,0000,,Ko, mình ko sao hết...vào đi!
Dialogue: 0,0:20:47.64,0:20:48.40,Default,,0000,0000,0000,,Đ, được rồi.
Dialogue: 0,0:20:52.71,0:20:55.15,Default,,0000,0000,0000,,Wataru, nó đang vào sâu bên trong!
Dialogue: 0,0:20:55.58,0:20:58.48,Default,,0000,0000,0000,,Rena, của cậu khít quá!
Dialogue: 0,0:21:07.83,0:21:11.06,Default,,0000,0000,0000,,Tuyệt quá, tuyệt quá đi mất, Wataru!
Dialogue: 0,0:21:17.74,0:21:18.83,Default,,0000,0000,0000,,Wataru!
Dialogue: 0,0:21:20.97,0:21:22.94,Default,,0000,0000,0000,,Cậu khiến mình phát điên lên mất!
Dialogue: 0,0:21:23.01,0:21:25.54,Default,,0000,0000,0000,,Nó vào sâu quá!
Dialogue: 0,0:21:26.35,0:21:28.08,Default,,0000,0000,0000,,Rena, Rena...
Dialogue: 0,0:21:30.85,0:21:34.25,Default,,0000,0000,0000,,Oh Wataru, Wataru.
Dialogue: 0,0:21:48.54,0:21:52.30,Default,,0000,0000,0000,,R, Rena, nếu của cậu cứ tiếp tục khít lại như vậy...
Dialogue: 0,0:21:53.41,0:21:56.40,Default,,0000,0000,0000,,N, nhưng chuyện này thực sự rất tuyệt!
Dialogue: 0,0:21:59.68,0:22:03.71,Default,,0000,0000,0000,,Wataru, mình sắp ra rồi!
Dialogue: 0,0:22:04.79,0:22:05.88,Default,,0000,0000,0000,,M, mình cũng vậy!
Dialogue: 0,0:22:06.39,0:22:09.55,Default,,0000,0000,0000,,Mình ra đây, mình ra đây, mình ra đây! Wataru!
Dialogue: 0,0:22:15.96,0:22:17.43,Default,,0000,0000,0000,,Nhiều quá...
Dialogue: 0,0:22:18.63,0:22:20.53,Default,,0000,0000,0000,,Cậu ra nhiều thật đó...
Dialogue: 0,0:22:22.10,0:22:25.80,Default,,0000,0000,0000,,Bên trong mình đang đầy ắp...
Dialogue: 0,0:22:26.41,0:22:28.34,Default,,0000,0000,0000,,Phải, đó là...
Dialogue: 0,0:22:34.05,0:22:35.64,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:22:40.99,0:22:42.25,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:22:42.32,0:22:43.29,Default,,0000,0000,0000,,Rena...
Dialogue: 0,0:22:45.99,0:22:48.55,Default,,0000,0000,0000,,Đừng lo lắng gì hết, Rena.
Dialogue: 0,0:22:49.80,0:22:51.20,Default,,0000,0000,0000,,Wataru.
Dialogue: 0,0:22:53.47,0:22:54.66,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:22:54.74,0:22:56.10,Default,,0000,0000,0000,,Rena, thật mềm mại.
Dialogue: 0,0:22:58.37,0:23:01.40,Default,,0000,0000,0000,,Wataru, vuốt ve mình nữa đi...
Dialogue: 0,0:23:01.61,0:23:02.40,Default,,0000,0000,0000,,Uhm...
Dialogue: 0,0:23:05.11,0:23:08.78,Default,,0000,0000,0000,,Wataru, cậu đúng là một gã mê ngực.
Dialogue: 0,0:23:15.36,0:23:17.22,Default,,0000,0000,0000,,N, Ngực của cô...
Dialogue: 0,0:23:17.29,0:23:18.69,Default,,0000,0000,0000,,V, Vậy là sao!?
Dialogue: 0,0:23:19.73,0:23:20.59,Default,,0000,0000,0000,,U, um...
Dialogue: 0,0:23:21.86,0:23:22.73,Default,,0000,0000,0000,,Rena?
Dialogue: 0,0:23:23.10,0:23:23.99,Default,,0000,0000,0000,,Đừng nhìn!
Dialogue: 0,0:23:26.10,0:23:27.23,Default,,0000,0000,0000,,Wataru-kun!?
Dialogue: 0,0:23:27.47,0:23:28.70,Default,,0000,0000,0000,,C, cưng ko sao chứ!?
Dialogue: 0,0:23:29.30,0:23:30.53,Default,,0000,0000,0000,,Tôi cảm thấy...?
Dialogue: 0,0:23:31.44,0:23:32.34,Default,,0000,0000,0000,,Wataru!
Dialogue: 0,0:23:32.47,0:23:34.67,Default,,0000,0000,0000,,Hơ, thấy...
Dialogue: 0,0:23:35.58,0:23:37.37,Default,,0000,0000,0000,,Ở đâu ra lắm ngực thế này...
Dialogue: 0,0:23:44.55,0:23:47.21,Default,,0000,0000,0000,,Wataru-chan, cưng ổn chứ?
Dialogue: 0,0:23:48.16,0:23:49.28,Default,,0000,0000,0000,,Lại đây nào.
Dialogue: 0,0:24:00.37,0:24:03.77,Default,,0000,0000,0000,,Wataru-kun, ổn rồi mà.
Dialogue: 0,0:24:11.38,0:24:13.40,Default,,0000,0000,0000,,Đây là mùi hương của Fumie-san...
Dialogue: 0,0:24:14.71,0:24:15.95,Default,,0000,0000,0000,,Wataru!
