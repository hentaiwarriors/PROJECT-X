Dialogue: 0,0:00:12.62,0:00:14.19,Default,,0000,0000,0000,,Xin anh đấy.
Dialogue: 0,0:00:14.19,0:00:18.86,Default,,0000,0000,0000,,Ta sẽ thủ dâm, nhưng làm ơn đừng ép ta phải cởi quần lót.
Dialogue: 0,0:00:19.08,0:00:24.61,Default,,0000,0000,0000,,Ta thật sự không thể vạch "bím" thủ dâm ngay trước mặt đàn ông như vậy được.
Dialogue: 0,0:00:24.61,0:00:27.34,Default,,0000,0000,0000,,Làm vậy xấu hổ chết đi được!
Dialogue: 0,0:00:27.75,0:00:31.09,Default,,0000,0000,0000,,Thủ dâm kiểu đó thì khác nào bắt ta "cưỡi ngựa xem hoa"?
Dialogue: 0,0:00:31.09,0:00:33.09,Default,,0000,0000,0000,,Cô hãy mau tiến hành đi.
Dialogue: 0,0:00:33.09,0:00:36.48,Default,,0000,0000,0000,,Như cách mà cô vẫn thường làm đó, Công chúa.
Dialogue: 0,0:00:37.31,0:00:40.26,Default,,0000,0000,0000,,Màn này thật là ấn tượng, Công chúa nhỉ?
Dialogue: 0,0:00:40.26,0:00:41.61,Default,,0000,0000,0000,,Khốn kiếp!
Dialogue: 0,0:00:41.61,0:00:47.06,Default,,0000,0000,0000,,Ngươi dám bắt ép ta làm chuyện xấu xa này...
Dialogue: 0,0:00:47.06,0:00:50.24,Default,,0000,0000,0000,,Không hoàn thành nghĩa vụ của mình thì phải chịu phạt.
Dialogue: 0,0:00:51.37,0:00:55.21,Default,,0000,0000,0000,,Thiên tử phạm tội, xử như thứ dân, phải không hả Công chúa Lunatemis?
Dialogue: 0,0:00:56.57,0:01:02.19,Default,,0000,0000,0000,,Đó là điều luật thiêng liêng của Đại Nguyệt vương triều.
Dialogue: 0,0:01:03.05,0:01:08.04,Default,,0000,0000,0000,,Một gã thuộc hạ lại dám thản nhiên ngắm nhìn ta thủ dâm như vậy...
Dialogue: 0,0:01:08.47,0:01:10.96,Default,,0000,0000,0000,,Đúng là xấu hổ muốn chết mà!
Dialogue: 0,0:01:11.74,0:01:16.56,Default,,0000,0000,0000,,Để coi chiếc quần lót thanh nhã của cô mang tư vị gì.
Dialogue: 0,0:01:16.56,0:01:19.38,Default,,0000,0000,0000,,Ngươi định làm gì quần lót của ta?!
Dialogue: 0,0:01:19.38,0:01:24.76,Default,,0000,0000,0000,,Một thằng đàn ông cầm quần lót của phụ nữ, ngoài ngửi ra thì còn trò gì khác.
Dialogue: 0,0:01:24.76,0:01:26.59,Default,,0000,0000,0000,,Mùi ghê quá nhỉ.
Dialogue: 0,0:01:26.59,0:01:28.42,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:01:28.42,0:01:31.87,Default,,0000,0000,0000,,Câm miệng! Ta luôn luôn tắm rửa sạch sẽ!
Dialogue: 0,0:01:31.87,0:01:33.60,Default,,0000,0000,0000,,Làm sao có mùi được chứ!
Dialogue: 0,0:01:33.60,0:01:36.53,Default,,0000,0000,0000,,Chỗ này có vết dơ này, Công chúa.
Dialogue: 0,0:01:36.53,0:01:38.11,Default,,0000,0000,0000,,Không phải như vậy!
Dialogue: 0,0:01:38.94,0:01:40.37,Default,,0000,0000,0000,,Thật đáng kinh tởm.
Dialogue: 0,0:01:40.37,0:01:42.60,Default,,0000,0000,0000,,Ngươi định bắt ta làm chuyện này bao lâu nữa?
Dialogue: 0,0:01:44.18,0:01:50.73,Default,,0000,0000,0000,,Cô bị ta hết nhìn lại ngắm, lại còn ngửi cả quần lót nữa, thế mà âm hộ lại ướt đẫm như vậy ư?
Dialogue: 0,0:01:50.73,0:01:53.11,Default,,0000,0000,0000,,Hóa ra Công chúa lại dâm đãng tới mức này!
Dialogue: 0,0:01:54.08,0:01:58.17,Default,,0000,0000,0000,,Chiếc quần lót này hẳn là chứa rất nhiều "tinh hoa" của cô nhỉ.
Dialogue: 0,0:01:58.17,0:02:00.13,Default,,0000,0000,0000,,Ngươi gạt ta...
Dialogue: 0,0:02:00.13,0:02:02.46,Default,,0000,0000,0000,,Rõ ràng ta đã giặt sạch nó!
Dialogue: 0,0:02:03.14,0:02:08.48,Default,,0000,0000,0000,,"Tinh hoa" tích tụ cả năm trời sao có thể bị rửa trôi dễ dàng như vậy chứ.
Dialogue: 0,0:02:08.48,0:02:12.63,Default,,0000,0000,0000,,Gã thuộc hạ đáng ghét này vẫn đang hít hà nó...
Dialogue: 0,0:02:13.31,0:02:15.62,Default,,0000,0000,0000,,Đừng ngửi nữa...
Dialogue: 0,0:02:18.27,0:02:21.53,Default,,0000,0000,0000,,Không được, hắn vẫn đang nhìn chằm chằm...
Dialogue: 0,0:02:21.54,0:02:23.76,Default,,0000,0000,0000,,Đến lúc rồi nhỉ?
Dialogue: 0,0:02:23.76,0:02:26.52,Default,,0000,0000,0000,,Công chúa, giang chân rộng ra nào.
Dialogue: 0,0:02:26.95,0:02:29.78,Default,,0000,0000,0000,,Dừng lại! Đừng chạm vào người ta!
Dialogue: 0,0:02:30.01,0:02:32.66,Default,,0000,0000,0000,,Không... Ta không thể kháng cự...
Dialogue: 0,0:02:32.66,0:02:37.26,Default,,0000,0000,0000,,Một gã thuộc hạ thấp kém mà lại dám...
Dialogue: 0,0:02:37.26,0:02:39.05,Default,,0000,0000,0000,,Ta sắp ra rồi!
Dialogue: 0,0:02:39.05,0:02:40.88,Default,,0000,0000,0000,,Không được, ta không thể!
Dialogue: 0,0:02:40.88,0:02:42.73,Default,,0000,0000,0000,,Không thể làm vậy...
Dialogue: 0,0:02:42.73,0:02:44.99,Default,,0000,0000,0000,,Ta... Ta... Ta sắp ra rồi!
Dialogue: 0,0:02:44.99,0:02:46.08,Default,,0000,0000,0000,,Chuyện gì thế này?
Dialogue: 0,0:02:46.08,0:02:48.08,Default,,0000,0000,0000,,Không, ta sợ lắm!
Dialogue: 0,0:02:50.90,0:02:53.14,Default,,0000,0000,0000,,Đánh cắp biểu tượng!{wtf lol}
Dialogue: 0,0:02:59.80,0:03:02.14,Default,,0000,0000,0000,,Thôi mà, ta kiệt sức rồi.
Dialogue: 0,0:03:03.73,0:03:05.16,Default,,0000,0000,0000,,Ngươi làm gì vậy, Katsuma?
Dialogue: 0,0:03:06.10,0:03:09.45,Default,,0000,0000,0000,,Khoan đã, chẳng phải hình phạt của ta đã kết thúc rồi sao?
Dialogue: 0,0:03:09.45,0:03:13.63,Default,,0000,0000,0000,,Nhìn xem, ngắm nhìn Công chúa thủ dâm đã biết ta thành bộ dạng này đó!?
Dialogue: 0,0:03:13.63,0:03:15.74,Default,,0000,0000,0000,,Không, tránh xa ta ra.
Dialogue: 0,0:03:15.74,0:03:17.86,Default,,0000,0000,0000,,Ta bảo ngươi dừng lại cơ mà!
Dialogue: 0,0:03:17.86,0:03:21.38,Default,,0000,0000,0000,,Lunatemis, không được cử động. Nằm yên ở đó.
Dialogue: 0,0:03:22.35,0:03:24.17,Default,,0000,0000,0000,,Cơ thể ta bất động rồi.
Dialogue: 0,0:03:24.17,0:03:26.86,Default,,0000,0000,0000,,Cô rất thích chuyện này phải không, Công chúa?
Dialogue: 0,0:03:26.86,0:03:28.84,Default,,0000,0000,0000,,Làm ơn ngừng lại đi.
Dialogue: 0,0:03:29.35,0:03:33.15,Default,,0000,0000,0000,,Ta chưa làm chuyện này bao giờ, vậy nên...
Dialogue: 0,0:03:33.15,0:03:37.23,Default,,0000,0000,0000,,Ngạc nhiên chưa? Vậy mà cô lại khoái thủ dâm như vậy.
Dialogue: 0,0:03:37.23,0:03:41.14,Default,,0000,0000,0000,,Chuyện này sẽ kết thúc trước khi cô kịp đếm hết số vết dơ trên trần nhà.
Dialogue: 0,0:03:42.43,0:03:43.72,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:03:43.72,0:03:46.84,Default,,0000,0000,0000,,Thư giãn nào, không thì đau lắm đấy.
Dialogue: 0,0:03:47.58,0:03:51.80,Default,,0000,0000,0000,,Vừa thủ dâm xong nên cơ thể ta đang rất nhạy cảm...
Dialogue: 0,0:03:53.66,0:03:56.75,Default,,0000,0000,0000,,Nhìn kìa, nó vào hết bên trong rồi.
Dialogue: 0,0:03:57.46,0:03:59.49,Default,,0000,0000,0000,,Đau quá... Rút nó ra...
Dialogue: 0,0:04:00.08,0:04:03.26,Default,,0000,0000,0000,,Trinh tiết của ta...
Dialogue: 0,0:04:04.07,0:04:07.49,Default,,0000,0000,0000,,Đừng thô bạo như vậy!
Dialogue: 0,0:04:08.56,0:04:13.46,Default,,0000,0000,0000,,Nó đang bự lên bên trong...
Dialogue: 0,0:04:13.46,0:04:15.55,Default,,0000,0000,0000,,và chà xát mãnh liệt khắp nơi!
Dialogue: 0,0:04:16.52,0:04:22.04,Default,,0000,0000,0000,,Lunatemis đã mất đi sự trinh trắng.
Dialogue: 0,0:04:23.24,0:04:25.62,Default,,0000,0000,0000,,Không! Ta không chịu nổi nữa rồi!
Dialogue: 0,0:04:33.02,0:04:35.43,Default,,0000,0000,0000,,Làm ơn hãy nhẹ nhàng một chút.
Dialogue: 0,0:04:36.40,0:04:40.03,Default,,0000,0000,0000,,Không! Nếu ngươi thô bạo như vậy, ta sẽ...
Dialogue: 0,0:04:40.03,0:04:41.91,Default,,0000,0000,0000,,Làm ơn hãy tha cho ta!
Dialogue: 0,0:04:41.91,0:04:45.18,Default,,0000,0000,0000,,Nếu hình phạt này còn tiếp diễn...
Dialogue: 0,0:04:45.38,0:04:48.16,Default,,0000,0000,0000,,Nó bự quá đi mất!
Dialogue: 0,0:04:48.16,0:04:48.95,Default,,0000,0000,0000,,Và thô bạo nữa!
Dialogue: 0,0:04:48.95,0:04:49.81,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:04:50.31,0:04:53.25,Default,,0000,0000,0000,,Đừng bóp hai bầu vú của ta.
Dialogue: 0,0:04:57.18,0:04:59.78,Default,,0000,0000,0000,,Cảm giác này thật sự rất khó chịu.
Dialogue: 0,0:04:59.78,0:05:01.26,Default,,0000,0000,0000,,Ta sắp ra rồi!
Dialogue: 0,0:05:03.14,0:05:05.78,Default,,0000,0000,0000,,Ta bắt đầu cảm thấy dễ chịu.
Dialogue: 0,0:05:11.15,0:05:13.52,Default,,0000,0000,0000,,Ôi không, sướng quá đi mất!
Dialogue: 0,0:05:14.61,0:05:18.08,Default,,0000,0000,0000,,Ta sắp ra rồi! Ta sắp ra nữa rồi!
Dialogue: 0,0:05:18.30,0:05:21.16,Default,,0000,0000,0000,,Ta chỉ vừa mới ra khi thủ dâm ban nãy...
Dialogue: 0,0:05:21.16,0:05:24.96,Default,,0000,0000,0000,,Và lần này, hắn lại khiến ta ra thêm lần nữa!
Dialogue: 0,0:05:25.57,0:05:28.50,Default,,0000,0000,0000,,Ta sẽ bắn vào bên trong cô, Công chúa!
Dialogue: 0,0:05:28.50,0:05:29.66,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:05:29.66,0:05:31.11,Default,,0000,0000,0000,,Đừng bắn vào trong
Dialogue: 0,0:05:31.11,0:05:32.95,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:05:34.91,0:05:37.84,Default,,0000,0000,0000,,Ta sắp ra rồi, ta sắp ra rồi, ta sắp ra rồi!
Dialogue: 0,0:05:37.84,0:05:41.77,Default,,0000,0000,0000,,Ta ra rồi!
Dialogue: 0,0:05:43.24,0:05:46.15,Default,,0000,0000,0000,,Nó đang tràn vào... bên trong...
Dialogue: 0,0:05:46.15,0:05:47.90,Default,,0000,0000,0000,,Nhiều quá.
Dialogue: 0,0:05:48.17,0:05:51.43,Default,,0000,0000,0000,,Ta đã bảo là không được cơ mà...
Dialogue: 0,0:05:57.95,0:06:02.91,Title,,0000,0000,0000,,{\fad(1000,850)\fnUVN Bay Buom Hep Nang\pos(243,387)}Ngươi sẽ {\c&H0662FF&}trừng phạt {\c&HFFFFFF&}ta ư!?
Dialogue: 0,0:05:57.95,0:06:02.91,Title,,0000,0000,0000,,{\bord1.5\shad0\fad(1000,850)\fnUVN Nguyen Du\3c&HFFFFFF&\c&H0A0A0A&\pos(350,32)}Baka-sub {\c&H0425FF&}trân trọng {\c&H0A0A0A&}giới thiệu
Dialogue: 0,0:05:59.91,0:06:02.91,Title,,0000,0000,0000,,{\fad(300,850)\fnUVN Bay Buom Hep Nang\pos(490,384)\c&H005EFE&}Ta sẽ chơi cô!
Dialogue: 0,0:06:07.46,0:06:10.19,Default,,0000,0000,0000,,Ai cho phép ngươi ngồi chễm trệ trên đó hả?
Dialogue: 0,0:06:10.19,0:06:12.18,Default,,0000,0000,0000,,Chỗ này là hoàng cung.
Dialogue: 0,0:06:12.18,0:06:16.68,Default,,0000,0000,0000,,Chính là nơi Hoàng hậu triệu kiến quần thần.
Dialogue: 0,0:06:16.68,0:06:20.39,Default,,0000,0000,0000,,Và ta đang an vị trên ngai vàng của Quốc vương.
Dialogue: 0,0:06:20.39,0:06:26.80,Default,,0000,0000,0000,,Chỉ riêng việc dám to gan hạ nhục ta đã đủ khép ngươi vào tội chết rồi.
Dialogue: 0,0:06:26.80,0:06:29.41,Default,,0000,0000,0000,,Vậy mà ngươi còn dám trèo lên ngai vàng sao?!
Dialogue: 0,0:06:29.41,0:06:30.15,Default,,0000,0000,0000,,Khốn kiếp!
Dialogue: 0,0:06:30.41,0:06:33.65,Default,,0000,0000,0000,,Đừng sồn sồn lên như thế, Công chúa.
Dialogue: 0,0:06:33.65,0:06:34.59,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:06:34.59,0:06:37.20,Default,,0000,0000,0000,,Ta sẽ lấy mạng ngươi ngay lập tức!
Dialogue: 0,0:06:37.78,0:06:40.95,Default,,0000,0000,0000,,Lunatemis! Lại đây nào, không được phép kháng cự.
Dialogue: 0,0:06:42.60,0:06:45.95,Default,,0000,0000,0000,,Lại nữa rồi... Cơ thể ta...
Dialogue: 0,0:06:45.95,0:06:47.41,Default,,0000,0000,0000,,Biết ngay mà.
Dialogue: 0,0:06:47.41,0:06:49.11,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:06:49.92,0:06:52.36,Default,,0000,0000,0000,,Ồ, Công chúa!
Dialogue: 0,0:06:52.36,0:06:54.32,Default,,0000,0000,0000,,Âm hộ của cô ướt đẫm rồi nè.
Dialogue: 0,0:06:54.32,0:06:58.30,Default,,0000,0000,0000,,Cô thích chơi nhau với ta đến thế cơ à?
Dialogue: 0,0:06:59.12,0:07:03.82,Default,,0000,0000,0000,,Không... tại vì ngươi vừa mới...
Dialogue: 0,0:07:03.82,0:07:06.31,Default,,0000,0000,0000,,Cơ thể ta không thể cử động...
Dialogue: 0,0:07:06.31,0:07:10.78,Default,,0000,0000,0000,,Hay là vì lúc này được ta hẹn gặp khiến cô háo hức đến phát điên?
Dialogue: 0,0:07:10.78,0:07:12.19,Default,,0000,0000,0000,,Hỗn xược!
Dialogue: 0,0:07:12.77,0:07:15.99,Default,,0000,0000,0000,,Để ta giải thích cho cô nhé, Công chúa hư hỏng.
Dialogue: 0,0:07:15.99,0:07:19.35,Default,,0000,0000,0000,,Lunatemis! Bú nó đi!
Dialogue: 0,0:07:22.07,0:07:23.63,Default,,0000,0000,0000,,Cơ thể ta...
Dialogue: 0,0:07:23.63,0:07:25.25,Default,,0000,0000,0000,,đang tự hành động...
Dialogue: 0,0:07:27.47,0:07:32.12,Default,,0000,0000,0000,,Từ lâu, Đại Nguyệt vương triều đã có dã tâm thôn tính cả thế giới...
Dialogue: 0,0:07:32.12,0:07:36.46,Default,,0000,0000,0000,,Sức mạnh và giai cấp được quyết định bởi biểu tượng Mặt trăng trên cơ thể ngay từ khi chúng ta ra đời.
Dialogue: 0,0:07:36.46,0:07:39.34,Default,,0000,0000,0000,,Đứng đầu chính là Song Nguyệt, đại biểu cho Nữ hoàng.
Dialogue: 0,0:07:39.34,0:07:42.16,Default,,0000,0000,0000,,Kế đến là Viên Nguyệt, đại biểu cho Công chúa.
Dialogue: 0,0:07:42.16,0:07:44.43,Default,,0000,0000,0000,,Cũng chính là địa vị hiện nay của cô.
Dialogue: 0,0:07:44.43,0:07:46.58,Default,,0000,0000,0000,,Tiếp theo là Bán Nguyệt, đại biểu cho thống soái cao cấp.
Dialogue: 0,0:07:46.58,0:07:50.71,Default,,0000,0000,0000,,Còn ta là Nguyệt Khuyết, thống soái thứ cấp.
Dialogue: 0,0:07:50.71,0:07:53.90,Default,,0000,0000,0000,,Thấp kém nhất là An Nguyệt, đại biểu cho binh lính.
Dialogue: 0,0:07:55.66,0:08:00.51,Default,,0000,0000,0000,,Ta có thể đánh cắp sức mạnh từ biểu tượng Mặt trăng.
Dialogue: 0,0:08:00.51,0:08:01.85,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:08:01.85,0:08:03.56,Default,,0000,0000,0000,,Sức mạnh đó gọi là: Đánh cắp biểu tượng.
Dialogue: 0,0:08:03.56,0:08:08.02,Default,,0000,0000,0000,,Khi ai đó đạt cực khoái, ta sẽ dễ dàng chiếm đoạt biểu tượng Mặt trăng của họ.
Dialogue: 0,0:08:08.37,0:08:11.43,Default,,0000,0000,0000,,Sao có thể như vậy?
Dialogue: 0,0:08:11.43,0:08:16.37,Default,,0000,0000,0000,,Bằng chứng là, lúc này cô đã ngoan ngoãn nghe lời ta.
Dialogue: 0,0:08:16.37,0:08:19.00,Default,,0000,0000,0000,,Đó là... bởi vì...
Dialogue: 0,0:08:19.83,0:08:25.45,Default,,0000,0000,0000,,Khi mất đi biểu tượng Mặt trăng của mình, địa vị của cô còn thấp kém hơn cả binh sĩ.
Dialogue: 0,0:08:25.45,0:08:28.64,Default,,0000,0000,0000,,Và cô đã trở thành nô lệ của ta.
Dialogue: 0,0:08:28.64,0:08:31.66,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:08:33.03,0:08:36.90,Default,,0000,0000,0000,,Chỉ vừa mới mất "zin" mà cô đã có thể "thổi kèn" điêu luyện như vậy ư.
Dialogue: 0,0:08:36.90,0:08:38.41,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:08:39.45,0:08:40.61,Default,,0000,0000,0000,,Ta sắp ra rồi.
Dialogue: 0,0:08:40.61,0:08:42.73,Default,,0000,0000,0000,,Ta sẽ bắn lên gương mặt xinh đẹp của cô!
Dialogue: 0,0:08:43.28,0:08:46.53,Default,,0000,0000,0000,,Không! Đừng bắn lên mặt ta!
Dialogue: 0,0:08:49.20,0:08:51.80,Default,,0000,0000,0000,,A! Ghê quá!
Dialogue: 0,0:08:51.80,0:08:54.54,Default,,0000,0000,0000,,Lunatemis! Không được phép lau mặt!
Dialogue: 0,0:08:54.82,0:08:59.15,Default,,0000,0000,0000,,Tại sao... gương mặt ta... dơ dáy.
Dialogue: 0,0:08:59.15,0:09:05.32,Default,,0000,0000,0000,,Cứ ở yên đó cho tới khi cô nhớ kĩ mùi vị tinh dịch của chủ nhân mình.
Dialogue: 0,0:09:06.78,0:09:10.78,Default,,0000,0000,0000,,Thứ nhớp nháp đó đang ở trên mặt ta...
Dialogue: 0,0:09:10.78,0:09:12.75,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:09:12.75,0:09:15.56,Default,,0000,0000,0000,,Tại sao lại như vậy?
Dialogue: 0,0:09:15.56,0:09:21.85,Default,,0000,0000,0000,,Đừng lo, ta sẽ giấu kín việc cô bị mất đi biểu tượng.
Dialogue: 0,0:09:21.85,0:09:24.16,Default,,0000,0000,0000,,Sau này ta sẽ trả lại nó cho cô.
Dialogue: 0,0:09:24.16,0:09:25.58,Default,,0000,0000,0000,,Thật ư?
Dialogue: 0,0:09:25.58,0:09:26.61,Default,,0000,0000,0000,,Phải.
Dialogue: 0,0:09:26.61,0:09:28.70,Default,,0000,0000,0000,,Chuyện đó đối với ta dễ như trở bàn tay vậy.
Dialogue: 0,0:09:29.85,0:09:34.25,Default,,0000,0000,0000,,Ta chỉ yêu cầu cô phải ngoan ngoãn phục tùng trong một thời gian.
Dialogue: 0,0:09:34.25,0:09:36.25,Default,,0000,0000,0000,,Vậy ngươi muốn ta làm gì?
Dialogue: 0,0:09:36.25,0:09:39.46,Default,,0000,0000,0000,,Thức thời như vậy là tốt, quả không hổ danh là Công chúa.
Dialogue: 0,0:09:40.80,0:09:43.87,Default,,0000,0000,0000,,Thống soái cao cấp Kaguya, người nắm giữ biểu tượng Bán Nguyệt.
Dialogue: 0,0:09:43.87,0:09:48.48,Default,,0000,0000,0000,,Hãy dò tìm yếu điểm và tống giam cô ta vào Hình ngục.
Dialogue: 0,0:09:49.30,0:09:50.84,Default,,0000,0000,0000,,Chuyện đó...
Dialogue: 0,0:09:50.84,0:09:53.30,Default,,0000,0000,0000,,Cô không có tư cách từ chối đâu.
Dialogue: 0,0:09:53.30,0:09:57.34,Default,,0000,0000,0000,,Số phận của cô vẫn đang nằm trong tay ta.
Dialogue: 0,0:09:57.90,0:09:59.50,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:10:00.05,0:10:03.23,Default,,0000,0000,0000,,Ta vẫn chưa nghe thấy câu trả lời của cô, Công chúa.
Dialogue: 0,0:10:04.05,0:10:05.87,Default,,0000,0000,0000,,Như ngươi mong muốn.
Dialogue: 0,0:10:11.52,0:10:14.66,Default,,0000,0000,0000,,Kazuma, tối nay chúng ta lại làm chuyện đó sao?
Dialogue: 0,0:10:14.66,0:10:16.80,Default,,0000,0000,0000,,Tất nhiên rồi.
Dialogue: 0,0:10:16.80,0:10:19.65,Default,,0000,0000,0000,,Cô không thể tìm ra yếu điểm của Kaguya.
Dialogue: 0,0:10:19.65,0:10:21.13,Default,,0000,0000,0000,,Giờ là lúc ta trừng phạt cô.
Dialogue: 0,0:10:22.05,0:10:23.30,Default,,0000,0000,0000,,Nó vào bên trong rồi!
Dialogue: 0,0:10:23.80,0:10:27.63,Default,,0000,0000,0000,,Việc đó vô cùng khó khăn, nhất là khi ta còn cả núi công việc mỗi ngày.
Dialogue: 0,0:10:27.63,0:10:36.30,Default,,0000,0000,0000,,Để điệp vụ tiến hành thuận lợi, chúng ta đã lấy danh nghĩa Nguyệt giới, và đặt lãnh sự quán tại Địa cầu.
Dialogue: 0,0:10:36.30,0:10:38.84,Default,,0000,0000,0000,,nhằm thu thập thông tin, phải không?
Dialogue: 0,0:10:40.54,0:10:43.75,Default,,0000,0000,0000,,Các thống soái khác đều phải xông pha trận mạc để hoàn thành nhiệm vụ.
Dialogue: 0,0:10:43.75,0:10:50.83,Default,,0000,0000,0000,,Còn cô chỉ việc đóng giả nữ sinh và rung đùi thu thập tin tức.
Dialogue: 0,0:10:51.57,0:10:52.99,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:10:53.59,0:10:57.59,Default,,0000,0000,0000,,Tìm điểm yếu của Kaguya khó đến vậy sao?
Dialogue: 0,0:10:58.39,0:11:03.59,Default,,0000,0000,0000,,Nhưng đống bài tập về nhà, các kì kiểm tra... cũng đủ khiến ta bận bù đầu!
Dialogue: 0,0:11:04.23,0:11:11.00,Default,,0000,0000,0000,,Chưa kể, đêm nào ngươi cũng giày vò ta đến sáng mới chịu dừng!
Dialogue: 0,0:11:11.00,0:11:15.43,Default,,0000,0000,0000,,Ai bảo cô không hoàn thành tốt nhiệm vụ được giao.
Dialogue: 0,0:11:16.17,0:11:19.85,Default,,0000,0000,0000,,Hết đêm này qua đêm khác, cơ thể ngọc ngà của ta bị ngươi...
Dialogue: 0,0:11:20.55,0:11:24.79,Default,,0000,0000,0000,,Không! Cơ thể ta...
Dialogue: 0,0:11:25.32,0:11:27.10,Default,,0000,0000,0000,,Có vẻ như cô đang rất thỏa mãn.
Dialogue: 0,0:11:27.10,0:11:29.86,Default,,0000,0000,0000,,Cô khoái thằng nhỏ của ta đến mức đó ư?
Dialogue: 0,0:11:31.50,0:11:33.35,Default,,0000,0000,0000,,Ta không liên quan!
Dialogue: 0,0:11:33.35,0:11:35.29,Default,,0000,0000,0000,,Ngươi đừng hòng...
Dialogue: 0,0:11:35.29,0:11:37.29,Default,,0000,0000,0000,,Không phải chỗ đó!
Dialogue: 0,0:11:37.29,0:11:39.21,Default,,0000,0000,0000,,Không, dừng lại, dừng lại!
Dialogue: 0,0:11:39.21,0:11:40.45,Default,,0000,0000,0000,,Thôi mà!
Dialogue: 0,0:11:41.85,0:11:44.47,Default,,0000,0000,0000,,Nếu ngươi tiếp tục, ta sẽ...
Dialogue: 0,0:11:44.47,0:11:45.32,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:11:45.32,0:11:46.97,Default,,0000,0000,0000,,Đừng bảo là cô sắp ra rồi nhé?
Dialogue: 0,0:11:46.97,0:11:48.54,Default,,0000,0000,0000,,Không, không!
Dialogue: 0,0:11:48.54,0:11:50.09,Default,,0000,0000,0000,,Ta không muốn...
Dialogue: 0,0:11:50.09,0:11:51.64,Default,,0000,0000,0000,,Ngươi thật đáng ghét...
Dialogue: 0,0:11:52.51,0:11:56.64,Default,,0000,0000,0000,,Vậy hằng đêm ta với cô đang làm cái trò gì vậy!
Dialogue: 0,0:11:57.38,0:12:00.05,Default,,0000,0000,0000,,Ta không có...
Dialogue: 0,0:12:00.05,0:12:01.02,Default,,0000,0000,0000,,Không phải với ngươi...
Dialogue: 0,0:12:02.09,0:12:03.48,Default,,0000,0000,0000,,Ta sắp ra rồi!
Dialogue: 0,0:12:03.48,0:12:04.68,Default,,0000,0000,0000,,Ta không muốn...
Dialogue: 0,0:12:04.68,0:12:05.51,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:12:05.51,0:12:08.79,Default,,0000,0000,0000,,Nếu cứ như vậy, ta sẽ...
Dialogue: 0,0:12:09.19,0:12:13.62,Default,,0000,0000,0000,,Ta ra rồi, ra rồi, ra rồi!
Dialogue: 0,0:12:18.71,0:12:20.44,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:12:20.44,0:12:22.42,Default,,0000,0000,0000,,Ta còn phải đến trường.
Dialogue: 0,0:12:23.55,0:12:26.65,Default,,0000,0000,0000,,Ngươi chơi ta cả đêm còn chưa chán sao.
Dialogue: 0,0:12:26.65,0:12:30.99,Default,,0000,0000,0000,,Làm ơn, dừng lại đi mà.
Dialogue: 0,0:12:30.99,0:12:34.26,Default,,0000,0000,0000,,Ta còn phải đến trường.
Dialogue: 0,0:12:34.26,0:12:38.35,Default,,0000,0000,0000,,Không, ta sắp ra nữa rồi!
Dialogue: 0,0:12:39.22,0:12:43.01,Default,,0000,0000,0000,,Nó đang tràn vào... Ngươi lại bắn vào bên trong ta rồi!
Dialogue: 0,0:12:43.01,0:12:48.95,Default,,0000,0000,0000,,Tinh dịch của ngươi bắn vào bên trong đã khiến ta lên đỉnh!
Dialogue: 0,0:12:56.74,0:12:57.99,Default,,0000,0000,0000,,Đồ biến thái!
Dialogue: 0,0:12:58.93,0:13:01.82,Default,,0000,0000,0000,,Cô mới chính là kẻ biến thái.
Dialogue: 0,0:13:01.82,0:13:04.15,Default,,0000,0000,0000,,Tới lúc rồi, Công chúa.
Dialogue: 0,0:13:04.15,0:13:06.56,Default,,0000,0000,0000,,Cô mau tới trường đi.
Dialogue: 0,0:13:06.56,0:13:09.22,Default,,0000,0000,0000,,Chuyện này thật kì quặc.
Dialogue: 0,0:13:09.22,0:13:12.79,Default,,0000,0000,0000,,Ta sẽ hộ tống cô tới trường, Công chúa.
Dialogue: 0,0:13:16.47,0:13:20.92,Default,,0000,0000,0000,,Kazuma... ta đã tìm ra điểm yếu của Kaguya.
Dialogue: 0,0:13:20.92,0:13:22.41,Default,,0000,0000,0000,,Để sau nói đi.
Dialogue: 0,0:13:22.41,0:13:26.46,Default,,0000,0000,0000,,Sau khi cô hoàn thành "nghĩa vụ" của sáng hôm nay, ta sẽ xem xét.
Dialogue: 0,0:13:26.46,0:13:30.15,Default,,0000,0000,0000,,Nhưng mà... ở đây ư?
Dialogue: 0,0:13:30.15,0:13:31.94,Default,,0000,0000,0000,,Cởi ra nào.
Dialogue: 0,0:13:32.57,0:13:38.69,Default,,0000,0000,0000,,Trước mặt đàn ông... người khác sẽ nhìn thấy mất.
Dialogue: 0,0:13:38.69,0:13:40.13,Default,,0000,0000,0000,,Làm ơn nhanh nhanh lên!
Dialogue: 0,0:13:40.94,0:13:42.80,Default,,0000,0000,0000,,Cô nói cái gì cơ?
Dialogue: 0,0:13:42.80,0:13:46.87,Default,,0000,0000,0000,,Cô không rửa sạch tinh dịch sót lại từ đêm qua mà đã mặc luôn đồng phục à,
Dialogue: 0,0:13:46.87,0:13:49.74,Default,,0000,0000,0000,,âm hộ của cô ngập trong tinh dịch,
Dialogue: 0,0:13:50.63,0:13:57.09,Default,,0000,0000,0000,,gắn máy rung vào hai đầu vú và âm hộ,
Dialogue: 0,0:13:57.34,0:14:00.72,Default,,0000,0000,0000,,và sau đó thản nhiên tới trường như học sinh gương mẫu.
Dialogue: 0,0:14:00.72,0:14:04.53,Default,,0000,0000,0000,,Chính ngươi ép ta làm thế còn gì!
Dialogue: 0,0:14:04.53,0:14:10.04,Default,,0000,0000,0000,,Ta đã đồng ý cho cô rửa mặt, cơ mà sao vẫn còn tinh dịch dây ở trên tóc thế kia!
Dialogue: 0,0:14:10.61,0:14:12.36,Default,,0000,0000,0000,,Thật đáng ghê tởm.
Dialogue: 0,0:14:12.36,0:14:17.25,Default,,0000,0000,0000,,Chắc là có ai nhìn thấy bộ dạng kì quái của Công chúa cao quý rồi nhỉ.
Dialogue: 0,0:14:17.88,0:14:21.74,Default,,0000,0000,0000,,Làm ơn... dừng lại đi...
Dialogue: 0,0:14:22.20,0:14:24.87,Default,,0000,0000,0000,,Hình như cô hứng lên rồi thì phải.
Dialogue: 0,0:14:26.55,0:14:29.82,Default,,0000,0000,0000,,Quần lót của cô ướt đẫm tinh dịch và dâm thủy.
Dialogue: 0,0:14:30.29,0:14:32.00,Default,,0000,0000,0000,,Có cả nước tiểu nè!
Dialogue: 0,0:14:32.00,0:14:33.30,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:14:33.76,0:14:36.44,Default,,0000,0000,0000,,Mùi vị đặc trưng khiến người khác nhức đầu chóng mặt.
Dialogue: 0,0:14:36.44,0:14:40.94,Default,,0000,0000,0000,,Không, ta không có...
Dialogue: 0,0:14:41.56,0:14:42.60,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:14:42.60,0:14:44.92,Default,,0000,0000,0000,,Ngay tại nơi này...
Dialogue: 0,0:14:44.92,0:14:48.35,Default,,0000,0000,0000,,Không, người khác sẽ nhìn thấy chúng ta mất...
Dialogue: 0,0:14:48.35,0:14:52.34,Default,,0000,0000,0000,,Không ngờ một kẻ tôi tớ lại dám chơi ta từ phía sau như vậy!
Dialogue: 0,0:14:52.90,0:14:55.68,Default,,0000,0000,0000,,Đừng thô bạo như vậy!
Dialogue: 0,0:14:55.68,0:14:59.76,Default,,0000,0000,0000,,Vật vã cả đêm đã khiến cơ thể ta cực kì nhạy cảm!
Dialogue: 0,0:14:59.76,0:15:03.33,Default,,0000,0000,0000,,Nếu ai đó thấy ta, một Công chúa cao quý, trong bộ dạng này...
Dialogue: 0,0:15:03.62,0:15:05.12,Default,,0000,0000,0000,,Cô nói đúng.
Dialogue: 0,0:15:05.12,0:15:11.54,Default,,0000,0000,0000,,Công chúa của Đại Nguyệt vương triều- mối đe dọa của Địa cầu, lại bị đè ra chơi giữa ban ngày!
Dialogue: 0,0:15:11.54,0:15:13.60,Default,,0000,0000,0000,,Không! Câm miệng đi!
Dialogue: 0,0:15:14.50,0:15:16.99,Default,,0000,0000,0000,,Đại Nguyệt, Địa cầu,
Dialogue: 0,0:15:16.99,0:15:21.05,Default,,0000,0000,0000,,và tất cả các thế lực thù địch sẽ đều biết chuyện này!
Dialogue: 0,0:15:21.40,0:15:22.80,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:15:22.80,0:15:26.69,Default,,0000,0000,0000,,Nếu chuyện đó xảy ra, ta sẽ không còn mặt mũi nào sống tiếp!
Dialogue: 0,0:15:28.80,0:15:32.56,Default,,0000,0000,0000,,Đừng mà! Mọi người sẽ nghe thấy tiếng nhịp mông của ngươi!
Dialogue: 0,0:15:32.56,0:15:34.28,Default,,0000,0000,0000,,Thô bạo quá!
Dialogue: 0,0:15:34.28,0:15:36.45,Default,,0000,0000,0000,,Chậm lại!
Dialogue: 0,0:15:36.73,0:15:41.34,Default,,0000,0000,0000,,Tiếng rên của cô to hơn tiếng đẩy của ta nhiều!
Dialogue: 0,0:15:43.45,0:15:46.40,Default,,0000,0000,0000,,Để xem cô kiềm chế được bao lâu.
Dialogue: 0,0:15:51.43,0:15:54.56,Default,,0000,0000,0000,,Thấy không? Lần này thì ta nghe rõ mồn một!
Dialogue: 0,0:15:56.50,0:15:58.09,Default,,0000,0000,0000,,Được rồi, Công chúa.
Dialogue: 0,0:15:58.09,0:16:00.93,Default,,0000,0000,0000,,Cho ta biết điểm yếu của Kaguya nào?
Dialogue: 0,0:16:01.95,0:16:03.97,Default,,0000,0000,0000,,Bây giờ ư?
Dialogue: 0,0:16:03.97,0:16:07.52,Default,,0000,0000,0000,,Hãy báo cáo rõ ràng và cầu xin phần thưởng!
Dialogue: 0,0:16:07.52,0:16:09.50,Default,,0000,0000,0000,,Ta không thể...
Dialogue: 0,0:16:09.50,0:16:13.01,Default,,0000,0000,0000,,Nếu làm tốt, ta sẽ khiến cô thỏa mãn.
Dialogue: 0,0:16:13.86,0:16:19.20,Default,,0000,0000,0000,,Nhẹ nhàng thôi... nhẹ nhàng thôi... không...
Dialogue: 0,0:16:19.80,0:16:22.80,Default,,0000,0000,0000,,Ta sẽ không bao giờ cầu xin điều đó...
Dialogue: 0,0:16:23.73,0:16:26.70,Default,,0000,0000,0000,,Đừng mà, đừng mà! Đừng nhịp nữa!
Dialogue: 0,0:16:26.70,0:16:29.35,Default,,0000,0000,0000,,Đừng mà, đừng mà, ta...
Dialogue: 0,0:16:29.35,0:16:32.04,Default,,0000,0000,0000,,Ta xin lỗi.
Dialogue: 0,0:16:32.04,0:16:33.99,Default,,0000,0000,0000,,Điểm yếu của Kaguya chính là...
Dialogue: 0,0:16:33.99,0:16:35.75,Default,,0000,0000,0000,,ngươi, Kazuma.
Dialogue: 0,0:16:35.75,0:16:39.00,Default,,0000,0000,0000,,Cô ta đã lớn lên cùng ngươi.
Dialogue: 0,0:16:41.02,0:16:46.06,Default,,0000,0000,0000,,Bởi vậy cô ta sẽ không bao giờ bỏ rơi ngươi.
Dialogue: 0,0:16:46.71,0:16:49.22,Default,,0000,0000,0000,,Cô ấy đã nói như vậy hả?
Dialogue: 0,0:16:49.75,0:16:54.19,Default,,0000,0000,0000,,Kazuma... ngươi là bạn thân của Kaguya ư?
Dialogue: 0,0:16:54.77,0:16:58.13,Default,,0000,0000,0000,,Công chúa, cô đang ghen tị đấy à?
Dialogue: 0,0:16:58.13,0:16:59.96,Default,,0000,0000,0000,,Không đời nào!
Dialogue: 0,0:16:59.96,0:17:01.90,Default,,0000,0000,0000,,Ngươi đừng có nằm mơ!
Dialogue: 0,0:17:01.90,0:17:05.16,Default,,0000,0000,0000,,Cô sợ Kaguya sẽ cướp mất người trong mộng của cô sao?
Dialogue: 0,0:17:05.16,0:17:06.62,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:17:06.62,0:17:10.15,Default,,0000,0000,0000,,Nhiều lúc cô cũng dễ thương đấy nhỉ?
Dialogue: 0,0:17:10.15,0:17:13.25,Default,,0000,0000,0000,,Không, đồ ngốc! Ngươi thôi ngay đi!
Dialogue: 0,0:17:13.25,0:17:18.54,Default,,0000,0000,0000,,Này Công chúa dâm đãng dễ thương, cô không cầu xin phần thưởng à?
Dialogue: 0,0:17:20.65,0:17:24.83,Default,,0000,0000,0000,,Sâu quá đi mất...
Dialogue: 0,0:17:24.83,0:17:26.97,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:17:26.97,0:17:29.01,Default,,0000,0000,0000,,Làm ơn.
Dialogue: 0,0:17:29.52,0:17:32.35,Default,,0000,0000,0000,,Làm ơn hãy thỏa mãn ta bằng dương vật của ngươi.
Dialogue: 0,0:17:33.45,0:17:38.39,Default,,0000,0000,0000,,Ta bị chơi đùa ngay tại nơi này!
Dialogue: 0,0:17:38.58,0:17:39.62,Default,,0000,0000,0000,,Ta hết chịu nổi rồi.
Dialogue: 0,0:17:39.62,0:17:42.20,Default,,0000,0000,0000,,Người khác sẽ nhìn thấy chúng ta mất!
Dialogue: 0,0:17:42.20,0:17:44.82,Default,,0000,0000,0000,,Đừng mà, ta sắp ra rồi!
Dialogue: 0,0:17:44.82,0:17:46.19,Default,,0000,0000,0000,,Đừng mà, ta sắp ra rồi!
Dialogue: 0,0:17:46.19,0:17:48.26,Default,,0000,0000,0000,,Ra rồi, ra rồi, ra rồi!
Dialogue: 0,0:17:54.39,0:17:58.99,Default,,0000,0000,0000,,Tinh dịch của Kazuma đang tràn vào bên trong.
Dialogue: 0,0:18:00.25,0:18:03.16,Default,,0000,0000,0000,,Tiếp theo sẽ là cô, Kaguya.
Dialogue: 0,0:18:08.80,0:18:11.33,Default,,0000,0000,0000,,Cô thật ngu ngốc, Kaguya.
Dialogue: 0,0:18:11.33,0:18:15.21,Default,,0000,0000,0000,,Nếu bỏ lại tôi, cô sẽ không thất bại trong nhiệm vụ này.
Dialogue: 0,0:18:15.21,0:18:17.19,Default,,0000,0000,0000,,Không phải lỗi của anh.
Dialogue: 0,0:18:17.19,0:18:19.44,Default,,0000,0000,0000,,Là do tôi quá kém cỏi.
Dialogue: 0,0:18:19.44,0:18:21.32,Default,,0000,0000,0000,,Chỉ cần anh được an toàn, tôi sẵn sàng chịu phạt.
Dialogue: 0,0:18:23.20,0:18:25.66,Default,,0000,0000,0000,,Không! Anh đang làm gì vậy, Kazuma?
Dialogue: 0,0:18:25.66,0:18:27.33,Default,,0000,0000,0000,,Tất nhiên là mấy trò đồi trụy rồi.
Dialogue: 0,0:18:27.89,0:18:30.17,Default,,0000,0000,0000,,Họ trừng phạt tội nhân theo cách này sao?
Dialogue: 0,0:18:30.17,0:18:33.45,Default,,0000,0000,0000,,Cô cảm thấy xấu hổ phải không?
Dialogue: 0,0:18:33.45,0:18:34.80,Default,,0000,0000,0000,,Thật sao?
Dialogue: 0,0:18:34.80,0:18:37.80,Default,,0000,0000,0000,,Cũng được thôi, vì người ra tay là anh.
Dialogue: 0,0:18:37.80,0:18:39.56,Default,,0000,0000,0000,,Hả? Cô vừa nói gì cơ?
Dialogue: 0,0:18:39.56,0:18:42.86,Default,,0000,0000,0000,,Không có gì, anh cứ tiếp tục cho xong đi.
Dialogue: 0,0:18:43.72,0:18:45.45,Default,,0000,0000,0000,,Không, gì thế...
Dialogue: 0,0:18:46.46,0:18:47.64,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:18:50.68,0:18:53.26,Default,,0000,0000,0000,,Không! Hình phạt này thật là đồi trụy!
Dialogue: 0,0:18:53.26,0:18:53.96,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:18:53.96,0:18:56.75,Default,,0000,0000,0000,,Tôi có thể chịu mọi hình phạt tàn khốc, nhưng mà...
Dialogue: 0,0:18:57.48,0:19:00.21,Default,,0000,0000,0000,,Nếu anh cứ đè tôi xuống như vậy...
Dialogue: 0,0:19:01.00,0:19:02.92,Default,,0000,0000,0000,,Ướt đẫm cả rồi ư?
Dialogue: 0,0:19:02.92,0:19:04.93,Default,,0000,0000,0000,,Không ngờ cô lại dâm đãng như vậy.
Dialogue: 0,0:19:04.93,0:19:07.37,Default,,0000,0000,0000,,Không! Đừng nói thế!
Dialogue: 0,0:19:07.37,0:19:09.64,Default,,0000,0000,0000,,Tôi không hề dâm đãng!
Dialogue: 0,0:19:10.90,0:19:13.16,Default,,0000,0000,0000,,Anh xấu quá, Kazuma.
Dialogue: 0,0:19:13.16,0:19:15.62,Default,,0000,0000,0000,,Tại sao anh lại trừng phạt tôi bằng cách này?
Dialogue: 0,0:19:16.10,0:19:19.89,Default,,0000,0000,0000,,Chẳng phải âm hộ cô đang co giật vì sung sướng sao.
Dialogue: 0,0:19:19.89,0:19:21.29,Default,,0000,0000,0000,,Không! Không đúng!
Dialogue: 0,0:19:21.29,0:19:23.13,Default,,0000,0000,0000,,Tôi không có sướng...
Dialogue: 0,0:19:23.85,0:19:26.63,Default,,0000,0000,0000,,Tôi sắp mất tự chủ rồi, tôi sắp...
Dialogue: 0,0:19:28.30,0:19:29.79,Default,,0000,0000,0000,,Không, Kazuma!
Dialogue: 0,0:19:30.17,0:19:31.19,Default,,0000,0000,0000,,Nó đang ra!
Dialogue: 0,0:19:31.19,0:19:33.75,Default,,0000,0000,0000,,Thứ đó đang trào ra từ bên trong!
Dialogue: 0,0:19:34.64,0:19:37.04,Default,,0000,0000,0000,,Vẫn chưa đủ sao?
Dialogue: 0,0:19:37.04,0:19:39.16,Default,,0000,0000,0000,,Anh đã lăng nhục tôi này giờ rồi còn gì!
Dialogue: 0,0:19:39.16,0:19:43.16,Default,,0000,0000,0000,,Hay là để tôi nhét một cái đồ chơi cỡ lớn vào âm hộ cô?
Dialogue: 0,0:19:43.68,0:19:44.86,Default,,0000,0000,0000,,Không, đừng làm vậy!
Dialogue: 0,0:19:45.54,0:19:48.35,Default,,0000,0000,0000,,Kazuma, anh xấu quá à!
Dialogue: 0,0:19:48.35,0:19:54.37,Default,,0000,0000,0000,,Không, tôi không muốn đánh mất trinh tiết quý giá vì một cái đồ chơi!
Dialogue: 0,0:19:55.13,0:19:56.25,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:19:56.25,0:19:58.87,Default,,0000,0000,0000,,Tôi sợ lắm, Kazuma!
Dialogue: 0,0:19:58.87,0:20:01.95,Default,,0000,0000,0000,,Chà, dù sao cũng là bạn thuở nhỏ với nhau.
Dialogue: 0,0:20:01.95,0:20:03.56,Default,,0000,0000,0000,,Tha cho cô một lần vậy.
Dialogue: 0,0:20:03.56,0:20:04.21,Default,,0000,0000,0000,,Kazuma.
Dialogue: 0,0:20:04.21,0:20:05.13,Default,,0000,0000,0000,,Kế hoạch hai.
Dialogue: 0,0:20:05.47,0:20:06.81,Default,,0000,0000,0000,,Vào đi nào.
Dialogue: 0,0:20:11.00,0:20:12.89,Default,,0000,0000,0000,,Công chúa... Tại sao?
Dialogue: 0,0:20:12.89,0:20:14.75,Default,,0000,0000,0000,,Kazuma, anh lề mề quá đó.
Dialogue: 0,0:20:14.75,0:20:17.56,Default,,0000,0000,0000,,Nhìn xem ta đã thành bộ dạng gì này?
Dialogue: 0,0:20:19.55,0:20:21.49,Default,,0000,0000,0000,,Chuyện này là sao, Kazuma?
Dialogue: 0,0:20:21.49,0:20:24.72,Default,,0000,0000,0000,,Tôi nghĩ ba chúng ta có thể vui vẻ cùng nhau một chút.
Dialogue: 0,0:20:24.72,0:20:26.86,Default,,0000,0000,0000,,Lunatemis, lại đây.
Dialogue: 0,0:20:26.86,0:20:28.57,Default,,0000,0000,0000,,Giúp cô ấy thư giãn đi.
Dialogue: 0,0:20:29.13,0:20:31.52,Default,,0000,0000,0000,,Không, làm ơn đừng...
Dialogue: 0,0:20:32.01,0:20:36.60,Default,,0000,0000,0000,,Kaguya, cô đang sở hữu một bộ ngực hoàn hảo và âm hộ trinh trắng.
Dialogue: 0,0:20:36.60,0:20:39.95,Default,,0000,0000,0000,,Làm ơn đừng nhìn mà.
Dialogue: 0,0:20:39.95,0:20:44.50,Default,,0000,0000,0000,,Âm hộ của ta cũng không kém, nhưng Kazuma đã tàn phá nó mỗi ngày.
Dialogue: 0,0:20:44.50,0:20:47.47,Default,,0000,0000,0000,,Có lẽ nó đã biến dạng rồi!
Dialogue: 0,0:20:47.47,0:20:48.96,Default,,0000,0000,0000,,Công chúa?
Dialogue: 0,0:20:52.57,0:20:55.37,Default,,0000,0000,0000,,Kaguya, hương vị của cô thật tuyệt.
Dialogue: 0,0:20:56.55,0:20:59.53,Default,,0000,0000,0000,,Kazuma, Công chúa, tại sao?
Dialogue: 0,0:21:00.45,0:21:04.90,Default,,0000,0000,0000,,Đừng nghĩ ngợi nữa, hãy tận hưởng khoảnh khắc này, Kaguya.
Dialogue: 0,0:21:04.90,0:21:09.40,Default,,0000,0000,0000,,Lunatemis, nếu làm cho cô ấy sung sướng, ta sẽ trọng thưởng cho cô.
Dialogue: 0,0:21:09.40,0:21:11.16,Default,,0000,0000,0000,,Thật ư?
Dialogue: 0,0:21:11.16,0:21:15.40,Default,,0000,0000,0000,,Không, ta làm vậy không phải vì ham hố "phần thưởng" của anh đâu nhé!
Dialogue: 0,0:21:15.40,0:21:18.17,Default,,0000,0000,0000,,Không, làm vậy không được!
Dialogue: 0,0:21:19.08,0:21:20.93,Default,,0000,0000,0000,,Đừng nhét ngón tay của cô vào đó!
Dialogue: 0,0:21:22.20,0:21:23.76,Default,,0000,0000,0000,,K-Kazuma, đừng nhìn tôi.
Dialogue: 0,0:21:23.76,0:21:24.69,Default,,0000,0000,0000,,Đừng nhìn tôi mà.
Dialogue: 0,0:21:26.76,0:21:28.73,Default,,0000,0000,0000,,Ngón tay cô chọc ngoáy ghê quá!
Dialogue: 0,0:21:28.73,0:21:30.27,Default,,0000,0000,0000,,Không, tôi sắp...
Dialogue: 0,0:21:30.27,0:21:31.90,Default,,0000,0000,0000,,Tôi sắp rên rỉ rồi.
Dialogue: 0,0:21:31.90,0:21:34.12,Default,,0000,0000,0000,,Kazuma sẽ nghe thấy mất!
Dialogue: 0,0:21:34.12,0:21:36.08,Default,,0000,0000,0000,,Công chúa, đừng mà!
Dialogue: 0,0:21:36.08,0:21:41.48,Default,,0000,0000,0000,,Thôi mà, cơ thể tôi đang nhột nhạt không ngừng!
Dialogue: 0,0:21:41.48,0:21:43.87,Default,,0000,0000,0000,,Có lẽ bây giờ tôi nên nhét nó vào bên trong cô nhỉ?
Dialogue: 0,0:21:43.87,0:21:45.32,Default,,0000,0000,0000,,Cô muốn vậy phải không?
Dialogue: 0,0:21:45.76,0:21:48.99,Default,,0000,0000,0000,,Kazuma, Công chúa đang ở đây!
Dialogue: 0,0:21:48.99,0:21:50.39,Default,,0000,0000,0000,,Thì sao?
Dialogue: 0,0:21:50.39,0:21:55.51,Default,,0000,0000,0000,,Cô sẽ hiến dâng trinh tiết cho tôi ngay trước mặt Công chúa!
Dialogue: 0,0:21:55.51,0:21:59.83,Default,,0000,0000,0000,,Không... "lần đầu" trước mặt người khác ư?
Dialogue: 0,0:22:00.15,0:22:01.92,Default,,0000,0000,0000,,Kazuma, còn ta thì sao?
Dialogue: 0,0:22:01.92,0:22:03.89,Default,,0000,0000,0000,,Ta sẽ thỏa mãn cô sau.
Dialogue: 0,0:22:03.89,0:22:05.83,Default,,0000,0000,0000,,Bú Kaguya đi nào.
Dialogue: 0,0:22:05.83,0:22:08.05,Default,,0000,0000,0000,,Âm hộ của phụ nữ cũng rất tuyệt phải không?
Dialogue: 0,0:22:08.05,0:22:10.24,Default,,0000,0000,0000,,Âm hộ của Kaguya rất ngon lành.
Dialogue: 0,0:22:10.24,0:22:13.14,Default,,0000,0000,0000,,Nhưng dương vật của Kazuma vẫn là số một.
Dialogue: 0,0:22:13.14,0:22:18.34,Default,,0000,0000,0000,,Công chúa đang đích thân thỏa mãn tôi.
Dialogue: 0,0:22:18.34,0:22:19.04,Default,,0000,0000,0000,,Tôi vô cùng tự hào.
Dialogue: 0,0:22:19.40,0:22:25.06,Default,,0000,0000,0000,,Nhưng nếu anh đâm vào và khiến tôi có thai...
Dialogue: 0,0:22:25.06,0:22:27.41,Default,,0000,0000,0000,,Thì anh đừng hòng trốn tránh trách nhiệm!
Dialogue: 0,0:22:27.41,0:22:30.19,Default,,0000,0000,0000,,Phải, tôi sẽ chịu mọi trách nhiệm.
Dialogue: 0,0:22:30.19,0:22:32.87,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:22:32.87,0:22:34.76,Default,,0000,0000,0000,,Đau quá, đau quá!
Dialogue: 0,0:22:34.76,0:22:36.65,Default,,0000,0000,0000,,Không, rút nó ra, rút nó ra!
Dialogue: 0,0:22:36.65,0:22:39.85,Default,,0000,0000,0000,,Tiếng rên rỉ của cô thật dễ thương, Kaguya.
Dialogue: 0,0:22:39.85,0:22:41.20,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:22:41.20,0:22:43.89,Default,,0000,0000,0000,,Tôi đang bị phá trinh trước mặt người khác!
Dialogue: 0,0:22:43.89,0:22:47.21,Default,,0000,0000,0000,,{chinchin}Công chúa đang chăm chú ngắm nhìn mọi chuyện!
Dialogue: 0,0:22:47.21,0:22:49.96,Default,,0000,0000,0000,,Lần đầu cảm giác sung sướng chứ hả?
Dialogue: 0,0:22:49.96,0:22:51.56,Default,,0000,0000,0000,,Không, không!
Dialogue: 0,0:22:51.56,0:22:54.94,Default,,0000,0000,0000,,Tôi chỉ cảm thấy đau đớn và xấu hổ thôi!
Dialogue: 0,0:22:56.95,0:22:57.80,Default,,0000,0000,0000,,Đừng nhìn mà.
Dialogue: 0,0:22:57.80,0:23:00.23,Default,,0000,0000,0000,,Đừng nhìn bộ dạng đáng xấu hổ của tôi lúc này!
Dialogue: 0,0:23:01.31,0:23:03.96,Default,,0000,0000,0000,,Công chúa vẫn đang xem tôi làm tình.
Dialogue: 0,0:23:03.96,0:23:06.89,Default,,0000,0000,0000,,Tôi sẽ không còn mặt mũi nào ra ngoài nữa!
Dialogue: 0,0:23:08.82,0:23:11.40,Default,,0000,0000,0000,,Không, đừng làm trò kì quặc như vậy!
Dialogue: 0,0:23:12.00,0:23:16.60,Default,,0000,0000,0000,,Không, thằng nhỏ của Kazuma thật tuyệt vời!
Dialogue: 0,0:23:17.86,0:23:21.14,Default,,0000,0000,0000,,Không... Tôi là một thống soái.
Dialogue: 0,0:23:21.14,0:23:24.76,Default,,0000,0000,0000,,Tôi sẽ không bị sa đọa bởi dục vọng thấp hèn!
Dialogue: 0,0:23:24.76,0:23:29.22,Default,,0000,0000,0000,,Vậy thì tôi sẽ chơi cô mỗi ngày, cho tới khi cô sa đọa!
Dialogue: 0,0:23:29.83,0:23:33.80,Default,,0000,0000,0000,,Không! Tôi không thể ngày ngày làm chuyện đáng xấu hổ này!
Dialogue: 0,0:23:33.80,0:23:35.43,Default,,0000,0000,0000,,Tôi sẽ phát điên mất!
Dialogue: 0,0:23:35.43,0:23:37.28,Default,,0000,0000,0000,,Tôi sẽ đâm nó vào bên trong cô mỗi ngày.
Dialogue: 0,0:23:37.28,0:23:42.34,Default,,0000,0000,0000,,Nếu mang thai, hãy mời Công chúa và các thống soái khác tới vào lúc cô sinh con!
Dialogue: 0,0:23:42.34,0:23:43.78,Default,,0000,0000,0000,,Đừng mà, đừng mà, đừng mà!
Dialogue: 0,0:23:43.78,0:23:46.99,Default,,0000,0000,0000,,Tôi không muốn người khác trông thấy mình làm tình hay sinh con!
Dialogue: 0,0:23:47.59,0:23:51.85,Default,,0000,0000,0000,,Cô ta phấn khích khi bị người khác ngắm nhìn. Đồ con gái dâm đãng!
Dialogue: 0,0:23:51.85,0:23:53.84,Default,,0000,0000,0000,,Dễ thương lắm, Kaguya!
Dialogue: 0,0:23:53.84,0:23:55.51,Default,,0000,0000,0000,,Nhưng mà, nhưng mà...
Dialogue: 0,0:23:55.51,0:23:59.28,Default,,0000,0000,0000,,Không ngờ tình dục lại tuyệt vời như vậy!
Dialogue: 0,0:23:59.89,0:24:05.39,Default,,0000,0000,0000,,Hưng phấn khi bị người khác ngắm nhìn, có lẽ tôi đúng là đồ dâm đãng!
Dialogue: 0,0:24:05.39,0:24:08.12,Default,,0000,0000,0000,,Đừng nhìn mà! Đừng nhìn vú và âm hộ tôi!
Dialogue: 0,0:24:08.12,0:24:09.80,Default,,0000,0000,0000,,Nếu còn tiếp tục nhìn, tôi sẽ...
Dialogue: 0,0:24:11.14,0:24:13.36,Default,,0000,0000,0000,,Tôi sẽ bắn vào bên trong cô, Kaguya!
Dialogue: 0,0:24:14.24,0:24:16.35,Default,,0000,0000,0000,,Không, đừng bắn vào bên trong.
Dialogue: 0,0:24:16.35,0:24:18.69,Default,,0000,0000,0000,,Đừng làm thế trước mặt người khác!
Dialogue: 0,0:24:18.69,0:24:19.58,Default,,0000,0000,0000,,Bắn ra ngoài đi mà!
Dialogue: 0,0:24:20.63,0:24:23.95,Default,,0000,0000,0000,,Hãy để Công chúa nhìn cô mang thai!
Dialogue: 0,0:24:23.95,0:24:27.80,Default,,0000,0000,0000,,Không, tôi không muốn mang thai trước mặt Công chúa đâu!
Dialogue: 0,0:24:27.80,0:24:29.52,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:24:29.98,0:24:34.07,Default,,0000,0000,0000,,Thằng nhỏ của anh đang đâm sâu vào bên trong tôi!
Dialogue: 0,0:24:35.71,0:24:36.84,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:24:38.83,0:24:40.99,Default,,0000,0000,0000,,Đánh cắp biểu tượng!
Dialogue: 0,0:24:42.19,0:24:45.27,Default,,0000,0000,0000,,Tinh dịch nóng bỏng của Kazuma đang tràn vào bên trong.
Dialogue: 0,0:24:45.27,0:24:48.45,Default,,0000,0000,0000,,Nếu mang thai, tôi sẽ bị tước ngôi thống soái mất!
Dialogue: 0,0:24:48.85,0:24:51.70,Default,,0000,0000,0000,,Kazuma, tới lượt ta!
Dialogue: 0,0:24:52.99,0:24:55.14,Default,,0000,0000,0000,,Thằng nhỏ của Kazuma thật tuyệt vời!
Dialogue: 0,0:24:55.14,0:24:59.65,Default,,0000,0000,0000,,Âm hộ của ta nóng ran khi chứng kiến anh làm tình với Kaguya!
Dialogue: 0,0:24:59.65,0:25:02.25,Default,,0000,0000,0000,,Hôm nay ta sẽ hấp khô anh!
Dialogue: 0,0:25:03.68,0:25:05.70,Default,,0000,0000,0000,,Kazuma, sướng không hả?
Dialogue: 0,0:25:05.70,0:25:08.64,Default,,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:25:09.08,0:25:13.30,Default,,0000,0000,0000,,Vậy mà chúng ta vẫn chưa hẹn hò, huống hồ là kết hôn.
Dialogue: 0,0:25:13.30,0:25:15.59,Default,,0000,0000,0000,,Nhưng tinh dịch của anh đang lấp đầy tử cung tôi.
Dialogue: 0,0:25:15.59,0:25:17.15,Default,,0000,0000,0000,,Ngay trước mặt người khác nữa!
Dialogue: 0,0:25:18.16,0:25:23.60,Default,,0000,0000,0000,,Kazuma, tôi không ngờ anh lại có kĩ năng Đánh cắp biểu tượng.
Dialogue: 0,0:25:24.02,0:25:25.97,Default,,0000,0000,0000,,Đừng làm điều gì dại dột nhé.
Dialogue: 0,0:25:25.97,0:25:29.62,Default,,0000,0000,0000,,Dù chuyện này không xảy ra, tôi vẫn sẽ nghe lời anh mà...
Dialogue: 0,0:25:30.55,0:25:32.10,Default,,0000,0000,0000,,Này, Kaguya!
Dialogue: 0,0:25:32.10,0:25:35.90,Default,,0000,0000,0000,,Ta vẫn còn đang ngồi đây mà cô đã buông lời tán tỉnh ngay được à?
Dialogue: 0,0:25:35.90,0:25:41.51,Default,,0000,0000,0000,,Tốt nhất là cô nên hầu hạ Kazuma tiếp đi!
Dialogue: 0,0:25:42.10,0:25:45.88,Default,,0000,0000,0000,,Cô nghĩ mình là ai chứ, đồ tay sai thấp kém!
Dialogue: 0,0:25:47.19,0:25:53.10,Default,,0000,0000,0000,,Kazuma sủng ái ta hơn cô!
Dialogue: 0,0:25:53.10,0:25:54.59,Default,,0000,0000,0000,,Đừng có ra vẻ ta đây ghê gớm!
Dialogue: 0,0:25:54.85,0:25:56.88,Default,,0000,0000,0000,,Không đúng!
Dialogue: 0,0:25:56.88,0:26:01.26,Default,,0000,0000,0000,,Anh ấy là bạn thuở nhỏ của tôi, ai hơn ai kém thì cô tự hiểu!
Dialogue: 0,0:26:02.36,0:26:03.75,Default,,0000,0000,0000,,Anh đang rất hưng phấn phải không?
Dialogue: 0,0:26:03.75,0:26:07.42,Default,,0000,0000,0000,,Hai cô nhún qua nhún lại như vậy, không hưng phấn thì chả lẽ tôi bất lực à?
Dialogue: 0,0:26:07.42,0:26:09.16,Default,,0000,0000,0000,,Anh thật biến thái!
Dialogue: 0,0:26:09.79,0:26:13.12,Default,,0000,0000,0000,,Đừng sao lãng vì ả tay sai hèn kém đó!
Dialogue: 0,0:26:13.12,0:26:16.93,Default,,0000,0000,0000,,Âm hộ của ta tuyệt hơn rất nhiều, phải không?
Dialogue: 0,0:26:17.55,0:26:23.53,Default,,0000,0000,0000,,"Bướm" hoàng thất so với "bướm" thứ dân phải nói là một trời một vực!
Dialogue: 0,0:26:24.35,0:26:26.25,Default,,0000,0000,0000,,Âm hộ của Lunatemis đang co giật.
Dialogue: 0,0:26:26.25,0:26:28.01,Default,,0000,0000,0000,,Tôi sẽ bắn vào bên trong.
Dialogue: 0,0:26:28.01,0:26:31.16,Default,,0000,0000,0000,,Công chúa, đừng hòng phá đám tôi.
Dialogue: 0,0:26:31.16,0:26:35.24,Default,,0000,0000,0000,,Âm hộ của tôi mới khiến anh ấy sung sướng như vậy.
Dialogue: 0,0:26:35.24,0:26:38.80,Default,,0000,0000,0000,,Âm hộ của gái tân tuyệt hơn nhiều phải không?
Dialogue: 0,0:26:38.80,0:26:41.74,Default,,0000,0000,0000,,Nhìn anh ấy thỏa mãn chưa kìa!
Dialogue: 0,0:26:43.19,0:26:45.29,Default,,0000,0000,0000,,Không đúng!
Dialogue: 0,0:26:45.29,0:26:50.22,Default,,0000,0000,0000,,Âm hộ dâm đãng của ta đã khiến nó bự lên như vậy!
Dialogue: 0,0:26:50.22,0:26:51.69,Default,,0000,0000,0000,,Này hai mẹ trẻ.
Dialogue: 0,0:26:51.69,0:26:52.95,Default,,0000,0000,0000,,Đừng kèn cựa nhau nữa được không?
Dialogue: 0,0:26:52.95,0:26:56.26,Default,,0000,0000,0000,,Đồ ngốc! Ai thèm kèm cựa chứ!
Dialogue: 0,0:26:56.26,0:26:59.79,Default,,0000,0000,0000,,Phải! Đều là do anh ra lệnh cả chứ ai!
Dialogue: 0,0:26:59.79,0:27:02.71,Default,,0000,0000,0000,,Mau ra đi nào!
Dialogue: 0,0:27:02.71,0:27:06.00,Default,,0000,0000,0000,,Ta đã trổ hết tài nghệ của mình!
Dialogue: 0,0:27:06.92,0:27:10.36,Default,,0000,0000,0000,,Tôi đã chiều chuộng anh còn hơn cả bạn trai!
Dialogue: 0,0:27:10.36,0:27:12.10,Default,,0000,0000,0000,,Mau ra đi nào!
Dialogue: 0,0:27:12.10,0:27:13.14,Default,,0000,0000,0000,,Còn lâu!
Dialogue: 0,0:27:13.14,0:27:16.48,Default,,0000,0000,0000,,Âm hộ của ta sẽ khiến Kazuma lên đỉnh!
Dialogue: 0,0:27:17.28,0:27:18.11,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:27:18.11,0:27:21.58,Default,,0000,0000,0000,,Kazuma, lưỡi của anh thật tuyệt!
Dialogue: 0,0:27:22.45,0:27:26.05,Default,,0000,0000,0000,,Tôi sắp mất tự chủ rồi!
Dialogue: 0,0:27:26.05,0:27:29.02,Default,,0000,0000,0000,,Mông tôi cứ thế nhịp không ngừng!
Dialogue: 0,0:27:30.46,0:27:34.44,Default,,0000,0000,0000,,Thằng nhỏ bự con của anh đang đâm sâu vào trong!
Dialogue: 0,0:27:34.44,0:27:37.41,Default,,0000,0000,0000,,Sướng quá đi mất!
Dialogue: 0,0:27:37.41,0:27:39.20,Default,,0000,0000,0000,,Sướng quá, ôi không...
Dialogue: 0,0:27:39.20,0:27:41.91,Default,,0000,0000,0000,,Âm hộ của ta nóng ran lên rồi!
Dialogue: 0,0:27:43.84,0:27:44.91,Default,,0000,0000,0000,,Ta sắp ra rồi.
Dialogue: 0,0:27:44.91,0:27:49.39,Default,,0000,0000,0000,,Thân là Công chúa, vậy mà ta lại van xin thằng nhỏ của anh ta và nhịp mông điên cuồng...
Dialogue: 0,0:27:49.39,0:27:50.80,Default,,0000,0000,0000,,Ta sắp ra rồi!
Dialogue: 0,0:27:50.80,0:27:52.03,Default,,0000,0000,0000,,Tôi cũng vậy...
Dialogue: 0,0:27:52.03,0:27:55.42,Default,,0000,0000,0000,,Không... Tôi sắp ra cùng Công chúa rồi!
Dialogue: 0,0:27:55.42,0:27:57.85,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:27:57.85,0:28:02.31,Default,,0000,0000,0000,,Ta cũng ra rồi, ra rồi, ra rồi!
Dialogue: 0,0:28:04.50,0:28:05.55,Default,,0000,0000,0000,,Thành công rồi.
Dialogue: 0,0:28:05.55,0:28:06.69,Default,,0000,0000,0000,,Ta thành công thật rồi!
Dialogue: 0,0:28:07.73,0:28:10.70,Default,,0000,0000,0000,,Với kĩ năng "Đánh cắp biểu tượng"...
Dialogue: 0,0:28:11.14,0:28:14.87,Default,,0000,0000,0000,,Ta sẽ chinh phục tất cả lũ thống soái kiêu ngạo đó!
