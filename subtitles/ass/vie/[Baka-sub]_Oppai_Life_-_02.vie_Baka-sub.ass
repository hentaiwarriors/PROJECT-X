Dialogue: 0,0:00:11.09,0:00:13.61,Default,,0000,0000,0000,,C-Chika-chan, đừng làm vậy!
Dialogue: 0,0:00:26.60,0:00:29.54,Default,,0000,0000,0000,,Senpai, anh kiềm chế giỏi lắm, đúng không?
Dialogue: 0,0:00:30.07,0:00:31.04,Default,,0000,0000,0000,,Được rồi...
Dialogue: 0,0:00:39.15,0:00:42.55,Default,,0000,0000,0000,,Chika-chan, em không được làm như vậy.
Dialogue: 0,0:00:46.36,0:00:47.42,Default,,0000,0000,0000,,Có khi nào...
Dialogue: 0,0:00:48.93,0:00:50.39,Default,,0000,0000,0000,,Không, khoan đã...
Dialogue: 0,0:00:50.70,0:00:52.56,Default,,0000,0000,0000,,Không đời nào Chika-chan lại làm vậy...
Dialogue: 0,0:00:54.70,0:00:55.72,Default,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:00:56.03,0:00:57.40,Default,,0000,0000,0000,,Naoto-senpai...
Dialogue: 0,0:00:57.64,0:00:58.93,Default,,0000,0000,0000,,Naoto-senpai!
Dialogue: 0,0:00:59.84,0:01:00.67,Default,,0000,0000,0000,,Chika!
Dialogue: 0,0:01:00.87,0:01:05.83,Default,,0000,0000,0000,,Ba em không hề nuôi dạy con gái mình theo kiểu này... phải không!
Dialogue: 0,0:01:10.01,0:01:12.21,Default,,0000,0000,0000,,Sao hả, Senpai? Sướng không?
Dialogue: 0,0:01:15.52,0:01:18.51,Default,,0000,0000,0000,,C-Chika-chan, anh đã nói không...
Dialogue: 0,0:01:19.99,0:01:23.89,Default,,0000,0000,0000,,Trông anh thích thú như vậy thì "không" cái nỗi gì?
Dialogue: 0,0:01:24.36,0:01:25.89,Default,,0000,0000,0000,,Nhìn nè! Nhìn nè!
Dialogue: 0,0:01:26.43,0:01:30.33,Default,,0000,0000,0000,,Nếu em cứ làm như vậy, anh sẽ sớm trở nên vô dụng thôi...
Dialogue: 0,0:01:32.40,0:01:33.70,Default,,0000,0000,0000,,Trở nên vô dụng?
Dialogue: 0,0:01:33.87,0:01:36.34,Default,,0000,0000,0000,,Vậy thì cả hai chúng ta sẽ cùng nhau trở nên vô dụng.
Dialogue: 0,0:01:36.41,0:01:38.84,Default,,0000,0000,0000,,Cùng nhau... cùng nhau...
Dialogue: 0,0:01:39.58,0:01:42.67,Default,,0000,0000,0000,,Anh thật sự đã phản bội Aya-neechan!
Dialogue: 0,0:01:42.95,0:01:44.71,Default,,0000,0000,0000,,Anh đã phản bội chị ấy!
Dialogue: 0,0:01:45.98,0:01:47.88,Default,,0000,0000,0000,,Senpai, thằng nhỏ của anh lại bự lên kìa.
Dialogue: 0,0:01:48.02,0:01:50.75,Default,,0000,0000,0000,,Sắp ra rồi phải không? Sắp ra rồi phải không?
Dialogue: 0,0:01:52.26,0:01:56.56,Default,,0000,0000,0000,,Nó đang thượng lên, dường như bên trong nó ấn chứa linh khí của loai rồng.
Dialogue: 0,0:01:56.73,0:02:02.26,Default,,0000,0000,0000,,Hỗn hợp sữa đặc... Hỗn hợp sữa đặc thượng hạng sắp phun ra!
Dialogue: 0,0:02:05.54,0:02:08.40,Default,,0000,0000,0000,,Phun ra rồi!
Dialogue: 0,0:02:24.12,0:02:25.71,Default,,0000,0000,0000,,Mùi vị của Senpai...
Dialogue: 0,0:02:26.49,0:02:29.09,Default,,0000,0000,0000,,Anh biết là xấu, nhưng...
Dialogue: 0,0:02:30.16,0:02:31.19,Default,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:02:31.96,0:02:37.53,Default,,0000,0000,0000,,Senpai, em không định kể chuyện này cho Aya-neechan đâu.
Dialogue: 0,0:02:37.77,0:02:40.70,Default,,0000,0000,0000,,Nhưng, em chỉ muốn...
Dialogue: 0,0:02:44.48,0:02:46.91,Default,,0000,0000,0000,,hòa làm một với anh...
Dialogue: 0,0:02:48.21,0:02:50.54,Default,,0000,0000,0000,,Nếu không được thì cũng chẳng sao.
Dialogue: 0,0:02:50.78,0:02:54.88,Default,,0000,0000,0000,,Nhưng lúc đó, anh...
Dialogue: 0,0:02:56.42,0:02:59.08,Default,,0000,0000,0000,,lại dẫn Aya-neechan theo cùng...
Dialogue: 0,0:02:59.56,0:03:03.02,Default,,0000,0000,0000,,Khiến cho em cảm thấy mình không còn nằm trong bộ ba thân thiết ngày xưa nữa.
Dialogue: 0,0:03:03.56,0:03:09.26,Default,,0000,0000,0000,,Còn tệ hơn cả việc giương mắt nhìn anh sống hạnh phúc tại một nơi xa lạ nào đó.
Dialogue: 0,0:03:10.13,0:03:11.12,Default,,0000,0000,0000,,Bởi vậy mà...
Dialogue: 0,0:03:11.90,0:03:12.93,Default,,0000,0000,0000,,Bởi vậy mà...
Dialogue: 0,0:03:24.18,0:03:26.21,Default,,0000,0000,0000,,Anh xin lỗi, Chika-chan.
Dialogue: 0,0:03:30.32,0:03:32.69,Default,,0000,0000,0000,,Senpai, anh thật tốt.
Dialogue: 0,0:03:33.09,0:03:35.15,Default,,0000,0000,0000,,Không, anh chẳng tốt đẹp gì đâu.
Dialogue: 0,0:03:35.76,0:03:37.49,Default,,0000,0000,0000,,Nếu anh thật sự tốt như em nói...
Dialogue: 0,0:03:38.60,0:03:41.62,Default,,0000,0000,0000,,thì Chika-chan đã không phải buồn phiền thế này.
Dialogue: 0,0:03:42.47,0:03:43.73,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:03:46.40,0:03:47.63,Default,,0000,0000,0000,,Anh rất tốt.
Dialogue: 0,0:03:48.24,0:03:51.87,Default,,0000,0000,0000,,Đừng diễn lại trò mà anh thường dùng để khích lệ em hồi nhỏ nữa!
Dialogue: 0,0:03:51.94,0:03:54.34,Default,,0000,0000,0000,,Lúc trước anh luôn làm vậy sao?
Dialogue: 0,0:04:01.29,0:04:02.05,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:04:16.20,0:04:20.43,Default,,0000,0000,0000,,Nè, chúng ta có thể làm chuyện đó một lần cuồi cùng được không?
Dialogue: 0,0:04:26.98,0:04:29.17,Default,,0000,0000,0000,,Naoto-senpai, đút vào đi...
Dialogue: 0,0:04:29.25,0:04:32.38,Default,,0000,0000,0000,,Em ướt đẫm rồi nè.
Dialogue: 0,0:04:32.45,0:04:34.71,Default,,0000,0000,0000,,Cứ đút nó vào đi.
Dialogue: 0,0:04:35.65,0:04:39.95,Default,,0000,0000,0000,,Lúc nãy khi em liếm tinh dịch của anh...
Dialogue: 0,0:04:40.19,0:04:43.79,Default,,0000,0000,0000,,tận sâu trong tử cung em vẫn còn run rẩy.
Dialogue: 0,0:04:44.00,0:04:45.72,Default,,0000,0000,0000,,Bởi vậy... Mau lên anh!
Dialogue: 0,0:04:51.87,0:04:55.70,Default,,0000,0000,0000,,Senpai, anh đâm vào bên trong em thật dễ dàng.
Dialogue: 0,0:04:58.04,0:05:03.04,Default,,0000,0000,0000,,Nhưng khi nó cọ xát phía bên ngoài, sướng lắm!
Dialogue: 0,0:05:14.83,0:05:16.02,Default,,0000,0000,0000,,Senpai...
Dialogue: 0,0:05:16.09,0:05:17.15,Default,,0000,0000,0000,,Senpai...
Dialogue: 0,0:05:17.23,0:05:18.96,Default,,0000,0000,0000,,Em, em...
Dialogue: 0,0:05:19.20,0:05:20.32,Default,,0000,0000,0000,,Em rất...
Dialogue: 0,0:05:20.40,0:05:21.73,Default,,0000,0000,0000,,Em rất...
Dialogue: 0,0:05:21.83,0:05:24.39,Default,,0000,0000,0000,,yêu anh, Senpai!
Dialogue: 0,0:05:35.81,0:05:38.28,Default,,0000,0000,0000,,Em không muốn rời xa anh, Senpai.
Dialogue: 0,0:05:38.35,0:05:42.51,Default,,0000,0000,0000,,Em muốn được ở bên cạnh anh mãi mãi... mãi mãi!
Dialogue: 0,0:05:44.52,0:05:49.76,Default,,0000,0000,0000,,Khi ôm chặt lấy cơ thể anh, ngực em cũng bị ép chặt...
Dialogue: 0,0:05:49.83,0:05:52.19,Default,,0000,0000,0000,,sướng lắm!
Dialogue: 0,0:05:52.43,0:05:56.06,Default,,0000,0000,0000,,Hai đầu vú bị siết chặt làm rộn lên cảm giác kích thích, có lẽ...
Dialogue: 0,0:05:56.13,0:06:00.54,Default,,0000,0000,0000,,... em sắp đạt tới cao trào rồi.
Dialogue: 0,0:06:22.63,0:06:25.69,Default,,0000,0000,0000,,Senpai, đừng xoa bóp ngực em.
Dialogue: 0,0:06:25.76,0:06:27.92,Default,,0000,0000,0000,,Nếu anh xoa bóp ngực em nhiều như vậy...
Dialogue: 0,0:06:28.00,0:06:31.53,Default,,0000,0000,0000,,em sẽ lạc lối vì khoái cảm tột cùng mất!
Dialogue: 0,0:06:39.24,0:06:41.27,Default,,0000,0000,0000,,Sướng quá!
Dialogue: 0,0:06:41.48,0:06:45.01,Default,,0000,0000,0000,,Không, em sẽ siết chặt nó cho anh biết tay!
Dialogue: 0,0:06:47.05,0:06:48.42,Default,,0000,0000,0000,,K-Không!
Dialogue: 0,0:06:49.39,0:06:51.12,Default,,0000,0000,0000,,Em sẽ uống hết sữa của anh!
Dialogue: 0,0:06:51.29,0:06:57.29,Default,,0000,0000,0000,,Em sẽ uống hết số sữa đặc thượng hạng mà anh phun ra!
Dialogue: 0,0:07:35.80,0:07:38.00,Default,,0000,0000,0000,,Ayane-san.
Dialogue: 0,0:07:45.28,0:07:46.61,Default,,0000,0000,0000,,Tới ngay.
Dialogue: 0,0:07:48.21,0:07:49.41,Default,,0000,0000,0000,,Xin hỏi...
Dialogue: 0,0:07:49.78,0:07:50.75,Default,,0000,0000,0000,,Aya...
Dialogue: 0,0:07:50.95,0:07:53.88,Default,,0000,0000,0000,,Tôi có chuyện cần gặp Yukishiro.
Dialogue: 0,0:07:53.95,0:07:56.04,Default,,0000,0000,0000,,Ồ, cô ấy à?
Dialogue: 0,0:07:56.15,0:07:57.98,Default,,0000,0000,0000,,Cô ấy bị cảm nên nghỉ rồi.
Dialogue: 0,0:07:58.06,0:07:59.52,Default,,0000,0000,0000,,Ơ, vậy à?
Dialogue: 0,0:08:26.52,0:08:30.89,Default,,0000,0000,0000,,"Mình cũng rất yêu Senpai" ư?
Dialogue: 0,0:08:33.72,0:08:36.96,Default,,0000,0000,0000,,Chắc Aya-neechan cũng yêu anh ấy nhiều như vậy.
Dialogue: 0,0:08:38.80,0:08:41.23,Default,,0000,0000,0000,,Không, không, không được nghĩ vậy!
Dialogue: 0,0:08:41.30,0:08:44.56,Default,,0000,0000,0000,,Aya-neechan là tình địch! Chị ấy là tình địch mà mình căm ghét!
Dialogue: 0,0:08:45.64,0:08:49.70,Default,,0000,0000,0000,,Sẽ dễ dàng hơn nếu mình nghĩ theo cách đó.
Dialogue: 0,0:08:54.98,0:08:57.45,Default,,0000,0000,0000,,Giờ mình phải làm sao đây?
Dialogue: 0,0:09:40.56,0:09:43.19,Default,,0000,0000,0000,,Vậy chị gọi em ra đây có chuyện gì vậy?
Dialogue: 0,0:09:43.26,0:09:45.96,Default,,0000,0000,0000,,Em đang chuẩn bị cho bài kiểm tra đầu vào.
Dialogue: 0,0:09:46.03,0:09:51.73,Default,,0000,0000,0000,,Vậy nên em khó mà ngồi lâu cùng chị được.
Dialogue: 0,0:09:57.07,0:09:58.17,Default,,0000,0000,0000,,Chị...
Dialogue: 0,0:09:58.28,0:09:59.26,Default,,0000,0000,0000,,Ơ...
Dialogue: 0,0:09:59.58,0:10:02.05,Default,,0000,0000,0000,,Không có gì giữa Naoto-kun và chị hết.
Dialogue: 0,0:10:03.01,0:10:06.54,Default,,0000,0000,0000,,Nên em không cần phải lo lắng gì cả, nhé?
Dialogue: 0,0:10:07.72,0:10:08.81,Default,,0000,0000,0000,,Bởi vì...
Dialogue: 0,0:10:09.15,0:10:11.38,Default,,0000,0000,0000,,... Chika-chan, em đang chuẩn bị cho bài kiểm tra, đúng không?
Dialogue: 0,0:10:11.62,0:10:14.68,Default,,0000,0000,0000,,Chị không muốn khiến em nghĩ ngợi lung tung.
Dialogue: 0,0:10:15.79,0:10:18.66,Default,,0000,0000,0000,,Nên chị đã quyết định nói với em càng sớm càng tốt.
Dialogue: 0,0:10:19.93,0:10:22.09,Default,,0000,0000,0000,,Aya-neechan, chị bằng lòng như vậy sao?
Dialogue: 0,0:10:23.80,0:10:25.63,Default,,0000,0000,0000,,Em đang nói gì vậy?
Dialogue: 0,0:10:26.64,0:10:29.00,Default,,0000,0000,0000,,Em đã ngủ với Naoto-senpai.
Dialogue: 0,0:10:29.84,0:10:34.74,Default,,0000,0000,0000,,Nhưng trong lòng anh ấy vẫn chỉ có duy nhất mình chị.
Dialogue: 0,0:10:38.92,0:10:40.82,Default,,0000,0000,0000,,Thế nên, chị ghét em đúng không?
Dialogue: 0,0:10:40.89,0:10:44.25,Default,,0000,0000,0000,,Vậy nên đừng có ngồi ngây ra đó. Đừng ngồi ngây ra đó!
Dialogue: 0,0:11:13.42,0:11:15.21,Default,,0000,0000,0000,,Xin lỗi nhé, Chika-chan.
Dialogue: 0,0:11:15.69,0:11:19.35,Default,,0000,0000,0000,,Sẽ tốt hơn nếu em tỏ tình trước chị.
Dialogue: 0,0:11:20.26,0:11:23.42,Default,,0000,0000,0000,,Nhưng... nhưng, Aya-neechan, chị bằng lòng như vậy sao?
Dialogue: 0,0:11:23.49,0:11:24.86,Default,,0000,0000,0000,,Chị sẽ ổn thôi.
Dialogue: 0,0:11:24.93,0:11:28.96,Default,,0000,0000,0000,,Từ lâu chị đã biết tình cảm của em dành cho Naoto-kun.
Dialogue: 0,0:11:29.03,0:11:30.00,Default,,0000,0000,0000,,Nói dối!
Dialogue: 0,0:11:30.23,0:11:31.17,Default,,0000,0000,0000,,Chị không nói dối.
Dialogue: 0,0:11:31.24,0:11:36.17,Default,,0000,0000,0000,,Nếu vậy thì, từ lâu em cũng đã biết tình cảm của chị dành cho Naoto-senpai rồi!
Dialogue: 0,0:11:37.34,0:11:39.00,Default,,0000,0000,0000,,Vậy thì sao!
Dialogue: 0,0:11:41.21,0:11:42.11,Default,,0000,0000,0000,,C-Cái gì?
Dialogue: 0,0:11:42.75,0:11:45.87,Default,,0000,0000,0000,,Thế em bảo chị phải làm sao?
Dialogue: 0,0:11:46.28,0:11:51.88,Default,,0000,0000,0000,,Hôm qua, nhìn nét mặt của em là chị đã đoán ra tất cả.
Dialogue: 0,0:11:52.26,0:11:54.66,Default,,0000,0000,0000,,Và nếu ba chúng ta còn tiếp tục mối quan hệ này...
Dialogue: 0,0:11:54.73,0:11:57.56,Default,,0000,0000,0000,,Chị sẽ cản trở hai người âu yếm tình tứ bên nhau.
Dialogue: 0,0:11:57.63,0:12:00.60,Default,,0000,0000,0000,,Phải, em nói xem chuyện này giờ nên giải quyết thế nào?
Dialogue: 0,0:12:00.66,0:12:02.00,Default,,0000,0000,0000,,Không có, đúng không?
Dialogue: 0,0:12:03.07,0:12:04.66,Default,,0000,0000,0000,,Cái đó...
Dialogue: 0,0:12:04.80,0:12:07.93,Default,,0000,0000,0000,,Chị hiểu tình cảm của em.
Dialogue: 0,0:12:08.31,0:12:12.00,Default,,0000,0000,0000,,Nhưng tình cảm của chị cũng rất quan trọng.
Dialogue: 0,0:12:14.08,0:12:15.88,Default,,0000,0000,0000,,A-Aya-neechan?
Dialogue: 0,0:12:21.99,0:12:23.75,Default,,0000,0000,0000,,Mình phải là sao đây?
Dialogue: 0,0:12:35.93,0:12:37.23,Default,,0000,0000,0000,,Cái đó...
Dialogue: 0,0:12:37.30,0:12:40.36,Default,,0000,0000,0000,,Mình chỉ là một thằng ngốc, nhưng...
Dialogue: 0,0:12:40.67,0:12:42.66,Default,,0000,0000,0000,,Thế nào, Naoto-kun? Sướng không?
Dialogue: 0,0:12:43.27,0:12:46.47,Default,,0000,0000,0000,,Đây là lần đầu tiên em sử dụng phòng này, đúng không?
Dialogue: 0,0:12:47.21,0:12:50.37,Default,,0000,0000,0000,,Không một thằng đàn ông nào có thể cưỡng lại sức hấp dẫn từ bọn họ.
Dialogue: 0,0:12:51.48,0:12:53.64,Default,,0000,0000,0000,,Nhưng mình phải có đáp án dành cho họ.
Dialogue: 0,0:12:53.88,0:12:58.25,Default,,0000,0000,0000,,Nếu không, thì mình chẳng đáng mặt đàn ông.
Dialogue: 0,0:12:59.82,0:13:00.81,Default,,0000,0000,0000,,Đúng không?
Dialogue: 0,0:13:19.51,0:13:21.44,Default,,0000,0000,0000,,Nè, Aya-neechan...
Dialogue: 0,0:13:21.51,0:13:24.97,Default,,0000,0000,0000,,Ở đây ngượng lắm, chúng ta tạm rời đi trước đã.
Dialogue: 0,0:13:27.92,0:13:30.98,Default,,0000,0000,0000,,Cứ như mình là kẻ đanh đá vậy? Không thể nào!
Dialogue: 0,0:13:31.42,0:13:34.79,Default,,0000,0000,0000,,Được rồi, Chika-chan, hãy giải quyết dứt điểm chuyện này.
Dialogue: 0,0:13:34.96,0:13:37.09,Default,,0000,0000,0000,,Bao gồm cả chuyện ngoại tình của Naoto-kun...
Dialogue: 0,0:13:37.16,0:13:41.96,Default,,0000,0000,0000,,Chị nghĩ tốt nhất là chúng ta nên kết thúc nó ở đây.
Dialogue: 0,0:13:42.47,0:13:43.69,Default,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:13:44.17,0:13:47.70,Default,,0000,0000,0000,,Đây là con người thật của Aya-neechan sao?
Dialogue: 0,0:13:48.47,0:13:51.20,Default,,0000,0000,0000,,Cả hai đứa, biết lỗi của mình chưa?
Dialogue: 0,0:13:53.04,0:13:54.77,Default,,0000,0000,0000,,À, chị ấy vốn là người như vậy.
Dialogue: 0,0:13:57.78,0:13:59.68,Default,,0000,0000,0000,,Chika-chan, có gì đáng cười sao?
Dialogue: 0,0:13:59.75,0:14:04.55,Default,,0000,0000,0000,,À, em nhớ hồi nhỏ cả em và anh ấy đều sợ chị như sợ cọp.
Dialogue: 0,0:14:05.19,0:14:06.55,Default,,0000,0000,0000,,Ý em là sao?
Dialogue: 0,0:14:08.83,0:14:10.05,Default,,0000,0000,0000,,Đúng rồi!
Dialogue: 0,0:14:10.16,0:14:10.92,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:14:10.99,0:14:14.95,Default,,0000,0000,0000,,Rõ ràng là chị rất thích Naoto-senpai.
Dialogue: 0,0:14:15.57,0:14:17.69,Default,,0000,0000,0000,,D-Dĩ nhiên...
Dialogue: 0,0:14:17.84,0:14:20.27,Default,,0000,0000,0000,,Chị thích cậu ấy, và không chỉ có vậy...
Dialogue: 0,0:14:20.54,0:14:23.90,Default,,0000,0000,0000,,Nhưng chị không cho rằng làm thế là đúng.
Dialogue: 0,0:14:28.31,0:14:29.94,Default,,0000,0000,0000,,Tin nhắn của Naoto-senpai.
Dialogue: 0,0:14:32.02,0:14:34.21,Default,,0000,0000,0000,,Naoto-kun cũng nhắn cho chị nè.
Dialogue: 0,0:14:35.15,0:14:37.55,Default,,0000,0000,0000,,Em biết mình không phải một chàng trai tốt...
Dialogue: 0,0:14:37.62,0:14:39.92,Default,,0000,0000,0000,,em đã làm điều không phải với cả Chika-chan và Ayane-san...
Dialogue: 0,0:14:39.99,0:14:44.26,Default,,0000,0000,0000,,dù có xin lỗi cũng không thể chuộc hết lỗi lầm.
Dialogue: 0,0:14:44.56,0:14:48.55,Default,,0000,0000,0000,,Anh muốn gặp trực tiếp hai người để kết thúc chuyện này.
Dialogue: 0,0:14:48.70,0:14:51.22,Default,,0000,0000,0000,,Hãy đến phòng của anh.
Dialogue: 0,0:14:53.57,0:14:55.30,Default,,0000,0000,0000,,Nè, Aya-neechan...
Dialogue: 0,0:14:55.94,0:14:57.43,Default,,0000,0000,0000,,Gì thế, Chika-chan?
Dialogue: 0,0:14:57.98,0:15:01.91,Default,,0000,0000,0000,,Naoto-senpai muốn gặp mặt chúng ta.
Dialogue: 0,0:15:02.28,0:15:06.38,Default,,0000,0000,0000,,Đúng vậy. Chúng ta cần phải chuẩn bị trước.
Dialogue: 0,0:15:06.75,0:15:11.15,Default,,0000,0000,0000,,Nhưng nếu anh ấy không chọn cả hai, chị định sẽ làm sao?
Dialogue: 0,0:15:11.62,0:15:15.49,Default,,0000,0000,0000,,Naoto-kun trông bề ngoài vậy thôi, chị tin cậu ấy là người có trách nhiệm...
Dialogue: 0,0:15:15.56,0:15:17.15,Default,,0000,0000,0000,,chuyện đó sẽ không xảy ra đâu.
Dialogue: 0,0:15:17.23,0:15:20.72,Default,,0000,0000,0000,,Nếu không được chọn, em sẽ buồn lắm, nhưng mà...
Dialogue: 0,0:15:22.37,0:15:28.00,Default,,0000,0000,0000,,Em không muốn trông thấy Naoto-senpai đi với cô gái nào khác ngoài chị.
Dialogue: 0,0:15:29.51,0:15:30.80,Default,,0000,0000,0000,,Nếu chuyện đó xảy ra...
Dialogue: 0,0:15:30.87,0:15:32.10,Default,,0000,0000,0000,,Nếu chuyện đó xảy ra?
Dialogue: 0,0:15:42.92,0:15:45.58,Default,,0000,0000,0000,,Này cả hai người, mời vào...
Dialogue: 0,0:15:50.06,0:15:52.12,Default,,0000,0000,0000,,T-Trước hết...
Dialogue: 0,0:15:54.10,0:15:55.16,Default,,0000,0000,0000,,Gì vậy?
Dialogue: 0,0:16:04.27,0:16:10.18,Default,,0000,0000,0000,,Tình cảm giữa anh và Ayane-san đã rạn nứt, nhưng anh cũng không thể hẹn hò với Chika-chan được.
Dialogue: 0,0:16:13.45,0:16:17.18,Default,,0000,0000,0000,,Anh đã làm chuyện đó với Chika-chan.
Dialogue: 0,0:16:17.45,0:16:20.58,Default,,0000,0000,0000,,Anh biết mình không phải một thằng đàn ông tốt.
Dialogue: 0,0:16:20.89,0:16:23.09,Default,,0000,0000,0000,,Nhưng nói vậy cũng không có nghĩa là anh sẽ...
Dialogue: 0,0:16:23.16,0:16:26.60,Default,,0000,0000,0000,,chia tay với Ayane-san và bắt đầu hẹn hò với Chika-chan.
Dialogue: 0,0:16:27.10,0:16:31.00,Default,,0000,0000,0000,,Vì vậy hãy quên anh đi!
Dialogue: 0,0:16:42.01,0:16:45.24,Default,,0000,0000,0000,,Nè, Naoto-kun, đó là quyết định của em à?
Dialogue: 0,0:16:45.32,0:16:47.94,Default,,0000,0000,0000,,V-Vâng, em đã suy nghĩ rất nhiều...
Dialogue: 0,0:16:49.19,0:16:52.21,Default,,0000,0000,0000,,Anh có thể nghe ý kiến của bọn em không?
Dialogue: 0,0:16:52.32,0:16:53.41,Default,,0000,0000,0000,,Dĩ nhiên!
Dialogue: 0,0:16:53.79,0:16:57.32,Default,,0000,0000,0000,,Anh không chắc là mình có thể trả lời ngay lúc này được.
Dialogue: 0,0:17:08.84,0:17:12.43,Default,,0000,0000,0000,,Chính anh là kẻ đã đưa hai người vào hoàn cảnh này.
Dialogue: 0,0:17:12.51,0:17:14.60,Default,,0000,0000,0000,,Nhưng việc chọn một trong hai người là điều không thể.
Dialogue: 0,0:17:14.68,0:17:18.11,Default,,0000,0000,0000,,Cả ba chúng ta chơi thân với nhau từ nhỏ, anh rất thích vẻ đáng yêu của Chika-chan.
Dialogue: 0,0:17:18.18,0:17:20.55,Default,,0000,0000,0000,,Và anh cũng yêu nét quyến rũ thành thục của Ayane-san.
Dialogue: 0,0:17:20.78,0:17:24.69,Default,,0000,0000,0000,,Nói cách khác, cảm giác của anh...
Dialogue: 0,0:17:24.76,0:17:27.78,Default,,0000,0000,0000,,Anh không thể quyết định nổi việc chọn ai và bỏ ai.
Dialogue: 0,0:17:27.96,0:17:31.79,Default,,0000,0000,0000,,Không, không, không phải đâu. Ý anh là...
Dialogue: 0,0:17:32.16,0:17:35.53,Default,,0000,0000,0000,,Vậy tại sao anh không bắt cá hai tay luôn đi?
Dialogue: 0,0:17:35.67,0:17:37.00,Default,,0000,0000,0000,,Phải không, Aya-neechan?
Dialogue: 0,0:17:37.07,0:17:38.33,Default,,0000,0000,0000,,Đ-Đúng vậy.
Dialogue: 0,0:17:38.40,0:17:41.06,Default,,0000,0000,0000,,Đề nghị như vậy được không nhỉ?
Dialogue: 0,0:17:41.14,0:17:43.77,Default,,0000,0000,0000,,Cả hai? Đó là chuyện không thể...
Dialogue: 0,0:17:44.41,0:17:46.40,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:17:51.65,0:17:53.45,Default,,0000,0000,0000,,Bên nào cũng được.
Dialogue: 0,0:17:55.92,0:17:58.79,Default,,0000,0000,0000,,Đúng như anh nghĩ, như vậy là không được.
Dialogue: 0,0:18:01.29,0:18:02.42,Default,,0000,0000,0000,,Được mà!
Dialogue: 0,0:18:02.49,0:18:06.49,Default,,0000,0000,0000,,Đây là phương pháp tốt nhất mà chúng em nghĩ ra.
Dialogue: 0,0:18:06.73,0:18:09.96,Default,,0000,0000,0000,,Naoto-senpai, anh chỉ cần mỉm cười.
Dialogue: 0,0:18:14.27,0:18:17.21,Default,,0000,0000,0000,,Chị cũng nghĩ như vậy là tốt nhất.
Dialogue: 0,0:18:17.34,0:18:19.64,Default,,0000,0000,0000,,Giống như hồi bé vậy.
Dialogue: 0,0:18:20.38,0:18:23.87,Default,,0000,0000,0000,,Thôi nào, Aya-neechan cũng đã nói vậy rồi.
Dialogue: 0,0:18:36.66,0:18:39.60,Default,,0000,0000,0000,,Nhưng đạo lý...
Dialogue: 0,0:18:39.66,0:18:42.69,Default,,0000,0000,0000,,Chỉ được cái nói mồm là giỏi, anh sắp ra rồi nè.
Dialogue: 0,0:18:44.20,0:18:46.60,Default,,0000,0000,0000,,Đúng không, Senpai?
Dialogue: 0,0:18:54.91,0:18:56.61,Default,,0000,0000,0000,,Senpai, sao vậy?
Dialogue: 0,0:18:56.68,0:18:58.74,Default,,0000,0000,0000,,Em nói đúng.
Dialogue: 0,0:18:59.22,0:19:01.41,Default,,0000,0000,0000,,Anh chỉ được mỗi cái mồm, còn lại đều cực kì vô dụng.
Dialogue: 0,0:19:01.49,0:19:04.35,Default,,0000,0000,0000,,Hừ, anh vẫn còn nói như vậy sao?
Dialogue: 0,0:19:04.42,0:19:06.36,Default,,0000,0000,0000,,Senpai, làm gì có thằng đàn ông nào như anh.
Dialogue: 0,0:19:06.42,0:19:07.52,Default,,0000,0000,0000,,Em bằng lòng như vậy sao?
Dialogue: 0,0:19:07.59,0:19:08.52,Default,,0000,0000,0000,,Ừm!
Dialogue: 0,0:19:08.96,0:19:10.43,Default,,0000,0000,0000,,Còn Ayane-san thì sao?
Dialogue: 0,0:19:12.46,0:19:14.86,Default,,0000,0000,0000,,Rồi, công tắc đã được bật!
Dialogue: 0,0:19:15.50,0:19:18.30,Default,,0000,0000,0000,,Hệt như giấc mơ vậy.
Dialogue: 0,0:19:19.00,0:19:21.03,Default,,0000,0000,0000,,Đó không phải là mơ. Đó...
Dialogue: 0,0:19:21.10,0:19:22.63,Default,,0000,0000,0000,,là sự thật.
Dialogue: 0,0:19:28.45,0:19:32.40,Default,,0000,0000,0000,,Tinh dịch của Senpai thật đặc biệt.
Dialogue: 0,0:19:33.12,0:19:35.14,Default,,0000,0000,0000,,Đến lượt chị, Aya-neechan.
Dialogue: 0,0:19:43.49,0:19:47.55,Default,,0000,0000,0000,,Nước bọt của em trộn lẫn với tinh dịch của Senpai hương vị thế nào?
Dialogue: 0,0:19:50.23,0:19:51.53,Default,,0000,0000,0000,,Ngon lắm.
Dialogue: 0,0:19:57.27,0:20:01.54,Default,,0000,0000,0000,,Aya-neechan, chị xấu lắm!
Dialogue: 0,0:20:01.81,0:20:04.44,Default,,0000,0000,0000,,Chớ có dại mà đùa với chị.
Dialogue: 0,0:20:04.51,0:20:05.71,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:20:06.85,0:20:09.18,Default,,0000,0000,0000,,Em vừa nảy ra một ý hay.
Dialogue: 0,0:20:11.89,0:20:13.69,Default,,0000,0000,0000,,K-Không...
Dialogue: 0,0:20:13.92,0:20:16.45,Default,,0000,0000,0000,,H-Hãy dừng chuyện này lại đi.
Dialogue: 0,0:20:16.69,0:20:18.99,Default,,0000,0000,0000,,Nhưng Chika-chan đã nói... đúng không?
Dialogue: 0,0:20:19.46,0:20:23.52,Default,,0000,0000,0000,,Đúng rồi đó, Naoto-senpai. Nếu anh không chịu thì sẽ có chuyện tồi tệ xảy ra đó.
Dialogue: 0,0:20:23.60,0:20:24.37,Default,,0000,0000,0000,,Em ấy đã nói vậy.
Dialogue: 0,0:20:24.37,0:20:25.49,Default,,0000,0000,0000,,Không thể nào...
Dialogue: 0,0:20:27.17,0:20:28.26,Default,,0000,0000,0000,,K-Không...
Dialogue: 0,0:20:28.37,0:20:30.43,Default,,0000,0000,0000,,Aya-neechan, thật thô bạo.
Dialogue: 0,0:20:30.57,0:20:32.01,Default,,0000,0000,0000,,Em sẽ liếm nó dùm chị.
Dialogue: 0,0:20:37.78,0:20:40.84,Default,,0000,0000,0000,,Chà, âm vật của chị đang phồng lên này.
Dialogue: 0,0:20:40.92,0:20:42.01,Default,,0000,0000,0000,,Em sẽ véo nó thử xem sao.
Dialogue: 0,0:20:42.09,0:20:44.14,Default,,0000,0000,0000,,Không, không!
Dialogue: 0,0:20:44.22,0:20:45.78,Default,,0000,0000,0000,,Đừng cấu véo chỗ đó!
Dialogue: 0,0:21:04.98,0:21:07.67,Default,,0000,0000,0000,,Được rồi Aya-neechan, chúng ta tiếp tục nào.
Dialogue: 0,0:21:12.45,0:21:14.64,Default,,0000,0000,0000,,Sâu hơn... Sâu hơn chút nữa...
Dialogue: 0,0:21:15.95,0:21:19.82,Default,,0000,0000,0000,,Nó đâm vào sâu bên trong chị, đúng không, Aya-neechan?
Dialogue: 0,0:21:20.82,0:21:23.05,Default,,0000,0000,0000,,Sướng quá!
Dialogue: 0,0:21:23.13,0:21:25.72,Default,,0000,0000,0000,,Nó đang đâm vào điểm cực khoái của chị.
Dialogue: 0,0:21:26.63,0:21:28.00,Default,,0000,0000,0000,,Dạ dày của chị!
Dialogue: 0,0:21:28.13,0:21:30.07,Default,,0000,0000,0000,,Dạ dày của chị!
Dialogue: 0,0:21:39.04,0:21:41.41,Default,,0000,0000,0000,,Nó đang cọ xát bên trong thành dạ dày.
Dialogue: 0,0:21:42.01,0:21:43.45,Default,,0000,0000,0000,,Đau lắm!
Dialogue: 0,0:21:43.91,0:21:46.91,Default,,0000,0000,0000,,Đau quá đi mất!
Dialogue: 0,0:22:03.53,0:22:08.44,Default,,0000,0000,0000,,Không! Hậu môn chị sắp phun ra rồi!
Dialogue: 0,0:22:22.19,0:22:23.31,Default,,0000,0000,0000,,Xác chết sống lại!
Dialogue: 0,0:22:23.59,0:22:26.82,Default,,0000,0000,0000,,Chika-chan, chị sẽ cùng em chia sẻ món quà tặng này.
Dialogue: 0,0:22:27.46,0:22:31.26,Default,,0000,0000,0000,,Nếu em muốn từ chối thì sao ...
Dialogue: 0,0:22:32.50,0:22:35.59,Default,,0000,0000,0000,,Em nghĩ mình có thể chạy thoát được sao?
Dialogue: 0,0:22:44.67,0:22:46.70,Default,,0000,0000,0000,,Nó đang cọ xát ở bên trong!
Dialogue: 0,0:22:46.88,0:22:49.47,Default,,0000,0000,0000,,Nó đang cọ xát tại cuống tử cung của em!
Dialogue: 0,0:23:01.69,0:23:04.66,Default,,0000,0000,0000,,Chika-chan, mỗi lần nó đâm vào hậu môn...
Dialogue: 0,0:23:04.73,0:23:07.39,Default,,0000,0000,0000,,nước tiểu của em lại trào ra như suối vậy.
Dialogue: 0,0:23:08.03,0:23:09.19,Default,,0000,0000,0000,,Dễ thương ghê ta.
Dialogue: 0,0:23:10.90,0:23:14.70,Default,,0000,0000,0000,,Aya-neechan, đừng nói những điều biến thái như vậy.
Dialogue: 0,0:23:14.84,0:23:16.43,Default,,0000,0000,0000,,Bởi vì... bởi vì...
Dialogue: 0,0:23:16.61,0:23:22.10,Default,,0000,0000,0000,,Khi Naoto-senpai chà xát với cuống tử cung của em...
Dialogue: 0,0:23:22.18,0:23:23.65,Default,,0000,0000,0000,,Em sắp ra rồi!
Dialogue: 0,0:23:23.81,0:23:28.22,Default,,0000,0000,0000,,Tử cung em sắp đạt cực khoái rồi, em sắp không nhịn nổi nữa rồi!
Dialogue: 0,0:23:32.92,0:23:33.98,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:23:34.66,0:23:35.85,Default,,0000,0000,0000,,Em ra rồi!
Dialogue: 0,0:23:39.53,0:23:43.26,Default,,0000,0000,0000,,Không, em phải ra cùng lúc với Naoto-kun.
Dialogue: 0,0:23:43.33,0:23:45.63,Default,,0000,0000,0000,,Nhé? Chika-chan, cố lên nào.
Dialogue: 0,0:23:46.14,0:23:48.33,Default,,0000,0000,0000,,Em sẽ cố...
Dialogue: 0,0:23:48.41,0:23:51.93,Default,,0000,0000,0000,,Em sẽ cố hết sức, mạnh nữa lên nào!
Dialogue: 0,0:23:54.68,0:23:56.65,Default,,0000,0000,0000,,Mạnh nữa lên!
Dialogue: 0,0:23:56.78,0:23:59.72,Default,,0000,0000,0000,,Nữa đi! Em, em...
Dialogue: 0,0:23:59.78,0:24:04.58,Default,,0000,0000,0000,,Em muốn uống sữa đặc của anh, Naoto-senpai!
Dialogue: 0,0:24:05.32,0:24:07.95,Default,,0000,0000,0000,,Em sẽ uống sữa đặc của Naoto-kun bằng hậu môn của mình.
Dialogue: 0,0:24:12.80,0:24:17.93,Default,,0000,0000,0000,,E-Em ra rồi! Ra bằng hậu môn!
Dialogue: 0,0:02:10.27,0:02:20.22,Default,,0000,0000,0000,,{\fs45\bord3\fad(3600,1400)\fnUVN Phuong Tay\3c&HFFFFFF&\c&H3300CB&\pos(355,115)}Baka-sub trân trọng {\c&H0A8EDF&}giới thiệu
