Dialogue: 0,0:00:17.84,0:00:23.64,Default,,0000,0000,0000,,Đầu tiên hãy chầm chậm liếm xung quanh âm hộ tôi.
Dialogue: 0,0:00:23.78,0:00:24.87,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:00:27.55,0:00:31.65,Default,,0000,0000,0000,,Phải, đưa lưỡi lên xuống ở đoạn giữa khe.
Dialogue: 0,0:00:38.79,0:00:41.42,Default,,0000,0000,0000,,Tsuzaki-kun, sướng quá.
Dialogue: 0,0:00:42.26,0:00:43.46,Default,,0000,0000,0000,,Anh làm tốt lắm.
Dialogue: 0,0:00:44.40,0:00:47.73,Default,,0000,0000,0000,,Âm hộ của cô thật hồng hào và xinh đẹp.
Dialogue: 0,0:00:51.91,0:00:53.74,Default,,0000,0000,0000,,Thấy sao hả, Kanae-san?
Dialogue: 0,0:00:55.14,0:00:56.04,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:00:56.41,0:00:58.61,Default,,0000,0000,0000,,Anh cứ tiếp tục làm vậy đi.
Dialogue: 0,0:00:59.38,0:01:04.72,Default,,0000,0000,0000,,Chỗ đó nhạy cảm lắm, anh phải làm thật nhẹ nhàng đấy nhé.
Dialogue: 0,0:01:04.95,0:01:05.61,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:01:09.79,0:01:10.76,Default,,0000,0000,0000,,Kanae-san.
Dialogue: 0,0:01:12.96,0:01:13.89,Default,,0000,0000,0000,,Tsuzaki-kun...
Dialogue: 0,0:01:14.80,0:01:19.29,Default,,0000,0000,0000,,Bây giờ hãy mân mê vào thật sâu bên trong âm hộ tôi.
Dialogue: 0,0:01:20.50,0:01:24.56,Default,,0000,0000,0000,,Tuyệt quá, chỗ đó của cô ướt đẫm rồi nè.
Dialogue: 0,0:01:28.08,0:01:29.04,Default,,0000,0000,0000,,Đồ ngốc.
Dialogue: 0,0:01:30.58,0:01:32.67,Default,,0000,0000,0000,,Anh đang làm tôi ngượng đó.
Dialogue: 0,0:01:33.22,0:01:34.34,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:01:45.96,0:01:50.59,Default,,0000,0000,0000,,Khít quá đi mất, dường như nó sắp nuốt chửng ngón tay tôi.
Dialogue: 0,0:01:52.77,0:01:55.03,Default,,0000,0000,0000,,Phải rồi, tiếp tục đi.
Dialogue: 0,0:02:19.86,0:02:21.35,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:02:21.80,0:02:22.79,Default,,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:02:27.74,0:02:29.70,Default,,0000,0000,0000,,Mạnh nữa lên.
Dialogue: 0,0:02:40.85,0:02:42.65,Default,,0000,0000,0000,,Mau ra đi nào, Kanae-san.
Dialogue: 0,0:02:52.13,0:02:54.32,Default,,0000,0000,0000,,Ối, không...
Dialogue: 0,0:02:57.70,0:02:58.63,Default,,0000,0000,0000,,Á!
Dialogue: 0,0:03:34.50,0:03:35.80,Default,,0000,0000,0000,,Hể?
Dialogue: 0,0:03:36.67,0:03:39.77,Default,,0000,0000,0000,,Phụ đạo cho Chiho-san môn kế toán sao ?
Dialogue: 0,0:03:43.18,0:03:47.08,Default,,0000,0000,0000,,Ừ, nhờ thế mà chúng tôi được ở một mình...
Dialogue: 0,0:03:49.38,0:03:53.38,Default,,0000,0000,0000,,... nhưng vì phải dạy học nên muốn tán tỉnh cưa cẩm cũng khó lắm.
Dialogue: 0,0:03:53.92,0:03:55.32,Default,,0000,0000,0000,,Không cần phải vội vàng.
Dialogue: 0,0:03:55.82,0:04:01.66,Default,,0000,0000,0000,,Việc thường xuyên gặp tôi trong ngày có vẻ khó khăn nhỉ...
Dialogue: 0,0:04:02.70,0:04:06.19,Default,,0000,0000,0000,,... chưa kể cậu còn phải dạy Chiho-san vào buổi tối nữa chứ.
Dialogue: 0,0:04:06.37,0:04:07.60,Default,,0000,0000,0000,,Không cần lo cho tôi đâu.
Dialogue: 0,0:04:07.94,0:04:13.74,Default,,0000,0000,0000,,Tiến vẫn dạy rất tốt, không có gì đáng phàn nàn cả.
Dialogue: 0,0:04:17.51,0:04:21.18,Default,,0000,0000,0000,,Tôi ghé cửa hàng tạp hóa bên dưới nhà đây.
Dialogue: 0,0:04:22.02,0:04:24.95,Default,,0000,0000,0000,,Tôi cũng định mua vài thứ, chúng ta đi cùng nhé.
Dialogue: 0,0:05:10.60,0:05:14.06,Default,,0000,0000,0000,,Có lẽ mình nên mua đồ ăn tối luôn nhỉ.
Dialogue: 0,0:05:14.67,0:05:17.30,Default,,0000,0000,0000,,Ồ, anh hay ăn mì gói lắm à?
Dialogue: 0,0:05:17.77,0:05:19.67,Default,,0000,0000,0000,,Tôi là chàng trai độc thân mà.
Dialogue: 0,0:05:20.14,0:05:24.34,Default,,0000,0000,0000,,Suốt từ hôm dùng bữa tối tại nhà cô tới giờ, tôi toàn ăn như vậy.
Dialogue: 0,0:05:24.91,0:05:28.01,Default,,0000,0000,0000,,Tài nấu nướng của cô là nhất đấy.
Dialogue: 0,0:05:31.99,0:05:33.39,Default,,0000,0000,0000,,C-Chiho-san?
Dialogue: 0,0:05:34.96,0:05:37.43,Default,,0000,0000,0000,,Hai người đi mua sắm cùng nhau à?
Dialogue: 0,0:05:38.03,0:05:39.99,Default,,0000,0000,0000,,Không, không như cô nghĩ đâu.
Dialogue: 0,0:05:40.50,0:05:41.29,Default,,0000,0000,0000,,Mẹ ơi!
Dialogue: 0,0:05:42.90,0:05:43.89,Default,,0000,0000,0000,,Konomi-chan!
Dialogue: 0,0:05:44.27,0:05:45.82,Default,,0000,0000,0000,,Mẹ đi mua sắm với Ba hả?
Dialogue: 0,0:05:46.30,0:05:48.10,Default,,0000,0000,0000,,Hai người nhìn như một cặp vậy.
Dialogue: 0,0:05:50.17,0:05:51.33,Default,,0000,0000,0000,,Xin phép hai người.
Dialogue: 0,0:05:51.54,0:05:52.34,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:05:58.75,0:06:00.54,Default,,0000,0000,0000,,Tệ rồi đây.
Dialogue: 0,0:06:00.88,0:06:01.97,Default,,0000,0000,0000,,Làm sao bây giờ?
Dialogue: 0,0:06:02.28,0:06:05.78,Default,,0000,0000,0000,,Mẹ ơi, bộ con vừa làm sai gì à?
Dialogue: 0,0:06:07.46,0:06:08.29,Default,,0000,0000,0000,,Không đâu.
Dialogue: 0,0:06:10.53,0:06:13.22,Default,,0000,0000,0000,,Bé không có làm sai gì hết.
Dialogue: 0,0:06:25.11,0:06:27.51,Default,,0000,0000,0000,,E rằng Chiho-san vẫn còn giận mình...
Dialogue: 0,0:06:27.64,0:06:28.27,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:06:29.38,0:06:32.58,Default,,0000,0000,0000,,Chiho-san, là tôi-Tsuzaki đây.
Dialogue: 0,0:06:33.02,0:06:34.51,Default,,0000,0000,0000,,Mời anh vào nhà.
Dialogue: 0,0:06:35.35,0:06:39.58,Default,,0000,0000,0000,,Ơn trời, mình cứ lo là cô ấy không muốn nhìn mặt mình nữa chứ.
Dialogue: 0,0:06:39.89,0:06:40.86,Default,,0000,0000,0000,,Tôi xin phép.
Dialogue: 0,0:06:44.03,0:06:45.39,Default,,0000,0000,0000,,Mùi thơm quá.
Dialogue: 0,0:06:47.66,0:06:49.10,Default,,0000,0000,0000,,Chào anh, Tsuzaki-san.
Dialogue: 0,0:06:49.93,0:06:51.69,Default,,0000,0000,0000,,Cô chuẩn bị ăn cơm tối hả?
Dialogue: 0,0:06:52.37,0:06:54.03,Default,,0000,0000,0000,,Vậy tôi quay lại sau nha.
Dialogue: 0,0:06:55.64,0:06:59.97,Default,,0000,0000,0000,,Nếu anh không phiền thì dùng bữa tối cùng tôi luôn nhé?
Dialogue: 0,0:07:00.61,0:07:02.91,Default,,0000,0000,0000,,Ý cô là...
Dialogue: 0,0:07:03.61,0:07:06.60,Default,,0000,0000,0000,,Tôi có thể ăn tối cùng cô sao?
Dialogue: 0,0:07:07.18,0:07:07.84,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:07:08.55,0:07:10.25,Default,,0000,0000,0000,,Tốt quá, cảm ơn cô.
Dialogue: 0,0:07:10.65,0:07:12.52,Default,,0000,0000,0000,,Hôm nay là ngày hạnh phúc nhất đời tôi.
Dialogue: 0,0:07:21.46,0:07:22.90,Default,,0000,0000,0000,,Đồ ăn ngon quá.
Dialogue: 0,0:07:24.07,0:07:25.47,Default,,0000,0000,0000,,Cảm ơn anh.
Dialogue: 0,0:07:26.17,0:07:30.94,Default,,0000,0000,0000,,Được thưởng thức món ăn do Chiho-san nấu quả là diễm phúc.
Dialogue: 0,0:07:31.44,0:07:34.14,Default,,0000,0000,0000,,Nhưng sao hôm nay đột nhiên...
Dialogue: 0,0:07:34.54,0:07:35.77,Default,,0000,0000,0000,,Ờ thì...
Dialogue: 0,0:07:35.91,0:07:37.57,Default,,0000,0000,0000,,Không có gì.
Dialogue: 0,0:07:38.05,0:07:41.35,Default,,0000,0000,0000,,Chẳng qua tôi nghe anh than vãn là phải ăn mì gói suốt.
Dialogue: 0,0:07:41.75,0:07:44.55,Default,,0000,0000,0000,,Xin lỗi đã khiến cô lo lắng.
Dialogue: 0,0:07:45.02,0:07:46.39,Default,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:07:46.72,0:07:52.09,Default,,0000,0000,0000,,Tay nghề của tôi chỉ hơn đồ ăn sẵn ở siêu thị một chút thôi.
Dialogue: 0,0:07:52.89,0:07:56.06,Default,,0000,0000,0000,,Chứ so với Kanae thì tôi nào dám...
Dialogue: 0,0:07:57.83,0:07:59.43,Default,,0000,0000,0000,,C-Chiho-san.
Dialogue: 0,0:08:01.70,0:08:06.04,Default,,0000,0000,0000,,Dạo này anh với Kanae-san có vẻ hợp nhau nhỉ.
Dialogue: 0,0:08:07.41,0:08:13.44,Default,,0000,0000,0000,,Được Kanae-san nấu cho có khác, thảo nào anh suốt ngày sang chơi.
Dialogue: 0,0:08:13.82,0:08:15.34,Default,,0000,0000,0000,,Ấy không phải!
Dialogue: 0,0:08:16.18,0:08:17.85,Default,,0000,0000,0000,,Cô hiểu nhầm rồi.
Dialogue: 0,0:08:18.39,0:08:21.72,Default,,0000,0000,0000,,Tôi chỉ ghé chỗ cô ấy để xin vài lời khuyên thôi.
Dialogue: 0,0:08:22.29,0:08:26.69,Default,,0000,0000,0000,,Cô ấy là phụ nữ có chồng nên biết nhiều về cách ứng xử đó mà.
Dialogue: 0,0:08:27.80,0:08:29.66,Default,,0000,0000,0000,,Bộ anh gặp rắc rối về tình cảm à?
Dialogue: 0,0:08:30.23,0:08:32.43,Default,,0000,0000,0000,,Ừ, đúng vậy...
Dialogue: 0,0:08:33.60,0:08:37.04,Default,,0000,0000,0000,,Vậy anh cứ nói tôi nghe thử đi.
Dialogue: 0,0:08:37.81,0:08:41.33,Default,,0000,0000,0000,,Tôi cũng từng có chồng mà.
Dialogue: 0,0:08:42.04,0:08:44.44,Default,,0000,0000,0000,,Làm sao có thể xin ý kiến từ chính người phụ nữ mà tôi thầm thương trộm nhớ chứ?
Dialogue: 0,0:08:46.05,0:08:48.17,Default,,0000,0000,0000,,Ừm thì...
Dialogue: 0,0:08:48.58,0:08:49.71,Default,,0000,0000,0000,,Tsuzaki-san.
Dialogue: 0,0:08:53.86,0:08:56.19,Default,,0000,0000,0000,,Chắc cô cũng biết tình cảm mà tôi dành cho cô.
Dialogue: 0,0:08:58.76,0:09:02.09,Default,,0000,0000,0000,,Tôi rất thích Chiho-san.
Dialogue: 0,0:09:03.67,0:09:07.26,Default,,0000,0000,0000,,Nhưng Ryuhei là người đàn ông duy nhất tôi yêu.
Dialogue: 0,0:09:08.34,0:09:13.40,Default,,0000,0000,0000,,Tôi biết cô không thể quên người chồng quá cố.
Dialogue: 0,0:09:14.04,0:09:18.91,Default,,0000,0000,0000,,Nhưng cô không cần phải dè dặt chuyện cô yêu ai hay ai yêu cô.
Dialogue: 0,0:09:21.85,0:09:22.75,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:09:22.95,0:09:23.85,Default,,0000,0000,0000,,Anh yêu em.
Dialogue: 0,0:09:25.09,0:09:25.92,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:09:32.79,0:09:33.95,Default,,0000,0000,0000,,Tsuzaki-san.
Dialogue: 0,0:09:34.46,0:09:35.52,Default,,0000,0000,0000,,Anh yêu em.
Dialogue: 0,0:09:48.71,0:09:49.90,Default,,0000,0000,0000,,Tsuzaki-san...
Dialogue: 0,0:09:50.81,0:09:52.01,Default,,0000,0000,0000,,Đừng mà...
Dialogue: 0,0:09:55.82,0:09:56.78,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:10:01.96,0:10:04.48,Default,,0000,0000,0000,,Tsuzaki-san...
Dialogue: 0,0:10:20.54,0:10:21.51,Default,,0000,0000,0000,,Tsuzaki-san.
Dialogue: 0,0:10:24.51,0:10:25.64,Default,,0000,0000,0000,,Em ướt đẫm rồi nè.
Dialogue: 0,0:10:26.98,0:10:29.68,Default,,0000,0000,0000,,Sướng không hả, Chiho-san?
Dialogue: 0,0:10:30.22,0:10:32.05,Default,,0000,0000,0000,,Không, đừng có nói vậy.
Dialogue: 0,0:10:32.79,0:10:35.98,Default,,0000,0000,0000,,Kanae cũng kêu tôi đừng chú ý đến những chuyện như thế.
Dialogue: 0,0:10:37.33,0:10:40.85,Default,,0000,0000,0000,,Quần lót của em dơ rồi, để anh cởi ra nhé.
Dialogue: 0,0:10:52.44,0:10:53.60,Default,,0000,0000,0000,,Em xấu hổ quá.
Dialogue: 0,0:11:11.36,0:11:11.95,Default,,0000,0000,0000,,Ah, đừng...
Dialogue: 0,0:11:15.00,0:11:17.02,Default,,0000,0000,0000,,Anh làm tuyệt quá.
Dialogue: 0,0:11:27.51,0:11:28.37,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:11:29.64,0:11:30.74,Default,,0000,0000,0000,,Em sẵn sàng chưa?
Dialogue: 0,0:11:32.38,0:11:33.11,Default,,0000,0000,0000,,Đợi đã!
Dialogue: 0,0:11:34.08,0:11:36.61,Default,,0000,0000,0000,,À này...
Dialogue: 0,0:11:37.19,0:11:40.28,Default,,0000,0000,0000,,Um, anh nên dùng bao cao su.
Dialogue: 0,0:11:41.09,0:11:43.28,Default,,0000,0000,0000,,À, được thôi...
Dialogue: 0,0:11:44.29,0:11:47.19,Default,,0000,0000,0000,,Nhưng anh không có mang theo cái nào hết.
Dialogue: 0,0:11:50.53,0:11:54.99,Default,,0000,0000,0000,,Từ ngày chồng mất em cũng không để dành cái nào cả...
Dialogue: 0,0:11:58.87,0:12:02.90,Default,,0000,0000,0000,,Bộ anh thật sự phải dùng bao cao su hả?
Dialogue: 0,0:12:03.28,0:12:04.97,Default,,0000,0000,0000,,Dĩ nhiên rồi!
Dialogue: 0,0:12:05.48,0:12:08.28,Default,,0000,0000,0000,,Bởi vì hiện giờ chúng ta chưa có làm đám cưới mà.
Dialogue: 0,0:12:08.38,0:12:09.51,Default,,0000,0000,0000,,Đám cưới?
Dialogue: 0,0:12:10.12,0:12:11.45,Default,,0000,0000,0000,,Em với anh?
Dialogue: 0,0:12:12.69,0:12:14.81,Default,,0000,0000,0000,,K-Không, ý em không phải như vậy...
Dialogue: 0,0:12:15.49,0:12:18.15,Default,,0000,0000,0000,,Hay để em dùng miệng giúp anh thỏa mãn nhé...
Dialogue: 0,0:12:18.33,0:12:18.92,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:12:19.26,0:12:22.46,Default,,0000,0000,0000,,Khi đàn ông không thể xuất ra được thì bứt rứt khó chịu lắm phải không?
Dialogue: 0,0:12:22.56,0:12:23.83,Default,,0000,0000,0000,,C-Chiho-san?
Dialogue: 0,0:12:28.10,0:12:31.90,Default,,0000,0000,0000,,Em không thành thục chuyện này lắm, vậy nên anh đừng phiền lòng nha
Dialogue: 0,0:12:38.78,0:12:39.80,Default,,0000,0000,0000,,Thế nào hả?
Dialogue: 0,0:12:40.48,0:12:41.88,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:12:42.48,0:12:44.28,Default,,0000,0000,0000,,Tiếp tục đi em.
Dialogue: 0,0:12:45.05,0:12:45.58,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:13:10.68,0:13:13.58,Default,,0000,0000,0000,,Chiho-san, anh sắp ra rồi.
Dialogue: 0,0:13:16.25,0:13:18.65,Default,,0000,0000,0000,,Nếu em tiếp tục, anh sẽ...
Dialogue: 0,0:13:24.56,0:13:26.58,Default,,0000,0000,0000,,Anh xin lỗi, Chiho-san.
Dialogue: 0,0:13:27.26,0:13:28.25,Default,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:13:28.60,0:13:32.16,Default,,0000,0000,0000,,Lâu rồi em không có làm chuyện này.
Dialogue: 0,0:13:35.74,0:13:37.07,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:13:49.18,0:13:51.38,Default,,0000,0000,0000,,Ông chủ, cho cho tôi ít bao cao su!
Dialogue: 0,0:13:52.29,0:13:56.05,Default,,0000,0000,0000,,Không đâu anh bạn, tôi không cho đâu, tôi chỉ bán thôi.
Dialogue: 0,0:13:56.86,0:13:59.62,Default,,0000,0000,0000,,Ý tôi là bán cho tôi mấy cái!
Dialogue: 0,0:14:00.70,0:14:01.99,Default,,0000,0000,0000,,Được thôi.
Dialogue: 0,0:14:03.10,0:14:08.37,Default,,0000,0000,0000,,Lúc này thì mua một hộp có tặng kèm quà khuyến mãi đó.
Dialogue: 0,0:14:08.77,0:14:10.10,Default,,0000,0000,0000,,Cứ đưa cả cho tôi đi !
Dialogue: 0,0:14:12.27,0:14:13.54,Default,,0000,0000,0000,,Cám ơn.
Dialogue: 0,0:14:15.18,0:14:17.87,Default,,0000,0000,0000,,Một liều thuốc kích thích?
Dialogue: 0,0:14:19.18,0:14:21.38,Default,,0000,0000,0000,,Sao giống thuốc kích dục thế nhỉ ?
Dialogue: 0,0:14:22.15,0:14:25.14,Default,,0000,0000,0000,,Chắc là hàng nhái thôi.
Dialogue: 0,0:14:25.59,0:14:26.21,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:14:28.92,0:14:31.02,Default,,0000,0000,0000,,Không ai được phép lên tầng thượng hết!
Dialogue: 0,0:14:35.00,0:14:37.52,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:14:38.17,0:14:40.86,Default,,0000,0000,0000,,Cái gì thế này?
Dialogue: 0,0:14:41.50,0:14:44.67,Default,,0000,0000,0000,,Cơ thể tôi đang nóng ran lên.
Dialogue: 0,0:14:45.01,0:14:46.47,Default,,0000,0000,0000,,M-Mikoto-san?
Dialogue: 0,0:14:53.72,0:14:54.31,Default,,0000,0000,0000,,Tôi...
Dialogue: 0,0:14:54.92,0:14:56.35,Default,,0000,0000,0000,,M-Mikoto-san?
Dialogue: 0,0:14:58.35,0:15:02.26,Default,,0000,0000,0000,,Tsuzaki, trong người tôi khó chịu quá.
Dialogue: 0,0:15:02.66,0:15:03.68,Default,,0000,0000,0000,,Tôi không thể...
Dialogue: 0,0:15:04.43,0:15:05.62,Default,,0000,0000,0000,,Tôi không chịu được nữa rồi!
Dialogue: 0,0:15:14.30,0:15:17.70,Default,,0000,0000,0000,,Ồ, Tsuzaki, thằng nhỏ của anh bự lên rồi nè.
Dialogue: 0,0:15:30.22,0:15:32.91,Default,,0000,0000,0000,,Mikoto-san, chúng ta không nên làm vậy.
Dialogue: 0,0:15:33.35,0:15:34.51,Default,,0000,0000,0000,,Ai quan tâm chứ?
Dialogue: 0,0:15:35.06,0:15:37.32,Default,,0000,0000,0000,,Đâu có ai nhòm ngó chỗ này đâu.
Dialogue: 0,0:15:54.98,0:15:59.57,Default,,0000,0000,0000,,Đây này, tôi đang trông chừng các người đây...
Dialogue: 0,0:16:00.25,0:16:05.24,Default,,0000,0000,0000,,... quái vật độc ác ngoài vũ trụ, đang tiến tới Trái Đất.
Dialogue: 0,0:16:06.32,0:16:12.49,Default,,0000,0000,0000,,Nhưng tôi không thể nào ngừng nhìn vào những sinh vật hứng tình này!
Dialogue: 0,0:16:12.73,0:16:18.50,Default,,0000,0000,0000,,Phải thủ dâm thật nhanh, nếu không thì trái đất sẽ bị hủy diệt.
Dialogue: 0,0:16:38.25,0:16:40.72,Default,,0000,0000,0000,,Cuối cùng thì mối quan hệ của anh với Chiho-san đã tiến triển.
Dialogue: 0,0:16:41.56,0:16:45.01,Default,,0000,0000,0000,,Vậy thì hôm nay anh phải học cách làm tình thực sự đó.
Dialogue: 0,0:16:45.56,0:16:48.55,Default,,0000,0000,0000,,Làm tình thực sự?
Dialogue: 0,0:16:49.70,0:16:50.60,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:16:50.93,0:16:53.59,Default,,0000,0000,0000,,Vậy thì tôi có một yêu cầu?
Dialogue: 0,0:16:54.67,0:16:55.47,Default,,0000,0000,0000,,Chuyện gì?
Dialogue: 0,0:16:56.40,0:16:58.50,Default,,0000,0000,0000,,Cô có thể mặc bộ đồ này không?
Dialogue: 0,0:16:59.11,0:16:59.87,Default,,0000,0000,0000,,Oa.
Dialogue: 0,0:17:03.71,0:17:05.54,Default,,0000,0000,0000,,Làm sao anh có vậy?
Dialogue: 0,0:17:05.78,0:17:07.25,Default,,0000,0000,0000,,Đó là quà khuyến mãi số hai
Dialogue: 0,0:17:07.55,0:17:08.78,Default,,0000,0000,0000,,Quà khuyến mãi số hai?
Dialogue: 0,0:17:09.18,0:17:11.08,Default,,0000,0000,0000,,Đừng để ý.
Dialogue: 0,0:17:11.55,0:17:16.75,Default,,0000,0000,0000,,Ông chủ tặng tôi bộ đồ này nhưng tôi nghĩ nó hơi quá khổ so với Chiho-san, vậy nên...
Dialogue: 0,0:17:17.36,0:17:18.29,Default,,0000,0000,0000,,Này.
Dialogue: 0,0:17:18.93,0:17:21.19,Default,,0000,0000,0000,,Trong trường hợp này, anh phải nói là...
Dialogue: 0,0:17:21.30,0:17:24.42,Default,,0000,0000,0000,,..."Tôi thấy cô mặc bộ đồ này rất hợp đó"
Dialogue: 0,0:17:25.27,0:17:26.23,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:17:27.94,0:17:28.92,Default,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:17:29.10,0:17:31.20,Default,,0000,0000,0000,,Để tôi mặc bộ này cho anh xem.
Dialogue: 0,0:17:32.64,0:17:34.23,Default,,0000,0000,0000,,Cảm ơn nhiều.
Dialogue: 0,0:17:43.05,0:17:44.04,Default,,0000,0000,0000,,Trông tôi thế nào?
Dialogue: 0,0:17:44.69,0:17:47.68,Default,,0000,0000,0000,,Cô mặc nó nhìn đẹp lắm.
Dialogue: 0,0:17:50.09,0:17:53.46,Default,,0000,0000,0000,,Tôi không hề mặc quần áo lót đâu nhé.
Dialogue: 0,0:17:54.06,0:17:55.19,Default,,0000,0000,0000,,Oa.
Dialogue: 0,0:17:56.36,0:17:59.63,Default,,0000,0000,0000,,Hôm nay là ngày an toàn của tôi, vậy nên không cần tới thứ này, nhưng...
Dialogue: 0,0:18:00.57,0:18:02.63,Default,,0000,0000,0000,,Anh có muốn tập đeo bao cao su không?
Dialogue: 0,0:18:02.87,0:18:06.07,Default,,0000,0000,0000,,Không, tôi không muốn đeo bao cao su.
Dialogue: 0,0:18:09.88,0:18:11.28,Default,,0000,0000,0000,,Thành thật là tốt đó.
Dialogue: 0,0:18:17.65,0:18:18.88,Default,,0000,0000,0000,,Tới đây nào.
Dialogue: 0,0:18:19.35,0:18:20.55,Default,,0000,0000,0000,,Tôi vào đây...
Dialogue: 0,0:18:27.90,0:18:31.09,Default,,0000,0000,0000,,Âm hộ của cô vẫn khít như mọi khi.
Dialogue: 0,0:18:43.51,0:18:46.14,Default,,0000,0000,0000,,Thằng nhỏ của tôi có tuyệt không hả?
Dialogue: 0,0:18:49.52,0:18:50.81,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:18:53.49,0:18:59.09,Default,,0000,0000,0000,,Đừng làm mạnh như vậy, anh phải học các điều chỉnh nhịp độ.
Dialogue: 0,0:18:59.69,0:19:00.68,Default,,0000,0000,0000,,Giống vậy hả?
Dialogue: 0,0:19:03.77,0:19:04.75,Default,,0000,0000,0000,,Phải rồi...
Dialogue: 0,0:19:05.47,0:19:06.36,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:19:09.10,0:19:10.00,Default,,0000,0000,0000,,Kanae-san!
Dialogue: 0,0:19:23.48,0:19:24.18,Default,,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:19:25.29,0:19:27.65,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:19:28.39,0:19:29.12,Default,,0000,0000,0000,,Tôi cũng sắp ra rồi...
Dialogue: 0,0:19:29.59,0:19:31.75,Default,,0000,0000,0000,,Âm hộ của cô khít quá.
Dialogue: 0,0:19:37.83,0:19:38.82,Default,,0000,0000,0000,,Thằng nhỏ của anh đang đâm sâu vào bên trong tôi!
Dialogue: 0,0:19:41.44,0:19:42.84,Default,,0000,0000,0000,,Kanae-san, tôi không nhịn nổi nữa rồi!
Dialogue: 0,0:19:43.07,0:19:44.56,Default,,0000,0000,0000,,Chúng ta ra cùng nhau nhé.
Dialogue: 0,0:19:45.41,0:19:48.31,Default,,0000,0000,0000,,Tôi muốn ra cùng với anh.
Dialogue: 0,0:19:48.98,0:19:49.50,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:19:50.68,0:19:51.67,Default,,0000,0000,0000,,Ra rồi.
Dialogue: 0,0:19:52.35,0:19:53.18,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:19:54.25,0:19:55.91,Default,,0000,0000,0000,,Tôi không nhịn nổi rồi...
Dialogue: 0,0:19:57.08,0:19:57.98,Default,,0000,0000,0000,,Thêm chút nữa thôi!
Dialogue: 0,0:20:18.91,0:20:21.24,Default,,0000,0000,0000,,Anh có thể ra bên trong mà.
Dialogue: 0,0:20:22.11,0:20:25.04,Default,,0000,0000,0000,,Nhưng dù sao thì cũng vẫn còn xác xuất nhỏ khả năng cô bị dính bầu mà.
Dialogue: 0,0:20:26.18,0:20:30.71,Default,,0000,0000,0000,,Hơn nữa cô còn là một phụ nữ đã có chồng, tôi sẽ không gánh nổi trách nhiệm đâu.
Dialogue: 0,0:20:34.39,0:20:35.58,Default,,0000,0000,0000,,Cũng phải...
Dialogue: 0,0:20:54.08,0:20:57.98,Default,,0000,0000,0000,,Em không có giỏi chuyện chăn gối, nhưng em sẽ cố hết sức mình.
Dialogue: 0,0:20:58.48,0:20:59.64,Default,,0000,0000,0000,,Anh cũng vậy.
Dialogue: 0,0:21:00.25,0:21:01.81,Default,,0000,0000,0000,,Ối, xin lỗi.
Dialogue: 0,0:21:02.42,0:21:03.41,Default,,0000,0000,0000,,Đợi một lát...
Dialogue: 0,0:21:04.12,0:21:05.21,Default,,0000,0000,0000,,Tsuzaki-san?
Dialogue: 0,0:21:05.79,0:21:08.08,Default,,0000,0000,0000,,Cứ để nguyên nó trên ngón tay đi, Chiho-san.
Dialogue: 0,0:21:08.82,0:21:09.31,Default,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:21:10.19,0:21:12.52,Default,,0000,0000,0000,,Anh rất cảm kích sự quan tâm của em.
Dialogue: 0,0:21:13.03,0:21:16.59,Default,,0000,0000,0000,,Nhưng đừng ép buộc bản thân phải quên đi người chồng quá cố.
Dialogue: 0,0:21:17.70,0:21:22.47,Default,,0000,0000,0000,,Vì em không phải là mẫu người có thể quên người mình yêu một cách dễ dàng.
Dialogue: 0,0:21:23.17,0:21:24.40,Default,,0000,0000,0000,,Tsuzaki-san.
Dialogue: 0,0:21:25.71,0:21:27.37,Default,,0000,0000,0000,,Nhưng nếu em không phiền thì...
Dialogue: 0,0:21:27.91,0:21:31.47,Default,,0000,0000,0000,,Em có thể gọi anh bằng tên thân mật được không?
Dialogue: 0,0:21:35.18,0:21:36.81,Default,,0000,0000,0000,,Satoru-san.
Dialogue: 0,0:21:37.25,0:21:38.28,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:22:05.15,0:22:08.21,Default,,0000,0000,0000,,Anh thành thạo chuyện này quá...
Dialogue: 0,0:22:09.35,0:22:12.29,Default,,0000,0000,0000,,Chắc là anh phải qua lại với nhiều cô rồi ha.
Dialogue: 0,0:22:12.69,0:22:14.28,Default,,0000,0000,0000,,Không phải vậy.
Dialogue: 0,0:22:14.82,0:22:20.16,Default,,0000,0000,0000,,Anh chỉ làm hết mình để khiến em hạnh phúc thôi.
Dialogue: 0,0:22:21.63,0:22:22.76,Default,,0000,0000,0000,,Cảm ơn anh.
Dialogue: 0,0:22:33.71,0:22:35.61,Default,,0000,0000,0000,,Em ướt đẫm rồi nè.
Dialogue: 0,0:22:37.75,0:22:38.77,Default,,0000,0000,0000,,Xấu hổ quá.
Dialogue: 0,0:22:39.18,0:22:44.71,Default,,0000,0000,0000,,Vậy có nghĩa là em cảm thấy sung sướng, anh vui lắm.
Dialogue: 0,0:22:46.19,0:22:50.45,Default,,0000,0000,0000,,Anh làm chuyện này thật tuyệt.
Dialogue: 0,0:22:52.53,0:22:55.29,Default,,0000,0000,0000,,Satoru-san, thằng nhỏ của anh chào cờ rồi kìa.
Dialogue: 0,0:22:56.03,0:22:59.69,Default,,0000,0000,0000,,Chiho-san cũng có khác gì anh đâu.
Dialogue: 0,0:23:00.17,0:23:01.03,Default,,0000,0000,0000,,Ôi, không.
Dialogue: 0,0:23:01.70,0:23:02.67,Default,,0000,0000,0000,,Ừm thì...
Dialogue: 0,0:23:06.67,0:23:09.11,Default,,0000,0000,0000,,Cuối cùng chúng ta đã có thể hòa làm một.
Dialogue: 0,0:23:10.81,0:23:12.71,Default,,0000,0000,0000,,Anh yêu em, Chiho-san.
Dialogue: 0,0:23:13.55,0:23:17.01,Default,,0000,0000,0000,,Em cũng yêu anh. Satoru-san.
Dialogue: 0,0:23:18.19,0:23:20.28,Default,,0000,0000,0000,,Em xin lỗi, Ryuhei-san.
Dialogue: 0,0:23:20.89,0:23:23.72,Default,,0000,0000,0000,,Nhưng em chắc anh có thể hiểu và tha thứ cho em.
Dialogue: 0,0:23:27.09,0:23:28.15,Default,,0000,0000,0000,,Anh vào nhé?
Dialogue: 0,0:23:31.17,0:23:32.26,Default,,0000,0000,0000,,Em không sao chứ?
Dialogue: 0,0:23:33.43,0:23:34.37,Default,,0000,0000,0000,,Không sao đâu...
Dialogue: 0,0:23:34.74,0:23:37.50,Default,,0000,0000,0000,,Chắc lâu rồi nên nó mới khít lại như vậy...
Dialogue: 0,0:23:38.84,0:23:42.83,Default,,0000,0000,0000,,Cũng có thể là do thằng nhỏ của anh quá bự.
Dialogue: 0,0:23:43.58,0:23:44.64,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:23:56.72,0:23:57.52,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:24:12.17,0:24:12.97,Default,,0000,0000,0000,,Chiho-san!
Dialogue: 0,0:24:13.91,0:24:14.70,Default,,0000,0000,0000,,Chiho-san!
Dialogue: 0,0:24:15.81,0:24:16.97,Default,,0000,0000,0000,,Satoru-san!
Dialogue: 0,0:24:21.28,0:24:22.41,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:24:23.22,0:24:25.52,Default,,0000,0000,0000,,Đầu óc em hoàn toàn trống rỗng.
Dialogue: 0,0:24:27.86,0:24:30.42,Default,,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:24:31.96,0:24:34.89,Default,,0000,0000,0000,,Bên trong em thật khít và nóng bỏng.
Dialogue: 0,0:24:35.16,0:24:37.53,Default,,0000,0000,0000,,Anh sắp ra rồi.
Dialogue: 0,0:24:41.00,0:24:42.13,Default,,0000,0000,0000,,Em cũng vậy...
Dialogue: 0,0:24:43.20,0:24:46.54,Default,,0000,0000,0000,,Satoru-san, anh làm tuyệt quá đi mất.
Dialogue: 0,0:25:03.09,0:25:03.92,Default,,0000,0000,0000,,Chiho-san.
Dialogue: 0,0:25:05.76,0:25:06.78,Default,,0000,0000,0000,,Chúng ta ra cùng nhau nhé.
Dialogue: 0,0:25:09.13,0:25:11.60,Default,,0000,0000,0000,,Vâng, Satoru-san.
Dialogue: 0,0:26:12.23,0:26:13.99,Default,,0000,0000,0000,,Ai vậy ta, bậy giờ hãy còn sớm mà?
Dialogue: 0,0:26:15.30,0:26:17.20,Default,,0000,0000,0000,,Xin chào!
Dialogue: 0,0:26:18.00,0:26:20.16,Default,,0000,0000,0000,,Tôi lại nấu đồ ăn quá tay rồi...
Dialogue: 0,0:26:20.80,0:26:21.17,Default,,0000,0000,0000,,Ủa?
Dialogue: 0,0:26:21.80,0:26:22.93,Default,,0000,0000,0000,,Tsuzaki-kun?
Dialogue: 0,0:26:23.54,0:26:24.80,Default,,0000,0000,0000,,Kanae-san.
Dialogue: 0,0:26:26.01,0:26:26.87,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:26:27.71,0:26:28.83,Default,,0000,0000,0000,,Chúc mừng hai người!
Dialogue: 0,0:26:31.08,0:26:36.21,Default,,0000,0000,0000,,Nhưng bất cứ khi nào cậu cần "phụ đạo" thêm, tôi luôn sẵn lòng giúp đỡ.
Dialogue: 0,0:26:37.69,0:26:38.65,Default,,0000,0000,0000,,Satoru-san.
Dialogue: 0,0:26:40.82,0:26:43.05,Default,,0000,0000,0000,,Có chuyện gì vậy?
Dialogue: 0,0:26:43.79,0:26:44.69,Default,,0000,0000,0000,,À, chỉ là...
Dialogue: 0,0:26:44.79,0:26:45.72,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:26:49.70,0:26:52.89,Default,,0000,0000,0000,,Chỉ là quà mừng cho 2 người thôi.
Dialogue: 0,0:26:53.33,0:26:54.53,Default,,0000,0000,0000,,Kanae-san?
