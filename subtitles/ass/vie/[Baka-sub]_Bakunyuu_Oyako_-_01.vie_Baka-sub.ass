Dialogue: 0,0:00:00.04,0:00:06.02,Default,,0000,0000,0000,,{\fad(500,0)\pos(320,300)}CHIẾN TRANH GIỮA CÁC BỘ NGỰC
Dialogue: 0,0:00:00.04,0:00:06.02,Default,,0000,0000,0000,,{\fs20\fad(500,0)\pos(358,370)}Tập 1
Dialogue: 0,0:00:11.87,0:00:13.86,Default,,0000,0000,0000,,Không thể chấp nhận!
Dialogue: 0,0:00:14.81,0:00:15.97,Default,,0000,0000,0000,,Không công bằng!
Dialogue: 0,0:00:16.78,0:00:19.41,Default,,0000,0000,0000,,Này, hạ giọng xuống một chút đi.
Dialogue: 0,0:00:19.81,0:00:22.18,Default,,0000,0000,0000,,Sao lại là cậu, tớ không được à?
Dialogue: 0,0:00:23.02,0:00:27.35,Default,,0000,0000,0000,,Này, cậu là người gọi tớ ra đây, cái thái độ đó là sao chứ?
Dialogue: 0,0:00:28.69,0:00:30.16,Default,,0000,0000,0000,,Cậu biết tớ đang nói về cái gì mà.
Dialogue: 0,0:00:30.96,0:00:32.26,Default,,0000,0000,0000,,Làm sao tớ biết được?
Dialogue: 0,0:00:34.30,0:00:38.63,Default,,0000,0000,0000,,{\i1}Mình ở đây vì Yoshio gọi mình tới.{\i0}
Dialogue: 0,0:00:39.70,0:00:44.57,Default,,0000,0000,0000,,{\i1}Chả hiểu vì sao cậu ta lại hét vào mặt mình như thế, mà...{\i0}
Dialogue: 0,0:00:45.04,0:00:50.98,Default,,0000,0000,0000,,{\i1}...chắc là có chuyện gì đó liên quan đến cô gái có bộ ngực vĩ đại này.{\i0}
Dialogue: 0,0:00:52.11,0:00:55.41,Default,,0000,0000,0000,,{\i1}Phải nói là... Bộ ngực rất tuyệt...{\i0}
Dialogue: 0,0:00:56.18,0:00:59.42,Default,,0000,0000,0000,,{\i1}Mình có thể làm thế này... thế kia... với nó!{\i0}
Dialogue: 0,0:00:59.62,0:01:00.38,Default,,0000,0000,0000,,Ừm....
Dialogue: 0,0:01:01.42,0:01:05.76,Default,,0000,0000,0000,,Em... Em là Reina Toyohara.
Dialogue: 0,0:01:06.49,0:01:06.93,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:01:08.13,0:01:11.29,Default,,0000,0000,0000,,Em muốn nhờ anh một chuyện, Yusuke.
Dialogue: 0,0:01:12.03,0:01:15.56,Default,,0000,0000,0000,,Anh... Anh có thể làm gia sư riêng cho em được không?
Dialogue: 0,0:01:17.67,0:01:23.01,Default,,0000,0000,0000,,Làm ơn! Chỉ có anh mới giúp được em, làm ơn!
Dialogue: 0,0:01:25.71,0:01:30.01,Default,,0000,0000,0000,,Ê... Không, chắc phải là cỡ F...
Dialogue: 0,0:01:30.82,0:01:34.22,Default,,0000,0000,0000,,Anh không thể giúp sao?
Dialogue: 0,0:01:35.82,0:01:40.35,Default,,0000,0000,0000,,Reina, cô chắc chứ, lớp học của tôi khó lắm?
Dialogue: 0,0:01:40.60,0:01:42.12,Default,,0000,0000,0000,,Vâng! Em hiểu ạ!
Dialogue: 0,0:01:44.20,0:01:47.19,Default,,0000,0000,0000,,Dạy học riêng cho một cô gái có bộ ngực khổng lồ.
Dialogue: 0,0:01:49.14,0:01:49.63,Default,,0000,0000,0000,,Vậy được rồi!
Dialogue: 0,0:01:50.00,0:01:50.37,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:01:51.11,0:01:53.07,Default,,0000,0000,0000,,Thật không công bằng.
Dialogue: 0,0:01:58.71,0:02:00.20,Default,,0000,0000,0000,,Vâng? Ai đấy?
Dialogue: 0,0:02:01.72,0:02:02.08,Default,,0000,0000,0000,,Chào!
Dialogue: 0,0:02:03.12,0:02:05.71,Default,,0000,0000,0000,,Mừng quá! Vậy là anh đến thật!
Dialogue: 0,0:02:06.62,0:02:08.71,Default,,0000,0000,0000,,Mẹ ơi! Thầy của con đến rồi!
Dialogue: 0,0:02:09.19,0:02:10.02,Default,,0000,0000,0000,,Được!
Dialogue: 0,0:02:12.69,0:02:13.92,Default,,0000,0000,0000,,{\i1}Một bộ ngực khủng bố!{\i0}
Dialogue: 0,0:02:14.30,0:02:20.83,Default,,0000,0000,0000,,Xin chào. Tôi là mẹ của Reina, Mitsuki. Xin lỗi vì con gái tôi làm phiền cậu.
Dialogue: 0,0:02:21.17,0:02:25.97,Default,,0000,0000,0000,,Nó chả muốn ai ngoài cậu làm gia sư cả, vậy nên...
Dialogue: 0,0:02:26.21,0:02:28.87,Default,,0000,0000,0000,,Thôi nào, mẹ, đừng nói thế!
Dialogue: 0,0:02:29.18,0:02:30.91,Default,,0000,0000,0000,,{\i1}Cỡ H!{\i0}
Dialogue: 0,0:02:33.18,0:02:34.17,Default,,0000,0000,0000,,Ôi...
Dialogue: 0,0:02:36.45,0:02:39.85,Default,,0000,0000,0000,,Được rồi, Reina, em hãy làm các câu hỏi trong sách.
Dialogue: 0,0:02:40.19,0:02:40.85,Default,,0000,0000,0000,,Vâng, sensei.
Dialogue: 0,0:02:48.16,0:02:49.96,Default,,0000,0000,0000,,{\i1}Một bộ ngực không thể tin được.{\i0}
Dialogue: 0,0:02:50.43,0:02:54.66,Default,,0000,0000,0000,,{\i1}Sao mình có thể kìm nén khi nó ở ngay trước mặt?{\i0}
Dialogue: 0,0:02:59.31,0:03:00.43,Default,,0000,0000,0000,,{\i1}Không chịu nổi...{\i0}
Dialogue: 0,0:03:02.34,0:03:06.58,Default,,0000,0000,0000,,{\i1}Reina, tôi là gia sư, nhưng còn có một con người khác bên trong...{\i0}
Dialogue: 0,0:03:06.95,0:03:10.58,Default,,0000,0000,0000,,{\i1}Tôi thuộc chủng tộc người-yêu-ngực!{\i0}
Dialogue: 0,0:03:12.69,0:03:14.31,Default,,0000,0000,0000,,Bắt được em rồi, Reina!
Dialogue: 0,0:03:14.56,0:03:16.58,Default,,0000,0000,0000,,Anh làm gì thế, sensei?
Dialogue: 0,0:03:17.66,0:03:20.56,Default,,0000,0000,0000,,S... sensei... K... Không...
Dialogue: 0,0:03:20.73,0:03:24.29,Default,,0000,0000,0000,,Em tuyệt thật, Reina. Ngực em như cái gối ôm ấy.
Dialogue: 0,0:03:48.09,0:03:50.19,Default,,0000,0000,0000,,Sao đột nhiên... Anh không thể...
Dialogue: 0,0:03:50.19,0:03:52.66,Default,,0000,0000,0000,,Em mặc đồ mỏng để kích thích anh mà.
Dialogue: 0,0:03:53.06,0:03:56.43,Default,,0000,0000,0000,,Anh biết em đang hy vọng chuyện này sẽ xảy ra.
Dialogue: 0,0:03:56.90,0:03:57.89,Default,,0000,0000,0000,,Điều đó là...
Dialogue: 0,0:04:02.64,0:04:04.90,Default,,0000,0000,0000,,Ô! Nhìn nó này!
Dialogue: 0,0:04:12.55,0:04:13.45,Default,,0000,0000,0000,,Reina.
Dialogue: 0,0:04:18.99,0:04:20.01,Default,,0000,0000,0000,,Chúng thật tuyệt...
Dialogue: 0,0:04:31.60,0:04:32.76,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:04:50.55,0:04:54.28,Default,,0000,0000,0000,,Sao? Cảm thấy thế nào?
Dialogue: 0,0:05:09.30,0:05:10.33,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:05:11.44,0:05:12.17,Default,,0000,0000,0000,,Gì thế?
Dialogue: 0,0:05:12.77,0:05:15.47,Default,,0000,0000,0000,,Đầu vú... Làm ơn vuốt ve nó nữa.
Dialogue: 0,0:05:27.79,0:05:28.55,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:05:41.57,0:05:46.97,Default,,0000,0000,0000,,Ngay... Ngay đó... Đầu vú em rất nhạy cảm.
Dialogue: 0,0:05:47.31,0:05:50.00,Default,,0000,0000,0000,,Đầu vú nhạy cảm à.
Dialogue: 0,0:06:13.20,0:06:13.93,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:06:17.67,0:06:20.04,Default,,0000,0000,0000,,Đầu ngực em cứng rồi nè.
Dialogue: 0,0:06:20.34,0:06:23.00,Default,,0000,0000,0000,,Là bởi vì anh chú ý nhiều đến nó.
Dialogue: 0,0:06:24.68,0:06:28.27,Default,,0000,0000,0000,,Phải... Phải, sensei... Nữa đi... Bú nó mạnh nữa...
Dialogue: 0,0:06:31.29,0:06:32.34,Default,,0000,0000,0000,,Làm ơn đừng dừng lại.
Dialogue: 0,0:06:41.36,0:06:44.99,Default,,0000,0000,0000,,Phải rồi như thế... như thế thật là sướng!
Dialogue: 0,0:07:07.99,0:07:08.72,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:07:20.50,0:07:21.33,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:07:30.51,0:07:31.31,Default,,0000,0000,0000,,Sensei!
Dialogue: 0,0:07:36.18,0:07:40.02,Default,,0000,0000,0000,,Nào, dùng ngực em để làm anh sướng với.
Dialogue: 0,0:07:41.29,0:07:42.19,Default,,0000,0000,0000,,Nó bự quá.
Dialogue: 0,0:07:45.33,0:07:47.26,Default,,0000,0000,0000,,Sensei... Anh định làm gì thế?
Dialogue: 0,0:07:47.63,0:07:48.39,Default,,0000,0000,0000,,Làm tình với ngực em.
Dialogue: 0,0:07:49.13,0:07:50.56,Default,,0000,0000,0000,,Làm tình... với ngực?
Dialogue: 0,0:07:51.23,0:07:52.76,Default,,0000,0000,0000,,Thế này này.
Dialogue: 0,0:07:57.37,0:07:59.60,Default,,0000,0000,0000,,Cái của anh đang chen vào giữa ngực em.
Dialogue: 0,0:08:01.14,0:08:03.20,Default,,0000,0000,0000,,Sensei, như vậy có sướng không?
Dialogue: 0,0:08:05.98,0:08:09.61,Default,,0000,0000,0000,,Ngực của em tuyệt lắm. Nào, Reina, di chuyển nó đi.
Dialogue: 0,0:08:10.22,0:08:12.02,Default,,0000,0000,0000,,V... Vâng... Thế này ạ?
Dialogue: 0,0:08:13.96,0:08:16.79,Default,,0000,0000,0000,,Nó chặt lắm... Thật là tuyệt.
Dialogue: 0,0:08:34.01,0:08:37.84,Default,,0000,0000,0000,,Sensei, sự cọ xát... Thật là tuyệt...
Dialogue: 0,0:08:42.05,0:08:42.74,Default,,0000,0000,0000,,Reina...
Dialogue: 0,0:09:21.12,0:09:22.28,Default,,0000,0000,0000,,Reina.
Dialogue: 0,0:09:29.50,0:09:32.43,Default,,0000,0000,0000,,Reina, nếu em cứ làm thế... anh sẽ...
Dialogue: 0,0:09:35.74,0:09:39.10,Default,,0000,0000,0000,,Cái của anh bắn ra nhiều quá này...
Dialogue: 0,0:09:41.38,0:09:42.17,Default,,0000,0000,0000,,Ngon thật...
Dialogue: 0,0:09:43.18,0:09:45.17,Default,,0000,0000,0000,,Mẹ, sensei chuẩn bị về rồi!
Dialogue: 0,0:09:45.51,0:09:46.10,Default,,0000,0000,0000,,Được!
Dialogue: 0,0:09:47.21,0:09:48.44,Default,,0000,0000,0000,,Được rồi, tôi về nhé.
Dialogue: 0,0:09:49.18,0:09:49.74,Default,,0000,0000,0000,,Yusuke?
Dialogue: 0,0:09:50.62,0:09:50.91,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:09:51.62,0:09:55.08,Default,,0000,0000,0000,,Yusuke, nhờ cậu để ý đến Reina.
Dialogue: 0,0:10:00.23,0:10:03.32,Default,,0000,0000,0000,,Vâng. Tôi sẽ cố hết sức.
Dialogue: 0,0:10:22.45,0:10:25.42,Default,,0000,0000,0000,,Ngực Reina thật mềm mại... quá tuyệt vời.
Dialogue: 0,0:10:25.89,0:10:27.29,Default,,0000,0000,0000,,{\i1}Bắt được em rồi, Reina!{\i0}
Dialogue: 0,0:10:27.42,0:10:29.62,Default,,0000,0000,0000,,{\i1}Anh đang làm gì thế sensei?{\i0}
Dialogue: 0,0:10:31.06,0:10:31.85,Default,,0000,0000,0000,,{\i1}S... sensei...{\i0}
Dialogue: 0,0:10:32.26,0:10:33.45,Default,,0000,0000,0000,,Mà hơn nữa...
Dialogue: 0,0:10:34.33,0:10:37.99,Default,,0000,0000,0000,,{\i1}Yusuke, nhờ cậu để ý đến Reina.{\i0}
Dialogue: 0,0:10:41.34,0:10:46.00,Default,,0000,0000,0000,,Mình cá là ngực Mitsuki cũng\N mềm mại và lộng lẫy không kém...
Dialogue: 0,0:10:47.38,0:10:49.78,Default,,0000,0000,0000,,Đời mình bao quanh bởi những bộ ngực bự! Hoan hô!
Dialogue: 0,0:11:01.39,0:11:01.88,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:11:03.49,0:11:05.05,Default,,0000,0000,0000,,Ồ, xin chào.
Dialogue: 0,0:11:06.29,0:11:07.56,Default,,0000,0000,0000,,Chào buổi tối.
Dialogue: 0,0:11:09.13,0:11:10.06,Default,,0000,0000,0000,,Reina đâu ạ?
Dialogue: 0,0:11:10.40,0:11:13.86,Default,,0000,0000,0000,,Xin lỗi. Reina vừa mới đi tắm.
Dialogue: 0,0:11:14.40,0:11:15.20,Default,,0000,0000,0000,,Vậy à?!
Dialogue: 0,0:11:15.97,0:11:18.34,Default,,0000,0000,0000,,Chắc là tại tôi đến hơi sớm.
Dialogue: 0,0:11:19.37,0:11:21.81,Default,,0000,0000,0000,,Vậy tôi sẽ chờ cô ấy ở trong phòng.
Dialogue: 0,0:11:22.48,0:11:23.17,Default,,0000,0000,0000,,À, phải rồi!
Dialogue: 0,0:11:24.25,0:11:29.55,Default,,0000,0000,0000,,Bà hàng xóm vừa cho một cái bánh,\N cậu vào phòng khách ăn một chút nhé.
Dialogue: 0,0:11:30.58,0:11:31.07,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:11:32.05,0:11:32.99,Default,,0000,0000,0000,,Đi mà, làm ơn?
Dialogue: 0,0:11:34.36,0:11:35.69,Default,,0000,0000,0000,,Cậu không được từ chối đâu.
Dialogue: 0,0:11:36.42,0:11:37.52,Default,,0000,0000,0000,,Đợi đã, tôi...
Dialogue: 0,0:11:45.17,0:11:45.96,Default,,0000,0000,0000,,Đây rồi.
Dialogue: 0,0:11:46.60,0:11:49.36,Default,,0000,0000,0000,,Con bé tắm lâu lắm, cậu cứ thoải mái nhé.
Dialogue: 0,0:11:49.84,0:11:50.74,Default,,0000,0000,0000,,Vâng, cảm ơn...
Dialogue: 0,0:11:56.54,0:11:57.74,Default,,0000,0000,0000,,Chúng thật tuyệt!
Dialogue: 0,0:12:01.72,0:12:02.81,Default,,0000,0000,0000,,Yusuke.
Dialogue: 0,0:12:03.15,0:12:03.91,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:12:04.42,0:12:05.98,Default,,0000,0000,0000,,Cậu làm tình với Reina hôm qua phải không?
Dialogue: 0,0:12:07.29,0:12:09.52,Default,,0000,0000,0000,,Mitsuki, cô nói về việc gì thế ạ?
Dialogue: 0,0:12:10.79,0:12:12.22,Default,,0000,0000,0000,,Tôi biết hết mọi thứ.
Dialogue: 0,0:12:12.66,0:12:17.26,Default,,0000,0000,0000,,Con bé yêu cậu từ lúc nó gặp cậu trong ngày hội trường.
Dialogue: 0,0:12:18.47,0:12:21.09,Default,,0000,0000,0000,,Vì thế mà nó nhờ Yoshio giới thiệu với cậu...
Dialogue: 0,0:12:21.30,0:12:24.60,Default,,0000,0000,0000,,...và nhờ cậu làm gia sư riêng cho nó.
Dialogue: 0,0:12:25.84,0:12:27.57,Default,,0000,0000,0000,,Cậu lấy mất sự trinh trắng của Reina?
Dialogue: 0,0:12:28.28,0:12:29.61,Default,,0000,0000,0000,,Không, tôi vẫn chưa làm thế.
Dialogue: 0,0:12:30.01,0:12:32.91,Default,,0000,0000,0000,,Lạ nhỉ. Sao cậu lại tỏ ra rụt rè thế?
Dialogue: 0,0:12:36.12,0:12:37.14,Default,,0000,0000,0000,,Mitsuki!
Dialogue: 0,0:12:37.38,0:12:44.35,Default,,0000,0000,0000,,Cậu thấy thế nào. Của tôi tốt hơn nhiều của Reina chứ...
Dialogue: 0,0:12:54.97,0:12:57.70,Default,,0000,0000,0000,,Kinh thật... Vòng ngực cỡ H!
Dialogue: 0,0:12:59.04,0:13:01.27,Default,,0000,0000,0000,,Một kích thước không thể tả được.
Dialogue: 0,0:13:01.91,0:13:03.47,Default,,0000,0000,0000,,Ngực của Reina cũng không bằng.
Dialogue: 0,0:13:05.28,0:13:07.94,Default,,0000,0000,0000,,Phải gọi là một bộ ngực khủng bố!
Dialogue: 0,0:13:18.43,0:13:21.33,Default,,0000,0000,0000,,Phải... Cậu thấy ngực tôi thế nào?
Dialogue: 0,0:13:22.16,0:13:24.97,Default,,0000,0000,0000,,Ngực của cô Mitsuki... thật là bự...
Dialogue: 0,0:13:24.67,0:13:25.59,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:13:26.37,0:13:28.73,Default,,0000,0000,0000,,Đừng gọi tôi là cô Mitsuki. Mitsuki được rồi.
Dialogue: 0,0:13:28.97,0:13:30.70,Default,,0000,0000,0000,,Vâng Mitsuki!
Dialogue: 0,0:14:00.60,0:14:01.23,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:14:06.87,0:14:08.03,Default,,0000,0000,0000,,Sướng lắm...
Dialogue: 0,0:14:25.53,0:14:27.05,Default,,0000,0000,0000,,Giờ đến lượt tôi.
Dialogue: 0,0:14:54.26,0:14:59.28,Default,,0000,0000,0000,,Ôi, của cậu bự thật, tôi sẽ chơi với nó sau.
Dialogue: 0,0:15:02.53,0:15:03.43,Default,,0000,0000,0000,,Mitsuki...
Dialogue: 0,0:15:04.43,0:15:08.30,Default,,0000,0000,0000,,Này, Yusuke? Của tôi có sướng hơn của Reina không?
Dialogue: 0,0:15:09.20,0:15:11.40,Default,,0000,0000,0000,,Nó rất tuyệt...
Dialogue: 0,0:16:17.81,0:16:20.37,Default,,0000,0000,0000,,Ôi không... Mitsuki, tôi không giữ được...
Dialogue: 0,0:16:20.98,0:16:23.41,Default,,0000,0000,0000,,Không, nếu muốn ra thì hãy ra bên trong tôi.
Dialogue: 0,0:17:04.52,0:17:05.64,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:17:20.73,0:17:22.20,Default,,0000,0000,0000,,Yusuke...
Dialogue: 0,0:17:26.54,0:17:29.24,Default,,0000,0000,0000,,Tuyệt lắm... Ngực cô có vị thật tuyệt...
Dialogue: 0,0:17:31.95,0:17:34.94,Default,,0000,0000,0000,,Cả âm hộ và bầu ngực tôi đều cảm thấy nó...
Dialogue: 0,0:17:45.49,0:17:46.55,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:18:09.48,0:18:10.25,Default,,0000,0000,0000,,Mitsuki.
Dialogue: 0,0:18:16.02,0:18:16.82,Default,,0000,0000,0000,,Yusuke...
Dialogue: 0,0:18:20.76,0:18:22.02,Default,,0000,0000,0000,,Tôi làm cô sướng chứ?
Dialogue: 0,0:18:23.73,0:18:24.70,Default,,0000,0000,0000,,Sướng lắm...
Dialogue: 0,0:18:36.44,0:18:38.71,Default,,0000,0000,0000,,Yusuke, làm mạnh lên nữa đi!
Dialogue: 0,0:18:50.36,0:18:51.09,Default,,0000,0000,0000,,Mitsuki.
Dialogue: 0,0:18:53.36,0:18:54.12,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:18:56.23,0:18:58.49,Default,,0000,0000,0000,,Yusuke, cậu tuyệt lắm!
Dialogue: 0,0:19:11.85,0:19:13.97,Default,,0000,0000,0000,,Nó vào cả bên trong!
Dialogue: 0,0:19:15.08,0:19:17.81,Default,,0000,0000,0000,,Cô muốn nó phải không, Mitsuki?
Dialogue: 0,0:19:27.73,0:19:29.63,Default,,0000,0000,0000,,Phải... Phải...
Dialogue: 0,0:19:36.94,0:19:37.80,Default,,0000,0000,0000,,Mitsuki...
Dialogue: 0,0:19:38.47,0:19:39.13,Default,,0000,0000,0000,,Thật là tuyệt.
Dialogue: 0,0:20:03.16,0:20:04.13,Default,,0000,0000,0000,,Yusuke.
Dialogue: 0,0:20:04.57,0:20:05.19,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:20:05.50,0:20:08.09,Default,,0000,0000,0000,,Cậu chỉ làm thế với tôi thôi, được không?
Dialogue: 0,0:20:08.20,0:20:10.40,Default,,0000,0000,0000,,Nếu cậu làm thế với Reina, tôi sẽ không tha đâu.
Dialogue: 0,0:20:11.14,0:20:13.33,Default,,0000,0000,0000,,Cuộc đời với những bộ ngực bự! Hoan hô!
Dialogue: 0,0:20:15.28,0:20:20.94,Default,,0000,0000,0000,,Nóng thật! Em vừa tắm xong mà đã đổ mồ hôi lại rồi.
Dialogue: 0,0:20:21.32,0:20:24.51,Default,,0000,0000,0000,,{\i1}Của Mitsuki rất ngon, nhưng của Reina cũng đâu kém...{\i0}
Dialogue: 0,0:20:25.09,0:20:26.14,Default,,0000,0000,0000,,Này, sensei...
Dialogue: 0,0:20:27.29,0:20:29.95,Default,,0000,0000,0000,,Hôm nay anh định dạy em gì thế?
Dialogue: 0,0:20:30.29,0:20:32.42,Default,,0000,0000,0000,,Hôm này chúng ta sẽ học toán...
Dialogue: 0,0:20:32.99,0:20:35.36,Default,,0000,0000,0000,,Hôm nay chỉ thế thôi ạ?
Dialogue: 0,0:20:38.03,0:20:41.43,Default,,0000,0000,0000,,Tất nhiên là không!
Dialogue: 0,0:20:44.40,0:20:46.57,Default,,0000,0000,0000,,Anh thích mơn trớn ngực em thế...
Dialogue: 0,0:20:47.24,0:20:51.18,Default,,0000,0000,0000,,Bởi vì anh là người-yêu-ngực!
Dialogue: 0,0:21:07.66,0:21:08.92,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:21:21.24,0:21:24.11,Default,,0000,0000,0000,,Sensei... Không biết em bị làm sao...
Dialogue: 0,0:21:24.28,0:21:27.68,Default,,0000,0000,0000,,...mà trong quần lót em bắt đầu ươn ướt.
Dialogue: 0,0:21:27.85,0:21:30.21,Default,,0000,0000,0000,,Dưới này à? Đây phải không?
Dialogue: 0,0:21:57.71,0:22:00.54,Default,,0000,0000,0000,,Dưới này của em cũng nhạy cảm như bên trên vậy.
Dialogue: 0,0:22:05.95,0:22:09.05,Default,,0000,0000,0000,,Sensei, như thế... thật sướng.
Dialogue: 0,0:22:23.87,0:22:24.70,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:22:27.67,0:22:30.61,Default,,0000,0000,0000,,Sensei, em sắp ra rồi...
Dialogue: 0,0:22:32.88,0:22:34.14,Default,,0000,0000,0000,,Em ra rồi!
Dialogue: 0,0:22:42.22,0:22:43.12,Default,,0000,0000,0000,,Reina.
Dialogue: 0,0:22:43.56,0:22:44.42,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:22:48.96,0:22:51.56,Default,,0000,0000,0000,,Của anh thật là bự... và ngon nữa.
Dialogue: 0,0:23:08.65,0:23:10.84,Default,,0000,0000,0000,,Tuyệt lắm, Reina.
Dialogue: 0,0:23:22.10,0:23:23.86,Default,,0000,0000,0000,,Hãy để anh làm tình em.
Dialogue: 0,0:23:29.40,0:23:30.46,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:23:31.74,0:23:32.80,Default,,0000,0000,0000,,Bắt đầu nhé.
Dialogue: 0,0:23:36.61,0:23:40.41,Default,,0000,0000,0000,,Em kết nối với sensei. Chúng ta giờ là một!
Dialogue: 0,0:24:08.94,0:24:09.77,Default,,0000,0000,0000,,Reina.
Dialogue: 0,0:24:09.88,0:24:12.00,Default,,0000,0000,0000,,Anh quên mất là ngực em nhạy cảm.
Dialogue: 0,0:24:12.58,0:24:20.18,Default,,0000,0000,0000,,Anh thật ích kỉ, sensei. Anh cứ chạm vào những nơi nhạy cảm của em.
Dialogue: 0,0:25:16.44,0:25:17.24,Default,,0000,0000,0000,,S... sensei...
Dialogue: 0,0:25:43.94,0:25:45.87,Default,,0000,0000,0000,,S... sensei...
Dialogue: 0,0:25:49.31,0:25:51.24,Default,,0000,0000,0000,,Reina, anh sắp ra đây.
Dialogue: 0,0:25:52.68,0:25:56.58,Default,,0000,0000,0000,,Vâng... Em cũng sắp ra!
Dialogue: 0,0:26:05.93,0:26:08.26,Default,,0000,0000,0000,,Reina, mẹ đem trà lên đây.
Dialogue: 0,0:26:11.87,0:26:13.06,Default,,0000,0000,0000,,Hai người...
Dialogue: 0,0:26:13.83,0:26:15.16,Default,,0000,0000,0000,,Mitsuki...
Dialogue: 0,0:26:15.57,0:26:17.97,Default,,0000,0000,0000,,{\i1}Cậu chỉ làm thế với tôi thôi nhé?{\i0}
Dialogue: 0,0:26:18.24,0:26:20.60,Default,,0000,0000,0000,,{\i1}Nếu cậu làm thế với Reina, tôi sẽ không tha đâu.{\i0}
Dialogue: 0,0:26:21.68,0:26:24.11,Default,,0000,0000,0000,,{\i1}Ôi, không!{\i0}
Dialogue: 0,0:26:52.77,0:26:56.54,Default,,0000,0000,0000,,Không hay rồi... nếu thế này,\N cuộc sống với những bộ ngực bự sẽ...
Dialogue: 0,0:26:56.54,0:27:01.54,Default,,0000,0000,0000,,
