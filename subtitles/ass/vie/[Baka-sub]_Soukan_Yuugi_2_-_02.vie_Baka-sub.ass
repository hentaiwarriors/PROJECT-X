Dialogue: 0,0:04:42.63,0:04:50.60,game 1,,0000,0000,0000,,{\pos(152,75)}Makoto
Dialogue: 0,0:04:42.63,0:04:50.60,game 1,,0000,0000,0000,,{\pos(149,128)}Yuna
Dialogue: 0,0:04:42.63,0:04:50.60,game 1,,0000,0000,0000,,{\pos(150,188)}Misaki{TS}
Dialogue: 0,0:04:48.01,0:04:48.14,Card,,0000,0000,0000,,{\frz30.002\frx346\fry70\move(417,289,417,280,0,111)}Tình dục đồng giới nữ
Dialogue: 0,0:04:48.14,0:04:50.60,Card,,0000,0000,0000,,{\pos(414,292)}Tình dục đồng giới nữ
Dialogue: 0,0:04:51.46,0:04:53.15,Default,,0000,0000,0000,,Hoan hô!
Dialogue: 0,0:04:53.58,0:04:56.18,Default,,0000,0000,0000,,Đúng là có Thần phù hộ!
Dialogue: 0,0:04:56.19,0:04:56.77,Default,,0000,0000,0000,,Không thể nào.
Dialogue: 0,0:04:57.85,0:05:03.58,Default,,0000,0000,0000,,Cái này là do trò chơi điều khiển và sắp đặt, chị không có tội tình gì đâu nhé.
Dialogue: 0,0:05:09.75,0:05:10.54,Default,,0000,0000,0000,,Hina?
Dialogue: 0,0:05:11.19,0:05:12.86,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:05:13.41,0:05:14.98,Default,,0000,0000,0000,,Là Hina-san của cậu đây.
Dialogue: 0,0:05:15.72,0:05:17.34,Default,,0000,0000,0000,,Nee-chan, chạy mau!
Dialogue: 0,0:05:23.59,0:05:24.20,Default,,0000,0000,0000,,Yuna?
Dialogue: 0,0:05:25.09,0:05:28.39,Default,,0000,0000,0000,,Em không thích anh cư xử như vậy đâu, Onii-chan.
Dialogue: 0,0:05:29.91,0:05:33.25,Default,,0000,0000,0000,,Misa, tụi mình cùng nhau vui vẻ nào.
Dialogue: 0,0:05:35.81,0:05:36.49,Default,,0000,0000,0000,,Hừ.
Dialogue: 0,0:05:40.12,0:05:42.42,Default,,0000,0000,0000,,Kasukabe-san!
Dialogue: 0,0:05:43.44,0:05:48.30,Default,,0000,0000,0000,,Tại sao cô cứ ba lần bồn lượt tìm cách trốn kiểm tra y tế vậy hả?
Dialogue: 0,0:05:49.92,0:05:54.09,Default,,0000,0000,0000,,Naruse-san... Chuyện này có nguyên nhân sâu sa cả đó.
Dialogue: 0,0:05:54.47,0:05:59.21,Default,,0000,0000,0000,,Nguyên nhân sâu sa của cô có đủ để bù cho cơn giận của tôi không hả?
Dialogue: 0,0:06:01.58,0:06:05.87,Default,,0000,0000,0000,,Em giai bé bỏng, cứu chị với!
Dialogue: 0,0:06:06.45,0:06:07.46,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:06:13.70,0:06:16.54,Default,,0000,0000,0000,,Anh mà đóng vào thì chúng ta ra ngoài làm sao được!
Dialogue: 0,0:06:17.98,0:06:18.86,Default,,0000,0000,0000,,Bỏ mẹ.
Dialogue: 0,0:06:19.48,0:06:25.00,Default,,0000,0000,0000,,Makoto... Cả em nữa, Yuna-chan...\NHai đứa định bắt chị chịu đựng chuyện này đến bao giờ?
Dialogue: 0,0:06:25.49,0:06:27.22,Default,,0000,0000,0000,,Bình tĩnh nào, Nee-chan...
Dialogue: 0,0:06:27.55,0:06:31.55,Default,,0000,0000,0000,,Em muốn làm chuyện tồi tệ đó với chị, cũng như với Natsuki-san, phải không?
Dialogue: 0,0:06:31.86,0:06:33.30,Default,,0000,0000,0000,,Nee-chan...
Dialogue: 0,0:06:33.66,0:06:38.79,Default,,0000,0000,0000,,Dù đã nhiều lần chứng kiến thói vô tâm của em, nhưng chị vẫn chưa bao giờ nghĩ xấu về em.
Dialogue: 0,0:06:41.81,0:06:44.18,Default,,0000,0000,0000,,Tại sao mọi chuyện lại trở nên thế này?
Dialogue: 0,0:06:47.23,0:06:48.46,Default,,0000,0000,0000,,Thôi bỏ đi, Onii-chan.
Dialogue: 0,0:06:49.34,0:06:53.63,Default,,0000,0000,0000,,Chị ấy không chịu hợp tác thì thôi, chúng ta tìm cách hoàn thành trò chơi trước đã.
Dialogue: 0,0:06:54.31,0:06:55.16,Default,,0000,0000,0000,,Yuna?
Dialogue: 0,0:06:55.56,0:06:58.89,game 1,,0000,0000,0000,,{\pos(154,23)}Natsuki
Dialogue: 0,0:06:55.56,0:06:58.89,game 1,,0000,0000,0000,,{\pos(152,75)}Makoto
Dialogue: 0,0:06:55.56,0:06:58.89,game 1,,0000,0000,0000,,{\pos(149,128)}Yuna
Dialogue: 0,0:06:55.56,0:06:58.89,game 1,,0000,0000,0000,,{\pos(150,188)}Misaki{TS}
Dialogue: 0,0:06:55.56,0:06:58.89,Card,,0000,0000,0000,,{\pos(414,292)\t(1000,3317,\c&HFFFFFF&)}{\t(1000,3317,\3c&HFFFFFF&)}Tình dục đồng giới nữ
Dialogue: 0,0:07:11.09,0:07:13.71,Default,,0000,0000,0000,,Chị không muốn làm điều đó.
Dialogue: 0,0:07:17.13,0:07:20.73,Default,,0000,0000,0000,,Onee-chan, ngừng phàn nàn và ngoan ngoãn hợp tác với em đi nào.
Dialogue: 0,0:07:21.63,0:07:24.00,Default,,0000,0000,0000,,Âm hộ của chị ướt đẫm rồi nè.
Dialogue: 0,0:07:24.54,0:07:28.20,Default,,0000,0000,0000,,Chắc là vì trông thấy em chơi đùa với thằng nhỏ của Onii-chan phải không?
Dialogue: 0,0:07:28.92,0:07:29.98,Default,,0000,0000,0000,,Không phải...
Dialogue: 0,0:07:32.10,0:07:32.79,Default,,0000,0000,0000,,... như vậy.
Dialogue: 0,0:07:35.38,0:07:36.35,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:07:37.01,0:07:39.75,Default,,0000,0000,0000,,Yuna-chan, dừng lại đi!
Dialogue: 0,0:07:59.20,0:08:00.97,Default,,0000,0000,0000,,Sao hả? Sướng không nào?
Dialogue: 0,0:08:01.24,0:08:06.05,Default,,0000,0000,0000,,Vì làm chuyện này mỗi ngày nên em biết nó sung sướng tới mức nào.
Dialogue: 0,0:08:12.84,0:08:13.89,Default,,0000,0000,0000,,Dừng lại đi mà.
Dialogue: 0,0:08:15.82,0:08:19.01,Default,,0000,0000,0000,,Makoto... Cả em nữa, Yuna-chan.
Dialogue: 0,0:08:22.25,0:08:25.58,Default,,0000,0000,0000,,Onee-chan, bộ ngực của chị bự thiệt đó. Khiến em phát ghen luôn.
Dialogue: 0,0:08:26.70,0:08:31.95,Default,,0000,0000,0000,,Chắc chắn Onii-chan rất thích những cặp vú bự như thế này.
Dialogue: 0,0:08:33.68,0:08:34.68,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:08:35.24,0:08:37.70,Default,,0000,0000,0000,,Yuna-chan, đau chị.
Dialogue: 0,0:08:39.21,0:08:43.67,Default,,0000,0000,0000,,Nhưng mẹ em ngực cũng bự, chắc sau này nó sẽ lớn dần lên.
Dialogue: 0,0:08:44.49,0:08:46.79,Default,,0000,0000,0000,,Rồi em sẽ bự không thua gì chị đâu.
Dialogue: 0,0:08:47.92,0:08:49.97,Default,,0000,0000,0000,,Đừng mà, hai đầu ti chị.
Dialogue: 0,0:08:51.48,0:08:54.13,Default,,0000,0000,0000,,Yuna-chan, dừng lại đi mà...
Dialogue: 0,0:08:56.12,0:08:58.48,Default,,0000,0000,0000,,Makoto, cả em nữa... Chuyện này là sai trái.
Dialogue: 0,0:08:59.16,0:09:00.54,Default,,0000,0000,0000,,Cực kì sai trái...
Dialogue: 0,0:09:12.19,0:09:16.28,Default,,0000,0000,0000,,Tuy miệng nói vậy nhưng chị vẫn phấn khích khi được Onii-chan ngắm nhìn.
Dialogue: 0,0:09:16.81,0:09:18.30,Default,,0000,0000,0000,,Chị đang rất thỏa mãn!
Dialogue: 0,0:09:18.36,0:09:20.71,Default,,0000,0000,0000,,Không phải như vậy... Không phải mà...
Dialogue: 0,0:09:25.00,0:09:28.53,Default,,0000,0000,0000,,Hai đầu ti chị cứng lên rồi nè!
Dialogue: 0,0:09:28.56,0:09:30.99,Default,,0000,0000,0000,,Đừng mà! Đừng mà! Đừng mà!
Dialogue: 0,0:09:35.19,0:09:38.34,Default,,0000,0000,0000,,Chị thèm muốn thằng nhỏ của anh ấy.
Dialogue: 0,0:09:48.82,0:09:50.00,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:09:54.61,0:09:55.48,Default,,0000,0000,0000,,Makoto...
Dialogue: 0,0:09:56.39,0:09:58.41,Default,,0000,0000,0000,,Không, chị sắp ra rồi!
Dialogue: 0,0:10:03.51,0:10:04.43,Default,,0000,0000,0000,,Chị ra rồi!
Dialogue: 0,0:10:04.95,0:10:09.32,Default,,0000,0000,0000,,Chị ra rồi! Chị ra rồi!
Dialogue: 0,0:10:27.52,0:10:28.15,Default,,0000,0000,0000,,Yuna?
Dialogue: 0,0:10:28.98,0:10:31.66,Default,,0000,0000,0000,,Yuna, quần áo em đâu! Sao lại khỏa thân như thế!
Dialogue: 0,0:10:32.11,0:10:34.51,Default,,0000,0000,0000,,Thằng nhỏ của anh cũng chui ra kia kìa.
Dialogue: 0,0:10:34.96,0:10:36.22,Default,,0000,0000,0000,,Đúng vậy, nhưng mà...
Dialogue: 0,0:11:19.08,0:11:22.21,Default,,0000,0000,0000,,Em yêu anh, Onii-chan.
Dialogue: 0,0:11:24.00,0:11:25.96,Default,,0000,0000,0000,,Chắc hẳn anh cũng chẳng để ý đúng không?
Dialogue: 0,0:11:26.84,0:11:30.64,Default,,0000,0000,0000,,À thì, tất nhiên là em rất đáng yêu...
Dialogue: 0,0:11:31.09,0:11:34.12,Default,,0000,0000,0000,,Nhiều lúc anh cũng nghĩ về em mỗi khi thủ dâm...
Dialogue: 0,0:11:36.35,0:11:37.26,Default,,0000,0000,0000,,Nhưng mà em là...
Dialogue: 0,0:11:37.46,0:11:38.19,Default,,0000,0000,0000,,Là em gái của anh chứ gì?
Dialogue: 0,0:11:39.28,0:11:42.52,Default,,0000,0000,0000,,Thế còn Misaki Onee-chan thì khác gì đâu.
Dialogue: 0,0:12:00.49,0:12:02.94,Default,,0000,0000,0000,,Yuna, em không sao chứ?
Dialogue: 0,0:12:03.89,0:12:06.32,Default,,0000,0000,0000,,À, em không sao...
Dialogue: 0,0:12:07.99,0:12:09.81,Default,,0000,0000,0000,,... ráng đợi thêm chút nữa nghe anh?
Dialogue: 0,0:12:14.90,0:12:16.53,Default,,0000,0000,0000,,Nhưng còn Natsuki-san...
Dialogue: 0,0:12:16.73,0:12:19.67,Default,,0000,0000,0000,,Cứ để mẹ muốn làm gì ba thì làm.
Dialogue: 0,0:12:21.43,0:12:25.61,Default,,0000,0000,0000,,Nhưng em không muốn nghe người khác dạy bảo làm gì hay không làm gì.
Dialogue: 0,0:12:26.31,0:12:28.52,Default,,0000,0000,0000,,Ờ, em nói phải.
Dialogue: 0,0:12:28.90,0:12:32.71,Default,,0000,0000,0000,,Vậy nên em muốn thành thật với cảm xúc của chính mình.
Dialogue: 0,0:12:34.20,0:12:36.29,Default,,0000,0000,0000,,Yuna, em không hối hận khi chọn anh chứ?
Dialogue: 0,0:12:37.28,0:12:42.66,Default,,0000,0000,0000,,Có lẽ Natsuki-san nói đúng, anh không phải một chàng trai tốt.
Dialogue: 0,0:12:45.42,0:12:47.01,Default,,0000,0000,0000,,Không đến mức như vậy đâu.
Dialogue: 0,0:12:47.89,0:12:49.16,Default,,0000,0000,0000,,Sau tất cả, anh vẫn là...
Dialogue: 0,0:12:50.40,0:12:53.13,Default,,0000,0000,0000,,Anh vẫn là anh trai yêu dấu của em.
Dialogue: 0,0:12:59.05,0:12:59.87,Default,,0000,0000,0000,,Cám ơn em.
Dialogue: 0,0:13:29.49,0:13:32.05,Default,,0000,0000,0000,,Yuna, em không sao chứ?
Dialogue: 0,0:13:33.59,0:13:36.53,Default,,0000,0000,0000,,Vâng, em không sao.
Dialogue: 0,0:13:42.88,0:13:48.00,Default,,0000,0000,0000,,Nếu không có thứ này, e rằng em sẽ không thể thành thật với chính bản thân mình được.
Dialogue: 0,0:13:48.99,0:13:54.16,Default,,0000,0000,0000,,Vậy nên em cho rằng trò chơi này không xấu xa như mẹ nói.
Dialogue: 0,0:13:54.50,0:13:55.47,Default,,0000,0000,0000,,Có lẽ vậy.
Dialogue: 0,0:13:55.97,0:13:59.60,Default,,0000,0000,0000,,Được tặng trò chơi này quả là một niềm hạnh phúc.
Dialogue: 0,0:14:00.46,0:14:01.36,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:14:02.78,0:14:04.59,Default,,0000,0000,0000,,Nói đi, Onii-chan...
Dialogue: 0,0:14:04.91,0:14:05.94,Default,,0000,0000,0000,,Hở?
Dialogue: 0,0:14:06.42,0:14:10.58,Default,,0000,0000,0000,,Dù không phải người phụ nữ đầu tiên của anh...
Dialogue: 0,0:14:11.27,0:14:12.98,Default,,0000,0000,0000,,... nhưng em chẳng hề quan tâm tới chuyện đó.
Dialogue: 0,0:14:34.52,0:14:38.43,Default,,0000,0000,0000,,Ừm, vậy nên em...
Dialogue: 0,0:14:42.32,0:14:45.12,Default,,0000,0000,0000,,... muốn làm chuyện đó cùng anh.
Dialogue: 0,0:14:49.50,0:14:52.00,Default,,0000,0000,0000,,Y-Yuna, em không sao chứ?
Dialogue: 0,0:14:53.73,0:14:58.18,Default,,0000,0000,0000,,Vâng, hơi đau một chút.
Dialogue: 0,0:15:03.03,0:15:04.73,Default,,0000,0000,0000,,Anh di chuyển được chưa?
Dialogue: 0,0:15:05.21,0:15:07.63,Default,,0000,0000,0000,,Nó khít tới mức khiến anh sắp ra rồi.
Dialogue: 0,0:15:08.61,0:15:10.28,Default,,0000,0000,0000,,Vâng, làm đi anh.
Dialogue: 0,0:15:23.56,0:15:24.98,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:15:26.06,0:15:27.90,Default,,0000,0000,0000,,Anh xin lỗi, Yuna.
Dialogue: 0,0:15:28.84,0:15:32.03,Default,,0000,0000,0000,,Chà xát bên trong em sướng quá đi mất.
Dialogue: 0,0:15:38.35,0:15:40.97,Default,,0000,0000,0000,,Âm hộ em làm cho anh phấn khích ư?
Dialogue: 0,0:15:42.77,0:15:45.19,Default,,0000,0000,0000,,Cảm giác bên trong em thật là tuyệt.
Dialogue: 0,0:15:45.40,0:15:47.80,Default,,0000,0000,0000,,Chúng mình vừa hôn nhau vừa làm chuyện đó đi anh.
Dialogue: 0,0:16:08.61,0:16:11.02,Default,,0000,0000,0000,,Onii-chan, nhột quá.
Dialogue: 0,0:16:12.42,0:16:15.78,Default,,0000,0000,0000,,Anh tưởng làm vậy mới khiến em dịu bớt cơn đau chứ.
Dialogue: 0,0:16:16.70,0:16:17.50,Default,,0000,0000,0000,,Cám ơn anh.
Dialogue: 0,0:16:18.43,0:16:19.01,Default,,0000,0000,0000,,Sao thế?
Dialogue: 0,0:16:19.69,0:16:20.75,Default,,0000,0000,0000,,Không, không có gì.
Dialogue: 0,0:16:21.32,0:16:22.19,Default,,0000,0000,0000,,Nói cho anh nghe đi.
Dialogue: 0,0:16:23.18,0:16:25.44,Default,,0000,0000,0000,,Anh là một tên mê ngực bự.
Dialogue: 0,0:16:26.86,0:16:30.14,Default,,0000,0000,0000,,Nhưng đừng lo. Ngực em sẽ phát triển sớm thôi.
Dialogue: 0,0:16:39.56,0:16:40.87,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:16:51.24,0:16:54.34,Default,,0000,0000,0000,,Tại sao Makoto và Yuna-chan lại...
Dialogue: 0,0:16:56.68,0:16:59.54,Default,,0000,0000,0000,,Makoto, sao em lại chọn Yuna-chan?
Dialogue: 0,0:17:05.03,0:17:08.37,Default,,0000,0000,0000,,Còn Yuna-chan, tại sao phải là Makoto chứ?
Dialogue: 0,0:17:13.70,0:17:15.72,Default,,0000,0000,0000,,Onii-chan, nhột quá.
Dialogue: 0,0:17:16.43,0:17:19.58,Default,,0000,0000,0000,,Anh tưởng làm vậy mới khiến em dịu bớt cơn đau chứ.
Dialogue: 0,0:17:20.27,0:17:21.67,Default,,0000,0000,0000,,Vâng, cám ơn anh.
Dialogue: 0,0:17:30.93,0:17:33.54,Default,,0000,0000,0000,,Giờ nó đâm vào dễ dàng hơn rồi đó, Yuna.
Dialogue: 0,0:17:34.38,0:17:38.05,Default,,0000,0000,0000,,Vâng, giờ em cảm thấy dễ chịu hơn rất nhiều.
Dialogue: 0,0:17:45.94,0:17:50.01,Default,,0000,0000,0000,,Onii-chan, anh lại chơi đùa ngực em nữa rồi.
Dialogue: 0,0:17:51.42,0:17:53.87,Default,,0000,0000,0000,,Anh làm vậy thì ngực em mới mau lớn được chứ, đúng không?
Dialogue: 0,0:17:54.23,0:17:57.84,Default,,0000,0000,0000,,Ban nãy em cũng nói rồi đó, anh rất mê ngực.
Dialogue: 0,0:17:58.91,0:18:00.30,Default,,0000,0000,0000,,Giống như của Onee-chan hả?
Dialogue: 0,0:18:00.84,0:18:02.68,Default,,0000,0000,0000,,Ờ, có lẽ vậy...
Dialogue: 0,0:18:10.54,0:18:16.37,Default,,0000,0000,0000,,Thằng nhỏ bự con của Makoto đang đâm vào thân thể mảnh mai của Yuna-chan.
Dialogue: 0,0:18:30.58,0:18:33.44,Default,,0000,0000,0000,,Makoto... Makoto...
Dialogue: 0,0:18:33.95,0:18:36.42,Default,,0000,0000,0000,,Em đã thuộc về chị từ rất lâu rồi...
Dialogue: 0,0:18:39.72,0:18:42.68,Default,,0000,0000,0000,,Âm hộ của chị thật cô đơn.
Dialogue: 0,0:18:44.06,0:18:48.45,Default,,0000,0000,0000,,Makoto... Makoto... Makoto...
Dialogue: 0,0:18:49.15,0:18:50.45,Default,,0000,0000,0000,,Giống như của Onee-chan hả?
Dialogue: 0,0:18:51.69,0:18:53.66,Default,,0000,0000,0000,,Ờ, có lẽ vậy...
Dialogue: 0,0:18:54.11,0:18:55.81,Default,,0000,0000,0000,,Makoto!
Dialogue: 0,0:19:04.11,0:19:05.47,Default,,0000,0000,0000,,Nee-chan?
Dialogue: 0,0:19:20.77,0:19:21.93,Default,,0000,0000,0000,,Nè, Onii-chan.
Dialogue: 0,0:19:24.30,0:19:26.27,Default,,0000,0000,0000,,Anh mau làm chuyện đó với Onee-chan đi.
Dialogue: 0,0:19:26.64,0:19:28.19,Default,,0000,0000,0000,,Ế?
Dialogue: 0,0:19:28.94,0:19:33.01,Default,,0000,0000,0000,,Onee-chan đã chứng kiến tất cả chuyện đó nãy giờ rồi.
Dialogue: 0,0:19:34.28,0:19:37.91,Default,,0000,0000,0000,,Có lẽ chị ấy cũng đã nhận ra tình cảm thật sự của mình.
Dialogue: 0,0:19:41.29,0:19:42.93,Default,,0000,0000,0000,,Nhưng còn em thì sao, Yuna?
Dialogue: 0,0:19:44.26,0:19:49.68,Default,,0000,0000,0000,,Đừng lo cho em. Sau tất cả, cảm xúc của anh vẫn là thứ quan trọng nhất.
Dialogue: 0,0:19:51.81,0:19:53.40,Default,,0000,0000,0000,,Vậy còn cảm xúc của em thì sao?
Dialogue: 0,0:19:55.52,0:19:59.54,Default,,0000,0000,0000,,Em nghĩ ba chúng ta có thể cùng làm chuyện đó...
Dialogue: 0,0:20:00.98,0:20:01.75,Default,,0000,0000,0000,,... được không?
Dialogue: 0,0:20:13.84,0:20:15.72,Default,,0000,0000,0000,,Anh chiều lòng Onee-chan trước đi.
Dialogue: 0,0:20:15.98,0:20:16.96,Default,,0000,0000,0000,,Yuna-chan?
Dialogue: 0,0:20:18.04,0:20:22.05,Default,,0000,0000,0000,,Ban nãy em làm chuyện đó rồi, giờ tới lượt chị đó.
Dialogue: 0,0:20:22.98,0:20:24.47,Default,,0000,0000,0000,,Phải không, Onii-chan?
Dialogue: 0,0:20:24.93,0:20:26.35,Default,,0000,0000,0000,,Ờ.
Dialogue: 0,0:20:30.62,0:20:33.40,Default,,0000,0000,0000,,Vậy em vào đây.
Dialogue: 0,0:20:39.33,0:20:41.30,Default,,0000,0000,0000,,Em bắt đầu di chuyển nhé.
Dialogue: 0,0:20:41.46,0:20:42.98,Default,,0000,0000,0000,,Không, đừng mà...
Dialogue: 0,0:20:51.81,0:20:53.68,Default,,0000,0000,0000,,Makoto... Makoto...
Dialogue: 0,0:21:20.21,0:21:22.14,Default,,0000,0000,0000,,Dừng lại, Yuna-chan...
Dialogue: 0,0:21:25.55,0:21:26.81,Default,,0000,0000,0000,,Em không dừng lại đâu.
Dialogue: 0,0:21:27.49,0:21:30.32,Default,,0000,0000,0000,,Hai đầu ti của chị thật hồng hào xinh xắn.
Dialogue: 0,0:21:33.67,0:21:34.48,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:21:50.34,0:21:53.94,Default,,0000,0000,0000,,Thằng nhỏ của anh đâm vào trong thật tuyệt, Onii-chan!
Dialogue: 0,0:21:54.14,0:21:55.63,Default,,0000,0000,0000,,Em vui lắm!
Dialogue: 0,0:22:03.75,0:22:05.77,Default,,0000,0000,0000,,Đừng mà! Đừng mà, Onee-chan...
Dialogue: 0,0:22:06.09,0:22:11.83,Default,,0000,0000,0000,,Lưỡi của chị kết hợp cùng thằng nhỏ của anh ấy sẽ làm em phát điên mất!
Dialogue: 0,0:22:12.63,0:22:14.72,Default,,0000,0000,0000,,Yuna-chan, liếm của chị đi.
Dialogue: 0,0:22:21.27,0:22:28.21,Default,,0000,0000,0000,,Yuna-chan, lưỡi của em đang đùa giỡn nơi Makoto đâm vào.
Dialogue: 0,0:22:33.67,0:22:38.55,Default,,0000,0000,0000,,Âm vật của hai chúng ta đang chà xát vào nhau!
Dialogue: 0,0:22:38.80,0:22:41.25,Default,,0000,0000,0000,,Em thật dâm đãng, Yuna-chan.
Dialogue: 0,0:22:41.74,0:22:44.12,Default,,0000,0000,0000,,Em sắp làm chị ra rồi!
Dialogue: 0,0:22:48.98,0:22:51.47,Default,,0000,0000,0000,,Với Onii-chan và Onee-chan!
Dialogue: 0,0:22:51.79,0:22:54.20,Default,,0000,0000,0000,,Chị có thể cảm thấy cơ thể nóng bỏng của hai người.
Dialogue: 0,0:22:55.23,0:22:59.90,Default,,0000,0000,0000,,Onii-chan, nếu tiếp tục như vậy, anh sẽ ra phải không?
Dialogue: 0,0:22:59.94,0:23:03.27,Default,,0000,0000,0000,,Cả hai người thật dâm đãng...
Dialogue: 0,0:23:03.65,0:23:06.35,Default,,0000,0000,0000,,Làm chị cũng muốn dâm đãng theo!
Dialogue: 0,0:23:10.72,0:23:12.37,Default,,0000,0000,0000,,Em sắp ra rồi...
Dialogue: 0,0:23:12.67,0:23:16.43,Default,,0000,0000,0000,,Bọn em sắp ra rồi, Makoto!
Dialogue: 0,0:23:16.46,0:23:20.22,Default,,0000,0000,0000,,Bọn em ra rồi! Bọn em ra rồi! Bọn em ra rồi!
Dialogue: 0,0:23:49.30,0:23:50.83,Default,,0000,0000,0000,,Mơ với chả mộng...
Dialogue: 0,0:24:05.63,0:24:09.54,Default,,0000,0000,0000,,Có vẻ như ba đứa vẫn còn trẻ con hơn mình tưởng.
Dialogue: 0,0:24:16.19,0:24:19.38,Default,,0000,0000,0000,,Cứu chị với, em giai bé bỏng...
Dialogue: 0,0:24:19.65,0:24:22.12,Default,,0000,0000,0000,,Makoto-kun, đừng có nhúng tay vào!
Dialogue: 0,0:24:22.40,0:24:26.22,Default,,0000,0000,0000,,Kasukabe-san sẽ không trốn thoát dễ dàng như vậy đâu!
Dialogue: 0,0:24:26.50,0:24:30.65,Default,,0000,0000,0000,,Không ngờ mình lại bị sát hại dưới tay một y tá ngay trong bệnh...
Dialogue: 0,0:24:30.93,0:24:34.13,Default,,0000,0000,0000,,Đừng có đùa kiểu đó!
Dialogue: 0,0:24:37.09,0:24:40.06,Default,,0000,0000,0000,,Senpai, gieo nhân nào thì gặt quả ấy thôi.
Dialogue: 0,0:25:05.70,0:25:06.33,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:00:01.70,0:00:06.70,Default,,0000,0000,0000,,
