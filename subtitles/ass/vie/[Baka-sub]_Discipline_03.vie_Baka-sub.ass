Dialogue: 0,0:00:02.00,0:00:03.73,Default,,0000,0000,0000,,Tất cả là của tôi.
Dialogue: 0,0:00:08.27,0:00:12.17,Default,,0000,0000,0000,,Giờ hãy vào bên trong và làm cho tôi thỏa mãn.
Dialogue: 0,0:00:21.42,0:00:22.91,Default,,0000,0000,0000,,Tuyệt quá! Tuyệt quá!
Dialogue: 0,0:00:37.73,0:00:43.40,Default,,0000,0000,0000,,Tuyệt quá! Chưa bao giờ mình cảm thấy thế này!
Dialogue: 0,0:00:47.54,0:00:51.88,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:00:55.05,0:00:57.11,Default,,0000,0000,0000,,Thật tuyệt diệu!
Dialogue: 0,0:00:58.59,0:01:00.68,Default,,0000,0000,0000,,Ô, phải...!
Dialogue: 0,0:01:11.27,0:01:16.10,Default,,0000,0000,0000,,Ôi, đúng rồi... ra đi, ra đi!
Dialogue: 0,0:01:21.71,0:01:22.79,Default,,0000,0000,0000,,Tất cả học sinh!
Dialogue: 0,0:01:23.11,0:01:26.10,Default,,0000,0000,0000,,Có cháy ở tòa nhà Hội Thân hữu.
Dialogue: 0,0:01:28.58,0:01:29.38,Default,,0000,0000,0000,,Ruri-chan?
Dialogue: 0,0:01:29.65,0:01:35.02,Default,,0000,0000,0000,,Bình tĩnh thoát ra khỏi đó,\N nếu không muốn bị thiêu sống!
Dialogue: 0,0:01:36.12,0:01:39.49,Default,,0000,0000,0000,,Này! Đi đâu đó? Định bỏ ta lại đây à?
Dialogue: 0,0:01:39.76,0:01:40.76,Default,,0000,0000,0000,,Hãy chờ đã!
Dialogue: 0,0:01:41.53,0:01:42.09,Default,,0000,0000,0000,,Đến lúc rồi!
Dialogue: 0,0:01:47.14,0:01:48.40,Default,,0000,0000,0000,,Otokawa-san!
Dialogue: 0,0:01:48.64,0:01:49.77,Default,,0000,0000,0000,,Takurou-kun!
Dialogue: 0,0:01:53.84,0:01:54.60,Default,,0000,0000,0000,,Đi thôi!
Dialogue: 0,0:01:54.81,0:01:55.44,Default,,0000,0000,0000,,Ừ!
Dialogue: 0,0:02:09.80,0:02:13.54,Default,,0000,0000,0000,,{\fad(1100,0)\bord0\3c&HFFFFFF&\4c&HFFFFFF&\pos(56,458)\c&HD4D4D4&}baka-sub
Dialogue: 0,0:02:09.80,0:02:13.54,Default,,0000,0000,0000,,{\fs26\fad(1100,0)\bord0\3c&HFFFFFF&\4c&HFFFFFF&\pos(92,477)\c&HD4D4D4&}http://baka-sub.net/
Dialogue: 0,0:02:10.31,0:02:13.54,Default,,0000,0000,0000,,{\fad(1100,0)\bord2\shad0\4c&HFFFFFF&\b1\3c&H000000&\c&HD4D4D4&\pos(320,397)}- Chương 3 -
Dialogue: 0,0:02:26.21,0:02:27.11,Default,,0000,0000,0000,,Cháy à?
Dialogue: 0,0:02:27.71,0:02:29.64,Default,,0000,0000,0000,,Chúng ta còn chưa phóng hỏa mà.
Dialogue: 0,0:02:31.85,0:02:35.28,Default,,0000,0000,0000,,Không cần biết đang xảy ra chuyện gì,\N đây là cơ hội của chúng ta!
Dialogue: 0,0:02:35.65,0:02:37.74,Default,,0000,0000,0000,,Tiếp tục làm theo kế hoạch!
Dialogue: 0,0:02:37.85,0:02:38.58,Default,,0000,0000,0000,,Ừ!
Dialogue: 0,0:02:39.12,0:02:41.95,Default,,0000,0000,0000,,Một, hai, một, hai.
Dialogue: 0,0:02:47.50,0:02:48.86,Default,,0000,0000,0000,,Thành công rực rỡ!
Dialogue: 0,0:02:49.26,0:02:53.86,Default,,0000,0000,0000,,Phải, Mai-Mai giỏi thật đấy!\N Không có gì mà cậu không làm được!
Dialogue: 0,0:02:54.17,0:02:54.83,Default,,0000,0000,0000,,À, phải.
Dialogue: 0,0:02:58.71,0:03:01.77,Default,,0000,0000,0000,,Được rồi, chúng ta đi đón bọn họ thôi!
Dialogue: 0,0:03:01.84,0:03:02.94,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:03:07.15,0:03:08.85,Default,,0000,0000,0000,,Sao vậy, Mai-Mai?
Dialogue: 0,0:03:09.85,0:03:13.09,Default,,0000,0000,0000,,Các cậu không nghĩ chúng ta nên theo dõi một lúc nữa à?
Dialogue: 0,0:03:14.02,0:03:15.61,Default,,0000,0000,0000,,Ờ, phải.
Dialogue: 0,0:03:31.04,0:03:34.74,Default,,0000,0000,0000,,Đau quá, chuyện gì đã xảy ra ở đây vậy chứ?
Dialogue: 0,0:03:38.18,0:03:38.80,Default,,0000,0000,0000,,Hơ?!
Dialogue: 0,0:03:41.92,0:03:42.71,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:03:43.39,0:03:44.91,Default,,0000,0000,0000,,Dừng lại ngay!
Dialogue: 0,0:03:45.92,0:03:47.95,Default,,0000,0000,0000,,Các người đang làm gì?
Dialogue: 0,0:03:50.56,0:03:54.09,Default,,0000,0000,0000,,Có biết tôi là ai không? Bỏ tôi xuống!
Dialogue: 0,0:03:54.40,0:03:56.09,Default,,0000,0000,0000,,Tôi bảo bỏ tôi xuống!
Dialogue: 0,0:04:05.17,0:04:08.90,Default,,0000,0000,0000,,Nhìn em kìa, chắc là vui lắm hả!
Dialogue: 0,0:04:11.51,0:04:15.13,Default,,0000,0000,0000,,Ogiwara, liếm mạnh hơn một chút đi.
Dialogue: 0,0:04:16.58,0:04:18.35,Default,,0000,0000,0000,,Vâng, Reina-sama.
Dialogue: 0,0:04:26.46,0:04:30.16,Default,,0000,0000,0000,,Matsuno, tôi muốn cô đến kiểm tra Hội thân hữu một chút.
Dialogue: 0,0:04:30.70,0:04:33.57,Default,,0000,0000,0000,,Vâng... xin lỗi... Reina-sama...
Dialogue: 0,0:04:33.80,0:04:34.23,Default,,0000,0000,0000,,Chuyện gì?
Dialogue: 0,0:04:35.10,0:04:39.19,Default,,0000,0000,0000,,Tôi cũng muốn làm thế với...\N Ogiwara-sensei... được không?
Dialogue: 0,0:04:39.61,0:04:43.86,Default,,0000,0000,0000,,Không được, cô không thấy Ogiwara đang phục vụ tôi à.
Dialogue: 0,0:04:44.48,0:04:45.14,Default,,0000,0000,0000,,Đi đi!
Dialogue: 0,0:04:45.61,0:04:47.55,Default,,0000,0000,0000,,Vâng, tôi xin phép.
Dialogue: 0,0:04:49.32,0:04:55.59,Default,,0000,0000,0000,,Nhanh lên Takurou. Không biết cậu bé đó có phải là chìa khóa?
Dialogue: 0,0:05:01.83,0:05:07.39,Default,,0000,0000,0000,,Nè, mọi người, cảm ơn nhiều lắm, vì tôi mà các cậu...
Dialogue: 0,0:05:09.30,0:05:11.90,Default,,0000,0000,0000,,Coi nào, cậu nói gì thế, Saorin?
Dialogue: 0,0:05:12.17,0:05:15.27,Default,,0000,0000,0000,,Sao phải nói vậy, cậu đâu phải là người có lỗi.
Dialogue: 0,0:05:15.58,0:05:18.21,Default,,0000,0000,0000,,Là tại cái bọn người ngu ngốc của Hội Thân hữu đó...
Dialogue: 0,0:05:18.45,0:05:19.21,Default,,0000,0000,0000,,Ừ, cảm ơn.
Dialogue: 0,0:05:19.81,0:05:23.51,Default,,0000,0000,0000,,Nhưng là do tôi lẻn vào đó mà không nghĩ đến hậu quả.
Dialogue: 0,0:05:24.15,0:05:27.29,Default,,0000,0000,0000,,Không đúng, Morimoto Leona là kẻ xấu.
Dialogue: 0,0:05:27.59,0:05:29.75,Default,,0000,0000,0000,,Otokawa-san, cậu không phải lo gì cả.
Dialogue: 0,0:05:32.29,0:05:35.32,Default,,0000,0000,0000,,Ồ? Sao vậy? Có chuyện gì đã xảy ra à?
Dialogue: 0,0:05:35.53,0:05:38.21,Default,,0000,0000,0000,,Saorin đỏ mặt kìa. Ta-kun cũng thế!
Dialogue: 0,0:05:38.77,0:05:42.80,Default,,0000,0000,0000,,Này! Tớ biết các cậu nhìn thấy hết mà, đừng có chọc nữa!
Dialogue: 0,0:05:46.91,0:05:47.50,Default,,0000,0000,0000,,Nhưng từ giờ...
Dialogue: 0,0:05:48.14,0:05:51.68,Default,,0000,0000,0000,,...chúng ta sẽ phải đối đầu với Hội Thân hữu.
Dialogue: 0,0:05:52.30,0:05:55.58,Default,,0000,0000,0000,,Họ chắc sẽ đoán ra tôi.
Dialogue: 0,0:06:00.19,0:06:02.81,Default,,0000,0000,0000,,Coi nào, nếu mọi chuyện trở nên tồi tệ...
Dialogue: 0,0:06:03.33,0:06:06.24,Default,,0000,0000,0000,,...chúng ta chỉ việc giao Takurou cho họ.
Dialogue: 0,0:06:06.69,0:06:08.29,Default,,0000,0000,0000,,Ồ, phải đó!
Dialogue: 0,0:06:08.60,0:06:10.53,Default,,0000,0000,0000,,Hở? Nói vậy là sao chứ...
Dialogue: 0,0:06:11.03,0:06:12.66,Default,,0000,0000,0000,,Ừ, ý đó hay đấy.
Dialogue: 0,0:06:13.00,0:06:14.70,Default,,0000,0000,0000,,Không phải chứ, Otokawa-san.
Dialogue: 0,0:06:14.87,0:06:15.93,Default,,0000,0000,0000,,Đùa thôi, đùa thôi!
Dialogue: 0,0:06:16.37,0:06:19.10,Default,,0000,0000,0000,,Tha cho tôi đi nào!
Dialogue: 0,0:06:36.22,0:06:38.28,Default,,0000,0000,0000,,Tôi hết kiên nhẫn rồi đấy!
Dialogue: 0,0:06:38.32,0:06:41.53,Default,,0000,0000,0000,,Thả tôi ra ngay, nếu không tôi sẽ thật sự nổi giận!
Dialogue: 0,0:06:47.34,0:06:49.74,Default,,0000,0000,0000,,Này, cho tôi biết các người là ai!
Dialogue: 0,0:06:53.72,0:06:54.49,Default,,0000,0000,0000,,Thử máy... thử máy.
Dialogue: 0,0:06:55.41,0:06:56.97,Default,,0000,0000,0000,,Morimoto Leona.
Dialogue: 0,0:06:57.28,0:07:00.15,Default,,0000,0000,0000,,Hôm nay, bọn ta sẽ thay trời trừng phạt cô.
Dialogue: 0,0:07:00.82,0:07:03.41,Default,,0000,0000,0000,,Trừng phạt? Các người nói gì thế?
Dialogue: 0,0:07:03.75,0:07:05.28,Default,,0000,0000,0000,,Chuyện này nghĩa là sao?
Dialogue: 0,0:07:05.62,0:07:09.65,Default,,0000,0000,0000,,Nếu không dừng lại ngay,\N tôi sẽ không tha thứ cho mấy người.
Dialogue: 0,0:07:12.09,0:07:16.29,Default,,0000,0000,0000,,Này, bình tĩnh! Không có chuyện gì đâu!
Dialogue: 0,0:07:16.60,0:07:19.50,Default,,0000,0000,0000,,Morimoto Leona, vì những\N hành động sai trái cô đã làm...
Dialogue: 0,0:07:19.77,0:07:22.83,Default,,0000,0000,0000,,...Chúa đã nổi giạn và quyết không dung thứ...
Dialogue: 0,0:07:23.61,0:07:27.57,Default,,0000,0000,0000,,Hôm nay, bọn ta, sứ giả của Chúa, sẽ răng dạy ngươi.
Dialogue: 0,0:07:27.87,0:07:32.11,Default,,0000,0000,0000,,Được rồi, bắt đầu trừng phạt, chuẩn bị tinh thần đi!
Dialogue: 0,0:07:43.89,0:07:47.49,Default,,0000,0000,0000,,Morimoto Leona, cô sẽ chịu được bao lâu?
Dialogue: 0,0:07:53.50,0:07:55.26,Default,,0000,0000,0000,,Nào, làm thôi!
Dialogue: 0,0:07:55.94,0:07:56.96,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:07:57.37,0:07:59.90,Default,,0000,0000,0000,,Nào, ta hãy chuẩn bị yến tiệc!
Dialogue: 0,0:09:30.57,0:09:31.73,Default,,0000,0000,0000,,Này, tôi nữa.
Dialogue: 0,0:09:44.08,0:09:46.95,Default,,0000,0000,0000,,Thật hôi thối, giống hơi thở của một lão già!
Dialogue: 0,0:09:47.38,0:09:48.58,Default,,0000,0000,0000,,Ngươi... im đi!
Dialogue: 0,0:09:56.59,0:09:57.75,Default,,0000,0000,0000,,Khốn kiếp.
Dialogue: 0,0:11:30.85,0:11:37.52,Default,,0000,0000,0000,,Onee-sama! Em bị tấn công và bị ép làm những điều không thể nói.
Dialogue: 0,0:11:37.69,0:11:40.09,Default,,0000,0000,0000,,Vậy mà chị nói sẽ không làm gì cả sao?
Dialogue: 0,0:11:41.03,0:11:42.76,Default,,0000,0000,0000,,Không phải vậy...
Dialogue: 0,0:11:42.97,0:11:48.16,Default,,0000,0000,0000,,...trước hết, em phải kể cho chị nghe chuyện gì xảy ra đã.
Dialogue: 0,0:11:48.64,0:11:50.47,Default,,0000,0000,0000,,Chuyện gì à? Thì là...
Dialogue: 0,0:11:51.57,0:11:55.00,Default,,0000,0000,0000,,...đại loại là... tất cả những chuyện đó...
Dialogue: 0,0:11:58.88,0:12:00.05,Default,,0000,0000,0000,,...đã xảy ra với em.
Dialogue: 0,0:12:01.94,0:12:04.63,Default,,0000,0000,0000,,Chị hiểu rồi... hiểu rồi, Leona.
Dialogue: 0,0:12:05.29,0:12:07.31,Default,,0000,0000,0000,,Matsuno, đem cô ta vào đây.
Dialogue: 0,0:12:13.13,0:12:13.96,Default,,0000,0000,0000,,Hơ?
Dialogue: 0,0:12:16.66,0:12:21.11,Default,,0000,0000,0000,,Nào, nói cho ta biết chính xác chuyện gì xảy ra ở phòng điều khiển.
Dialogue: 0,0:12:21.60,0:12:22.69,Default,,0000,0000,0000,,Phòng điều khiển?
Dialogue: 0,0:12:23.40,0:12:25.45,Default,,0000,0000,0000,,Vâng, hôm qua...
Dialogue: 0,0:12:26.14,0:12:28.56,Default,,0000,0000,0000,,Tôi ở lại để kiểm tra thiết bị...
Dialogue: 0,0:12:28.99,0:12:31.08,Default,,0000,0000,0000,,...nhưng một nữ sinh đã tới.
Dialogue: 0,0:12:31.91,0:12:33.01,Default,,0000,0000,0000,,Một nữ sinh?
Dialogue: 0,0:12:33.48,0:12:38.11,Default,,0000,0000,0000,,Vâng, tôi hỏi cô ta cần gì, nhưng cô ta không trả lời.
Dialogue: 0,0:12:39.09,0:12:40.71,Default,,0000,0000,0000,,Sau đó, cô ta lại gần tôi...
Dialogue: 0,0:12:43.26,0:12:45.75,Default,,0000,0000,0000,,...trước khi tôi kịp nhận ra, cô ta hôn tôi.
Dialogue: 0,0:13:17.12,0:13:21.50,Default,,0000,0000,0000,,Không! Tôi không... tôi sắp ra rồi!
Dialogue: 0,0:13:22.13,0:13:24.19,Default,,0000,0000,0000,,Có chuyện đó nữa sao?
Dialogue: 0,0:13:25.30,0:13:30.24,Default,,0000,0000,0000,,Vâng, tôi xin lỗi, tôi không kiềm chế được, nó sướng quá.
Dialogue: 0,0:13:30.77,0:13:33.47,Default,,0000,0000,0000,,Cùng khoảng thời gian đó, hệ thống điều khiển bị kiểm soát...
Dialogue: 0,0:13:33.71,0:13:37.14,Default,,0000,0000,0000,,...và Leona gặp rắc rối.
Dialogue: 0,0:13:37.48,0:13:38.65,Default,,0000,0000,0000,,Cô ta là ai?
Dialogue: 0,0:13:39.31,0:13:42.28,Default,,0000,0000,0000,,Cô ta hẳn là người đứng đằng sau chuyện đó!
Dialogue: 0,0:13:42.98,0:13:45.82,Default,,0000,0000,0000,,Cô ta là ai và đang ở đâu?
Dialogue: 0,0:13:53.39,0:13:55.69,Default,,0000,0000,0000,,Người ở ký túc xá, Kaneda Maiko.
Dialogue: 0,0:13:56.00,0:13:59.13,Default,,0000,0000,0000,,Bạn của chàng trai yêu quý, Hayami-kun.
Dialogue: 0,0:14:01.74,0:14:02.79,Default,,0000,0000,0000,,Là cô ta.
Dialogue: 0,0:14:03.24,0:14:07.14,Default,,0000,0000,0000,,Nè, Leona, Chị có ý này, em sẽ nghe chứ?
Dialogue: 0,0:14:08.84,0:14:09.68,Default,,0000,0000,0000,,Hừm?
Dialogue: 0,0:14:14.18,0:14:16.05,Default,,0000,0000,0000,,Một lực lượng giấu mặt?
Dialogue: 0,0:14:16.28,0:14:18.95,Default,,0000,0000,0000,,Phải, làm nhục hết mức có thể.
Dialogue: 0,0:14:19.72,0:14:22.94,Default,,0000,0000,0000,,Phải, đúng vậy, em sẽ làm thế.
Dialogue: 0,0:14:23.52,0:14:27.95,Default,,0000,0000,0000,,Cứ chờ đó. Kaneda Maiko!
Dialogue: 0,0:14:32.37,0:14:33.54,Default,,0000,0000,0000,,Hơ?
Dialogue: 0,0:14:44.56,0:14:45.77,Default,,0000,0000,0000,,Sudou của câu lạc bộ nhiếp ảnh.
Dialogue: 0,0:14:46.78,0:14:49.44,Default,,0000,0000,0000,,Bọn ta phạt cậu vì dám nhìn lén...
Dialogue: 0,0:14:49.95,0:14:51.89,Default,,0000,0000,0000,,...nhưng nếu chấp nhận điều kiện, cậu sẽ được thả.
Dialogue: 0,0:14:53.32,0:14:54.95,Default,,0000,0000,0000,,Cậu muốn nghe điều kiện chứ?
Dialogue: 0,0:15:06.80,0:15:08.63,Default,,0000,0000,0000,,Kyoutou-sensei, hôm qua làm tốt lắm.
Dialogue: 0,0:15:15.31,0:15:17.50,Default,,0000,0000,0000,,Thật sự thì chuyện đó rất tuyệt.
Dialogue: 0,0:15:17.91,0:15:19.07,Default,,0000,0000,0000,,Con phù thủy hấp dẫn đó...
Dialogue: 0,0:15:19.35,0:15:22.81,Default,,0000,0000,0000,,...Morimoto Leona bị hành hạ không thương tiếc.
Dialogue: 0,0:15:23.42,0:15:28.86,Default,,0000,0000,0000,,Vì nó mà giáo viên chúng ta phải làm những chuyện kinh khủng.
Dialogue: 0,0:15:29.29,0:15:31.37,Default,,0000,0000,0000,,Từ hôm nay, chúng ta sẽ lấy lại những gì đã mất!
Dialogue: 0,0:15:31.40,0:15:34.25,Default,,0000,0000,0000,,Machida, hãy cố lên, còn nhiều việc phải làm!
Dialogue: 0,0:15:35.26,0:15:36.52,Default,,0000,0000,0000,,Hừm?! Gì thế?
Dialogue: 0,0:15:36.60,0:15:38.19,Default,,0000,0000,0000,,Kyoutou, nghe thông báo đi.
Dialogue: 0,0:15:38.43,0:15:38.86,Default,,0000,0000,0000,,Ô? Hừm.
Dialogue: 0,0:15:39.07,0:15:43.50,Default,,0000,0000,0000,,{\an8\i1}Ogiwara-sensei và Machida Kyoutou-sensei...{\i0}
Dialogue: 0,0:15:43.77,0:15:46.14,Default,,0000,0000,0000,,{\an8\i1}...đến văn phòng hiệu trưởng ngay lập tức.{\i0}
Dialogue: 0,0:15:51.61,0:15:53.64,Default,,0000,0000,0000,,Có thể là chuyện gì nhỉ?
Dialogue: 0,0:15:58.29,0:16:02.35,Default,,0000,0000,0000,,Nào, mọi người, chừng này là đã đủ.
Dialogue: 0,0:16:02.66,0:16:05.21,Default,,0000,0000,0000,,Có một chuyện tôi muốn yêu cầu các cậu.
Dialogue: 0,0:16:05.69,0:16:08.72,Default,,0000,0000,0000,,Hôm qua, đã có một việc xảy ra ở trường.
Dialogue: 0,0:16:09.83,0:16:12.51,Default,,0000,0000,0000,,Tôi muốn mọi người giúp đỡ...
Dialogue: 0,0:16:12.61,0:16:16.30,Default,,0000,0000,0000,,...tìm và trừng phạt kẻ gây ra vụ việc đó.
Dialogue: 0,0:16:17.41,0:16:19.90,Default,,0000,0000,0000,,Người đó là một nữ sinh.
Dialogue: 0,0:16:21.08,0:16:23.54,Default,,0000,0000,0000,,Leona sẽ giải thích chi tiết.
Dialogue: 0,0:16:24.48,0:16:25.19,Default,,0000,0000,0000,,Leona!
Dialogue: 0,0:16:32.99,0:16:34.28,Default,,0000,0000,0000,,Tôi sẽ nói thẳng!
Dialogue: 0,0:16:34.72,0:16:37.00,Default,,0000,0000,0000,,Búa của công lý phải được giáng xuống cô ta.
Dialogue: 0,0:16:37.49,0:16:39.13,Default,,0000,0000,0000,,Đó là nhiệm vụ duy nhất.
Dialogue: 0,0:16:42.75,0:16:45.96,Default,,0000,0000,0000,,Machida, ông có mùi của một lão già.
Dialogue: 0,0:16:46.67,0:16:50.36,Default,,0000,0000,0000,,Nhưng lần này, sự kinh tởm đó là cần thiết.
Dialogue: 0,0:16:55.04,0:16:59.00,Default,,0000,0000,0000,,Tôi muốn các anh dùng cái cơ thể và cái dương vật bẩn thỉu đó...
Dialogue: 0,0:16:59.18,0:17:02.08,Default,,0000,0000,0000,,...nện cô ta với tất cả những gì các anh có!
Dialogue: 0,0:17:07.79,0:17:11.38,Default,,0000,0000,0000,,Machida, Ogiwara, Sudou, hiểu rồi chứ?
Dialogue: 0,0:17:11.63,0:17:12.44,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:17:17.87,0:17:19.53,Default,,0000,0000,0000,,Nè, Kaneda-san đâu rồi?
Dialogue: 0,0:17:19.87,0:17:22.66,Default,,0000,0000,0000,,Cậu ấy bỏ bữa tối, chuyện đó cũng thường xảy ra.
Dialogue: 0,0:17:23.00,0:17:24.70,Default,,0000,0000,0000,,Ta-kun, để tôi giải thích!
Dialogue: 0,0:17:25.01,0:17:27.38,Default,,0000,0000,0000,,Lý do Mai-Mai không ăn tối đó là...
Dialogue: 0,0:17:28.31,0:17:31.15,Default,,0000,0000,0000,,Bởi vì ngay lúc này, cậu ta đang thủ dâm trong phòng.
Dialogue: 0,0:17:31.98,0:17:33.31,Default,,0000,0000,0000,,Nè, thôi đi.
Dialogue: 0,0:17:34.52,0:17:36.21,Default,,0000,0000,0000,,Thủ dâm á!
Dialogue: 0,0:17:53.00,0:17:55.44,Default,,0000,0000,0000,,Này, chuyện này nghĩa là sao?
Dialogue: 0,0:17:58.27,0:18:02.04,Default,,0000,0000,0000,,Xin chào, Kaneda Maiko-san!
Dialogue: 0,0:18:02.44,0:18:05.08,Default,,0000,0000,0000,,Cô muốn gì, Morimoto Leona-san?
Dialogue: 0,0:18:06.35,0:18:08.82,Default,,0000,0000,0000,,Hm?! Không quan trọng tôi là ai.
Dialogue: 0,0:18:09.42,0:18:12.21,Default,,0000,0000,0000,,Cô là người đã làm một chuyện rất tồi tệ.
Dialogue: 0,0:18:12.59,0:18:14.25,Default,,0000,0000,0000,,Ô, chuyện ngày hôm qua?
Dialogue: 0,0:18:15.26,0:18:18.85,Default,,0000,0000,0000,,Còn "Ô" được à! Sao cô có thể làm thế với tôi!
Dialogue: 0,0:18:19.26,0:18:20.78,Default,,0000,0000,0000,,Bắt cóc tôi...
Dialogue: 0,0:18:20.94,0:18:24.39,Default,,0000,0000,0000,,...rồi làm những chuyện kinh khủng và ghê tởm đó!
Dialogue: 0,0:18:25.07,0:18:27.77,Default,,0000,0000,0000,,Hãy chuẩn bị đón nhận những gì sắp xảy ra với cô!
Dialogue: 0,0:18:28.34,0:18:28.99,Default,,0000,0000,0000,,Sao?
Dialogue: 0,0:18:30.77,0:18:33.23,Default,,0000,0000,0000,,"Sao" cái gì? Cô sẽ sớm thấy thôi!
Dialogue: 0,0:18:33.87,0:18:36.57,Default,,0000,0000,0000,,Cô là người đứng đằng sau! Tôi có chứng cứ.
Dialogue: 0,0:18:36.94,0:18:38.50,Default,,0000,0000,0000,,Ý cô là sao?
Dialogue: 0,0:18:39.11,0:18:43.24,Default,,0000,0000,0000,,Được rồi, nếu vậy tôi không có lựa chọn nào khác.
Dialogue: 0,0:18:43.72,0:18:48.42,Default,,0000,0000,0000,,Mọi người! Không cần kiềm chế, hãy cho cô ta một trận ra trò!
Dialogue: 0,0:18:50.62,0:18:51.49,Default,,0000,0000,0000,,Nhanh lên!
Dialogue: 0,0:18:57.46,0:18:59.63,Default,,0000,0000,0000,,Không có cách nào khác, chúng ta làm thôi!
Dialogue: 0,0:19:00.47,0:19:01.46,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:19:35.17,0:19:36.57,Default,,0000,0000,0000,,Nóng!
Dialogue: 0,0:20:35.90,0:20:37.53,Default,,0000,0000,0000,,Các người làm gì thế?!
Dialogue: 0,0:20:37.73,0:20:40.19,Default,,0000,0000,0000,,Chưa được! Nữa đi, nữa đi, nữa đi!
Dialogue: 0,0:21:02.16,0:21:03.62,Default,,0000,0000,0000,,Bọn ngốc.
Dialogue: 0,0:21:04.12,0:21:05.59,Default,,0000,0000,0000,,Machida! Sudou!
Dialogue: 0,0:21:06.16,0:21:09.67,Default,,0000,0000,0000,,Hãy đâm cái thứ nhỏ bé và kinh tởm của hai người vào cùng một lúc...
Dialogue: 0,0:21:09.94,0:21:11.43,Default,,0000,0000,0000,,...làm cô ta ra đi!
Dialogue: 0,0:21:11.93,0:21:13.90,Default,,0000,0000,0000,,Nhỏ bé, kinh tởm?
Dialogue: 0,0:21:14.10,0:21:17.07,Default,,0000,0000,0000,,Sao? Hiệu phó và học sinh á?
Dialogue: 0,0:21:17.47,0:21:18.15,Default,,0000,0000,0000,,Thì sao chứ?
Dialogue: 0,0:21:18.67,0:21:23.61,Default,,0000,0000,0000,,Khốn kiếp! Ta vẫn đủ sức làm một cô gái phải rên rỉ!
Dialogue: 0,0:21:26.78,0:21:29.68,Default,,0000,0000,0000,,Ôi, này, không phải lỗ đó chứ.
Dialogue: 0,0:22:27.47,0:22:30.97,Default,,0000,0000,0000,,Thôi được rồi! Lần này chính ta sẽ ra tay!
Dialogue: 0,0:22:44.43,0:22:45.45,Default,,0000,0000,0000,,À, Maiko.
Dialogue: 0,0:22:45.76,0:22:46.19,Default,,0000,0000,0000,,Hở?
Dialogue: 0,0:22:46.36,0:22:47.69,Default,,0000,0000,0000,,Tối qua có chuyện gì vậy?
Dialogue: 0,0:22:47.96,0:22:49.07,Default,,0000,0000,0000,,Không có gì.
Dialogue: 0,0:22:49.50,0:22:51.06,Default,,0000,0000,0000,,Này, Maiko...
Dialogue: 0,0:22:51.21,0:22:53.12,Default,,0000,0000,0000,,...hôm nay da cậu trong đẹp thật.
Dialogue: 0,0:22:53.70,0:22:56.91,Default,,0000,0000,0000,,Vậy à? Lâu rồi tôi mới làm nhiều đến thế.
Dialogue: 0,0:22:57.40,0:23:00.24,Default,,0000,0000,0000,,Sao? Thủ dâm á?
Dialogue: 0,0:23:11.19,0:23:13.38,Default,,0000,0000,0000,,O-Onee-sama...
