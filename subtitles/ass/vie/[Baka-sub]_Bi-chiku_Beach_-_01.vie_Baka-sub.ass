Dialogue: 0,0:00:57.02,0:01:02.22,Default,,0000,0000,0000,,Mỗi khi hắn ta chụp hình, cơ thể chúng tôi đều nóng ran lên...
Dialogue: 0,0:01:02.78,0:01:05.49,Default,,0000,0000,0000,,và không thể kìm nén những suy nghĩ xấu xa của mình!
Dialogue: 0,0:01:06.52,0:01:07.78,Default,,0000,0000,0000,,Ôi không…
Dialogue: 0,0:01:07.78,0:01:11.46,Default,,0000,0000,0000,,Dâm thủy cứ thế trào ra từ chỗ đó...
Dialogue: 0,0:01:14.57,0:01:18.41,Default,,0000,0000,0000,,Này Kaede-tan, trông cô thật khêu gợi.
Dialogue: 0,0:01:18.41,0:01:22.35,Default,,0000,0000,0000,,Tư thế này... chẳng phải rất giống nhân vật em gái sao?
Dialogue: 0,0:01:24.45,0:01:27.23,Default,,0000,0000,0000,,Mình... Mình phải cố chịu đựng.
Dialogue: 0,0:01:27.23,0:01:28.96,Default,,0000,0000,0000,,Kẻo họ phát hiện ra mất!
Dialogue: 0,0:01:36.45,0:01:39.25,Default,,0000,0000,0000,,Ngày mai lại có buổi chụp hình đúng không?
Dialogue: 0,0:01:39.25,0:01:41.83,Default,,0000,0000,0000,,Anh hãy thôi ngay mấy trò đó đi.
Dialogue: 0,0:01:41.83,0:01:43.83,Default,,0000,0000,0000,,Cô vẫn còn hứng tình, phải không?
Dialogue: 0,0:01:44.45,0:01:47.38,Default,,0000,0000,0000,,Trước khi quay lại làm việc, chúng ta phải giải quyết xong vụ này mới được.
Dialogue: 0,0:01:51.02,0:01:53.18,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:01:55.32,0:01:58.64,Default,,0000,0000,0000,,Kaede-chan, thư giãn đi nào.
Dialogue: 0,0:02:00.03,0:02:02.13,Default,,0000,0000,0000,,Tôi chịu thôi.
Dialogue: 0,0:02:02.13,0:02:03.09,Default,,0000,0000,0000,,Tại sao...
Dialogue: 0,0:02:03.45,0:02:04.94,Default,,0000,0000,0000,,Tại vì...{romeyo add}
Dialogue: 0,0:02:06.61,0:02:08.69,Default,,0000,0000,0000,,Tôi sẽ tiếp tục cho đến khi cô chịu nói mới thôi.
Dialogue: 0,0:02:11.68,0:02:12.48,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:02:12.48,0:02:16.31,Default,,0000,0000,0000,,Nếu anh tiếp tục kích thích nó… Tôi sẽ…
Dialogue: 0,0:02:17.27,0:02:18.78,Default,,0000,0000,0000,,Tôi sẽ ra mất!
Dialogue: 0,0:02:20.00,0:02:22.37,Default,,0000,0000,0000,,Cái gì ra cơ?
Dialogue: 0,0:02:22.55,0:02:25.22,Default,,0000,0000,0000,,Đồ biến thái!
Dialogue: 0,0:02:25.22,0:02:27.67,Default,,0000,0000,0000,,Anh biết thừa rồi còn hỏi...
Dialogue: 0,0:02:28.10,0:02:30.26,Default,,0000,0000,0000,,Không biết thì tôi mới hỏi cô chứ.
Dialogue: 0,0:02:34.26,0:02:35.82,Default,,0000,0000,0000,,Nước tiểu...
Dialogue: 0,0:02:37.27,0:02:37.91,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:02:38.37,0:02:39.91,Default,,0000,0000,0000,,Tôi chả nghe thấy cô nói gì cả.
Dialogue: 0,0:02:42.46,0:02:45.99,Default,,0000,0000,0000,,Nước tiểu... Tôi sắp tè rồi.
Dialogue: 0,0:02:46.81,0:02:49.56,Default,,0000,0000,0000,,Làm ơn cho tôi vào nhà tắm!
Dialogue: 0,0:02:49.56,0:02:51.01,Default,,0000,0000,0000,,Xin anh đấy.
Dialogue: 0,0:02:51.73,0:02:55.02,Default,,0000,0000,0000,,Đừng lo, nơi này không có ai đâu.
Dialogue: 0,0:02:55.02,0:02:56.46,Default,,0000,0000,0000,,Cô cứ "xả" luôn ở đây cũng được.
Dialogue: 0,0:02:58.29,0:03:00.71,Default,,0000,0000,0000,,Không đời nào…
Dialogue: 0,0:03:06.32,0:03:08.20,Default,,0000,0000,0000,,Buông tôi ra...
Dialogue: 0,0:03:10.83,0:03:14.15,Default,,0000,0000,0000,,Thôi nào, thôi nào... cứ thoải mái tè ra đi.
Dialogue: 0,0:03:18.62,0:03:19.37,Default,,0000,0000,0000,,Tôi sẽ tè ra mất...
Dialogue: 0,0:03:22.18,0:03:22.87,Default,,0000,0000,0000,,Không được...
Dialogue: 0,0:03:22.87,0:03:26.76,Default,,0000,0000,0000,,Không... Đừng mà, đừng mà, đừng mà.
Dialogue: 0,0:03:26.76,0:03:29.54,Default,,0000,0000,0000,,Tôi tè rồi!
Dialogue: 0,0:03:31.47,0:03:34.53,Default,,0000,0000,0000,,Không thể ngừng lại được!
Dialogue: 0,0:03:35.84,0:03:36.51,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:03:36.51,0:03:37.88,Default,,0000,0000,0000,,Đừng nhìn!
Dialogue: 0,0:03:38.73,0:03:42.05,Default,,0000,0000,0000,,Đừng có nhìn tôi!
Dialogue: 0,0:03:44.73,0:03:46.04,Default,,0000,0000,0000,,Tuyệt ghê!
Dialogue: 0,0:03:46.47,0:03:48.54,Default,,0000,0000,0000,,Đây chính là nước tiểu của một người mẫu ảnh!
Dialogue: 0,0:03:48.54,0:03:50.82,Default,,0000,0000,0000,,Tôi không kiềm chế nổi nữa rồi!
Dialogue: 0,0:03:52.85,0:03:54.22,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:03:55.90,0:03:56.89,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:03:57.74,0:03:59.52,Default,,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:04:00.34,0:04:03.10,Default,,0000,0000,0000,,Ít ra anh cũng phải lau sạch nước tiểu đã chứ.
Dialogue: 0,0:04:13.40,0:04:15.19,Default,,0000,0000,0000,,Thả tôi xuống...
Dialogue: 0,0:04:15.19,0:04:16.87,Default,,0000,0000,0000,,Thả tôi xuống!
Dialogue: 0,0:04:18.46,0:04:22.92,Default,,0000,0000,0000,,Tiểu xong rồi mà cô vẫn còn hào hứng và phấn khích như vậy ư?!
Dialogue: 0,0:04:26.72,0:04:30.17,Default,,0000,0000,0000,,Không đời nào...
Dialogue: 0,0:04:34.45,0:04:36.94,Default,,0000,0000,0000,,Anh xấu lắm...
Dialogue: 0,0:04:41.88,0:04:47.35,Default,,0000,0000,0000,,Âm hộ ướt đẫm của cô đang quấn chặt lấy thằng nhỏ của tôi.
Dialogue: 0,0:04:54.04,0:04:56.01,Default,,0000,0000,0000,,Đừng... bú nó.
Dialogue: 0,0:04:58.46,0:04:59.73,Default,,0000,0000,0000,,Sướng không hả?
Dialogue: 0,0:05:03.22,0:05:05.85,Default,,0000,0000,0000,,Sướng quá!
Dialogue: 0,0:05:08.45,0:05:10.48,Default,,0000,0000,0000,,Nếu anh bú hai đầu ti, tôi sẽ...
Dialogue: 0,0:05:16.12,0:05:16.84,Default,,0000,0000,0000,,Sâu quá!
Dialogue: 0,0:05:16.84,0:05:20.01,Default,,0000,0000,0000,,Nó đang đâm sâu vào bên trong!
Dialogue: 0,0:05:20.70,0:05:23.73,Default,,0000,0000,0000,,Đâm vào trong cái gì thế hả?
Dialogue: 0,0:05:25.93,0:05:29.34,Default,,0000,0000,0000,,Anh lại thế nữa rồi, muốn tôi nói ra ư...
Dialogue: 0,0:05:29.34,0:05:30.19,Default,,0000,0000,0000,,Tất nhiên rồi...
Dialogue: 0,0:05:32.25,0:05:36.71,Default,,0000,0000,0000,,Thằng nhỏ của anh đang đâm sâu vào...
Dialogue: 0,0:05:38.23,0:05:43.78,Default,,0000,0000,0000,,Thằng nhỏ của anh đang đâm sâu vào âm hộ tôi
Dialogue: 0,0:05:47.46,0:05:50.32,Default,,0000,0000,0000,,Kaede, cô thật quyến rũ.
Dialogue: 0,0:05:53.11,0:05:54.14,Default,,0000,0000,0000,,Đừng nói nữa...
Dialogue: 0,0:05:54.14,0:05:57.96,Default,,0000,0000,0000,,Đừng nói những lời xấu xa đó nữa...
Dialogue: 0,0:05:59.74,0:06:01.41,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:06:01.41,0:06:02.84,Default,,0000,0000,0000,,Tôi sẽ phát điên mất.
Dialogue: 0,0:06:03.56,0:06:07.85,Default,,0000,0000,0000,,Nếu anh cứ hùng hục như vậy, âm hộ tôi sẽ rách mất đó!
Dialogue: 0,0:06:09.01,0:06:10.32,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:06:11.04,0:06:12.61,Default,,0000,0000,0000,,Tôi sẽ bắn vào bên trong!
Dialogue: 0,0:06:13.92,0:06:14.64,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:06:14.64,0:06:16.09,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:06:16.09,0:06:19.02,Default,,0000,0000,0000,,Đừng bắn vào bên trong!
Dialogue: 0,0:06:19.02,0:06:20.13,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:06:20.13,0:06:21.65,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:06:22.42,0:06:25.40,Default,,0000,0000,0000,,Đừng mà! Đừng mà!
Dialogue: 0,0:06:31.42,0:06:33.02,Default,,0000,0000,0000,,Không đời nào...
Dialogue: 0,0:06:35.99,0:06:37.43,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:06:37.43,0:06:40.02,Default,,0000,0000,0000,,Tinh dịch nóng bỏng của anh đang tràn vào bên trong...
Dialogue: 0,0:06:42.93,0:06:46.17,Default,,0000,0000,0000,,Tôi phải làm vậy thì cô mới trở lại bình thường được chứ.
Dialogue: 0,0:06:48.05,0:06:49.59,Default,,0000,0000,0000,,Biến thái.
Dialogue: 0,0:06:49.59,0:06:54.31,Default,,0000,0000,0000,,Anh là đồ biến thái!
Dialogue: 0,0:07:14.41,0:07:16.91,Default,,0000,0000,0000,,N-Nagisa-chan, cười lên nào.
Dialogue: 0,0:07:17.50,0:07:18.96,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:07:19.74,0:07:23.16,Default,,0000,0000,0000,,Khêu gợi quá, Nagisa-chan!
Dialogue: 0,0:07:23.49,0:07:25.40,Default,,0000,0000,0000,,Đó là...
Dialogue: 0,0:07:26.82,0:07:32.27,Default,,0000,0000,0000,,Không, anh ta... đã ép mình phải mặc bộ đồ này.
Dialogue: 0,0:07:49.88,0:07:53.46,Default,,0000,0000,0000,,Nagisa-chan, không lẽ nào... cô đang hứng tình ư?
Dialogue: 0,0:07:54.71,0:07:55.82,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:07:55.82,0:07:56.92,Default,,0000,0000,0000,,Chụp hình nhiều vào, mọi người!
Dialogue: 0,0:07:58.50,0:07:59.70,Default,,0000,0000,0000,,Mình bị phát hiện rồi.
Dialogue: 0,0:07:59.70,0:08:03.47,Default,,0000,0000,0000,,Bọn họ đang chụp hình khoảnh khắc đáng xấu hổ này!
Dialogue: 0,0:08:09.02,0:08:10.44,Default,,0000,0000,0000,,Anh kia!
Dialogue: 0,0:08:10.44,0:08:11.80,Default,,0000,0000,0000,,Anh làm gì thế hả?
Dialogue: 0,0:08:11.80,0:08:14.63,Default,,0000,0000,0000,,Tránh xa Nagisa-chan ra!
Dialogue: 0,0:08:16.65,0:08:17.42,Default,,0000,0000,0000,,Nagisa...
Dialogue: 0,0:08:17.42,0:08:18.57,Default,,0000,0000,0000,,Cô biết phải làm gì rồi chứ?
Dialogue: 0,0:08:20.79,0:08:21.79,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:08:22.23,0:08:25.96,Default,,0000,0000,0000,,Mọi người, không phải như các anh nghĩ đâu.
Dialogue: 0,0:08:26.37,0:08:27.45,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:08:27.91,0:08:31.19,Default,,0000,0000,0000,,Đây là một tư thế chụp hình.
Dialogue: 0,0:08:31.19,0:08:32.21,Default,,0000,0000,0000,,Ế!?
Dialogue: 0,0:08:32.83,0:08:40.97,Default,,0000,0000,0000,,Hãy coi đây là một sự kiện đặc biệt, mọi người có thể thoải mái chụp những bức hình "độc" của tôi.
Dialogue: 0,0:08:41.25,0:08:44.93,Default,,0000,0000,0000,,Anh ta là một diễn viên.
Dialogue: 0,0:08:44.93,0:08:47.06,Default,,0000,0000,0000,,Thật không đấy?
Dialogue: 0,0:08:49.22,0:08:55.95,Default,,0000,0000,0000,,Mọi người, tiếp tục chụp hình đi nào.
Dialogue: 0,0:08:57.38,0:08:57.92,Default,,0000,0000,0000,,Này,
Dialogue: 0,0:08:58.06,0:08:58.98,Default,,0000,0000,0000,,chúng ta nên làm gì đây?
Dialogue: 0,0:08:59.26,0:09:01.91,Default,,0000,0000,0000,,Nếu Nagisa-chan đã nói vậy...
Dialogue: 0,0:09:05.90,0:09:07.34,Default,,0000,0000,0000,,thì chắc không vấn đề gì.
Dialogue: 0,0:09:11.11,0:09:13.98,Default,,0000,0000,0000,,Mọi người đang ngắm nhìn mình.
Dialogue: 0,0:09:13.98,0:09:15.15,Default,,0000,0000,0000,,Bọn họ đang ngắm nhìn mình!
Dialogue: 0,0:09:15.81,0:09:19.03,Default,,0000,0000,0000,,Xấu hổ chết đi được!
Dialogue: 0,0:09:25.52,0:09:26.67,Default,,0000,0000,0000,,Chưa đâu.
Dialogue: 0,0:09:27.44,0:09:29.33,Default,,0000,0000,0000,,Màn đặc sắc bây giờ mới thực sự bắt đầu.
Dialogue: 0,0:09:32.87,0:09:35.55,Default,,0000,0000,0000,,Anh định nhét nó vào bên trong thật ư?
Dialogue: 0,0:09:35.55,0:09:37.67,Default,,0000,0000,0000,,Anh định làm thật ư?
Dialogue: 0,0:09:37.67,0:09:39.62,Default,,0000,0000,0000,,Này… anh đùa đấy à?
Dialogue: 0,0:09:39.62,0:09:41.57,Default,,0000,0000,0000,,Thôi kệ, lo chụp hình cái đã!
Dialogue: 0,0:09:50.74,0:09:52.56,Default,,0000,0000,0000,,Đâm sâu vào...
Dialogue: 0,0:09:53.93,0:09:58.03,Default,,0000,0000,0000,,Khít hơn hẳn mọi khi, cô hứng lên rồi hả, Nagisa?
Dialogue: 0,0:09:58.03,0:10:00.50,Default,,0000,0000,0000,,Không, không phải thế,
Dialogue: 0,0:10:00.55,0:10:04.09,Default,,0000,0000,0000,,Tôi không phóng đãng như vậy.
Dialogue: 0,0:10:04.09,0:10:06.57,Default,,0000,0000,0000,,Cô thật dâm đãng, Nagisa-chan.
Dialogue: 0,0:10:07.26,0:10:08.24,Default,,0000,0000,0000,,Quá sức khêu gợi,
Dialogue: 0,0:10:08.24,0:10:09.83,Default,,0000,0000,0000,,khêu gợi quá đi mất!
Dialogue: 0,0:10:11.04,0:10:12.26,Default,,0000,0000,0000,,Họ đang chụp hình mình.
Dialogue: 0,0:10:12.26,0:10:17.69,Default,,0000,0000,0000,,Họ đang chụp cảnh mình làm tình.
Dialogue: 0,0:10:27.05,0:10:29.30,Default,,0000,0000,0000,,Tư thế này...
Dialogue: 0,0:10:30.14,0:10:31.10,Default,,0000,0000,0000,,Xấu hổ quá đi mất!
Dialogue: 0,0:10:31.56,0:10:34.86,Default,,0000,0000,0000,,Này, cô mau nói gì với bọn họ đi chứ.
Dialogue: 0,0:10:38.90,0:10:47.24,Default,,0000,0000,0000,,Mọi người, hãy tranh thủ chớp lấy những khoảnh khắc trần trụi này của tôi.
Dialogue: 0,0:10:48.81,0:10:49.63,Default,,0000,0000,0000,,Thật kinh khủng!
Dialogue: 0,0:10:49.63,0:10:50.78,Default,,0000,0000,0000,,Hai bầu ngực...
Dialogue: 0,0:10:50.78,0:10:52.98,Default,,0000,0000,0000,,Và cả âm hộ tôi nữa...
Dialogue: 0,0:10:54.91,0:10:58.23,Default,,0000,0000,0000,,Anh muốn tôi phô ra trước mặt bọn họ bao lâu cũng được!
Dialogue: 0,0:11:03.40,0:11:05.09,Default,,0000,0000,0000,,Nói hay lắm, Nagisa.
Dialogue: 0,0:11:05.09,0:11:08.26,Default,,0000,0000,0000,,Tôi sẽ làm cô ra trước mặt bọn họ, hãy xem đó như là phần thưởng dành cho cô.
Dialogue: 0,0:11:09.47,0:11:10.76,Default,,0000,0000,0000,,{need TL}
Dialogue: 0,0:11:16.37,0:11:17.58,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:11:17.58,0:11:20.03,Default,,0000,0000,0000,,Và đây là pha cuối cùng!
Dialogue: 0,0:11:23.01,0:11:29.45,Default,,0000,0000,0000,,Mọi người, tôi sắp ra rồi!
Dialogue: 0,0:11:31.06,0:11:36.99,Default,,0000,0000,0000,,Mọi người, hãy chớp lấy những bức hình "cực khoái" của tôi!
Dialogue: 0,0:11:38.12,0:11:46.90,Default,,0000,0000,0000,,Và chụp hình khi cơ thể tôi phủ đầy tinh dịch.
Dialogue: 0,0:11:54.15,0:11:54.69,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:11:54.69,0:11:55.42,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:11:58.22,0:11:59.95,Default,,0000,0000,0000,,Tôi ra rồi...
Dialogue: 0,0:11:59.95,0:12:01.44,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:12:25.30,0:12:27.94,Default,,0000,0000,0000,,Họ vẫn đang chụp...
Dialogue: 0,0:12:28.49,0:12:33.12,Default,,0000,0000,0000,,Thân thể, bầu ngực, và cả âm hộ mình...
Dialogue: 0,0:12:33.60,0:12:36.83,Default,,0000,0000,0000,,Họ chụp hình cả rồi.
Dialogue: 0,0:12:54.41,0:12:55.07,Default,,0000,0000,0000,,Nào.
Dialogue: 0,0:12:55.44,0:12:56.56,Default,,0000,0000,0000,,Mau lên.
Dialogue: 0,0:12:57.21,0:12:59.22,Default,,0000,0000,0000,,Tôi biết rồi.
Dialogue: 0,0:13:04.39,0:13:07.10,Default,,0000,0000,0000,,Như vậy đã được chưa?
Dialogue: 0,0:13:07.87,0:13:11.21,Default,,0000,0000,0000,,Nếu cô không chà xát tử tế thì còn gọi gì là mát-xa ngực nữa.
Dialogue: 0,0:13:12.93,0:13:14.63,Default,,0000,0000,0000,,Nếu cô không muốn thì thôi đi vậy.
Dialogue: 0,0:13:14.63,0:13:18.32,Default,,0000,0000,0000,,Tôi làm, tôi làm là được chứ gì?
Dialogue: 0,0:13:27.45,0:13:28.30,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:13:28.30,0:13:29.28,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:13:30.43,0:13:33.66,Default,,0000,0000,0000,,Đừng kích thích tôi nữa.
Dialogue: 0,0:13:33.66,0:13:35.30,Default,,0000,0000,0000,,Tiếp tục đi.
Dialogue: 0,0:13:35.90,0:13:38.34,Default,,0000,0000,0000,,Nếu cô không tiếp tục, chuyện này sẽ không kết thúc đâu.
Dialogue: 0,0:13:40.03,0:13:41.25,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:13:41.25,0:13:42.33,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:13:42.33,0:13:43.84,Default,,0000,0000,0000,,Xấu hổ chết mất, nhưng mà...
Dialogue: 0,0:13:43.84,0:13:49.27,Default,,0000,0000,0000,,hai bầu ngực tôi nóng ran, còn đầu ti thì tê dại và rỉ nước.
Dialogue: 0,0:13:49.61,0:13:52.57,Default,,0000,0000,0000,,Bộ ngực cô nhạy cảm thật đó.
Dialogue: 0,0:13:53.53,0:13:56.98,Default,,0000,0000,0000,,Không, không đúng...
Dialogue: 0,0:13:56.98,0:14:01.89,Default,,0000,0000,0000,,đó là bởi vì... anh cứ chụp hình tôi hoài...
Dialogue: 0,0:14:02.95,0:14:05.42,Default,,0000,0000,0000,,Cô nói dối cũng chẳng ăn thua đâu.
Dialogue: 0,0:14:08.15,0:14:09.72,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:14:10.26,0:14:12.24,Default,,0000,0000,0000,,Hai đầu ti...
Dialogue: 0,0:14:13.24,0:14:16.28,Default,,0000,0000,0000,,Đừng chạm vào hai đầu ti tôi!
Dialogue: 0,0:14:18.21,0:14:22.94,Default,,0000,0000,0000,,Chà chà, nếu bộ ngực của cô không khiến tôi thỏa mãn, tôi sẽ đâm nó vào bên trong.
Dialogue: 0,0:14:23.94,0:14:24.89,Default,,0000,0000,0000,,Không được...
Dialogue: 0,0:14:25.23,0:14:26.14,Default,,0000,0000,0000,,Bởi vì,
Dialogue: 0,0:14:26.96,0:14:30.97,Default,,0000,0000,0000,,nếu cô không nhanh lên, buổi chụp hình sẽ bắt đầu ngay đó.
Dialogue: 0,0:14:35.48,0:14:37.31,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:14:37.31,0:14:39.52,Default,,0000,0000,0000,,Anh muốn làm gì tôi cũng được.
Dialogue: 0,0:14:40.13,0:14:43.45,Default,,0000,0000,0000,,Kể cả làm "chuyện đó".
Dialogue: 0,0:14:48.30,0:14:49.20,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:14:50.25,0:14:51.12,Default,,0000,0000,0000,,Làm ơn,
Dialogue: 0,0:14:51.46,0:14:53.05,Default,,0000,0000,0000,,đừng đùa giỡn tôi nữa mà.
Dialogue: 0,0:14:56.73,0:15:00.85,Default,,0000,0000,0000,,Trang phục của tôi sẽ dơ mất.
Dialogue: 0,0:15:03.64,0:15:05.80,Default,,0000,0000,0000,,Cô ướt đẫm rồi nè.
Dialogue: 0,0:15:06.51,0:15:08.58,Default,,0000,0000,0000,,Đúng là dâm nữ...
Dialogue: 0,0:15:09.40,0:15:10.30,Default,,0000,0000,0000,,Nanami!
Dialogue: 0,0:15:20.90,0:15:24.22,Default,,0000,0000,0000,,Đừng bảo là cô đã ra rồi nhé?
Dialogue: 0,0:15:26.16,0:15:28.60,Default,,0000,0000,0000,,Còn lâu...
Dialogue: 0,0:15:34.46,0:15:36.03,Default,,0000,0000,0000,,Đừng hòng gạt tôi.
Dialogue: 0,0:15:36.81,0:15:38.00,Default,,0000,0000,0000,,Cô ra rồi phải không?
Dialogue: 0,0:15:39.53,0:15:44.50,Default,,0000,0000,0000,,Tại vì... anh đâm vào đột ngột quá...
Dialogue: 0,0:15:46.48,0:15:50.60,Default,,0000,0000,0000,,Nanami, cô dâm thật đó, tôi chỉ mới đâm vào thôi mà đã lên đỉnh rồi.
Dialogue: 0,0:15:50.60,0:15:52.25,Default,,0000,0000,0000,,Đừng nói nữa!
Dialogue: 0,0:15:53.25,0:15:57.07,Default,,0000,0000,0000,,Tôi không hề biến thái hay đồi trụy!
Dialogue: 0,0:15:57.07,0:16:01.41,Default,,0000,0000,0000,,Anh chính là người có lỗi trong chuyện này!
Dialogue: 0,0:16:04.12,0:16:04.96,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:16:04.96,0:16:06.04,Default,,0000,0000,0000,,Bầu ngực tôi...
Dialogue: 0,0:16:09.09,0:16:14.02,Default,,0000,0000,0000,,Tôi sẽ kết thúc chuyện này trước khi buổi chụp hình diễn ra!
Dialogue: 0,0:16:20.51,0:16:24.43,Default,,0000,0000,0000,,Tư thế này hệt như loài vật vậy...
Dialogue: 0,0:16:26.45,0:16:27.75,Default,,0000,0000,0000,,Phải bám vào gờ đá.
Dialogue: 0,0:16:30.58,0:16:31.78,Default,,0000,0000,0000,,Sướng không hả?
Dialogue: 0,0:16:32.53,0:16:35.57,Default,,0000,0000,0000,,Xấu hổ chết đi được!
Dialogue: 0,0:16:36.41,0:16:39.46,Default,,0000,0000,0000,,Làm ơn kết thúc chuyện này nhanh lên!
Dialogue: 0,0:16:44.52,0:16:46.71,Default,,0000,0000,0000,,Tôi ra rồi, Nanami!
Dialogue: 0,0:16:46.71,0:16:49.28,Default,,0000,0000,0000,,Hãy ra cùng nhau nhé?
Dialogue: 0,0:16:50.01,0:16:51.68,Default,,0000,0000,0000,,Ra cùng nhau?
Dialogue: 0,0:16:51.97,0:16:55.65,Default,,0000,0000,0000,,Nếu ra cùng tôi thì anh sẽ thỏa mãn ư?
Dialogue: 0,0:16:56.83,0:17:00.81,Default,,0000,0000,0000,,Thôi được rồi, tôi sẽ ra cùng anh.
Dialogue: 0,0:17:00.81,0:17:05.58,Default,,0000,0000,0000,,Vậy nên làm ơn tha cho tôi!
Dialogue: 0,0:17:10.41,0:17:12.38,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:17:14.41,0:17:16.90,Default,,0000,0000,0000,,Tôi sẽ ra cùng anh!
Dialogue: 0,0:17:30.83,0:17:32.04,Default,,0000,0000,0000,,Cô đúng là đồ ngốc.
Dialogue: 0,0:17:32.73,0:17:35.82,Default,,0000,0000,0000,,Làm gì có chuyện ra cùng nhau thì tôi sẽ tha cho cô.
Dialogue: 0,0:17:37.95,0:17:39.52,Default,,0000,0000,0000,,Anh thật xấu xa.
Dialogue: 0,0:17:40.32,0:17:48.33,Default,,0000,0000,0000,,Anh làm dơ bộ đồ này thì lát nữa tôi lấy gì chụp hình đây...
Dialogue: 0,0:17:49.92,0:17:51.63,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:17:52.14,0:17:56.36,Default,,0000,0000,0000,,Tôi sẽ chuẩn bị cho cô một bộ mới.
Dialogue: 0,0:17:56.36,0:17:57.34,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:18:12.58,0:18:14.71,Default,,0000,0000,0000,,Bọn họ đang ngắm nhìn mình...
Dialogue: 0,0:18:16.02,0:18:19.75,Default,,0000,0000,0000,,Bộ đồ này còn ghê hơn cả không mặc gì ấy chứ!
Dialogue: 0,0:18:23.56,0:18:25.70,Default,,0000,0000,0000,,Này Nanami-chan...
Dialogue: 0,0:18:25.70,0:18:28.50,Default,,0000,0000,0000,,Bộ đồ này tuyệt lắm!
Dialogue: 0,0:18:28.50,0:18:31.80,Default,,0000,0000,0000,,Phải, trang phục này rất táo bạo,
Dialogue: 0,0:18:32.73,0:18:35.94,Default,,0000,0000,0000,,Tôi muốn chiều lòng quý khán giả hết cỡ mà.
Dialogue: 0,0:18:35.94,0:18:37.94,Default,,0000,0000,0000,,Trông chán lắm à?
Dialogue: 0,0:18:37.94,0:18:44.47,Default,,0000,0000,0000,,Tuyệt vời! Hãy cho chúng tôi thấy nhiều tư thế táo bạo hơn nữa!
Dialogue: 0,0:18:45.06,0:18:45.67,Default,,0000,0000,0000,,Ế?
Dialogue: 0,0:18:45.67,0:18:50.71,Default,,0000,0000,0000,,Nhưng mà... bộ trang phục này...
Dialogue: 0,0:18:51.26,0:18:53.08,Default,,0000,0000,0000,,Cô vẫn hay làm thế mà!
Dialogue: 0,0:18:53.50,0:18:54.52,Default,,0000,0000,0000,,Không được sao?
Dialogue: 0,0:18:54.52,0:18:58.15,Default,,0000,0000,0000,,Ý tôi không phải vậy...
Dialogue: 0,0:19:04.92,0:19:06.00,Default,,0000,0000,0000,,Nanami-chan?
Dialogue: 0,0:19:07.84,0:19:10.40,Default,,0000,0000,0000,,Đừng lo, tôi không sao.
Dialogue: 0,0:19:10.40,0:19:15.86,Default,,0000,0000,0000,,Thôi được, hôm nay tôi sẽ đặc cách chiều lòng mọi người.
Dialogue: 0,0:19:19.49,0:19:20.83,Default,,0000,0000,0000,,Nanami-tan!
Dialogue: 0,0:19:20.83,0:19:22.45,Default,,0000,0000,0000,,Xấu hổ quá...
Dialogue: 0,0:19:22.45,0:19:23.99,Default,,0000,0000,0000,,Xấu hổ chết đi được!
Dialogue: 0,0:19:24.95,0:19:27.37,Default,,0000,0000,0000,,Đừng nhìn tôi bằng cặp mắt dâm dục đó!
Dialogue: 0,0:19:28.06,0:19:30.84,Default,,0000,0000,0000,,Mình phải kết thúc nhanh chuyện này mới được!
Dialogue: 0,0:19:32.07,0:19:36.12,Default,,0000,0000,0000,,Kẻo lại hứng lên nữa thì chết!
Dialogue: 0,0:19:50.91,0:19:56.02,Default,,0000,0000,0000,,Từ đó tới giờ, chúng tôi luôn bị gã nhiếp ảnh biến thái đó bỡn cợt.
Dialogue: 0,0:19:56.34,0:20:00.42,Default,,0000,0000,0000,,Dẫn tới xuất hiện khá nhiều bức hình khêu gợi.
Dialogue: 0,0:20:02.79,0:20:05.76,Default,,0000,0000,0000,,Bầu ngực và thậm chí là âm hộ, mọi thứ...
Dialogue: 0,0:20:06.54,0:20:11.40,Default,,0000,0000,0000,,Nhưng lạ thật, dù lúc đầu có hơi xấu hổ...
Dialogue: 0,0:20:11.76,0:20:18.41,Default,,0000,0000,0000,,chúng tôi dần cảm thấy thoải mái trước ống kính người hâm mộ.
Dialogue: 0,0:20:19.59,0:20:22.58,Default,,0000,0000,0000,,Không ai có thể cưỡng lại được.
Dialogue: 0,0:20:22.58,0:20:28.21,Default,,0000,0000,0000,,Khi về đất liền, chúng tôi vẫn muốn thực hiện những buổi chụp hình bí mật như vậy.
Dialogue: 0,0:20:29.42,0:20:32.15,Default,,0000,0000,0000,,Nếu bạn muốn, hãy đến tìm chúng tôi.
Dialogue: 0,0:20:35.70,0:20:37.07,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:20:37.07,0:20:40.26,Default,,0000,0000,0000,,Tôi ra nữa rồi!
Dialogue: 0,0:20:41.54,0:20:44.04,Default,,0000,0000,0000,,Mọi người, chụp hình tôi đi!
Dialogue: 0,0:20:44.04,0:20:45.30,Default,,0000,0000,0000,,Chụp hình lúc tôi cực khoái đi!
Dialogue: 0,0:20:45.30,0:20:47.72,Default,,0000,0000,0000,,Cơ thể dâm đãng của tôi!
Dialogue: 0,0:20:47.72,0:20:50.43,Default,,0000,0000,0000,,Chụp hình nữa đi!
Dialogue: 0,0:20:50.43,0:20:51.86,Default,,0000,0000,0000,,Làm ơn!
Dialogue: 0,0:20:51.86,0:20:53.02,Default,,0000,0000,0000,,Anh nữa!
Dialogue: 0,0:20:53.62,0:20:54.62,Default,,0000,0000,0000,,Cả anh nữa!
Dialogue: 0,0:20:54.62,0:21:01.08,Default,,0000,0000,0000,,Anh muốn bắn lên chỗ nào cũng được!
Dialogue: 0,0:21:01.08,0:21:03.19,Default,,0000,0000,0000,,Bắn hết ra đi nào!
Dialogue: 0,0:21:05.19,0:21:07.19,Default,,0000,0000,0000,,
