Dialogue: 0,0:00:04.14,0:00:10.43,Default,,0000,0000,0000,,Tôi đã lỡ tay biến gã đầy tớ Tachibana Ichiro thành quái vật xúc tu.
Dialogue: 0,0:00:11.42,0:00:13.65,Default,,0000,0000,0000,,Để giúp cậu ta hấp thu ma thuật,
Dialogue: 0,0:00:13.65,0:00:17.10,Default,,0000,0000,0000,,làm tình là phương pháp duy nhất.
Dialogue: 0,0:00:17.83,0:00:26.63,Default,,0000,0000,0000,,Nhưng sau đó Kaya Blanche thuộc liên minh phù thủy Viễn Đông đã xuất hiện.
Dialogue: 0,0:00:28.21,0:00:32.00,Default,,0000,0000,0000,,Khoan khoan... Cậu khiến Yuko-sensei mang thai rồi ư?
Dialogue: 0,0:00:32.00,0:00:34.20,Default,,0000,0000,0000,,Cậu định làm gì tôi, tên đầu đất?!
Dialogue: 0,0:00:51.15,0:00:53.00,Default,,0000,0000,0000,,Thời cơ đã chín muồi.
Dialogue: 0,0:00:55.60,0:01:01.62,Default,,0000,0000,0000,,{\bord0\fnUVN Chim Bien Nhe\c&HB9B427&\pos(369,468)}Phần 03    Cạm bẫy của Eisen Faust
Dialogue: 0,0:00:55.60,0:01:01.62,Default,,0000,0000,0000,,{\bord4.5\fs35\c&HA26ADB&\4c&H080204&\fnUVN Bay Buom Nang\pos(372,40)\3c&H080204&}Baka-sub {\c&H23DCEE&}trân trọng {\c&HA86268&}giới thiệu
Dialogue: 0,0:00:55.60,0:01:01.62,Default,,0000,0000,0000,,{\bord2.5\fs35\c&HA26ADB&\3c&HFFFFFF&\4c&H080204&\fnUVN Bay Buom Nang\pos(372,40)}Baka-sub {\bord0\c&H23DCEE&}trân trọng {\bord2.5\c&HA86268&\3c&HFFFFFF&}giới thiệu
Dialogue: 0,0:01:01.62,0:01:04.37,Default,,0000,0000,0000,,Sao lại đột ngột như vậy hả trời!
Dialogue: 0,0:01:04.37,0:01:07.54,Default,,0000,0000,0000,,Tôi khiến Yuko-sensei mang thai thật rồi!
Dialogue: 0,0:01:07.54,0:01:09.88,Default,,0000,0000,0000,,Mình vừa làm cái quái gì thế này?!
Dialogue: 0,0:01:09.88,0:01:11.89,Default,,0000,0000,0000,,Đồ não cạn!
Dialogue: 0,0:01:11.89,0:01:14.08,Default,,0000,0000,0000,,"Bắn" cho lắm vào, giờ lãnh hậu quả?!
Dialogue: 0,0:01:14.08,0:01:15.37,Default,,0000,0000,0000,,Tên dâm tặc!
Dialogue: 0,0:01:16.38,0:01:18.16,Default,,0000,0000,0000,,Cô ấy không thực sự mang thai.
Dialogue: 0,0:01:18.17,0:01:20.43,Default,,0000,0000,0000,,Kaya? Vậy là sao?
Dialogue: 0,0:01:20.43,0:01:22.69,Default,,0000,0000,0000,,Đó là ma thuật tái sinh.
Dialogue: 0,0:01:22.69,0:01:23.85,Default,,0000,0000,0000,,Cái gì cơ?!
Dialogue: 0,0:01:24.55,0:01:27.90,Default,,0000,0000,0000,,Đó là một loại mật pháp cấm kị,
Dialogue: 0,0:01:27.90,0:01:30.47,Default,,0000,0000,0000,,cần tới một người đàn ông có ma pháp di truyền làm vật chủ,
Dialogue: 0,0:01:30.47,0:01:32.20,Default,,0000,0000,0000,,và sau đó hắn sẽ hấp thu ma thuật của phù thủy
Dialogue: 0,0:01:32.20,0:01:34.52,Default,,0000,0000,0000,,để làm hồi sinh người chết.
Dialogue: 0,0:01:34.54,0:01:36.07,Default,,0000,0000,0000,,Ma thuật cấm!
Dialogue: 0,0:01:36.55,0:01:38.85,Default,,0000,0000,0000,,Kẻ phù phép không phải con người.
Dialogue: 0,0:01:38.85,0:01:43.92,Default,,0000,0000,0000,,Hắn là một con rối bằng bùn đang cố hồi sinh ma pháp sư Faust.
Dialogue: 0,0:01:43.92,0:01:46.08,Default,,0000,0000,0000,,Một trong 7 Địa yêu,
Dialogue: 0,0:01:46.48,0:01:48.12,Default,,0000,0000,0000,,Eisen Faust!
Dialogue: 0,0:01:58.09,0:02:00.89,Default,,0000,0000,0000,,Ngươi phát hiện ra rồi ư?
Dialogue: 0,0:02:05.64,0:02:07.55,Default,,0000,0000,0000,,Đây là pháp thuật gì vậy?!
Dialogue: 0,0:02:07.55,0:02:08.39,Default,,0000,0000,0000,,Không lẽ nào!
Dialogue: 0,0:02:10.04,0:02:12.42,Default,,0000,0000,0000,,Nó đang ra! Tôi sắp sinh rồi!
Dialogue: 0,0:02:24.44,0:02:26.20,Default,,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:02:26.86,0:02:28.15,Default,,0000,0000,0000,,Kết giới ma thuật?!
Dialogue: 0,0:02:28.15,0:02:29.67,Default,,0000,0000,0000,,Chúng ta không thể lại gần nó.
Dialogue: 0,0:02:58.95,0:03:01.97,Default,,0000,0000,0000,,Nếu ngươi hút mạnh như vậy...
Dialogue: 0,0:03:01.97,0:03:04.25,Default,,0000,0000,0000,,Ta sẽ ra mất!
Dialogue: 0,0:03:06.91,0:03:10.14,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:03:10.14,0:03:15.38,Default,,0000,0000,0000,,Tôi bị quái vật dày vò tra tấn đến mức phún xuất rồi!
Dialogue: 0,0:03:23.92,0:03:25.23,Default,,0000,0000,0000,,Cái gì!?
Dialogue: 0,0:03:27.85,0:03:31.89,Default,,0000,0000,0000,,Dường như nó... đang tràn vào bầu ngực tôi...
Dialogue: 0,0:03:31.89,0:03:35.35,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:03:35.71,0:03:39.17,Default,,0000,0000,0000,,Ngươi định cưỡng bức ta bằng những chiếc gai đó ư!
Dialogue: 0,0:03:45.28,0:03:50.15,Default,,0000,0000,0000,,Đừng mà... đừng đâm chiếc gai lớn đó vào trong!
Dialogue: 0,0:03:51.30,0:03:55.08,Default,,0000,0000,0000,,Nếu ngươi tiêm nó vào bên trong ngực...
Dialogue: 0,0:03:57.27,0:04:01.69,Default,,0000,0000,0000,,Sữa của tôi đang phọt ra! Tôi ra rồi!
Dialogue: 0,0:04:02.35,0:04:04.18,Default,,0000,0000,0000,,Hai bầu ngực của tôi đang phún xuất!
Dialogue: 0,0:04:06.15,0:04:12.04,Default,,0000,0000,0000,,Nó đang chà xát hai bầu ngực tôi... Cảm giác rất...
Dialogue: 0,0:04:17.50,0:04:21.59,Default,,0000,0000,0000,,Tôi ra rồi... Tôi sắp sinh rồi!
Dialogue: 0,0:04:22.53,0:04:24.78,Default,,0000,0000,0000,,Tôi ra rồi! Ra rồi!
Dialogue: 0,0:04:24.78,0:04:31.21,Default,,0000,0000,0000,,Ma thuật hắc ám đã khiến âm hộ tôi vô cùng nhạy cảm. Tôi sắp sinh rồi!
Dialogue: 0,0:04:31.71,0:04:34.21,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:04:44.97,0:04:46.40,Default,,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:04:53.52,0:04:56.66,Default,,0000,0000,0000,,Tôi phải phá hủy quả trứng đáng nguyền rủa này ngay khi còn có thể.
Dialogue: 0,0:05:00.32,0:05:02.54,Default,,0000,0000,0000,,Biến mất rồi ư?!
Dialogue: 0,0:05:02.54,0:05:05.53,Default,,0000,0000,0000,,Làm tốt lắm.
Dialogue: 0,0:05:06.50,0:05:08.53,Default,,0000,0000,0000,,Tên ma pháp sư khốn kiếp!{?}
Dialogue: 0,0:05:08.53,0:05:10.94,Default,,0000,0000,0000,,Eisen Faust!
Dialogue: 0,0:05:10.94,0:05:12.54,Default,,0000,0000,0000,,Chính là hắn ta?
Dialogue: 0,0:05:13.21,0:05:17.16,Default,,0000,0000,0000,,Vậy là xong, Tachibana Ichiro.
Dialogue: 0,0:05:17.16,0:05:19.50,Default,,0000,0000,0000,,Futaba Lily Ramses.
Dialogue: 0,0:05:19.50,0:05:21.30,Default,,0000,0000,0000,,Ta đánh giá cao sự trợ giúp của cô.
Dialogue: 0,0:05:21.96,0:05:24.05,Default,,0000,0000,0000,,Vô cùng đa tạ.
Dialogue: 0,0:05:24.05,0:05:28.34,Default,,0000,0000,0000,,Dám lừa gạt bọn ta ra nông nỗi này! Ngươi không thoát được đâu!
Dialogue: 0,0:05:28.34,0:05:30.55,Default,,0000,0000,0000,,Xác định đi, Eisen Faust!
Dialogue: 0,0:05:31.50,0:05:36.56,Default,,0000,0000,0000,,Rất có chí khí. Nhưng mà ta không rảnh chơi đùa với cô.
Dialogue: 0,0:05:39.95,0:05:41.56,Default,,0000,0000,0000,,Đứng lại!
Dialogue: 0,0:05:41.56,0:05:44.48,Default,,0000,0000,0000,,Hắn đứng đằng sau tất cả mọi chuyện!
Dialogue: 0,0:05:46.25,0:05:47.32,Default,,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:05:49.70,0:05:51.37,Default,,0000,0000,0000,,Sư phụ!
Dialogue: 0,0:05:51.37,0:05:53.09,Default,,0000,0000,0000,,Sư phụ?
Dialogue: 0,0:06:03.48,0:06:05.17,Default,,0000,0000,0000,,Đừng lại gần sư phụ của ta!
Dialogue: 0,0:06:06.78,0:06:08.18,Default,,0000,0000,0000,,Tên yêu quái đê tiện!
Dialogue: 0,0:06:18.38,0:06:20.63,Default,,0000,0000,0000,,Cô ấy vẫn chưa tỉnh lại sao?
Dialogue: 0,0:06:25.16,0:06:28.47,Default,,0000,0000,0000,,Kaya-chan, cô và Yuko-sensei quen biết nhau ư?
Dialogue: 0,0:06:28.96,0:06:33.49,Default,,0000,0000,0000,,Xin sư phụ tha thứ vì tôi sẽ làm trái ý người.
Dialogue: 0,0:06:33.49,0:06:34.19,Default,,0000,0000,0000,,Kaya-chan?
Dialogue: 0,0:06:36.22,0:06:39.01,Default,,0000,0000,0000,,Có chuyện gì vậy?
Dialogue: 0,0:06:39.63,0:06:46.21,Default,,0000,0000,0000,,Chỉ có một cách duy nhất để cứu được sư phụ và ngăn chặn âm mưu của Eisen Faust.
Dialogue: 0,0:06:49.03,0:06:51.72,Default,,0000,0000,0000,,Thế cách đó là...
Dialogue: 0,0:06:51.72,0:06:55.22,Default,,0000,0000,0000,,Sẽ không đau đâu. Chỉ một nhát là xong.
Dialogue: 0,0:06:59.10,0:07:01.01,Default,,0000,0000,0000,,Cô đùa kiểu gì kì vậy!?
Dialogue: 0,0:07:01.01,0:07:01.99,Default,,0000,0000,0000,,Cái này chết người đấy!
Dialogue: 0,0:07:12.18,0:07:13.24,Default,,0000,0000,0000,,Quân giết người!
Dialogue: 0,0:07:13.24,0:07:14.24,Default,,0000,0000,0000,,Cô ta định khử tôi!
Dialogue: 0,0:07:20.80,0:07:22.33,Default,,0000,0000,0000,,Ngừng tại đây thôi.
Dialogue: 0,0:07:24.85,0:07:27.85,Default,,0000,0000,0000,,Nếu tôi kết liễu vật chủ tàng trữ ma pháp hồi sinh,
Dialogue: 0,0:07:27.85,0:07:29.43,Default,,0000,0000,0000,,mọi chuyện sẽ được giải quyết.
Dialogue: 0,0:07:29.43,0:07:31.76,Default,,0000,0000,0000,,Tôi có thể cam đoan với cô...
Dialogue: 0,0:07:31.76,0:07:33.99,Default,,0000,0000,0000,,Bởi vì tôi vẫn chưa muốn chết mà!
Dialogue: 0,0:07:33.99,0:07:36.47,Default,,0000,0000,0000,,Loài yêu quái ti tiện không đáng được cảm thông.
Dialogue: 0,0:07:36.85,0:07:39.67,Default,,0000,0000,0000,,Chết đi, Tachibana Ichiro!
Dialogue: 0,0:07:47.65,0:07:48.39,Default,,0000,0000,0000,,Lily!
Dialogue: 0,0:07:48.39,0:07:49.81,Default,,0000,0000,0000,,Cậu tới để cứu tôi sao?
Dialogue: 0,0:07:49.81,0:07:51.97,Default,,0000,0000,0000,,Cậu đang làm gì vậy, đồ đần!
Dialogue: 0,0:07:52.50,0:07:53.70,Default,,0000,0000,0000,,Cô định ngăn cản tôi sao?
Dialogue: 0,0:07:54.25,0:08:00.45,Default,,0000,0000,0000,,Tôi là Futaba Lily Ramses, đồ đệ của phù thủy Thái dương Yuko-sensei!
Dialogue: 0,0:08:01.47,0:08:02.69,Default,,0000,0000,0000,,Từ khi nào vậy?
Dialogue: 0,0:08:02.69,0:08:04.20,Default,,0000,0000,0000,,Câm đi đồ ngu!
Dialogue: 0,0:08:04.70,0:08:09.51,Default,,0000,0000,0000,,Một con nhóc như cô cũng xứng làm đồ đệ của phù thủy Thái dương ư?
Dialogue: 0,0:08:09.51,0:08:10.91,Default,,0000,0000,0000,,Thật nực cười.
Dialogue: 0,0:08:10.91,0:08:12.80,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:08:12.80,0:08:17.16,Default,,0000,0000,0000,,Nếu vẫn cố tình ngăn cản tôi, cô sẽ không chỉ lãnh một vết cào "nhẹ nhàng" như vậy đâu!
Dialogue: 0,0:08:19.25,0:08:20.93,Default,,0000,0000,0000,,Lily, cậu chảy máu kìa!
Dialogue: 0,0:08:20.93,0:08:25.03,Default,,0000,0000,0000,,Không cần cậu lo. Tránh qua một bên đi, đồ đần.
Dialogue: 0,0:08:25.03,0:08:27.10,Default,,0000,0000,0000,,Mau tránh đường, nhóc con.
Dialogue: 0,0:08:27.10,0:08:30.94,Default,,0000,0000,0000,,Cô thì lớn hơn ai? Đừng coi thường người khác như vậy, biết chưa hả?!
Dialogue: 0,0:08:31.61,0:08:32.97,Default,,0000,0000,0000,,Thật thú vị.
Dialogue: 0,0:08:32.97,0:08:35.53,Default,,0000,0000,0000,,Thay vì nói khoác, hãy dùng thực lực để chứng minh,
Dialogue: 0,0:08:35.53,0:08:38.47,Default,,0000,0000,0000,,nếu cô muốn làm đồ đệ của cô ấy!
Dialogue: 0,0:08:38.47,0:08:41.45,Default,,0000,0000,0000,,Tôi không hề nói khoác!
Dialogue: 0,0:08:47.71,0:08:52.65,Default,,0000,0000,0000,,Tôi sẽ không tha cho bất kì kẻ nào dám bôi nhọ danh tính của phù thủy Thái dương!
Dialogue: 0,0:09:02.22,0:09:05.23,Default,,0000,0000,0000,,Không tệ chút nào, Kaya Blanche.
Dialogue: 0,0:09:06.73,0:09:07.73,Default,,0000,0000,0000,,Hai người mau ngừng tay.
Dialogue: 0,0:09:16.85,0:09:17.70,Default,,0000,0000,0000,,Sư phụ!
Dialogue: 0,0:09:21.20,0:09:25.71,Default,,0000,0000,0000,,Kaya, Ichiro có ý nghĩa rất quan trọng đối với tôi.
Dialogue: 0,0:09:26.22,0:09:33.21,Default,,0000,0000,0000,,Dù cô là tuần sát sứ còn cậu ấy là yêu nhân, cũng đừng vội vàng xuống tay!
Dialogue: 0,0:09:33.21,0:09:37.97,Default,,0000,0000,0000,,Tôi sẽ gánh toàn bộ trách nhiệm. Làm ơn đi, Kaya!
Dialogue: 0,0:09:44.20,0:09:50.73,Default,,0000,0000,0000,,Cô ta là ai? Sao cứ luôn mồm gọi cô là Sư phụ như vậy?
Dialogue: 0,0:09:51.90,0:09:55.56,Default,,0000,0000,0000,,Mẹ của Kaya là bạn thân nhất của cô.
Dialogue: 0,0:09:56.60,0:10:04.27,Default,,0000,0000,0000,,Vào thời khắc đối diện với kết cục bi thảm, cô ấy đã gửi gắm con gái mình cho cô.
Dialogue: 0,0:10:05.42,0:10:09.31,Default,,0000,0000,0000,,Vậy nên Kaya vẫn luôn luôn coi cô là sư phụ.
Dialogue: 0,0:10:09.31,0:10:13.00,Default,,0000,0000,0000,,Vậy tại sao cô ấy lại cố giết em?
Dialogue: 0,0:10:13.63,0:10:21.14,Default,,0000,0000,0000,,Do quá lo lắng mà thôi, cô đã sa vào bẫy của Eisen và mất đi phần lớn ma thuật của mình.
Dialogue: 0,0:10:21.14,0:10:23.89,Default,,0000,0000,0000,,Bởi vậy hai em đừng trách cô ấy.
Dialogue: 0,0:10:25.93,0:10:30.27,Default,,0000,0000,0000,,Dù cô đòi hỏi hơi quá mức, nhưng làm ơn hãy giúp cô ấy.
Dialogue: 0,0:10:31.37,0:10:32.03,Default,,0000,0000,0000,,Làm ơn đi!
Dialogue: 0,0:10:33.15,0:10:34.23,Default,,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:10:45.87,0:10:49.52,Default,,0000,0000,0000,,Bọn em sẽ lo chuyện này. Cô cứ nghỉ ngơi đi.
Dialogue: 0,0:10:58.75,0:11:00.85,Default,,0000,0000,0000,,Chúng ta phải đuổi theo Eisen ngay.
Dialogue: 0,0:11:00.85,0:11:03.71,Default,,0000,0000,0000,,Khoan đã! Còn vết thương thì sao?
Dialogue: 0,0:11:03.71,0:11:05.54,Default,,0000,0000,0000,,Cái này thì nhằm nhò gì!
Dialogue: 0,0:11:05.54,0:11:07.05,Default,,0000,0000,0000,,Phải để tôi khám qua đã!
Dialogue: 0,0:11:07.53,0:11:08.25,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:11:10.39,0:11:11.69,Default,,0000,0000,0000,,Cậu vẫn đang chảy máu nè!
Dialogue: 0,0:11:11.69,0:11:13.73,Default,,0000,0000,0000,,Không nhằm nhò gì đâu mà!
Dialogue: 0,0:11:16.05,0:11:18.40,Default,,0000,0000,0000,,Cám ơn... cậu đã trợ giúp tôi những lúc nguy nan.
Dialogue: 0,0:11:23.32,0:11:26.50,Default,,0000,0000,0000,,Tôi phải kiểm tra xem cậu còn bị thương chỗ nào không.
Dialogue: 0,0:11:27.47,0:11:30.77,Default,,0000,0000,0000,,Được, xem ra cậu còn có lương tâm.
Dialogue: 0,0:11:31.90,0:11:34.00,Default,,0000,0000,0000,,Hai đầu ti cậu cứng lên rồi nè.
Dialogue: 0,0:11:35.40,0:11:41.51,Default,,0000,0000,0000,,Nếu không phải cậu chạm vào... Nếu không phải cậu đòi khám bệnh, thì chúng đâu có như vậy!
Dialogue: 0,0:11:45.30,0:11:50.10,Default,,0000,0000,0000,,Cậu định khám cả chỗ này sao? Đồ dê cụ.
Dialogue: 0,0:11:54.03,0:11:57.10,Default,,0000,0000,0000,,Tuyệt quá, sao mình lại ướt đẫm như vậy?
Dialogue: 0,0:11:58.45,0:12:00.21,Default,,0000,0000,0000,,Ngón tay của cậu chọc vào thật thoải mái.
Dialogue: 0,0:12:01.01,0:12:02.99,Default,,0000,0000,0000,,Dâm thủy của tôi đang trào ra!
Dialogue: 0,0:12:03.72,0:12:09.74,Default,,0000,0000,0000,,Cậu đang đùa giỡn âm hộ tôi... ngón tay dâm đãng của cậu sắp khiến tôi ra!
Dialogue: 0,0:12:09.74,0:12:12.75,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:12:14.86,0:12:16.00,Default,,0000,0000,0000,,Tôi ra rồi...
Dialogue: 0,0:12:16.00,0:12:19.17,Default,,0000,0000,0000,,Đều do ngón tay của cậu...
Dialogue: 0,0:12:26.39,0:12:30.47,Default,,0000,0000,0000,,Vậy ra đây là hương vị của dâm thủy sao...
Dialogue: 0,0:12:31.00,0:12:35.48,Default,,0000,0000,0000,,Có lẽ tôi sẽ dùng tới các "dụng cụ" cần thiết để kiểm tra mới được.
Dialogue: 0,0:12:35.48,0:12:37.91,Default,,0000,0000,0000,,Cậu chuẩn bị sẵn sàng đi nhé.
Dialogue: 0,0:12:40.11,0:12:42.32,Default,,0000,0000,0000,,Tôi có sự lựa chọn nào khác sao?
Dialogue: 0,0:12:50.10,0:12:54.58,Default,,0000,0000,0000,,Sẵn sàng rồi hả? Tôi đưa "dụng cụ" vào... sâu bên trong nhé?
Dialogue: 0,0:12:57.10,0:13:00.96,Default,,0000,0000,0000,,Muốn sâu... cỡ nào cũng được...
Dialogue: 0,0:13:06.79,0:13:12.22,Default,,0000,0000,0000,,Nó đâm vào bên trong rồi! Đúng là dương vật luôn hơn xa ngón tay!
Dialogue: 0,0:13:12.22,0:13:15.74,Default,,0000,0000,0000,,Tuyệt quá! Thằng nhỏ của cậu đang lấp đầy bên trong tôi!
Dialogue: 0,0:13:16.20,0:13:20.08,Default,,0000,0000,0000,,Kiểm tra... Kiểm tra tôi sâu hơn nữa đi!
Dialogue: 0,0:13:30.33,0:13:33.52,Default,,0000,0000,0000,,Tuyệt ghê! Cậu ra nhiều chưa kìa.
Dialogue: 0,0:13:33.52,0:13:38.72,Default,,0000,0000,0000,,Nhiều quá xá! Cứ như là âm hộ tôi tự phún xuất vậy.
Dialogue: 0,0:13:45.36,0:13:48.39,Default,,0000,0000,0000,,Cậu định khám cả chỗ này sao?
Dialogue: 0,0:13:49.72,0:13:53.59,Default,,0000,0000,0000,,Tất nhiên rồi. Tôi phải khám toàn thân.
Dialogue: 0,0:14:04.15,0:14:08.11,Default,,0000,0000,0000,,Hậu môn?! Tay chọc hậu môn, còn trym đâm âm hộ luôn à?!
Dialogue: 0,0:14:08.63,0:14:12.12,Default,,0000,0000,0000,,Nhiều quá! Âm hộ của tôi không chứa nổi nữa rồi!
Dialogue: 0,0:14:14.26,0:14:16.12,Default,,0000,0000,0000,,Liếm sạch đi nào.
Dialogue: 0,0:14:27.70,0:14:34.91,Default,,0000,0000,0000,,Tôi đang tè ra... đừng nhìn chằm chằm như thế, đồ ngốc...
Dialogue: 0,0:14:38.59,0:14:41.65,Default,,0000,0000,0000,,Chúng ta khám tiếp nào.
Dialogue: 0,0:15:19.65,0:15:22.07,Default,,0000,0000,0000,,Cậu có chắc là cái la bàn này dẫn đúng đường không?
Dialogue: 0,0:15:22.07,0:15:25.56,Default,,0000,0000,0000,,Hả? Cậu không tin bảo bối của Yuko-sensei sao?
Dialogue: 0,0:15:25.56,0:15:30.25,Default,,0000,0000,0000,,Không phải thế! Nhưng khu này bỏ hoang mà?
Dialogue: 0,0:15:30.25,0:15:32.82,Default,,0000,0000,0000,,Cậu sợ chứ gì?
Dialogue: 0,0:15:32.82,0:15:35.83,Default,,0000,0000,0000,,Sợ... sợ... sợ cái quái gì!
Dialogue: 0,0:15:38.70,0:15:41.37,Default,,0000,0000,0000,,Đó là sủng vật của Eisen!
Dialogue: 0,0:15:44.70,0:15:47.96,Default,,0000,0000,0000,,Đúng là nơi này rồi! Đi nào, Ichiro!
Dialogue: 0,0:15:47.96,0:15:49.19,Default,,0000,0000,0000,,Này, khoan đã...
Dialogue: 0,0:15:53.86,0:15:55.65,Default,,0000,0000,0000,,Hẳn là ở dưới này.
Dialogue: 0,0:16:10.03,0:16:10.85,Default,,0000,0000,0000,,Đến nơi rồi.
Dialogue: 0,0:16:17.87,0:16:21.37,Default,,0000,0000,0000,,Chào mừng đến lâu đài của ta.
Dialogue: 0,0:16:21.37,0:16:23.07,Default,,0000,0000,0000,,Rất hân hạnh được đón tiếp quý khách.
Dialogue: 0,0:16:23.47,0:16:28.09,Default,,0000,0000,0000,,Khách quý đến chơi mà ngươi không pha trà mời khách sao?
Dialogue: 0,0:16:28.89,0:16:33.74,Default,,0000,0000,0000,,Xin thứ lỗi. Hiện ta đang tiếp một vị khách quý khác.
Dialogue: 0,0:16:42.14,0:16:43.66,Default,,0000,0000,0000,,Kaya-chan!
Dialogue: 0,0:16:43.66,0:16:47.86,Default,,0000,0000,0000,,Ái chà, cô bị Eisen tóm rồi à? Gà quá cơ.
Dialogue: 0,0:16:53.83,0:16:54.60,Default,,0000,0000,0000,,Đừng có nhìn tôi!
Dialogue: 0,0:16:55.11,0:16:58.15,Default,,0000,0000,0000,,Eisen Faust, ngươi không lầm đấy chứ?
Dialogue: 0,0:16:58.15,0:17:01.25,Default,,0000,0000,0000,,Cô ta đang âm mưu chiếm đoạt "tài sản" của ta.
Dialogue: 0,0:17:01.25,0:17:03.83,Default,,0000,0000,0000,,Tài sản... Khoan đã, cậu ám chỉ tôi hả?
Dialogue: 0,0:17:03.83,0:17:06.58,Default,,0000,0000,0000,,Xin lỗi tình yêu, con tin của ngươi vô giá trị.
Dialogue: 0,0:17:07.21,0:17:11.17,Default,,0000,0000,0000,,Vậy là cô mặc kệ chuyện gì sẽ xảy đến với cô ta phải không?
Dialogue: 0,0:17:17.42,0:17:18.50,Default,,0000,0000,0000,,Kaya-chan!
Dialogue: 0,0:17:19.53,0:17:21.75,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:17:21.75,0:17:24.24,Default,,0000,0000,0000,,Ta chỉ tiêm cho cô ta một ít xuân dược thôi.
Dialogue: 0,0:17:25.10,0:17:26.52,Default,,0000,0000,0000,,Xuân dược?
Dialogue: 0,0:17:29.64,0:17:35.26,Default,,0000,0000,0000,,Nhớ không, lúc còn nhỏ, ta đã bắt ngươi làm con tin và cưỡng bức mẹ ngươi!
Dialogue: 0,0:17:37.55,0:17:39.15,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:17:40.56,0:17:42.02,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:17:42.98,0:17:47.55,Default,,0000,0000,0000,,Sau khi xuân dược ngấm, mẹ của ngươi đã phát cuồng với hai bầu ngực đó.
Dialogue: 0,0:17:48.82,0:17:50.31,Default,,0000,0000,0000,,Ngay trước mắt ngươi.
Dialogue: 0,0:17:50.70,0:17:52.25,Default,,0000,0000,0000,,Thật kinh khủng...
Dialogue: 0,0:17:53.22,0:17:55.37,Default,,0000,0000,0000,,Dừng lại... Dừng lại....
Dialogue: 0,0:17:55.37,0:18:02.06,Default,,0000,0000,0000,,Một phù thủy danh giá đã biến thành con vật dâm đãng dưới tay ta!
Dialogue: 0,0:18:04.40,0:18:05.64,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:18:08.75,0:18:12.61,Default,,0000,0000,0000,,Ta sẽ giết ngươi! Ta sẽ giết ngươi!
Dialogue: 0,0:18:13.65,0:18:22.12,Default,,0000,0000,0000,,Giờ cô nhóc năm xưa đã khôn lớn và quay về báo thù cho mẹ, nhưng lại trở thành vật thí nghiệm của ta!
Dialogue: 0,0:18:25.51,0:18:27.58,Default,,0000,0000,0000,,Tên yêu quái khốn kiếp!
Dialogue: 0,0:18:28.03,0:18:31.73,Default,,0000,0000,0000,,Hiểu rồi! Bởi vậy nên cô ta mới ghét yêu nhân như vậy!
Dialogue: 0,0:18:31.73,0:18:33.16,Default,,0000,0000,0000,,Bỉ ổi...
Dialogue: 0,0:18:33.90,0:18:36.84,Default,,0000,0000,0000,,Ngươi thật bỉ ổi, Eisen Faust!
Dialogue: 0,0:18:37.36,0:18:39.63,Default,,0000,0000,0000,,Thì sao nào?
Dialogue: 0,0:18:40.30,0:18:42.35,Default,,0000,0000,0000,,Ta sẽ tiêu diệt ngươi!
Dialogue: 0,0:18:42.68,0:18:43.84,Default,,0000,0000,0000,,Vô ích thôi.
Dialogue: 0,0:18:48.59,0:18:51.79,Default,,0000,0000,0000,,Này, cậu làm gì vậy, đồ đần?!
Dialogue: 0,0:18:51.79,0:18:54.66,Default,,0000,0000,0000,,Tôi không biết! Cơ thể tôi mất tự chủ rồi!
Dialogue: 0,0:18:54.66,0:18:56.21,Default,,0000,0000,0000,,Cậu nói cái gì?!
Dialogue: 0,0:19:02.09,0:19:04.34,Default,,0000,0000,0000,,Giờ thì cô khỏi phá phách nữa nhé.
Dialogue: 0,0:19:04.34,0:19:07.93,Default,,0000,0000,0000,,Mọi thứ đã chuẩn bị sẵn sàng, chàng trai.
Dialogue: 0,0:19:07.93,0:19:09.62,Default,,0000,0000,0000,,Cưỡng bức cô ta đi.
Dialogue: 0,0:19:09.62,0:19:11.45,Default,,0000,0000,0000,,Kaya-chan?!
Dialogue: 0,0:19:12.64,0:19:16.67,Default,,0000,0000,0000,,Hoang đường! Tại sao ta phải nghe lời ngươi?!
Dialogue: 0,0:19:18.16,0:19:24.68,Default,,0000,0000,0000,,Cậu chắc chứ? Nếu chậm trễ, xuân dược sẽ làm cô ta phát điên.
Dialogue: 0,0:19:26.76,0:19:32.19,Default,,0000,0000,0000,,Cưỡng bức tôi đi! Tôi hết chịu nổi rồi! Làm ơn hãy cưỡng bức tôi đi!{(c) nirvana}
Dialogue: 0,0:19:33.37,0:19:34.15,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:19:34.85,0:19:39.15,Default,,0000,0000,0000,,Tôi chưa muốn chết! Xem như là cậu cứu mạng tôi đi!
Dialogue: 0,0:19:40.15,0:19:41.15,Default,,0000,0000,0000,,Đừng bận tâm về chuyện đó.
Dialogue: 0,0:19:42.45,0:19:43.43,Default,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:19:45.30,0:19:48.91,Default,,0000,0000,0000,,Được thôi! Nhưng lần sau sẽ không qua loa như vậy đâu!
Dialogue: 0,0:19:48.91,0:19:50.94,Default,,0000,0000,0000,,Thật chả sung sướng gì khi vui vẻ trước mặt gã này!
Dialogue: 0,0:19:52.33,0:19:55.99,Default,,0000,0000,0000,,Cậu thật xấu xa, Tachibana Ichiro.
Dialogue: 0,0:19:55.99,0:19:58.59,Default,,0000,0000,0000,,Cậu muốn làm gì tôi cũng được...
Dialogue: 0,0:19:59.50,0:20:03.15,Default,,0000,0000,0000,,Nhưng đây là lần đầu của tôi...
Dialogue: 0,0:20:04.36,0:20:06.68,Default,,0000,0000,0000,,Đừng bận tâm.
Dialogue: 0,0:20:06.68,0:20:09.94,Default,,0000,0000,0000,,Xin lỗi, tôi hết chịu nổi rồi!
Dialogue: 0,0:20:21.92,0:20:24.01,Default,,0000,0000,0000,,Dừng lại, Ichiro!
Dialogue: 0,0:20:29.03,0:20:34.91,Default,,0000,0000,0000,,Đừng mà... Đừng chà xát nhiều chỗ cùng lúc như vậy!
Dialogue: 0,0:20:34.91,0:20:39.75,Default,,0000,0000,0000,,Tôi sẽ ra mất... mạnh quá... Cơ thể tôi dường sư hóa thành một âm hộ khổng lồ vậy!
Dialogue: 0,0:20:39.75,0:20:41.75,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:20:43.50,0:20:46.77,Default,,0000,0000,0000,,Ngừng lại... Tôi không thể ngừng được!
Dialogue: 0,0:20:46.77,0:20:49.98,Default,,0000,0000,0000,,Tôi không chịu nổi nữa rồi!
Dialogue: 0,0:20:52.48,0:20:54.61,Default,,0000,0000,0000,,Đừng... Đừng nhìn mà...
Dialogue: 0,0:20:54.61,0:20:59.48,Default,,0000,0000,0000,,Đừng nhìn chằm chằm vào âm hộ ướt đẫm của tôi!
Dialogue: 0,0:21:10.02,0:21:14.65,Default,,0000,0000,0000,,Tôi biết mà... Dù không có kinh nghiệm, tôi vẫn biết...
Dialogue: 0,0:21:14.65,0:21:17.24,Default,,0000,0000,0000,,Đó là dương...
Dialogue: 0,0:21:19.54,0:21:21.92,Default,,0000,0000,0000,,Nó bự quá!
Dialogue: 0,0:21:23.53,0:21:25.89,Default,,0000,0000,0000,,Lần đầu tiên nó đâm vào bên trong tôi!
Dialogue: 0,0:21:25.89,0:21:28.81,Default,,0000,0000,0000,,Nó đang chà xát bên trong!
Dialogue: 0,0:21:28.81,0:21:33.02,Default,,0000,0000,0000,,Lần đầu của tôi... Dương vật của yêu quái xúc tu đang chà xát bên trong!
Dialogue: 0,0:21:33.02,0:21:37.29,Default,,0000,0000,0000,,Tôi đã mất trinh tiết vào tay quái vật xúc tu!
Dialogue: 0,0:21:37.95,0:21:43.53,Default,,0000,0000,0000,,Nếu cứ như vậy, tôi sẽ biến thành nô lệ của cậu đó!
Dialogue: 0,0:21:43.55,0:21:46.99,Default,,0000,0000,0000,,Tôi không thể ngừng lại! Tôi muốn chiếm được cô!
Dialogue: 0,0:21:47.62,0:21:51.75,Default,,0000,0000,0000,,Chính tôi yêu cầu điều đó mà. Vậy nên cậu muốn sao cũng được!
Dialogue: 0,0:21:51.75,0:21:55.79,Default,,0000,0000,0000,,Đâm nó vào sâu hơn nữa đi,
Dialogue: 0,0:21:55.79,0:22:02.25,Default,,0000,0000,0000,,khiến cho âm hộ của tôi biến dạng vĩnh viễn đi!
Dialogue: 0,0:22:10.43,0:22:17.27,Default,,0000,0000,0000,,Mau kết thúc chuyện này... Tôi sẽ trở thành nô lệ của cậu!
Dialogue: 0,0:22:24.38,0:22:29.09,Default,,0000,0000,0000,,Ra rồi! Tinh dịch của cậu đang lấp đầy tử cung tôi!
Dialogue: 0,0:22:30.20,0:22:34.47,Default,,0000,0000,0000,,Tuyệt quá... Tôi ra rồi!
Dialogue: 0,0:22:51.59,0:22:56.60,Default,,0000,0000,0000,,Dừng lại! Cậu không nghe tôi nói gì sao?
Dialogue: 0,0:22:57.58,0:23:00.56,Default,,0000,0000,0000,,Kaya. Thè lưỡi ra nào.
Dialogue: 0,0:23:10.89,0:23:13.25,Default,,0000,0000,0000,,Đây chính là điều mà Eisen mong muốn...
Dialogue: 0,0:23:14.35,0:23:16.79,Default,,0000,0000,0000,,Không thể cứu Yuko-sensei bằng cách này được!
Dialogue: 0,0:23:17.45,0:23:22.23,Default,,0000,0000,0000,,Tuyệt quá... nước miếng của cậu thật tuyệt vời...
Dialogue: 0,0:23:22.23,0:23:26.88,Default,,0000,0000,0000,,Tâm trí trống rỗng... Tôi không thể sống thiếu cậu!
Dialogue: 0,0:23:28.60,0:23:32.57,Default,,0000,0000,0000,,Nữa đi... Tôi muốn nữa...
Dialogue: 0,0:23:32.57,0:23:36.45,Default,,0000,0000,0000,,Hãy đưa tôi lên thiên đàng đi...
Dialogue: 0,0:25:22.75,0:25:25.55,Default,,0000,0000,0000,,Khoan đã! Ichiro, cậu có bị thần kinh không vậy?!
Dialogue: 0,0:25:25.55,0:25:27.02,Default,,0000,0000,0000,,Phần sau chính là tập cuối!
Dialogue: 0,0:25:27.02,0:25:28.75,Default,,0000,0000,0000,,Phàm nhân to gan!
Dialogue: 0,0:25:29.93,0:25:31.25,Default,,0000,0000,0000,,Có vẻ như nó là thật...
Dialogue: 0,0:25:31.25,0:25:33.03,Default,,0000,0000,0000,,Thật sao?
Dialogue: 0,0:25:33.03,0:25:37.48,Default,,0000,0000,0000,,Ồ Ichiro, nó lại bự lên nữa kìa
Dialogue: 0,0:25:37.48,0:25:39.48,Default,,0000,0000,0000,,Đó là phản ứng sinh lý của đàn ông!{lit physiological phenomenon}
Dialogue: 0,0:25:39.48,0:25:42.00,Default,,0000,0000,0000,,Cậu muốn gì nào, đồ dê cụ!
Dialogue: 0,0:25:42.00,0:25:45.32,Default,,0000,0000,0000,,Vì đây là lần chót, vậy nên bọn tôi sẽ hấp khô cậu!
Dialogue: 0,0:25:45.32,0:25:46.77,Default,,0000,0000,0000,,Thôi thì đành vậy...
Dialogue: 0,0:25:46.77,0:25:48.72,Default,,0000,0000,0000,,Cám ơn mọi người!
Dialogue: 0,0:25:48.72,0:25:52.06,Default,,0000,0000,0000,,Tachibana Ichiro, đêm nay sẽ rất thú vị đấy!
Dialogue: 0,0:25:52.92,0:25:54.10,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:25:55.24,0:25:56.18,Default,,0000,0000,0000,,Xác định đi!
Dialogue: 0,0:25:56.94,0:25:59.27,Default,,0000,0000,0000,,Đây chính là kết cục của cậu!
Dialogue: 0,0:26:00.31,0:26:03.05,Default,,0000,0000,0000,,Đừng bỏ lỡ phần cuối!
