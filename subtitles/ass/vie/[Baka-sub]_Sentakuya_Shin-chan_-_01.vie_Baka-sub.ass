Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:20.80,0:00:23.76,Default,,0000,0000,0020,,Tôi vừa lỡ tay làm đổ nước lên một tấm thảm lớn...
Dialogue: 0,0:00:24.00,0:00:25.60,Default,,0000,0000,0020,,Liệu nó có loang ra và làm hư cả tấm thảm ko?
Dialogue: 0,0:00:25.73,0:00:28.03,Default,,0000,0000,0020,,Tôi cần xem qua vết bẩn đó trước đã...
Dialogue: 0,0:00:28.50,0:00:29.76,Default,,0000,0000,0020,,Xin phép cô.
Dialogue: 0,0:00:33.60,0:00:36.43,Default,,0000,0000,0020,,Um... Vết bẩn nằm ở đâu nhỉ?
Dialogue: 0,0:00:36.83,0:00:38.63,Default,,0000,0000,0020,,Tôi vẫn chưa tìm thấy...
Dialogue: 0,0:00:39.20,0:00:40.43,Default,,0000,0000,0020,,Ở đây nè.
Dialogue: 0,0:00:40.86,0:00:41.83,Default,,0000,0000,0020,,Hả?
Dialogue: 0,0:00:49.89,0:00:54.03,Default,,0000,0000,0020,,Liệu "nước" của tôi có loang rộng ra nữa ko nhỉ?
Dialogue: 0,0:00:54.73,0:00:56.46,Default,,0000,0000,0020,,Đây là...
Dialogue: 0,0:00:57.06,0:00:58.53,Default,,0000,0000,0020,,Anh làm sạch nó được chứ?
Dialogue: 0,0:00:58.73,0:01:00.60,Default,,0000,0000,0020,,Ko vấn đề gì.
Dialogue: 0,0:01:00.79,0:01:02.86,Default,,0000,0000,0020,,Cô đang yêu cầu dịch vụ chăm sóc đặc biệt của tiệm giặt ủi phải ko ạ?
Dialogue: 0,0:01:06.06,0:01:07.46,Default,,0000,0000,0020,,Trông cậy cả vào anh đấy.
Dialogue: 0,0:01:07.59,0:01:08.73,Default,,0000,0000,0020,,Vậy thì...
Dialogue: 0,0:01:15.00,0:01:16.76,Default,,0000,0000,0020,,Anh làm tuyệt quá.
Dialogue: 0,0:01:16.90,0:01:19.73,Default,,0000,0000,0020,,Có vẻ như vết bẩn vẫn đang tiếp tục loang rộng ra.
Dialogue: 0,0:01:20.96,0:01:24.60,Default,,0000,0000,0020,,Vậy thì tôi sẽ cở nó ra trước khi nó bị vấy bẩn hơn nữa.
Dialogue: 0,0:01:27.83,0:01:29.80,Default,,0000,0000,0020,,Anh làm việc này tài tình nhỉ.
Dialogue: 0,0:01:31.06,0:01:32.96,Default,,0000,0000,0020,,Tôi vốn là thợ giặt ủi chuyên nghiệp mà.
Dialogue: 0,0:01:34.23,0:01:35.23,Default,,0000,0000,0020,,Chưa được.
Dialogue: 0,0:01:36.16,0:01:37.23,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:01:42.59,0:01:44.10,Default,,0000,0000,0020,,Ko việc gì phải vội cả.
Dialogue: 0,0:01:44.36,0:01:46.86,Default,,0000,0000,0020,,Chúng ta sẽ tiếp tục chuyện này trên giường...
Dialogue: 0,0:01:48.73,0:01:50.26,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:01:58.46,0:02:00.96,Default,,0000,0000,0020,,Phải rồi.\NThanh niên quả là tuyệt diệu...
Dialogue: 0,0:02:01.09,0:02:04.90,Default,,0000,0000,0020,,Thô bạo quá.\NThằng nhỏ của anh thật mạnh mẽ...
Dialogue: 0,0:02:06.09,0:02:07.13,Default,,0000,0000,0020,,Ở đó!
Dialogue: 0,0:02:07.26,0:02:08.40,Default,,0000,0000,0020,,Là ở đó!
Dialogue: 0,0:02:08.53,0:02:11.50,Default,,0000,0000,0020,,Làm sạch toàn bộ thân sau của tôi nào!
Dialogue: 0,0:02:14.90,0:02:16.23,Default,,0000,0000,0020,,Phải rồi...
Dialogue: 0,0:02:17.29,0:02:18.46,Default,,0000,0000,0020,,Thật mạnh mẽ...
Dialogue: 0,0:02:18.60,0:02:20.06,Default,,0000,0000,0020,,...và dữ dội...
Dialogue: 0,0:02:21.23,0:02:24.53,Default,,0000,0000,0020,,Cô Masayo, làm sạch bên trong cô đúng là điều tuyệt vời nhất...
Dialogue: 0,0:02:25.26,0:02:28.90,Default,,0000,0000,0020,,Tôi ko thể chịu thua những cô ả nhà kế bên được.
Dialogue: 0,0:02:32.06,0:02:36.37,Default,,0000,0000,0020,,Vậy nên tôi sẽ "sấy khô" anh bằng kĩ thuật riêng của tôi.
Dialogue: 0,0:02:41.50,0:02:42.96,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:02:47.36,0:02:51.50,Default,,0000,0000,0020,,Nếu cô nhịp mạnh như vậy,\Nnó sẽ càng khít hơn nữa, tôi...
Dialogue: 0,0:02:55.33,0:02:58.46,Default,,0000,0000,0020,,anh còn đang thanh niên mà,\Nđừng ra nhanh như vậy chứ.
Dialogue: 0,0:02:58.60,0:03:00.56,Default,,0000,0000,0020,,Ra như vậy chẳng phải quá dễ dàng sao?
Dialogue: 0,0:03:02.86,0:03:05.36,Default,,0000,0000,0020,,Tôi thì lại thích thế này hơn...
Dialogue: 0,0:03:06.93,0:03:08.06,Default,,0000,0000,0020,,Vậy nên
Dialogue: 0,0:03:10.16,0:03:11.83,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:03:13.73,0:03:16.53,Default,,0000,0000,0020,,Tôi ko sao, tuyệt quá đi mất!
Dialogue: 0,0:03:20.00,0:03:21.66,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:03:24.56,0:03:26.03,Default,,0000,0000,0020,,Tôi sắp...
Dialogue: 0,0:03:28.06,0:03:30.16,Default,,0000,0000,0020,,Cho phép anh ra bên trong tôi đó...
Dialogue: 0,0:03:32.60,0:03:36.93,Default,,0000,0000,0020,,Hãy lấp đầy bên trong tôi bằng tinh dịch nóng bỏng của anh!
Dialogue: 0,0:03:38.93,0:03:40.10,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:03:41.39,0:03:42.66,Default,,0000,0000,0020,,Cô Masayo...
Dialogue: 0,0:03:43.86,0:03:44.83,Default,,0000,0000,0020,,Ra bên trong...
Dialogue: 0,0:03:44.93,0:03:47.06,Default,,0000,0000,0020,,Làm ơn hãy ra bên trong tôi...
Dialogue: 0,0:03:47.16,0:03:48.96,Default,,0000,0000,0020,,Cô Masayo!
Dialogue: 0,0:03:50.76,0:03:53.20,Default,,0000,0000,0020,,Tinh dịch của thanh niên thật tuyệt vời!
Dialogue: 0,0:04:03.96,0:04:05.93,Default,,0000,0000,0020,,Tên tôi là Shinii Ouhashi.
Dialogue: 0,0:04:06.39,0:04:08.83,Default,,0000,0000,0020,,Tôi là nhân viên bán thời gian của tiệm giặt ủi Asai.
Dialogue: 0,0:04:09.56,0:04:10.76,Default,,0000,0000,0020,,Mọi người gọi tôi là...
Dialogue: 0,0:04:10.89,0:04:12.20,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:04:13.46,0:04:16.23,Default,,0000,0000,0020,,Tôi vừa gặp chút rắc rối, anh tạm ngừng công việc và giúp tôi một tay được ko?
Dialogue: 0,0:04:16.36,0:04:16.36,Default,,0000,0000,0020,,Ah, vâng.
Dialogue: 0,0:04:17.16,0:04:18.13,Default,,0000,0000,0020,,Ah, vâng.\NĐến luôn nhé.
Dialogue: 0,0:04:18.26,0:04:20.06,Default,,0000,0000,0020,,Cám ơn cô đã tin tưởng.
Dialogue: 0,0:04:21.26,0:04:24.00,Default,,0000,0000,0020,,Là vậy đó... Mọi người gọi tôi là...
Dialogue: 0,0:04:31.80,0:04:36.30,Default,,0000,0000,0020,,Shin -siêu giặt ủi.
Dialogue: 0,0:04:36.83,0:04:48.60,Default,,0000,0000,0445,,Translated by Invincible_Dark_Thunder
Dialogue: 0,0:04:36.83,0:04:48.60,Default,,0000,0000,0020,,Mặt trái nghề giặt ủi\NPhần 1
Dialogue: 0,0:05:01.33,0:05:03.63,Default,,0000,0000,0020,,Tôi làm công việc này đã được gần một tháng.
Dialogue: 0,0:05:04.33,0:05:09.93,Default,,0000,0000,0020,,Sức lực và vẻ ngoài ưa nhìn đã giúp tôi chiếm được cảm tình của các bà nội trợ xung quanh đây...
Dialogue: 0,0:05:10.23,0:05:12.20,Default,,0000,0000,0020,,Tiệm giặt ủi Asai
Dialogue: 0,0:05:12.86,0:05:15.90,Default,,0000,0000,0020,,...nhưng còn một người mà tôi vẫn chưa thể chinh phục nổi...
Dialogue: 0,0:05:18.66,0:05:20.00,Default,,0000,0000,0020,,Tôi về rồi đây.
Dialogue: 0,0:05:20.13,0:05:21.53,Default,,0000,0000,0020,,Anh làm việc chăm chỉ quá.
Dialogue: 0,0:05:21.66,0:05:23.13,Default,,0000,0000,0020,,Ngoài kia nóng lắm phải ko?
Dialogue: 0,0:05:23.36,0:05:26.20,Default,,0000,0000,0020,,Tôi pha sẵn trà lúa mạch cho anh rồi đây, cứ ngồi nghỉ đi nhé.
Dialogue: 0,0:05:26.33,0:05:31.10,Default,,0000,0000,0020,,Cô ấy là Sayaka, một góa phụ, và cũng là bà chủ của cửa tiệm này.
Dialogue: 0,0:05:32.16,0:05:33.46,Default,,0000,0000,0020,,Cám ơn cô.
Dialogue: 0,0:05:33.93,0:05:36.50,Default,,0000,0000,0020,,Anh nhanh thay áo đi, kẻo mồ hôi làm nó ướt sũng bây giờ .
Dialogue: 0,0:05:37.29,0:05:41.67,Default,,0000,0000,0020,,Đây là áo của chồng tôi, có lẽ kích cỡ cũng tương đương , anh mặc tạm nhé.
Dialogue: 0,0:05:41.83,0:05:43.33,Default,,0000,0000,0020,,Ah, vâng.
Dialogue: 0,0:05:46.93,0:05:50.90,Default,,0000,0000,0020,,Kiểu dáng hơi cũ,\Ncó lẽ nó ko hợp với anh?
Dialogue: 0,0:05:52.50,0:05:53.73,Default,,0000,0000,0020,,À ko, ko vấn đề gì đâu ạ!
Dialogue: 0,0:05:54.40,0:05:55.60,Default,,0000,0000,0020,,Vậy thì tốt quá.
Dialogue: 0,0:05:58.93,0:06:04.10,Default,,0000,0000,0020,,Ngay cả lúc này, hình bóng người chồng quá cố vẫn luôn chiếm trọn trái tim cô ấy.
Dialogue: 0,0:06:06.26,0:06:10.70,Default,,0000,0000,0020,,Tôi luôn muốn sưởi ấm trái tim cô đơn của cô ấy.
Dialogue: 0,0:06:12.56,0:06:14.06,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:06:23.50,0:06:24.56,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:06:24.70,0:06:25.80,Default,,0000,0000,0020,,Ko...
Dialogue: 0,0:06:26.36,0:06:27.83,Default,,0000,0000,0020,,Anh ko được phép, Shin...
Dialogue: 0,0:06:27.96,0:06:31.40,Default,,0000,0000,0020,,Tôi biết tình cảm của anh dành cho tôi,\Nnhưng chồng tôi vẫn...
Dialogue: 0,0:06:32.09,0:06:35.36,Default,,0000,0000,0020,,Tại nơi này, hình bóng Yutaro vẫn luôn tồn tại...
Dialogue: 0,0:06:35.86,0:06:36.96,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:06:37.26,0:06:39.86,Default,,0000,0000,0020,,Nhưng ngực của cô đang phủ nhận điều đó này.
Dialogue: 0,0:06:42.63,0:06:45.86,Default,,0000,0000,0020,,Nó đã cứng lên như vậy rồi. Rõ ràng cô muốn điều đó, phải ko cô Sayaka?
Dialogue: 0,0:06:46.53,0:06:48.96,Default,,0000,0000,0020,,Làm ơn hãy tha thứ cho em, Yutaro...
Dialogue: 0,0:06:52.13,0:06:53.10,Default,,0000,0000,0020,,Ko...
Dialogue: 0,0:06:53.29,0:06:54.50,Default,,0000,0000,0020,,Chỗ đó...
Dialogue: 0,0:06:54.66,0:06:56.00,Default,,0000,0000,0020,,Cô ướt đẫm rồi nè...
Dialogue: 0,0:06:56.13,0:06:57.83,Default,,0000,0000,0020,,Cô ko làm chuyện đó bao lâu rồi vậy?
Dialogue: 0,0:06:58.23,0:06:59.20,Default,,0000,0000,0020,,Đồ ngốc...
Dialogue: 0,0:06:59.29,0:07:01.40,Default,,0000,0000,0020,,Đó là vì em muốn dành nó cho anh.
Dialogue: 0,0:07:02.70,0:07:04.40,Default,,0000,0000,0020,,Vậy nên hãy nhét nó vào bên trong tôi đi.
Dialogue: 0,0:07:06.00,0:07:07.63,Default,,0000,0000,0020,,Cô Sayaka... Tôi...
Dialogue: 0,0:07:10.46,0:07:11.90,Default,,0000,0000,0020,,Oh, Shin!
Dialogue: 0,0:07:14.23,0:07:16.73,Default,,0000,0000,0020,,Em cô đơn lắm, Shin...
Dialogue: 0,0:07:16.83,0:07:19.80,Default,,0000,0000,0020,,Đừng rời xa em...\NEm ko thể sống thiếu anh được!
Dialogue: 0,0:07:20.10,0:07:22.73,Default,,0000,0000,0020,,Tôi sẽ luôn ở bên cạnh em.
Dialogue: 0,0:07:22.89,0:07:24.36,Default,,0000,0000,0020,,Tôi ko thể sống thiếu em được.
Dialogue: 0,0:07:24.60,0:07:25.63,Default,,0000,0000,0020,,Ôm em đi...
Dialogue: 0,0:07:25.73,0:07:27.70,Default,,0000,0000,0020,,Vào sâu hơn nữa đi... sâu bên trong em...
Dialogue: 0,0:07:28.19,0:07:29.16,Default,,0000,0000,0020,,Tuyệt quá đi mất...
Dialogue: 0,0:07:29.26,0:07:32.43,Default,,0000,0000,0020,,Cảm giác này...\NEm đã luôn mong chờ nó...
Dialogue: 0,0:07:34.33,0:07:37.10,Default,,0000,0000,0020,,Mạnh nữa lên...\NMạnh nữa lên!
Dialogue: 0,0:07:38.93,0:07:42.70,Default,,0000,0000,0020,,Hãy khiến em quên đi hình bóng của anh ấy...
Dialogue: 0,0:07:42.89,0:07:44.86,Default,,0000,0000,0020,,Biến em thành người phụ nữ của anh!
Dialogue: 0,0:07:48.50,0:07:50.56,Default,,0000,0000,0020,,Em ko thể ngừng lại lúc này, Shin!
Dialogue: 0,0:07:54.39,0:07:56.56,Default,,0000,0000,0020,,Chơi em mạnh nữa lên nào, Shin!
Dialogue: 0,0:07:57.43,0:07:59.50,Default,,0000,0000,0020,,Tuyệt quá, Shin!
Dialogue: 0,0:07:59.73,0:08:01.56,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:08:01.86,0:08:02.86,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:08:04.23,0:08:07.20,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:08:14.30,0:08:15.56,Default,,0000,0000,0020,,Shin?
Dialogue: 0,0:08:16.33,0:08:17.73,Default,,0000,0000,0020,,Shin?
Dialogue: 0,0:08:18.82,0:08:19.70,Default,,0000,0000,0020,,Shin?
Dialogue: 0,0:08:19.73,0:08:20.19,Default,,0000,0000,0020,,Ah...
Dialogue: 0,0:08:23.39,0:08:24.73,Default,,0000,0000,0020,,Chuyện gì thế?
Dialogue: 0,0:08:25.53,0:08:26.60,Default,,0000,0000,0020,,Ah, um...
Dialogue: 0,0:08:27.36,0:08:28.63,Default,,0000,0000,0020,,Trông nó hợp với tôi ko?
Dialogue: 0,0:08:30.56,0:08:33.53,Default,,0000,0000,0020,,Shin, anh vui tính thật đấy.
Dialogue: 0,0:08:35.33,0:08:36.70,Default,,0000,0000,0020,,Uhm, trông hợp với anh lắm.
Dialogue: 0,0:08:37.56,0:08:38.96,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:08:40.36,0:08:41.33,Default,,0000,0000,0020,,Đây.
Dialogue: 0,0:08:41.43,0:08:43.53,Default,,0000,0000,0020,,Bộ đồ tiếp theo cần giao.
Dialogue: 0,0:08:43.70,0:08:45.06,Default,,0000,0000,0020,,Um...
Dialogue: 0,0:08:46.70,0:08:49.06,Default,,0000,0000,0020,,Trông cậy cả vào anh đấy, Shin.
Dialogue: 0,0:08:50.03,0:08:51.40,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:08:55.73,0:08:57.06,Default,,0000,0000,0020,,Tiệm giặt ủi Asai 
Dialogue: 0,0:08:58.53,0:09:02.23,Default,,0000,0000,0020,,Nụ cười của cô ấy luôn khiến tôi chết sững như vậy...
Dialogue: 0,0:09:02.93,0:09:05.70,Default,,0000,0000,0020,,Và khi tôi nghĩ về cô ấy...
Dialogue: 0,0:09:06.89,0:09:09.23,Default,,0000,0000,0020,,Ko ko! Thế này ko ổn...
Dialogue: 0,0:09:09.36,0:09:10.36,Default,,0000,0000,0020,,Phải kiềm chế...
Dialogue: 0,0:09:10.46,0:09:12.20,Default,,0000,0000,0020,,Shinji, kiềm chế nào!
Dialogue: 0,0:09:12.29,0:09:15.50,Default,,0000,0000,0020,,Kiềm chế như vậy ko tốt đâu.\NQuần lót anh sẽ bị dơ đó.
Dialogue: 0,0:09:17.29,0:09:19.26,Default,,0000,0000,0020,,C-Cô Mari!?
Dialogue: 0,0:09:21.96,0:09:24.43,Default,,0000,0000,0020,,Cô Mari, đây là...
Dialogue: 0,0:09:26.13,0:09:27.40,Default,,0000,0000,0020,,Oh, Shin!
Dialogue: 0,0:09:38.73,0:09:41.56,Default,,0000,0000,0020,,Đây là công việc của cậu mà, Shin.
Dialogue: 0,0:09:41.73,0:09:43.03,Default,,0000,0000,0020,,Ah, vâng.
Dialogue: 0,0:09:43.96,0:09:45.40,Default,,0000,0000,0020,,Công việc, huh...
Dialogue: 0,0:09:46.03,0:09:50.73,Default,,0000,0000,0020,,Cô Mari có vẻ ko hiểu rõ công việc của thợ giặt ủi.
Dialogue: 0,0:09:54.33,0:09:55.50,Default,,0000,0000,0020,,Cô Mari này, dạo này cô sống ra sao?
Dialogue: 0,0:09:55.63,0:09:57.53,Default,,0000,0000,0020,,Vốn tiếng Nhật của cô khá hơn chưa?
Dialogue: 0,0:09:57.63,0:09:59.90,Default,,0000,0000,0020,,Làm dâu ở Nhật Bản quả là ko dễ dàng nhỉ?
Dialogue: 0,0:10:00.83,0:10:03.10,Default,,0000,0000,0020,,Phải đó, có nhiều thứ thật khó giải thích ở đây.
Dialogue: 0,0:10:03.26,0:10:07.40,Default,,0000,0000,0020,,Chắc chắn rồi. Nếu cô cần giúp điều gì, làm ơn cứ nói với tôi.
Dialogue: 0,0:10:07.89,0:10:09.26,Default,,0000,0000,0020,,Tôi sẽ hỗ trợ cô hết mình.
Dialogue: 0,0:10:09.36,0:10:10.90,Default,,0000,0000,0020,,Cám ơn anh, Shin.
Dialogue: 0,0:10:11.46,0:10:14.26,Default,,0000,0000,0020,,Về những gì cô đã nhìn thấy trong xe ban nãy...
Dialogue: 0,0:10:14.60,0:10:16.70,Default,,0000,0000,0020,,...Làm ơn hãy coi như cô ko trông thấy gì cả.
Dialogue: 0,0:10:16.80,0:10:18.83,Default,,0000,0000,0020,,Nếu bà chủ mà biết chuyện này, tôi sẽ bị...
Dialogue: 0,0:10:19.30,0:10:21.70,Default,,0000,0000,0020,,Tôi ko hiểu rõ về văn hóa Nhật Bản cho lắm...
Dialogue: 0,0:10:21.83,0:10:24.46,Default,,0000,0000,0020,,...nhưng tình dục thì ở nơi nào cũng giống nhau.
Dialogue: 0,0:10:24.56,0:10:25.53,Default,,0000,0000,0020,,Sao cơ?
Dialogue: 0,0:10:30.23,0:10:33.40,Default,,0000,0000,0020,,Shin, làm ơn hãy vụt tôi bằng thứ này.
Dialogue: 0,0:10:33.99,0:10:36.46,Default,,0000,0000,0020,,Tôi biết cô là người bạo dâm...
Dialogue: 0,0:10:36.60,0:10:38.70,Default,,0000,0000,0020,,...nhưng hình như cách này ko được phổ biến cho lắm.
Dialogue: 0,0:10:38.80,0:10:39.86,Default,,0000,0000,0020,,Mau lên nào!
Dialogue: 0,0:10:44.73,0:10:45.70,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:10:45.80,0:10:47.53,Default,,0000,0000,0020,,Tôi bắt đầu nhé. Cô sẵn sàng chưa?
Dialogue: 0,0:10:47.66,0:10:49.83,Default,,0000,0000,0020,,Vụt tôi đi, mau lên nào!
Dialogue: 0,0:10:50.43,0:10:52.16,Default,,0000,0000,0020,,Một, hai...
Dialogue: 0,0:10:59.83,0:11:00.80,Default,,0000,0000,0020,,Đau...
Dialogue: 0,0:11:00.93,0:11:02.03,Default,,0000,0000,0020,,Đau quá...
Dialogue: 0,0:11:03.36,0:11:06.46,Default,,0000,0000,0020,,Vậy tôi ngừng lại nhé, cô Mari?
Dialogue: 0,0:11:10.76,0:11:13.00,Default,,0000,0000,0020,,Ko, cảm giác đau đớn này thật tuyệt...
Dialogue: 0,0:11:17.99,0:11:19.13,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:11:19.30,0:11:22.26,Default,,0000,0000,0020,,...Nếu cô nhịp mông liên tục như vậy, tôi sẽ...
Dialogue: 0,0:11:22.99,0:11:27.47,Default,,0000,0000,0020,,Shin, tay anh đang nắm chặt nó kìa.\NMuốn làm chuyện đó với tôi phải ko?
Dialogue: 0,0:11:27.59,0:11:29.10,Default,,0000,0000,0020,,Uhm, đó là...
Dialogue: 0,0:11:31.09,0:11:35.23,Default,,0000,0000,0020,,Kìm nén như thế là ko tốt.\NCho tôi thấy của thằng nhỏ của anh nào.
Dialogue: 0,0:11:35.33,0:11:36.83,Default,,0000,0000,0020,,Thằng nhỏ của đàn ông Nhật Bản...
Dialogue: 0,0:11:36.93,0:11:38.96,Default,,0000,0000,0020,,Shin, thằng nhỏ của anh...
Dialogue: 0,0:11:41.73,0:11:42.73,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:11:42.90,0:11:44.26,Default,,0000,0000,0020,,Ko chịu nổi nữa rồi!
Dialogue: 0,0:11:48.36,0:11:51.96,Default,,0000,0000,0020,,Trong lúc vụt tôi, thằng nhỏ của anh đã bự lên thế này ư.
Dialogue: 0,0:11:52.13,0:11:54.20,Default,,0000,0000,0020,,Chúng ta quả là rất hợp nhau.
Dialogue: 0,0:11:54.40,0:11:56.76,Default,,0000,0000,0020,,Đó là tật xấu của tôi mà.
Dialogue: 0,0:11:56.90,0:11:59.40,Default,,0000,0000,0020,,Thôi thích đàn ông thật thà.
Dialogue: 0,0:11:59.73,0:12:00.86,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:12:03.73,0:12:04.70,Default,,0000,0000,0020,,Thế nào?
Dialogue: 0,0:12:04.80,0:12:06.50,Default,,0000,0000,0020,,Sướng ko hả, Shin?
Dialogue: 0,0:12:06.63,0:12:08.36,Default,,0000,0000,0020,,Tuyệt lắm, cô Mari...
Dialogue: 0,0:12:09.26,0:12:12.73,Default,,0000,0000,0020,,Vậy thì tiếp theo, tôi sẽ làm thế này.
Dialogue: 0,0:12:18.70,0:12:20.03,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:12:22.06,0:12:24.76,Default,,0000,0000,0020,,Coi nào, Shin!\NAnh phải động tay đi chứ.
Dialogue: 0,0:12:26.76,0:12:29.20,Default,,0000,0000,0020,,Cùng nhau hưởng hoan lạc nào.
Dialogue: 0,0:12:30.73,0:12:31.86,Default,,0000,0000,0020,,Cô Mari!
Dialogue: 0,0:12:35.93,0:12:38.76,Default,,0000,0000,0020,,Càng vụt mạnh tay thì cô ấy càng bú nó mạnh hơn...
Dialogue: 0,0:12:38.86,0:12:39.96,Default,,0000,0000,0020,,Tuyệt quá đi mất...
Dialogue: 0,0:12:40.09,0:12:42.73,Default,,0000,0000,0020,,Vụt tôi mạnh lên nào!
Dialogue: 0,0:12:45.13,0:12:47.40,Default,,0000,0000,0020,,Cô Mari! Cô Mari...
Dialogue: 0,0:12:48.46,0:12:50.46,Default,,0000,0000,0020,,Thằng nhỏ của tôi vào trong cuống họng cô rồi...
Dialogue: 0,0:12:50.56,0:12:51.63,Default,,0000,0000,0020,,Tôi sắp...
Dialogue: 0,0:12:57.20,0:12:58.43,Default,,0000,0000,0020,,Cô Mari!
Dialogue: 0,0:12:58.53,0:13:00.66,Default,,0000,0000,0020,,Shin... Shin...
Dialogue: 0,0:13:08.83,0:13:10.63,Default,,0000,0000,0020,,Tiếp theo, chúng ta sẽ dùng đến thứ này.
Dialogue: 0,0:13:14.96,0:13:18.63,Default,,0000,0000,0020,,Sẽ có nhiều trò vui lắm đấy, Shin.
Dialogue: 0,0:13:21.83,0:13:23.56,Default,,0000,0000,0020,,Thứ này sao, huh...
Dialogue: 0,0:13:28.13,0:13:30.10,Default,,0000,0000,0020,,Shin, mau lên nào!
Dialogue: 0,0:13:33.76,0:13:36.46,Default,,0000,0000,0020,,Sướng quá đi mất!
Dialogue: 0,0:13:36.59,0:13:39.26,Default,,0000,0000,0020,,Dường như hai đầu vú của tôi sắp rời ra!
Dialogue: 0,0:13:39.40,0:13:40.36,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:13:41.73,0:13:43.70,Default,,0000,0000,0020,,Chúng sắp rách ra rồi,\Ntôi chết mất!
Dialogue: 0,0:13:43.90,0:13:45.90,Default,,0000,0000,0020,,Tôi muốn một cái ở đây nữa!
Dialogue: 0,0:13:46.30,0:13:47.86,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:13:53.33,0:13:56.90,Default,,0000,0000,0020,,Thôi nào, hãy kẹp nó vào âm vật của tôi đi mà.
Dialogue: 0,0:13:57.03,0:13:58.83,Default,,0000,0000,0020,,Giờ là lúc chúng ta cùng nhau!
Dialogue: 0,0:13:59.16,0:14:00.50,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:14:05.90,0:14:07.60,Default,,0000,0000,0020,,Tuyệt quá đi mất!
Dialogue: 0,0:14:10.23,0:14:12.06,Default,,0000,0000,0020,,Nhét nó vào trong đi!
Dialogue: 0,0:14:12.63,0:14:13.90,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:14:18.30,0:14:19.36,Default,,0000,0000,0020,,Nó đang vào...
Dialogue: 0,0:14:19.50,0:14:20.83,Default,,0000,0000,0020,,Đang vào sâu bên trong tôi...
Dialogue: 0,0:14:20.96,0:14:21.96,Default,,0000,0000,0020,,Tôi ko thể dừng lại được...
Dialogue: 0,0:14:22.06,0:14:24.00,Default,,0000,0000,0020,,Cảm giác này quá sức tuyệt vời!
Dialogue: 0,0:14:27.13,0:14:29.00,Default,,0000,0000,0020,,Tuyệt quá... Cô Mari...
Dialogue: 0,0:14:29.19,0:14:31.50,Default,,0000,0000,0020,,Nó nuốt trọn thằng nhỏ của tôi...
Dialogue: 0,0:14:32.16,0:14:35.73,Default,,0000,0000,0020,,Shin, của anh vẫn đang bự lên bên trong tôi...
Dialogue: 0,0:14:35.83,0:14:36.86,Default,,0000,0000,0020,,Tuyệt quá...
Dialogue: 0,0:14:37.06,0:14:39.63,Default,,0000,0000,0020,,Shin... Chơi tôi mạnh nữa lên nào!
Dialogue: 0,0:14:39.73,0:14:40.86,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:14:45.43,0:14:49.43,Default,,0000,0000,0020,,Sao cô làm chuyện này cùng tôi mà ko phải với chồng cô?
Dialogue: 0,0:14:49.53,0:14:51.80,Default,,0000,0000,0020,,Anh ta hoàn toàn ko biết gì về khát khao thầm kín của phụ nữ cả!
Dialogue: 0,0:14:51.96,0:14:54.06,Default,,0000,0000,0020,,Dù anh ta có tốt đến mức nào...
Dialogue: 0,0:14:54.16,0:14:58.10,Default,,0000,0000,0020,,...thì chuyện này cũng khó mà chấp nhận được,\Nphụ nữ vẫn luôn là phụ nữ.
Dialogue: 0,0:14:58.83,0:15:00.96,Default,,0000,0000,0020,,Và điều này thì ở đâu cũng vậy.
Dialogue: 0,0:15:10.13,0:15:13.60,Default,,0000,0000,0020,,Tuyệt quá đi mất!\NShin, thằng nhỏ của anh tuyệt quá!
Dialogue: 0,0:15:16.66,0:15:18.56,Default,,0000,0000,0020,,Cô Mari, Tôi sắp...
Dialogue: 0,0:15:18.83,0:15:20.10,Default,,0000,0000,0020,,Tôi ko chịu nổi nữa rồi!
Dialogue: 0,0:15:21.33,0:15:23.00,Default,,0000,0000,0020,,Ra đi nào, Shin...
Dialogue: 0,0:15:23.10,0:15:24.93,Default,,0000,0000,0020,,Ra hết vào bên trong tôi đi!
Dialogue: 0,0:15:26.83,0:15:28.70,Default,,0000,0000,0020,,Cô Mari!
Dialogue: 0,0:15:31.60,0:15:33.40,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:15:40.89,0:15:42.50,Default,,0000,0000,0020,,Của anh đây, Shin.
Dialogue: 0,0:15:42.79,0:15:43.86,Default,,0000,0000,0020,,Cám ơn cô.
Dialogue: 0,0:15:43.96,0:15:48.20,Default,,0000,0000,0020,,Nó đã bị dơ khi chúng ta làm chuyện đó, thật may là anh đã đồng ý giặt nó.
Dialogue: 0,0:15:49.13,0:15:51.03,Default,,0000,0000,0020,,Vâng, hi vọng lần sau chuyện đó sẽ ko xảy ra nữa.
Dialogue: 0,0:15:51.46,0:15:53.80,Default,,0000,0000,0020,,Shin, quay lại đây sớm nhé.
Dialogue: 0,0:15:54.43,0:15:58.00,Default,,0000,0000,0020,,Phụ nữ ai mà chả cần đến đàn ông chứ.
Dialogue: 0,0:16:14.13,0:16:15.40,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:16:16.46,0:16:19.26,Default,,0000,0000,0020,,Phụ nữ ai mà chả...
Dialogue: 0,0:16:20.20,0:16:21.63,Default,,0000,0000,0020,,...cần đến đàn ông.
Dialogue: 0,0:16:22.86,0:16:25.53,Default,,0000,0000,0020,,Có lẽ cô Sayaka là trường hợp ngoại lệ...
Dialogue: 0,0:16:27.43,0:16:29.26,Default,,0000,0000,0020,,...Hi vọng thế.
Dialogue: 0,0:16:33.66,0:16:35.60,Default,,0000,0000,0020,,Tôi về rồi đây.
Dialogue: 0,0:16:37.20,0:16:38.16,Default,,0000,0000,0020,,Huh?
Dialogue: 0,0:16:38.26,0:16:39.73,Default,,0000,0000,0020,,Cô Sayaka?
Dialogue: 0,0:16:41.79,0:16:43.70,Default,,0000,0000,0020,,Cô ấy ra ngoài rồi ư?
Dialogue: 0,0:16:44.00,0:16:46.00,Default,,0000,0000,0020,,Ra ngoài mà ko khóa cửa tiệm như vậy...
Dialogue: 0,0:16:46.20,0:16:47.23,Default,,0000,0000,0020,,Thì bất cẩn quá...
Dialogue: 0,0:16:48.73,0:16:52.36,Default,,0000,0000,0020,,Hay là cô ấy đang tắm hoặc bận làm gì đó.
Dialogue: 0,0:16:52.63,0:16:54.66,Default,,0000,0000,0020,,Uhm, để ra thử phía sau xem sao.
Dialogue: 0,0:16:59.60,0:17:00.56,Default,,0000,0000,0020,,Hả?
Dialogue: 0,0:17:01.29,0:17:03.40,Default,,0000,0000,0020,,Cô Sayaka đang khóc ư?
Dialogue: 0,0:17:04.79,0:17:05.76,Default,,0000,0000,0020,,Cô ấy vẫn còn...
Dialogue: 0,0:17:07.33,0:17:09.30,Default,,0000,0000,0020,,...thương nhớ chồng mình...
Dialogue: 0,0:17:09.43,0:17:10.43,Default,,0000,0000,0020,,...đến mức này sao?
Dialogue: 0,0:17:24.23,0:17:25.96,Default,,0000,0000,0020,,Cô Sayaka?
Dialogue: 0,0:17:30.46,0:17:32.06,Default,,0000,0000,0020,,Ko phải, cô ấy đâu có khóc...
Dialogue: 0,0:17:33.33,0:17:35.66,Default,,0000,0000,0020,,Cô ấy đang thủ dâm...
Dialogue: 0,0:17:39.06,0:17:40.70,Default,,0000,0000,0020,,Thấy ko, tôi đã bảo anh mà.
Dialogue: 0,0:17:40.96,0:17:44.66,Default,,0000,0000,0020,,Phụ nữ lúc nào cũng cần có đàn ông bên cạnh...
Dialogue: 0,0:17:48.83,0:17:50.13,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:17:53.93,0:17:55.63,Default,,0000,0000,0020,,Đây là mùi của anh ấy...
Dialogue: 0,0:17:55.79,0:17:59.13,Default,,0000,0000,0020,,Ko ngờ anh ấy đổ mồ hôi nhiều như vậy khi làm việc cho mình...
Dialogue: 0,0:18:00.86,0:18:03.83,Default,,0000,0000,0020,,Ko... Như vậy vẫn chưa đủ...
Dialogue: 0,0:18:04.33,0:18:05.60,Default,,0000,0000,0020,,Đó là chiếc áo...
Dialogue: 0,0:18:05.73,0:18:08.90,Default,,0000,0000,0020,,...mà cô Sayaka nói sẽ giặt dùm mình.
Dialogue: 0,0:18:11.23,0:18:12.46,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:18:12.59,0:18:14.10,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:18:18.83,0:18:20.30,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:18:23.93,0:18:25.86,Default,,0000,0000,0020,,Mùi của Shin...
Dialogue: 0,0:18:26.06,0:18:27.63,Default,,0000,0000,0020,,Mùi của đàn ông...
Dialogue: 0,0:18:38.80,0:18:41.53,Default,,0000,0000,0020,,Oh ko, quần lót của mình ướt mất...
Dialogue: 0,0:18:44.50,0:18:46.03,Default,,0000,0000,0020,,S-Shin?
Dialogue: 0,0:18:47.19,0:18:48.40,Default,,0000,0000,0020,,O-Oh ko!
Dialogue: 0,0:18:53.30,0:18:54.40,Default,,0000,0000,0020,,Coi chừng!
Dialogue: 0,0:18:59.96,0:19:02.26,Default,,0000,0000,0020,,Ouch...
Dialogue: 0,0:19:08.73,0:19:10.56,Default,,0000,0000,0020,,X-Xin lỗi!
Dialogue: 0,0:19:11.23,0:19:12.20,Default,,0000,0000,0020,,Ah!
Dialogue: 0,0:19:13.06,0:19:14.03,Default,,0000,0000,0020,,Ah!
Dialogue: 0,0:19:14.13,0:19:15.13,Default,,0000,0000,0020,,Đây là...
Dialogue: 0,0:19:15.66,0:19:17.60,Default,,0000,0000,0020,,Anh ấy đang...
Dialogue: 0,0:19:22.20,0:19:24.10,Default,,0000,0000,0020,,C-Cô Sayaka?
Dialogue: 0,0:19:24.20,0:19:27.10,Default,,0000,0000,0020,,Rất lâu rồi tôi chưa từng được thấy thứ đó ...
Dialogue: 0,0:19:27.23,0:19:29.46,Default,,0000,0000,0020,,Nó thật dài... thật ấm...
Dialogue: 0,0:19:30.56,0:19:32.46,Default,,0000,0000,0020,,Từ khi anh ấy qua đời...
Dialogue: 0,0:19:33.36,0:19:34.60,Default,,0000,0000,0020,,Cô Sayaka?
Dialogue: 0,0:19:36.60,0:19:38.33,Default,,0000,0000,0020,,Oh ko, tôi...
Dialogue: 0,0:19:38.56,0:19:39.70,Default,,0000,0000,0020,,Xin lỗi!
Dialogue: 0,0:19:44.43,0:19:47.40,Default,,0000,0000,0020,,Mùi của đàn ông chính là điểm yếu của tôi...
Dialogue: 0,0:19:47.79,0:19:51.70,Default,,0000,0000,0020,,Từ khi chồng tôi qua đời, tôi luôn phải tự thỏa mãn mình theo cách này...
Dialogue: 0,0:19:52.29,0:19:55.40,Default,,0000,0000,0020,,Vậy nên khi lỡ ngửi áo của anh, Shin...
Dialogue: 0,0:19:55.86,0:19:57.66,Default,,0000,0000,0020,,...Tôi đã ko thể kiềm chế nổi...
Dialogue: 0,0:19:57.83,0:19:59.30,Default,,0000,0000,0020,,Dù sao thì tôi vẫn là phụ nữ...
Dialogue: 0,0:20:00.26,0:20:02.66,Default,,0000,0000,0020,,Thật sự xin lỗi anh...
Dialogue: 0,0:20:03.53,0:20:08.03,Default,,0000,0000,0020,,Dù biết điều này là sai trái,\Ncơ thể tôi vẫn đang hối thúc...
Dialogue: 0,0:20:08.96,0:20:10.20,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:20:15.06,0:20:16.70,Default,,0000,0000,0020,,Cô... Sayaka...
Dialogue: 0,0:20:19.36,0:20:23.16,Default,,0000,0000,0020,,Tôi ko thể chịu nổi nữa rồi\Ncho tôi thưởng thức nó nào.
Dialogue: 0,0:20:23.79,0:20:25.40,Default,,0000,0000,0020,,Dương vật của đàn ông!
Dialogue: 0,0:20:28.70,0:20:31.03,Default,,0000,0000,0020,,Nó đang co giật kìa, thật nam tính...
Dialogue: 0,0:20:31.70,0:20:33.83,Default,,0000,0000,0020,,Thằng nhỏ... của Shin...
Dialogue: 0,0:20:35.43,0:20:38.00,Default,,0000,0000,0020,,Như vậy thật ko công bằng. Cô Sayaka...
Dialogue: 0,0:20:38.20,0:20:40.26,Default,,0000,0000,0020,,Tôi muốn làm điều đó với cô...
Dialogue: 0,0:20:43.06,0:20:44.33,Default,,0000,0000,0020,,Tùy anh...
Dialogue: 0,0:20:48.23,0:20:50.16,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:20:52.13,0:20:56.93,Default,,0000,0000,0020,,Hãy khiến tôi ướt đẫm đi nào, Shin.
Dialogue: 0,0:21:00.93,0:21:02.36,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:21:07.60,0:21:09.63,Default,,0000,0000,0020,,Đừng giỡn tôi nữa mà. Liếm qua quần lót thì có gì hay chứ.
Dialogue: 0,0:21:09.93,0:21:12.20,Default,,0000,0000,0020,,Quậy lưỡi của anh mạnh nữa lên nào, Shin.
Dialogue: 0,0:21:13.06,0:21:14.06,Default,,0000,0000,0020,,Yes...
Dialogue: 0,0:21:17.43,0:21:19.26,Default,,0000,0000,0020,,Wow, của cô Sayaka...
Dialogue: 0,0:21:20.56,0:21:22.53,Default,,0000,0000,0020,,Hồng hào và đẹp quá...
Dialogue: 0,0:21:27.43,0:21:29.23,Default,,0000,0000,0020,,Shin, là chỗ đó! Ngay chỗ đó!
Dialogue: 0,0:21:29.39,0:21:31.36,Default,,0000,0000,0020,,Quậy lưỡi anh quanh đó mạnh nữa lên nào!
Dialogue: 0,0:21:34.56,0:21:37.06,Default,,0000,0000,0020,,Shin, tôi ko chịu nổi nữa rồi.
Dialogue: 0,0:21:37.16,0:21:38.13,Default,,0000,0000,0020,,Làm ơn hãy nhét nó vào!
Dialogue: 0,0:21:38.23,0:21:40.76,Default,,0000,0000,0020,,Nhét nó vào bên trong âm hộ của tôi đi!
Dialogue: 0,0:21:41.06,0:21:43.03,Default,,0000,0000,0020,,Tôi cũng muốn như vậy, cô Sayaka...
Dialogue: 0,0:21:44.06,0:21:45.76,Default,,0000,0000,0020,,Oh, Shin...
Dialogue: 0,0:21:46.70,0:21:47.96,Default,,0000,0000,0020,,Tuyệt quá.
Dialogue: 0,0:21:56.46,0:21:58.13,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:22:00.70,0:22:02.13,Default,,0000,0000,0020,,Nó đang vào sâu bên trong!
Dialogue: 0,0:22:02.53,0:22:05.43,Default,,0000,0000,0020,,Tuyệt quá đi mất!
Dialogue: 0,0:22:10.59,0:22:13.30,Default,,0000,0000,0020,,Tôi sẽ khiến cô hứng thú hơn nữa!
Dialogue: 0,0:22:15.80,0:22:19.73,Default,,0000,0000,0020,,Ôi chúa ơi, lâu rồi chưa ai nắn ngực tôi mạnh như vậy!
Dialogue: 0,0:22:19.89,0:22:21.36,Default,,0000,0000,0020,,Mạnh nữa lên nào!
Dialogue: 0,0:22:29.50,0:22:31.16,Default,,0000,0000,0020,,Như tôi hằng mong ước, đồ thật bao giờ cũng tốt hơn!
Dialogue: 0,0:22:31.26,0:22:32.90,Default,,0000,0000,0020,,Tốt hơn rất nhiều!
Dialogue: 0,0:22:41.59,0:22:45.80,Default,,0000,0000,0020,,Shin, hãy ở lại đây cùng tôi đêm nay.
Dialogue: 0,0:22:54.50,0:22:56.03,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:22:56.16,0:22:58.86,Default,,0000,0000,0020,,Tuyệt quá, Shin. Tuyệt quá đi mất...
Dialogue: 0,0:23:04.13,0:23:06.83,Default,,0000,0000,0020,,Ngay chỗ đó... sướng quá đi ...
Dialogue: 0,0:23:07.80,0:23:09.10,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:23:09.20,0:23:10.36,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:23:12.93,0:23:15.83,Default,,0000,0000,0020,,Shin, của anh vẫn đang bự lên nè...
Dialogue: 0,0:23:16.39,0:23:19.36,Default,,0000,0000,0020,,Shin, nếu anh muốn ra,\NHãy ra cùng với tôi.
Dialogue: 0,0:23:20.73,0:23:21.96,Default,,0000,0000,0020,,Cô Sayaka!
Dialogue: 0,0:23:27.23,0:23:28.63,Default,,0000,0000,0020,,Phải rồi... Nữa đi...
Dialogue: 0,0:23:28.76,0:23:31.03,Default,,0000,0000,0020,,Chơi tôi mạnh nữa lên!
Dialogue: 0,0:23:31.66,0:23:32.83,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:23:34.76,0:23:36.43,Default,,0000,0000,0020,,Lưng của anh...
Dialogue: 0,0:23:36.56,0:23:38.33,Default,,0000,0000,0020,,Lưng của một người đàn ông...
Dialogue: 0,0:23:38.49,0:23:39.76,Default,,0000,0000,0020,,Hạnh phúc quá!
Dialogue: 0,0:23:45.73,0:23:47.36,Default,,0000,0000,0020,,Thằng nhỏ của anh thật mạnh mẽ!
Dialogue: 0,0:23:48.10,0:23:49.83,Default,,0000,0000,0020,,Nó đang vào sâu bên trong tôi...
Dialogue: 0,0:23:49.99,0:23:51.13,Default,,0000,0000,0020,,Tuyệt quá...
Dialogue: 0,0:23:51.90,0:23:53.40,Default,,0000,0000,0020,,Tuyệt quá đi mất!
Dialogue: 0,0:23:54.16,0:23:55.36,Default,,0000,0000,0020,,Tôi ko chịu nổi nữa rồi...
Dialogue: 0,0:23:55.46,0:23:58.76,Default,,0000,0000,0020,,Âm hộ của cô Sayaka đang nuốt chửng nó!
Dialogue: 0,0:24:00.16,0:24:01.23,Default,,0000,0000,0020,,Ra bên trong...
Dialogue: 0,0:24:01.36,0:24:03.30,Default,,0000,0000,0020,,Ra vào bên trong tôi!
Dialogue: 0,0:24:04.26,0:24:06.10,Default,,0000,0000,0020,,Shin, tôi ra đây...
Dialogue: 0,0:24:06.23,0:24:07.30,Default,,0000,0000,0020,,Tôi ra đây!
Dialogue: 0,0:24:29.69,0:24:31.66,Default,,0000,0000,0020,,Cô Sayaka...
Dialogue: 0,0:24:34.46,0:24:35.76,Default,,0000,0000,0020,,Đừng.
Dialogue: 0,0:24:38.40,0:24:43.00,Default,,0000,0000,0020,,Làm ơn giữ nó ở bên trong tôi một chút nữa thôi...
Dialogue: 0,0:24:44.63,0:24:45.73,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:24:53.06,0:24:56.13,Default,,0000,0000,0020,,Thằng nhỏ của anh đang co giật bên trong tôi...
