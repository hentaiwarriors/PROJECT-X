Dialogue: 0,0:00:13.71,0:00:15.21,EN,,0000,0000,0000,,Mizuho-senpai,
Dialogue: 0,0:00:15.21,0:00:19.48,EN,,0000,0000,0000,,Về câu hỏi này em có một chút không hiểu...
Dialogue: 0,0:00:20.38,0:00:24.75,EN,,0000,0000,0000,,À... đầu tiên cậu phải áp dụng phương trình này ở đây.
Dialogue: 0,0:00:25.96,0:00:28.06,EN,,0000,0000,0000,,Xin lỗi nhé, senpai.
Dialogue: 0,0:00:29.72,0:00:32.09,EN,,0000,0000,0000,,Phải phiền chị ở lại sao giờ học chỉ vì bọn em.
Dialogue: 0,0:00:33.43,0:00:34.39,EN,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:00:34.40,0:00:36.76,EN,,0000,0000,0000,,Chỉ dạy cho người khác là sở trường của chị.
Dialogue: 0,0:00:39.20,0:00:42.54,EN,,0000,0000,0000,,S-Senpai, chị không thể làm thế...
Dialogue: 0,0:00:50.24,0:00:54.78,EN,,0000,0000,0000,,Nhưng bầu không khí lớp học của khóa trên có khác nhỉ.
Dialogue: 0,0:00:54.78,0:00:55.68,EN,,0000,0000,0000,,Vậy ư?
Dialogue: 0,0:00:59.32,0:01:03.82,EN,,0000,0000,0000,,Hừm... Chị thường ngồi ở chỗ đó sao, Mizuho-senpai?
Dialogue: 0,0:01:04.03,0:01:06.49,EN,,0000,0000,0000,,Phải. Sao thế?
Dialogue: 0,0:01:06.83,0:01:09.80,EN,,0000,0000,0000,,À, không. Không có gì.
Dialogue: 0,0:01:21.91,0:01:26.15,EN,,0000,0000,0000,,Senpai, em sắp rồi...
Dialogue: 0,0:01:27.38,0:01:28.78,EN,,0000,0000,0000,,A, ôi không!
Dialogue: 0,0:02:17.60,0:02:22.84,EN,,0000,0000,0000,,Đây là giấy mời của Mizuho đến nhà chị ấy.
Dialogue: 0,0:02:34.00,0:02:45.03,Accelerando,,0000,0000,0000,,{\pos(411,64)}Baka-sub trân trọng giới thiệu
Dialogue: 0,0:02:38.72,0:02:45.03,Accelerando,,0000,0000,0000,,{\fs30\pos(431,446)\c&HFFFFFF&\3c&H333A80&}#1 [ Tuyến xe ngọt ngào ]
Dialogue: 0,0:03:05.05,0:03:06.41,EN,,0000,0000,0000,,Lạ à nha.
Dialogue: 0,0:03:06.71,0:03:09.12,EN,,0000,0000,0000,,Cô ấy thường ở trên tuyến xe này-
Dialogue: 0,0:03:12.29,0:03:14.35,EN,,0000,0000,0000,,Tay nắm đứt rồi...
Dialogue: 0,0:03:14.59,0:03:17.52,EN,,0000,0000,0000,,Con nhỏ này...
Dialogue: 0,0:03:34.44,0:03:35.34,EN,,0000,0000,0000,,K-Không thể nào!
Dialogue: 0,0:03:38.95,0:03:39.38,EN,,0000,0000,0000,,Hể?
Dialogue: 0,0:03:40.51,0:03:45.19,EN,,0000,0000,0000,,Ơ... Ơ... C-Chào... b-buổi sáng.
Dialogue: 0,0:03:47.69,0:03:51.66,EN,,0000,0000,0000,,Không lẽ, mình đã có cơ hội để có thể trò chuyện với cô ấy...
Dialogue: 0,0:03:52.99,0:03:57.03,EN,,0000,0000,0000,,Chúng ta đang đến Bosoyokojo, Bosoyokojo.
Dialogue: 0,0:04:07.98,0:04:09.61,EN,,0000,0000,0000,,Suy cho cùng thì nó không đông lắm nhỉ.
Dialogue: 0,0:04:11.78,0:04:12.48,EN,,0000,0000,0000,,Phải.
Dialogue: 0,0:04:14.82,0:04:16.95,EN,,0000,0000,0000,,A, cái ông đó.
Dialogue: 0,0:04:17.48,0:04:20.55,EN,,0000,0000,0000,,Lúc nào ông ta cũng đứng cạnh phụ nữ.
Dialogue: 0,0:04:22.72,0:04:26.96,EN,,0000,0000,0000,,Ông ta... không chỉ đơn thuần là một tên biến thái.
Dialogue: 0,0:04:33.70,0:04:34.77,EN,,0000,0000,0000,,Cô ấy cười rồi!
Dialogue: 0,0:04:35.90,0:04:37.30,EN,,0000,0000,0000,,Cậu nhìn tốt nhỉ?
Dialogue: 0,0:04:38.24,0:04:39.31,EN,,0000,0000,0000,,Không hẳn-
Dialogue: 0,0:04:39.87,0:04:40.94,EN,,0000,0000,0000,,Không phải.
Dialogue: 0,0:04:43.44,0:04:44.98,EN,,0000,0000,0000,,Tôi đang nói về mình.
Dialogue: 0,0:04:49.62,0:04:53.39,EN,,0000,0000,0000,,Có hai người sẽ lên tàu ở trạm kế tiếp.
Dialogue: 0,0:04:53.62,0:04:56.62,EN,,0000,0000,0000,,Để xem, có một anh chàng trông như củ khoai tây,
Dialogue: 0,0:04:56.72,0:05:00.26,EN,,0000,0000,0000,,Và một ông già lắm chuyện phải không?
Dialogue: 0,0:05:01.33,0:05:04.53,EN,,0000,0000,0000,,Cậu muốn đoán xem ai sẽ bước lên tàu trước không?
Dialogue: 0,0:05:04.67,0:05:07.00,EN,,0000,0000,0000,,Ơ? Ơ, cũng được.
Dialogue: 0,0:05:07.47,0:05:10.10,EN,,0000,0000,0000,,Tôi nghĩ mình sẽ chọn ông già lắm chuyện.
Dialogue: 0,0:05:10.10,0:05:12.64,EN,,0000,0000,0000,,Vậy, tôi sẽ chọn anh chàng như củ khoai tây.
Dialogue: 0,0:05:15.01,0:05:16.98,EN,,0000,0000,0000,,Đến rồi kìa...
Dialogue: 0,0:05:17.85,0:05:21.51,EN,,0000,0000,0000,,Không, không, thật là!
Dialogue: 0,0:05:25.95,0:05:26.75,EN,,0000,0000,0000,,Phải vậy chứ!
Dialogue: 0,0:05:28.52,0:05:29.66,EN,,0000,0000,0000,,Tôi thắng rồi.
Dialogue: 0,0:05:32.93,0:05:35.90,EN,,0000,0000,0000,,C-C-Cái gì?
Dialogue: 0,0:05:40.87,0:05:44.20,EN,,0000,0000,0000,,Cái gì ư? Phần thưởng dành cho người chiến thắng.
Dialogue: 0,0:05:53.38,0:05:54.65,EN,,0000,0000,0000,,Nứng rồi.
Dialogue: 0,0:06:14.10,0:06:16.37,EN,,0000,0000,0000,,Áo len của tôi sướng lắm phải không?
Dialogue: 0,0:06:27.58,0:06:29.48,EN,,0000,0000,0000,,Bao quy đầu dày nhỉ.
Dialogue: 0,0:06:37.29,0:06:39.26,EN,,0000,0000,0000,,Nó đã nóng như vầy...
Dialogue: 0,0:06:53.07,0:06:56.41,EN,,0000,0000,0000,,Nhìn này, bây giờ nó trở nên nhớp nháp hết cả rồi.
Dialogue: 0,0:07:05.45,0:07:08.09,EN,,0000,0000,0000,,Không sao đâu, cứ ra nếu cậu muốn.
Dialogue: 0,0:07:21.80,0:07:23.97,EN,,0000,0000,0000,,Dọn hết đống này chắc vất vả lắm.
Dialogue: 0,0:07:27.77,0:07:30.44,EN,,0000,0000,0000,,Vậy, chúng ta hãy chơi thêm một lần nữa.
Dialogue: 0,0:07:32.21,0:07:37.15,EN,,0000,0000,0000,,Nếu cậu thắng, cậu có thể dùng... thứ này.
Dialogue: 0,0:07:40.95,0:07:44.72,EN,,0000,0000,0000,,Lần này, tôi sẽ để cho cậu chơi tôi đến khi cậu ra mới thôi.
Dialogue: 0,0:07:48.26,0:07:49.23,EN,,0000,0000,0000,,Phía đó.
Dialogue: 0,0:07:52.93,0:07:55.63,EN,,0000,0000,0000,,Người phụ nữ đó sẽ xuống ở trạm kế tiếp.
Dialogue: 0,0:07:57.10,0:08:01.37,EN,,0000,0000,0000,,Trong số hai người đứng phía trước cô ta, ai sẽ ngồi xuống?
Dialogue: 0,0:08:02.38,0:08:03.14,EN,,0000,0000,0000,,Sao nào?
Dialogue: 0,0:08:04.28,0:08:08.11,EN,,0000,0000,0000,,Vậy... có lẽ là ông già lắm chuyện.
Dialogue: 0,0:08:28.20,0:08:29.37,EN,,0000,0000,0000,,Được rồi!
Dialogue: 0,0:08:31.10,0:08:32.30,EN,,0000,0000,0000,,Cậu may thật đấy.
Dialogue: 0,0:08:52.03,0:08:54.79,EN,,0000,0000,0000,,Cậu muốn nhìn chỗ đó của con gái dữ vậy sao?
Dialogue: 0,0:08:55.10,0:08:56.96,EN,,0000,0000,0000,,Không. Ơ...
Dialogue: 0,0:08:58.30,0:09:00.23,EN,,0000,0000,0000,,Hãy bôi trơn nó một chút...
Dialogue: 0,0:09:17.15,0:09:20.39,EN,,0000,0000,0000,,Tinh dịch của cậu thật là đặc.
Dialogue: 0,0:09:20.75,0:09:23.09,EN,,0000,0000,0000,,Như vầy có lẽ tôi sẽ có thai mất.
Dialogue: 0,0:09:30.86,0:09:34.27,EN,,0000,0000,0000,,Được rồi. Đừng phí thời gian nữa.
Dialogue: 0,0:09:34.87,0:09:36.13,EN,,0000,0000,0000,,Ư-Ừm.
Dialogue: 0,0:09:43.08,0:09:48.01,EN,,0000,0000,0000,,Mặc dù tôi không di chuyển, nhưng bên trong cô đang rộn lên...
Dialogue: 0,0:09:53.32,0:09:55.89,EN,,0000,0000,0000,,Nếu cậu di chuyển sẽ còn sướng hơn nữa đấy.
Dialogue: 0,0:10:05.80,0:10:09.77,EN,,0000,0000,0000,,Dù cho cậu vừa mới ra lúc nãy, nhưng cậu vẫn còn cứng như vầy...
Dialogue: 0,0:10:12.61,0:10:16.04,EN,,0000,0000,0000,,Bởi vì... Bởi vì... nó sướng lắm...
Dialogue: 0,0:10:37.43,0:10:42.53,EN,,0000,0000,0000,,Phải, nó sướng quá đi mất!
Dialogue: 0,0:10:43.60,0:10:48.47,EN,,0000,0000,0000,,Lúc nào tôi cũng chờ thằng nhỏ của cậu, hãy mạnh bạo hơn nữa!
Dialogue: 0,0:10:50.41,0:10:54.01,EN,,0000,0000,0000,,Làm ơn, mạnh hơn nữa, chơi tôi mạnh hơn nữa...
Dialogue: 0,0:10:54.01,0:10:58.55,EN,,0000,0000,0000,,Cậu có thể đâm vào bên trong âm hộ tôi như tất cả cậu muốn!
Dialogue: 0,0:11:00.62,0:11:03.49,EN,,0000,0000,0000,,Không được, tôi sắp...
Dialogue: 0,0:11:11.50,0:11:15.67,EN,,0000,0000,0000,,Được mà. Ở đâu cũng được, cậu cứ ra chỗ nào cậu muốn!
Dialogue: 0,0:11:29.22,0:11:31.65,EN,,0000,0000,0000,,Cậu sẽ đục một lỗ xuyên qua bức tường mất!
Dialogue: 0,0:11:44.36,0:11:46.00,EN,,0000,0000,0000,,Cậu cứ như một con bò.
Dialogue: 0,0:11:47.20,0:11:50.57,EN,,0000,0000,0000,,Trạm kế, Shinbojo, Shinbojo.
Dialogue: 0,0:11:52.10,0:11:54.67,EN,,0000,0000,0000,,A... Tôi phải xuống rồi...
Dialogue: 0,0:11:55.21,0:11:56.37,EN,,0000,0000,0000,,Vẫn chưa được đâu.
Dialogue: 0,0:11:59.71,0:12:00.75,EN,,0000,0000,0000,,Trò chơi cuối cùng.
Dialogue: 0,0:12:06.75,0:12:12.59,EN,,0000,0000,0000,,Nếu tôi hét lên ở đây, cậu nghĩ mọi người ở toa nào kéo đến trước?
Dialogue: 0,0:12:18.46,0:12:19.90,EN,,0000,0000,0000,,Nào, tao nào đây?
Dialogue: 0,0:12:20.17,0:12:23.54,EN,,0000,0000,0000,,Nếu cậu chọn đúng, tôi sẽ làm như không có chuyện gì xảy ra!
Dialogue: 0,0:12:23.90,0:12:25.10,EN,,0000,0000,0000,,{\i1}Tiếng hét đến từ sau cánh cửa!{\i0}
Dialogue: 0,0:12:45.52,0:12:47.26,EN,,0000,0000,0000,,Nè, nè, cậu có nghe không?
Dialogue: 0,0:12:47.26,0:12:50.73,EN,,0000,0000,0000,,Sáng này, họ bắt được một tên biến thái trên tàu!
Dialogue: 0,0:12:50.73,0:12:52.03,EN,,0000,0000,0000,,Không thể nào! Thật sao?
Dialogue: 0,0:12:52.03,0:12:54.23,EN,,0000,0000,0000,,Tớ nghe hắn ta còn khá trẻ!
Dialogue: 0,0:12:54.23,0:12:56.10,EN,,0000,0000,0000,,Thật kinh tởm, cậu chắc chứ?
Dialogue: 0,0:13:03.74,0:13:07.31,EN,,0000,0000,0000,,Lúc nào tôi cũng quan sát cẩn thận những người xung quanh mình.
Dialogue: 0,0:13:26.30,0:13:29.33,EN,,0000,0000,0000,,N-Ngôi nhà lớn quá...
Dialogue: 0,0:13:37.28,0:13:40.98,EN,,0000,0000,0000,,Cậu đến trễ đấy. Chị nghĩ là đã đưa cậu một cái bản đồ rất tuyệt mà.
Dialogue: 0,0:13:41.78,0:13:43.21,EN,,0000,0000,0000,,Ý chị là nó phải không?
Dialogue: 0,0:13:43.85,0:13:45.05,EN,,0000,0000,0000,,Đây là loại bản đồ gì-
Dialogue: 0,0:13:45.05,0:13:48.62,EN,,0000,0000,0000,,Đừng ngại, cứ vào đi, hiện giờ ở nhà không có ai cả.
Dialogue: 0,0:13:57.43,0:13:59.66,EN,,0000,0000,0000,,{\fs30\an5\pos(414,20)}Chú thích: Calpis một loại sữa chua lạnh.
Dialogue: 0,0:13:56.43,0:13:59.66,EN,,0000,0000,0000,,Cậu khát rồi phải không, thế Calpis thì sao?
Dialogue: 0,0:14:00.07,0:14:02.23,EN,,0000,0000,0000,,A! Thật là hoài niệm.
Dialogue: 0,0:14:04.04,0:14:07.67,EN,,0000,0000,0000,,Nhìn nè, nhìn nè! Trông cứ như tinh dịch vậy.
Dialogue: 0,0:14:08.15,0:14:09.86,EN,,0000,0000,0000,,Cái đó.
Dialogue: 0,0:14:14.21,0:14:16.15,EN,,0000,0000,0000,,Chị sẽ giúp cậu uống nó nhé?
Dialogue: 0,0:14:16.15,0:14:16.71,EN,,0000,0000,0000,,Hể?
Dialogue: 0,0:14:24.06,0:14:26.02,EN,,0000,0000,0000,,S-Senpai?
Dialogue: 0,0:14:37.74,0:14:42.07,EN,,0000,0000,0000,,Khác hẳn nước hoa, mùi hương ngọt ngào của một cô gái...
Dialogue: 0,0:14:43.41,0:14:48.58,EN,,0000,0000,0000,,Gần đây, chỉ cần mùi hương của senpai là đủ khiến cho tâm trí mình hoàn toàn trống rỗng.
Dialogue: 0,0:14:49.48,0:14:50.88,EN,,0000,0000,0000,,Có lẽ là phản ứng có điều kiện.
Dialogue: 0,0:14:54.45,0:14:58.39,EN,,0000,0000,0000,,Nếu bố mà thấy chúng ta đang làm chuyện như vầy, thì sẽ tệ lắm đấy.
Dialogue: 0,0:15:00.56,0:15:03.69,EN,,0000,0000,0000,,Có lẽ ống ấy sẽ giết cậu mất.
Dialogue: 0,0:15:03.70,0:15:05.70,EN,,0000,0000,0000,,C-Chị đang đùa nhỉ?
Dialogue: 0,0:15:06.13,0:15:07.20,EN,,0000,0000,0000,,Thật đấy.
Dialogue: 0,0:15:07.33,0:15:11.90,EN,,0000,0000,0000,,Lúc trước có một người con trai đến đây, \Nvà đã bị ông ấy tống ra khỏi của chỉ sau hai giây...
Dialogue: 0,0:15:12.17,0:15:14.77,EN,,0000,0000,0000,,Lần kế tiếp thì chị chỉ biết là cậu ta đã gào thét trong phòng của bố.
Dialogue: 0,0:15:14.91,0:15:15.81,EN,,0000,0000,0000,,Sau đó?
Dialogue: 0,0:15:16.01,0:15:16.87,EN,,0000,0000,0000,,Ai mà biết.
Dialogue: 0,0:15:21.15,0:15:22.45,EN,,0000,0000,0000,,Nó hết rồi.
Dialogue: 0,0:15:23.62,0:15:24.68,EN,,0000,0000,0000,,Chị sẽ rót lại
Dialogue: 0,0:15:24.68,0:15:27.42,EN,,0000,0000,0000,,A-A... Em đã hết khát rồi...
Dialogue: 0,0:15:27.72,0:15:29.25,EN,,0000,0000,0000,,Ý chị không phải vậy.
Dialogue: 0,0:15:29.45,0:15:31.22,EN,,0000,0000,0000,,Chị muốn rót từ đây ra cơ.
Dialogue: 0,0:15:35.33,0:15:36.73,EN,,0000,0000,0000,,S-Senpai.
Dialogue: 0,0:15:38.26,0:15:41.17,EN,,0000,0000,0000,,Nhìn này, bé ngoan vừa mới tỉnh giấc!
Dialogue: 0,0:15:41.90,0:15:44.70,EN,,0000,0000,0000,,Hay là em ấy tỉnh nãy giờ rồi hả?
Dialogue: 0,0:15:57.12,0:15:58.65,EN,,0000,0000,0000,,Lưỡi của chị có sướng không?
Dialogue: 0,0:16:00.45,0:16:04.36,EN,,0000,0000,0000,,Vâng, nó rất là mền mại.
Dialogue: 0,0:16:09.93,0:16:12.53,EN,,0000,0000,0000,,Chị sẽ đút nó vào khe nhỏ này.
Dialogue: 0,0:16:21.71,0:16:25.34,EN,,0000,0000,0000,,Senpai, lưỡi của chị vào rồi...
Dialogue: 0,0:16:57.84,0:17:01.18,EN,,0000,0000,0000,,Nhìn nè, nó đã được nhiêu đây rồi nè.
Dialogue: 0,0:17:01.68,0:17:04.38,EN,,0000,0000,0000,,Chị đang mong đợi chúng ta có thể làm đầy nó.
Dialogue: 0,0:17:04.38,0:17:06.68,EN,,0000,0000,0000,,Bố về rồi đây, Mizuho!
Dialogue: 0,0:17:08.75,0:17:10.15,EN,,0000,0000,0000,,Là bố của chị.
Dialogue: 0,0:17:12.42,0:17:14.12,EN,,0000,0000,0000,,Như vầy chắc sẽ có tắm máu đấy.
Dialogue: 0,0:17:14.13,0:17:17.86,EN,,0000,0000,0000,,Đ-Đừng bình tĩnh như vậy, cứu em với!
Dialogue: 0,0:17:17.86,0:17:19.86,EN,,0000,0000,0000,,Phải rồi. Lối này!
Dialogue: 0,0:17:21.70,0:17:23.03,EN,,0000,0000,0000,,Cứ trốn ở trong này!
Dialogue: 0,0:17:25.77,0:17:27.37,EN,,0000,0000,0000,,Gì vậy? Con ở đó à?
Dialogue: 0,0:17:27.71,0:17:29.11,EN,,0000,0000,0000,,Mừng bố đã về!
Dialogue: 0,0:17:29.51,0:17:32.01,EN,,0000,0000,0000,,Ta đói rồi, nấu cái gì đó cho ta đi.
Dialogue: 0,0:17:32.34,0:17:34.01,EN,,0000,0000,0000,,Thế Carbonara được không?
Dialogue: 0,0:17:34.01,0:17:36.01,EN,,0000,0000,0000,,Được. Phiền con quá.
Dialogue: 0,0:17:38.72,0:17:42.09,EN,,0000,0000,0000,,Tomohisa-kun, cậu vẫn còn nứng ư?
Dialogue: 0,0:17:42.99,0:17:46.92,EN,,0000,0000,0000,,Cửa đang mở, cậu cứ đút thằng nhỏ qua khe cửa đó...
Dialogue: 0,0:17:51.56,0:17:55.73,EN,,0000,0000,0000,,Sao thế? Lần này chị sẽ giữ nó trong cổ họng của mình.
Dialogue: 0,0:17:56.47,0:17:59.20,EN,,0000,0000,0000,,Cậu muốn ra bên trong miệng chị phải không?
Dialogue: 0,0:18:40.71,0:18:46.75,EN,,0000,0000,0000,,Ở bên trong tất cả đang bị trộn lên, với mì ống, và cả lưỡi của chị...
Dialogue: 0,0:19:16.92,0:19:17.85,EN,,0000,0000,0000,,Senpai...
Dialogue: 0,0:19:25.12,0:19:26.22,EN,,0000,0000,0000,,Chị bị nghẹn...
Dialogue: 0,0:19:27.39,0:19:31.40,EN,,0000,0000,0000,,Trong khi chị mang thức ăn cho ống ấy, cậu hãy chạy lên phòng trên tầng hai.
Dialogue: 0,0:19:31.56,0:19:32.33,EN,,0000,0000,0000,,Đã rõ.
Dialogue: 0,0:19:33.77,0:19:34.87,EN,,0000,0000,0000,,Đã khiến bố đợi lâu!
Dialogue: 0,0:19:35.60,0:19:37.37,EN,,0000,0000,0000,,Ừ... trong ngon quá.
Dialogue: 0,0:19:38.30,0:19:39.50,EN,,0000,0000,0000,,Là mèo đấy.
Dialogue: 0,0:19:40.57,0:19:43.57,EN,,0000,0000,0000,,Vậy, con sẽ về phòng của mình đây.
Dialogue: 0,0:19:46.41,0:19:47.71,EN,,0000,0000,0000,,Mọi chuyện ổn cả rồi.
Dialogue: 0,0:19:48.38,0:19:49.35,EN,,0000,0000,0000,,Vâng.
Dialogue: 0,0:19:49.85,0:19:52.52,EN,,0000,0000,0000,,Nào, cậu vẫn còn có thể ra nữa phải không?
Dialogue: 0,0:20:10.47,0:20:16.21,EN,,0000,0000,0000,,T-Tuyệt quá, âm hộ ẩm ướt của chị đang tạo nên những âm thanh dâm đãng!
Dialogue: 0,0:20:18.61,0:20:23.61,EN,,0000,0000,0000,,Senpai... âm hộ của chị sướng lắm.
Dialogue: 0,0:20:32.56,0:20:33.89,EN,,0000,0000,0000,,Có đau không?
Dialogue: 0,0:20:33.89,0:20:36.93,EN,,0000,0000,0000,,Nó đau, nhưng mà nó cũng sướng nữa...
Dialogue: 0,0:20:36.93,0:20:38.86,EN,,0000,0000,0000,,Vậy ư. Thế thì...
Dialogue: 0,0:20:51.84,0:20:54.04,EN,,0000,0000,0000,,Tuyệt quá, tuyệt quá!
Dialogue: 0,0:20:54.15,0:20:56.88,EN,,0000,0000,0000,,Nó đang co giật bên trong chị thật thô bạo!
Dialogue: 0,0:20:57.88,0:21:02.89,EN,,0000,0000,0000,,S-Senpai, nếu chị làm mạnh như vậy...
Dialogue: 0,0:21:03.89,0:21:07.59,EN,,0000,0000,0000,,Không được! Cậu phải giữ nó lại...
Dialogue: 0,0:21:29.51,0:21:30.71,EN,,0000,0000,0000,,Phải rồi...
Dialogue: 0,0:22:07.62,0:22:10.09,EN,,0000,0000,0000,,Lần này, cậu hãy ở trên.
Dialogue: 0,0:22:41.85,0:22:43.09,EN,,0000,0000,0000,,T-Tuyệt!
Dialogue: 0,0:22:50.56,0:22:51.43,EN,,0000,0000,0000,,Senpai...
Dialogue: 0,0:22:53.53,0:22:54.26,EN,,0000,0000,0000,,Em sắp...
Dialogue: 0,0:22:57.20,0:23:00.24,EN,,0000,0000,0000,,T-Tất cả vào trong miệng của chị, làm ơn!
Dialogue: 0,0:23:31.14,0:23:31.67,EN,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:24:29.46,0:24:33.23,EN,,0000,0000,0000,,Ha... Chị nghĩ mình sẽ có thai bằng miệng quá!
Dialogue: 0,0:24:33.40,0:24:35.16,EN,,0000,0000,0000,,Nhìn này! Không thể tin được!
Dialogue: 0,0:24:35.67,0:24:38.40,EN,,0000,0000,0000,,Xin lỗi, chúng ta xong rồi phải không?
Dialogue: 0,0:24:40.14,0:24:42.07,EN,,0000,0000,0000,,Mizuho, con rảnh không?
Dialogue: 0,0:24:44.47,0:24:45.68,EN,,0000,0000,0000,,Con có trong đó không?
Dialogue: 0,0:24:46.48,0:24:50.11,EN,,0000,0000,0000,,Xin lỗi, hôm nay cậu về đi nhé, trước khi ông ấy nổi điên lên vì hiểu lầm...
Dialogue: 0,0:24:50.11,0:24:51.98,EN,,0000,0000,0000,,N-Nhưng bằng cách nào?!
Dialogue: 0,0:24:52.15,0:24:53.78,EN,,0000,0000,0000,,Cậu có thể trốn ra bằng lối cửa sổ...
Dialogue: 0,0:24:53.92,0:24:55.79,EN,,0000,0000,0000,,Rồi. Giày của cậu!
Dialogue: 0,0:24:56.02,0:24:58.39,EN,,0000,0000,0000,,Còn đây là bản đồ đi đến trạm xe!
Dialogue: 0,0:25:00.16,0:25:01.09,EN,,0000,0000,0000,,Ta vào đây!
Dialogue: 0,0:25:01.89,0:25:03.83,EN,,0000,0000,0000,,Là mèo đấy. He...he...he~
Dialogue: 0,0:25:05.03,0:25:05.46,EN,,0000,0000,0000,,Hể?
Dialogue: 0,0:25:07.03,0:25:08.46,EN,,0000,0000,0000,,Mình biết ngay mà.
Dialogue: 0,0:25:12.07,0:25:16.61,EN,,0000,0000,0000,,Với senpai mình là cái gì cơ chứ?
Dialogue: 0,0:25:28.55,0:25:30.22,EN,,0000,0000,0000,,Còn tiếp...
Dialogue: 0,0:00:00.00,0:00:04.70,Default,,0000,0000,0000,,
