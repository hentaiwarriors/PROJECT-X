Dialogue: 0,0:00:02.09,0:00:05.02,Default,,0000,0000,0000,,( Project này thuộc về website : Baka-sub.net )
Dialogue: 0,0:00:05.29,0:00:09.07,Default,,0000,0000,0000,,Bạn ko nên khiêu chiến một cách bừa bãi
Dialogue: 0,0:00:10.56,0:00:14.66,Default,,0000,0000,0000,,Nếu đối phương quá mạnh, bạn phải có sự chuẩn bị kĩ lưỡng.
Dialogue: 0,0:00:15.30,0:00:18.25,Default,,0000,0000,0000,,Tấn công một gã khỏe mạnh mà ko có chuẩn bị gì là điều rất ngu ngốc.
Dialogue: 0,0:00:24.79,0:00:26.93,Default,,0000,0000,0000,,Có vẻ như đất nước tôi cũng khá là ngu ngốc.
Dialogue: 0,0:00:27.90,0:00:31.06,Default,,0000,0000,0000,,Họ ko hề suy nghĩ cẩn trọng một chút nào khi phát động chiến tranh.
Dialogue: 0,0:00:33.91,0:00:36.55,Default,,0000,0000,0000,,Còn giờ thì quá muộn rồi.
Dialogue: 0,0:00:43.03,0:00:48.23,Default,,0000,0000,0000,,DAIAKUJI
Dialogue: 0,0:03:11.99,0:03:13.68,Default,,0000,0000,0000,,(Tại sao? Tại sao? DAIAKUJI)
Dialogue: 0,0:03:15.49,0:03:16.22,Default,,0000,0000,0000,,Cái quái gì thế này?
Dialogue: 0,0:03:16.59,0:03:19.29,Default,,0000,0000,0000,,Sao tôi lại bé thế này nhỉ?
Dialogue: 0,0:03:19.49,0:03:21.36,Default,,0000,0000,0000,,Lại còn cái quang cảnh hài hước này nữa.
Dialogue: 0,0:03:21.36,0:03:21.81,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:03:22.37,0:03:24.40,Default,,0000,0000,0000,,Sat-chan, cô trông ko khác bình thường là mấy nhỉ.
Dialogue: 0,0:03:24.95,0:03:25.71,Default,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:03:26.34,0:03:30.74,Default,,0000,0000,0000,,"Có chút rắc rối khi giải thích mối quan hệ giữa các nhân vật trong bộ phim này."
Dialogue: 0,0:03:30.83,0:03:32.07,Default,,0000,0000,0000,,Đúng đấy chứ
Dialogue: 0,0:03:32.49,0:03:35.75,Default,,0000,0000,0000,,"Và vì thế những cảnh nóng sẽ bị cắt ngắn đi bởi chuyện này."
Dialogue: 0,0:03:35.75,0:03:36.94,Default,,0000,0000,0000,,Cái này thì ko ổn chút nào.
Dialogue: 0,0:03:37.19,0:03:40.98,Default,,0000,0000,0000,,"Thế nên tôi cho phép giải thích điều đó ngay tại đây."
Dialogue: 0,0:03:40.98,0:03:41.77,Default,,0000,0000,0000,,Thằng nào viết thế?
Dialogue: 0,0:03:42.18,0:03:44.47,Default,,0000,0000,0000,,Đó là chủ ý của đạo diễn.
Dialogue: 0,0:03:44.47,0:03:45.31,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:03:45.34,0:03:46.08,Default,,0000,0000,0000,,(OUIME)
Dialogue: 0,0:03:46.62,0:03:47.78,Default,,0000,0000,0000,,Đầu tiên, Ouime.
Dialogue: 0,0:03:47.85,0:03:49.22,Default,,0000,0000,0000,,Tôi giải thích cái này ấy hả?
Dialogue: 0,0:03:49.80,0:03:52.99,Default,,0000,0000,0000,,Quốc gia này, Ouime là đất nước chiếm đóng quốc gia của tôi.
Dialogue: 0,0:03:52.99,0:03:55.44,Default,,0000,0000,0000,,Đúng vậy. Chúng ta đã thua trận.
Dialogue: 0,0:03:55.65,0:03:57.44,Default,,0000,0000,0000,,Thế nên điều đó là hiển nhiên.
Dialogue: 0,0:03:58.17,0:04:00.13,Default,,0000,0000,0000,,Ouime là đất nước mà ở đó phụ nữ nắm mọi quyền hành trong tay.
Dialogue: 0,0:04:00.45,0:04:02.13,Default,,0000,0000,0000,,Phụ nữ ở đó có địa vị cao hơn đàn ông.
Dialogue: 0,0:04:02.54,0:04:05.61,Default,,0000,0000,0000,,Đó là lí do tại sao ở đất nước này quân đội, các công ti...
Dialogue: 0,0:04:06.21,0:04:10.12,Default,,0000,0000,0000,,và thậm chí là các cửa hàng bán đồ lưu niệm cũng do phụ nữ cai quản và quy định luật lệ.
Dialogue: 0,0:04:10.82,0:04:14.76,Default,,0000,0000,0000,,Nếu bạn ko tuân thủ luật lệ, bạn sẽ bị tống giam như một tên côn đồ.
Dialogue: 0,0:04:14.76,0:04:15.34,Default,,0000,0000,0000,,(WAKAME GUMI)
Dialogue: 0,0:04:15.86,0:04:18.64,Default,,0000,0000,0000,,Wakame-gumi cũng thế, nơi mà Akuji là một trong những người có quyền kế vị.
Dialogue: 0,0:04:19.25,0:04:22.27,Default,,0000,0000,0000,,Wakame-gumi thuộc sở hữu của ông ngoại tôi.
Dialogue: 0,0:04:22.27,0:04:26.33,Default,,0000,0000,0000,,Ông ta sở hữu nó một cách hợp pháp nhờ việc bắt bồ với chính con gái của mình.
Dialogue: 0,0:04:26.88,0:04:29.81,Default,,0000,0000,0000,,Riêng tôi thì chả thấy hợp pháp ở chỗ nào cả.
Dialogue: 0,0:04:30.08,0:04:32.48,Default,,0000,0000,0000,,Nhưng có vẻ điều này giống như những việc mà cha tôi hay làm.
Dialogue: 0,0:04:32.48,0:04:36.97,Default,,0000,0000,0000,,Đúng đấy. Ông ngoại tôi đã làm cho nhiều phụ nữ mang thai, và thế là sinh ra cô.
Dialogue: 0,0:04:37.34,0:04:39.97,Default,,0000,0000,0000,,"Nhiều phụ nữ là sao?" Sao anh dám nói về tôi với thái độ đó hả?
Dialogue: 0,0:04:39.97,0:04:41.54,Default,,0000,0000,0000,,Xin lỗi. Tôi ko ám chỉ cô đâu.
Dialogue: 0,0:04:41.96,0:04:45.73,Default,,0000,0000,0000,,Đừng có quên rằng: Tôi, Satsu Takega là dì của anh đấy.
Dialogue: 0,0:04:45.95,0:04:46.93,Default,,0000,0000,0000,,Thì thế nên tôi đã xin lỗi cô rồi mà.
Dialogue: 0,0:04:47.12,0:04:48.49,Default,,0000,0000,0000,,Tôi xin lỗi!
Dialogue: 0,0:04:48.93,0:04:50.30,Default,,0000,0000,0000,,Coi chừng cái miệng anh đó nghe chưa.
Dialogue: 0,0:04:51.53,0:04:56.84,Default,,0000,0000,0000,,Tôi biết là tôi có rất nhiều cô dì chú bác mà, có điều tôi chả biết một ai cả.
Dialogue: 0,0:04:57.56,0:04:59.62,Default,,0000,0000,0000,,Và số lượng này sẽ còn tăng nữa.
Dialogue: 0,0:05:00.75,0:05:04.30,Default,,0000,0000,0000,,Sao cũng được, ông ngoại tôi rời gia đình, và đi chu du khắp nơi.
Dialogue: 0,0:05:04.71,0:05:06.54,Default,,0000,0000,0000,,Còn cha tôi thì đã tử trận trong cuộc chiến.
Dialogue: 0,0:05:07.36,0:05:09.45,Default,,0000,0000,0000,,Gia đình tôi đã li tán.
Dialogue: 0,0:05:10.06,0:05:16.49,Default,,0000,0000,0000,,Sau khi tôi ra tù, tình nhân của ông ngoại tôi đã nắm toàn bộ quyền lực.
Dialogue: 0,0:05:16.99,0:05:21.15,Default,,0000,0000,0000,,Mọi quyền hành đều nằm trong tay phụ nữ, giống hệt như ở Ouime.
Dialogue: 0,0:05:22.46,0:05:24.18,Default,,0000,0000,0000,,Tôi đã cố trở về nhà.
Dialogue: 0,0:05:24.42,0:05:29.67,Default,,0000,0000,0000,,Nhưng bà ta đã ra lệnh cho người của mình đánh gục tôi và quăng tôi ra ngoài.
Dialogue: 0,0:05:30.45,0:05:31.24,Default,,0000,0000,0000,,Anh yếu quá.
Dialogue: 0,0:05:31.68,0:05:33.66,Default,,0000,0000,0000,,Hãy rèn luyện chăm chỉ thêm nữa đi.
Dialogue: 0,0:05:34.09,0:05:37.91,Default,,0000,0000,0000,,Tôi vừa mới ra tù. Làm thế quái nào tôi có đủ sức lực được chứ.
Dialogue: 0,0:05:38.90,0:05:40.79,Default,,0000,0000,0000,,Thế bây giờ anh đang làm gì?
Dialogue: 0,0:05:41.61,0:05:45.49,Default,,0000,0000,0000,,Tôi được cứu bởi Yoko Aoba, người đứng đầu một tổ chức tình nguyện.
Dialogue: 0,0:05:45.93,0:05:48.75,Default,,0000,0000,0000,,Để trả ơn cô ấy, tôi đã gia nhập tổ chức.
Dialogue: 0,0:05:49.95,0:05:51.34,Default,,0000,0000,0000,,Tổ chức tình nguyện?
Dialogue: 0,0:05:51.98,0:05:52.94,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:05:53.86,0:05:56.76,Default,,0000,0000,0000,,Nó được thành lập bởi những người có xu hướng duy tâm.
Dialogue: 0,0:05:57.14,0:06:00.26,Default,,0000,0000,0000,,Đây chỉ thuần túy là tổ chức tình nguyện. Tôi khẳng định như vậy.
Dialogue: 0,0:06:00.54,0:06:01.81,Default,,0000,0000,0000,,Những cái này viết hết trong sách mà?
Dialogue: 0,0:06:02.42,0:06:03.76,Default,,0000,0000,0000,,Thế họ hoạt động ra sao?
Dialogue: 0,0:06:04.51,0:06:07.53,Default,,0000,0000,0000,,Thu nhập của chúng tôi thông qua các cửa hàng và...
Dialogue: 0,0:06:07.86,0:06:11.91,Default,,0000,0000,0000,,việc điều hành các tụ điểm giải trí dành cho người lớn.
Dialogue: 0,0:06:12.82,0:06:14.72,Default,,0000,0000,0000,,Đây là cái gọi là "công việc tình nguyện" đấy hả?
Dialogue: 0,0:06:15.01,0:06:16.30,Default,,0000,0000,0000,,Vì tôi đã ở đây.
Dialogue: 0,0:06:17.27,0:06:18.95,Default,,0000,0000,0000,,Nên bây giờ nơi này đã trở nên quá nhỏ bé với tôi.
Dialogue: 0,0:06:19.54,0:06:23.57,Default,,0000,0000,0000,,Nhưng một ngày nào đó, nơi này sẽ vượt qua cái ổ chứa ở Osaka.
Dialogue: 0,0:06:23.57,0:06:24.10,Default,,0000,0000,0000,,(Tình hình OSAKA)
Dialogue: 0,0:06:24.50,0:06:25.16,Default,,0000,0000,0000,,Dừng lại nào.
Dialogue: 0,0:06:25.74,0:06:28.22,Default,,0000,0000,0000,,Tình hình Osaka hiện giờ thế nào rồi?
Dialogue: 0,0:06:29.33,0:06:30.42,Default,,0000,0000,0000,,Trước tiên sẽ là Wakame-gumi.
Dialogue: 0,0:06:30.97,0:06:32.26,Default,,0000,0000,0000,,Kẻ nắm quyền ở đây là Ichihashi Ran.
Dialogue: 0,0:06:32.88,0:06:36.03,Default,,0000,0000,0000,,Bà ta là một người phụ nữ dâm đãng, người đã chiếm lấy gia đình này bằng cách quyến rũ ông ngoại tôi.
Dialogue: 0,0:06:37.01,0:06:38.21,Default,,0000,0000,0000,,Haruko Tsujiya làm việc cho bà ta.
Dialogue: 0,0:06:39.05,0:06:43.57,Default,,0000,0000,0000,,Cô ta là vợ của một trong số các đệ tử của tôi, nhưng cô ta lại rất căm ghét tôi.
Dialogue: 0,0:06:45.12,0:06:47.88,Default,,0000,0000,0000,,Motoko Kaga. Tôi gọi cô ấy là Toko.
Dialogue: 0,0:06:48.38,0:06:50.88,Default,,0000,0000,0000,,Cha cô ấy là một thành viên của ủy ban chấp pháp ở Wakame-gumi.
Dialogue: 0,0:06:51.41,0:06:52.94,Default,,0000,0000,0000,,Hiện giờ ko biết ông ta ở đâu.
Dialogue: 0,0:06:53.88,0:06:56.60,Default,,0000,0000,0000,,Nhưng dù sao, hồi nhỏ hai chúng tôi chơi với nhau rất thân.
Dialogue: 0,0:06:57.92,0:07:02.75,Default,,0000,0000,0000,,Đây là Ai Morita, người rất mê phim về các hiệp sĩ. Và còn nhiều người khác nữa.
Dialogue: 0,0:07:03.58,0:07:05.84,Default,,0000,0000,0000,,Và cả tổ chức tình nguyện của tôi nữa.
Dialogue: 0,0:07:06.41,0:07:08.58,Default,,0000,0000,0000,,Người lãnh đạo tổ chức, như tôi đã nói ở trên. Yoko Aoba.
Dialogue: 0,0:07:09.62,0:07:12.26,Default,,0000,0000,0000,,Có cả Mirai Kako và Tomoko Kinugasa nữa.
Dialogue: 0,0:07:12.68,0:07:16.29,Default,,0000,0000,0000,,Tổ chức có quy mô còn tương đối nhỏ, nhưng với tài năng của tôi, nó sẽ...
Dialogue: 0,0:07:16.29,0:07:17.91,Default,,0000,0000,0000,,Tôi nghe anh nói điều này rồi.
Dialogue: 0,0:07:19.11,0:07:21.46,Default,,0000,0000,0000,,Oh,cô ấy là Minka Haku.
Dialogue: 0,0:07:22.07,0:07:23.65,Default,,0000,0000,0000,,Cô ấy mở một hiệu ăn ở Kobe.
Dialogue: 0,0:07:24.34,0:07:28.06,Default,,0000,0000,0000,,Từ khi tôi yêu cô ấy cách đây sáu năm, tôi đã luôn mời cô ấy đi chơi.
Dialogue: 0,0:07:28.57,0:07:30.80,Default,,0000,0000,0000,,Nhưng cô ấy vẫn từ chối tôi vì cô ấy ko thích xã hội đen.
Dialogue: 0,0:07:31.49,0:07:34.74,Default,,0000,0000,0000,,Tôi muốn cô ấy thuộc về tôi. Nhưng tôi ko thể sử dụng vũ lực cho chuyện này được.
Dialogue: 0,0:07:36.02,0:07:37.00,Default,,0000,0000,0000,,Quay lại vấn đề chính nào...
Dialogue: 0,0:07:37.86,0:07:41.11,Default,,0000,0000,0000,,Nako-kyo đang dần củng cố vững chắc vị thế của mình.
Dialogue: 0,0:07:41.88,0:07:44.82,Default,,0000,0000,0000,,Nói một cách trang trọng, phải gọi là Kanze-nago-shintou-kyo.
Dialogue: 0,0:07:45.37,0:07:47.49,Default,,0000,0000,0000,,Người sáng lập ra Nako-kyo là Yume.
Dialogue: 0,0:07:48.63,0:07:51.19,Default,,0000,0000,0000,,Cô ta được coi như một vị thánh sống.
Dialogue: 0,0:07:51.19,0:07:53.49,Default,,0000,0000,0000,,Nhưng có vẻ như có điều gì đó ám muội.
Dialogue: 0,0:07:54.97,0:07:57.89,Default,,0000,0000,0000,,Hai người hầu cận cô ta là Yoko Komiya và Nene Tsukise.
Dialogue: 0,0:07:59.33,0:08:01.72,Default,,0000,0000,0000,,Họ được gọi là các thánh nữ. Thật chả có gì hay ho cả.
Dialogue: 0,0:08:02.32,0:08:02.93,Default,,0000,0000,0000,,(Những vấn đề khác)
Dialogue: 0,0:08:03.34,0:08:07.62,Default,,0000,0000,0000,,Wakame-gumi và Nako-kyo hiện giờ đang nắm quyền kiểm soát Osaka .
Dialogue: 0,0:08:08.28,0:08:13.64,Default,,0000,0000,0000,,Ouime thì đang kiểm soát tất cả, và còn nhiều gã khốn khác ở Osaka nữa.
Dialogue: 0,0:08:13.94,0:08:19.55,Default,,0000,0000,0000,,Ko cần biết chúng là ai, tôi sẽ hạ gục nếu là đàn ôn và cưỡng bức nếu là phụ nữ. Thế là ổn.
Dialogue: 0,0:08:20.27,0:08:23.48,Default,,0000,0000,0000,,Được rồi, còn những thứ quái quỉ này.
Dialogue: 0,0:08:24.00,0:08:25.94,Default,,0000,0000,0000,,Sao bọn chúng lại có trong phim nhỉ?
Dialogue: 0,0:08:26.37,0:08:28.59,Default,,0000,0000,0000,,Thôi kệ, tôi sẽ làm tất cả những điều cần làm.
Dialogue: 0,0:08:28.83,0:08:29.27,Default,,0000,0000,0000,,Và...
Dialogue: 0,0:08:29.86,0:08:32.65,Default,,0000,0000,0000,,Và đến đây là chấm dứt việc giới thiệu.
Dialogue: 0,0:08:32.70,0:08:34.40,Default,,0000,0000,0000,,Tất cả những việc tôi phải làm đó là phải tàn nhẫn hơn nữa.
Dialogue: 0,0:08:34.61,0:08:36.31,Default,,0000,0000,0000,,Tôi có nên như thế ko nhỉ?
Dialogue: 0,0:08:36.43,0:08:38.61,Default,,0000,0000,0000,,Yeah. Tôi mong điều đó sớm xảy ra.
Dialogue: 0,0:08:39.32,0:08:41.21,Default,,0000,0000,0000,,Anh vừa nói "sớm" hả?
Dialogue: 0,0:08:41.43,0:08:43.10,Default,,0000,0000,0000,,Tôi hi vọng vào một tương lai tươi sáng hơn.
Dialogue: 0,0:08:45.94,0:08:47.05,Default,,0000,0000,0000,,Chúc may mắn.
Dialogue: 0,0:08:49.30,0:08:51.37,Default,,0000,0000,0000,,Tôi sẽ tới chỗ Tamanegi xem thế nào.
Dialogue: 0,0:09:02.74,0:09:04.05,Default,,0000,0000,0000,,Oh. Chào anh.
Dialogue: 0,0:09:05.60,0:09:06.73,Default,,0000,0000,0000,,Cô ta là người kế tiếp hả?
Dialogue: 0,0:09:08.70,0:09:09.35,Default,,0000,0000,0000,,Nhờ cả vào ông đấy.
Dialogue: 0,0:09:10.17,0:09:11.35,Default,,0000,0000,0000,,Cứ giao cô ta cho tôi.
Dialogue: 0,0:09:19.89,0:09:20.55,Default,,0000,0000,0000,,Tôi đang ở đâu thế này?
Dialogue: 0,0:09:22.19,0:09:22.79,Default,,0000,0000,0000,,Đây là nơi nào?
Dialogue: 0,0:09:25.40,0:09:26.64,Default,,0000,0000,0000,,Gì thế này?!
Dialogue: 0,0:09:29.28,0:09:30.23,Default,,0000,0000,0000,,Ông là ai?
Dialogue: 0,0:09:30.92,0:09:33.94,Default,,0000,0000,0000,,Tên tôi là Tamanegi.
Dialogue: 0,0:09:34.46,0:09:40.10,Default,,0000,0000,0000,,Tôi làm việc cho chủ nhân của tôi, Akuji. Tôi là người huấn luyện riêng của anh ấy.
Dialogue: 0,0:09:42.53,0:09:42.85,Default,,0000,0000,0000,,Ko
Dialogue: 0,0:09:43.91,0:09:46.04,Default,,0000,0000,0000,,Dừng lại, nhột quá.
Dialogue: 0,0:09:49.86,0:09:52.24,Default,,0000,0000,0000,,Dừng lại! Đừng động vào chỗ đó!
Dialogue: 0,0:09:52.84,0:09:55.78,Default,,0000,0000,0000,,Có vẻ như cô chưa biết chút gì về bổn phận của một phụ nữ.
Dialogue: 0,0:09:56.61,0:10:00.73,Default,,0000,0000,0000,,Trước tiên có lẽ cô cần phải được chỉ bảo về điều này.
Dialogue: 0,0:10:04.39,0:10:07.16,Default,,0000,0000,0000,,Ko, ông làm cái gì vậy?
Dialogue: 0,0:10:12.23,0:10:13.59,Default,,0000,0000,0000,,Thật kinh khủng.
Dialogue: 0,0:10:15.16,0:10:16.17,Default,,0000,0000,0000,,Nhột quá.
Dialogue: 0,0:10:18.56,0:10:20.55,Default,,0000,0000,0000,,Đau quá! Đau quá!
Dialogue: 0,0:10:20.86,0:10:22.27,Default,,0000,0000,0000,,Dừng lại. Cứu tôi với!
Dialogue: 0,0:10:23.77,0:10:24.40,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:10:24.81,0:10:25.99,Default,,0000,0000,0000,,Làm ơn đừng dừng lại.
Dialogue: 0,0:10:33.86,0:10:35.46,Default,,0000,0000,0000,,Cở thể cô căng ra, sau đó thư giãn.
Dialogue: 0,0:10:35.46,0:10:40.77,Default,,0000,0000,0000,,Sự kết hợp này sẽ làm tê liệt trí não cô ta, và làm nó ngập tràn trong phấn khích.
Dialogue: 0,0:10:52.80,0:10:54.40,Default,,0000,0000,0000,,Cô đã ướt thế này rồi à.
Dialogue: 0,0:10:56.21,0:10:58.23,Default,,0000,0000,0000,,Có triển vọng đấy.
Dialogue: 0,0:11:07.21,0:11:10.44,Default,,0000,0000,0000,,Cố lên nào. Hãy cho ta thấy con người thực của cô.
Dialogue: 0,0:11:10.58,0:11:12.68,Default,,0000,0000,0000,,Biết làm sao được...
Dialogue: 0,0:11:13.46,0:11:15.66,Default,,0000,0000,0000,,Tôi đột nhiên có cảm giác rất kì lạ.
Dialogue: 0,0:11:21.22,0:11:24.27,Default,,0000,0000,0000,,Đây là lần đầu tiên như thế này.
Dialogue: 0,0:11:27.88,0:11:29.00,Default,,0000,0000,0000,,Cái gì thế?
Dialogue: 0,0:11:30.83,0:11:32.25,Default,,0000,0000,0000,,Ko. Đừng.
Dialogue: 0,0:11:33.35,0:11:35.38,Default,,0000,0000,0000,,Ko, ông đang làm cái gì vậy?
Dialogue: 0,0:11:35.60,0:11:37.05,Default,,0000,0000,0000,,Dừng lại! Dừng ngay lại!
Dialogue: 0,0:11:50.78,0:11:52.29,Default,,0000,0000,0000,,Ko. Thứ gì đó...
Dialogue: 0,0:11:52.52,0:11:54.95,Default,,0000,0000,0000,,Thứ gì đó, thứ gì đó đang ra.
Dialogue: 0,0:11:55.01,0:11:57.90,Default,,0000,0000,0000,,Tôi sợ quá, Tôi sợ quá! Giúp tôi với!
Dialogue: 0,0:11:58.31,0:11:59.98,Default,,0000,0000,0000,,Cô có thể ra mà.
Dialogue: 0,0:12:16.89,0:12:19.16,Default,,0000,0000,0000,,Cuộc huấn luyện rất khả quan.
Dialogue: 0,0:12:19.23,0:12:21.14,Default,,0000,0000,0000,,Tôi sẽ để anh tự mình xem xét.
Dialogue: 0,0:12:21.44,0:12:22.91,Default,,0000,0000,0000,,Để xem nào.
Dialogue: 0,0:12:41.41,0:12:42.86,Default,,0000,0000,0000,,Ông làm tốt lắm, Tamanegi.
Dialogue: 0,0:12:43.55,0:12:46.74,Default,,0000,0000,0000,,Ông đã huấn luyện thành công một cô gái hoàn toàn chưa có kinh nghiệm về chuyện này.
Dialogue: 0,0:12:48.80,0:12:49.96,Default,,0000,0000,0000,,Đây mới chỉ là sự khởi đầu thôi.
Dialogue: 0,0:12:51.23,0:12:54.76,Default,,0000,0000,0000,,Cô ta cực kì có triển vọng đấy.
Dialogue: 0,0:12:59.80,0:13:02.30,Default,,0000,0000,0000,,Đừng quên bổn phận của cô.
Dialogue: 0,0:13:02.60,0:13:05.91,Default,,0000,0000,0000,,Hãy để ông chủ của ta thưởng thức co.
Dialogue: 0,0:13:24.41,0:13:25.33,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:13:43.98,0:13:45.98,Default,,0000,0000,0000,,Cô ta chắc chắn vẫn còn trinh.
Dialogue: 0,0:14:04.51,0:14:05.22,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:14:06.51,0:14:09.53,Default,,0000,0000,0000,,Đau quá. Hey don't run away.
Dialogue: 0,0:14:21.42,0:14:24.15,Default,,0000,0000,0000,,Cô cũng cảm thấy sướng, huh!
Dialogue: 0,0:14:24.45,0:14:25.89,Default,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,0:14:27.79,0:14:28.85,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:14:31.84,0:14:33.40,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:14:49.60,0:14:51.65,Default,,0000,0000,0000,,Oh, ko!
Dialogue: 0,0:14:59.07,0:15:00.55,Default,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,0:15:06.23,0:15:09.00,Default,,0000,0000,0000,,Cô có thể mang lại cho chúng ta rất nhiều tiền.
Dialogue: 0,0:15:11.61,0:15:13.74,Default,,0000,0000,0000,,Tôi ra rồi! Tôi ra rồi!
Dialogue: 0,0:15:13.74,0:15:18.48,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:15:27.69,0:15:29.51,Default,,0000,0000,0000,,Một khách hàng đang tới!
Dialogue: 0,0:15:34.55,0:15:35.86,Default,,0000,0000,0000,,Chào mừng ngài!
