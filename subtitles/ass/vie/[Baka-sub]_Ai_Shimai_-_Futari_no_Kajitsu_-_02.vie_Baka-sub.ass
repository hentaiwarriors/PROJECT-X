Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:16.73,0:00:18.19,Default,,0000,0000,0000,,Làm ơn hãy tha cho tôi.
Dialogue: 0,0:00:18.26,0:00:21.96,Default,,0000,0000,0000,,¥200,000 là khoản tiền quá lớn đối với tôi.
Dialogue: 0,0:00:22.40,0:00:27.43,Default,,0000,0000,0000,,Vậy thì cô sẽ mãi mãi thuộc về tôi, theo cách này, Yukie.
Dialogue: 0,0:00:55.76,0:00:57.70,Default,,0000,0000,0000,,Cô ta đã bị chinh phục hoàn toàn.
Dialogue: 0,0:00:57.77,0:00:59.93,Default,,0000,0000,0000,,Còn khoản tiền đã thỏa thuận thì sao?
Dialogue: 0,0:01:00.00,0:01:02.30,Default,,0000,0000,0000,,Anh đã làm cho nó biến mất hoàn toàn khỏi tâm trí cô ta.
Dialogue: 0,0:01:02.50,0:01:04.47,Default,,0000,0000,0000,,Làm tốt lắm, Taketo.
Dialogue: 0,0:01:06.88,0:01:08.40,Default,,0000,0000,0000,,Yumi...
Dialogue: 0,0:01:08.58,0:01:10.17,Default,,0000,0000,0000,,...anh thật xấu xa phải ko?
Dialogue: 0,0:01:17.09,0:01:19.35,Default,,0000,0000,0000,,Một giọt máu đào hơn ao nước lã.
Dialogue: 0,0:01:19.42,0:01:21.51,Default,,0000,0000,0000,,Nếu anh làm chuyện này.
Dialogue: 0,0:01:22.32,0:01:26.32,Default,,0000,0000,0000,,Cái gì? Nhưng anh yêu em mà.
Dialogue: 0,0:01:28.00,0:01:29.62,Default,,0000,0000,0000,,Ko. Bây giờ chưa phải lúc.
Dialogue: 0,0:01:30.23,0:01:31.76,Default,,0000,0000,0000,,Chưa phải lúc ư?
Dialogue: 0,0:01:32.13,0:01:35.43,Default,,0000,0000,0000,,Lúc này anh chưa đủ sức đối đầu với cha anh được đâu, Taketo.
Dialogue: 0,0:01:40.08,0:01:43.98,Default,,0000,0000,0000,,Immoral Sisters\N~Đôi quả ngọt chín sớm~
Dialogue: 0,0:01:48.18,0:01:50.01,Default,,0000,0000,0000,,Đêm thứ hai\N"Nữ sinh gương mẫu sa ngã"
Dialogue: 0,0:01:52.42,0:01:54.62,Default,,0000,0000,0000,,Phần A:\NRumi, nữ sinh gương mẫu
Dialogue: 0,0:02:06.47,0:02:08.10,Default,,0000,0000,0000,,Chúng ta quen nhau hả?
Dialogue: 0,0:02:09.37,0:02:11.03,Default,,0000,0000,0000,,Muốn biết tôi là ai thì coi cái này đi.
Dialogue: 0,0:02:16.51,0:02:20.74,Default,,0000,0000,0000,,Chuyện gì sẽ xảy ra nếu tôi đưa nó cho cha cô nhỉ, liệu ông ta có bị sa thải ko?
Dialogue: 0,0:02:54.05,0:02:56.52,Default,,0000,0000,0000,,Giờ nghỉ trưa kết thúc rồi. Tôi phải quay về lớp.
Dialogue: 0,0:02:56.59,0:02:59.35,Default,,0000,0000,0000,,Này, chúng ta vẫn chưa nói xong mà.
Dialogue: 0,0:03:00.39,0:03:02.19,Default,,0000,0000,0000,,Anh muốn gì ở tôi?
Dialogue: 0,0:03:02.26,0:03:05.32,Default,,0000,0000,0000,,Cô bắt đầu thông minh hơn rồi đấy.
Dialogue: 0,0:03:07.96,0:03:09.36,Default,,0000,0000,0000,,Đừng động vào người tôi.
Dialogue: 0,0:03:09.43,0:03:11.02,Default,,0000,0000,0000,,Này, này.
Dialogue: 0,0:03:12.77,0:03:14.67,Default,,0000,0000,0000,,Cô ta sao thế nhỉ?
Dialogue: 0,0:03:19.34,0:03:20.74,Default,,0000,0000,0000,,Đợi đã!
Dialogue: 0,0:03:21.14,0:03:25.08,Default,,0000,0000,0000,,Em là Tomoko Kitazawa, phải ko?
Dialogue: 0,0:03:25.28,0:03:26.34,Default,,0000,0000,0000,,Vâng ạ?
Dialogue: 0,0:03:26.41,0:03:30.32,Default,,0000,0000,0000,,Rumi khó gần nhỉ. Cô ấy luôn luôn như vậy sao?
Dialogue: 0,0:03:30.75,0:03:32.65,Default,,0000,0000,0000,,Ko hẳn là như vậy...
Dialogue: 0,0:03:32.92,0:03:35.98,Default,,0000,0000,0000,,Trông em ko có vẻ gì là khó gần như chị em.
Dialogue: 0,0:03:36.06,0:03:38.58,Default,,0000,0000,0000,,Anh là Taketo. Chúng ta kết bạn nhé.
Dialogue: 0,0:03:39.96,0:03:42.29,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:03:42.70,0:03:44.32,Default,,0000,0000,0000,,Này, đợi đã.
Dialogue: 0,0:03:46.80,0:03:48.93,Default,,0000,0000,0000,,Sao thế nhỉ?
Dialogue: 0,0:04:00.28,0:04:02.81,Default,,0000,0000,0000,,Kitazawa, em ko sao chứ?
Dialogue: 0,0:04:05.59,0:04:07.08,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:04:07.69,0:04:08.68,Default,,0000,0000,0000,,Câu lạc bộ Tennis
Dialogue: 0,0:04:11.89,0:04:13.66,Default,,0000,0000,0000,,<i>Mình ko thể do dự được.</i>
Dialogue: 0,0:04:13.93,0:04:16.56,Default,,0000,0000,0000,,<i>Mình cần làm việc này để bảo vệ gia đình mình.</i>
Dialogue: 0,0:04:20.27,0:04:23.33,Default,,0000,0000,0000,,Rumi, vậy là cô được nghỉ buổi học hôm nay hả.
Dialogue: 0,0:04:24.04,0:04:27.07,Default,,0000,0000,0000,,Tôi đã giả vờ trượt chân khi tập luyện.
Dialogue: 0,0:04:27.14,0:04:30.87,Default,,0000,0000,0000,,Tốt lắm. Cô còn chuyện quan trọng hơn phải làm.
Dialogue: 0,0:04:31.25,0:04:35.81,Default,,0000,0000,0000,,Ngài hiệu trưởng đang rất nóng lòng được gặp cô đó.
Dialogue: 0,0:04:48.06,0:04:49.33,Default,,0000,0000,0000,,Tôi ra ngay đây.
Dialogue: 0,0:04:49.93,0:04:51.52,Default,,0000,0000,0000,,Ai đó?
Dialogue: 0,0:04:52.37,0:04:55.39,Default,,0000,0000,0000,,Chủ nhân của cô về rồi đây.
Dialogue: 0,0:04:57.14,0:05:01.04,Default,,0000,0000,0000,,Hai đứa con gái của cô thật chẳng biết điều gì cả.
Dialogue: 0,0:05:02.01,0:05:03.60,Default,,0000,0000,0000,,Cậu ko có...
Dialogue: 0,0:05:03.68,0:05:07.14,Default,,0000,0000,0000,,Chết tiệt. Cả hai người bọn họ đều từ chối tôi!
Dialogue: 0,0:05:10.72,0:05:12.24,Default,,0000,0000,0000,,Tôi sẽ bắt cô trả giá cho chuyện này.
Dialogue: 0,0:05:12.32,0:05:13.65,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:05:13.72,0:05:15.52,Default,,0000,0000,0000,,Cởi đồ ra mau lên.
Dialogue: 0,0:05:16.86,0:05:20.85,Default,,0000,0000,0000,,Sao thế? Chẳng phài cô nói sẽ làm theo bất cứ điều gì tôi yêu cầu sao?
Dialogue: 0,0:05:20.93,0:05:25.63,Default,,0000,0000,0000,,Hay cô đã quên khoản tiền ¥200,000 rồi?\NTôi vẫn đang giữ bản giao kèo đó đây.
Dialogue: 0,0:05:33.94,0:05:37.38,Default,,0000,0000,0000,,Nghe máy đi. Chồng cô lại gọi nữa, phải ko?
Dialogue: 0,0:05:41.85,0:05:43.51,Default,,0000,0000,0000,,Chào ông xã.
Dialogue: 0,0:05:43.79,0:05:45.31,Default,,0000,0000,0000,,Vâng, vâng.
Dialogue: 0,0:05:45.52,0:05:47.32,Default,,0000,0000,0000,,Ba mẹ con em vẫn khỏe.
Dialogue: 0,0:05:49.76,0:05:51.75,Default,,0000,0000,0000,,Ko có chuyện gì bất thường cả.
Dialogue: 0,0:05:55.60,0:06:00.03,Default,,0000,0000,0000,,Ông xã, em nhớ anh lắm.
Dialogue: 0,0:06:00.64,0:06:02.23,Default,,0000,0000,0000,,Em muốn chuyện đó.
Dialogue: 0,0:06:03.67,0:06:07.20,Default,,0000,0000,0000,,Vì thế mà...
Dialogue: 0,0:06:14.65,0:06:16.38,Default,,0000,0000,0000,,Sao anh có thể...?
Dialogue: 0,0:06:16.45,0:06:21.22,Default,,0000,0000,0000,,Anh muốn em phải đợi bao lâu nữa?
Dialogue: 0,0:06:26.73,0:06:29.82,Default,,0000,0000,0000,,Em ko thể sống như thế này mãi được...
Dialogue: 0,0:06:50.69,0:06:53.18,Default,,0000,0000,0000,,Anh ko làm gì khác được sao? Nhưng mà em...
Dialogue: 0,0:06:53.96,0:06:56.82,Default,,0000,0000,0000,,Em ko thể chịu đựng chuyện đó thêm được nữa!
Dialogue: 0,0:06:59.36,0:07:00.83,Default,,0000,0000,0000,,Cô diễn kịch đạt thật đấy.
Dialogue: 0,0:07:00.90,0:07:05.16,Default,,0000,0000,0000,,Giả bộ như đang làm chuyện đó qua điện thoại, để bưng bít gã chồng vô dụng của cô.
Dialogue: 0,0:07:06.43,0:07:09.80,Default,,0000,0000,0000,,Làm ơn hãy tha cho tôi. Làm ơn.
Dialogue: 0,0:07:11.51,0:07:14.87,Default,,0000,0000,0000,,Vậy hãy quay mặt lại đây và cởi hết đồ ra đi!
Dialogue: 0,0:07:25.95,0:07:27.58,Default,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,0:07:48.34,0:07:50.64,Default,,0000,0000,0000,,Cô làm khá đấy chứ nhỉ?
Dialogue: 0,0:07:54.72,0:07:57.51,Default,,0000,0000,0000,,Tôi chưa từng làm điều này với bất kì ai, kể cả chồng tôi.
Dialogue: 0,0:07:57.79,0:08:01.65,Default,,0000,0000,0000,,Taketo, tôi có thể bú thêm chút nữa được ko?
Dialogue: 0,0:08:04.76,0:08:06.42,Default,,0000,0000,0000,,Này, này.
Dialogue: 0,0:08:06.56,0:08:08.79,Default,,0000,0000,0000,,Trò vui chỉ mới bắt đầu thôi mà.
Dialogue: 0,0:08:09.33,0:08:10.66,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:08:26.41,0:08:27.94,Default,,0000,0000,0000,,Tôi đây.
Dialogue: 0,0:08:28.85,0:08:30.34,Default,,0000,0000,0000,,Con về rồi đây.
Dialogue: 0,0:08:35.56,0:08:37.08,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:08:41.20,0:08:42.60,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:08:43.43,0:08:44.80,Default,,0000,0000,0000,,Rumi!
Dialogue: 0,0:08:47.30,0:08:48.89,Default,,0000,0000,0000,,Mừng cô trở về nhà.
Dialogue: 0,0:08:48.97,0:08:50.63,Default,,0000,0000,0000,,Làm sao anh dám!
Dialogue: 0,0:08:51.31,0:08:53.80,Default,,0000,0000,0000,,Bởi vì cô đã cư xử rất thô lỗ với tôi.
Dialogue: 0,0:08:53.88,0:08:57.71,Default,,0000,0000,0000,,Nên bà ta sẽ phải chịu phạt thay cô.
Dialogue: 0,0:08:59.18,0:09:00.84,Default,,0000,0000,0000,,Chúng ta làm tiếp nào!
Dialogue: 0,0:09:09.52,0:09:12.86,Default,,0000,0000,0000,,Mẹ xin lỗi. Làm ơn hãy tha thứ cho mẹ.
Dialogue: 0,0:09:17.80,0:09:20.06,Default,,0000,0000,0000,,Để mẹ tôi yên! Tránh xa mẹ tôi ra!
Dialogue: 0,0:09:20.13,0:09:22.00,Default,,0000,0000,0000,,Cô vừa nói gì?
Dialogue: 0,0:09:22.07,0:09:25.47,Default,,0000,0000,0000,,Tôi sẽ ko để anh tiếp tục dày vò mẹ tôi nữa!
Dialogue: 0,0:09:25.54,0:09:28.03,Default,,0000,0000,0000,,Chuyện này càng lúc càng thú vị đây.
Dialogue: 0,0:09:36.95,0:09:38.48,Default,,0000,0000,0000,,Chị...
Dialogue: 0,0:09:46.13,0:09:47.79,Default,,0000,0000,0000,,Rumi, đừng.
Dialogue: 0,0:09:48.96,0:09:50.73,Default,,0000,0000,0000,,Làm ơn, đừng.
Dialogue: 0,0:09:57.57,0:09:59.30,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:10:00.71,0:10:02.07,Default,,0000,0000,0000,,Rumi!
Dialogue: 0,0:10:02.18,0:10:03.94,Default,,0000,0000,0000,,Cô, im miệng ngay!
Dialogue: 0,0:10:07.28,0:10:10.45,Default,,0000,0000,0000,,Nhưng nếu con ko làm thế này, mẹ sẽ...
Dialogue: 0,0:10:10.82,0:10:12.51,Default,,0000,0000,0000,,Đừng lo cho con.
Dialogue: 0,0:10:12.59,0:10:14.95,Default,,0000,0000,0000,,Mẹ sẽ ko phải một mình chịu đựng tất cả những chuyện này nữa.
Dialogue: 0,0:10:15.66,0:10:18.59,Default,,0000,0000,0000,,Tình cảm mẹ con xúc động chưa kìa.\NThật là nực cười.
Dialogue: 0,0:10:18.66,0:10:21.32,Default,,0000,0000,0000,,Này. Bú mạnh nữa lên xem nào.
Dialogue: 0,0:10:21.63,0:10:24.62,Default,,0000,0000,0000,,Tôi sẽ làm! Tôi sẽ làm hết sức mình để anh hài lòng.
Dialogue: 0,0:10:26.73,0:10:28.10,Default,,0000,0000,0000,,Rumi!
Dialogue: 0,0:10:32.57,0:10:34.30,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:10:38.75,0:10:40.87,Default,,0000,0000,0000,,Tôi ko thể đợi thêm được nữa.
Dialogue: 0,0:10:42.32,0:10:43.72,Default,,0000,0000,0000,,Thế này được chưa?
Dialogue: 0,0:10:43.79,0:10:47.02,Default,,0000,0000,0000,,A ha. Cô cũng muốn điều đó hả.
Dialogue: 0,0:10:48.69,0:10:50.68,Default,,0000,0000,0000,,Cô sẵn sàng chưa?
Dialogue: 0,0:10:50.76,0:10:51.99,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:11:04.07,0:11:05.30,Default,,0000,0000,0000,,Rumi.
Dialogue: 0,0:11:06.71,0:11:12.08,Default,,0000,0000,0000,,Đừng lo mẹ à. Con sẽ ko sao đâu. Vì thế, mẹ đừng lo gì hết.
Dialogue: 0,0:11:12.15,0:11:16.05,Default,,0000,0000,0000,,Cô im miệng lại đi! Muốn làm tôi rối trí hả.
Dialogue: 0,0:11:22.62,0:11:24.96,Default,,0000,0000,0000,,Cô ta khít thật.
Dialogue: 0,0:11:25.29,0:11:27.28,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:11:28.06,0:11:31.50,Default,,0000,0000,0000,,Ko! Đừng ra bên trong Rumi!
Dialogue: 0,0:11:31.90,0:11:33.53,Default,,0000,0000,0000,,Để xem nào.
Dialogue: 0,0:11:54.12,0:11:56.78,Default,,0000,0000,0000,,Ra đi nào. Tôi thách anh dám ra bên trong tôi đó.
Dialogue: 0,0:11:56.86,0:11:58.59,Default,,0000,0000,0000,,Nếu tôi mang thai...
Dialogue: 0,0:11:58.66,0:12:02.62,Default,,0000,0000,0000,,Anh sẽ ko thể rũ bỏ trách nhiệm của mình một cách dễ dàng đâu .
Dialogue: 0,0:12:38.53,0:12:42.83,Default,,0000,0000,0000,,Đồ điên! Điên hết cỡ! Anh ta dám ra vào bên trong mình thật ư?
Dialogue: 0,0:12:47.78,0:12:51.27,Default,,0000,0000,0000,,Chị, em xin lỗi.
Dialogue: 0,0:12:59.52,0:13:01.92,Default,,0000,0000,0000,,Có tiếng gì ở tầng trên vậy?
Dialogue: 0,0:13:12.77,0:13:16.90,Default,,0000,0000,0000,,Cô ta trông rất ngây thơ, nhưng bên dưới cô ta lại ướt hết rồi.
Dialogue: 0,0:13:19.87,0:13:23.24,Default,,0000,0000,0000,,Những nữ sinh ở độ tuổi cô ta ai cũng háo hức với chuyện này, phải ko?
Dialogue: 0,0:13:27.75,0:13:29.08,Default,,0000,0000,0000,,Nhìn này.
Dialogue: 0,0:13:32.65,0:13:34.38,Default,,0000,0000,0000,,Có vẻ cô ta sẵn sàng rồi.
Dialogue: 0,0:13:35.46,0:13:36.82,Default,,0000,0000,0000,,Ông già.
Dialogue: 0,0:14:05.15,0:14:08.05,Default,,0000,0000,0000,,Làm ơn hãy bảo con trai ông dừng việc đó lại.
Dialogue: 0,0:14:10.02,0:14:12.89,Default,,0000,0000,0000,,Nếu cứ tiếp tục như vậy...
Dialogue: 0,0:14:20.40,0:14:24.77,Default,,0000,0000,0000,,Cô muốn hủy hoại chính cuộc đời mình sao?
Dialogue: 0,0:14:25.04,0:14:27.77,Default,,0000,0000,0000,,Nếu cô im lặng và chấp nhận chuyện đó...
Dialogue: 0,0:14:27.84,0:14:30.74,Default,,0000,0000,0000,,...thì ko có lí do gì chuyện này sẽ đến tai chồng cô cả.
Dialogue: 0,0:14:32.61,0:14:36.24,Default,,0000,0000,0000,,Cô ko muốn hủy hoại hạnh phúc gia đình mình chứ?
Dialogue: 0,0:14:40.32,0:14:42.95,Default,,0000,0000,0000,,Phần B:\NTomoko, Đóa hoa mỏng manh
Dialogue: 0,0:14:46.79,0:14:50.75,Default,,0000,0000,0000,,Thằng Taketo. Nó vẫn chưa học được cách bắt người khác phục tùng mình.
Dialogue: 0,0:15:03.61,0:15:08.55,Default,,0000,0000,0000,,Sao ta có thể bỏ qua hai viên ngọc này chứ?
Dialogue: 0,0:15:21.13,0:15:24.93,Default,,0000,0000,0000,,Rumi thật dũng mãnh, giờ mới là sáng sớm thôi mà.
Dialogue: 0,0:15:25.00,0:15:28.77,Default,,0000,0000,0000,,Hình như cô ấy vừa gặp chuyện gì đó. Cô ấy trông không giống với mọi khi.
Dialogue: 0,0:15:28.84,0:15:31.93,Default,,0000,0000,0000,,Có thể cô ấy đang yêu.
Dialogue: 0,0:15:37.95,0:15:39.57,Default,,0000,0000,0000,,Chúng ta quen nhau hả?
Dialogue: 0,0:15:40.75,0:15:42.41,Default,,0000,0000,0000,,Muốn biết tôi là ai thì hãy coi tấm hình này.
Dialogue: 0,0:15:51.79,0:15:53.56,Default,,0000,0000,0000,,Đừng động vào người tôi!
Dialogue: 0,0:15:54.16,0:15:57.22,Default,,0000,0000,0000,,Mẹ của Taketo qua đời từ khi nó còn nhỏ, nó ko hề có tình thương của mẹ...
Dialogue: 0,0:15:57.30,0:16:00.83,Default,,0000,0000,0000,,...vì thế nó trở nên thèm muốn những cô gái mà ta để mắt tới.
Dialogue: 0,0:16:01.04,0:16:04.00,Default,,0000,0000,0000,,Anh là Taketo. Chúng ta kết bạn nhé.
Dialogue: 0,0:16:04.47,0:16:06.70,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:16:07.44,0:16:08.67,Default,,0000,0000,0000,,Này, Yumi.
Dialogue: 0,0:16:08.74,0:16:12.11,Default,,0000,0000,0000,,Cô biết mình phải làm gì rồi chứ.
Dialogue: 0,0:16:12.91,0:16:16.08,Default,,0000,0000,0000,,Vâng. Cứ để tôi lo việc này.
Dialogue: 0,0:16:20.79,0:16:23.31,Default,,0000,0000,0000,,Kitazawa, em ko sao chứ?
Dialogue: 0,0:16:26.09,0:16:27.58,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:16:27.86,0:16:30.92,Default,,0000,0000,0000,,Rumi. Vậy là cô được nghỉ buổi học hôm nay.
Dialogue: 0,0:16:31.30,0:16:34.50,Default,,0000,0000,0000,,Tôi đã giả vờ trượt chân khi tập luyện.
Dialogue: 0,0:16:36.27,0:16:37.79,Default,,0000,0000,0000,,Chị...
Dialogue: 0,0:16:38.34,0:16:42.97,Default,,0000,0000,0000,,Ngài hiệu trưởng đang rất nóng lòng được gặp cô đấy.
Dialogue: 0,0:16:45.98,0:16:49.97,Default,,0000,0000,0000,,Cô gái, ta sẵn sàng rồi đây.
Dialogue: 0,0:16:50.85,0:16:54.95,Default,,0000,0000,0000,,Sao cô ko mặc bộ đồ cô dùng khi chơi tennis? Ta rất thích điều đó.
Dialogue: 0,0:17:15.24,0:17:18.34,Default,,0000,0000,0000,,Ngoan lắm. Bú mạnh nữa lên nào.
Dialogue: 0,0:17:22.58,0:17:25.07,Default,,0000,0000,0000,,Chiếc quần lót dễ thương đấy.
Dialogue: 0,0:17:29.32,0:17:31.59,Default,,0000,0000,0000,,Ko! Ko được chụp hình tôi như thế này.
Dialogue: 0,0:17:31.66,0:17:36.12,Default,,0000,0000,0000,,Chẳng phải cô thường làm tất cả những chuyện này một mình sao?
Dialogue: 0,0:17:36.80,0:17:38.26,Default,,0000,0000,0000,,Đừng hiểu sai ý tôi.
Dialogue: 0,0:17:38.33,0:17:41.63,Default,,0000,0000,0000,,Tôi chỉ muốn bảo vệ gia đình mình. Đó là lí do tại sao tôi chấp nhận làm thế này.
Dialogue: 0,0:17:41.70,0:17:45.50,Default,,0000,0000,0000,,Này, đừng có rời miệng ra khi ta chưa cho phép!
Dialogue: 0,0:17:47.71,0:17:51.37,Default,,0000,0000,0000,,Cô dám nói là sẽ gánh vác khoản tiền trong bản giao kèo sao?
Dialogue: 0,0:17:51.45,0:17:53.57,Default,,0000,0000,0000,,Thật láo xược.
Dialogue: 0,0:17:54.65,0:17:58.91,Default,,0000,0000,0000,,Nếu cô làm ko tốt, cô sẽ ko bao giờ có thể thoát khỏi chuyện này.
Dialogue: 0,0:17:59.42,0:18:01.79,Default,,0000,0000,0000,,Hay là việc này quá sức đối với cô?
Dialogue: 0,0:18:01.86,0:18:04.65,Default,,0000,0000,0000,,Cô có cần thêm sự trợ giúp của Tomoko ko?
Dialogue: 0,0:18:04.73,0:18:07.82,Default,,0000,0000,0000,,Ko! Đừng động đến Tomoko!
Dialogue: 0,0:18:07.90,0:18:09.42,Default,,0000,0000,0000,,Câm ngay!
Dialogue: 0,0:18:09.60,0:18:12.59,Default,,0000,0000,0000,,Những lời lẽ xấc xược đó từ đâu ra vậy hả?
Dialogue: 0,0:18:12.67,0:18:14.57,Default,,0000,0000,0000,,Từ hai đầu vú?
Dialogue: 0,0:18:20.37,0:18:22.93,Default,,0000,0000,0000,,Hay từ chỗ này hả?
Dialogue: 0,0:18:26.01,0:18:27.74,Default,,0000,0000,0000,,Mùi vị tuyệt thật.
Dialogue: 0,0:18:29.42,0:18:32.51,Default,,0000,0000,0000,,Chắc chắn là mùi hương trinh tiết của cô.
Dialogue: 0,0:18:37.53,0:18:39.58,Default,,0000,0000,0000,,Ko, đừng!
Dialogue: 0,0:18:47.33,0:18:48.82,Default,,0000,0000,0000,,Tha cho tôi.
Dialogue: 0,0:18:48.90,0:18:51.30,Default,,0000,0000,0000,,Cô phải nói là, "làm ơn hãy tha thứ cho tôi."
Dialogue: 0,0:18:52.11,0:18:54.01,Default,,0000,0000,0000,,Làm ơn hãy tha thứ cho tôi.
Dialogue: 0,0:18:54.91,0:18:58.00,Default,,0000,0000,0000,,Ngoan lắm. Ta sẽ thưởng cho cô một thứ.
Dialogue: 0,0:18:59.21,0:19:00.48,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:19:13.89,0:19:16.26,Default,,0000,0000,0000,,Ko, tôi đã nói là ko mà!
Dialogue: 0,0:19:32.01,0:19:35.50,Default,,0000,0000,0000,,Ôi trời, xem cô sung sướng tới mức nào kìa.
Dialogue: 0,0:19:35.58,0:19:38.35,Default,,0000,0000,0000,,Sự thực là cô rất thích điều đó, phải ko?
Dialogue: 0,0:19:38.42,0:19:40.22,Default,,0000,0000,0000,,Ko! Ko đời nào!
Dialogue: 0,0:19:40.82,0:19:44.28,Default,,0000,0000,0000,,Phải rồi, là như vậy. Việc này đã bắt đầu trở thành thói quen rồi, phải ko?
Dialogue: 0,0:19:44.59,0:19:47.19,Default,,0000,0000,0000,,cô đang rất sung sướng, phải ko?
Dialogue: 0,0:19:48.40,0:19:50.56,Default,,0000,0000,0000,,Ko, đừng nói như thế.
Dialogue: 0,0:19:50.63,0:19:53.83,Default,,0000,0000,0000,,Nếu cô còn nói như thế, tôi sẽ ra mất.
Dialogue: 0,0:19:57.17,0:19:59.07,Default,,0000,0000,0000,,Ta cũng sắp ra rồi.
Dialogue: 0,0:19:59.57,0:20:03.10,Default,,0000,0000,0000,,Coi nào, ngài hiệu trưởng cũng sắp ra rồi. Đổi tư thế nào.
Dialogue: 0,0:20:03.94,0:20:05.31,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:20:13.72,0:20:15.81,Default,,0000,0000,0000,,Ta ra rồi!
Dialogue: 0,0:20:17.89,0:20:20.83,Default,,0000,0000,0000,,Ngoan lắm. Cô đã làm đúng như những gì tôi bảo.
Dialogue: 0,0:20:21.93,0:20:26.37,Default,,0000,0000,0000,,Cô có thích chúng ko? Có ngon ko?
Dialogue: 0,0:20:55.33,0:20:59.32,Default,,0000,0000,0000,,<i>Mình sẽ ko để họ động tới mẹ và Tomoko, bằng bất cứ giá nào.</i>
Dialogue: 0,0:20:59.57,0:21:03.40,Default,,0000,0000,0000,,<i>Đúng thế, mình là người duy nhất phài làm điều này.</i>
Dialogue: 0,0:21:16.02,0:21:19.35,Default,,0000,0000,0000,,<i>Tại sao chị lại làm chuyện đó?</i>
Dialogue: 0,0:21:25.86,0:21:29.19,Default,,0000,0000,0000,,<i>Nóng quá, mình phát điên lên mất.</i>
Dialogue: 0,0:21:31.13,0:21:34.90,Default,,0000,0000,0000,,<i>Mẹ cũng trở nên rất lạ trong những ngày gần đây. Mình ko hề biết tại sao.</i>
Dialogue: 0,0:21:34.97,0:21:37.60,Default,,0000,0000,0000,,<i>Tại sao cơ thể mình lại như thế này?</i>
Dialogue: 0,0:21:42.61,0:21:45.20,Default,,0000,0000,0000,,<i>Oh, ko. Việc này đã bắt đầu trở thành thói quen mất rồi.</i>
Dialogue: 0,0:22:03.90,0:22:05.92,Default,,0000,0000,0000,,Vâng, tôi xong ngay đây.
Dialogue: 0,0:22:10.10,0:22:12.00,Default,,0000,0000,0000,,Tôi sẽ nói cho cô biết tại sao.
Dialogue: 0,0:22:12.07,0:22:15.63,Default,,0000,0000,0000,,Tôi sẽ nói cho cô biết tại sao Rumi lại làm những chuyện như vậy.
Dialogue: 0,0:22:28.09,0:22:32.58,Default,,0000,0000,0000,,Nếu những bức ảnh này lộ ra, cả hai người sẽ gặp rắc rối lớn đó.
Dialogue: 0,0:22:33.46,0:22:36.26,Default,,0000,0000,0000,,Làm ơn, hãy cho tôi biết tôi phải làm gì?
Dialogue: 0,0:22:36.73,0:22:40.00,Default,,0000,0000,0000,,Cô rất quan tâm tới gia đình của mình phải ko.
Dialogue: 0,0:22:40.17,0:22:43.23,Default,,0000,0000,0000,,Một cô gái như cô rất dễ bị sa ngã đấy.
Dialogue: 0,0:22:44.54,0:22:45.73,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:22:46.24,0:22:51.11,Default,,0000,0000,0000,,Cô cần phải làm một vài chuyện, và thỏa mãn một người đàn ông.
Dialogue: 0,0:22:51.91,0:22:54.97,Default,,0000,0000,0000,,Đó là cách duy nhất để giúp đỡ gia đình cô.
Dialogue: 0,0:22:55.55,0:22:57.31,Default,,0000,0000,0000,,Làm ơn đừng!
Dialogue: 0,0:22:58.75,0:23:03.31,Default,,0000,0000,0000,,Yumi, mang cô ta tới đây mau. Ta ko thể chờ lâu hơn nữa.
Dialogue: 0,0:23:03.59,0:23:05.96,Default,,0000,0000,0000,,<i>Cô ta đã đồng ý rồi. Chúng tôi đang trên đường tới đây.</i>
Dialogue: 0,0:23:06.03,0:23:09.26,Default,,0000,0000,0000,,Tốt lắm. Cuối cùng thì, cô ta cũng sắp thuộc về ta.
Dialogue: 0,0:23:17.40,0:23:18.70,Default,,0000,0000,0000,,Chuyện gì thế...
Dialogue: 0,0:23:18.77,0:23:21.83,Default,,0000,0000,0000,,Suỵt! Có vẻ như họ đang vui vẻ với nhau.
Dialogue: 0,0:23:35.36,0:23:37.02,Default,,0000,0000,0000,,Sao thế?
Dialogue: 0,0:23:37.39,0:23:41.23,Default,,0000,0000,0000,,Đừng nên làm phiền họ lúc này. Lên phòng cô nào.
Dialogue: 0,0:24:07.25,0:24:09.65,Default,,0000,0000,0000,,Hay là tôi sẽ tiếp tục làm thế này?
Dialogue: 0,0:24:25.41,0:24:30.07,Default,,0000,0000,0000,,Mừng cô trở về nhà. "Show" diễn mới chỉ bắt đầu. Ko phiền khi coi cùng ta chứ?
Dialogue: 0,0:24:30.64,0:24:32.04,Default,,0000,0000,0000,,Ông ta là ai?
Dialogue: 0,0:24:32.11,0:24:34.81,Default,,0000,0000,0000,,Tôi đã bảo với cô rồi, nếu cô muốn giúp gia đình mình...
Dialogue: 0,0:24:34.88,0:24:38.94,Default,,0000,0000,0000,,...cô cần phải hiến dâng trinh tiết của mình cho một người.
Dialogue: 0,0:24:39.99,0:24:41.61,Default,,0000,0000,0000,,Ko thể nào...
Dialogue: 0,0:24:43.29,0:24:46.12,Default,,0000,0000,0000,,Cô còn đợi gì nữa hả?\N"Show" diễn sắp bắt đầu rồi.
Dialogue: 0,0:24:46.19,0:24:48.92,Default,,0000,0000,0000,,Tomoko, hãy cùng xem với ông ấy nào.
Dialogue: 0,0:24:49.63,0:24:51.86,Default,,0000,0000,0000,,<i>Rumi, đừng, làm ơn, đừng.</i>
Dialogue: 0,0:24:52.30,0:24:53.99,Default,,0000,0000,0000,,<i>Ko sao đâu, mẹ.</i>
Dialogue: 0,0:24:54.07,0:24:56.09,Default,,0000,0000,0000,,<i>Mẹ sẽ ko phải chịu đựng chuyện này một mình nữa đâu.</i>
Dialogue: 0,0:24:56.17,0:24:59.83,Default,,0000,0000,0000,,<i>Con sẽ ko để một tên khốn như cậu ta đụng tới mẹ!</i>
Dialogue: 0,0:25:00.17,0:25:02.67,Default,,0000,0000,0000,,Chuyện này càng lúc càng thú vị rồi đây.
Dialogue: 0,0:25:03.38,0:25:04.78,Default,,0000,0000,0000,,Chị.
Dialogue: 0,0:25:14.92,0:25:18.95,Default,,0000,0000,0000,,Coi kìa, chị cô đã hiến thân mình để cứu mẹ của cô.
Dialogue: 0,0:25:19.03,0:25:21.02,Default,,0000,0000,0000,,Cô ấy thật dũng cảm , phải ko?
Dialogue: 0,0:25:22.73,0:25:27.56,Default,,0000,0000,0000,,Đừng quay mặt đi. Cô ko muốn cứu chị cô sao?
Dialogue: 0,0:25:29.04,0:25:32.53,Default,,0000,0000,0000,,Tha lỗi cho tôi. Tôi ko làm được.
Dialogue: 0,0:25:33.04,0:25:34.84,Default,,0000,0000,0000,,Tôi ko giống như Rumi.
Dialogue: 0,0:25:35.61,0:25:37.08,Default,,0000,0000,0000,,Vì sao thế?
Dialogue: 0,0:25:38.35,0:25:40.25,Default,,0000,0000,0000,,- Bởi vì, bởi vì...\N- Bởi vì?
Dialogue: 0,0:25:40.31,0:25:43.94,Default,,0000,0000,0000,,Tôi ko xinh đẹp bằng chị Rumi.
Dialogue: 0,0:25:44.22,0:25:46.91,Default,,0000,0000,0000,,Cô nhầm rôi. Cô rất xinh đẹp.
Dialogue: 0,0:25:47.15,0:25:48.64,Default,,0000,0000,0000,,Thật ư?
Dialogue: 0,0:25:55.53,0:25:57.26,Default,,0000,0000,0000,,Góc nhìn tuyệt đấy.
Dialogue: 0,0:25:58.80,0:26:00.66,Default,,0000,0000,0000,,Ko! Đừng nhìn!
Dialogue: 0,0:26:05.94,0:26:08.37,Default,,0000,0000,0000,,Mùi vị thật tuyệt vời.
Dialogue: 0,0:26:08.71,0:26:11.77,Default,,0000,0000,0000,,Mùi vị của quần lót đã được giặt sạch.
Dialogue: 0,0:26:12.95,0:26:15.85,Default,,0000,0000,0000,,Mùi vị của trinh nữ,\Nướt rồi này.
Dialogue: 0,0:26:15.92,0:26:18.54,Default,,0000,0000,0000,,Đó chính là hương vị của thiếu nữ!
Dialogue: 0,0:26:22.22,0:26:25.49,Default,,0000,0000,0000,,Ko, ko, đừng nhìn, đừng nhìn!
Dialogue: 0,0:26:31.70,0:26:35.33,Default,,0000,0000,0000,,Cô ko kháng cự lại được đâu. Tôi sẽ khiến cô ra thật nhiều.
Dialogue: 0,0:26:35.87,0:26:39.33,Default,,0000,0000,0000,,Trông kìa, Rumi đang bú cái đó của cậu ta.
Dialogue: 0,0:26:40.04,0:26:43.57,Default,,0000,0000,0000,,Cô cũng muốn ngậm một thứ tương tự như vậy, phải ko?
Dialogue: 0,0:26:49.68,0:26:54.59,Default,,0000,0000,0000,,Tất cả điều này chỉ nhằm giúp gia đình cô.\NCô có thể bỏ mặc Rumi một mình được sao?
Dialogue: 0,0:27:00.66,0:27:03.82,Default,,0000,0000,0000,,Vậy sao cô ko trở thành phụ nữ ngay bây giờ, ở chính nơi này?
Dialogue: 0,0:27:09.20,0:27:11.10,Default,,0000,0000,0000,,Cô ta đã sẵn sàng rồi.
