Dialogue: 0,0:00:06.47,0:00:08.64,Default,,0000,0000,0000,,Fumi-nee, sao chị trở về vậy?
Dialogue: 0,0:00:09.24,0:00:12.24,Default,,0000,0000,0000,,Em ko nói gì bởi vì chuyện xảy ra ở bờ sông, đúng ko?
Dialogue: 0,0:00:12.35,0:00:13.78,Default,,0000,0000,0000,,Em có bị mù ko vậy?
Dialogue: 0,0:00:13.88,0:00:14.64,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:00:16.75,0:00:17.24,Default,,0000,0000,0000,,Này.
Dialogue: 0,0:00:17.39,0:00:18.51,Default,,0000,0000,0000,,Vâng, chuyện gì vậy?
Dialogue: 0,0:00:18.65,0:00:20.21,Default,,0000,0000,0000,,Shinobu thế nào rồi?
Dialogue: 0,0:00:20.42,0:00:22.39,Default,,0000,0000,0000,,Shinobu vẫn ổn.
Dialogue: 0,0:00:22.49,0:00:24.05,Default,,0000,0000,0000,,Mày đang đùa chị đấy à!
Dialogue: 0,0:00:24.19,0:00:25.66,Default,,0000,0000,0000,,Ko, nhưng mà...
Dialogue: 0,0:00:25.96,0:00:28.99,Default,,0000,0000,0000,,Chị nói lại lần nữa này. Mắt em mù chả khác gì loài dơi.
Dialogue: 0,0:00:29.56,0:00:32.09,Default,,0000,0000,0000,,Để chị nói với em một điều.
Dialogue: 0,0:00:32.27,0:00:39.04,Default,,0000,0000,0000,,Con bé cố tình tỏ ra như thế hoặc có thể nó cảm thấy khó chịu ngầm trong bụng mà ko nói ra.
Dialogue: 0,0:00:39.14,0:00:41.13,Default,,0000,0000,0000,,Vâng, Em...
Dialogue: 0,0:00:41.34,0:00:44.11,Default,,0000,0000,0000,,Shinobu, anh tình cờ gặp Fumi-nee.
Dialogue: 0,0:00:44.25,0:00:46.04,Default,,0000,0000,0000,,Anh nghĩ anh sẽ về muộn đấy.
Dialogue: 0,0:00:46.15,0:00:47.27,Default,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:00:47.58,0:00:49.64,Default,,0000,0000,0000,,Đây là món ăn đặc biệt của cô. Thịt mực với nước sốt cải nóng!
Dialogue: 0,0:00:59.19,0:01:00.49,Default,,0000,0000,0000,,Take-chan...
Dialogue: 0,0:01:00.86,0:01:03.16,Default,,0000,0000,0000,,Take-chan...Take-chan...
Dialogue: 0,0:01:07.97,0:01:11.63,Default,,0000,0000,0000,,Chị biết mình ko nên nói điều này nhưng...
Dialogue: 0,0:01:11.84,0:01:15.71,Default,,0000,0000,0000,,Sao em ko xem Shinobu như một cô gái thực sự ?
Dialogue: 0,0:01:15.94,0:01:17.88,Default,,0000,0000,0000,,Em thật sự ko biết.
Dialogue: 0,0:01:18.75,0:01:21.87,Default,,0000,0000,0000,,Vậy đây là câu trả lời của em sao.
Dialogue: 0,0:01:22.08,0:01:25.38,Default,,0000,0000,0000,,Nhưng đó là điều em muốn nói.
Dialogue: 0,0:01:25.52,0:01:26.89,Default,,0000,0000,0000,,Đặc biệt là với chị.
Dialogue: 0,0:01:27.02,0:01:31.55,Default,,0000,0000,0000,,Em ko cần phải nói gì đâu. Chị biết em định nói điều gì rồi.
Dialogue: 0,0:01:32.16,0:01:34.56,Default,,0000,0000,0000,,À này, em có muốn qua chỗ chị ko?
Dialogue: 0,0:01:35.53,0:01:39.30,Default,,0000,0000,0000,,Chị nghĩ mình đã uống hơi nhiều. Đưa chi về phòng nhé.
Dialogue: 0,0:01:39.50,0:01:40.69,Default,,0000,0000,0000,,Chị chắc chứ?
Dialogue: 0,0:01:40.80,0:01:43.24,Default,,0000,0000,0000,,Này, em đang nghĩ điều gì đó xấu xa phải ko.
Dialogue: 0,0:01:45.94,0:01:47.41,Default,,0000,0000,0000,,Ko, Takeshi.
Dialogue: 0,0:01:47.78,0:01:50.30,Default,,0000,0000,0000,,Đừng chỉ chơi mỗi bộ ngực chị.
Dialogue: 0,0:01:50.41,0:01:53.57,Default,,0000,0000,0000,,Nhưng rõ ràng chị muốn em chơi đùa với chúng mà .
Dialogue: 0,0:01:55.05,0:01:57.42,Default,,0000,0000,0000,,Em xấu thật đấy.
Dialogue: 0,0:01:58.09,0:01:59.95,Default,,0000,0000,0000,,Vậy giờ em sẽ chơi với cái này.
Dialogue: 0,0:02:00.15,0:02:02.25,Default,,0000,0000,0000,,Đừng động vào chỗ đó.
Dialogue: 0,0:02:04.23,0:02:07.22,Default,,0000,0000,0000,,Chỗ đó của chị ướt thật.
Dialogue: 0,0:02:08.03,0:02:11.16,Default,,0000,0000,0000,,Ko, đừng nói thế.
Dialogue: 0,0:02:12.43,0:02:15.23,Default,,0000,0000,0000,,Tuyệt lắm. Chị ướt hết rồi này.
Dialogue: 0,0:02:15.37,0:02:16.73,Default,,0000,0000,0000,,Cái nhỏ xíu này cũng cứng lên rồi.
Dialogue: 0,0:02:16.87,0:02:19.17,Default,,0000,0000,0000,,Em khá lắm, Takeshi, chị rất thích như vậy!
Dialogue: 0,0:02:19.54,0:02:21.57,Default,,0000,0000,0000,,Hai đầu vú của chị cũng nứng lên rồi.
Dialogue: 0,0:02:21.74,0:02:24.97,Default,,0000,0000,0000,,Takeshi, đừng có nói chỗ này chỗ kia của chị cứng như vậy.
Dialogue: 0,0:02:25.21,0:02:27.18,Default,,0000,0000,0000,,Chị là phụ nữ, em biết mà.
Dialogue: 0,0:02:27.58,0:02:29.91,Default,,0000,0000,0000,,Bây giờ đến chỗ nào nhỉ?
Dialogue: 0,0:02:30.02,0:02:31.01,Default,,0000,0000,0000,,Dừng lại.
Dialogue: 0,0:02:31.12,0:02:33.88,Default,,0000,0000,0000,,Chị chưa chơi bằng chỗ này bao giờ.
Dialogue: 0,0:02:33.99,0:02:37.19,Default,,0000,0000,0000,,Đừng chơi nó mạnh quá. Nhẹ nhàng thôi.
Dialogue: 0,0:02:37.53,0:02:38.72,Default,,0000,0000,0000,,Takeshi.
Dialogue: 0,0:02:39.06,0:02:40.55,Default,,0000,0000,0000,,Thật tuyệt vời, Takeshi.
Dialogue: 0,0:02:40.70,0:02:44.13,Default,,0000,0000,0000,,thật tiếc là lúc trước chị chỉ coi em là em trai mình.
Dialogue: 0,0:02:44.43,0:02:47.20,Default,,0000,0000,0000,,Tuyệt quá, Takeshi.
Dialogue: 0,0:02:47.50,0:02:50.20,Default,,0000,0000,0000,,Được lắm, có vẻ như chị rất muốn em.
Dialogue: 0,0:02:50.47,0:02:53.84,Default,,0000,0000,0000,,Vậy thì em sẽ cố hết sức để làm chị vừa lòng.
Dialogue: 0,0:02:55.11,0:02:57.60,Default,,0000,0000,0000,,Đừng liếm chị như một con cún như vậy.
Dialogue: 0,0:02:57.75,0:02:59.08,Default,,0000,0000,0000,,Xấu hổ quá.
Dialogue: 0,0:02:59.18,0:03:01.21,Default,,0000,0000,0000,,Chị xấu hổ lắm.
Dialogue: 0,0:03:01.52,0:03:05.21,Default,,0000,0000,0000,,Mình đang giang hai chân ra như một con ếch.
Dialogue: 0,0:03:05.79,0:03:08.02,Default,,0000,0000,0000,,Chị tè ra mất. Em sẽ làm chị tè ra mất!
Dialogue: 0,0:03:08.16,0:03:13.86,Default,,0000,0000,0000,,Đừng phát ra những âm thanh kì cục như vậy...
Dialogue: 0,0:03:14.26,0:03:18.36,Default,,0000,0000,0000,,Ko Takeshi. Ko phải như thế.
Dialogue: 0,0:03:19.37,0:03:20.70,Default,,0000,0000,0000,,Ko phải chỗ đó.
Dialogue: 0,0:03:20.87,0:03:22.06,Default,,0000,0000,0000,,Nhưng... đừng dừng lại.
Dialogue: 0,0:03:22.44,0:03:25.37,Default,,0000,0000,0000,,Làm ơn. Liếm nó tiệp đi.
Dialogue: 0,0:03:25.47,0:03:27.53,Default,,0000,0000,0000,,Nữa. làm như thế nữa.
Dialogue: 0,0:03:27.64,0:03:30.04,Default,,0000,0000,0000,,Liếm nó với tư thế kì quái này.
Dialogue: 0,0:03:31.41,0:03:33.35,Default,,0000,0000,0000,,Đừng chỉ liếm một chỗ.
Dialogue: 0,0:03:33.45,0:03:38.21,Default,,0000,0000,0000,,Nếu em cứ liếm như thế, cái nhỏ xíu đó của chị sẽ phồng lên mất. Chị xấu hổ lắm.
Dialogue: 0,0:03:38.72,0:03:43.02,Default,,0000,0000,0000,,Chị ra đây!
Dialogue: 0,0:03:44.86,0:03:46.88,Default,,0000,0000,0000,,Em vào nhé, Fumi-nee.
Dialogue: 0,0:03:47.03,0:03:48.39,Default,,0000,0000,0000,,Làm đi, Takeshi.
Dialogue: 0,0:04:04.45,0:04:07.04,Default,,0000,0000,0000,,Tuyệt quá, Takeshi.
Dialogue: 0,0:04:07.21,0:04:10.88,Default,,0000,0000,0000,,Nếu em cứ đẩy mạnh như vậy, chị sẽ tè ra mất.
Dialogue: 0,0:04:11.12,0:04:13.75,Default,,0000,0000,0000,,Chị sẽ tè ra ngoài mất.
Dialogue: 0,0:04:15.16,0:04:17.12,Default,,0000,0000,0000,,Ko Takeshi. Đừng...
Dialogue: 0,0:04:18.39,0:04:21.38,Default,,0000,0000,0000,,Ko, đừng nhìn vào lỗ đít chị.
Dialogue: 0,0:04:21.56,0:04:23.76,Default,,0000,0000,0000,,Xấu hổ quá.
Dialogue: 0,0:04:45.59,0:04:49.78,Default,,0000,0000,0000,,Chị ra rồi!
Dialogue: 0,0:04:54.63,0:04:57.29,Default,,0000,0000,0000,,Cuối cùng em đã thuộc về chị, Takeshi.
Dialogue: 0,0:04:57.50,0:04:59.43,Default,,0000,0000,0000,,Chị sẽ ko để em đi đâu hết.
Dialogue: 0,0:04:59.57,0:05:01.54,Default,,0000,0000,0000,,Giờ ta tiếp tục nào.
Dialogue: 0,0:05:01.67,0:05:02.80,Default,,0000,0000,0000,,Được ko, Takeshi?
Dialogue: 0,0:05:39.41,0:05:41.03,Default,,0000,0000,0000,,Em đang nắn bóp bộ ngực chị.
Dialogue: 0,0:05:41.64,0:05:44.04,Default,,0000,0000,0000,,Làm thế với hai đầu vú chị nữa.
Dialogue: 0,0:05:46.28,0:05:48.08,Default,,0000,0000,0000,,Hãy để em ngắm gương mặt chi nào.
Dialogue: 0,0:05:48.18,0:05:50.51,Default,,0000,0000,0000,,Đừng xấu tính thế.
Dialogue: 0,0:05:50.65,0:05:51.71,Default,,0000,0000,0000,,Chỉ là ngắm gương mặt chị thôi mà.
Dialogue: 0,0:05:51.85,0:05:52.91,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:05:53.05,0:05:57.18,Default,,0000,0000,0000,,Nếu em nhìn chị, chị sẽ tè ra vì xấu hổ mất.
Dialogue: 0,0:05:57.79,0:05:59.95,Default,,0000,0000,0000,,Chị luôn luôn phấn khích như vậy sao.
Dialogue: 0,0:06:00.39,0:06:04.33,Default,,0000,0000,0000,,Đó chỉ là vì chị quá yêu em, ôi xấu hổ quá.
Dialogue: 0,0:06:22.18,0:06:24.81,Default,,0000,0000,0000,,Chỗ đó đang nóng ran lên...
Dialogue: 0,0:06:25.49,0:06:27.25,Default,,0000,0000,0000,,Chị ko thể giữ lâu hơn được nữa.
Dialogue: 0,0:06:31.16,0:06:33.89,Default,,0000,0000,0000,,Chị sẽ tè nếu em cứ tiếp tục thế này.
Dialogue: 0,0:06:52.98,0:06:55.35,Default,,0000,0000,0000,,Ko sao đâu Fumi. Chị cứ tè ra đi.
Dialogue: 0,0:06:55.58,0:06:57.55,Default,,0000,0000,0000,,Như thế sướng chứ?
Dialogue: 0,0:06:59.19,0:07:03.21,Default,,0000,0000,0000,,Thật xấu hổ nếu chị làm như vậy ngay ở đây.
Dialogue: 0,0:07:03.49,0:07:06.26,Default,,0000,0000,0000,,Chị ko muốn tè đâu.
Dialogue: 0,0:07:06.53,0:07:08.72,Default,,0000,0000,0000,,Hình như chị sắp tè ra rồi.
Dialogue: 0,0:07:10.56,0:07:12.56,Default,,0000,0000,0000,,Chị ko chịu nổi nữa rồi. Chị ko chịu nổi nữa rồi.
Dialogue: 0,0:07:13.80,0:07:17.43,Default,,0000,0000,0000,,Chị tè đây!
Dialogue: 0,0:07:19.64,0:07:21.47,Default,,0000,0000,0000,,Chị xin lỗi, Takeshi.
Dialogue: 0,0:07:21.58,0:07:24.64,Default,,0000,0000,0000,,Chị tè hết cả ra rồi. Chị xin lỗi...
Dialogue: 0,0:07:24.91,0:07:30.78,Default,,0000,0000,0000,,Chị biết mình từng đối xử ko tốt với em, nhưng chị vẫn muốn em chỉ yêu mình chị trong suốt cuộc đời này.
Dialogue: 0,0:07:31.75,0:07:33.41,Default,,0000,0000,0000,,Mình đang ở đâu thế này?
Dialogue: 0,0:07:33.55,0:07:38.12,Default,,0000,0000,0000,,À nhớ rồi, mình đã tới chỗ Fumi-nee và chị ấy ép mình uống khá nhiều.
Dialogue: 0,0:07:38.26,0:07:40.06,Default,,0000,0000,0000,,Hiểu rồi, hiểu rồi.
Dialogue: 0,0:07:41.10,0:07:46.09,Default,,0000,0000,0000,,Cái quái gì vậy? Ko phải là mơ chứ?
Dialogue: 0,0:07:47.27,0:07:49.40,Default,,0000,0000,0000,,Cái gì? Ko thể nào!
Dialogue: 0,0:07:54.24,0:07:55.83,Default,,0000,0000,0000,,Con ả ngốc nghếch.
Dialogue: 0,0:07:56.24,0:07:58.11,Default,,0000,0000,0000,,Tao sẽ thưởng thức nó.
Dialogue: 0,0:07:58.28,0:08:02.31,Default,,0000,0000,0000,,Tao sẽ dày vò thứ mà mày gọi là cuộc đời mày.
Dialogue: 0,0:08:10.79,0:08:13.45,Default,,0000,0000,0000,,Cô ta vẫn ướt kể cả khi chúng tao hành hạ cô ta.
Dialogue: 0,0:08:14.63,0:08:17.50,Default,,0000,0000,0000,,Cô thích như thế, đúng ko?
Dialogue: 0,0:08:17.77,0:08:20.53,Default,,0000,0000,0000,,Có thể là do dòng máu dâm đãng của cô ta.
Dialogue: 0,0:08:21.64,0:08:24.57,Default,,0000,0000,0000,,Mày cũng đang hưởng thụ nó còn gì.
Dialogue: 0,0:08:24.81,0:08:28.74,Default,,0000,0000,0000,,Cô ả dâm đãng ngu ngốc. Mang về nhà một gã mới, hả?
Dialogue: 0,0:08:28.84,0:08:31.57,Default,,0000,0000,0000,,Để tao làm vài trò.
Dialogue: 0,0:08:31.75,0:08:33.11,Default,,0000,0000,0000,,Cô là đồ dâm đãng.
Dialogue: 0,0:08:44.93,0:08:46.05,Default,,0000,0000,0000,,Thế nào, có thích ko?
Dialogue: 0,0:08:47.26,0:08:48.82,Default,,0000,0000,0000,,Như vậy sướng chứ?
Dialogue: 0,0:09:02.68,0:09:03.64,Default,,0000,0000,0000,,Tao ra rồi!
Dialogue: 0,0:09:07.68,0:09:10.74,Default,,0000,0000,0000,,Này, thấy cái đó của tao thế nào?
Dialogue: 0,0:09:21.70,0:09:23.36,Default,,0000,0000,0000,,Bú tiếp đi.
Dialogue: 0,0:09:46.19,0:09:49.02,Default,,0000,0000,0000,,Sướng phải ko?
Dialogue: 0,0:09:50.42,0:09:52.32,Default,,0000,0000,0000,,Chỗ này trông ngon thật.
Dialogue: 0,0:10:00.23,0:10:04.26,Default,,0000,0000,0000,,Ko! Dừng lại ngay!
Dialogue: 0,0:10:16.75,0:10:19.45,Default,,0000,0000,0000,,Fumi-nee!
Dialogue: 0,0:10:27.09,0:10:30.39,Default,,0000,0000,0000,,Fujino-san! Tình trạng Fumi-nee hiện giờ rất tệ.
Dialogue: 0,0:10:30.60,0:10:32.96,Default,,0000,0000,0000,,Sao cô ko ở đó với chị ấy?
Dialogue: 0,0:10:33.17,0:10:35.64,Default,,0000,0000,0000,,Cô là mẹ ruột chị ấy mà, đúng ko?
Dialogue: 0,0:10:35.80,0:10:40.47,Default,,0000,0000,0000,,Con bé là con ruột cô nhưng nó lại ko coi cô là mẹ nó...
Dialogue: 0,0:10:40.64,0:10:42.07,Default,,0000,0000,0000,,...Cô ko muốn trông thấy mặt nó!
Dialogue: 0,0:10:43.78,0:10:45.64,Default,,0000,0000,0000,,Chờ đã? Fujino-san?
Dialogue: 0,0:10:46.15,0:10:47.88,Default,,0000,0000,0000,,Fujino-san? Fujino-san?
Dialogue: 0,0:10:49.62,0:10:50.58,Default,,0000,0000,0000,,Ko ăn thua rồi.
Dialogue: 0,0:10:50.75,0:10:51.62,Default,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:10:51.72,0:10:52.58,Default,,0000,0000,0000,,Cái chết tiệt gì vậy?
Dialogue: 0,0:10:52.72,0:10:55.59,Default,,0000,0000,0000,,Tình trạng Fumi-nee xấu thế này mà cô ta vẫn có thái độ như vậy sao?
Dialogue: 0,0:10:55.69,0:10:58.32,Default,,0000,0000,0000,,Tại sao tất cả ko thể là một gia đình hạnh phúc chứ?
Dialogue: 0,0:10:58.63,0:11:01.82,Default,,0000,0000,0000,,Mẹ cũng ko hề quan tâm tới em.
Dialogue: 0,0:11:02.73,0:11:05.36,Default,,0000,0000,0000,,Fumi-chan đột ngột bỏ đi...
Dialogue: 0,0:11:05.53,0:11:08.40,Default,,0000,0000,0000,,Tại sao ko ai chịu chú ý đến em chứ?
Dialogue: 0,0:11:08.67,0:11:09.93,Default,,0000,0000,0000,,Tại sao lại như vậy?
Dialogue: 0,0:11:10.04,0:11:10.94,Default,,0000,0000,0000,,Cho em biết tại sao đi.
Dialogue: 0,0:11:11.17,0:11:14.66,Default,,0000,0000,0000,,Nhưng anh sẽ ko rời bỏ em, đúng ko, Take-chan?
Dialogue: 0,0:11:19.28,0:11:22.48,Default,,0000,0000,0000,,Cháu định nói với cô chuyện gì nào?
Dialogue: 0,0:11:23.62,0:11:27.25,Default,,0000,0000,0000,,Shinobu cảm thấy cô đang bỏ rới em ấy.
Dialogue: 0,0:11:27.72,0:11:32.18,Default,,0000,0000,0000,,Và cháu cũng chắc rằng Fumi-nee bỏ nhà đi là vì cô.
Dialogue: 0,0:11:32.63,0:11:36.08,Default,,0000,0000,0000,,Fujino-san, Nếu cô muốn tiếp tục chà đạp người khác như vậy...
Dialogue: 0,0:11:36.20,0:11:38.19,Default,,0000,0000,0000,,Thì cháu sẽ buộc phải mời cô ra khỏi ngôi nhà này.
Dialogue: 0,0:11:38.67,0:11:39.69,Default,,0000,0000,0000,,Đó là tất cả những gì cháu muốn nói.
Dialogue: 0,0:11:45.71,0:11:46.70,Default,,0000,0000,0000,,Cô bỏ thứ gì vào miệng cháu đấy?
Dialogue: 0,0:11:51.48,0:11:58.41,sign,,0000,0000,0000,,{\move(374,284,374,340)}Oshino
Dialogue: 0,0:11:53.91,0:11:55.64,Default,,0000,0000,0000,,Cháu ko thể cử động, đúng ko?
Dialogue: 0,0:11:56.08,0:11:58.21,Default,,0000,0000,0000,,Đó là một loại thuốc đặc biệt.
Dialogue: 0,0:11:58.82,0:12:02.59,Default,,0000,0000,0000,,Nhưng cái đó của cháu thì ko bị ảnh hưởng gì đâu.
Dialogue: 0,0:12:03.16,0:12:07.86,Default,,0000,0000,0000,,Hơn nữa, cơ thể cháu sẽ nhạy cảm hơn rất nhiều lần.
Dialogue: 0,0:12:40.83,0:12:45.70,Default,,0000,0000,0000,,Cái của cháu là thứ tuyệt nhất. Nó thật mập mạp và ngon lành.
Dialogue: 0,0:12:52.37,0:12:54.90,Default,,0000,0000,0000,,Nhìn đi Take-san.
Dialogue: 0,0:12:55.08,0:12:58.74,Default,,0000,0000,0000,,Của cô ko tốt hơn của Fumino sao?
Dialogue: 0,0:12:59.51,0:13:02.74,Default,,0000,0000,0000,,Cô  cũng sẽ ko để Shinobu có được cháu.
Dialogue: 0,0:13:03.08,0:13:06.11,Default,,0000,0000,0000,,Bây giờ cô sẽ...
Dialogue: 0,0:13:07.19,0:13:09.62,Default,,0000,0000,0000,,Cô sẽ hạ mông xuống .
Dialogue: 0,0:13:10.69,0:13:12.96,Default,,0000,0000,0000,,Đúng rồi, tuyệt quá.
Dialogue: 0,0:13:13.29,0:13:20.22,Default,,0000,0000,0000,,Nó đang di chuyển ở bên trong cô.
Dialogue: 0,0:13:20.57,0:13:23.09,Default,,0000,0000,0000,,Cô muốn cháu di chuyển mạnh hơn nữa.
Dialogue: 0,0:13:24.07,0:13:25.87,Default,,0000,0000,0000,,Cô ko thể dừng lại được.
Dialogue: 0,0:13:26.84,0:13:27.86,Default,,0000,0000,0000,,Nữa đi.
Dialogue: 0,0:13:30.41,0:13:37.21,Default,,0000,0000,0000,,Cô muốn cháu dùng cái mập mạp của cháu quấy đảo bên trong cô.
Dialogue: 0,0:13:38.02,0:13:42.01,Default,,0000,0000,0000,,Cô muốn cháu đâm vào mạnh hơn nữa.
Dialogue: 0,0:13:42.12,0:13:44.11,Default,,0000,0000,0000,,Đâm hẳn vào trong cô đi.
Dialogue: 0,0:13:44.76,0:13:46.92,Default,,0000,0000,0000,,Trông này Take-san.
Dialogue: 0,0:13:47.13,0:13:48.53,Default,,0000,0000,0000,,Nó thật mềm, phải ko?
Dialogue: 0,0:13:48.66,0:13:52.33,Default,,0000,0000,0000,,Hai đầu vú của cô dẻo như kẹo gôm, đúng ko?
Dialogue: 0,0:13:52.93,0:13:57.63,Default,,0000,0000,0000,,Nhưng nó thực sự chỉ toàn là thịt, và giống như món thịt bò hầm.
Dialogue: 0,0:13:58.51,0:14:01.37,Default,,0000,0000,0000,,Bộ ngực của cô cũng chắc và thơm ngon như thế.
Dialogue: 0,0:14:01.84,0:14:04.57,Default,,0000,0000,0000,,Cô muốn cháu thưởng thức chúng bằng lưỡi của mình.
Dialogue: 0,0:14:04.68,0:14:06.58,Default,,0000,0000,0000,,Cô sẽ thết đãi cháu cả đêm nay.
Dialogue: 0,0:14:06.68,0:14:08.58,Default,,0000,0000,0000,,Bây giờ hãy thưởng thức món ăn đi nào.
Dialogue: 0,0:14:08.72,0:14:11.12,Default,,0000,0000,0000,,Hãy chắc là đã liếm sạch tất cả.
Dialogue: 0,0:14:17.06,0:14:18.52,Default,,0000,0000,0000,,Sướng quá đi.
Dialogue: 0,0:14:18.69,0:14:20.68,Default,,0000,0000,0000,,Cơ thể cô đang nóng ran lên.
Dialogue: 0,0:14:21.73,0:14:25.69,Default,,0000,0000,0000,,Cái đó của cháu làm chỗ đó của cô khít lại.
Dialogue: 0,0:14:26.67,0:14:31.97,Default,,0000,0000,0000,,Khi cô di chuyển như thế, Cái đó của cô cứ thít chặt lại.
Dialogue: 0,0:14:33.77,0:14:36.77,Default,,0000,0000,0000,,Ôi chúa ơi, ko thể dừng lại được.
Dialogue: 0,0:14:37.01,0:14:40.17,Default,,0000,0000,0000,,Cô ko thể tiếp tục được nữa, Cô sẽ ra mất.
Dialogue: 0,0:14:40.91,0:14:43.31,Default,,0000,0000,0000,,Cô phát điên lên mất.
Dialogue: 0,0:14:43.88,0:14:46.68,Default,,0000,0000,0000,,Cô ko thể giữ lâu hơn nữa.
Dialogue: 0,0:14:48.79,0:14:55.59,Default,,0000,0000,0000,,Cơ thể cô đang quằn quại bên trên người cháu.
Dialogue: 0,0:14:56.46,0:14:58.06,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:15:00.10,0:15:03.16,Default,,0000,0000,0000,,Hình như cơ thể cô mất kiểm soát rồi.
Dialogue: 0,0:15:04.24,0:15:06.43,Default,,0000,0000,0000,,Nhưng cô ko quan tâm.
Dialogue: 0,0:15:07.21,0:15:09.20,Default,,0000,0000,0000,,Cô ko cần gì hết.
Dialogue: 0,0:15:11.71,0:15:14.11,Default,,0000,0000,0000,,Cái đó của cháu...
Dialogue: 0,0:15:15.08,0:15:18.75,Default,,0000,0000,0000,,Cô cảm thấy...
Dialogue: 0,0:15:18.85,0:15:21.25,Default,,0000,0000,0000,,Này Takeshi...
Dialogue: 0,0:15:21.72,0:15:23.38,Default,,0000,0000,0000,,Hãy ra nó vào bên trong cô.
Dialogue: 0,0:15:25.89,0:15:28.69,Default,,0000,0000,0000,,Sướng quá đi.
Dialogue: 0,0:15:33.53,0:15:38.20,Default,,0000,0000,0000,,No đang vào bên trong. Cô có thể cảm thấy cháu đang ra!
Dialogue: 0,0:15:43.88,0:15:47.81,Default,,0000,0000,0000,,Fujino-san, làm ơn dừng lại đi.
Dialogue: 0,0:15:47.98,0:15:51.42,Default,,0000,0000,0000,,Ko. Cô sẽ tiếp tục tận hưởng nó.
Dialogue: 0,0:16:28.16,0:16:29.92,Default,,0000,0000,0000,,Uống đi Take-san.
Dialogue: 0,0:16:30.16,0:16:33.13,Default,,0000,0000,0000,,Cô sẽ cho cháu nước của cô.
Dialogue: 0,0:16:38.53,0:16:40.16,Default,,0000,0000,0000,,Tắm nước nóng thật là dễ chịu.
Dialogue: 0,0:16:40.27,0:16:42.63,Default,,0000,0000,0000,,Tiếp tục nào, Take-san.
Dialogue: 0,0:16:44.84,0:16:48.64,Default,,0000,0000,0000,,Đừng đùa cô chứ. Thuốc vẫn còn thời gian tác dụng mà.
Dialogue: 0,0:16:48.90,0:16:52.14,sign,,0000,0000,0000,,{\pos(390,100)\an8\fad(2000,0)\i1}Cảm ơn vì tất cả. - Takeshi{\i0}
Dialogue: 0,0:16:50.71,0:16:52.14,Default,,0000,0000,0000,,Sao mọi việc lại trở nên thế này?
Dialogue: 0,0:16:52.51,0:16:54.14,Default,,0000,0000,0000,,Cô ko hề muốn như thế.
Dialogue: 0,0:16:54.41,0:16:56.94,Default,,0000,0000,0000,,Cô ko muốn mất đi tất cả những người thân yêu của mình.
Dialogue: 0,0:16:57.22,0:16:59.71,Default,,0000,0000,0000,,Cô đã làm gì sai chứ?
Dialogue: 0,0:16:59.95,0:17:03.18,Default,,0000,0000,0000,,Đầu tiên cô kết hôn với người đàn ông mà mình ko mong muốn, nhưng sau đó cô đã kết hôn được với người cô hằng mong ước.
Dialogue: 0,0:17:03.32,0:17:05.55,Default,,0000,0000,0000,,Nhưng chỉ trong chốc lát cô lại mất đi tất cả.
Dialogue: 0,0:17:05.73,0:17:09.79,Default,,0000,0000,0000,,Cô đã cố tìm một người bạn đời khác nhưng ko thành công.
Dialogue: 0,0:17:09.93,0:17:12.99,Default,,0000,0000,0000,,Và sau cùng cô tìm thấy cháu, người mang trong mình dòng máu của anh ấy.
Dialogue: 0,0:17:13.20,0:17:16.86,Default,,0000,0000,0000,,Cháu ko cần phải trở thành chồng của cô.
Dialogue: 0,0:17:17.10,0:17:20.07,Default,,0000,0000,0000,,Ý cô là, cháu là con trai anh ấy?
Dialogue: 0,0:17:20.14,0:17:23.44,Default,,0000,0000,0000,,Là con trai duy nhất của anh ấy.
Dialogue: 0,0:17:23.58,0:17:27.41,Default,,0000,0000,0000,,Nhưng tại sao? Nói cho cô đi, làm ơn...
Dialogue: 0,0:17:27.61,0:17:29.61,Default,,0000,0000,0000,,Cô van xin cháu.
Dialogue: 0,0:17:29.75,0:17:31.84,Default,,0000,0000,0000,,Đừng rời bỏ cô.
Dialogue: 0,0:17:34.05,0:17:36.89,Default,,0000,0000,0000,,Fujino-san, cô đã hài lòng chưa?
Dialogue: 0,0:17:38.56,0:17:41.96,Default,,0000,0000,0000,,Cám ơn vì tất cả những gì cô đã dành cho cháu.
Dialogue: 0,0:17:42.26,0:17:44.39,Default,,0000,0000,0000,,Cháu sẽ rời khỏi căn nhà này.
Dialogue: 0,0:17:45.40,0:17:50.56,Default,,0000,0000,0000,,Đã quá muộn để nói ra điều này nhưng, đáng lẽ ra cháu cần phải rời khỏi đây ngay từ lúc đó.
Dialogue: 0,0:17:51.00,0:17:57.50,Default,,0000,0000,0000,,Fujino-san, cô ko nhận ra là mình phải yêu thương con gái ruột của mình hơn cháu sao?
Dialogue: 0,0:17:58.35,0:18:02.04,Default,,0000,0000,0000,,Cho đến khi chúng ta có thể cùng nhau sống hạnh phúc dưới một mái nhà...
Dialogue: 0,0:18:02.38,0:18:04.61,Default,,0000,0000,0000,,Cháu sẽ ko quay lại nơi này nữa.
Dialogue: 0,0:18:09.82,0:18:10.85,Default,,0000,0000,0000,,Take-chan...
Dialogue: 0,0:18:12.39,0:18:14.26,Default,,0000,0000,0000,,Anh muốn nói với em chuyện gì nào?
Dialogue: 0,0:18:14.39,0:18:17.49,Default,,0000,0000,0000,,Shinobu, Anh đang định rời khỏi ngôi nhà này.
Dialogue: 0,0:18:17.70,0:18:19.86,Default,,0000,0000,0000,,Mọi chuyện có lẽ sẽ tốt hơn.
Dialogue: 0,0:18:20.13,0:18:22.23,Default,,0000,0000,0000,,Sao anh lại nói thế?
Dialogue: 0,0:18:23.80,0:18:26.24,Default,,0000,0000,0000,,Anh có  biết anh là thứ duy nhất còn lại với em lúc này ko.
Dialogue: 0,0:18:26.74,0:18:29.17,Default,,0000,0000,0000,,Anh đã hứa với em lúc ở bệnh viện.
Dialogue: 0,0:18:29.38,0:18:33.37,Default,,0000,0000,0000,,Em nói ngiêm túc đấy. Chỉ cần ở bên cạnh anh, em ko cần bất cứ thứ gì khác.
Dialogue: 0,0:18:33.61,0:18:35.60,Default,,0000,0000,0000,,Đó chỉ là do em quá kích động thôi.
Dialogue: 0,0:18:35.72,0:18:36.88,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:18:37.42,0:18:42.18,Default,,0000,0000,0000,,Em biết anh làm điều gì với mẹ em hằng đêm.
Dialogue: 0,0:18:42.32,0:18:45.69,Default,,0000,0000,0000,,Anh có biết điều đó khó khăn với em thế nào ko?
Dialogue: 0,0:18:47.33,0:18:50.52,Default,,0000,0000,0000,,Nhưng em ko thể kìm nén lòng mình lâu hơn nữa.
Dialogue: 0,0:18:50.66,0:18:53.39,Default,,0000,0000,0000,,Em rất yêu anh, Take-chan.
Dialogue: 0,0:18:53.67,0:18:57.36,Default,,0000,0000,0000,,Shinobu, Anh hiểu mà.
Dialogue: 0,0:18:57.87,0:19:00.40,Default,,0000,0000,0000,,Nhưng sau khi anh đi, em sẽ sớm quên đi tất cả thôi.
Dialogue: 0,0:19:01.07,0:19:07.41,Default,,0000,0000,0000,,Em ko ngốc như anh nghĩ đâu, để em cho anh thấy.
Dialogue: 0,0:19:20.06,0:19:21.79,Default,,0000,0000,0000,,Wow, nó to thật.
Dialogue: 0,0:19:22.33,0:19:24.82,Default,,0000,0000,0000,,Shinobu, cứ làm như em muốn.
Dialogue: 0,0:20:32.53,0:20:33.69,Default,,0000,0000,0000,,Nó nóng thật.
Dialogue: 0,0:20:34.00,0:20:37.94,Default,,0000,0000,0000,,sữa của anh tràn hết ra ngoài rồi.
Dialogue: 0,0:20:38.57,0:20:40.44,Default,,0000,0000,0000,,Em nghĩ mình sẽ tan chảy ra mất.
Dialogue: 0,0:20:40.87,0:20:42.34,Default,,0000,0000,0000,,Em ổn chứ, Shinobu?
Dialogue: 0,0:20:42.51,0:20:43.64,Default,,0000,0000,0000,,Em ko sao.
Dialogue: 0,0:20:43.71,0:20:44.73,Default,,0000,0000,0000,,Xin lỗi em.
Dialogue: 0,0:20:45.88,0:20:49.47,Default,,0000,0000,0000,,Thật tuyệt vời. Nó giống như thạch vậy.
Dialogue: 0,0:20:50.22,0:20:52.24,Default,,0000,0000,0000,,Nó còn hơn cả điều em mong đợi.
Dialogue: 0,0:20:52.35,0:20:58.19,Default,,0000,0000,0000,,Sau khi anh ra nhiều sữa như vậy mà cái đó của anh vẫn cứng thế này.
Dialogue: 0,0:20:58.46,0:21:01.79,Default,,0000,0000,0000,,Cái đó của anh đúng là một con quái vật hung hãn.
Dialogue: 0,0:21:13.37,0:21:15.17,Default,,0000,0000,0000,,Dừng lại, Take-chan.
Dialogue: 0,0:21:15.54,0:21:16.70,Default,,0000,0000,0000,,Đâu đấy.
Dialogue: 0,0:21:16.81,0:21:18.14,Default,,0000,0000,0000,,Chưa đến lúc mà.
Dialogue: 0,0:21:18.71,0:21:21.41,Default,,0000,0000,0000,,Đau quá, Đau quá, Đau quá.
Dialogue: 0,0:21:24.95,0:21:26.42,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:21:31.59,0:21:33.56,Default,,0000,0000,0000,,Dừng lại! Đau quá!
Dialogue: 0,0:21:34.06,0:21:35.82,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:21:43.74,0:21:45.10,Default,,0000,0000,0000,,Ko, đau lắm!
Dialogue: 0,0:21:46.51,0:21:48.67,Default,,0000,0000,0000,,Đau quá. Đau quá.
Dialogue: 0,0:21:56.62,0:21:59.05,Default,,0000,0000,0000,,Ko, ko phải chỗ đó.
Dialogue: 0,0:21:59.62,0:22:01.52,Default,,0000,0000,0000,,Anh nhầm chỗ rồi.
Dialogue: 0,0:22:02.69,0:22:04.71,Default,,0000,0000,0000,,Đau quá. Đau quá.
Dialogue: 0,0:22:07.09,0:22:08.08,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:22:42.13,0:22:42.82,Default,,0000,0000,0000,,Đợi đã nào.
Dialogue: 0,0:22:43.86,0:22:45.99,Default,,0000,0000,0000,,Take-chan, Đau quá.
Dialogue: 0,0:22:54.34,0:22:56.81,Default,,0000,0000,0000,,Đau quá, Đau quá.
Dialogue: 0,0:23:15.03,0:23:17.39,Default,,0000,0000,0000,,Đau quá, Đau quá.
Dialogue: 0,0:23:18.77,0:23:20.82,Default,,0000,0000,0000,,Đau quá, Đau quá.
Dialogue: 0,0:23:21.13,0:23:24.37,Default,,0000,0000,0000,,Ko, Đau quá. Ko!
Dialogue: 0,0:23:26.54,0:23:28.37,Default,,0000,0000,0000,,Anh xin lỗi, Shinobu.
Dialogue: 0,0:23:28.74,0:23:31.27,Default,,0000,0000,0000,,Anh đang cố để quên đi Fumi-nee, Fujino-san, và mọi thứ...
Dialogue: 0,0:23:31.38,0:23:34.18,Default,,0000,0000,0000,,...bằng cách chịu đựng nó và cố tỏ ra mình đã trưởng thành.
Dialogue: 0,0:23:34.38,0:23:38.75,Default,,0000,0000,0000,,Nhưng khi anh làm điều đó vơi em, những thứ bị đè nén đó trong anh đã trỗi dậy.
Dialogue: 0,0:23:38.99,0:23:41.32,Default,,0000,0000,0000,,Và anh đã trút tất cả lên cở thể em .
Dialogue: 0,0:23:41.42,0:23:43.25,Default,,0000,0000,0000,,Anh xin lỗi. Anh thật sự xin lỗi.
Dialogue: 0,0:23:43.89,0:23:46.79,Default,,0000,0000,0000,,Em ko sao mà.
Dialogue: 0,0:23:46.93,0:23:49.22,Default,,0000,0000,0000,,Em hiểu cảm giác của anh.
Dialogue: 0,0:23:49.36,0:23:52.33,Default,,0000,0000,0000,,Em khóc chỉ vì em hơi kinh ngạc.
Dialogue: 0,0:23:52.57,0:23:56.52,Default,,0000,0000,0000,,Nhưng em vẫn luôn muốn làm điều đó với anh.
Dialogue: 0,0:23:56.67,0:24:02.84,Default,,0000,0000,0000,,Âm hộ của em luôn dành cho anh.
Dialogue: 0,0:24:03.11,0:24:05.94,Default,,0000,0000,0000,,Làm ơn hãy chấp nhận em làm vợ anh.
Dialogue: 0,0:24:06.08,0:24:09.41,Default,,0000,0000,0000,,Em muốn anh hãy ra tất cả vào bên trong em.
Dialogue: 0,0:24:23.43,0:24:25.12,Default,,0000,0000,0000,,Em thực sự ổn chứ, Shinobu?
Dialogue: 0,0:24:28.70,0:24:32.90,Default,,0000,0000,0000,,Em luôn muốn anh làm thế, nên ko sao cả đâu.
Dialogue: 0,0:25:04.60,0:25:07.27,Default,,0000,0000,0000,,Em thấy sướng chứ?
Dialogue: 0,0:25:07.57,0:25:11.77,Default,,0000,0000,0000,,Đó là vì em đã dùng ngón tay của mình trước đó.
Dialogue: 0,0:25:13.38,0:25:15.81,Default,,0000,0000,0000,,Anh có thể làm nhanh hơn nữa.
Dialogue: 0,0:25:16.05,0:25:18.57,Default,,0000,0000,0000,,Hãy làm như anh muốn.
Dialogue: 0,0:25:19.05,0:25:22.95,Default,,0000,0000,0000,,Anh có thể đâm sâu hơn nữa, biến em trở thành đàn bà thực thụ.
Dialogue: 0,0:25:25.53,0:25:27.89,Default,,0000,0000,0000,,Em bắt đầu thấy sướng rồi.
Dialogue: 0,0:25:28.09,0:25:30.19,Default,,0000,0000,0000,,Sướng quá, Take-chan.
Dialogue: 0,0:25:31.60,0:25:34.76,Default,,0000,0000,0000,,Em yêu cậu bé của anh.
Dialogue: 0,0:25:52.02,0:25:52.92,Default,,0000,0000,0000,,Anh sắp ra rồi.
Dialogue: 0,0:25:53.29,0:25:55.69,Default,,0000,0000,0000,,Hãy ra vào bên trong em, Take-chan.
Dialogue: 0,0:26:17.21,0:26:19.61,Default,,0000,0000,0000,,Em yêu anh Take-chan.
Dialogue: 0,0:28:28.14,0:28:31.11,sign,,0000,0000,0000,,{\pos(320,312)}Một năm sau
Dialogue: 0,0:28:32.65,0:28:33.81,Default,,0000,0000,0000,,Anh về rồi đây.
Dialogue: 0,0:28:33.95,0:28:35.04,Default,,0000,0000,0000,,Mừng cháu trở về.
Dialogue: 0,0:28:35.18,0:28:37.08,Default,,0000,0000,0000,,Mừng anh trở về Take-chan.
Dialogue: 0,0:28:37.22,0:28:38.77,Default,,0000,0000,0000,,Takeshi, chào em.
Dialogue: 0,0:28:38.99,0:28:41.11,Default,,0000,0000,0000,,Mừng chị đã quay lại Fumi-nee.
Dialogue: 0,0:29:40.15,0:29:45.14,Default,,0000,0000,0000,,Cả bốn chúng tôi đã tiếp tục chung sống hạnh phúc dưới một mái nhà.
Dialogue: 0,0:29:45.35,0:29:48.68,Default,,0000,0000,0000,,Một gia đình mà mọi thành viên đều ko thể sống thiếu nhau!
