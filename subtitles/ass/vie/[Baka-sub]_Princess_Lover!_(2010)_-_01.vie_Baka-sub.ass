Dialogue: 0,0:03:13.74,0:03:15.67,Default,,0000,0000,0000,,Anh vẫn chưa thỏa mãn sao?
Dialogue: 0,0:03:16.21,0:03:21.09,Default,,0000,0000,0000,,Anh vẫn cố... luồn sâu vào chỗ yếu nhược nhất của em...
Dialogue: 0,0:03:21.45,0:03:24.14,Default,,0000,0000,0000,,Nếu anh còn tiếp tục...
Dialogue: 0,0:03:25.37,0:03:30.64,Default,,0000,0000,0000,,T-Tại sao... cơ thể người phụ nữ...
Dialogue: 0,0:03:31.54,0:03:32.76,Default,,0000,0000,0000,,...lại dễ bị chi phối đến vậy?!
Dialogue: 0,0:03:33.85,0:03:38.16,Default,,0000,0000,0000,,Hơi ấm... đang lan dần khắp cơ thể em...
Dialogue: 0,0:03:40.15,0:03:42.03,Default,,0000,0000,0000,,Đầu óc em hoàn toàn trống rỗng...
Dialogue: 0,0:03:42.03,0:03:44.20,Default,,0000,0000,0000,,Em... Em...
Dialogue: 0,0:03:52.68,0:03:56.54,Default,,0000,0000,0000,,Không... cảm giác này...
Dialogue: 0,0:03:59.35,0:04:02.11,Default,,0000,0000,0000,,Này, k-khoan đã...
Dialogue: 0,0:04:02.11,0:04:03.61,Default,,0000,0000,0000,,Teppei!
Dialogue: 0,0:04:15.11,0:04:16.52,Default,,0000,0000,0000,,Em không nên kiềm chế làm gì.
Dialogue: 0,0:04:39.06,0:04:40.05,Default,,0000,0000,0000,,Marchez!
Dialogue: 0,0:04:47.85,0:04:50.35,Default,,0000,0000,0000,,Hay quá!
Dialogue: 0,0:04:50.60,0:04:53.35,Default,,0000,0000,0000,,Anh chắc chứ, Teppei-sama?
Dialogue: 0,0:04:53.35,0:04:55.58,Default,,0000,0000,0000,,Về việc tiếp tục giữ im lặng với cô ấy?
Dialogue: 0,0:04:59.15,0:05:02.25,Default,,0000,0000,0000,,Sâu bướm phải trải qua giai đoạn ngài trước khi hóa bướm.
Dialogue: 0,0:05:02.23,0:05:05.61,Default,,0000,0000,0000,,Có lẽ vài ngày tới sẽ khó ăn khó nói lắm đây.
Dialogue: 0,0:05:05.60,0:05:07.87,Default,,0000,0000,0000,,"Hả?  Mình đang bay."
Dialogue: 0,0:05:08.75,0:05:11.00,Default,,0000,0000,0000,,"Thế giới lúc này trông khác quá."
Dialogue: 0,0:05:10.99,0:05:12.95,Default,,0000,0000,0000,,"Cảnh sắc thật rực rỡ phải không?"
Dialogue: 0,0:05:13.41,0:05:15.21,Default,,0000,0000,0000,,"Trong khi trí óc mình vẫn là một con sâu bướm."
Dialogue: 0,0:05:15.85,0:05:20.34,Default,,0000,0000,0000,,Anh kể chuyện đó với em mỗi ngày, vậy nên em nhớ rõ lắm.
Dialogue: 0,0:05:23.45,0:05:28.76,Default,,0000,0000,0000,,Anh không thực sự thuộc về thế giới này, có lẽ vậy.
Dialogue: 0,0:05:28.99,0:05:32.25,Default,,0000,0000,0000,,Có lẽ em là người duy nhất cho rằng điều đó không đúng.
Dialogue: 0,0:05:32.25,0:05:38.22,Default,,0000,0000,0000,,Được ở bên và phục vụ Teppei-sama là khoảng thời gian tuyệt vời nhất của em.
Dialogue: 0,0:05:46.15,0:05:47.19,Default,,0000,0000,0000,,Teppei-sama.
Dialogue: 0,0:05:51.98,0:05:54.72,Default,,0000,0000,0000,,Cô ấy không hề thích thú khi đứng dưới ánh hào quang.
Dialogue: 0,0:05:54.72,0:05:59.66,Default,,0000,0000,0000,,Người nhu mì như tiểu thư nhà Van Hossen đúng là có một không hai.
Dialogue: 0,0:05:59.66,0:06:01.63,Default,,0000,0000,0000,,Cám ơn em đã quan tâm.
Dialogue: 0,0:06:02.49,0:06:07.94,Default,,0000,0000,0000,,Có lẽ anh đã bức ép một người nhu mì như cô ấy hơi quá đáng.
Dialogue: 0,0:06:13.34,0:06:17.94,Default,,0000,0000,0000,,Em vẫn luôn thích nghe những lời nhận xét chân thành và khoan dung của anh.
Dialogue: 0,0:06:18.38,0:06:19.04,Default,,0000,0000,0000,,Hử?
Dialogue: 0,0:06:23.74,0:06:26.10,Default,,0000,0000,0000,,Xin lỗi đã khiến tiểu thư đợi lâu, Sylvie-sama.
Dialogue: 0,0:06:27.21,0:06:30.19,Default,,0000,0000,0000,,Xe hộ tống đã sẵn sàng.
Dialogue: 0,0:06:30.19,0:06:32.94,Default,,0000,0000,0000,,Teppei-sama hình như đang tìm tiểu thư.
Dialogue: 0,0:06:32.94,0:06:34.94,Default,,0000,0000,0000,,Tiểu thư đã gặp cậu chủ chưa?
Dialogue: 0,0:06:35.34,0:06:38.61,Default,,0000,0000,0000,,Nhắn lại với anh ấy là tôi về trước.
Dialogue: 0,0:06:39.34,0:06:41.96,Default,,0000,0000,0000,,Tôi tự mình về nhà được.
Dialogue: 0,0:06:41.96,0:06:44.00,Default,,0000,0000,0000,,Nếu vậy, tôi sẽ báo lại với cậu chủ.
Dialogue: 0,0:06:45.83,0:06:48.22,Default,,0000,0000,0000,,Hôm nay cô tới võ đường muộn hơn mọi khi nhỉ?
Dialogue: 0,0:06:48.22,0:06:49.30,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:06:49.62,0:06:52.03,Default,,0000,0000,0000,,Cô tới cùng Arima-dono hả?!
Dialogue: 0,0:06:52.03,0:06:53.52,Default,,0000,0000,0000,,Anh ấy có nói...
Dialogue: 0,0:06:54.65,0:06:56.77,Default,,0000,0000,0000,,À, không có gì đâu.
Dialogue: 0,0:06:57.05,0:07:02.27,Default,,0000,0000,0000,,Bổn phận của tôi là ở bên Teppei-sama và thỏa mãn mọi yêu cầu của cậu ấy.
Dialogue: 0,0:07:03.66,0:07:09.32,Default,,0000,0000,0000,,Sự hài lòng của Teppei-sama cũng chính là niềm vui đối với tôi.
Dialogue: 0,0:07:19.71,0:07:22.51,Default,,0000,0000,0000,,Yuu-san, em thay đồ xong chưa?
Dialogue: 0,0:07:29.21,0:07:31.05,Default,,0000,0000,0000,,S-Sylvie!
Dialogue: 0,0:07:31.38,0:07:32.83,Default,,0000,0000,0000,,Arima-sama kìa!
Dialogue: 0,0:07:33.59,0:07:36.10,Default,,0000,0000,0000,,Có thật là cậu ấy không?
Dialogue: 0,0:07:41.06,0:07:42.95,Default,,0000,0000,0000,,Mùi hương còn sót lại này...
Dialogue: 0,0:07:42.95,0:07:45.97,Default,,0000,0000,0000,,Là của ả hầu gái vẫn thường phục vụ anh phải không?
Dialogue: 0,0:07:46.92,0:07:48.36,Default,,0000,0000,0000,,Anh không thấy xấu hổ sao?
Dialogue: 0,0:07:48.80,0:07:55.28,Default,,0000,0000,0000,,Có lẽ em hơi khiếm nhã khi xen vào mối quan hệ chủ nhân-hầu gái của một gia tộc khác,
Dialogue: 0,0:07:55.28,0:07:58.29,Default,,0000,0000,0000,,nhưng anh cũng phải có chừng mực chứ, Arima-dono.
Dialogue: 0,0:07:58.90,0:08:00.89,Default,,0000,0000,0000,,Anh không cảm thấy xấu hổ chút nào hay sao?
Dialogue: 0,0:08:00.89,0:08:02.54,Default,,0000,0000,0000,,Khi làm chuyện như vậy...
Dialogue: 0,0:08:12.55,0:08:17.01,Default,,0000,0000,0000,,Anh luôn đối xử bình đẳng với mọi người, ngay cả với những người hầu như em.
Dialogue: 0,0:08:17.01,0:08:22.06,Default,,0000,0000,0000,,Lòng nhân hậu và sự chân thành chính là điểm hấp dẫn nhất của anh, Teppei-sama.
Dialogue: 0,0:08:23.11,0:08:29.11,Default,,0000,0000,0000,,Tuy nhiên, xin hãy cho phép em được làm tròn bổn phận của mình.
Dialogue: 0,0:08:32.89,0:08:37.54,Default,,0000,0000,0000,,Không ai được phép để người thừa kế của gia tộc Arima mất đi danh dự bởi chính những người xung quanh mình.
Dialogue: 0,0:08:38.04,0:08:43.72,Default,,0000,0000,0000,,Nếu để cậu chủ phiền muộn, mọi người sẽ xem mình như kẻ không biết kính trên nhường dưới.
Dialogue: 0,0:08:43.72,0:08:46.44,Default,,0000,0000,0000,,Nhân cách cao đẹp của Teppei-sama sẽ tiếp tục tỏa sáng.
Dialogue: 0,0:08:47.76,0:08:50.20,Default,,0000,0000,0000,,Mình phải giúp cậu chủ xứng đáng với tư cách người thừa kế.
Dialogue: 0,0:08:50.20,0:08:57.01,Default,,0000,0000,0000,,Em sẵn lòng làm theo mọi điều anh muốn, để giúp anh sớm nhận ra nghĩa vụ và trách nhiệm của mình lúc này.
Dialogue: 0,0:09:17.32,0:09:18.41,Default,,0000,0000,0000,,Thật dâm đãng.
Dialogue: 0,0:09:18.83,0:09:22.29,Default,,0000,0000,0000,,Những chuyện như thế này là không thể tha thứ.
Dialogue: 0,0:09:22.29,0:09:24.54,Default,,0000,0000,0000,,Và em cũng sẽ không bao giờ tha thứ cho anh đâu.
Dialogue: 0,0:09:25.80,0:09:27.88,Default,,0000,0000,0000,,Đừng mơ có chuyện em tha thứ cho anh.
Dialogue: 0,0:09:27.88,0:09:32.54,Default,,0000,0000,0000,,{\fad(0,450)}Anh làm cho vị hôn thê của mình có những suy nghĩ đồi bại, không đứng đắn...
Dialogue: 0,0:09:59.73,0:10:01.86,Default,,0000,0000,0000,,Cô ta làm chuyện đó thế nào?
Dialogue: 0,0:10:01.86,0:10:02.82,Default,,0000,0000,0000,,Thế này phải không?
Dialogue: 0,0:10:12.71,0:10:18.77,Default,,0000,0000,0000,,Em sẽ không tha thứ cho mình vì đã tự nguyện gắn chặt cuộc đời cùng anh sau "lần đó".
Dialogue: 0,0:10:18.77,0:10:20.26,Default,,0000,0000,0000,,Hơn nữa...
Dialogue: 0,0:10:21.00,0:10:25.40,Default,,0000,0000,0000,,Em không thể tha thứ cho mình vì đã để cho anh dùng thân xác để thực hiện âm mưu trói buộc hèn hạ đó,
Dialogue: 0,0:10:25.40,0:10:27.94,Default,,0000,0000,0000,,và cả việc anh lấy em ra làm trò đùa!
Dialogue: 0,0:10:30.02,0:10:31.75,Default,,0000,0000,0000,,S-Sylvie!
Dialogue: 0,0:10:31.75,0:10:33.85,Default,,0000,0000,0000,,Đây không phải nơi để chúng ta...
Dialogue: 0,0:10:33.85,0:10:35.53,Default,,0000,0000,0000,,Mọi người sẽ trông thấy mất{\fscx300}-
Dialogue: 0,0:10:36.00,0:10:40.39,Default,,0000,0000,0000,,Hầu gái phục vụ chủ nhân của mình. Có gì mà anh phải e ngại chứ?
Dialogue: 0,0:10:42.53,0:10:46.25,Default,,0000,0000,0000,,Cảm giác bên trong ngực em có gì khác biệt không?
Dialogue: 0,0:10:46.92,0:10:47.80,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:10:49.38,0:10:52.39,Default,,0000,0000,0000,,Dù em có chút hiểu biết về chuyện này.
Dialogue: 0,0:10:52.39,0:10:57.99,Default,,0000,0000,0000,,Ba vẫn đưa em vài quyển tạp chí khiêu dâm, và nói rằng em cần học cách để trở thành phụ nữ thực thụ.
Dialogue: 0,0:10:57.99,0:11:01.90,Default,,0000,0000,0000,,Nhìn bề ngoài thì nó giống như một phần trong "chuyện vợ chồng" ban đêm...
Dialogue: 0,0:11:01.90,0:11:03.08,Default,,0000,0000,0000,,Sao hả?
Dialogue: 0,0:11:03.08,0:11:04.98,Default,,0000,0000,0000,,Em làm đúng không?
Dialogue: 0,0:11:05.45,0:11:06.93,Default,,0000,0000,0000,,À, ờ...
Dialogue: 0,0:11:07.30,0:11:10.02,Default,,0000,0000,0000,,Hi vọng thế này sẽ khiến anh thỏa mãn.
Dialogue: 0,0:11:12.22,0:11:15.10,Default,,0000,0000,0000,,Có vẻ như anh muốn kết thúc buổi "chiêu đãi" này rồi ha?!
Dialogue: 0,0:11:28.96,0:11:32.05,Default,,0000,0000,0000,,Em có... đáng yêu không?
Dialogue: 0,0:11:33.03,0:11:40.63,Default,,0000,0000,0000,,Em không cần... lời tán dương của anh...
Dialogue: 0,0:11:41.44,0:11:45.94,Default,,0000,0000,0000,,Nhưng trước nỗi khát khao hiện lên trong đôi mắt anh...
Dialogue: 0,0:11:51.85,0:11:54.14,Default,,0000,0000,0000,,Em không thể làm gì khác ngoài đáp lại nó...
Dialogue: 0,0:11:59.00,0:12:02.57,Default,,0000,0000,0000,,Cảm giác... thế nào hả?
Dialogue: 0,0:12:02.95,0:12:06.98,Default,,0000,0000,0000,,Em... không thực sự biết rõ...
Dialogue: 0,0:12:06.98,0:12:10.95,Default,,0000,0000,0000,,Làm cách nào để khiến anh sung sướng hơn nữa...
Dialogue: 0,0:12:16.94,0:12:18.80,Default,,0000,0000,0000,,Nói em nghe đi...
Dialogue: 0,0:12:20.90,0:12:23.50,Default,,0000,0000,0000,,Anh thấy dễ chịu chứ?
Dialogue: 0,0:12:29.50,0:12:30.46,Default,,0000,0000,0000,,Gì vậy?
Dialogue: 0,0:12:30.46,0:12:32.94,Default,,0000,0000,0000,,Nó đang bự lên... nóng quá!
Dialogue: 0,0:12:37.84,0:12:42.05,Default,,0000,0000,0000,,X-Xin lỗi, anh không thể kiềm chế nổi.
Dialogue: 0,0:12:44.70,0:12:47.53,Default,,0000,0000,0000,,Vậy ra đây chính là cội nguồn của sự sống ư?!
Dialogue: 0,0:12:48.55,0:12:50.88,Default,,0000,0000,0000,,Thảo nào mà nó lại nóng bỏng như vậy!?
Dialogue: 0,0:12:52.35,0:12:54.54,Default,,0000,0000,0000,,Một cảm giác ấm áp diệu kì...
Dialogue: 0,0:12:57.38,0:12:58.90,Default,,0000,0000,0000,,A-Arima-dono?!
Dialogue: 0,0:12:59.90,0:13:02.93,Default,,0000,0000,0000,,A-Anh làm gì vậy?  Mau dừng lại!
Dialogue: 0,0:13:06.32,0:13:07.34,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:13:07.77,0:13:11.29,Default,,0000,0000,0000,,Không phải chỉ cần làm cho đàn ông ra là xong hết sao?
Dialogue: 0,0:13:13.32,0:13:14.96,Default,,0000,0000,0000,,Nhưng đối với phụ nữ thì chưa xong đâu.
Dialogue: 0,0:13:14.96,0:13:16.98,Default,,0000,0000,0000,,Đ-Đừng làm vậy.
Dialogue: 0,0:13:16.98,0:13:19.50,Default,,0000,0000,0000,,Đêm qua chúng ta vừa làm chuyện đó xong mà, nếu tiếp tục{\fscx300}-
Dialogue: 0,0:13:19.50,0:13:22.10,Default,,0000,0000,0000,,Anh phải đáp lại tình yêu mà em dành cho anh chứ, Sylvie.
Dialogue: 0,0:13:22.10,0:13:24.82,Default,,0000,0000,0000,,Nhưng nếu vậy thì em sẽ phải kết thúc "tiệc chiêu đãi" lần nữa...
Dialogue: 0,0:13:30.40,0:13:34.41,Default,,0000,0000,0000,,L-Làm ơn dừng lại đi mà, A-Arima-dono?
Dialogue: 0,0:13:35.52,0:13:39.38,Default,,0000,0000,0000,,"Teppei".  Không phải đêm qua em đã gọi anh bằng cái tên này sao?
Dialogue: 0,0:13:39.81,0:13:41.87,Default,,0000,0000,0000,,Anh không thể... làm...
Dialogue: 0,0:13:49.35,0:13:52.12,Default,,0000,0000,0000,,Đ... Đêm qua em đã bảo anh rồi...
Dialogue: 0,0:13:54.65,0:13:57.81,Default,,0000,0000,0000,,Chuyện này có thể khiến chúng ta yêu nhau được sao?
Dialogue: 0,0:14:04.85,0:14:06.61,Default,,0000,0000,0000,,Tình yêu...
Dialogue: 0,0:14:15.39,0:14:17.43,Default,,0000,0000,0000,,Đ-Đừng mà...
Dialogue: 0,0:14:17.43,0:14:21.04,Default,,0000,0000,0000,,Nó đang đâm sâu vào bên trong, rát quá...
Dialogue: 0,0:14:21.95,0:14:23.63,Default,,0000,0000,0000,,Nếu anh nhét cả vào bên trong như vậy...
Dialogue: 0,0:14:26.98,0:14:29.43,Default,,0000,0000,0000,,Nhưng em không thể...
Dialogue: 0,0:14:29.43,0:14:33.09,Default,,0000,0000,0000,,Em không thể đáp lại tình cảm của anh được...
Dialogue: 0,0:14:34.64,0:14:37.65,Default,,0000,0000,0000,,Buông em ra, xin anh đấy...
Dialogue: 0,0:14:37.65,0:14:39.23,Default,,0000,0000,0000,,Buông em ra đi mà...
Dialogue: 0,0:14:40.58,0:14:46.74,Default,,0000,0000,0000,,Em đã nói không được rồi mà, sao thằng nhỏ của anh vẫn bự và cứng như vậy...
Dialogue: 0,0:14:47.88,0:14:49.65,Default,,0000,0000,0000,,Khoan đã... bự lên ư?
Dialogue: 0,0:14:49.65,0:14:50.99,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:14:51.50,0:14:55.00,Default,,0000,0000,0000,,Đêm qua anh đã ra nhiều như vậy mà...
Dialogue: 0,0:14:55.00,0:14:57.75,Default,,0000,0000,0000,,Anh không thể lặp lại điều đó trong hôm nay được.
Dialogue: 0,0:14:57.75,0:15:00.59,Default,,0000,0000,0000,,Em không cho phép anh làm vậy đâu!
Dialogue: 0,0:15:05.65,0:15:08.51,Default,,0000,0000,0000,,Nó đang co giật đấy ư?
Dialogue: 0,0:15:08.51,0:15:10.01,Default,,0000,0000,0000,,Nếu anh không mau mau dừng lại...
Dialogue: 0,0:15:10.01,0:15:12.18,Default,,0000,0000,0000,,Không, nó lại bự lên nữa rồi...
Dialogue: 0,0:15:12.50,0:15:14.77,Default,,0000,0000,0000,,Không... Mau dừng lại, mau dừng lại!
Dialogue: 0,0:15:24.86,0:15:27.18,Default,,0000,0000,0000,,Vậy là anh vẫn cố làm tới cùng ư?
Dialogue: 0,0:15:28.17,0:15:32.69,Default,,0000,0000,0000,,Bắn vào bên trong người ta nhiều như vậy mà chẳng báo trước gì cả...
Dialogue: 0,0:15:34.20,0:15:35.82,Default,,0000,0000,0000,,Khoan, chúng ta vẫn còn chưa tách nhau ra mà!
Dialogue: 0,0:15:35.82,0:15:37.05,Default,,0000,0000,0000,,Nữa ư?
Dialogue: 0,0:15:37.05,0:15:39.24,Default,,0000,0000,0000,,Đừng mà... Đừng mà!
Dialogue: 0,0:17:06.76,0:17:08.96,Default,,0000,0000,0000,,Mới chỉ có hai ngày thôi sao?
Dialogue: 0,0:17:09.61,0:17:12.79,Default,,0000,0000,0000,,Mình đã làm chuyện đó cùng anh ta không biết bao nhiêu lần...
Dialogue: 0,0:17:12.79,0:17:15.88,Default,,0000,0000,0000,,Tại sao mình lại để anh ta tự tung tự tác như vậy chứ?
Dialogue: 0,0:17:17.51,0:17:19.39,Default,,0000,0000,0000,,Không lẽ nào...
Dialogue: 0,0:17:25.62,0:17:28.78,Default,,0000,0000,0000,,Mình đã thực sự trở thành quân cờ dưới tay cha?
Dialogue: 0,0:17:31.15,0:17:34.65,Black,,0000,0000,0000,,{\fs50\pos(396,360)}Sáng ngày hôm kia    ...
Dialogue: 0,0:17:33.67,0:17:34.65,Default,,0000,0000,0000,,Teppei-sama.
Dialogue: 0,0:17:35.20,0:17:39.68,Default,,0000,0000,0000,,Xin lỗi anh, nhưng đây là chuyện khẩn cấp.
Dialogue: 0,0:17:43.42,0:17:45.11,Default,,0000,0000,0000,,Mấy giờ rồi?
Dialogue: 0,0:17:45.11,0:17:46.53,Default,,0000,0000,0000,,7 giờ 5.
Dialogue: 0,0:17:49.08,0:17:52.88,Default,,0000,0000,0000,,Em biết anh rất mệt, nhưng xin anh hãy cố gắng.
Dialogue: 0,0:17:54.75,0:17:56.07,Default,,0000,0000,0000,,Dậy nào anh.
Dialogue: 0,0:17:56.07,0:17:58.79,Default,,0000,0000,0000,,Chuyện này nằm ngoài quyền quyết định của em.
Dialogue: 0,0:18:01.90,0:18:05.73,Default,,0000,0000,0000,,Một quý ông chân chính lại bắt phụ nữ đợi lâu như vậy sao?!
Dialogue: 0,0:18:08.22,0:18:11.00,Default,,0000,0000,0000,,Thứ lỗi cho tôi vì đã làm phiền anh vào lúc sáng sớm thế này, Arima-dono.
Dialogue: 0,0:18:11.00,0:18:13.36,Default,,0000,0000,0000,,Chào buổi sáng, Teppei Onii-chan.
Dialogue: 0,0:18:14.28,0:18:14.96,Default,,0000,0000,0000,,Ế?
Dialogue: 0,0:18:16.31,0:18:20.48,Default,,0000,0000,0000,,Ba em sẽ đi công tác xa nhà trong 2 ngày tới,
Dialogue: 0,0:18:20.48,0:18:24.54,Default,,0000,0000,0000,,vậy nên ông đã giao phó tụi em cho một người, cho tới lúc ông trở về.
Dialogue: 0,0:18:24.54,0:18:25.95,Default,,0000,0000,0000,,Cho ai vậy?
Dialogue: 0,0:18:25.95,0:18:28.90,Default,,0000,0000,0000,,Hai chị em tôi đang đứng sờ sờ trước mặt anh còn gì.
Dialogue: 0,0:18:28.90,0:18:30.54,Default,,0000,0000,0000,,Chẳng phải quá rõ ràng sao?
Dialogue: 0,0:18:30.54,0:18:33.05,Default,,0000,0000,0000,,Hôm nay chị khó tính thế.
Dialogue: 0,0:18:33.05,0:18:35.56,Default,,0000,0000,0000,,Con gái mà hay ghen tị thế không biết.
Dialogue: 0,0:18:35.82,0:18:39.19,Default,,0000,0000,0000,,Tôi cũng vừa mới biết chuyện này đêm qua, thật chẳng hợp tình hợp lý gì cả.
Dialogue: 0,0:18:39.19,0:18:41.05,Default,,0000,0000,0000,,Tôi không vừa lòng với chuyện này một chút nào cả.
Dialogue: 0,0:18:41.97,0:18:45.36,Default,,0000,0000,0000,,Chị đừng ra vẻ không thích thú một chút nào về chuyện đó như vậy chứ.
Dialogue: 0,0:18:45.36,0:18:49.64,Default,,0000,0000,0000,,Thôi kệ, Onee-chan là Tsundere chính hiệu mà.
Dialogue: 0,0:18:50.71,0:18:54.69,Default,,0000,0000,0000,,Đúng là gia tộc anh có ơn cứu mạng với gia đình tôi.
Dialogue: 0,0:18:55.33,0:19:00.63,Default,,0000,0000,0000,,Hiển nhiên là chúng tôi có nghĩa vụ tuân theo mọi điều mà họ giao phó, cũng như biết ơn gia tộc anh trọn đời.
Dialogue: 0,0:19:01.45,0:19:06.33,Default,,0000,0000,0000,,Kể cả như vậy, gia tộc anh cũng không có quyền quyết định hôn sự của tôi.
Dialogue: 0,0:19:06.33,0:19:07.66,Default,,0000,0000,0000,,Anh có đồng ý không?
Dialogue: 0,0:19:10.31,0:19:15.09,Default,,0000,0000,0000,,Em không thích làm hôn thê của anh đến vậy sao?
Dialogue: 0,0:19:15.09,0:19:17.14,Default,,0000,0000,0000,,Nghe đau lòng thật đấy.
Dialogue: 0,0:19:17.14,0:19:18.95,Default,,0000,0000,0000,,Đó là một quan niệm chuẩn mực!
Dialogue: 0,0:19:18.95,0:19:20.95,Default,,0000,0000,0000,,Anh cũng có một phần trách nhiệm ở đây.
Dialogue: 0,0:19:20.95,0:19:23.90,Default,,0000,0000,0000,,Chỉ cần anh từ chối thẳng thừng...
Dialogue: 0,0:19:23.90,0:19:26.81,Default,,0000,0000,0000,,Nhưng anh yêu em mà, Sylvie.
Dialogue: 0,0:19:43.93,0:19:47.04,Default,,0000,0000,0000,,Chẳng phải anh đã bị tôi từ chối dứt khoát rồi sao?
Dialogue: 0,0:19:51.53,0:19:56.65,Default,,0000,0000,0000,,Im lặng như vậy nghĩa là bị tôi nói trúng tim đen rồi phải không?
Dialogue: 0,0:19:59.01,0:20:05.60,Default,,0000,0000,0000,,Tôi được sinh ra trong một gia đình quý tộc thuộc dòng dõi hoàng gia.
Dialogue: 0,0:20:05.60,0:20:08.52,Default,,0000,0000,0000,,Tôi đang bị ép phải làm tròn bổn phận với Tổ quốc.
Dialogue: 0,0:20:08.96,0:20:13.06,Default,,0000,0000,0000,,Tôi không đủ khả năng ràng buộc ai, hay bị ai đó ràng buộc.
Dialogue: 0,0:20:18.93,0:20:20.96,Default,,0000,0000,0000,,Sao anh dám làm chuyện đó mà không xin phép tôi!
Dialogue: 0,0:20:20.96,0:20:22.98,Default,,0000,0000,0000,,Anh không thấy như vậy là quá hèn hạ sao?!
Dialogue: 0,0:20:22.98,0:20:27.11,Default,,0000,0000,0000,,Điều quan trọng nhất với anh lúc này là bày tỏ tình cảm của mình với em.
Dialogue: 0,0:20:28.55,0:20:30.65,Default,,0000,0000,0000,,Đừng áp đặt tình cảm của anh lên người khác như vậy.
Dialogue: 0,0:20:30.65,0:20:34.40,Default,,0000,0000,0000,,Anh không thể lay chuyển quyết định của tôi bằng những lời đường mật đó đâu...
Dialogue: 0,0:20:35.16,0:20:36.04,Default,,0000,0000,0000,,Nếu đã như vậy...
Dialogue: 0,0:20:37.63,0:20:39.08,Default,,0000,0000,0000,,Arima-dono!
Dialogue: 0,0:20:47.22,0:20:51.06,Default,,0000,0000,0000,,Thân là quý tộc, chúng ta bị bắt phải lãnh đạo người khác!
Dialogue: 0,0:20:51.83,0:20:55.94,Default,,0000,0000,0000,,Anh đã sẵn sàng phá bỏ tiền lệ đó chưa?
Dialogue: 0,0:20:56.56,0:20:57.98,Default,,0000,0000,0000,,Đồ ngốc...
Dialogue: 0,0:21:00.02,0:21:03.76,Default,,0000,0000,0000,,Anh... thật sự...
Dialogue: 0,0:21:04.62,0:21:09.08,Default,,0000,0000,0000,,...thèm muốn thân xác tôi ... đến vậy sao?!
Dialogue: 0,0:21:09.98,0:21:12.54,Default,,0000,0000,0000,,Đừng so sánh nó với dục vọng.
Dialogue: 0,0:21:12.95,0:21:14.32,Default,,0000,0000,0000,,Anh đau lòng lắm.
Dialogue: 0,0:21:17.27,0:21:18.63,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:21:21.90,0:21:24.05,Default,,0000,0000,0000,,Đối xử thô bạo với hôn thê mình như vậy...
Dialogue: 0,0:21:24.05,0:21:26.69,Default,,0000,0000,0000,,Anh muốn tôi căm hận anh cả đời sao?
Dialogue: 0,0:21:26.69,0:21:29.66,Default,,0000,0000,0000,,Chỉ cần được ở bên em, Sylvie,
Dialogue: 0,0:21:29.66,0:21:31.56,Default,,0000,0000,0000,,Dù trong mắt em, anh là kẻ xấu xa ti tiện đến mức nào, anh cũng mặc kệ.
Dialogue: 0,0:21:32.93,0:21:37.80,Default,,0000,0000,0000,,Anh định chinh phục tôi bằng cách này sao?
Dialogue: 0,0:21:41.07,0:21:46.90,Default,,0000,0000,0000,,Chuyện chúng ta sắp làm thật sự đáng xấu hổ.
Dialogue: 0,0:21:51.00,0:21:54.90,Default,,0000,0000,0000,,Chưa kể nơi mà chúng ta sắp làm chuyện đó, thật tồi tệ làm sao...
Dialogue: 0,0:21:55.89,0:21:57.88,Default,,0000,0000,0000,,Vậy à, sau khi anh làm "vấy bẩn" em...
Dialogue: 0,0:21:59.27,0:22:02.12,Default,,0000,0000,0000,,Em nghĩ rằng mình có thể trở lại như trước kia được sao, Sylvie?
Dialogue: 0,0:22:05.45,0:22:07.16,Default,,0000,0000,0000,,Tôi không còn là trẻ con nữa.
Dialogue: 0,0:22:07.63,0:22:11.38,Default,,0000,0000,0000,,Anh sắp biến tôi trở thành phụ nữ.
Dialogue: 0,0:22:13.04,0:22:17.41,Default,,0000,0000,0000,,Đừng nói là anh có dự định khác vào lúc này đấy nhé?
Dialogue: 0,0:22:19.55,0:22:21.92,Default,,0000,0000,0000,,Tôi sẽ trở thành người phụ nữ vô cùng phiền phức.
Dialogue: 0,0:22:22.51,0:22:25.70,Default,,0000,0000,0000,,Cướp đi trinh tiết của tôi là tội lỗi không thể tha thứ.
Dialogue: 0,0:22:25.70,0:22:27.92,Default,,0000,0000,0000,,Anh sẽ phải dùng cả cuộc đời mình để bù đắp cho tôi.
Dialogue: 0,0:22:28.42,0:22:31.28,Default,,0000,0000,0000,,Cuộc đời anh sẽ gắn chặt với tôi.
Dialogue: 0,0:22:33.41,0:22:35.29,Default,,0000,0000,0000,,Hòa làm một...
Dialogue: 0,0:22:39.41,0:22:44.16,Default,,0000,0000,0000,,Arima-dono và mình... đã hòa làm một...
Dialogue: 0,0:22:44.16,0:22:46.84,Default,,0000,0000,0000,,Nó vào bên trong rồi... cơ thể em như đang tan ra...
Dialogue: 0,0:22:46.84,0:22:48.23,Default,,0000,0000,0000,,Rát quá...
Dialogue: 0,0:22:48.23,0:22:51.89,Default,,0000,0000,0000,,Rát quá đi mất, vào rồi!
Dialogue: 0,0:23:10.78,0:23:14.56,Default,,0000,0000,0000,,B-Bên trong em... đang nóng bừng lên...
Dialogue: 0,0:23:14.56,0:23:16.54,Default,,0000,0000,0000,,Tại sao chứ...
Dialogue: 0,0:23:17.75,0:23:19.44,Default,,0000,0000,0000,,Cơ thể em như đang sôi lên...
Dialogue: 0,0:23:21.93,0:23:24.75,Default,,0000,0000,0000,,Anh đang thiêu cháy cơ thể em, cảm giác tê dại này...
Dialogue: 0,0:23:28.50,0:23:29.83,Default,,0000,0000,0000,,Làm đi anh...
Dialogue: 0,0:23:33.36,0:23:34.95,Default,,0000,0000,0000,,Làm đi anh...
Dialogue: 0,0:23:36.44,0:23:40.44,Default,,0000,0000,0000,,Anh đang tàn phá bên trong cơ thể em...
Dialogue: 0,0:23:41.34,0:23:43.79,Default,,0000,0000,0000,,Sao lại có kẻ độc ác như anh chứ...
Dialogue: 0,0:23:44.65,0:23:47.90,Default,,0000,0000,0000,,Anh làm em đau đớn vô cùng...
Dialogue: 0,0:23:51.90,0:23:53.00,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:23:53.00,0:23:55.57,Default,,0000,0000,0000,,Anh đang vào sâu bên trong em...
Dialogue: 0,0:23:57.68,0:24:01.00,Default,,0000,0000,0000,,Cơ thể em đang phơi bày trước mắt anh...
Dialogue: 0,0:24:01.00,0:24:03.13,Default,,0000,0000,0000,,Bỏng rát và tê dại...
Dialogue: 0,0:24:03.12,0:24:05.79,Default,,0000,0000,0000,,Dường như trí óc em cũng đang bị thiêu cháy...
Dialogue: 0,0:24:08.48,0:24:12.02,Default,,0000,0000,0000,,Không... Em không muốn rên rỉ một cách dâm đãng như vậy...
Dialogue: 0,0:24:13.23,0:24:15.37,Default,,0000,0000,0000,,Chuyện này là sao?
Dialogue: 0,0:24:15.37,0:24:18.81,Default,,0000,0000,0000,,Em mất tự chủ rồi... Thật đáng sợ...
Dialogue: 0,0:24:18.81,0:24:21.43,Default,,0000,0000,0000,,Anh đã làm gì em thế này?
Dialogue: 0,0:24:23.80,0:24:28.28,Default,,0000,0000,0000,,Đừng mà, Sylvie, nếu em cứ xoắn lấy anh như vậy, anh sẽ ra mất!
Dialogue: 0,0:24:29.54,0:24:30.95,Default,,0000,0000,0000,,Tên ngốc này...
Dialogue: 0,0:24:30.95,0:24:32.92,Default,,0000,0000,0000,,Đừng có ngớ ngẩn như vậy!
Dialogue: 0,0:24:32.92,0:24:35.49,Default,,0000,0000,0000,,Người ta không phải giống như anh đâu...
Dialogue: 0,0:24:36.17,0:24:39.61,Default,,0000,0000,0000,,Em không phải người chỉ biết theo đuổi dục vọng như anh!
Dialogue: 0,0:24:40.70,0:24:41.86,Default,,0000,0000,0000,,Nữa nào...
Dialogue: 0,0:24:42.62,0:24:45.70,Default,,0000,0000,0000,,Nữa nào, hai chúng ta sẽ bên nhau mãi mãi...
Dialogue: 0,0:24:45.70,0:24:47.32,Default,,0000,0000,0000,,Gắn kết mãi mãi...
Dialogue: 0,0:24:47.32,0:24:48.38,Default,,0000,0000,0000,,Thôi mà...
Dialogue: 0,0:24:54.98,0:24:57.81,Default,,0000,0000,0000,,Anh làm mạnh quá đó...
Dialogue: 0,0:24:58.37,0:25:00.03,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:25:00.03,0:25:03.31,Default,,0000,0000,0000,,Cảm giác ấm áp này đã chiếm lấy toàn bộ tâm trí em...
Dialogue: 0,0:25:13.65,0:25:15.50,Default,,0000,0000,0000,,Chóng mặt quá...
Dialogue: 0,0:25:16.33,0:25:19.28,Default,,0000,0000,0000,,Sự cuồng nhiệt của anh làm em mất tỉnh táo rồi...
Dialogue: 0,0:25:20.76,0:25:22.24,Default,,0000,0000,0000,,Trí tuệ của em đã...
Dialogue: 0,0:25:25.45,0:25:26.29,Default,,0000,0000,0000,,... biến mất...
Dialogue: 0,0:25:31.42,0:25:34.78,Default,,0000,0000,0000,,Thứ gì đó nóng bỏng đang trào ra bên trong em...
Dialogue: 0,0:25:35.71,0:25:37.83,Default,,0000,0000,0000,,Bên trong em...
Dialogue: 0,0:25:38.64,0:25:41.74,Default,,0000,0000,0000,,Không, không phải của em...
Dialogue: 0,0:25:41.74,0:25:43.35,Default,,0000,0000,0000,,Không phải!
Dialogue: 0,0:25:46.94,0:25:48.41,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:25:58.95,0:26:01.66,Default,,0000,0000,0000,,Nóng quá, cơ thể em đang tan chảy...
Dialogue: 0,0:26:03.16,0:26:05.46,Default,,0000,0000,0000,,Tinh dịch của anh đang tràn vào bên trong em...
Dialogue: 0,0:26:06.25,0:26:07.46,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:26:07.46,0:26:13.77,Default,,0000,0000,0000,,Tinh dịch nóng bỏng của anh đã lấp đầy bên trong, nó lấp đầy bên trong em rồi!
Dialogue: 0,0:26:18.05,0:26:20.76,Default,,0000,0000,0000,,Nó vẫn còn bự như vậy sao?
Dialogue: 0,0:26:22.04,0:26:23.50,Default,,0000,0000,0000,,Bên trong em đầy ắp rồi mà...
Dialogue: 0,0:26:24.81,0:26:26.41,Default,,0000,0000,0000,,Nó lại ra nữa rồi...
Dialogue: 0,0:26:27.50,0:26:29.53,Default,,0000,0000,0000,,Sao anh có thể...
Dialogue: 0,0:26:58.10,0:27:03.92,Default,,0000,0000,0000,,E rằng em sẽ khó mà kiểm soát nổi cảm xúc của anh nếu nó dồi dào như vậy...
Dialogue: 0,0:27:04.69,0:27:07.46,Default,,0000,0000,0000,,Em không muốn làm điều đó sao?
Dialogue: 0,0:27:11.50,0:27:13.15,Default,,0000,0000,0000,,Em đâu có nói là không muốn.
Dialogue: 0,0:27:14.19,0:27:16.94,Default,,0000,0000,0000,,Chỉ là em không đủ khả năng để kiểm soát nó.
Dialogue: 0,0:27:19.36,0:27:20.82,Default,,0000,0000,0000,,Gã ngốc.
