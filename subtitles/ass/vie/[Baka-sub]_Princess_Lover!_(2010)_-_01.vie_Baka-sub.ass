Dialogue: 0,0:01:02.91,0:01:05.27,Default,,0000,0000,0000,,Em tỉnh rồi sao, Sylvie?
Dialogue: 0,0:01:05.27,0:01:08.21,Default,,0000,0000,0000,,Em hiểu tình cảm mà anh dành cho em, Arima-dono.
Dialogue: 0,0:01:09.32,0:01:13.47,Default,,0000,0000,0000,,Bất kì ai nói rằng em không hạnh phúc thì đó chỉ là lời nói dối.
Dialogue: 0,0:01:13.47,0:01:17.57,Default,,0000,0000,0000,,Mọi lời lẽ của em đều không thể làm anh nhụt chí, và khi anh ôm lấy em...
Dialogue: 0,0:01:18.25,0:01:20.75,Default,,0000,0000,0000,,Vào lúc đó, con tim em đã thật sự rung động.
Dialogue: 0,0:01:24.83,0:01:29.61,Default,,0000,0000,0000,,Đây là lần đầu tiên... em có được cảm giác của người phụ nữ.
Dialogue: 0,0:01:29.61,0:01:33.11,Default,,0000,0000,0000,,Nhưng lúc này em đang rất bối rối...
Dialogue: 0,0:01:33.11,0:01:35.40,Default,,0000,0000,0000,,Em cần chút thời gian.
Dialogue: 0,0:01:36.00,0:01:41.02,Default,,0000,0000,0000,,Em muốn tiếp tục theo đuổi hoài bão của riêng mình, ở bên cạnh anh...
Dialogue: 0,0:01:49.55,0:01:51.95,Default,,0000,0000,0000,,Em mượn chiếc áo này một lúc nha.
Dialogue: 0,0:01:51.95,0:01:55.84,Default,,0000,0000,0000,,Em muốn cảm nhận hơi ấm của anh thêm chút nữa.
Dialogue: 0,0:01:56.29,0:01:57.64,Default,,0000,0000,0000,,Sylvie...
Dialogue: 0,0:01:58.25,0:02:00.55,Default,,0000,0000,0000,,Giờ thì em đã không còn cách nào để từ chối chuyện đó nữa.
Dialogue: 0,0:02:01.70,0:02:05.81,Default,,0000,0000,0000,,Anh đã làm "chuyện đó" với em, xem như người ta thuộc về anh rồi.
Dialogue: 0,0:02:26.43,0:02:28.33,Default,,0000,0000,0000,,A-Arima-dono...
Dialogue: 0,0:02:32.76,0:02:34.44,Default,,0000,0000,0000,,A-Anh làm gì vậy{\fscx300}-
Dialogue: 0,0:02:34.44,0:02:36.24,Default,,0000,0000,0000,,Thổi bùng ngọn lửa khát khao trong em.
Dialogue: 0,0:02:36.87,0:02:38.76,Default,,0000,0000,0000,,K-Không công bằng!
Dialogue: 0,0:02:38.76,0:02:42.68,Default,,0000,0000,0000,,Đã bảo anh đợi ở ngoài cơ mà, chẳng chịu nghe lời người ta gì cả...
Dialogue: 0,0:02:42.68,0:02:45.68,Default,,0000,0000,0000,,Lúc này trong đầu anh chỉ nghĩ tới chuyện đó mà thôi.
Dialogue: 0,0:02:43.93,0:02:45.68,Alternate,,0000,0000,0000,,Không được mà!
Dialogue: 0,0:02:46.23,0:02:48.22,Default,,0000,0000,0000,,Không, anh vẫn không chịu dừng lại ư?
