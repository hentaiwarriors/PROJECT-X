Dialogue: 0,0:00:15.53,0:00:16.49,Default,,0000,0000,0000,,{\bord0\i1\c&H101012&\pos(565,455)}Dinh thự nhà Jounouchi{\i0}
Dialogue: 0,0:00:22.83,0:00:29.40,Default,,0000,0000,0000,,Tsubame-sama, theo tôi nhớ thì phòng này được dọn cách đây có ba ngày.
Dialogue: 0,0:00:31.26,0:00:33.77,Default,,0000,0000,0000,,Sao tôi nhớ được chuyện đó chứ... {added extra two periods for emphasis}
Dialogue: 0,0:00:34.18,0:00:39.44,Default,,0000,0000,0000,,Ngày và đêm chả nghĩa lí gì với cách sống của tôi. Nên tôi chả quan tâm làm gì.
Dialogue: 0,0:00:39.44,0:00:40.38,Default,,0000,0000,0000,,Đành chịu thôi...
Dialogue: 0,0:00:40.39,0:00:41.97,Default,,0000,0000,0000,,Không hề. {repeat sentance?}
Dialogue: 0,0:00:42.52,0:00:45.63,Default,,0000,0000,0000,,Hể, Shun-chin. Đáng sợ quá đi.
Dialogue: 0,0:00:46.44,0:00:49.90,Default,,0000,0000,0000,,Chúng ta chuyển sang đề tài khác nhé, cậu hiểu ý tôi mà.
Dialogue: 0,0:00:49.90,0:00:53.28,Default,,0000,0000,0000,,Ôi trời, cô thật là bản tính khó dời.
Dialogue: 0,0:00:54.36,0:00:57.28,Default,,0000,0000,0000,,Cậu đang nói về mình à?
Dialogue: 0,0:01:11.83,0:01:17.13,Default,,0000,0000,0000,,Không biết phải nói sao, cậu làm tôi sướng nhanh quá đi.
Dialogue: 0,0:01:18.55,0:01:21.71,Default,,0000,0000,0000,,Cứ như tôi còn trinh vậy.
Dialogue: 0,0:01:27.77,0:01:29.29,Default,,0000,0000,0000,,T-Tuyệt quá...
Dialogue: 0,0:01:29.29,0:01:31.72,Default,,0000,0000,0000,,Đừng thô bạo như vậy...{alt: your so rough...}
Dialogue: 0,0:01:36.05,0:01:38.31,Default,,0000,0000,0000,,Không... Tay của cậu...
Dialogue: 0,0:01:39.23,0:01:41.38,Default,,0000,0000,0000,,Tôi sẽ tè ra mất! {alt: I c-can't keep from coming!}
Dialogue: 0,0:01:44.70,0:01:47.84,Default,,0000,0000,0000,,K-Không được, nó sắp ra... Nó sắp ra rồi...
Dialogue: 0,0:01:47.84,0:01:51.04,Default,,0000,0000,0000,,Nó ra rồi! Nó ra rồi!
Dialogue: 0,0:02:07.98,0:02:12.44,Default,,0000,0000,0000,,Lúc nào cậu cũng chạm đến tử cung của tôi.
Dialogue: 0,0:02:12.44,0:02:14.17,Default,,0000,0000,0000,,Chỉ mới vào mà đã vậy...
Dialogue: 0,0:02:15.23,0:02:16.42,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:02:17.74,0:02:22.47,Default,,0000,0000,0000,,Không được... Khi cậu rút ra, nó cọ xát bên trong tôi...
Dialogue: 0,0:02:25.12,0:02:27.55,Default,,0000,0000,0000,,Nó đang đâm vào bên trong tôi...
Dialogue: 0,0:02:27.55,0:02:30.54,Default,,0000,0000,0000,,Rút ra không được, đẩy vào cũng không xong.
Dialogue: 0,0:02:30.54,0:02:33.25,Default,,0000,0000,0000,,Vậy rốt cuộc tôi phải làm gì đây?
Dialogue: 0,0:02:33.26,0:02:37.45,Default,,0000,0000,0000,,K-Không phải, ý tôi là muốn cả hai cơ.
Dialogue: 0,0:02:37.45,0:02:40.87,Default,,0000,0000,0000,,Chỉ là... Chỉ là hãy chờ thêm một chút.
Dialogue: 0,0:02:40.87,0:02:45.77,Default,,0000,0000,0000,,Bởi vì, sướng như vầy mà không tận hưởng từ từ thì thật lãng phí...
Dialogue: 0,0:02:48.23,0:02:52.19,Default,,0000,0000,0000,,Nhưng tôi thì không thể kiềm chế được.
Dialogue: 0,0:02:53.23,0:02:56.05,Default,,0000,0000,0000,,Cậu tàn nhẫn quá đi, Shun-chin.
Dialogue: 0,0:02:56.45,0:03:00.24,Default,,0000,0000,0000,,Đâm vào bên trong tôi thô bạo như vậy... {alt: Thrusting into me so fast...}
Dialogue: 0,0:03:01.53,0:03:03.45,Default,,0000,0000,0000,,N-Nếu cứ như vậy...
Dialogue: 0,0:03:03.45,0:03:06.70,Default,,0000,0000,0000,,...chúng ta sẽ không nói chuyện tình yêu được.
Dialogue: 0,0:03:13.63,0:03:16.55,Default,,0000,0000,0000,,Yêu? Cô đang nói gì vậy?
Dialogue: 0,0:03:16.55,0:03:19.25,Default,,0000,0000,0000,,Cậu phản ứng thái quá rồi đó, Shun-chin.
Dialogue: 0,0:03:19.25,0:03:23.39,Default,,0000,0000,0000,,Cậu biết đấy. Tôi thật sự không hề có ý gì cả.
Dialogue: 0,0:03:23.39,0:03:29.96,Default,,0000,0000,0000,,Chỉ là... Chỉ là... tôi tò mò muốn biết cậu sẽ phản ứng thế nào thôi.
Dialogue: 0,0:03:31.60,0:03:36.36,Default,,0000,0000,0000,,Vậy sao chúng ta không quay lại mối quan hệ tình dục chỉ vì ham muốn của bản thân?
Dialogue: 0,0:03:37.57,0:03:40.03,Default,,0000,0000,0000,,Như thế hợp với chúng ta hơn.
Dialogue: 0,0:04:00.42,0:04:06.68,Default,,0000,0000,0000,,Nếu ham muốn còn quan trọng hơn cả nhân cách, thì chơi kiểu chó quá hợp rồi còn gì?
Dialogue: 0,0:04:08.31,0:04:11.58,Default,,0000,0000,0000,,C-Có thể cậu nói đúng...
Dialogue: 0,0:04:11.58,0:04:13.71,Default,,0000,0000,0000,,Thật là nguyên thủy.
Dialogue: 0,0:04:13.71,0:04:16.53,Default,,0000,0000,0000,,Là chó thì không được nói tiếng người!
Dialogue: 0,0:04:17.15,0:04:19.73,Default,,0000,0000,0000,,Cậu đâu cần phải nghiêm khắc như vậy...
Dialogue: 0,0:04:22.91,0:04:24.86,Default,,0000,0000,0000,,Đau quá! Không...
Dialogue: 0,0:04:25.53,0:04:26.78,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:04:30.83,0:04:34.34,Default,,0000,0000,0000,,T-Tôi sắp ra rồi...
Dialogue: 0,0:04:37.84,0:04:41.39,Default,,0000,0000,0000,,S-Sao cậu lại dừng lại, Shun-chin?
Dialogue: 0,0:04:47.68,0:04:52.82,Default,,0000,0000,0000,,Bị đánh khiến cô tè ra ư, Tsubame-sama? Cô thật giống một con chó. {alt: Getting hit makes you come, Miss Tsubame?}
Dialogue: 0,0:05:00.78,0:05:05.93,Default,,0000,0000,0000,,Shun-chin thật là một tên vô lại.\N Tôi không ngờ là cậu lại dám làm thế.
Dialogue: 0,0:05:05.93,0:05:09.70,Default,,0000,0000,0000,,Cậu vừa khám phá một cách hoàn toàn mới để đạt cực khoái đấy.
Dialogue: 0,0:05:10.28,0:05:12.87,Default,,0000,0000,0000,,Tôi vẫn chưa ra cơ mà.
Dialogue: 0,0:05:12.87,0:05:16.32,Default,,0000,0000,0000,,K-Không được... Lần này thật sự...
Dialogue: 0,0:05:16.32,0:05:20.40,Default,,0000,0000,0000,,Tôi chỉ vừa mới ra thôi... Nên còn nhạy cảm lắm...
Dialogue: 0,0:05:24.79,0:05:26.38,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:05:26.38,0:05:27.76,Default,,0000,0000,0000,,Hiệp hai nào!
Dialogue: 0,0:06:01.38,0:06:04.52,Default,,0000,0000,0000,,Tinh dịch của cậu vẫn còn ở trong người tôi đây này.
Dialogue: 0,0:06:05.51,0:06:07.15,Default,,0000,0000,0000,,Nó sắp chảy ra rồi...
Dialogue: 0,0:06:08.72,0:06:14.22,Default,,0000,0000,0000,,Cậu cứ ra vẻ tử tế, nhưng bây giờ tôi đã thấy được bộ mặt xấu xa của cậu.
Dialogue: 0,0:06:14.97,0:06:18.85,Default,,0000,0000,0000,,Thật khó mà đánh giá cậu qua vẻ bề ngoài.
Dialogue: 0,0:06:20.90,0:06:22.90,Default,,0000,0000,0000,,Cậu đang nghĩ gì thế?
Dialogue: 0,0:06:22.90,0:06:24.68,Default,,0000,0000,0000,,Cô đoán đi!
Dialogue: 0,0:06:25.73,0:06:27.12,Default,,0000,0000,0000,,Để xem nào...
Dialogue: 0,0:06:27.12,0:06:29.57,Default,,0000,0000,0000,,Về dòng họ Jounouchi đúng không?
Dialogue: 0,0:06:32.32,0:06:34.12,Default,,0000,0000,0000,,Đúng rồi chứ gì?
Dialogue: 0,0:06:35.70,0:06:38.46,Default,,0000,0000,0000,,Sao lại không? Cậu có thể có nó.
Dialogue: 0,0:06:38.46,0:06:41.48,Default,,0000,0000,0000,,Nhưng cô đâu phải người đứng đầu dòng họ.
Dialogue: 0,0:06:41.48,0:06:44.85,Default,,0000,0000,0000,,Vậy cậu thử chinh phục Shizuru-nee-san đi!
Dialogue: 0,0:06:44.85,0:06:48.88,Default,,0000,0000,0000,,Cậu có thể thế chỗ người chồng quá cố của chị ấy. Tôi sẽ giúp cậu.
Dialogue: 0,0:06:49.84,0:06:51.42,Default,,0000,0000,0000,,Ôi ôi.
Dialogue: 0,0:06:52.68,0:06:54.56,Default,,0000,0000,0000,,Mục đích của cô là gì?
Dialogue: 0,0:06:59.15,0:07:01.40,Default,,0000,0000,0000,,Chả gì cả.
Dialogue: 0,0:07:01.40,0:07:05.09,Default,,0000,0000,0000,,Chỉ là tôi đang phát điên vì cậu thôi.
Dialogue: 0,0:07:05.65,0:07:11.16,Default,,0000,0000,0000,,Nhờ sự sung sướng mà cậu mang lại nên công việc của tôi mới suôn sẻ như vầy.
Dialogue: 0,0:07:11.82,0:07:15.12,Default,,0000,0000,0000,,Tôi chỉ muốn đền đáp thôi ấy mà.
Dialogue: 0,0:07:16.41,0:07:21.98,Default,,0000,0000,0000,,Vả lại tôi có thể lấy cảm hứng từ câu chuyện xảo trá của cậu.
Dialogue: 0,0:07:22.67,0:07:26.50,Default,,0000,0000,0000,,Với tôi thì cái đó quan trong hơn.
Dialogue: 0,0:07:26.50,0:07:30.59,Default,,0000,0000,0000,,Cô bán đứng chị gái chỉ vì kế sinh nhai của mình thôi sao?
Dialogue: 0,0:07:30.59,0:07:32.36,Default,,0000,0000,0000,,Cô thật xấu xa.
Dialogue: 0,0:07:43.19,0:07:47.70,Default,,0000,0000,0000,,"Nếu bưng nhiều quá thì cậu sẽ vấp ngã đấy", Shun.
Dialogue: 0,0:07:47.70,0:07:51.45,Default,,0000,0000,0000,,Ý chị là chị sẽ thể hiện lòng trung thành của mình...
Dialogue: 0,0:07:51.45,0:07:57.88,Default,,0000,0000,0000,,...bằng cách tố cáo với họ Daichi Shun là một kẻ nguy hiểm sao?
Dialogue: 0,0:07:57.88,0:08:01.40,Default,,0000,0000,0000,,Ngu vừa thôi. Tôi chỉ nói chung chung thôi mà.
Dialogue: 0,0:08:01.40,0:08:05.28,Default,,0000,0000,0000,,Chỉ vì năm chị em đó là trung tâm của dòng họ này...
Dialogue: 0,0:08:05.28,0:08:09.00,Default,,0000,0000,0000,,...nên cậu nghĩ nếu điều khiển được họ cậu sẽ thành công à?
Dialogue: 0,0:08:09.55,0:08:11.86,Default,,0000,0000,0000,,Hể, chị lo cho tôi đấy à?
Dialogue: 0,0:08:11.86,0:08:16.40,Default,,0000,0000,0000,,Nếu cậu leo lên được, thì điều kiện việc làm của tôi sẽ được cải thiện.
Dialogue: 0,0:08:16.40,0:08:17.55,Default,,0000,0000,0000,,Là vậy đó.
Dialogue: 0,0:08:18.18,0:08:21.85,Default,,0000,0000,0000,,Vậy chúng ta thỏa thuận nhé?
Dialogue: 0,0:08:22.60,0:08:26.62,Default,,0000,0000,0000,,Không, cậu phải tự làm thôi.
Dialogue: 0,0:08:27.16,0:08:29.93,Default,,0000,0000,0000,,Phải, gắn hết sức.
Dialogue: 0,0:08:43.37,0:08:45.51,Default,,0000,0000,0000,,Chào buổi sáng, Rei-sama.
Dialogue: 0,0:08:46.08,0:08:47.42,Default,,0000,0000,0000,,Chào buổi sáng.
Dialogue: 0,0:09:05.64,0:09:06.60,Default,,0000,0000,0000,,{\bord0\i1\c&H101012&\pos(254,454)}Đêm đó{\i0}...
Dialogue: 0,0:09:25.50,0:09:26.90,Default,,0000,0000,0000,,Shizuru-sama?
Dialogue: 0,0:09:27.91,0:09:30.25,Default,,0000,0000,0000,,Hãy nuốt nó vào sâu trong cổ họng của cô đi.
Dialogue: 0,0:09:39.18,0:09:41.26,Default,,0000,0000,0000,,Tốt lắm, Shizuru-sama.
Dialogue: 0,0:09:41.26,0:09:45.39,Default,,0000,0000,0000,,Thấy cô nhiệt tình như thế làm tôi rất cảm động.
Dialogue: 0,0:09:46.98,0:09:51.80,Default,,0000,0000,0000,,Đã lâu cô không được bú nó kể từ khi chồng cô qua đời nhỉ?
Dialogue: 0,0:09:54.03,0:09:56.40,Default,,0000,0000,0000,,Làm ơn đừng nhắc đến anh ấy.{might want to change the period into an exclamation point. Depending on context of course}
Dialogue: 0,0:09:56.40,0:09:58.57,Default,,0000,0000,0000,,Cô vẫn chung thủy với anh ta à?
Dialogue: 0,0:09:59.99,0:10:03.23,Default,,0000,0000,0000,,Tôi nghĩ đó chính là điểm quyến rũ của cô.
Dialogue: 0,0:10:03.23,0:10:08.71,Default,,0000,0000,0000,,Nhưng cô vẫn tìm cách để thỏa mãn ham muốn của cơ thể mình.
Dialogue: 0,0:10:12.13,0:10:13.59,Default,,0000,0000,0000,,Anh yêu...
Dialogue: 0,0:10:14.67,0:10:17.11,Default,,0000,0000,0000,,Quả nhiên là đã bị cậu nhìn thấy. {do you want italic here?}
Dialogue: 0,0:10:17.11,0:10:21.58,Default,,0000,0000,0000,,Phải, thật khó tin là cô lại thiếu đứng đắng như thế.
Dialogue: 0,0:10:21.58,0:10:26.64,Default,,0000,0000,0000,,Cô biết là mình đang bị nhìn phải không? Nên mới kích thích như thế.
Dialogue: 0,0:10:27.68,0:10:31.11,Default,,0000,0000,0000,,Bây giờ, chúng ta chuyển sang bước tiếp theo nào.
Dialogue: 0,0:10:39.24,0:10:40.76,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:10:47.77,0:10:50.08,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:11:18.94,0:11:21.23,Default,,0000,0000,0000,,Shun-kun, tuyệt quá...
Dialogue: 0,0:11:21.23,0:11:24.89,Default,,0000,0000,0000,,Bây giờ thằng nhỏ của cậu đang ở bên trong tôi...
Dialogue: 0,0:11:24.89,0:11:31.45,Default,,0000,0000,0000,,Vì cô thủ dâm mỗi ngày nên bây giờ tôi đút vào thật dễ dàng.
Dialogue: 0,0:11:31.45,0:11:34.00,Default,,0000,0000,0000,,Không hẳn là mỗi ngày...
Dialogue: 0,0:11:40.59,0:11:45.72,Default,,0000,0000,0000,,Đầu... Đầu thằng nhỏ của cậu đang đâm vào sâu bên trong!
Dialogue: 0,0:11:46.47,0:11:50.42,Default,,0000,0000,0000,,Tôi có thể cảm thấy nó đang căng ra.
Dialogue: 0,0:11:52.39,0:11:54.44,Default,,0000,0000,0000,,Không được, chỗ đó!
Dialogue: 0,0:11:57.86,0:12:00.22,Default,,0000,0000,0000,,Cổ tử cung của tôi...
Dialogue: 0,0:12:04.70,0:12:06.48,Default,,0000,0000,0000,,Không! Không!
Dialogue: 0,0:12:06.48,0:12:11.04,Default,,0000,0000,0000,,Đừng cọ xát nó với thứ to lớn của cậu...
Dialogue: 0,0:12:11.04,0:12:15.63,Default,,0000,0000,0000,,T-Tôi chưa bao giờ... có cảm giác này...
Dialogue: 0,0:12:19.84,0:12:24.02,Default,,0000,0000,0000,,Vậy ư? Chồng cô không giỏi trong chuyện chăn gối nhỉ.
Dialogue: 0,0:12:24.02,0:12:26.42,Default,,0000,0000,0000,,Đừng nhắc tới anh ấy!
Dialogue: 0,0:12:26.42,0:12:28.95,Default,,0000,0000,0000,,Tôi thật sự rất yêu anh ấy!
Dialogue: 0,0:12:28.95,0:12:34.60,Default,,0000,0000,0000,,Dù vậy, những cảm xúc ấy không thể thỏa mãn được ham muốn của cô!
Dialogue: 0,0:12:35.76,0:12:38.05,Default,,0000,0000,0000,,K-Không được! Không được!
Dialogue: 0,0:12:38.05,0:12:42.67,Default,,0000,0000,0000,,Nếu cậu cứ đâm vào cổ tử cung tôi như thế tôi sẽ...
Dialogue: 0,0:12:53.15,0:12:54.86,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:13:00.71,0:13:02.33,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:13:02.93,0:13:04.26,Default,,0000,0000,0000,,Dù cô có nói vậy...
Dialogue: 0,0:13:04.26,0:13:07.90,Default,,0000,0000,0000,,Shun-kun đau lắm... Dừng lại đi...
Dialogue: 0,0:13:07.90,0:13:10.25,Default,,0000,0000,0000,,Cô dễ thương thật đấy, Shizuru-san.
Dialogue: 0,0:13:12.31,0:13:15.26,Default,,0000,0000,0000,,Nhưng tôi sẽ không dễ dãi với cô đâu.
Dialogue: 0,0:13:15.26,0:13:18.31,Default,,0000,0000,0000,,Bởi vì chuyện này đều là vì Shizuru-san.
Dialogue: 0,0:13:18.31,0:13:21.13,Default,,0000,0000,0000,,Không, Shun! Dừng lại đi!
Dialogue: 0,0:13:21.13,0:13:23.32,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:13:24.07,0:13:27.03,Default,,0000,0000,0000,,Nhưng không phải nó làm cô sướng lắm sao?
Dialogue: 0,0:13:27.61,0:13:30.22,Default,,0000,0000,0000,,Không hề có chuyện đó...
Dialogue: 0,0:13:30.22,0:13:34.80,Default,,0000,0000,0000,,Vậy sao? Mặc kệ, dù cô có nói gì thì tôi cũng không dừng lại đâu.
Dialogue: 0,0:13:36.50,0:13:37.71,Default,,0000,0000,0000,,Shun-kun?
Dialogue: 0,0:13:37.71,0:13:41.69,Default,,0000,0000,0000,,Với Shizuru-san thì phải làm như vầy.
Dialogue: 0,0:13:42.84,0:13:44.21,Default,,0000,0000,0000,,Không, tôi...
Dialogue: 0,0:13:44.72,0:13:50.45,Default,,0000,0000,0000,,Bởi vì nếu không làm như vầy cô sẽ có cảm giác mình đang phản bội anh ta...
Dialogue: 0,0:13:50.45,0:13:52.66,Default,,0000,0000,0000,,...và cô sẽ không thể ra được.
Dialogue: 0,0:13:57.00,0:13:59.14,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:14:00.27,0:14:05.61,Default,,0000,0000,0000,,Thằng nhỏ của cậu... Thằng nhỏ của cậu làm đầu óc tôi trống rỗng.
Dialogue: 0,0:14:18.37,0:14:20.30,Default,,0000,0000,0000,,Cậu đang khuấy động âm hộ của tôi...
Dialogue: 0,0:14:20.75,0:14:22.94,Default,,0000,0000,0000,,Tôi sắp ra rồi...
Dialogue: 0,0:14:22.94,0:14:25.87,Default,,0000,0000,0000,,Bên trong tôi nóng quá!
Dialogue: 0,0:14:26.65,0:14:29.64,Default,,0000,0000,0000,,Thứ nước dâm đãng sắp bắn ra rồi!
Dialogue: 0,0:14:29.64,0:14:33.96,Default,,0000,0000,0000,,Tôi ra đây! Tôi ra đây, tôi ra đây, tôi ra đây!
Dialogue: 0,0:14:55.79,0:14:56.83,Default,,0000,0000,0000,,Shun-kun...
Dialogue: 0,0:14:57.33,0:14:58.54,Default,,0000,0000,0000,,Gì thế?
Dialogue: 0,0:14:58.54,0:14:59.99,Default,,0000,0000,0000,,Cảm ơn cậu.
Dialogue: 0,0:15:01.29,0:15:03.46,Default,,0000,0000,0000,,Dù tôi đã già rồi...
Dialogue: 0,0:15:04.59,0:15:06.01,Default,,0000,0000,0000,,Không đâu.
Dialogue: 0,0:15:19.35,0:15:21.31,Default,,0000,0000,0000,,{\bord0\i1\c&H101012&\pos(555,469)}Đêm khác{\i0}...
Dialogue: 0,0:15:29.53,0:15:32.45,Default,,0000,0000,0000,,Chúng ta nên dùng trà thôi nếu không nó sẽ nguội mất.
Dialogue: 0,0:15:32.45,0:15:34.91,Default,,0000,0000,0000,,Không! Để em làm.
Dialogue: 0,0:15:34.91,0:15:39.33,Default,,0000,0000,0000,,Không lẽ em cố ý chuẩn bị nó cho tôi?
Dialogue: 0,0:15:39.91,0:15:44.38,Default,,0000,0000,0000,,Tomoe-san đã dạy em cách pha một ấm trà ngon.
Dialogue: 0,0:15:44.38,0:15:47.03,Default,,0000,0000,0000,,Em muốn anh nếm thử nó.
Dialogue: 0,0:15:47.80,0:15:50.51,Default,,0000,0000,0000,,Nên em mới gọi anh đến đây.
Dialogue: 0,0:15:51.88,0:15:53.59,Default,,0000,0000,0000,,Ra là vậy.
Dialogue: 0,0:15:53.59,0:15:56.89,Default,,0000,0000,0000,,Em đã có lòng như vậy thì làm sao tôi từ chối được.
Dialogue: 0,0:16:01.18,0:16:02.97,Default,,0000,0000,0000,,X-Xin mời.
Dialogue: 0,0:16:02.97,0:16:04.52,Default,,0000,0000,0000,,Cảm ơn em.
Dialogue: 0,0:16:08.73,0:16:11.03,Default,,0000,0000,0000,,Ơ... Nó thế nào ạ?
Dialogue: 0,0:16:11.03,0:16:13.67,Default,,0000,0000,0000,,Nó có ngon không ạ?
Dialogue: 0,0:16:17.16,0:16:19.70,Default,,0000,0000,0000,,Em không phải lo, nó ngon lắm.
Dialogue: 0,0:16:21.75,0:16:24.75,Default,,0000,0000,0000,,Hừm, Shun-san xấu quá đi!
Dialogue: 0,0:16:26.29,0:16:28.84,Default,,0000,0000,0000,,Xin lỗi, có lẽ tôi đùa hơi quá.
Dialogue: 0,0:16:29.73,0:16:32.72,Default,,0000,0000,0000,,Dạ không, tại em nóng lòng thôi.
Dialogue: 0,0:16:48.19,0:16:50.18,Default,,0000,0000,0000,,K-Không...
Dialogue: 0,0:16:53.95,0:16:56.10,Default,,0000,0000,0000,,S-Shun-san...
Dialogue: 0,0:17:04.71,0:17:06.00,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:17:06.43,0:17:07.71,Default,,0000,0000,0000,,Chuyện này...
Dialogue: 0,0:17:09.38,0:17:12.52,Default,,0000,0000,0000,,Không! Em không muốn như vầy.
Dialogue: 0,0:17:13.76,0:17:15.40,Default,,0000,0000,0000,,Em muốn tôi dừng lại à?
Dialogue: 0,0:17:16.72,0:17:21.33,Default,,0000,0000,0000,,Nhưng không phải Hina-sama rất có hứng thú với mấy chuyện như thế này sao?
Dialogue: 0,0:17:22.68,0:17:25.21,Default,,0000,0000,0000,,E-Em không có.
Dialogue: 0,0:17:25.21,0:17:26.89,Default,,0000,0000,0000,,Có.
Dialogue: 0,0:17:26.89,0:17:32.03,Default,,0000,0000,0000,,Nếu không sao em lại nhìn lén chuyện giữa tôi và Shizuru-sama.
Dialogue: 0,0:17:38.82,0:17:41.28,Default,,0000,0000,0000,,Anh đã biết rồi ư?
Dialogue: 0,0:17:41.28,0:17:45.35,Default,,0000,0000,0000,,Phải, thế nên tôi cũng sẽ làm như vậy với Hina-sama.
Dialogue: 0,0:18:09.44,0:18:11.19,Default,,0000,0000,0000,,N-Nó lớn quá...
Dialogue: 0,0:18:13.15,0:18:16.52,Default,,0000,0000,0000,,Không sau đâu, chỉ một lát là em sẽ quen thôi.
Dialogue: 0,0:18:16.52,0:18:17.65,Default,,0000,0000,0000,,V-Vâng.
Dialogue: 0,0:18:18.15,0:18:20.64,Default,,0000,0000,0000,,Nào hãy thử chạm vào nó đi.
Dialogue: 0,0:18:20.64,0:18:21.95,Default,,0000,0000,0000,,Nóng quá!
Dialogue: 0,0:18:21.95,0:18:25.10,Default,,0000,0000,0000,,Không sao đâu. Cứ tiếp tục đi.
Dialogue: 0,0:18:25.10,0:18:26.54,Default,,0000,0000,0000,,V-Vâng.
Dialogue: 0,0:18:30.77,0:18:33.46,Default,,0000,0000,0000,,Tuyệt quá, nó đang co giật.
Dialogue: 0,0:18:33.46,0:18:37.02,Default,,0000,0000,0000,,Phải, bởi vì tay của em rất là sướng.
Dialogue: 0,0:18:37.02,0:18:40.05,Default,,0000,0000,0000,,Bây giờ hãy di chuyển tay lên xuống.
Dialogue: 0,0:18:40.05,0:18:41.30,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:18:55.86,0:19:00.21,Default,,0000,0000,0000,,Em siết chặt quá đấy. Em hãy thử thả lỏng một chút.
Dialogue: 0,0:19:00.21,0:19:02.74,Default,,0000,0000,0000,,Vâng, em biết rồi.
Dialogue: 0,0:19:05.54,0:19:08.51,Default,,0000,0000,0000,,Đúng như vậy. Cảm giác thật là tuyệt.
Dialogue: 0,0:19:14.42,0:19:16.02,Default,,0000,0000,0000,,Em hãy dừng lại một chút.
Dialogue: 0,0:19:16.63,0:19:19.25,Default,,0000,0000,0000,,Hể, em đã làm gì sai ư?
Dialogue: 0,0:19:19.25,0:19:21.84,Default,,0000,0000,0000,,Không, không phải vậy.
Dialogue: 0,0:19:21.85,0:19:24.47,Default,,0000,0000,0000,,Tiếp theo tôi muốn em đưa nó vào trong miệng của mình.
Dialogue: 0,0:19:24.47,0:19:26.64,Default,,0000,0000,0000,,M-Miệng của em? {repeat?}
Dialogue: 0,0:19:28.04,0:19:29.15,Default,,0000,0000,0000,,Thật ư?
Dialogue: 0,0:19:30.89,0:19:32.44,Default,,0000,0000,0000,,Thế chúng ta sẽ dừng lại ở đây.
Dialogue: 0,0:19:33.10,0:19:34.79,Default,,0000,0000,0000,,T-Tại sao ạ?
Dialogue: 0,0:19:34.79,0:19:38.36,Default,,0000,0000,0000,,Em không làm được mà, nên chúng ta dừng lại ở đây thôi.
Dialogue: 0,0:19:38.36,0:19:40.16,Default,,0000,0000,0000,,K-Không thể nào...
Dialogue: 0,0:19:43.11,0:19:46.78,Default,,0000,0000,0000,,Ơ, em chỉ xin anh một chuyện.
Dialogue: 0,0:19:47.27,0:19:48.33,Default,,0000,0000,0000,,Là gì thế?
Dialogue: 0,0:19:48.33,0:19:53.79,Default,,0000,0000,0000,,E-Em muốn anh chỉ là của riêng mình em thôi!
Dialogue: 0,0:19:55.25,0:19:58.82,Default,,0000,0000,0000,,Được thôi, nếu Hina-sama đã muốn thế.
Dialogue: 0,0:19:58.82,0:20:00.47,Default,,0000,0000,0000,,Ngu gì đâu.
Dialogue: 0,0:20:02.80,0:20:04.69,Default,,0000,0000,0000,,Thế em đến đây.
Dialogue: 0,0:20:04.69,0:20:05.62,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:20:38.50,0:20:41.86,Default,,0000,0000,0000,,Thứ của Shun-san đang lớn lên...
Dialogue: 0,0:20:52.42,0:20:54.52,Default,,0000,0000,0000,,Hina-sama, tôi ra đây!
Dialogue: 0,0:21:04.70,0:21:07.97,Default,,0000,0000,0000,,Hãy nuốt hết chúng đi, Hina-sama.
Dialogue: 0,0:21:18.08,0:21:21.18,Default,,0000,0000,0000,,Cảm ơn em đã nuốt hết chúng.
Dialogue: 0,0:21:21.71,0:21:23.53,Default,,0000,0000,0000,,Lần này đến lượt tôi.
Dialogue: 0,0:21:24.13,0:21:25.61,Default,,0000,0000,0000,,K-Không...
Dialogue: 0,0:21:25.61,0:21:30.91,Default,,0000,0000,0000,,Chỉ phục vụ tôi thôi mà em đã hứng như vầy rồi ư, Hina-sama?
Dialogue: 0,0:21:31.77,0:21:34.26,Default,,0000,0000,0000,,E-Em không có...
Dialogue: 0,0:21:34.26,0:21:35.96,Default,,0000,0000,0000,,Thật không?
Dialogue: 0,0:21:36.98,0:21:38.65,Default,,0000,0000,0000,,Lạ à nha.
Dialogue: 0,0:21:38.65,0:21:41.52,Default,,0000,0000,0000,,Có lẽ tôi phải nhìn kĩ hơn.
Dialogue: 0,0:21:44.06,0:21:45.28,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:21:45.28,0:21:49.57,Default,,0000,0000,0000,,K-Không! Em xấu hổ lắm!
Dialogue: 0,0:21:51.21,0:21:53.42,Default,,0000,0000,0000,,Quả đúng như tôi nghĩ.
Dialogue: 0,0:21:54.04,0:21:55.21,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:21:59.25,0:22:00.90,Default,,0000,0000,0000,,Thôi...
Dialogue: 0,0:22:20.06,0:22:22.01,Default,,0000,0000,0000,,Hắn nghĩ mình là ai cơ chứ?
Dialogue: 0,0:22:22.98,0:22:25.24,Default,,0000,0000,0000,,Hina-nee-chan là của mình.
Dialogue: 0,0:22:27.69,0:22:29.35,Default,,0000,0000,0000,,Chỉ của mình mà thôi?
Dialogue: 0,0:22:30.07,0:22:33.27,Default,,0000,0000,0000,,S-Shun-san... Không được...
Dialogue: 0,0:22:35.91,0:22:41.07,Default,,0000,0000,0000,,N-Nếu anh cứ tiếp tục như vậy, em sẽ ra mất...
Dialogue: 0,0:22:46.63,0:22:49.65,Default,,0000,0000,0000,,H-Hina-nee-chan, chị...
Dialogue: 0,0:22:49.65,0:22:55.40,Default,,0000,0000,0000,,Nhìn chị như vậy khiến tay em di chuyển càng nhanh hơn nữa...
Dialogue: 0,0:22:59.89,0:23:01.44,Default,,0000,0000,0000,,K-Không được...
Dialogue: 0,0:23:03.77,0:23:06.71,Default,,0000,0000,0000,,Em... Em... Sắp ra... Sắp ra...
Dialogue: 0,0:23:06.71,0:23:08.88,Default,,0000,0000,0000,,E-Em sẽ tè ra mất! {alt: I-I'm going to come!}
Dialogue: 0,0:23:08.88,0:23:11.89,Default,,0000,0000,0000,,Em sẽ tè ra mất!
Dialogue: 0,0:23:13.86,0:23:16.25,Default,,0000,0000,0000,,Hina-nee-chan đã ra rồi ư?
Dialogue: 0,0:23:18.15,0:23:20.92,Default,,0000,0000,0000,,Em... Em cũng ra đây...
Dialogue: 0,0:23:20.92,0:23:25.34,Default,,0000,0000,0000,,Em sẽ ra cùng chị...
Dialogue: 0,0:23:35.89,0:23:39.88,Default,,0000,0000,0000,,Shun-san đã uống nước tiểu của Hina.
Dialogue: 0,0:23:46.11,0:23:50.14,Default,,0000,0000,0000,,Anh thật sự là của riêng mình em thôi phải không?
Dialogue: 0,0:24:03.67,0:24:07.59,Default,,0000,0000,0000,,Tsugumi, chị không cho phép em dùng hệ thống an ninh...
Dialogue: 0,0:24:07.59,0:24:11.51,Default,,0000,0000,0000,,...để theo dõi Hina. Em hiểu chứ?
Dialogue: 0,0:24:16.01,0:24:20.19,Default,,0000,0000,0000,,Tại sao chị lại để cho hắn ta làm thế với Hina-nee-chan?
Dialogue: 0,0:24:20.19,0:24:24.44,Default,,0000,0000,0000,,Nếu nâng cấp hệ thống an ninh thì hắn sẽ chết với em...
Dialogue: 0,0:24:24.44,0:24:28.94,Default,,0000,0000,0000,,Chị hi vọng em đừng nhầm lẫn. \NĐó là những gì Hina mong muốn.
Dialogue: 0,0:24:28.94,0:24:32.35,Default,,0000,0000,0000,,Vả lại Hina không phải là của riêng mình em.
Dialogue: 0,0:24:32.99,0:24:36.70,Default,,0000,0000,0000,,Hoặc là chị sẽ nói với Hina rằng em dùng thứ này để theo dõi em ấy?
Dialogue: 0,0:24:36.70,0:24:38.09,Default,,0000,0000,0000,,Em biết rồi!
Dialogue: 0,0:24:38.09,0:24:41.37,Default,,0000,0000,0000,,Em nghe rõ đây. Đây là những gì chúng ta đang cần.
Dialogue: 0,0:24:41.37,0:24:43.17,Default,,0000,0000,0000,,Hãy nhớ kĩ đấy.
Dialogue: 0,0:24:47.92,0:24:51.25,Default,,0000,0000,0000,,Họ gần như đã nằm trong lòng bàn tay của cậu rồi ư?
Dialogue: 0,0:24:51.25,0:24:56.41,Default,,0000,0000,0000,,Chị muốn biết à? Đừng nói với tôi là chị ganh tị vì tôi không ngó ngàn gì đến chị?
Dialogue: 0,0:24:56.41,0:24:59.62,Default,,0000,0000,0000,,Cậu đùa đấy à? Tôi mà thèm?
Dialogue: 0,0:25:06.10,0:25:10.50,Default,,0000,0000,0000,,Có lẽ cậu nghĩ những gì cần làm là lật đổ Shizuru...
Dialogue: 0,0:25:10.50,0:25:14.61,Default,,0000,0000,0000,,...nhưng không phải căn nhà cũ mới là trung tâm của nơi này sao?
Dialogue: 0,0:25:15.02,0:25:19.01,Default,,0000,0000,0000,,Căn nhà cũ? Rốt cuộc có gì ở đó?
Dialogue: 0,0:25:30.96,0:25:35.96,Default,,0000,0000,0000,,
