Dialogue: 0,0:00:08.05,0:00:12.24,Default,,0000,0000,0000,,Đồ khốn! Có thôi ngay không thì bảo?
Dialogue: 0,0:00:16.51,0:00:20.63,Default,,0000,0000,0000,,Chống cự làm gì? Nó sướng lắm mà.
Dialogue: 0,0:00:20.63,0:00:27.11,Default,,0000,0000,0000,,Ông giỡn mặt với tôi đấy à? Sướng cái con khỉ!
Dialogue: 0,0:00:29.99,0:00:37.80,Default,,0000,0000,0000,,Lúc nào ông cũng tự tiện làm theo ý mình...
Dialogue: 0,0:00:39.13,0:00:41.28,Default,,0000,0000,0000,,Ướt đẫm thế này rồi còn chối.
Dialogue: 0,0:00:41.28,0:00:42.02,Default,,0000,0000,0000,,Im đê!
Dialogue: 0,0:00:43.31,0:00:47.90,Default,,0000,0000,0000,,Tại sao cơ thể mình lại thích chuyện này cơ chứ?
Dialogue: 0,0:00:53.28,0:00:58.85,Default,,0000,0000,0000,,Thằng nhỏ của hắn ta đang chơi đùa ở bên trong mình!
Dialogue: 0,0:00:58.85,0:01:01.90,Default,,0000,0000,0000,,Cảm giác bị hiếp ngay tại bàn của mình thế nào hả?
Dialogue: 0,0:01:02.95,0:01:09.70,Default,,0000,0000,0000,,Hiếp cái đầu ông! Là tôi để cho ông chơi thì có!
Dialogue: 0,0:01:20.09,0:01:23.81,Default,,0000,0000,0000,,Sao tự nhiên ông dám hôn tôi?
Dialogue: 0,0:01:24.84,0:01:27.60,Default,,0000,0000,0000,,Vẻ mặt mê li của em đã nói lên tất cả rồi đó.
Dialogue: 0,0:01:28.54,0:01:32.88,Default,,0000,0000,0000,,Đừng làm đột ngột như vậy! Nè!
Dialogue: 0,0:01:36.29,0:01:39.20,Default,,0000,0000,0000,,Ông sẽ bắn tinh dịch của mình vào bên trong tôi phải không?
Dialogue: 0,0:01:40.37,0:01:45.77,Default,,0000,0000,0000,,Hãy bắn nó vào đi!
Dialogue: 0,0:02:01.17,0:02:05.77,Default,,0000,0000,0000,,Tinh dịch ông nhiều như vầy... Tôi sẽ có thai mất...
Dialogue: 0,0:02:15.42,0:02:18.51,Default,,0000,0000,0000,,Thật là. Chỗ nào ông cũng chơi tôi được cả.
Dialogue: 0,0:02:19.59,0:02:21.89,Default,,0000,0000,0000,,Tôi đã nói là không hứng thú với mấy con heo mập mà.
Dialogue: 0,0:02:22.67,0:02:26.77,Default,,0000,0000,0000,,Thôi nào, lớp học cũng kết thúc rồi. Ông nên về đi là vừa.
Dialogue: 0,0:02:27.17,0:02:28.94,Default,,0000,0000,0000,,Tôi về trước đây.
Dialogue: 0,0:02:31.25,0:02:32.71,Default,,0000,0000,0000,,Sensei!
Dialogue: 0,0:02:34.50,0:02:37.46,Default,,0000,0000,0000,,Satsuki, đừng hù tôi vậy chứ.
Dialogue: 0,0:02:37.46,0:02:39.79,Default,,0000,0000,0000,,Khoan đã, em đến hồi nào vậy?
Dialogue: 0,0:02:39.79,0:02:42.72,Default,,0000,0000,0000,,Thầy chỉ làm với Shizuka là hỏng công bằng đâu đấy.
Dialogue: 0,0:02:44.96,0:02:48.27,Default,,0000,0000,0000,,Nó dính đầy chất dịch bẩn thỉu.
Dialogue: 0,0:02:59.68,0:03:02.49,Default,,0000,0000,0000,,Satsuki, tôi...
Dialogue: 0,0:03:02.49,0:03:06.20,Default,,0000,0000,0000,,Từ khi nào mà thầy thân với Shizuka vậy hả?
Dialogue: 0,0:03:07.57,0:03:12.82,Default,,0000,0000,0000,,Từ khi thầy chơi đùa cậu ấy bằng thằng nhỏ của mình ư?
Dialogue: 0,0:03:14.01,0:03:18.00,Default,,0000,0000,0000,,Không... phải chuyện của em!
Dialogue: 0,0:03:19.79,0:03:25.17,Default,,0000,0000,0000,,Đúng vậy. Em xin lỗi, đó không phải là chuyện của em.
Dialogue: 0,0:03:25.17,0:03:27.57,Default,,0000,0000,0000,,Từ khi nào mà em...
Dialogue: 0,0:03:27.57,0:03:30.96,Default,,0000,0000,0000,,Không sao đâu, thầy không cần kiềm chế làm gì.
Dialogue: 0,0:03:30.96,0:03:34.56,Default,,0000,0000,0000,,Em sẽ liếm cho đến khi nó thật sạch mới thôi.
Dialogue: 0,0:03:42.04,0:03:45.73,Default,,0000,0000,0000,,Thằng nhỏ của thầy đang ở bên trong em!
Dialogue: 0,0:03:48.73,0:03:52.58,Default,,0000,0000,0000,,Nó tuyệt quá đi mất!
Dialogue: 0,0:03:52.92,0:03:56.50,Default,,0000,0000,0000,,Bởi vì nó là của thầy phải không?
Dialogue: 0,0:03:56.50,0:04:00.30,Default,,0000,0000,0000,,Ngốc ạ, em rên lớn quá! Lỡ ai nghe thấy thì sao?
Dialogue: 0,0:04:03.97,0:04:05.69,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:04:09.20,0:04:10.99,Default,,0000,0000,0000,,Thằng nhỏ của thầy...
Dialogue: 0,0:04:14.46,0:04:15.75,Default,,0000,0000,0000,,Satsuki!
Dialogue: 0,0:04:17.18,0:04:21.46,Default,,0000,0000,0000,,Hở? Mình nghĩ vừa mới nghe tiếng của cậu ấy mà.
Dialogue: 0,0:04:22.32,0:04:23.97,Default,,0000,0000,0000,,Cậu ấy đã đi đâu chứ?
Dialogue: 0,0:04:26.37,0:04:29.18,Default,,0000,0000,0000,,Thằng nhỏ của thầy đang run lên kìa.
Dialogue: 0,0:04:29.18,0:04:32.09,Default,,0000,0000,0000,,Thầy không mau ra là không được đâu đấy.
Dialogue: 0,0:04:32.09,0:04:34.30,Default,,0000,0000,0000,,Tôi biết rồi.
Dialogue: 0,0:04:35.86,0:04:41.72,Default,,0000,0000,0000,,Thô bạo quá! Thằng nhỏ của thầy đang nện vào âm hộ của em.
Dialogue: 0,0:04:43.21,0:04:48.01,Default,,0000,0000,0000,,Sướng quá! Sướng hơn khi làm với Shizuka nhiều!
Dialogue: 0,0:04:49.16,0:04:54.32,Default,,0000,0000,0000,,Sensei! Hãy hủy hoại âm hộ của em đi!
Dialogue: 0,0:05:01.22,0:05:09.04,Default,,0000,0000,0000,,Hãy thô bạo hơn nữa! Chơi đùa với âm hộ của em! Em muốn thầy ra thật nhiều!
Dialogue: 0,0:05:10.33,0:05:16.08,Default,,0000,0000,0000,,Thầy muốn ra bao nhiêu cũng được! Âm hộ em là của thầy đó!
Dialogue: 0,0:05:32.05,0:05:34.07,Default,,0000,0000,0000,,Tinh dịch của thầy...
Dialogue: 0,0:05:39.02,0:05:41.78,Default,,0000,0000,0000,,Thầy đã lấp đầy âm hộ của em...
Dialogue: 0,0:05:48.35,0:05:51.98,Default,,0000,0000,0000,,Thằng nhỏ của thầy vẫn còn lớn quá, nhưng mà...
Dialogue: 0,0:05:51.98,0:05:55.80,Default,,0000,0000,0000,,Để chụp ảnh xong rồi chúng ta lại làm tiếp nhé!
