Dialogue: 0,0:00:01.40,0:00:05.03,640x480,,0000,0000,0000,,Bộ Năm Kinh Hoàng đã điều khiển ngôi trường này sau khi hiệu trưởng cũ rời đi.
Dialogue: 0,0:00:05.37,0:00:11.24,640x480,,0000,0000,0000,,Sau những hành động bắt nạt của họ, \Ntôi đã gần như đánh mất niềm đam mê dạy học.
Dialogue: 0,0:00:12.01,0:00:16.34,640x480,,0000,0000,0000,,Thế rồi Marimo, chủ tịch hội học sinh, đã đến bên cạnh và giúp đỡ tôi.
Dialogue: 0,0:00:17.01,0:00:21.45,640x480,,0000,0000,0000,,Với sự giúp đỡ của em ấy, tôi đã nỗ lực hết mình...
Dialogue: 0,0:00:21.82,0:00:25.05,640x480,,0000,0000,0000,,...để tống khứ họ ra khỏi trường từng người một.
Dialogue: 0,0:00:25.19,0:00:30.63,640x480,,0000,0000,0000,,Mục tiêu đầu tiên là giáo viên tiếng Nhật, Kannonji Miki.
Dialogue: 0,0:00:31.53,0:00:39.13,640x480,,0000,0000,0000,,Tiếp theo là huấn luyện viên đội tuyển bơi lội, Onijima Mayumi.
Dialogue: 0,0:00:40.64,0:00:43.73,640x480,,0000,0000,0000,,Kế hoạch của chúng tôi đã thành công.
Dialogue: 0,0:00:44.24,0:00:49.77,640x480,,0000,0000,0000,,Họ đã trở thành trò cười đối với học sinh và rời khỏi trường.
Dialogue: 0,0:00:53.68,0:01:04.32,640x480,,0000,0000,0000,,{\fad(2000,0)\pos(328,84)\fs50\bord4\c&H281DE8&\3c&H000722&}Baka-sub trận trọng giới thiệu
Dialogue: 0,0:01:08.26,0:01:10.39,640x480,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:01:42.13,0:01:48.13,640x480,,0000,0000,0000,,Sensei, hãy làm mạnh hơn nữa, làm ơn!
Dialogue: 0,0:01:50.44,0:01:52.84,640x480,,0000,0000,0000,,Giống như thầy đã làm với Miki và Mayumi ư?
Dialogue: 0,0:01:55.11,0:01:57.44,640x480,,0000,0000,0000,,Bọn họ chả là gì cả.
Dialogue: 0,0:01:58.15,0:02:03.14,640x480,,0000,0000,0000,,Thầy là của em, nên hãy làm mạnh hơn nữa, làm ơn.
Dialogue: 0,0:02:04.75,0:02:07.09,640x480,,0000,0000,0000,,Em thật là một chủ tịch hội học sinh hư hỏng.
Dialogue: 0,0:02:16.70,0:02:20.90,640x480,,0000,0000,0000,,Thằng nhỏ của thầy đang lấp đầy bên trong em từ phía sau.
Dialogue: 0,0:02:22.37,0:02:24.20,640x480,,0000,0000,0000,,Nó đang đâm vào bên trong...
Dialogue: 0,0:02:24.81,0:02:27.80,640x480,,0000,0000,0000,,Sensei! Sensei!
Dialogue: 0,0:02:32.68,0:02:38.31,640x480,,0000,0000,0000,,Nó đang đâm vào bên trong, thứ to lớn và cứng cáp của thầy...
Dialogue: 0,0:02:38.82,0:02:41.31,640x480,,0000,0000,0000,,Không được, sensei...
Dialogue: 0,0:02:42.16,0:02:43.12,640x480,,0000,0000,0000,,Sensei!
Dialogue: 0,0:02:44.86,0:02:48.59,640x480,,0000,0000,0000,,Em ra đây~!
Dialogue: 0,0:03:10.19,0:03:12.55,640x480,,0000,0000,0000,,Còn lại ba người.
Dialogue: 0,0:03:17.53,0:03:20.15,640x480,,0000,0000,0000,,Hành vi của họ không bình thường chút nào.
Dialogue: 0,0:03:20.70,0:03:23.16,640x480,,0000,0000,0000,,Có thể có ai đó đứng sau chuyện này.
Dialogue: 0,0:03:24.57,0:03:25.73,640x480,,0000,0000,0000,,Một âm mưu?
Dialogue: 0,0:03:26.33,0:03:29.74,640x480,,0000,0000,0000,,Phải, ai đó đang có ý định đưa chúng ta vào bẫy.
Dialogue: 0,0:03:30.37,0:03:31.74,640x480,,0000,0000,0000,,Không thể nào?
Dialogue: 0,0:03:32.77,0:03:34.74,640x480,,0000,0000,0000,,Không hẳn là không thể.
Dialogue: 0,0:03:34.88,0:03:39.47,640x480,,0000,0000,0000,,Nếu loại bỏ được chúng ta, thì dĩ nhiên ngôi trường này sẽ nằm dưới quyền của hắn!
Dialogue: 0,0:03:39.65,0:03:41.28,640x480,,0000,0000,0000,,Nhưng là ai cơ chứ?
Dialogue: 0,0:03:41.35,0:03:42.98,640x480,,0000,0000,0000,,Tôi cũng đang tự hỏi.
Dialogue: 0,0:03:43.32,0:03:46.72,640x480,,0000,0000,0000,,Không một ai trong hội đồng nhà trường đủ can đảm.
Dialogue: 0,0:03:46.86,0:03:48.76,640x480,,0000,0000,0000,,Và cũng không phải giáo viên...
Dialogue: 0,0:03:59.77,0:04:01.76,640x480,,0000,0000,0000,,Không lẽ... Con khỉ đó?
Dialogue: 0,0:04:02.60,0:04:05.87,640x480,,0000,0000,0000,,Dù phải hay không, cũng không được lơ là.
Dialogue: 0,0:04:06.04,0:04:08.53,640x480,,0000,0000,0000,,Hãy chú ý mọi thứ xung quanh, được chứ?
Dialogue: 0,0:04:09.31,0:04:10.30,640x480,,0000,0000,0000,,Vâng.
Dialogue: 0,0:04:12.01,0:04:14.00,640x480,,0000,0000,0000,,Vậy, gặp lại sau nhé.
Dialogue: 0,0:04:27.30,0:04:29.86,640x480,,0000,0000,0000,,Mizuta Rui và Aizaki Kaori...
Dialogue: 0,0:04:30.77,0:04:34.03,640x480,,0000,0000,0000,,Có lẽ thật quá xa vời khi nghĩ tới họ...
Dialogue: 0,0:04:45.61,0:04:47.34,640x480,,0000,0000,0000,,Năm nhất ư?
Dialogue: 0,0:04:47.58,0:04:49.48,640x480,,0000,0000,0000,,Em ấy làm gì ở đây?
Dialogue: 0,0:04:58.36,0:05:01.56,640x480,,0000,0000,0000,,Chào, em ăn trưa ở đây mỗi ngày ư?
Dialogue: 0,0:05:02.10,0:05:07.97,640x480,,0000,0000,0000,,Vâng, có lẽ em cảm thấy thoải mái hơn khi được ở một mình.
Dialogue: 0,0:05:08.64,0:05:09.97,640x480,,0000,0000,0000,,Em không được phép sao?
Dialogue: 0,0:05:10.11,0:05:12.97,640x480,,0000,0000,0000,,Không, thầy không nghĩ vậy.
Dialogue: 0,0:05:13.38,0:05:15.57,640x480,,0000,0000,0000,,Thầy chỉ nghĩ có lẽ em đang cảm thấy cô đơn.
Dialogue: 0,0:05:16.44,0:05:21.07,640x480,,0000,0000,0000,,Em muốn được ở đây, chỗ này thật thoải mái.
Dialogue: 0,0:05:22.38,0:05:25.58,640x480,,0000,0000,0000,,Em ấy cũng bị xa lánh...
Dialogue: 0,0:05:36.43,0:05:40.09,640x480,,0000,0000,0000,,Em ấy làm tôi nhớ lại mình trước đây.
Dialogue: 0,0:05:56.45,0:05:57.98,640x480,,0000,0000,0000,,Thật không thể tin được!
Dialogue: 0,0:05:58.95,0:06:02.95,640x480,,0000,0000,0000,,Suốt trong giờ học, em chỉ chăm chú nhìn vào thứ vớ vẩn này!
Dialogue: 0,0:06:03.39,0:06:05.09,640x480,,0000,0000,0000,,Thật là đáng xấu hổ!
Dialogue: 0,0:06:05.49,0:06:08.39,640x480,,0000,0000,0000,,Tôi phải nói là em không hề có óc thẩm mĩ.
Dialogue: 0,0:06:08.56,0:06:11.56,640x480,,0000,0000,0000,,Không thể tha được, tuyệt đối không thể tha được!
Dialogue: 0,0:06:11.87,0:06:15.10,640x480,,0000,0000,0000,,Nghe kĩ đây, hãy đến phòng thực hành hóa sau giờ học!
Dialogue: 0,0:06:15.87,0:06:17.36,640x480,,0000,0000,0000,,Em nghe rõ rồi chứ?
Dialogue: 0,0:06:17.87,0:06:18.90,640x480,,0000,0000,0000,,Vâng.
Dialogue: 0,0:06:25.01,0:06:27.41,640x480,,0000,0000,0000,,Cô sẽ làm gì đây, Aizaki Kaori?
Dialogue: 0,0:06:39.53,0:06:42.59,640x480,,0000,0000,0000,,Hãy cở quần áo ra, cả đồ lót nữa!
Dialogue: 0,0:06:44.47,0:06:46.73,640x480,,0000,0000,0000,,Thật là một giáo viên mĩ thuật không ra gì!
Dialogue: 0,0:06:48.57,0:06:51.97,640x480,,0000,0000,0000,,Em nghĩ mình có thể biết được tình yêu đích thực...
Dialogue: 0,0:06:52.44,0:06:53.60,640x480,,0000,0000,0000,,...từ cuốn tạp chí khiêu dâm này ư?
Dialogue: 0,0:07:05.22,0:07:07.69,640x480,,0000,0000,0000,,Hãy bỏ tay ra khỏi thứ rác rưởi đó!
Dialogue: 0,0:07:09.96,0:07:14.45,640x480,,0000,0000,0000,,Chúng tôi sẽ cho em thấy \Nthế nào là tình yêu và làm tình thật sự.
Dialogue: 0,0:07:14.53,0:07:16.52,640x480,,0000,0000,0000,,Hãy nhìn cho kĩ vào.
Dialogue: 0,0:07:16.73,0:07:20.53,640x480,,0000,0000,0000,,Nếu như vậy sau này em sẽ không \Nchú ý đến những tạp chí rác rưởi đó nữa.
Dialogue: 0,0:07:43.49,0:07:44.75,640x480,,0000,0000,0000,,Em đang làm gì vậy?
Dialogue: 0,0:07:45.19,0:07:48.13,640x480,,0000,0000,0000,,Mau bỏ tay ra ngay!
Dialogue: 0,0:07:51.47,0:07:54.53,640x480,,0000,0000,0000,,Nữa đi, hãy liếm em nhiều hơn nữa...
Dialogue: 0,0:07:55.54,0:07:57.00,640x480,,0000,0000,0000,,Hãy liếm mạnh hơn nữa...
Dialogue: 0,0:08:07.85,0:08:10.91,640x480,,0000,0000,0000,,Mùi vị thật là ngon, Rui.
Dialogue: 0,0:08:16.93,0:08:21.19,640x480,,0000,0000,0000,,Onee-sama, trao nó cho em.
Dialogue: 0,0:08:23.53,0:08:25.80,640x480,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:08:34.28,0:08:38.27,640x480,,0000,0000,0000,,Onee-sama, chỗ đó, xung quanh âm vật.
Dialogue: 0,0:08:43.59,0:08:46.25,640x480,,0000,0000,0000,,Rui, em đẹp lắm.
Dialogue: 0,0:08:46.82,0:08:51.05,640x480,,0000,0000,0000,,Chỗ kín đáo của em đang chấp nhận chị và siết chặt ngón tay của chị.
Dialogue: 0,0:08:52.86,0:08:55.02,640x480,,0000,0000,0000,,Đừng nói những thứ xấu hổ như vậy...
Dialogue: 0,0:08:55.53,0:08:58.23,640x480,,0000,0000,0000,,Âm hộ em là của chị.
Dialogue: 0,0:08:58.57,0:09:01.97,640x480,,0000,0000,0000,,Vâng, mọi thứ của em điều thuộc về chị.
Dialogue: 0,0:09:02.57,0:09:05.16,640x480,,0000,0000,0000,,Không. Nữa đi, nữa đi!
Dialogue: 0,0:09:09.98,0:09:12.91,640x480,,0000,0000,0000,,Em không thể giữ lâu hơn được nữa.
Dialogue: 0,0:09:13.62,0:09:15.24,640x480,,0000,0000,0000,,Không được!
Dialogue: 0,0:09:23.66,0:09:25.68,640x480,,0000,0000,0000,,Chết tiệt, thật là nóng bỏng!
Dialogue: 0,0:09:32.10,0:09:35.09,640x480,,0000,0000,0000,,Aizaki-sensei, dương vật của em không thể cứng hơn được nữa...
Dialogue: 0,0:09:35.24,0:09:37.10,640x480,,0000,0000,0000,,Em không thể chịu được nữa.
Dialogue: 0,0:09:37.94,0:09:42.21,640x480,,0000,0000,0000,,Rui, hãy bú dương vật hôi thối và bẩn thỉu của em ấy nào!
Dialogue: 0,0:09:43.11,0:09:45.67,640x480,,0000,0000,0000,,Không thể. Em không làm được!
Dialogue: 0,0:09:46.11,0:09:50.95,640x480,,0000,0000,0000,,Không sao đâu, nếu em yêu chị thì không có gì phải sợ cả.
Dialogue: 0,0:09:51.29,0:09:55.25,640x480,,0000,0000,0000,,Chị sẽ yêu tất cả mọi thứ thuộc về em
Dialogue: 0,0:09:58.09,0:09:59.68,640x480,,0000,0000,0000,,Không, nó thật hôi thối!
Dialogue: 0,0:09:59.79,0:10:02.35,640x480,,0000,0000,0000,,Hãy tin ở chị, nhanh lên nào.
Dialogue: 0,0:10:04.70,0:10:07.03,640x480,,0000,0000,0000,,Onee-sama, không phải chỗ đó...
Dialogue: 0,0:10:08.60,0:10:10.66,640x480,,0000,0000,0000,,Onee-sama!
Dialogue: 0,0:10:20.32,0:10:22.61,640x480,,0000,0000,0000,,Cậu học sinh ấy thật đáng thương...
Dialogue: 0,0:10:22.98,0:10:25.32,640x480,,0000,0000,0000,,Nhưng bây giờ mình không thể ra ngoài được...
Dialogue: 0,0:10:25.42,0:10:27.75,640x480,,0000,0000,0000,,Mình đoán có thể lợi dụng cậu ta.
Dialogue: 0,0:10:28.09,0:10:29.72,640x480,,0000,0000,0000,,Đến đây nào.
Dialogue: 0,0:10:29.79,0:10:32.26,640x480,,0000,0000,0000,,Và đút thứ xấu xí này vào bên trong cô ấy.
Dialogue: 0,0:11:00.79,0:11:05.38,640x480,,0000,0000,0000,,Xin lỗi nhé. Nhưng đây không phải là thứ kích thích cho tình yêu của chúng ta sao?
Dialogue: 0,0:11:05.99,0:11:07.29,640x480,,0000,0000,0000,,Onee-sama!
Dialogue: 0,0:11:55.34,0:11:58.68,640x480,,0000,0000,0000,,Onee-sama, sao chị lại làm vậy?
Dialogue: 0,0:12:00.82,0:12:01.87,640x480,,0000,0000,0000,,Mizuta-sensei...
Dialogue: 0,0:12:05.39,0:12:07.32,640x480,,0000,0000,0000,,À, Sakagami.
Dialogue: 0,0:12:07.46,0:12:09.75,640x480,,0000,0000,0000,,Em đang hẹn hò với bạn trai à?
Dialogue: 0,0:12:09.96,0:12:15.09,640x480,,0000,0000,0000,,Sensei, thật là. Em không có hứng thú với con trai.
Dialogue: 0,0:12:25.41,0:12:29.43,640x480,,0000,0000,0000,,Cô có biết rằng lúc nào em cũng chú ý đến cô không?
Dialogue: 0,0:12:29.54,0:12:31.71,640x480,,0000,0000,0000,,Cô hiểu ý em chứ?
Dialogue: 0,0:12:35.88,0:12:38.78,640x480,,0000,0000,0000,,Sensei, em có vài thứ cần nói với cô.
Dialogue: 0,0:12:39.82,0:12:44.16,640x480,,0000,0000,0000,,Em lúc nào cũng rất ngưỡng mộ cô...
Dialogue: 0,0:12:44.76,0:12:46.06,640x480,,0000,0000,0000,,Em yêu cô, sensei!
Dialogue: 0,0:12:52.00,0:12:53.47,640x480,,0000,0000,0000,,Mình đã có được cô ấy.
Dialogue: 0,0:13:20.80,0:13:23.76,640x480,,0000,0000,0000,,Marimo-chan, em thật sự rất đẹp.
Dialogue: 0,0:13:23.87,0:13:27.10,640x480,,0000,0000,0000,,Sensei, em sẽ không buôn tay cô ra đâu.
Dialogue: 0,0:13:37.35,0:13:40.87,640x480,,0000,0000,0000,,Cô chẳng là gì ngoài một kẻ đạo đức giả.
Dialogue: 0,0:13:40.98,0:13:45.35,640x480,,0000,0000,0000,,Ngày phán quyết đang đến gần.
Dialogue: 0,0:14:21.49,0:14:22.89,640x480,,0000,0000,0000,,Chuyện gì ồn ào vậy nhỉ?
Dialogue: 0,0:14:23.59,0:14:25.99,640x480,,0000,0000,0000,,Gì chứ, mày đang khóc đấy ư?
Dialogue: 0,0:14:26.09,0:14:28.43,640x480,,0000,0000,0000,,Các em kia đang làm gì ở đây vậy? Dừng lại ngay!
Dialogue: 0,0:14:28.53,0:14:29.02,640x480,,0000,0000,0000,,Thôi chết!
Dialogue: 0,0:14:29.13,0:14:29.99,640x480,,0000,0000,0000,,Chạy thôi!
Dialogue: 0,0:14:34.94,0:14:36.37,640x480,,0000,0000,0000,,Em không sao chứ?
Dialogue: 0,0:14:38.47,0:14:39.37,640x480,,0000,0000,0000,,Vâng.
Dialogue: 0,0:15:02.00,0:15:03.26,640x480,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:15:04.60,0:15:08.59,640x480,,0000,0000,0000,,Aizaki-sensei, họ đang có một mối quan hệ bất chính.
Dialogue: 0,0:15:08.70,0:15:11.93,640x480,,0000,0000,0000,,Như thế cũng được sao? Giáo viên và học sinh lại có quan hệ như vậy.
Dialogue: 0,0:15:12.91,0:15:13.90,640x480,,0000,0000,0000,,Không thể bỏ qua được!
Dialogue: 0,0:15:14.08,0:15:15.87,640x480,,0000,0000,0000,,Tôi tuyệt đối sẽ không bỏ qua!
Dialogue: 0,0:15:16.08,0:15:20.04,640x480,,0000,0000,0000,,Nếu như vậy, tôi sẽ giúp cô. Họ đáng bị trừng phạt.
Dialogue: 0,0:15:20.55,0:15:23.45,640x480,,0000,0000,0000,,Đúng vậy, theo tôi thì Sakagami-san mới là người có lỗi.
Dialogue: 0,0:15:23.62,0:15:25.61,640x480,,0000,0000,0000,,Chính em ấy quyến rũ Mizuta-sensei.
Dialogue: 0,0:15:25.92,0:15:30.38,640x480,,0000,0000,0000,,Nếu không thì đời nào Rui... Mizuta-sensei lại làm như vậy!
Dialogue: 0,0:15:30.49,0:15:31.96,640x480,,0000,0000,0000,,Có lẽ là vậy.
Dialogue: 0,0:15:32.06,0:15:36.69,640x480,,0000,0000,0000,,Chúng ta không thể áp dụng những điều củ kỹ vào bọn trẻ thời nay.
Dialogue: 0,0:15:37.67,0:15:41.23,640x480,,0000,0000,0000,,Đúng vậy, em ấy cần phải được đặt vào đúng chỗ của mình.
Dialogue: 0,0:15:41.60,0:15:42.93,640x480,,0000,0000,0000,,Ý cô là sao?
Dialogue: 0,0:15:43.67,0:15:48.13,640x480,,0000,0000,0000,,Thầy không nghĩ là mình có thể bắt cóc Sakagami-san và dạy dỗ lại em ấy sao?
Dialogue: 0,0:15:48.51,0:15:49.53,640x480,,0000,0000,0000,,Hể?
Dialogue: 0,0:15:50.65,0:15:53.48,640x480,,0000,0000,0000,,Không cần phải lo, thầy không chỉ có một mình đâu.
Dialogue: 0,0:15:54.35,0:15:56.51,640x480,,0000,0000,0000,,Tôi biết một người có thể giúp thầy.
Dialogue: 0,0:16:04.06,0:16:05.18,640x480,,0000,0000,0000,,Cô ấy sắp đến rồi.
Dialogue: 0,0:16:05.29,0:16:08.23,640x480,,0000,0000,0000,,Thầy chắc là em có thể làm tình với chủ tịch hội học sinh chứ?
Dialogue: 0,0:16:08.70,0:16:11.10,640x480,,0000,0000,0000,,Phải, tôi sẽ để cho cậu lên trước.
Dialogue: 0,0:16:11.50,0:16:14.73,640x480,,0000,0000,0000,,Đè cô ấy xuống và nhanh chóng kéo vào đây.
Dialogue: 0,0:16:15.07,0:16:16.00,640x480,,0000,0000,0000,,Vâng.
Dialogue: 0,0:16:17.51,0:16:18.44,640x480,,0000,0000,0000,,Đến rồi.
Dialogue: 0,0:16:31.25,0:16:32.58,640x480,,0000,0000,0000,,Được rồi!
Dialogue: 0,0:16:32.75,0:16:36.02,640x480,,0000,0000,0000,,Giấc mơ của mình, được làm tình với chủ tịch hội học sinh Marimo!
Dialogue: 0,0:16:38.16,0:16:39.96,640x480,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:16:45.03,0:16:51.20,640x480,,0000,0000,0000,,Bây giờ cậu sẽ bị tống vào tù vì tội hiếp dâm.
Dialogue: 0,0:16:51.47,0:16:56.64,640x480,,0000,0000,0000,,Chúng tôi ở đây là để thay hội đồng trường điều tra Aizaki Kaori và Mizuta Rui.
Dialogue: 0,0:17:18.07,0:17:20.40,640x480,,0000,0000,0000,,Onee-sama, Marimo-chan đang ở đâu?
Dialogue: 0,0:17:21.44,0:17:23.30,640x480,,0000,0000,0000,,Em đang nói gì vậy?
Dialogue: 0,0:17:24.07,0:17:25.66,640x480,,0000,0000,0000,,Đừng có giả ngây!
Dialogue: 0,0:17:26.14,0:17:30.74,640x480,,0000,0000,0000,,Marimo-chan đã gọi cho em, và nói rằng chị đã bắt cóc em ấy.
Dialogue: 0,0:17:32.48,0:17:33.67,640x480,,0000,0000,0000,,Gọi cho em?
Dialogue: 0,0:17:34.38,0:17:39.28,640x480,,0000,0000,0000,,Con khỉ đã nói với chị là hắn \Nvà tên nô lệ đã để em ấy chạy thoát...
Dialogue: 0,0:17:44.33,0:17:48.63,640x480,,0000,0000,0000,,Tôi đã ghi lại quan hệ bất chính của hai người trong cuốn băng này...
Dialogue: 0,0:17:48.86,0:17:53.27,640x480,,0000,0000,0000,,...và sự sợ hãi của Aizaki đúng như dự định của tôi.
Dialogue: 0,0:17:53.37,0:17:56.10,640x480,,0000,0000,0000,,Tôi sẽ tố cáo hành động phạm pháp của hai người.
Dialogue: 0,0:17:56.50,0:17:57.60,640x480,,0000,0000,0000,,Cái gì...
Dialogue: 0,0:17:58.81,0:18:01.10,640x480,,0000,0000,0000,,Thầy có bằng chứng gì chứ?
Dialogue: 0,0:18:02.31,0:18:04.47,640x480,,0000,0000,0000,,Đoạn bằng này còn chưa đủ sao?
Dialogue: 0,0:18:05.68,0:18:06.54,640x480,,0000,0000,0000,,Này...
Dialogue: 0,0:18:10.52,0:18:14.61,640x480,,0000,0000,0000,,Bây giờ nô lệ của cô đã trở thành nhân chứng quan trọng của chúng tôi.
Dialogue: 0,0:18:17.39,0:18:20.29,640x480,,0000,0000,0000,,Làm ơn, đừng làm vậy...
Dialogue: 0,0:18:20.43,0:18:22.16,640x480,,0000,0000,0000,,Tôi sẽ làm mọi thứ.
Dialogue: 0,0:18:34.31,0:18:36.21,640x480,,0000,0000,0000,,Đầu tiên sẽ là cô.
Dialogue: 0,0:18:36.28,0:18:39.14,640x480,,0000,0000,0000,,Cậu không cần phải ngại. Cô ta là một kẻ cuồng dâm.
Dialogue: 0,0:18:39.21,0:18:41.08,640x480,,0000,0000,0000,,Cứ làm những gì mình muốn.
Dialogue: 0,0:18:43.82,0:18:45.62,640x480,,0000,0000,0000,,Làm ơn dừng lại đi!
Dialogue: 0,0:18:46.22,0:18:47.21,640x480,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:18:54.80,0:18:55.66,640x480,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:18:56.20,0:18:57.76,640x480,,0000,0000,0000,,Không phải vui lắm sao?
Dialogue: 0,0:18:58.40,0:18:59.70,640x480,,0000,0000,0000,,Cả bên đây nữa!
Dialogue: 0,0:18:59.80,0:19:01.13,640x480,,0000,0000,0000,,Làm ơn, dừng lại đi!
Dialogue: 0,0:19:01.54,0:19:04.10,640x480,,0000,0000,0000,,Sao hả? Như vầy sướng lắm phải không?
Dialogue: 0,0:19:04.44,0:19:05.77,640x480,,0000,0000,0000,,Kẻ đạo đức giả!
Dialogue: 0,0:19:05.94,0:19:09.14,640x480,,0000,0000,0000,,Cô sẽ cảm nhận được sự vui sướng của địa ngục!
Dialogue: 0,0:19:11.58,0:19:12.91,640x480,,0000,0000,0000,,Rui...
Dialogue: 0,0:19:15.35,0:19:16.51,640x480,,0000,0000,0000,,Nhìn này.
Dialogue: 0,0:19:16.62,0:19:20.11,640x480,,0000,0000,0000,,Ban đầu cô cảm thấy đau, nhưng trông như cô đang hứng lên vì nó.
Dialogue: 0,0:19:20.25,0:19:22.28,640x480,,0000,0000,0000,,Đó là bản chất của kẻ cuồng dâm.
Dialogue: 0,0:19:22.39,0:19:23.22,640x480,,0000,0000,0000,,Tiếp theo sẽ là chỗ này
Dialogue: 0,0:19:24.59,0:19:26.46,640x480,,0000,0000,0000,,Dừng lại đi! Âm vật của tôi đang bị kéo căng!
Dialogue: 0,0:19:26.56,0:19:28.19,640x480,,0000,0000,0000,,Làm ơn dừng lại đi!
Dialogue: 0,0:19:29.90,0:19:31.49,640x480,,0000,0000,0000,,Không!
Dialogue: 0,0:19:31.63,0:19:34.76,640x480,,0000,0000,0000,,Nhưng mà đưới này của cô ướt hết cả rồi.
Dialogue: 0,0:19:37.97,0:19:40.17,640x480,,0000,0000,0000,,A, cô ra mất rồi.
Dialogue: 0,0:19:40.31,0:19:43.77,640x480,,0000,0000,0000,,Được rồi, không hút chân không nữa, \Nchuyển sang dùng thuốc dưỡng da đặc biệt nào.
Dialogue: 0,0:19:43.91,0:19:46.21,640x480,,0000,0000,0000,,Nó sẽ khiến cô sướng điên lên.
Dialogue: 0,0:19:47.31,0:19:50.25,640x480,,0000,0000,0000,,Đây là hỗn hợp Wasabi, mù tạt và Tabasco.
Dialogue: 0,0:19:50.42,0:19:55.15,640x480,,0000,0000,0000,,Làm ơn, tôi sẽ làm mọi thứ thầy muốn!
Dialogue: 0,0:19:58.29,0:20:00.19,640x480,,0000,0000,0000,,Âm vật của cô đang lớn lên.
Dialogue: 0,0:20:00.33,0:20:02.16,640x480,,0000,0000,0000,,Bây giờ, tôi không biết cón ai chịu lấy cô nữa không.
Dialogue: 0,0:20:05.40,0:20:06.70,640x480,,0000,0000,0000,,Nó đang bắt đầu có tác dụng!
Dialogue: 0,0:20:07.50,0:20:09.63,640x480,,0000,0000,0000,,Cơn nóng sẽ không thể dừng lại được.
Dialogue: 0,0:20:09.74,0:20:11.73,640x480,,0000,0000,0000,,Cô sẽ van xin tôi làm cô sướng hơn nữa...
Dialogue: 0,0:20:12.24,0:20:13.80,640x480,,0000,0000,0000,,Nóng quá, nóng quá!
Dialogue: 0,0:20:13.97,0:20:15.64,640x480,,0000,0000,0000,,Làm ơn hãy rửa sạch nó!
Dialogue: 0,0:20:15.74,0:20:17.68,640x480,,0000,0000,0000,,Nóng quá! Nóng quá!
Dialogue: 0,0:20:20.35,0:20:22.51,640x480,,0000,0000,0000,,Tiếp theo là đến chỗ này!
Dialogue: 0,0:20:24.35,0:20:25.18,640x480,,0000,0000,0000,,Nóng quá!
Dialogue: 0,0:20:29.89,0:20:33.88,640x480,,0000,0000,0000,,Nóng quá... làm ơn! Bên trong âm hộ tôi đau quá! Đau quá!
Dialogue: 0,0:20:34.26,0:20:36.66,640x480,,0000,0000,0000,,Ai đó... làm ơn rửa sạch nó!
Dialogue: 0,0:20:36.76,0:20:38.56,640x480,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:20:44.24,0:20:45.97,640x480,,0000,0000,0000,,Sau cô không tự mình làm lấy?
Dialogue: 0,0:20:46.31,0:20:49.24,640x480,,0000,0000,0000,,Nóng quá! Đau quá! Ai đó cứu tôi với!
Dialogue: 0,0:20:49.88,0:20:54.01,640x480,,0000,0000,0000,,Nhìn này, cô lại ướt nữa rồi, thật là một con điếm!
Dialogue: 0,0:21:08.96,0:21:11.30,640x480,,0000,0000,0000,,Bây giờ đến lượt cô, đồ ti tiện!
Dialogue: 0,0:21:15.34,0:21:18.57,640x480,,0000,0000,0000,,Chuyện gì xảy ra với thái độ của cô khi bắt nạt học sinh vậy?
Dialogue: 0,0:21:18.74,0:21:22.68,640x480,,0000,0000,0000,,Tôi sẽ cho cô những trải nhiệm thật đặc biệt.
Dialogue: 0,0:21:23.41,0:21:24.31,640x480,,0000,0000,0000,,Mang cái chai đó đến đây!
Dialogue: 0,0:21:25.48,0:21:28.35,640x480,,0000,0000,0000,,Trông như cô thật sự rất căm thù đàn ông.
Dialogue: 0,0:21:28.55,0:21:31.54,640x480,,0000,0000,0000,,Vậy tại sao bây giờ chúng ta không làm sạch cái lỗ của cô bằng thứ này?
Dialogue: 0,0:21:31.75,0:21:34.31,640x480,,0000,0000,0000,,Hãy bắt đầu rửa sạch cơ thể của cô nào!
Dialogue: 0,0:21:35.59,0:21:38.96,640x480,,0000,0000,0000,,Không phải nó giống như cả thể xác và tâm hồn của cô đều được gột rửa ư?
Dialogue: 0,0:21:39.43,0:21:42.83,640x480,,0000,0000,0000,,Làm ơn, nóng quá, nóng quá! Làm ơn dừng lại đi!
Dialogue: 0,0:21:43.46,0:21:47.46,640x480,,0000,0000,0000,,Lấp đầy miệng cô ta bằng dương vật \Ncủa cậu để cô ta im lặng nào.
Dialogue: 0,0:21:48.34,0:21:53.43,640x480,,0000,0000,0000,,Nếu cô mà cắn thì những thứ còn tồi tệ hơn đang chờ cô đấy.
Dialogue: 0,0:21:57.95,0:22:00.38,640x480,,0000,0000,0000,,Nó làm tôi nhớ đến truyện Gia nhân và quái vật.
Dialogue: 0,0:22:06.79,0:22:09.59,640x480,,0000,0000,0000,,Chết tiệt, cô dám đem em ra làm trò đùa!
Dialogue: 0,0:22:09.76,0:22:10.62,640x480,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:22:11.46,0:22:12.36,640x480,,0000,0000,0000,,Em sắp ra rồi!
Dialogue: 0,0:22:12.63,0:22:14.03,640x480,,0000,0000,0000,,Ra rồi! Ra rồi!
Dialogue: 0,0:22:14.50,0:22:16.43,640x480,,0000,0000,0000,,Sensei! Em ra đây!
Dialogue: 0,0:22:19.73,0:22:21.36,640x480,,0000,0000,0000,,Nhanh quá đấy?
Dialogue: 0,0:22:21.94,0:22:25.10,640x480,,0000,0000,0000,,Xin lỗi, em đã không được thỏa mãn từ lâu lắm rồi.
Dialogue: 0,0:22:25.34,0:22:28.57,640x480,,0000,0000,0000,,Vậy à, nó phải còn sung sức lắm.
Dialogue: 0,0:22:28.91,0:22:31.43,640x480,,0000,0000,0000,,Tại sao cậu không thử lỗ khác?
Dialogue: 0,0:22:31.51,0:22:33.50,640x480,,0000,0000,0000,,Nhưng nhớ để dành hậu môn cho tôi!
Dialogue: 0,0:22:33.65,0:22:34.11,640x480,,0000,0000,0000,,Vâng!
Dialogue: 0,0:22:42.49,0:22:44.62,640x480,,0000,0000,0000,,Em đang đâm nó vào...
Dialogue: 0,0:22:44.73,0:22:46.85,640x480,,0000,0000,0000,,Cậu chỉ có thế thôi sao?
Dialogue: 0,0:22:46.96,0:22:51.36,640x480,,0000,0000,0000,,Thầy không nên đánh giá người khác qua vẻ bề ngoài.
Dialogue: 0,0:22:51.53,0:22:53.86,640x480,,0000,0000,0000,,Không chỉ có thế thôi đâu.
Dialogue: 0,0:22:54.00,0:22:57.84,640x480,,0000,0000,0000,,Em sẽ đút dương vật vào sâu hơn nữa.
Dialogue: 0,0:22:58.81,0:23:01.54,640x480,,0000,0000,0000,,Ồ, cô ướt rồi sao, sensei?
Dialogue: 0,0:23:01.74,0:23:03.84,640x480,,0000,0000,0000,,Quả nhiên cơ thể cô rất thành thật.
Dialogue: 0,0:23:12.15,0:23:15.88,640x480,,0000,0000,0000,,Sensei, em có thể ra bên trong cô không?
Dialogue: 0,0:23:17.02,0:23:18.86,640x480,,0000,0000,0000,,Em ra đây!
Dialogue: 0,0:23:19.46,0:23:21.45,640x480,,0000,0000,0000,,Không! Không được làm thế!
Dialogue: 0,0:23:21.86,0:23:26.60,640x480,,0000,0000,0000,,Nếu bắn thứ đó vào bên trong... Tôi sẽ... Tôi sẽ có thai mất!
Dialogue: 0,0:23:27.67,0:23:29.53,640x480,,0000,0000,0000,,Em ra đây! Em ra đây!
Dialogue: 0,0:23:29.67,0:23:34.73,640x480,,0000,0000,0000,,Em sẽ ra thật nhiều tinh dịch trong tử cung cô xem như là quà!
Dialogue: 0,0:23:34.84,0:23:36.47,640x480,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:23:40.11,0:23:43.17,640x480,,0000,0000,0000,,Chuyện gì đã xảy ra với mình?
Dialogue: 0,0:23:47.42,0:23:49.51,640x480,,0000,0000,0000,,Bây giờ đến lượt tôi.
Dialogue: 0,0:23:53.16,0:23:55.92,640x480,,0000,0000,0000,,Cảm giác này là gì đây?
Dialogue: 0,0:23:56.50,0:23:59.13,640x480,,0000,0000,0000,,Cô sẽ không bao giờ quên mùi vị của đàn ông.
Dialogue: 0,0:23:59.83,0:24:03.13,640x480,,0000,0000,0000,,Quả nhiên chỗ này là khít nhất.
Dialogue: 0,0:24:04.74,0:24:06.54,640x480,,0000,0000,0000,,Một cảm giác thật lạ...
Dialogue: 0,0:24:06.67,0:24:11.08,640x480,,0000,0000,0000,,Tạo sao tôi lại muốn nhiều hơn... nhiều hơn nữa?
Dialogue: 0,0:24:16.95,0:24:19.98,640x480,,0000,0000,0000,,Sensei, âm hộ cô đang co giật.
Dialogue: 0,0:24:20.59,0:24:24.02,640x480,,0000,0000,0000,,Cô sẽ bị học sinh của mình xem thường mất!
Dialogue: 0,0:24:29.90,0:24:31.89,640x480,,0000,0000,0000,,Phải như vậy chứ.
Dialogue: 0,0:24:32.13,0:24:34.12,640x480,,0000,0000,0000,,Đây là hình phạt vì đã giăng bẩy học sinh.
Dialogue: 0,0:24:34.27,0:24:36.67,640x480,,0000,0000,0000,,Tại sao cô không thử làm nô lệ xem sao?
Dialogue: 0,0:24:46.28,0:24:47.68,640x480,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:24:49.92,0:24:51.68,640x480,,0000,0000,0000,,Hãy ra cùng nhau!
Dialogue: 0,0:25:27.65,0:25:29.62,640x480,,0000,0000,0000,,Sensei! Sensei!
Dialogue: 0,0:25:29.96,0:25:32.29,640x480,,0000,0000,0000,,Được mà, cứ ra trong miệng của cô.
Dialogue: 0,0:25:33.53,0:25:38.02,640x480,,0000,0000,0000,,Em thật đáng yêu, em có thể ra thật nhiều trên mặt cô.
Dialogue: 0,0:25:39.33,0:25:42.23,640x480,,0000,0000,0000,,Hãy đút nó vào đây.
Dialogue: 0,0:26:01.66,0:26:04.22,640x480,,0000,0000,0000,,Sensei, em...
Dialogue: 0,0:26:09.06,0:26:10.12,640x480,,0000,0000,0000,,Yuma-chan.
Dialogue: 0,0:26:10.23,0:26:11.60,640x480,,0000,0000,0000,,Sensei.
Dialogue: 0,0:26:27.15,0:26:30.71,640x480,,0000,0000,0000,,Phải vậy đó, tuyệt lắm, Yuma-chan...
Dialogue: 0,0:26:40.33,0:26:43.79,640x480,,0000,0000,0000,,Sensei, sướng lắm!
Dialogue: 0,0:26:50.00,0:26:53.27,640x480,,0000,0000,0000,,Kyoko, bây giờ mọi thứ đã chấm dứt.
Dialogue: 0,0:27:03.85,0:27:05.11,640x480,,0000,0000,0000,,Em ấy...
Dialogue: 0,0:27:07.12,0:27:08.61,640x480,,0000,0000,0000,,Sensei!
Dialogue: 0,0:27:10.32,0:27:12.88,640x480,,0000,0000,0000,,Ơ, thầy xong rồi ư?
Dialogue: 0,0:27:13.03,0:27:14.62,640x480,,0000,0000,0000,,Ừm, thầy xong rồi.
Dialogue: 0,0:27:14.80,0:27:18.70,640x480,,0000,0000,0000,,Dù thầy đã có được bằng chứng nhưng có lẽ thầy không cần dùng đến nó.
Dialogue: 0,0:27:19.10,0:27:22.87,640x480,,0000,0000,0000,,Có lẽ Kyoko cũng là nạn nhân.
Dialogue: 0,0:27:23.10,0:27:25.77,640x480,,0000,0000,0000,,Thầy sẽ nói chuyện với cô ấy vào ngày mai.
Dialogue: 0,0:27:26.34,0:27:31.87,640x480,,0000,0000,0000,,Thầy sẽ cố gắng hết sức dù cho cuộc chiến ngày càng khó khăn hơn nữa.
Dialogue: 0,0:27:32.01,0:27:35.64,640x480,,0000,0000,0000,,Và rồi thầy sẽ đưa ngồi trường này trở lại như xưa!
Dialogue: 0,0:27:40.15,0:27:42.21,640x480,,0000,0000,0000,,Đó mới là Jo-sensei của em chứ!
