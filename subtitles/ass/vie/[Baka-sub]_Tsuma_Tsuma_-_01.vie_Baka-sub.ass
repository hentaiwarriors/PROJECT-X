Dialogue: 0,0:00:08.33,0:00:12.82,Default,,0000,0000,0000,,Project thuộc về website : Baka-sub.net
Dialogue: 0,0:00:30.63,0:00:31.82,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:00:34.30,0:00:36.49,Default,,0000,0000,0000,,Koutaro... dậy đi nào...
Dialogue: 0,0:00:47.08,0:00:48.51,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:00:49.38,0:00:50.94,Default,,0000,0000,0000,,Mẹ...!?
Dialogue: 0,0:00:57.45,0:00:59.15,Default,,0000,0000,0000,,Tha lỗi cho mẹ, Koutaro...
Dialogue: 0,0:01:00.02,0:01:03.01,Default,,0000,0000,0000,,Mẹ đã cảm thấy rất cô đơn sau khi cha con qua đời.
Dialogue: 0,0:01:03.46,0:01:04.12,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:01:05.06,0:01:07.86,Default,,0000,0000,0000,,Con thì lại giống hệt cha con...
Dialogue: 0,0:01:08.56,0:01:11.76,Default,,0000,0000,0000,,Dù hai chúng ta ko phải ruột thịt, nhưng mẹ vẫn thấy thật tội lỗi...
Dialogue: 0,0:01:11.90,0:01:13.42,Default,,0000,0000,0000,,Ko sao đâu mà, Mẹ...
Dialogue: 0,0:01:14.00,0:01:18.06,Default,,0000,0000,0000,,Từ khi trông thấy mẹ lần đầu, con đã thầm yêu mẹ... vì thế
Dialogue: 0,0:01:18.97,0:01:20.64,Default,,0000,0000,0000,,Cám ơn con, Koutaro.
Dialogue: 0,0:01:21.44,0:01:24.07,Default,,0000,0000,0000,,Con là người duy nhất mà mẹ tin tưởng lúc này...
Dialogue: 0,0:01:24.41,0:01:28.08,Default,,0000,0000,0000,,Mẹ... Con sẽ ko bao giờ...
Dialogue: 0,0:01:28.15,0:01:29.88,Default,,0000,0000,0000,,Ko sao đâu, làm đi con...
Dialogue: 0,0:01:31.12,0:01:33.49,Default,,0000,0000,0000,,Mẹ, ngực mẹ thật lớn và mềm...
Dialogue: 0,0:01:34.09,0:01:38.72,Default,,0000,0000,0000,,Cứ thoải mái bú chúng đi... mẹ là mẹ của con mà.
Dialogue: 0,0:01:38.79,0:01:39.99,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:01:46.20,0:01:49.40,Default,,0000,0000,0000,,Koutaro... mạnh nữa vào...
Dialogue: 0,0:01:54.88,0:01:56.43,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:01:59.98,0:02:04.11,Default,,0000,0000,0000,,Koutaro, hương vị ngực mẹ thế nào?
Dialogue: 0,0:02:06.66,0:02:07.59,Default,,0000,0000,0000,,Ngon lắm ạ...
Dialogue: 0,0:02:09.99,0:02:12.93,Default,,0000,0000,0000,,Làm ơn... nhét nó vào trong mẹ đi... Koutaro...!
Dialogue: 0,0:02:13.46,0:02:15.05,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:02:25.81,0:02:29.24,Default,,0000,0000,0000,,Mình đang làm tình với mẹ... mơ ước bấy lâu nay của mình đã thành sự thật.
Dialogue: 0,0:02:29.98,0:02:32.61,Default,,0000,0000,0000,,Koutaro, tuyệt lắm... rất tuyệt!
Dialogue: 0,0:02:32.68,0:02:36.34,Default,,0000,0000,0000,,Con làm còn tốt hơn cả cha con!
Dialogue: 0,0:02:36.82,0:02:40.35,Default,,0000,0000,0000,,Thật bất công khi cha lại có được mẹ trước con.
Dialogue: 0,0:02:40.42,0:02:42.19,Default,,0000,0000,0000,,Đừng nói như vậy...
Dialogue: 0,0:02:52.17,0:02:54.73,Default,,0000,0000,0000,,Mẹ, con sắp ra rồi... bên trong mẹ!
Dialogue: 0,0:02:56.64,0:03:01.34,Default,,0000,0000,0000,,Mẹ cũng sắp ra rồi... ra... cùng nhau nhé!
Dialogue: 0,0:03:02.54,0:03:04.74,Default,,0000,0000,0000,,Mẹ!!!
Dialogue: 0,0:03:04.81,0:03:07.75,Default,,0000,0000,0000,,Koutaro!!!
Dialogue: 0,0:03:14.32,0:03:16.05,Default,,0000,0000,0000,,Anh ơi!!!
Dialogue: 0,0:03:28.70,0:03:30.40,Default,,0000,0000,0000,,Anh, đến giờ mở cửa rồi!
Dialogue: 0,0:03:30.47,0:03:31.44,Default,,0000,0000,0000,,Mẹ đâu rồi!?
Dialogue: 0,0:03:31.51,0:03:33.03,Default,,0000,0000,0000,,Mẹ á?
Dialogue: 0,0:03:33.11,0:03:35.60,Default,,0000,0000,0000,,Huh, mình đã làm điều đó trong mơ đúng ko nhỉ?
Dialogue: 0,0:03:35.68,0:03:37.24,Default,,0000,0000,0000,,Làm cái gì hả anh?
Dialogue: 0,0:03:38.08,0:03:39.77,Default,,0000,0000,0000,,Đúng rồi, anh còn phải mở cửa hàng !
Dialogue: 0,0:03:50.56,0:03:52.55,Default,,0000,0000,0000,,Wow, này, dừng lại ngay!
Dialogue: 0,0:03:52.63,0:03:54.93,Default,,0000,0000,0000,,Anh là đồ biến thái!
Dialogue: 0,0:03:55.00,0:03:56.99,Default,,0000,0000,0000,,Em tự ý vào phòng anh nên mới thế, còn kêu ca cái gì nữa hả!
Dialogue: 0,0:03:57.07,0:03:59.66,Default,,0000,0000,0000,,Chết đi, tên biến thái!
Dialogue: 0,0:04:00.60,0:04:01.43,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:04:03.84,0:04:05.00,Default,,0000,0000,0000,,Cửa hiệu Sakura
Dialogue: 0,0:04:05.54,0:04:07.60,Default,,0000,0000,0000,,Cửa hiệu cha để lại.
Dialogue: 0,0:04:07.68,0:04:09.08,Default,,0000,0000,0000,,Đó là một báu vật vô giá với chúng tôi.
Dialogue: 0,0:04:09.14,0:04:14.41,Default,,0000,0000,0000,,Tôi sẽ làm việc chăm chỉ để bảo vệ cửa hiệu này, cả Mẹ và Nagisa nữa!
Dialogue: 0,0:04:17.09,0:04:17.71,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:04:21.72,0:04:24.82,Default,,0000,0000,0000,,Việc kinh doanh vẫn tốt chứ, ngài quản lý?
Dialogue: 0,0:04:25.76,0:04:27.16,Default,,0000,0000,0000,,Anh muốn gì!?
Dialogue: 0,0:04:28.83,0:04:32.56,Default,,0000,0000,0000,,Cậu có nghĩ rằng nơi này xứng với cái giá một triệu dollar?
Dialogue: 0,0:04:32.63,0:04:36.50,Default,,0000,0000,0000,,Với cửa hiệu nhỏ bé này, tôi nghĩ chắc ko được đâu.
Dialogue: 0,0:04:36.57,0:04:38.37,Default,,0000,0000,0000,,Tốt hơn cả là cậu nên nhượng lại nó.
Dialogue: 0,0:04:39.34,0:04:42.21,Default,,0000,0000,0000,,Tôi sẽ ko bao giờ nghe lời khuyên từ một kẻ phản bội như anh!
Dialogue: 0,0:04:43.68,0:04:44.65,Default,,0000,0000,0000,,Taiyo.
Dialogue: 0,0:04:46.28,0:04:47.25,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:04:47.52,0:04:49.54,Default,,0000,0000,0000,,Ái chà, ngạc nhiên đây... chào Minato.
Dialogue: 0,0:04:49.85,0:04:53.75,Default,,0000,0000,0000,,Hãy gọi cô ấy lạ Mẹ! Cô ấy đã phải\Ncoi sóc rất nhiều thứ khi cha qua đời!
Dialogue: 0,0:04:54.46,0:04:56.39,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:04:56.49,0:05:02.23,Default,,0000,0000,0000,,Chúng tôi sẽ cố gắng hết sức mình để duy trì cửa hiệu\Nvì thế làm ơn đừng quấy rầy chúng tôi nữa!
Dialogue: 0,0:05:02.70,0:05:05.83,Default,,0000,0000,0000,,Tình cảm gia đình đẹp đẽ thật đấy.
Dialogue: 0,0:05:05.90,0:05:09.34,Default,,0000,0000,0000,,Cả hai đã thực sự coi nhau là người thân rồi hả?
Dialogue: 0,0:05:09.44,0:05:10.96,Default,,0000,0000,0000,,Anh im đi!
Dialogue: 0,0:05:11.04,0:05:14.30,Default,,0000,0000,0000,,Sao cũng được, hãy cố mà duy trì nơi này.
Dialogue: 0,0:05:14.38,0:05:21.21,Default,,0000,0000,0000,,Nhân tiện đây, kể cả khi chúng tôi\Ngiành được quyền sở hữu nơi này, chúng tôi cũng sẽ ko đối xử tệ với hai người đâu.
Dialogue: 0,0:05:21.28,0:05:26.95,Default,,0000,0000,0000,,Ngài Kaneshiro là người rất rộng lượng!
Dialogue: 0,0:05:32.66,0:05:34.13,Default,,0000,0000,0000,,Nào ngài quản lí.
Dialogue: 0,0:05:34.20,0:05:36.46,Default,,0000,0000,0000,,Mẹ con ta cùng cố gắng nhé.
Dialogue: 0,0:05:37.00,0:05:39.30,Default,,0000,0000,0000,,Mẹ nói phải, con sẽ cố hết sức mình.
Dialogue: 0,0:05:40.64,0:05:42.19,Default,,0000,0000,0000,,Xin chào!
Dialogue: 0,0:05:43.34,0:05:45.60,Default,,0000,0000,0000,,Chúng tôi có rất nhiều thực phẩm tươi ngon, thưa cô!
Dialogue: 0,0:05:54.58,0:05:58.07,Default,,0000,0000,0000,,Wife with Wife Phần 1\N- Siêu thị dành cho các bà nội trợ -
Dialogue: 0,0:06:11.77,0:06:14.23,Default,,0000,0000,0000,,Cải bắp... 248 yen
Dialogue: 0,0:06:14.94,0:06:17.06,Default,,0000,0000,0000,,Rau diếp... 198 yen.
Dialogue: 0,0:06:21.91,0:06:23.04,Default,,0000,0000,0000,,Cảm ơn quí khách đã ghé thăm!
Dialogue: 0,0:06:29.25,0:06:31.48,Default,,0000,0000,0000,,Quản lí thì ko được nghĩ đến thứ này!
Dialogue: 0,0:06:32.12,0:06:33.28,Default,,0000,0000,0000,,Cái này giá bao nhiêu?
Dialogue: 0,0:06:33.56,0:06:34.52,Default,,0000,0000,0000,,V, Vâng!
Dialogue: 0,0:06:44.97,0:06:46.30,Default,,0000,0000,0000,,Này, bao nhiêu vậy anh?
Dialogue: 0,0:06:48.77,0:06:51.40,Default,,0000,0000,0000,,Cái này giá 350 yen hai quả
Dialogue: 0,0:06:52.57,0:06:54.10,Default,,0000,0000,0000,,Xin lỗi cô.
Dialogue: 0,0:06:59.78,0:07:01.61,Default,,0000,0000,0000,,Ngài quản lý?
Dialogue: 0,0:07:04.45,0:07:05.58,Default,,0000,0000,0000,,Có chuyện gì thế?
Dialogue: 0,0:07:06.42,0:07:08.36,Default,,0000,0000,0000,,Này Touko, cô thế nào rồi?
Dialogue: 0,0:07:09.89,0:07:11.09,Default,,0000,0000,0000,,Tôi ổn.
Dialogue: 0,0:07:12.09,0:07:13.92,Default,,0000,0000,0000,,Tôi đang kiểm tra vài chuyện.
Dialogue: 0,0:07:14.70,0:07:16.86,Default,,0000,0000,0000,,Cái này ghi sai giá rồi.
Dialogue: 0,0:07:16.93,0:07:19.59,Default,,0000,0000,0000,,Thiếu mất một số 0 này.
Dialogue: 0,0:07:20.17,0:07:23.33,Default,,0000,0000,0000,,Vậy à? Xin lỗi anh.
Dialogue: 0,0:07:24.34,0:07:25.70,Default,,0000,0000,0000,,Cái này cũng thế.
Dialogue: 0,0:07:26.17,0:07:28.17,Default,,0000,0000,0000,,Xin lỗi, tôi vô ý quá...
Dialogue: 0,0:07:33.15,0:07:35.31,Default,,0000,0000,0000,,Touko, chúng ta lên văn phòng nói chuyện chút nào.
Dialogue: 0,0:07:35.38,0:07:37.04,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:07:39.55,0:07:41.82,Default,,0000,0000,0000,,Này, Koutaro, đợi đã...!
Dialogue: 0,0:07:45.26,0:07:51.46,Default,,0000,0000,0000,,Koutaro... con trai tôi đang sốt nên\Ntôi đã phải chăm sóc nó đến khuya và...
Dialogue: 0,0:07:52.17,0:07:55.00,Default,,0000,0000,0000,,Touko, Hãy gọi tôi là ngài quản lí khi chúng ta ở cửa hàng.
Dialogue: 0,0:07:55.84,0:07:57.57,Default,,0000,0000,0000,,Tôi... xin lỗi...
Dialogue: 0,0:08:00.48,0:08:02.47,Default,,0000,0000,0000,,Cậu làm gì vậy?
Dialogue: 0,0:08:03.51,0:08:05.67,Default,,0000,0000,0000,,Ko, chúng ta đang trong giờ làm việc mà...
Dialogue: 0,0:08:06.51,0:08:10.14,Default,,0000,0000,0000,,Nếu Kou bị sốt, thì chắc cậu bé \Nko bú được nhiều.
Dialogue: 0,0:08:11.59,0:08:14.18,Default,,0000,0000,0000,,Mmm, mùi sữa thật đậm đặc.
Dialogue: 0,0:08:14.26,0:08:15.55,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:08:15.62,0:08:17.15,Default,,0000,0000,0000,,Ngài quản lý mới đúng...
Dialogue: 0,0:08:27.00,0:08:28.99,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:08:37.61,0:08:41.48,Default,,0000,0000,0000,,Touko, cái này là hình phạt dành cho sai sót chị mắc phải.
Dialogue: 0,0:08:42.95,0:08:44.15,Default,,0000,0000,0000,,Ngài quản lý...
Dialogue: 0,0:08:44.22,0:08:46.78,Default,,0000,0000,0000,,Tôi sẽ dạy cho chị sự tuân thủ kỉ luật.
Dialogue: 0,0:08:47.69,0:08:49.49,Default,,0000,0000,0000,,Trò vui mới chỉ bắt đầu thôi mà.
Dialogue: 0,0:08:50.66,0:08:52.59,Default,,0000,0000,0000,,Ngài quản lý, ko phải chỗ này!
Dialogue: 0,0:08:52.66,0:08:53.79,Default,,0000,0000,0000,,Đó chính là lí do tại sao...
Dialogue: 0,0:08:53.86,0:08:56.92,Default,,0000,0000,0000,,Chuyện này lại phấn khích đến như vậy...
Dialogue: 0,0:08:57.00,0:08:58.02,Default,,0000,0000,0000,,Tôi vào đây...
Dialogue: 0,0:09:01.94,0:09:03.63,Default,,0000,0000,0000,,Đây là nơi đã sinh ra một bé con ư...
Dialogue: 0,0:09:03.71,0:09:06.37,Default,,0000,0000,0000,,Nó thật mềm mại và cứ xoắn lấy cậu bé của tôi.
Dialogue: 0,0:09:07.24,0:09:10.04,Default,,0000,0000,0000,,Đừng nói những điều như vậy...
Dialogue: 0,0:09:22.56,0:09:25.05,Default,,0000,0000,0000,,Nó thật ướt, nhưng cũng rất tuyệt!
Dialogue: 0,0:09:33.74,0:09:37.19,Default,,0000,0000,0000,,Ko biết của Mẹ thì thế nào nhỉ...?
Dialogue: 0,0:10:02.33,0:10:03.56,Default,,0000,0000,0000,,M, Mẹ đấy à!?
Dialogue: 0,0:10:05.47,0:10:08.33,Default,,0000,0000,0000,,Để mẹ kì lưng cho con, Koutaro.
Dialogue: 0,0:10:09.37,0:10:11.84,Default,,0000,0000,0000,,Con nghĩ đó ko phải là... ý kiến hay...
Dialogue: 0,0:10:13.04,0:10:14.17,Default,,0000,0000,0000,,Vì sao chứ?
Dialogue: 0,0:10:14.74,0:10:15.94,Default,,0000,0000,0000,,Nếu Nagisa thấy chuyện này...
Dialogue: 0,0:10:16.98,0:10:20.85,Default,,0000,0000,0000,,Huh? Nagisa đi hát karaoke với bạn nó rồi.
Dialogue: 0,0:10:20.92,0:10:23.65,Default,,0000,0000,0000,,Vả lại, chúng ta là một gia đình mà.
Dialogue: 0,0:10:23.72,0:10:25.21,Default,,0000,0000,0000,,Đúng như vậy, nhưng...
Dialogue: 0,0:10:28.62,0:10:31.68,Default,,0000,0000,0000,,Phải chăng vì chúng ta ko phải ruột thịt nên con ko muốn như vậy...?
Dialogue: 0,0:10:33.29,0:10:35.16,Default,,0000,0000,0000,,Ko, ý con ko phải thế...
Dialogue: 0,0:10:36.30,0:10:38.86,Default,,0000,0000,0000,,Vâng... mẹ kì lưng cho con đi...
Dialogue: 0,0:10:40.47,0:10:41.56,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:10:47.31,0:10:50.44,Default,,0000,0000,0000,,Koutaro, lưng con giống hệt cha con.
Dialogue: 0,0:10:51.61,0:10:53.05,Default,,0000,0000,0000,,Thật nam tính.
Dialogue: 0,0:10:57.62,0:10:58.35,Default,,0000,0000,0000,,Ummm...
Dialogue: 0,0:10:58.99,0:11:01.22,Default,,0000,0000,0000,,Đừng cử động.
Dialogue: 0,0:11:05.83,0:11:06.62,Default,,0000,0000,0000,,Mẹ này...
Dialogue: 0,0:11:06.69,0:11:07.35,Default,,0000,0000,0000,,Hmm?
Dialogue: 0,0:11:07.53,0:11:09.83,Default,,0000,0000,0000,,Tại sao mẹ cưới cha con?
Dialogue: 0,0:11:10.16,0:11:11.53,Default,,0000,0000,0000,,Vì sao á...
Dialogue: 0,0:11:12.27,0:11:14.53,Default,,0000,0000,0000,,Chẳng phải việc cha mang con theo đã làm phiền hai người sao...
Dialogue: 0,0:11:15.07,0:11:17.30,Default,,0000,0000,0000,,Thì mẹ cũng mang theo Nagisa còn gì.
Dialogue: 0,0:11:19.71,0:11:23.64,Default,,0000,0000,0000,,Sau này mẹ sẽ cho con biết một vài chuyện...
Dialogue: 0,0:11:24.45,0:11:29.64,Default,,0000,0000,0000,,Về lí do tại sao mà mẹ và cha\Nlại yêu nhau thắm thiết như vậy.
Dialogue: 0,0:11:29.72,0:11:31.62,Default,,0000,0000,0000,,Điều này là sự thật đấy.
Dialogue: 0,0:11:32.99,0:11:34.15,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:11:37.56,0:11:38.46,Default,,0000,0000,0000,,Mẹ!?
Dialogue: 0,0:11:39.16,0:11:43.26,Default,,0000,0000,0000,,Mẹ rất yêu cha của con...
Dialogue: 0,0:11:43.33,0:11:46.16,Default,,0000,0000,0000,,Choichiro, sao anh lại bỏ em lại một mình thế này...
Dialogue: 0,0:11:46.67,0:11:47.79,Default,,0000,0000,0000,,Tại sao anh...
Dialogue: 0,0:11:48.34,0:11:49.63,Default,,0000,0000,0000,,Mẹ, hãy bình tĩnh nào...
Dialogue: 0,0:11:50.77,0:11:54.54,Default,,0000,0000,0000,,Choichiro... anh thật... tàn nhẫn với em...
Dialogue: 0,0:11:55.61,0:12:01.21,Default,,0000,0000,0000,,Mẹ đã ôm tôi rất chặt và đã thổ lộ tất cả cảm xúc mà từ lâu mẹ vẫn giấu kín...
Dialogue: 0,0:12:02.92,0:12:07.68,Default,,0000,0000,0000,,Chính điều này đã khiến tôi bắt đầu nhìn mẹ theo một cách khác...
Dialogue: 0,0:12:17.93,0:12:19.42,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:12:19.93,0:12:23.67,Default,,0000,0000,0000,,Và sau chuyện này, tôi đã gặp Touko...
Dialogue: 0,0:12:28.64,0:12:30.24,Default,,0000,0000,0000,,Con trai mẹ ngoan lắm.
Dialogue: 0,0:12:30.31,0:12:32.30,Default,,0000,0000,0000,,Thưa cô, cô có cần tôi giữ hộ chỗ đồ kia ko.
Dialogue: 0,0:12:33.05,0:12:35.11,Default,,0000,0000,0000,,Xin chào, ngài quản lý.
Dialogue: 0,0:12:36.15,0:12:39.78,Default,,0000,0000,0000,,Nếu cô ko vội, hãy để tôi mang chúng tới nhà cô.
Dialogue: 0,0:12:41.76,0:12:45.32,Default,,0000,0000,0000,,Xin chào, tôi là người của cửa hiệu Sakura, tôi mang đồ tới đây.
Dialogue: 0,0:12:45.96,0:12:48.36,Default,,0000,0000,0000,,Xin lỗi, tôi đang tắm.
Dialogue: 0,0:12:48.43,0:12:51.40,Default,,0000,0000,0000,,Anh có thể để nó lên chiếc bàn trong bếp giúp tôi được ko?
Dialogue: 0,0:12:52.13,0:12:53.96,Default,,0000,0000,0000,,Vâng thưa cô, ko vấn đề gì.
Dialogue: 0,0:12:55.77,0:12:56.93,Default,,0000,0000,0000,,Xong rồi đây...
Dialogue: 0,0:12:57.41,0:12:58.30,Default,,0000,0000,0000,,Cảm ơn anh.
Dialogue: 0,0:12:58.77,0:12:59.83,Default,,0000,0000,0000,,Ấy...!?
Dialogue: 0,0:13:00.37,0:13:02.57,Default,,0000,0000,0000,,Đừng ngại, thư giãn chút nào.
Dialogue: 0,0:13:02.64,0:13:05.58,Default,,0000,0000,0000,,Con trai tôi thì đã ngủ mà tôi thì đang rất buồn chán.
Dialogue: 0,0:13:05.65,0:13:08.74,Default,,0000,0000,0000,,Làm thứ gì đó vui vẻ với tôi nhé.
Dialogue: 0,0:13:16.49,0:13:17.79,Default,,0000,0000,0000,,Anh ko phiền chứ?
Dialogue: 0,0:13:19.89,0:13:22.26,Default,,0000,0000,0000,,Thưa cô, thế này thật ko phải...
Dialogue: 0,0:13:22.73,0:13:24.99,Default,,0000,0000,0000,,Cậu bé của anh đang nói phải kìa, ko đúng thế sao?
Dialogue: 0,0:13:25.60,0:13:27.80,Default,,0000,0000,0000,,Thưa cô, còn cậu bé...
Dialogue: 0,0:13:27.87,0:13:30.17,Default,,0000,0000,0000,,Kou thường ngủ say đến mức một cơn địa chấn cũng ko đánh thức nổi nó.
Dialogue: 0,0:13:30.24,0:13:31.30,Default,,0000,0000,0000,,Kou?
Dialogue: 0,0:13:31.47,0:13:33.27,Default,,0000,0000,0000,,Tên nó là Koutaro.
Dialogue: 0,0:13:36.94,0:13:38.67,Default,,0000,0000,0000,,Thưa cô, còn chồng cô thì sao...
Dialogue: 0,0:13:41.55,0:13:44.45,Default,,0000,0000,0000,,Anh ấy luôn bận rộn với công việc.
Dialogue: 0,0:13:44.52,0:13:49.15,Default,,0000,0000,0000,,Tôi ko được như ý trong chuyện đó cho lắm nên tôi cần anh giúp...
Dialogue: 0,0:13:50.39,0:13:52.38,Default,,0000,0000,0000,,Tôi muốn làm chuyện gì đó thật phấn khích...
Dialogue: 0,0:13:52.93,0:13:54.76,Default,,0000,0000,0000,,Thưa cô...
Dialogue: 0,0:13:54.83,0:13:57.63,Default,,0000,0000,0000,,Tên tôi là Touko...
Dialogue: 0,0:14:00.77,0:14:05.36,Default,,0000,0000,0000,,Cái của cậu đang nằm gọn giữ hai bầu vú của tôi.
Dialogue: 0,0:14:08.78,0:14:11.34,Default,,0000,0000,0000,,Thật là mềm mại và tuyệt diệu...!
Dialogue: 0,0:14:12.18,0:14:14.81,Default,,0000,0000,0000,,Cái của chồng tôi khá nhỏ nên chuyện đó chán chết đi được.
Dialogue: 0,0:14:17.85,0:14:22.85,Default,,0000,0000,0000,,Nó to thật, rất là to! Tôi thích nó rồi đấy!
Dialogue: 0,0:14:23.89,0:14:26.38,Default,,0000,0000,0000,,Vâng thưa cô, tuyệt lắm, thật là tuyệt!
Dialogue: 0,0:14:27.03,0:14:29.02,Default,,0000,0000,0000,,Cứ gọi tôi là Touko.
Dialogue: 0,0:14:29.46,0:14:30.23,Default,,0000,0000,0000,,Touko!
Dialogue: 0,0:14:35.90,0:14:39.84,Default,,0000,0000,0000,,Anh ra rồi sao? Nhanh thế!
Dialogue: 0,0:14:40.74,0:14:43.40,Default,,0000,0000,0000,,Vâng... đây là lần đầu của tôi...
Dialogue: 0,0:14:44.78,0:14:47.77,Default,,0000,0000,0000,,Ôi tuyệt nhỉ, vậy chắc hẳn tôi là người phụ nữ đầu tiên của cậu rồi!
Dialogue: 0,0:14:53.09,0:14:54.92,Default,,0000,0000,0000,,Tôi chưa bao giờ chơi với thứ to như vậy...
Dialogue: 0,0:14:55.69,0:14:57.42,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:15:05.83,0:15:07.13,Default,,0000,0000,0000,,Cái này gọi là làm tình sao...
Dialogue: 0,0:15:07.64,0:15:10.47,Default,,0000,0000,0000,,Đúng rồi, làm tình...
Dialogue: 0,0:15:12.11,0:15:14.17,Default,,0000,0000,0000,,Sướng hơn là thủ dâm chứ hả?
Dialogue: 0,0:15:14.41,0:15:15.64,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:15:19.38,0:15:23.01,Default,,0000,0000,0000,,Touko đã chọn tôi để giúp chị ta ấy thư giản.
Dialogue: 0,0:15:24.02,0:15:27.61,Default,,0000,0000,0000,,Tôi đã chấp nhận qua lại với chị ấy để tránh nghĩ tới Mẹ.
Dialogue: 0,0:15:28.49,0:15:32.65,Default,,0000,0000,0000,,Đây quả thật là một cách tuyệt vời để kìm nén cảm xúc thực của tôi...
Dialogue: 0,0:15:46.44,0:15:50.14,Default,,0000,0000,0000,,Tôi đã nhận Touko làm nhân viên bán hàng bán thời gian để phục vụ nhu cầu của mình.
Dialogue: 0,0:15:51.18,0:15:54.08,Default,,0000,0000,0000,,Tôi đã nghĩ điều đó sẽ khiến tôi ko bận tâm về Mẹ nữa.
Dialogue: 0,0:15:55.98,0:15:59.01,Default,,0000,0000,0000,,Touko, ra vào bên trong sướng thật đấy.
Dialogue: 0,0:15:59.09,0:16:01.35,Default,,0000,0000,0000,,Cô phải giữ gìn cẩn thận đấy nhé.
Dialogue: 0,0:16:02.36,0:16:03.58,Default,,0000,0000,0000,,Tôi có giống Ác quỉ ko đây...?
Dialogue: 0,0:16:04.63,0:16:06.89,Default,,0000,0000,0000,,Vâng, ko cần phải lo việc đó đâu...
Dialogue: 0,0:16:07.90,0:16:12.56,Default,,0000,0000,0000,,Tôi hi vọng Kou sẽ sớm bình phục.
Dialogue: 0,0:16:13.87,0:16:16.74,Default,,0000,0000,0000,,Cám ơn Koutaro, cậu thật tốt.
Dialogue: 0,0:16:16.80,0:16:18.97,Default,,0000,0000,0000,,Đó là lí do tại sao tôi thích cậu.
Dialogue: 0,0:16:19.04,0:16:20.77,Default,,0000,0000,0000,,Chị lại đùa rồi.
Dialogue: 0,0:16:21.81,0:16:24.71,Default,,0000,0000,0000,,Và nhớ gọi tôi là ngài quản lý khi chúng ta ta còn trong giờ làm việc đấy nhé.
Dialogue: 0,0:16:42.30,0:16:44.26,Default,,0000,0000,0000,,Anh nhìn cái gì thế!?
Dialogue: 0,0:16:44.33,0:16:45.49,Default,,0000,0000,0000,,Em im ngay đi!
Dialogue: 0,0:16:48.40,0:16:51.20,Default,,0000,0000,0000,,Mẹ ơi, Anh trai ngày càng biến thái!
Dialogue: 0,0:16:51.27,0:16:53.64,Default,,0000,0000,0000,,Anh ấy bây giờ có tật thích nhìn trộm con tắm.
Dialogue: 0,0:16:53.71,0:16:57.41,Default,,0000,0000,0000,,Im ngay, có thằng nào thấy hứng thú với cái thân hình phát triển quá lứa của em chứ!
Dialogue: 0,0:16:57.48,0:16:59.57,Default,,0000,0000,0000,,Anh vừa nói cái gì!?
Dialogue: 0,0:17:00.31,0:17:03.37,Default,,0000,0000,0000,,Mẹ có nghe thấy ko, anh ấy dám nói con thế cơ đấy?
Dialogue: 0,0:17:03.75,0:17:06.45,Default,,0000,0000,0000,,Đừng lo, cậu ấy đang xấu hổ đấy.
Dialogue: 0,0:17:06.52,0:17:07.42,Default,,0000,0000,0000,,Xấu hổ á?
Dialogue: 0,0:17:07.49,0:17:10.82,Default,,0000,0000,0000,,Đúng thế, bởi vì con gái mẹ càng lớn càng xinh đẹp.
Dialogue: 0,0:17:11.33,0:17:12.85,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:17:15.60,0:17:18.83,Default,,0000,0000,0000,,Koutaro, xin lỗi con...
Dialogue: 0,0:17:23.97,0:17:25.50,Default,,0000,0000,0000,,Chết tiệt...
Dialogue: 0,0:17:33.55,0:17:35.81,Default,,0000,0000,0000,,Oh, Touko. Kou thế nào rồi?
Dialogue: 0,0:17:36.72,0:17:41.48,Default,,0000,0000,0000,,Cảm ơn cậu, Kou khỏe hẳn rồi và từ giờ nó sẽ ko bú sữa mẹ nữa.
Dialogue: 0,0:17:42.39,0:17:44.65,Default,,0000,0000,0000,,Hai đầu ti tôi ê ẩm mấy hôm nay rồi.
Dialogue: 0,0:17:46.06,0:17:49.03,Default,,0000,0000,0000,,Này, chị nhớ để dành cho tôi một ít đấy nhé.
Dialogue: 0,0:17:49.10,0:17:51.26,Default,,0000,0000,0000,,Tôi nghĩ mình sẽ dùng chúng sau.
Dialogue: 0,0:17:51.70,0:17:54.33,Default,,0000,0000,0000,,Vâng, dĩ nhiên rồi thưa ngài quản lý.
Dialogue: 0,0:18:07.48,0:18:08.97,Default,,0000,0000,0000,,Thưa cô, làm ơn đợi đã.
Dialogue: 0,0:18:12.09,0:18:13.05,Default,,0000,0000,0000,,Thưa cô!
Dialogue: 0,0:18:13.12,0:18:15.15,Default,,0000,0000,0000,,Chuyện gì thế!?
Dialogue: 0,0:18:15.59,0:18:18.08,Default,,0000,0000,0000,,Cô có thể làm ơn cho tôi kiểm tra túi xách được ko?
Dialogue: 0,0:18:18.16,0:18:19.15,Default,,0000,0000,0000,,Vì sao chứ?
Dialogue: 0,0:18:19.26,0:18:20.49,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:18:20.56,0:18:22.05,Default,,0000,0000,0000,,Có chuyện gì sao?
Dialogue: 0,0:18:22.13,0:18:24.12,Default,,0000,0000,0000,,Vâng là thế này...
Dialogue: 0,0:18:25.03,0:18:26.93,Default,,0000,0000,0000,,Mình đúng là chưa nhìn thấy gì rõ ràng hết.
Dialogue: 0,0:18:27.00,0:18:28.90,Default,,0000,0000,0000,,Mình phải quyết định nhanh chuyện này.
Dialogue: 0,0:18:29.47,0:18:30.73,Default,,0000,0000,0000,,Cái gì vậy!?
Dialogue: 0,0:18:33.11,0:18:34.94,Default,,0000,0000,0000,,Đừng có xử sự vô lí như thế.
Dialogue: 0,0:18:35.01,0:18:36.44,Default,,0000,0000,0000,,Xin đừng to tiếng như vậy, thưa cô...
Dialogue: 0,0:18:37.14,0:18:38.61,Default,,0000,0000,0000,,Ngài quản lý, có vấn đề gì à?
Dialogue: 0,0:18:38.98,0:18:40.38,Default,,0000,0000,0000,,À, ko có gì.
Dialogue: 0,0:18:40.45,0:18:42.28,Default,,0000,0000,0000,,Cô làm ơn theo tôi.
Dialogue: 0,0:18:42.55,0:18:44.18,Default,,0000,0000,0000,,Bây giờ, đi nào.
Dialogue: 0,0:18:50.42,0:18:52.02,Default,,0000,0000,0000,,Nếu tôi sai, tôi sẽ tạ lỗi với cô.
Dialogue: 0,0:18:52.09,0:18:55.15,Default,,0000,0000,0000,,nhưng để chắc chắn, tôi sẽ phải kiểm tra.
Dialogue: 0,0:18:58.67,0:19:01.29,Default,,0000,0000,0000,,Um... Thưa cô? Thưa cô?
Dialogue: 0,0:19:02.87,0:19:07.17,Default,,0000,0000,0000,,Được thôi, nếu tôi la lên bây giờ, thì có vấn đề gì ko nhỉ?
Dialogue: 0,0:19:07.91,0:19:11.17,Default,,0000,0000,0000,,Thôi ngay đi! Cô đang đe dọa tôi đấy hả!?
Dialogue: 0,0:19:13.65,0:19:15.27,Default,,0000,0000,0000,,Thưa cô, làm ơn!
Dialogue: 0,0:19:22.89,0:19:24.12,Default,,0000,0000,0000,,Tôi rất xin lỗi!
Dialogue: 0,0:19:24.86,0:19:26.26,Default,,0000,0000,0000,,Tôi phải đi đây!
Dialogue: 0,0:19:27.23,0:19:28.59,Default,,0000,0000,0000,,Xin lỗi cô!
Dialogue: 0,0:19:30.83,0:19:33.70,Default,,0000,0000,0000,,Ê khoan đã, mình quên mất mục đích chính khi đưa cô ta ra đây rồi!
Dialogue: 0,0:19:36.40,0:19:39.80,Default,,0000,0000,0000,,Ôi giời, vớ vẩn có khi bị đuổi học vì chuyện này cũng nên?
Dialogue: 0,0:19:39.87,0:19:41.81,Default,,0000,0000,0000,,Mình ngớ ngẩn thật...
Dialogue: 0,0:19:42.24,0:19:43.94,Default,,0000,0000,0000,,Làm ơn dừng lại, buông tôi ra!
Dialogue: 0,0:19:47.62,0:19:48.64,Default,,0000,0000,0000,,Dừng lại ngay!
Dialogue: 0,0:19:48.72,0:19:51.21,Default,,0000,0000,0000,,Nếu cô chịu từ bỏ cửa hiệu đó, tôi sẽ ko động đến cô.
Dialogue: 0,0:19:51.29,0:19:55.35,Default,,0000,0000,0000,,Koutaro sẽ được bố trí một công việc khác còn cô sẽ trở thành vợ của ngài Kaneshiro.
Dialogue: 0,0:19:55.42,0:19:57.52,Default,,0000,0000,0000,,Và Nagisa sẽ trở thành vợ hai của tôi.
Dialogue: 0,0:19:57.59,0:19:59.15,Default,,0000,0000,0000,,Sao anh dám...!
Dialogue: 0,0:19:59.23,0:20:04.56,Default,,0000,0000,0000,,Cô sẽ ko bao giờ phải lo lắng về chuyện tiền bạc khi trở thành vợ của ngài Kaneshiro!
Dialogue: 0,0:20:05.30,0:20:06.63,Default,,0000,0000,0000,,Anh trai, chấm dứt trò này ngay!
Dialogue: 0,0:20:14.81,0:20:16.61,Default,,0000,0000,0000,,Một hiệp sĩ ở thời đại này ư?
Dialogue: 0,0:20:16.68,0:20:20.31,Default,,0000,0000,0000,,Sao cũng được, hãy suy nghĩ thật kĩ về điều tôi vừa nói.
Dialogue: 0,0:20:22.82,0:20:24.44,Default,,0000,0000,0000,,Mẹ ko sao chứ?
Dialogue: 0,0:20:25.62,0:20:27.81,Default,,0000,0000,0000,,Mẹ xin lỗi, Koutaro...
Dialogue: 0,0:20:27.89,0:20:29.75,Default,,0000,0000,0000,,Mẹ thật vô dụng...
Dialogue: 0,0:20:30.39,0:20:32.48,Default,,0000,0000,0000,,Ko hề, mẹ đã rất dũng cảm.
Dialogue: 0,0:20:33.96,0:20:35.36,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:20:45.47,0:20:48.77,Default,,0000,0000,0000,,Mình sẽ bảo vệ bằng được Mẹ, Nagisa và cả cửa hiệu này...
Dialogue: 0,0:21:01.59,0:21:04.96,Default,,0000,0000,0000,,Mình sẽ chăm lo cho mẹ, cửa hiệu... và Nagisa.
Dialogue: 0,0:21:08.93,0:21:14.53,Default,,0000,0000,0000,,Ko dễ dàng chút nào với hai người phụ nữ và một cửa hiệu...
Dialogue: 0,0:21:14.60,0:21:17.80,Default,,0000,0000,0000,,Đặc biệt là bây giờ khi cô em bé bỏng Nagisa\Nđã sắp trưởng thành.
Dialogue: 0,0:21:21.84,0:21:22.97,Default,,0000,0000,0000,,Và cả chuyện đó nữa...
Dialogue: 0,0:21:31.99,0:21:33.48,Default,,0000,0000,0000,,Mà mình căng thẳng vì cái gì kia chứ?
Dialogue: 0,0:21:33.55,0:21:37.75,Default,,0000,0000,0000,,Có lẽ cũng nên tự thưởng cho mình thứ gì đó vì đã làm việc chăm chỉ.
Dialogue: 0,0:21:39.43,0:21:40.76,Default,,0000,0000,0000,,Đúng rồi, mình sẽ...
Dialogue: 0,0:21:42.13,0:21:43.46,Default,,0000,0000,0000,,Làm điều đó...
Dialogue: 0,0:21:54.31,0:21:55.47,Default,,0000,0000,0000,,Nagisa...
Dialogue: 0,0:21:59.38,0:22:01.54,Default,,0000,0000,0000,,Nagisa, anh xin lỗi...
Dialogue: 0,0:22:12.49,0:22:14.62,Default,,0000,0000,0000,,Nagisa vẫn còn trinh
Dialogue: 0,0:22:16.63,0:22:19.03,Default,,0000,0000,0000,,Tuy nhiên, Mẹ lại ko thuộc về mình...
Dialogue: 0,0:22:43.92,0:22:46.55,Default,,0000,0000,0000,,Mẹ, con hi vọng mẹ đang đợi con.
Dialogue: 0,0:22:48.33,0:22:50.49,Default,,0000,0000,0000,,Đây là nơi cha và mẹ...
Dialogue: 0,0:23:01.78,0:23:01.88,Default,,0000,0000,0000,,A, ai đó!?
Dialogue: 0,0:23:01.88,0:23:02.84,Default,,0000,0000,0000,,A, ai đó!?
Dialogue: 0,0:23:04.18,0:23:05.70,Default,,0000,0000,0000,,Ko... Koutaro!?
Dialogue: 0,0:23:09.65,0:23:13.18,Default,,0000,0000,0000,,Mẹ, đừng to tiếng như thế. Nagisa đang ở tầng trên.
Dialogue: 0,0:23:13.69,0:23:15.75,Default,,0000,0000,0000,,Con làm gì thế, Koutaro!?
Dialogue: 0,0:23:15.82,0:23:17.12,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:23:17.83,0:23:18.92,Default,,0000,0000,0000,,Con sẽ ko dừng lại.
Dialogue: 0,0:23:18.99,0:23:22.33,Default,,0000,0000,0000,,Con đã quyết tâm phải chiếm cho bằng được mẹ.
Dialogue: 0,0:23:25.90,0:23:28.20,Default,,0000,0000,0000,,Mẹ cũng có trách nhiệm trong chuyện này, Mẹ.
Dialogue: 0,0:23:33.54,0:23:36.48,Default,,0000,0000,0000,,Mẹ, hương vị này thật thân thương.
Dialogue: 0,0:23:36.54,0:23:39.41,Default,,0000,0000,0000,,Con ghen tị với cha vì cha đã sở hữu chúng hằng đêm.
Dialogue: 0,0:23:39.48,0:23:41.35,Default,,0000,0000,0000,,Đừng nói những điều như vậy!
Dialogue: 0,0:23:44.42,0:23:45.58,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:23:50.59,0:23:52.86,Default,,0000,0000,0000,,Thế này ko được...!
Dialogue: 0,0:23:53.19,0:23:54.49,Default,,0000,0000,0000,,Nghe con nói đã, Mẹ!
Dialogue: 0,0:23:58.90,0:24:00.16,Default,,0000,0000,0000,,Mẹ, làm ơn đi mà!
Dialogue: 0,0:24:00.23,0:24:02.53,Default,,0000,0000,0000,,Làm ơn hãy hiểu cảm giác của con!
Dialogue: 0,0:24:03.04,0:24:04.13,Default,,0000,0000,0000,,Koutaro.
Dialogue: 0,0:24:17.18,0:24:20.71,Default,,0000,0000,0000,,Mẹ, ngực của mẹ thật hoàn hảo cho chuyện này.
Dialogue: 0,0:24:31.83,0:24:34.60,Default,,0000,0000,0000,,Nó có to hơn của cha con ko?
Dialogue: 0,0:24:41.78,0:24:43.54,Default,,0000,0000,0000,,Koutaro, đừng!
Dialogue: 0,0:24:44.01,0:24:46.14,Default,,0000,0000,0000,,Nhưng mẹ đã làm chuyện này với con trong mơ mà.
Dialogue: 0,0:24:46.41,0:24:47.54,Default,,0000,0000,0000,,Mơ ư?
Dialogue: 0,0:24:53.52,0:24:54.82,Default,,0000,0000,0000,,Con vào đây, Mẹ.
Dialogue: 0,0:24:55.16,0:24:56.45,Default,,0000,0000,0000,,Ko!!
Dialogue: 0,0:25:04.06,0:25:06.09,Default,,0000,0000,0000,,So với cha thì thế nào?
Dialogue: 0,0:25:06.60,0:25:10.13,Default,,0000,0000,0000,,Làm ơn, đừng nói như vậy Koutaro.
Dialogue: 0,0:25:11.41,0:25:12.34,Default,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:25:12.71,0:25:16.01,Default,,0000,0000,0000,,Làm ơn... đừng nhắc tới cha con vào lúc này...
Dialogue: 0,0:25:16.41,0:25:19.17,Default,,0000,0000,0000,,Ý mẹ là, mẹ đã chấp nhận con?
Dialogue: 0,0:25:26.19,0:25:27.74,Default,,0000,0000,0000,,Mẹ, con ra nhé.
Dialogue: 0,0:25:50.58,0:25:53.17,Default,,0000,0000,0000,,Mẹ, con sắp ra rồi!
Dialogue: 0,0:25:53.65,0:25:55.95,Default,,0000,0000,0000,,Mẹ cũng thế, Koutaro!
Dialogue: 0,0:25:56.22,0:25:57.58,Default,,0000,0000,0000,,Con có thể... được ko...
Dialogue: 0,0:25:57.65,0:25:59.12,Default,,0000,0000,0000,,Sao thế...?
Dialogue: 0,0:26:00.05,0:26:03.08,Default,,0000,0000,0000,,Ra... ra vào bên trong ấy, Mẹ!
Dialogue: 0,0:26:05.03,0:26:09.19,Default,,0000,0000,0000,,Con muốn ghi nhớ ngày hôm nay mãi mãi!
Dialogue: 0,0:26:09.86,0:26:11.76,Default,,0000,0000,0000,,Uhm... được thôi!
Dialogue: 0,0:26:11.83,0:26:14.93,Default,,0000,0000,0000,,Hãy ra vào bên trong mẹ đi!
Dialogue: 0,0:26:15.00,0:26:17.53,Default,,0000,0000,0000,,Vậy con ra đây, Mẹ!!!
Dialogue: 0,0:26:20.17,0:26:21.64,Default,,0000,0000,0000,,Koutaro!!
Dialogue: 0,0:26:24.21,0:26:26.44,Default,,0000,0000,0000,,Mẹ, con yêu mẹ.
Dialogue: 0,0:26:26.61,0:26:28.24,Default,,0000,0000,0000,,Koutaro...
Dialogue: 0,0:26:29.92,0:26:31.68,Default,,0000,0000,0000,,Đây là mơ hay thực vậy?
Dialogue: 0,0:26:31.75,0:26:35.78,Default,,0000,0000,0000,,Phải chăng mình mới chính là người đã phản bội cha?
Dialogue: 0,0:26:35.86,0:26:38.29,Default,,0000,0000,0000,,Nhưng mình ko thể quay trở lại...
Dialogue: 0,0:26:38.36,0:26:42.35,Default,,0000,0000,0000,,Mình sẽ yêu thương Mẹ suốt đời...
Dialogue: 0,0:26:43.03,0:26:44.66,Default,,0000,0000,0000,,Và cả Nagisa nữa...
Dialogue: 0,0:26:51.81,0:26:54.87,Default,,0000,0000,0000,,The End
