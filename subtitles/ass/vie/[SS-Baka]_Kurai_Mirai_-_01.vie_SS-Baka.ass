Dialogue: 0,0:00:25.92,0:00:27.82,Default,,0000,0000,0000,,Nhanh dậy đi nào!
Dialogue: 0,0:00:28.62,0:00:30.35,Default,,0000,0000,0000,,Thức ăn nguội hết bây giờ.
Dialogue: 0,0:00:31.29,0:00:32.78,Default,,0000,0000,0000,,Chút nữa đi...
Dialogue: 0,0:00:35.26,0:00:38.10,Default,,0000,0000,0000,,Chà, lúc nào cũng thế này...
Dialogue: 0,0:00:44.74,0:00:46.54,Default,,0000,0000,0000,,Hạ gậy xuống kìa!
Dialogue: 0,0:00:47.08,0:00:48.30,Default,,0000,0000,0000,,Không...!
Dialogue: 0,0:01:22.58,0:01:25.10,Default,,0000,0000,0000,,Mình, cảm giác này... đừng!
Dialogue: 0,0:01:27.22,0:01:30.91,Default,,0000,0000,0000,,Không, đồng phục của mình nhăn mất...!
Dialogue: 0,0:01:31.62,0:01:32.99,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:01:33.06,0:01:35.15,Default,,0000,0000,0000,,Vậy mình sẽ cởi nó ra cho cậu.
Dialogue: 0,0:01:38.76,0:01:41.75,Default,,0000,0000,0000,,Đừng... Cậu sẽ làm chúng ta bị muộn đấy.
Dialogue: 0,0:01:41.83,0:01:43.16,Default,,0000,0000,0000,,Đừng lo, đừng lo!
Dialogue: 0,0:01:44.50,0:01:49.63,Default,,0000,0000,0000,,Nhưng, nếu chúng ta đều muộn...\Nhọ sẽ phát hiện ra chúng ta cùng nhau mất!
Dialogue: 0,0:02:18.70,0:02:20.79,Default,,0000,0000,0000,,Mình ra rồi, ra rồi!
Dialogue: 0,0:02:30.11,0:02:31.88,Default,,0000,0000,0000,,Vậy... sướng không nào?
Dialogue: 0,0:03:06.41,0:03:12.39,Credits,,0000,0000,0000,,{\an7\blur3\fs25}Translator: TonBerry \N Editor: Frozen \N QC: Dark Knight
Dialogue: 0,0:03:06.41,0:03:12.39,Title Name,,0000,0000,0000,,{\fs45\blur3\pos(330,346)}Dark Future
Dialogue: 0,0:03:06.41,0:03:12.39,Title Name,,0000,0000,0000,,{\an8\fs45\b1\blur5\pos(336,91)}SS-Baka present
Dialogue: 0,0:03:09.45,0:03:12.39,Title,,0000,0000,0000,,{\pos(320,455)\fs50\blur2}Hồi 1 Quá khứ tàn khốc
Dialogue: 0,0:03:18.79,0:03:19.89,Default,,0000,0000,0000,,Chào buổi sáng!
Dialogue: 0,0:03:23.20,0:03:24.69,Default,,0000,0000,0000,,Này, biết tin gì chưa?
Dialogue: 0,0:03:25.57,0:03:28.00,Default,,0000,0000,0000,,Tin trường mình có ma ấy.
Dialogue: 0,0:03:28.10,0:03:32.13,Default,,0000,0000,0000,,Ừ, mình cũng đang tìm hiểu về nó đây.
Dialogue: 0,0:03:32.71,0:03:34.23,Default,,0000,0000,0000,,Sao rồi?
Dialogue: 0,0:03:34.94,0:03:37.57,Default,,0000,0000,0000,,Xem ra chưa có báo cáo gì về nó cả.
Dialogue: 0,0:03:38.25,0:03:39.37,Default,,0000,0000,0000,,Vậy sao...
Dialogue: 0,0:03:41.02,0:03:43.75,Default,,0000,0000,0000,,Mình nghe nói có người thấy nó chỗ gần nhà kho trước cổng bể bơi.
Dialogue: 0,0:03:43.85,0:03:47.22,Default,,0000,0000,0000,,Còn cái cậu học sinh mới nhìn thấy nó thì hôm nay lại mất tiêu.
Dialogue: 0,0:03:47.36,0:03:50.55,Default,,0000,0000,0000,,Nhưng mình không muốn làm phiền cậu ta ở nhà.
Dialogue: 0,0:03:53.50,0:03:58.19,Default,,0000,0000,0000,,Mình không nghĩ lại được nghe những lời nhận xét như thế này\Ntừ nữ nhà báo hàng đầu của chúng ta, Kaede.
Dialogue: 0,0:03:58.90,0:04:00.30,Default,,0000,0000,0000,,Umm, trưởng ban...
Dialogue: 0,0:04:01.64,0:04:06.10,Default,,0000,0000,0000,,Để thành nhà báo giỏi cậu phải có mặt tại\Nhiện trường và nói chuyện với tất cả mọi người ở đó.
Dialogue: 0,0:04:06.21,0:04:10.01,Default,,0000,0000,0000,,Sao lại đờ người ra thế kia, Kaede?
Dialogue: 0,0:04:10.61,0:04:12.44,Default,,0000,0000,0000,,Cậu ta thấy quần lót của cậu rồi.
Dialogue: 0,0:04:14.48,0:04:16.42,Default,,0000,0000,0000,,Trắng, một màu trắng tinh khiết.
Dialogue: 0,0:04:16.95,0:04:20.95,Default,,0000,0000,0000,,Những nhà báo như chúng ta phải tự mình tìm hiểu tất cả..!?
Dialogue: 0,0:04:22.19,0:04:23.71,Default,,0000,0000,0000,,...và phải liều mình cho.....
Dialogue: 0,0:04:23.79,0:04:25.02,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:04:32.83,0:04:37.64,Default,,0000,0000,0000,,Chủ đề của chúng ta lần này là, ''Bóng ma\Nđiều đang gây xôn xao khắp trường''.
Dialogue: 0,0:04:38.37,0:04:41.27,Default,,0000,0000,0000,,Hinoki sẽ kiểm tra trang web\Ncủa sinh viên để tìm kiếm thông tin.
Dialogue: 0,0:04:41.84,0:04:45.71,Default,,0000,0000,0000,,Kaede và tôi sẽ phỏng vấn những người có liên quan nếu có.
Dialogue: 0,0:04:46.18,0:04:47.71,Default,,0000,0000,0000,,Ừm, gì cũng được...
Dialogue: 0,0:04:47.85,0:04:51.05,Default,,0000,0000,0000,,Được rồi. Vậy chúng ta sẽ bắt đầu ngay sau khi tan học!
Dialogue: 0,0:05:02.16,0:05:03.96,Default,,0000,0000,0000,,Vẫn đông đúc như thường lệ...
Dialogue: 0,0:05:04.20,0:05:06.96,Default,,0000,0000,0000,,Mình quên béng việc chuẩn bị bữa trưa hôm nay rồi...
Dialogue: 0,0:05:07.07,0:05:10.56,Default,,0000,0000,0000,,Đều là tại Ma-kun cả...
Dialogue: 0,0:05:11.41,0:05:12.77,Default,,0000,0000,0000,,Shizutani Shion-san.
Dialogue: 0,0:05:14.54,0:05:16.27,Default,,0000,0000,0000,,À, xin lỗi nhé.
Dialogue: 0,0:05:19.45,0:05:20.97,Default,,0000,0000,0000,,Mình không sao.
Dialogue: 0,0:05:25.02,0:05:28.39,Default,,0000,0000,0000,,Mình đi vội nên quên không kịp mặc áo lót rồi...
Dialogue: 0,0:05:31.26,0:05:32.49,Default,,0000,0000,0000,,Có chuyện gì sao?
Dialogue: 0,0:05:33.26,0:05:39.00,Default,,0000,0000,0000,,Shion-san, cậu đang có điều gì đó\N bận tâm hay cần giữ kín phải không?
Dialogue: 0,0:05:40.27,0:05:41.33,Default,,0000,0000,0000,,Sự thật là...
Dialogue: 0,0:05:48.01,0:05:49.27,Default,,0000,0000,0000,,Nhìn đây.
Dialogue: 0,0:05:50.71,0:05:56.34,Default,,0000,0000,0000,,Chúng mình muốn những người đang gặp căng thẳng\Ntham gia để họ có thể thư thả.
Dialogue: 0,0:05:57.72,0:06:00.59,Default,,0000,0000,0000,,Liệu pháp điều trị bằng hương liệu?
Dialogue: 0,0:06:01.32,0:06:07.39,Default,,0000,0000,0000,,Chúng mình ở câu lạc bộ thư giãn\N và đảm bảo rằng sự căng thẳng của các bạn sẽ được giải tỏa bằng những dược liệu đặc trị của chúng mình.
Dialogue: 0,0:06:08.40,0:06:12.49,Default,,0000,0000,0000,,Chúng mình đang thiếu thời gian và\Nhỗ trợ, vì vậy làm ơn giữ bí mật giùm mình.
Dialogue: 0,0:06:12.63,0:06:14.76,Default,,0000,0000,0000,,Này, Shion?
Dialogue: 0,0:06:16.00,0:06:18.47,Default,,0000,0000,0000,,Ơ, cậu đến rồi à... để mình khao cậu...!?
Dialogue: 0,0:06:19.67,0:06:20.97,Default,,0000,0000,0000,,Chayamachi?
Dialogue: 0,0:06:21.48,0:06:22.70,Default,,0000,0000,0000,,Cậu đang làm gì thế?
Dialogue: 0,0:06:23.61,0:06:24.67,Default,,0000,0000,0000,,Không có gì.
Dialogue: 0,0:06:25.28,0:06:29.44,Default,,0000,0000,0000,,Shion và tớ đang nói chuyện về\Nvấn đề tâm linh trên thế giới, nhỉ?
Dialogue: 0,0:06:30.02,0:06:31.92,Default,,0000,0000,0000,,Tâm linh?
Dialogue: 0,0:06:32.05,0:06:33.49,Default,,0000,0000,0000,,Cái quái gì thế?
Dialogue: 0,0:06:33.99,0:06:37.08,Default,,0000,0000,0000,,Đó là những cái mà cái đầu cậu\Nkhông bao giờ hiểu nổi đâu.
Dialogue: 0,0:06:43.03,0:06:47.20,Default,,0000,0000,0000,,Hừ, cậu ta nghĩ cậu ta có thể làm mọi thứ chỉ vì\N sắp thành lớp trưởng nhiệm kỳ sau sao?
Dialogue: 0,0:06:47.97,0:06:50.87,Default,,0000,0000,0000,,Sao 2 cậu cứ như chó với mèo thế?
Dialogue: 0,0:06:51.07,0:06:55.13,Default,,0000,0000,0000,,Trời biết, chắc kiếp trước mình với nó là kẻ thù cũng nên.
Dialogue: 0,0:07:18.33,0:07:21.27,Notes Box,,0000,0000,0000,,{\p1\fad(500,0)}m 0 0 l 640 0 l 640 68 l 0 68 l 0 0{\p0}
Dialogue: 0,0:07:18.33,0:07:21.27,Notes,,0000,0000,0000,,{\fad(500,0)\fs35\pos(320,3)}Note: Câu lạc bộ thư giãn. Tổ chức hàng năm.\NLiệu pháp thảo dược, chào đón các bạn.
Dialogue: 0,0:07:24.41,0:07:26.67,Default,,0000,0000,0000,,Chắc là đây rồi.
Dialogue: 0,0:07:30.38,0:07:32.54,Default,,0000,0000,0000,,Chào mừng đến với câu lạc bộ thư giãn.
Dialogue: 0,0:07:43.96,0:07:45.52,Default,,0000,0000,0000,,Thật đẹp ghê!
Dialogue: 0,0:07:45.69,0:07:47.63,Default,,0000,0000,0000,,Vâng, mời cậu ngồi.
Dialogue: 0,0:07:50.63,0:07:52.40,Default,,0000,0000,0000,,Mùi hương thật dễ chịu.
Dialogue: 0,0:08:00.78,0:08:04.87,Default,,0000,0000,0000,,Một tân nữ sinh dễ thương, Mitsu Nadesico.
Dialogue: 0,0:08:05.71,0:08:07.48,Default,,0000,0000,0000,,Cô ta trông "thơm" thật đấy.
Dialogue: 0,0:08:08.72,0:08:10.81,Default,,0000,0000,0000,,Vậy, chúng ta bắt đầu nào.
Dialogue: 0,0:08:15.79,0:08:18.12,Default,,0000,0000,0000,,Ơ kìa, Sumire-senpai biến đâu rồi?
Dialogue: 0,0:08:18.59,0:08:23.19,Default,,0000,0000,0000,,Sumire-senpai đang đưa mấy cậu lính mới đi kiểm tra.
Dialogue: 0,0:08:24.70,0:08:27.36,Default,,0000,0000,0000,,Vậy ra chị ta đi đào tạo lính mới.
Dialogue: 0,0:08:27.47,0:08:29.73,Default,,0000,0000,0000,,Với kỹ thuật điêu luyện của chị ấy...
Dialogue: 0,0:08:33.34,0:08:36.78,Default,,0000,0000,0000,,Này Ma... Trưởng ban đâu?
Dialogue: 0,0:08:37.48,0:08:42.75,Default,,0000,0000,0000,,Tới lấy camera rồi biến mất tiêu rồi.
Dialogue: 0,0:08:43.05,0:08:44.81,Default,,0000,0000,0000,,Rồi vừa chạy vừa hô, ''Scoop, scoop!''
Dialogue: 0,0:08:49.06,0:08:51.42,Default,,0000,0000,0000,,Xì, cậu ta lại nhanh hơn mình 1 bước rồi!
Dialogue: 0,0:08:51.56,0:08:54.39,Default,,0000,0000,0000,,Xem nào... thế này... và thế này... được rồi!
Dialogue: 0,0:08:55.56,0:08:57.16,Default,,0000,0000,0000,,Scoop, scoop!
Dialogue: 0,0:08:57.73,0:09:00.43,Default,,0000,0000,0000,,Hai cậu ấy giống hệt nhau.
Dialogue: 0,0:09:07.14,0:09:12.08,Default,,0000,0000,0000,,Có tin đồn bóng ma xuất hiện trong clb bơi lội cũ.
Dialogue: 0,0:09:12.91,0:09:16.85,Default,,0000,0000,0000,,Câu lạc bộ bơi đã ngưng hoạt động 15\Nnăm trước vì 1 sự cố...?
Dialogue: 0,0:09:17.92,0:09:24.03,Default,,0000,0000,0000,,Không biết sự cố gì đây?
Dialogue: 0,0:09:24.03,0:09:27.05,Default,,0000,0000,0000,,Cả cái bể bơi tĩnh lặng thế này đến là rợn...
Dialogue: 0,0:09:28.90,0:09:31.09,Default,,0000,0000,0000,,Chắc ở chỗ này rồi...
Dialogue: 0,0:09:32.03,0:09:34.66,Default,,0000,0000,0000,,Sao vậy, các cậu sợ cái gì hả?
Dialogue: 0,0:09:38.97,0:09:40.63,Default,,0000,0000,0000,,Nhanh lên, đến đây nào.
Dialogue: 0,0:09:50.89,0:09:52.44,Default,,0000,0000,0000,,Các cậu còn chờ gì hả?
Dialogue: 0,0:09:53.96,0:09:57.79,Default,,0000,0000,0000,,Nhưng Sumire-senpai, em nghe nói chỗ này có ma...
Dialogue: 0,0:09:58.49,0:10:07.89,Default,,0000,0000,0000,,Nhát đệt, đã là 1 thành viên của câu lạc bộ thư giãn thì các cậu phải giải quyết được vấn đề này chứ.
Dialogue: 0,0:10:08.90,0:10:11.13,Default,,0000,0000,0000,,Chúng ta có mặt ở đây là để làm điều đó mà!
Dialogue: 0,0:10:11.91,0:10:14.43,Default,,0000,0000,0000,,Nhưng... một bài kiểm tra lòng dũng cảm thì?
Dialogue: 0,0:10:16.21,0:10:17.54,Default,,0000,0000,0000,,Nó là một phần trong đó.
Dialogue: 0,0:10:28.62,0:10:30.02,Default,,0000,0000,0000,,Chị không sao chứ ạ, Sumire-senpai?
Dialogue: 0,0:10:32.83,0:10:35.73,Default,,0000,0000,0000,,Tôi bị thương rồi, phiền cậu?
Dialogue: 0,0:10:37.20,0:10:38.79,Default,,0000,0000,0000,,V-Vâng!
Dialogue: 0,0:10:40.24,0:10:42.33,Default,,0000,0000,0000,,Xem ra nó còn... ở bên trong.
Dialogue: 0,0:10:44.87,0:10:46.74,Default,,0000,0000,0000,,Cậu có thấy... vết thương không?
Dialogue: 0,0:10:57.95,0:11:01.44,Default,,0000,0000,0000,,Sao vậy... nó đang co giật này?
Dialogue: 0,0:11:09.70,0:11:11.76,Default,,0000,0000,0000,,Có vẻ đang có chuyện lớn xảy ra đây.
Dialogue: 0,0:11:12.83,0:11:15.13,Default,,0000,0000,0000,,Đệt mie, ở đây éo thấy được gì.
Dialogue: 0,0:11:15.90,0:11:16.89,Default,,0000,0000,0000,,Được rồi...
Dialogue: 0,0:11:22.24,0:11:23.47,Default,,0000,0000,0000,,Vẫn còn cương lắm...
Dialogue: 0,0:11:25.15,0:11:26.64,Default,,0000,0000,0000,,Hư ghê cơ...
Dialogue: 0,0:11:31.35,0:11:33.45,Default,,0000,0000,0000,,Su, Sumire-senpai!
Dialogue: 0,0:11:41.36,0:11:43.26,Default,,0000,0000,0000,,Còn 2 cậu?
Dialogue: 0,0:11:43.33,0:11:44.16,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:11:51.51,0:11:52.60,Default,,0000,0000,0000,,E-Em xin lỗi!
Dialogue: 0,0:11:52.67,0:11:53.80,Default,,0000,0000,0000,,Em xin lỗi!
Dialogue: 0,0:11:54.24,0:11:56.23,Default,,0000,0000,0000,,Tôi không thể ra ngoài thế này, lau sạch nó đi.
Dialogue: 0,0:11:56.44,0:11:58.11,Default,,0000,0000,0000,,V-Vâng...
Dialogue: 0,0:11:58.28,0:12:00.44,Default,,0000,0000,0000,,Um... cái khăn...
Dialogue: 0,0:12:00.55,0:12:02.21,Default,,0000,0000,0000,,Các cậu đang nghĩ gì thế hả?!
Dialogue: 0,0:12:02.28,0:12:04.31,Default,,0000,0000,0000,,Dùng mồm mà lau sạch nó đi.
Dialogue: 0,0:12:15.26,0:12:17.99,Default,,0000,0000,0000,,Các cậu không nhanh là bẩn cả ra áo tôi đấy.
Dialogue: 0,0:12:37.19,0:12:38.02,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:12:38.65,0:12:41.35,Default,,0000,0000,0000,,Ai cho phép các cậu liếm môi và ngực tôi hả?!
Dialogue: 0,0:12:42.49,0:12:44.05,Default,,0000,0000,0000,,E-Em xin lỗi!
Dialogue: 0,0:12:44.86,0:12:49.26,Default,,0000,0000,0000,,Được thôi, để trừng phạt, hai cậu\Ncởi hết quần và đồ lót ra cho tôi.
Dialogue: 0,0:12:49.40,0:12:51.73,Default,,0000,0000,0000,,Sau đó liếm vết thương này.
Dialogue: 0,0:12:56.80,0:12:59.90,Default,,0000,0000,0000,,Cứ đến kỳ là tôi lại bấn lên.
Dialogue: 0,0:13:18.46,0:13:21.22,Default,,0000,0000,0000,,Tôi sẽ khiến hai cậu thấy sướng...
Dialogue: 0,0:13:54.93,0:13:56.09,Default,,0000,0000,0000,,Đừng có dừng lại!
Dialogue: 0,0:14:27.23,0:14:30.03,Default,,0000,0000,0000,,Tôi đã hiểu rõ vấn đề của cô.
Dialogue: 0,0:14:36.27,0:14:40.43,Default,,0000,0000,0000,,Nói chuyện thôi cũng khiến em cảm thấy dễ chịu hơn rồi.
Dialogue: 0,0:14:40.81,0:14:41.97,Default,,0000,0000,0000,,Đúng không nào?
Dialogue: 0,0:14:42.08,0:14:46.98,Default,,0000,0000,0000,,Giờ hãy thả lỏng và uống loại\N hỗn hợp trà thảo dược đặc biệt này nào.
Dialogue: 0,0:14:47.48,0:14:48.61,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:15:04.80,0:15:07.20,Default,,0000,0000,0000,,Các cậu phục hồi nhanh đấy...
Dialogue: 0,0:15:07.37,0:15:08.56,Default,,0000,0000,0000,,Đến đây nào.
Dialogue: 0,0:15:12.27,0:15:13.76,Default,,0000,0000,0000,,Có ma!!!
Dialogue: 0,0:15:26.39,0:15:29.75,Default,,0000,0000,0000,,Cậu định... giải quyết việc này thế nào đây?
Dialogue: 0,0:15:37.93,0:15:39.99,Default,,0000,0000,0000,,Chuyện gì thế này...?
Dialogue: 0,0:15:42.90,0:15:46.20,Default,,0000,0000,0000,,Bọn anh chỉ đang cố giải quyết vấn đề của cô em...
Dialogue: 0,0:15:48.64,0:15:50.47,Default,,0000,0000,0000,,D-Dừng lại đi!
Dialogue: 0,0:15:51.01,0:15:52.74,Default,,0000,0000,0000,,Em sẽ thấy tốt hơn nhanh thôi...
Dialogue: 0,0:15:52.81,0:15:54.30,Default,,0000,0000,0000,,D-Dừng lại đi!
Dialogue: 0,0:15:57.92,0:16:05.69,Default,,0000,0000,0000,,Nếu cô em còn chống cự, anh sẽ gửi cái\N"bí mật" này của cô em lên internet.
Dialogue: 0,0:16:08.06,0:16:09.53,Default,,0000,0000,0000,,Việc đó...
Dialogue: 0,0:16:14.70,0:16:16.23,Default,,0000,0000,0000,,Thư giãn chút đi nào...
Dialogue: 0,0:16:23.84,0:16:25.78,Default,,0000,0000,0000,,Không, không phải chỗ đó...
Dialogue: 0,0:16:31.25,0:16:32.81,Default,,0000,0000,0000,,Đau quá...!
Dialogue: 0,0:16:49.37,0:16:50.84,Default,,0000,0000,0000,,Mùi trinh nữ...
Dialogue: 0,0:16:52.17,0:16:53.11,Default,,0000,0000,0000,,Xấu hổ lắm..
Dialogue: 0,0:17:04.25,0:17:05.24,Default,,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:17:05.45,0:17:07.65,Default,,0000,0000,0000,,Muốn sờ thứ đó của anh sao?
Dialogue: 0,0:17:11.06,0:17:12.25,Default,,0000,0000,0000,,Sờ nó đi nào!
Dialogue: 0,0:17:13.56,0:17:16.05,Default,,0000,0000,0000,,Lỉều thuốc sẽ có tác dụng nhanh thôi.
Dialogue: 0,0:17:19.73,0:17:20.67,Default,,0000,0000,0000,,Thật nóng...
Dialogue: 0,0:17:39.69,0:17:41.21,Default,,0000,0000,0000,,Đừng làm tôi đau.
Dialogue: 0,0:17:41.29,0:17:42.45,Default,,0000,0000,0000,,Câm mồm!
Dialogue: 0,0:17:49.90,0:17:51.49,Default,,0000,0000,0000,,Đừng làm tôi đau...!
Dialogue: 0,0:18:04.05,0:18:06.48,Default,,0000,0000,0000,,Thế nào, sướng không nào cô em?!
Dialogue: 0,0:18:20.93,0:18:23.12,Default,,0000,0000,0000,,Chúng ta chỉ mới bắt đầu thôi mà.
Dialogue: 0,0:18:29.47,0:18:30.44,Default,,0000,0000,0000,,Đ-Đau quá...
Dialogue: 0,0:18:45.22,0:18:46.98,Default,,0000,0000,0000,,Đau quá, dừng lại đi!
Dialogue: 0,0:18:48.22,0:18:49.42,Default,,0000,0000,0000,,Hãy dừng lại đi...
Dialogue: 0,0:18:53.79,0:18:55.02,Default,,0000,0000,0000,,Im mồm đi!
Dialogue: 0,0:18:55.13,0:18:57.15,Default,,0000,0000,0000,,Sao cô không cầu xin bọn ta đi nào.
Dialogue: 0,0:18:57.26,0:19:03.03,Default,,0000,0000,0000,,Như là...''Làm ơn đừng cho nó vào\Nâm hộ đang chảy máu bốc mùi của tôi".
Dialogue: 0,0:19:04.17,0:19:06.16,Default,,0000,0000,0000,,Tôi không... thể nói vậy....
Dialogue: 0,0:19:07.54,0:19:08.87,Default,,0000,0000,0000,,Hãy dừng lại đi!
Dialogue: 0,0:19:09.54,0:19:11.67,Default,,0000,0000,0000,,Vậy hãy nói đi... nói đi!
Dialogue: 0,0:19:14.42,0:19:21.15,Default,,0000,0000,0000,,Làm ơn... cho nó vào... âm hộ bốc mùi của tôi...
Dialogue: 0,0:19:21.32,0:19:23.38,Default,,0000,0000,0000,,Nhanh lên, nhanh lên!
Dialogue: 0,0:19:23.46,0:19:26.59,Default,,0000,0000,0000,,Thật là một trinh nữ dơ bẩn.
Dialogue: 0,0:19:39.51,0:19:41.44,Default,,0000,0000,0000,,Đừng thô bạo vậy...
Dialogue: 0,0:19:45.18,0:19:47.55,Default,,0000,0000,0000,,Mình đang... mình đang... mất nó!
Dialogue: 0,0:19:52.42,0:19:56.98,Default,,0000,0000,0000,,Không... cảm giác... cảm giác này là...?
Dialogue: 0,0:19:57.89,0:19:59.86,Default,,0000,0000,0000,,Nó làm mình cuồng lên mất... nữa đi, nữa đi!
Dialogue: 0,0:20:27.52,0:20:28.39,Default,,0000,0000,0000,,Ouch...
Dialogue: 0,0:20:28.59,0:20:32.08,Default,,0000,0000,0000,,Aya-kun... hãy nhẹ nhàng hơn nào...
Dialogue: 0,0:20:32.96,0:20:34.45,Default,,0000,0000,0000,,À, vâng...
Dialogue: 0,0:20:50.71,0:20:53.04,Default,,0000,0000,0000,,Cậu... thật tuyệt...
Dialogue: 0,0:21:12.97,0:21:15.46,Default,,0000,0000,0000,,Không, cậu làm tôi như muốn phát điên.
Dialogue: 0,0:21:26.21,0:21:27.68,Default,,0000,0000,0000,,Cảm giác thật tuyệt...
Dialogue: 0,0:21:29.42,0:21:31.54,Default,,0000,0000,0000,,Hơn hẳn với lũ nhóc kia...!
Dialogue: 0,0:21:42.93,0:21:44.66,Default,,0000,0000,0000,,Tôi muốn cái đó của cậu!
Dialogue: 0,0:21:58.18,0:21:59.20,Default,,0000,0000,0000,,Senpai...!
Dialogue: 0,0:22:09.92,0:22:12.69,Default,,0000,0000,0000,,Em... không giữ nổi nữa rồi!
Dialogue: 0,0:22:23.64,0:22:24.60,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:22:26.74,0:22:28.90,Default,,0000,0000,0000,,Chị cũng muốn ra, Aya-kun...
Dialogue: 0,0:22:36.62,0:22:37.35,Default,,0000,0000,0000,,Thật tuyệt!
Dialogue: 0,0:22:45.73,0:22:47.75,Default,,0000,0000,0000,,A, đây chính là thứ chị cần!
Dialogue: 0,0:23:26.83,0:23:29.20,Default,,0000,0000,0000,,Xuất đi... hãy xuất hết vào trong chị đi nào!
Dialogue: 0,0:23:29.34,0:23:31.24,Default,,0000,0000,0000,,Chị đang đến kỳ...
Dialogue: 0,0:23:48.09,0:23:50.32,Default,,0000,0000,0000,,Em thích thế này hay thế này!?
Dialogue: 0,0:23:56.16,0:23:57.43,Default,,0000,0000,0000,,Đ-Đó là...!
Dialogue: 0,0:24:01.90,0:24:04.13,Default,,0000,0000,0000,,Đau quá, dừng lại đi...
Dialogue: 0,0:24:05.04,0:24:05.87,Default,,0000,0000,0000,,Thật sao?
Dialogue: 0,0:24:06.74,0:24:08.94,Default,,0000,0000,0000,,Cô thật sự muốn tôi dừng lại sao?
Dialogue: 0,0:24:16.45,0:24:17.97,Default,,0000,0000,0000,,Đ-Đừng...
Dialogue: 0,0:24:18.35,0:24:20.82,Default,,0000,0000,0000,,Cô đang ra lệnh cho tôi sao?
Dialogue: 0,0:24:23.76,0:24:28.89,Default,,0000,0000,0000,,Được thôi, nhưng cô phải làm sạch cái đó của tôi\Nkhỏi đống tinh dịch và máu.
Dialogue: 0,0:24:36.54,0:24:39.90,Default,,0000,0000,0000,,Được rồi, bây giờ... cho tôi thấy lỗ sau của cô nào.
Dialogue: 0,0:24:52.75,0:24:59.09,Default,,0000,0000,0000,,Không... nó thật là... cảm giác đó... nữa đi, nữa đi!
Dialogue: 0,0:25:05.60,0:25:06.93,Default,,0000,0000,0000,,Không phải chỗ đó!
Dialogue: 0,0:25:07.10,0:25:08.93,Default,,0000,0000,0000,,Không, xấu hổ quá!
Dialogue: 0,0:25:09.80,0:25:12.83,Default,,0000,0000,0000,,Nhanh lên... đút nó vào, đút nó vào đi!
Dialogue: 0,0:25:24.49,0:25:29.42,Default,,0000,0000,0000,,Nóng... bên trong mình... thật nóng...
Dialogue: 0,0:25:53.78,0:25:58.34,Default,,0000,0000,0000,,T-Tha cho tôi... tôi sắp...
Dialogue: 0,0:25:59.99,0:26:03.29,Default,,0000,0000,0000,,Cô... giờ cô đã là nô lệ của ta!
Dialogue: 0,0:26:10.56,0:26:13.56,Default,,0000,0000,0000,,Ra rồi...! Ra rồi! Ra rồi!
Dialogue: 0,0:26:16.90,0:26:20.34,Default,,0000,0000,0000,,Cái gì đó... nóng... đang chảy ra bên trong tôi...!
Dialogue: 0,0:26:32.49,0:26:33.98,Default,,0000,0000,0000,,Con mồi tiếp theo của chúng ta là ai?
Dialogue: 0,0:26:34.72,0:26:37.12,Default,,0000,0000,0000,,Chúng tôi đã mời cô ta đến chỗ chụp ảnh.
Dialogue: 0,0:26:38.13,0:26:42.22,Default,,0000,0000,0000,,Được rồi, tiếp theo... sẽ là câu lạc bộ chụp ảnh.
Dialogue: 0,0:26:43.03,0:26:44.43,Default,,0000,0000,0000,,Sẽ bận rộn đây.
Dialogue: 0,0:26:45.27,0:26:46.16,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:27:04.82,0:27:06.51,Default,,0000,0000,0000,,Ơ kìa?
Dialogue: 0,0:27:06.89,0:27:08.41,Default,,0000,0000,0000,,Cái gì đây nhỉ?
Dialogue: 0,0:27:16.30,0:27:18.03,Default,,0000,0000,0000,,Thứ này là...?
Dialogue: 0,0:27:19.03,0:27:22.30,Default,,0000,0000,0000,,Sắp phát hành... trên internet!?
Dialogue: 0,0:27:23.70,0:27:30.58,Default,,0000,0000,0000,,Nếu cô muốn những tấm anh này biến mất...\Nhãy đến với bữa tiệc... đặc biệt!?
Dialogue: 0,0:27:36.05,0:27:36.91,Default,,0000,0000,0000,,Ai đó!?
Dialogue: 0,0:27:36.91,0:27:38.91,Default,,0000,0000,0000,,
