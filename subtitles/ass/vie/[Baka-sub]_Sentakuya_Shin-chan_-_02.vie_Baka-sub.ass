Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:11.30,0:00:12.83,Default,,0000,0000,0020,,Xong xuôi!
Dialogue: 0,0:00:16.00,0:00:18.23,Default,,0000,0000,0020,,Tôi đi đây, cô Sayaka!
Dialogue: 0,0:00:19.60,0:00:20.57,Default,,0000,0000,0020,,Uhm.
Dialogue: 0,0:00:20.73,0:00:22.63,Default,,0000,0000,0020,,Um...
Dialogue: 0,0:00:23.07,0:00:25.27,Default,,0000,0000,0020,,Gặp lại anh sau.
Dialogue: 0,0:00:28.00,0:00:31.40,Default,,0000,0000,0020,,Từ ngày hôm đó, cuộc sống của tôi và cô ấy trở nên ngột ngạt.
Dialogue: 0,0:00:38.10,0:00:39.50,Default,,0000,0000,0020,,Shin! Nữa đi!
Dialogue: 0,0:00:39.60,0:00:41.73,Default,,0000,0000,0020,,Chơi tôi mạnh nữa lên!
Dialogue: 0,0:00:43.57,0:00:45.03,Default,,0000,0000,0020,,Chồng tôi...
Dialogue: 0,0:00:45.20,0:00:47.70,Default,,0000,0000,0020,,Hãy giúp tôi quên đi hình bóng anh ấy!
Dialogue: 0,0:00:47.90,0:00:49.87,Default,,0000,0000,0020,,Cô Sayaka!
Dialogue: 0,0:00:52.33,0:00:55.47,Default,,0000,0000,0020,,Chúng tôi thậm chí còn chưa hẹn hò với nhau trước khi chuyện đó xảy ra...
Dialogue: 0,0:00:57.17,0:01:00.80,Default,,0000,0000,0020,,Tôi đã thích cô Sayaka từ lâu, nên với tôi điều đó rất bình thường...
Dialogue: 0,0:01:01.57,0:01:07.17,Default,,0000,0000,0020,,...nhưng cô ấy lại cảm thấy có lỗi với người chồng quá cố của mình.
Dialogue: 0,0:01:12.67,0:01:15.63,Default,,0000,0000,0020,,Đây là toàn bộ số đồ cần giặt ủi hôm nay, phải ko ạ?
Dialogue: 0,0:01:16.26,0:01:18.37,Default,,0000,0000,0020,,Anh sao thế, Shin?
Dialogue: 0,0:01:18.73,0:01:20.50,Default,,0000,0000,0020,,Mặt anh thật khó coi.
Dialogue: 0,0:01:20.76,0:01:22.80,Default,,0000,0000,0020,,Ko phải đâu...
Dialogue: 0,0:01:24.33,0:01:26.83,Default,,0000,0000,0020,,Ko, rõ ràng là anh đang ko vui về điều gì đó, Shin.
Dialogue: 0,0:01:28.13,0:01:32.87,Default,,0000,0000,0020,,Ánh mắt hoang dại của anh ngày thường biến đâu rồi.
Dialogue: 0,0:01:33.37,0:01:35.67,Default,,0000,0000,0020,,Hoang dại là sao cơ?
Dialogue: 0,0:01:37.10,0:01:40.00,Default,,0000,0000,0020,,Trông anh thế này tôi buồn lắm đó.
Dialogue: 0,0:01:41.23,0:01:44.93,Default,,0000,0000,0020,,Chỉ cần khiến anh vui lên, tôi sẵn sàng làm bất cứ điều gì.
Dialogue: 0,0:01:45.10,0:01:46.70,Default,,0000,0000,0020,,Bất cứ điều gì thật sao?
Dialogue: 0,0:01:46.93,0:01:49.70,Default,,0000,0000,0020,,Tất nhiên rồi, dù cho đó là mấy chuyện đồi trụy chăng nữa.
Dialogue: 0,0:01:51.10,0:01:53.17,Default,,0000,0000,0020,,Chỉ cần anh muốn như vậy.
Dialogue: 0,0:01:54.73,0:01:55.97,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:01:57.03,0:02:00.40,Default,,0000,0000,0020,,Vậy phiền cô hãy giữ nó và sau đó giúp tôi nhé?
Dialogue: 0,0:02:00.70,0:02:02.03,Default,,0000,0000,0020,,Cái gì thế?
Dialogue: 0,0:02:06.73,0:02:08.93,Default,,0000,0000,0020,,Đến lúc cần thiết, nó sẽ phát huy công dụng...
Dialogue: 0,0:02:10.13,0:02:11.77,Default,,0000,0000,0020,,Wow!
Dialogue: 0,0:02:12.06,0:02:21.57,Default,,0000,0000,0020,,Mặt trái nghề giặt ủi\NPhần 2
Dialogue: 0,0:02:12.06,0:02:21.57,Default,,0000,0000,0445,,Translated by Invincible_Dark_Thunder
Dialogue: 0,0:02:25.20,0:02:26.57,Default,,0000,0000,0020,,Tiệm giặt ủi Asai!
Dialogue: 0,0:02:26.67,0:02:28.10,Default,,0000,0000,0020,,Làm ơn hãy liên hệ với chúng tôi.
Dialogue: 0,0:02:28.73,0:02:30.00,Default,,0000,0000,0020,,Tiệm giặt ủi Asai!
Dialogue: 0,0:02:30.36,0:02:31.37,Default,,0000,0000,0020,,Mời bác.
Dialogue: 0,0:02:31.63,0:02:34.00,Default,,0000,0000,0020,,Cám ơn bác ạ.
Dialogue: 0,0:02:37.63,0:02:42.90,Default,,0000,0000,0020,,Trông giống như mình đang lợi dụng lòng tốt của cô Mari.
Dialogue: 0,0:02:43.83,0:02:45.13,Default,,0000,0000,0020,,Mời chị.
Dialogue: 0,0:02:46.17,0:02:48.93,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi.
Dialogue: 0,0:02:52.96,0:02:54.70,Default,,0000,0000,0020,,Tiệm giặt ủi Asai!
Dialogue: 0,0:02:55.13,0:02:56.30,Default,,0000,0000,0020,,Mời cô.
Dialogue: 0,0:02:56.73,0:02:59.83,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi.
Dialogue: 0,0:03:02.00,0:03:04.00,Default,,0000,0000,0020,,Tiệm giặt ủi Asai...
Dialogue: 0,0:03:04.80,0:03:09.04,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi...
Dialogue: 0,0:03:13.30,0:03:14.80,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:03:14.96,0:03:16.33,Default,,0000,0000,0020,,Tôi ko chịu nổi nữa rồi...
Dialogue: 0,0:03:26.30,0:03:28.20,Default,,0000,0000,0020,,Cô Mari, chuyện này thật khó chịu, phải ko?
Dialogue: 0,0:03:28.77,0:03:32.17,Default,,0000,0000,0020,,Vậy sao chúng ta ko vui vẻ trong kia một chút nhỉ?!
Dialogue: 0,0:03:33.46,0:03:35.73,Default,,0000,0000,0020,,Tôi cũng đang có ý đó.
Dialogue: 0,0:03:37.90,0:03:39.77,Default,,0000,0000,0020,,Cô xem, khi làm thế này...
Dialogue: 0,0:03:41.03,0:03:43.53,Default,,0000,0000,0020,,...ko ai có thể nhìn vào bên trong xe...
Dialogue: 0,0:03:44.43,0:03:47.20,Default,,0000,0000,0020,,...nhưng chúng ta vẫn có thể nhìn thấy họ từ bên trong.
Dialogue: 0,0:03:47.37,0:03:49.57,Default,,0000,0000,0020,,Trông rõ quá.
Dialogue: 0,0:03:50.03,0:03:53.37,Default,,0000,0000,0020,,Cứ như là họ đang nhìn vào chúng ta vậy,\Nđiều đó khiến tôi hứng thú vô cùng!
Dialogue: 0,0:03:54.70,0:03:55.93,Default,,0000,0000,0020,,Tôi muốn cái đó của anh, Shin!
Dialogue: 0,0:03:56.03,0:03:57.03,Default,,0000,0000,0020,,Khoan đã!
Dialogue: 0,0:03:57.13,0:03:59.63,Default,,0000,0000,0020,,Tôi muốn cô lấy nó ra mà ko cần dùng tay cơ.
Dialogue: 0,0:04:00.06,0:04:02.03,Default,,0000,0000,0020,,Được thôi, đợi tôi chút.
Dialogue: 0,0:04:02.83,0:04:07.97,Default,,0000,0000,0020,,Shin, có phải khi người ta thỏa mãn...
Dialogue: 0,0:04:12.60,0:04:16.44,Default,,0000,0000,0020,,Có cảm giác mọi thứ bên trong sẽ trào hết hết ra ngoài, phải ko!
Dialogue: 0,0:04:16.56,0:04:17.67,Default,,0000,0000,0020,,Đúng vậy.
Dialogue: 0,0:04:18.00,0:04:22.30,Default,,0000,0000,0020,,Âm vật hồng hào của cô đang ướt đẫm nước kìa...
Dialogue: 0,0:04:23.56,0:04:24.53,Default,,0000,0000,0020,,Nó đang ra...
Dialogue: 0,0:04:24.67,0:04:26.00,Default,,0000,0000,0020,,Sắp ra rồi...
Dialogue: 0,0:04:26.17,0:04:27.13,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:04:27.23,0:04:28.37,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:04:29.37,0:04:30.33,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:04:30.50,0:04:31.53,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:04:32.17,0:04:34.00,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:04:37.83,0:04:38.87,Default,,0000,0000,0020,,Thế hả!
Dialogue: 0,0:04:40.97,0:04:42.30,Default,,0000,0000,0020,,Tuyệt quá đi mất..
Dialogue: 0,0:04:43.20,0:04:44.37,Default,,0000,0000,0020,,Tuyệt quá...
Dialogue: 0,0:04:44.63,0:04:46.10,Default,,0000,0000,0020,,Cô Mari vẫn đang ra.
Dialogue: 0,0:04:46.56,0:04:49.00,Default,,0000,0000,0020,,Nó vẫn trào ra như suối kìa, hay thật đấy!
Dialogue: 0,0:04:49.73,0:04:54.10,Default,,0000,0000,0020,,Vậy anh hãy đâm thằng nhỏ của mình vào ngọn suối đó đi.
Dialogue: 0,0:04:54.40,0:04:56.37,Default,,0000,0000,0020,,Tôi muốn nó, Shin!
Dialogue: 0,0:05:03.23,0:05:06.37,Default,,0000,0000,0020,,Khách hàng là Thượng đế, e rằng tôi ko còn sự lựa chọn nào khác.
Dialogue: 0,0:05:07.30,0:05:08.63,Default,,0000,0000,0020,,Mau lên! Mau lên nào!
Dialogue: 0,0:05:09.60,0:05:11.00,Default,,0000,0000,0020,,Nhét nó vào trong đi!
Dialogue: 0,0:05:11.10,0:05:12.43,Default,,0000,0000,0020,,Rất sẵn lòng phục vụ cô.
Dialogue: 0,0:05:16.97,0:05:18.43,Default,,0000,0000,0020,,Shin! Shin!
Dialogue: 0,0:05:18.63,0:05:20.67,Default,,0000,0000,0020,,Đồ thật đúng là thứ tuyệt vời nhất!
Dialogue: 0,0:05:29.60,0:05:31.33,Default,,0000,0000,0020,,Shin! Shin!
Dialogue: 0,0:05:33.57,0:05:34.80,Default,,0000,0000,0020,,Tuyệt quá.
Dialogue: 0,0:05:35.43,0:05:39.94,Default,,0000,0000,0020,,Cô làm xe rung ác quá đấy, mấy bà lớn tuổi đang nhìn đó.
Dialogue: 0,0:05:40.16,0:05:47.01,Default,,0000,0000,0020,,Vậy thì hãy cho mấy bà chê chồng đó thấy tình yêu của chúng ta mạnh mẽ đến mức nào!
Dialogue: 0,0:05:47.33,0:05:49.17,Default,,0000,0000,0020,,Ko vấn đề gì, cô Mari!
Dialogue: 0,0:05:51.66,0:05:53.67,Default,,0000,0000,0020,,Tưởng tượng đi nào.
Dialogue: 0,0:05:53.77,0:05:55.73,Default,,0000,0000,0020,,Đây là chuyện mà các cô vẫn thường làm ban đêm đó.
Dialogue: 0,0:05:55.83,0:05:57.17,Default,,0000,0000,0020,,Trông này, trông này!
Dialogue: 0,0:05:57.66,0:05:58.63,Default,,0000,0000,0020,,Cô!
Dialogue: 0,0:05:58.80,0:05:59.93,Default,,0000,0000,0020,,Cả cô nữa!
Dialogue: 0,0:06:00.47,0:06:01.43,Default,,0000,0000,0020,,Tuyệt quá!
Dialogue: 0,0:06:01.53,0:06:03.10,Default,,0000,0000,0020,,Tuyệt quá!
Dialogue: 0,0:06:03.33,0:06:05.60,Default,,0000,0000,0020,,Chà? Cô thỏa mãn chưa, cô Mari?
Dialogue: 0,0:06:06.03,0:06:07.07,Default,,0000,0000,0020,,Chưa phải lúc này...
Dialogue: 0,0:06:07.40,0:06:11.60,Default,,0000,0000,0020,,Coi nào, hãy phát vào mông tôi như anh vẫn thường làm đi, Shin!
Dialogue: 0,0:06:13.20,0:06:14.17,Default,,0000,0000,0020,,Tuyệt quá đi mất.
Dialogue: 0,0:06:14.27,0:06:15.83,Default,,0000,0000,0020,,Nó đang khít lại.
Dialogue: 0,0:06:16.53,0:06:18.70,Default,,0000,0000,0020,,Này, này, này.
Dialogue: 0,0:06:18.83,0:06:20.53,Default,,0000,0000,0020,,Phát mạnh lên nào.
Dialogue: 0,0:06:20.63,0:06:22.43,Default,,0000,0000,0020,,Mạnh nữa lên!
Dialogue: 0,0:06:25.83,0:06:27.07,Default,,0000,0000,0020,,Ah, Shin!
Dialogue: 0,0:06:30.43,0:06:32.80,Default,,0000,0000,0020,,Làm thế này sẽ khiến cô hưng phấn hơn đấy.
Dialogue: 0,0:06:35.20,0:06:36.60,Default,,0000,0000,0020,,Oh, Shin!
Dialogue: 0,0:06:36.73,0:06:38.03,Default,,0000,0000,0020,,Đau quá!
Dialogue: 0,0:06:38.13,0:06:39.10,Default,,0000,0000,0020,,Đau quá!
Dialogue: 0,0:06:40.90,0:06:42.23,Default,,0000,0000,0020,,Cô Mari! Cô Mari!
Dialogue: 0,0:06:42.50,0:06:45.03,Default,,0000,0000,0020,,Coi nào, tôi sẽ tiếp tục làm vậy đến khi nào cô ra mới thôi.
Dialogue: 0,0:06:46.73,0:06:49.27,Default,,0000,0000,0020,,Nữa đi! Phát tôi mạnh vào, Shin!
Dialogue: 0,0:06:49.96,0:06:52.03,Default,,0000,0000,0020,,Shin! Shin!
Dialogue: 0,0:07:12.30,0:07:15.37,Default,,0000,0000,0020,,Shin, anh ra nhiều quá.
Dialogue: 0,0:07:18.43,0:07:19.77,Default,,0000,0000,0020,,Tiếp nối bản tin là...
Dialogue: 0,0:07:20.10,0:07:21.97,Default,,0000,0000,0020,,Cô Mari, khoan đã...
Dialogue: 0,0:07:22.07,0:07:25.84,Default,,0000,0000,0020,,Thôi nào,\Nanh vẫn còn thanh niên! Thanh niên mà!
Dialogue: 0,0:07:27.73,0:07:28.70,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:07:28.83,0:07:29.80,Default,,0000,0000,0020,,Cô Mari...
Dialogue: 0,0:07:30.07,0:07:32.53,Default,,0000,0000,0020,,Cô Mari!
Dialogue: 0,0:07:49.47,0:07:51.73,Default,,0000,0000,0020,,Chà, làm việc này một mình chán thật...
Dialogue: 0,0:07:52.36,0:07:56.54,Default,,0000,0000,0020,,Đáng lẽ ra mình phải làm chuyện đó với Mari sau khi đã phát xong đống này.
Dialogue: 0,0:07:58.17,0:07:59.47,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi!
Dialogue: 0,0:08:00.60,0:08:02.03,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi!
Dialogue: 0,0:08:03.26,0:08:04.33,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi!
Dialogue: 0,0:08:04.63,0:08:06.03,Default,,0000,0000,0020,,Hãy liên hệ với chúng tôi!
Dialogue: 0,0:08:10.40,0:08:11.67,Default,,0000,0000,0020,,Ouch!
Dialogue: 0,0:08:14.73,0:08:16.57,Default,,0000,0000,0020,,Này, anh kia!
Dialogue: 0,0:08:16.67,0:08:17.93,Default,,0000,0000,0020,,Xin cô thứ lỗi!
Dialogue: 0,0:08:20.00,0:08:23.90,Default,,0000,0000,0020,,Này, muốn phát gì thì phát\N, nhưng phải để ý xung quanh chứ!
Dialogue: 0,0:08:24.17,0:08:25.23,Default,,0000,0000,0020,,Xin lỗi cô.
Dialogue: 0,0:08:25.63,0:08:27.13,Default,,0000,0000,0020,,Cô ko sao chứ?
Dialogue: 0,0:08:28.17,0:08:29.63,Default,,0000,0000,0020,,Trời, tóc tôi hỏng mất.
Dialogue: 0,0:08:31.27,0:08:32.47,Default,,0000,0000,0020,,Váy của tôi!
Dialogue: 0,0:08:33.43,0:08:35.73,Default,,0000,0000,0020,,Ôi ko,\NThành thật xin lỗi cô về chuyện này!
Dialogue: 0,0:08:35.90,0:08:37.17,Default,,0000,0000,0020,,Đó là cái mà tôi thích nhất...
Dialogue: 0,0:08:37.27,0:08:38.67,Default,,0000,0000,0020,,Xin lỗi cô! Mong cô thứ lỗi!
Dialogue: 0,0:08:39.43,0:08:43.84,Default,,0000,0000,0020,,Chúng tôi sẽ cố gắng giặt sạch nó và giao lại cho cô sớm nhất có thể!
Dialogue: 0,0:08:44.43,0:08:45.40,Default,,0000,0000,0020,,Mời cô xem qua!
Dialogue: 0,0:08:45.50,0:08:46.77,Default,,0000,0000,0020,,Tiệm giặt ủi Asai\NShinji Ouhashi
Dialogue: 0,0:08:47.00,0:08:48.83,Default,,0000,0000,0020,,Anh là thợ giặt ủi hả?
Dialogue: 0,0:08:49.70,0:08:51.23,Default,,0000,0000,0020,,Tiệm giặt ủi Asai?
Dialogue: 0,0:08:51.33,0:08:53.87,Default,,0000,0000,0020,,Dạ vâng! Chính là nơi đó ạ!
Dialogue: 0,0:08:57.23,0:08:58.23,Default,,0000,0000,0020,,Thật hả...
Dialogue: 0,0:08:59.27,0:09:01.23,Default,,0000,0000,0020,,Anh là một thợ giặt ủi, huh?
Dialogue: 0,0:09:02.60,0:09:04.77,Default,,0000,0000,0020,,Theo tôi nào!
Dialogue: 0,0:09:04.96,0:09:06.73,Default,,0000,0000,0020,,K-Khoan đã!
Dialogue: 0,0:09:17.13,0:09:18.13,Default,,0000,0000,0020,,Um...
Dialogue: 0,0:09:18.50,0:09:21.30,Default,,0000,0000,0020,,Cô Igarashi, sao cô lại dẫn tôi tới đây?
Dialogue: 0,0:09:21.80,0:09:25.64,Default,,0000,0000,0020,,Để anh làm công việc giặt ủi của mình chứ sao.
Dialogue: 0,0:09:25.86,0:09:29.10,Default,,0000,0000,0020,,Hiểu ý tôi chứ?
Dialogue: 0,0:09:30.00,0:09:30.97,Default,,0000,0000,0020,,K-Khoan đã!
Dialogue: 0,0:09:39.07,0:09:43.04,Default,,0000,0000,0020,,Cứ gọi tôi là Yuko...
Dialogue: 0,0:09:47.23,0:09:49.83,Default,,0000,0000,0020,,Đừng ngại, ngồi xuống đây nào.
Dialogue: 0,0:09:51.73,0:09:52.87,Default,,0000,0000,0020,,V-Vâng...
Dialogue: 0,0:09:56.46,0:10:00.07,Default,,0000,0000,0020,,Anh là nhân viên của tiệm giặt ủi, phải ko?
Dialogue: 0,0:10:00.30,0:10:01.80,Default,,0000,0000,0020,,Uh, vâng.
Dialogue: 0,0:10:03.23,0:10:05.87,Default,,0000,0000,0020,,Cửa tiệm hoạt động tốt chứ?
Dialogue: 0,0:10:06.06,0:10:07.17,Default,,0000,0000,0020,,Nếu cô hỏi vậy...
Dialogue: 0,0:10:07.37,0:10:12.84,Default,,0000,0000,0020,,Tình hình hiện giờ ko được tốt lắm,\Nchúng tôi đang cố gắng...
Dialogue: 0,0:10:13.17,0:10:14.17,Default,,0000,0000,0020,,Vậy hả...
Dialogue: 0,0:10:17.60,0:10:20.07,Default,,0000,0000,0020,,Hm... Xin cô đừng lo.
Dialogue: 0,0:10:20.37,0:10:23.74,Default,,0000,0000,0020,,Dứt khoát váy của cô sẽ được giặt sạch, cô Yuko.
Dialogue: 0,0:10:24.90,0:10:25.90,Default,,0000,0000,0020,,Huh?
Dialogue: 0,0:10:26.00,0:10:27.13,Default,,0000,0000,0020,,Chuyện gì vậy?
Dialogue: 0,0:10:27.43,0:10:30.37,Default,,0000,0000,0020,,Hình như tôi đã trông thấy cô ở đâu đó...
Dialogue: 0,0:10:31.73,0:10:33.07,Default,,0000,0000,0020,,Gì cơ? Gì cơ?
Dialogue: 0,0:10:33.27,0:10:35.03,Default,,0000,0000,0020,,Trong một bức hình lưu niệm nào đó à?
Dialogue: 0,0:10:35.50,0:10:38.33,Default,,0000,0000,0020,,Hay là chúng ta đã quen nhau từ kiếp trước?
Dialogue: 0,0:10:39.40,0:10:40.63,Default,,0000,0000,0020,,Ôi, nóng thật đó.
Dialogue: 0,0:10:41.00,0:10:43.47,Default,,0000,0000,0020,,Oh, trong đây nóng quá.
Dialogue: 0,0:10:44.17,0:10:46.93,Default,,0000,0000,0020,,Chắc chắn là tôi đã thấy cô ở đâu đó...
Dialogue: 0,0:10:47.30,0:10:49.17,Default,,0000,0000,0020,,Có lẽ tôi phải cởi bớt đồ ra thôi.
Dialogue: 0,0:10:51.50,0:10:52.53,Default,,0000,0000,0020,,Nóng quá...
Dialogue: 0,0:10:52.63,0:10:53.67,Default,,0000,0000,0020,,Nóng quá...
Dialogue: 0,0:10:59.06,0:11:00.07,Default,,0000,0000,0020,,Xong!
Dialogue: 0,0:11:00.83,0:11:04.50,Default,,0000,0000,0020,,Vậy là cửa tiệm của anh ko nổi tiếng cho lắm...
Dialogue: 0,0:11:05.70,0:11:08.13,Default,,0000,0000,0020,,...tiệm anh có bao nhiêu nhân viên?
Dialogue: 0,0:11:09.60,0:11:11.73,Default,,0000,0000,0020,,Um, chỉ có hai người thôi,\Nlà tôi và bà chủ...
Dialogue: 0,0:11:12.00,0:11:14.47,Default,,0000,0000,0020,,...ủa, mà sao cô lại hỏi những chuyện này?
Dialogue: 0,0:11:15.66,0:11:17.80,Default,,0000,0000,0020,,Và cả chuyện nổi tiếng hay ko nữa...
Dialogue: 0,0:11:18.13,0:11:19.43,Default,,0000,0000,0020,,Ôi...
Dialogue: 0,0:11:20.20,0:11:23.10,Default,,0000,0000,0020,,...chỉ là tò mò thôi mà.
Dialogue: 0,0:11:29.13,0:11:30.77,Default,,0000,0000,0020,,N-N-Này!
Dialogue: 0,0:11:32.47,0:11:33.93,Default,,0000,0000,0020,,Cô Yuko!
Dialogue: 0,0:11:35.16,0:11:37.30,Default,,0000,0000,0020,,Dù cửa tiệm vắng khách cỡ nào đi nữa...
Dialogue: 0,0:11:37.50,0:11:39.33,Default,,0000,0000,0020,,...chỉ mình hai người lo toan mọi việc liệu có vất vả lắm ko?
Dialogue: 0,0:11:39.47,0:11:40.77,Default,,0000,0000,0020,,Cái đó...
Dialogue: 0,0:11:40.93,0:11:43.70,Default,,0000,0000,0020,,Chắc lương anh cao lắm nhỉ?
Dialogue: 0,0:11:46.87,0:11:47.97,Default,,0000,0000,0020,,Cô Yuko...
Dialogue: 0,0:11:48.27,0:11:49.40,Default,,0000,0000,0020,,...làm ơn dừng lại!
Dialogue: 0,0:11:57.06,0:12:02.07,Default,,0000,0000,0020,,Hay là bà chủ góa chồng đã trả lương cho anh theo cách này?
Dialogue: 0,0:12:03.37,0:12:04.87,Default,,0000,0000,0020,,Góa chồng...
Dialogue: 0,0:12:05.03,0:12:06.50,Default,,0000,0000,0020,,Chuyện của cô Sayaka...
Dialogue: 0,0:12:06.90,0:12:07.97,Default,,0000,0000,0020,,Làm sao cô...
Dialogue: 0,0:12:15.30,0:12:16.77,Default,,0000,0000,0020,,Hiệp hội giặt ủi Oute
Dialogue: 0,0:12:16.87,0:12:18.13,Default,,0000,0000,0020,,Chủ tịch\NYuko Igarashi
Dialogue: 0,0:12:18.47,0:12:19.57,Default,,0000,0000,0020,,Nhớ ra rồi!
Dialogue: 0,0:12:21.13,0:12:22.13,Default,,0000,0000,0020,,Gì thế?
Dialogue: 0,0:12:22.23,0:12:23.87,Default,,0000,0000,0020,,Tôi đã thấy cô trên truyền hình một lần!
Dialogue: 0,0:12:24.06,0:12:25.23,Default,,0000,0000,0020,,Yuko Igarashi!
Dialogue: 0,0:12:25.53,0:12:28.00,Default,,0000,0000,0020,,Chủ tịch liên hiệp giặt ủi Oute!
Dialogue: 0,0:12:30.37,0:12:32.80,Default,,0000,0000,0020,,Oh, bị anh phát hiện mất rồi.
Dialogue: 0,0:12:34.47,0:12:35.47,Default,,0000,0000,0020,,Tiếc quá.
Dialogue: 0,0:12:36.30,0:12:38.40,Default,,0000,0000,0020,,Tôi đang định vui vẻ cùng anh một lúc nữa.
Dialogue: 0,0:12:40.73,0:12:41.77,Default,,0000,0000,0020,,Nhắc mới nhớ...
Dialogue: 0,0:12:41.87,0:12:45.37,Default,,0000,0000,0020,,...nghe nói một cửa tiệm lớn sắp khai trương ở ngay nhà ga trung tâm.
Dialogue: 0,0:12:45.70,0:12:47.53,Default,,0000,0000,0020,,Thì ra cô dụ tôi tới đây là có mục đích...
Dialogue: 0,0:12:47.73,0:12:50.33,Default,,0000,0000,0020,,...cô chắc hẳn đang âm mưu làm tiệm giặt ủi Asai phải đóng cửa.
Dialogue: 0,0:12:50.47,0:12:51.77,Default,,0000,0000,0020,,Đừng nói thế chứ.
Dialogue: 0,0:12:59.56,0:13:01.40,Default,,0000,0000,0020,,Ko như anh nghĩ đâu.
Dialogue: 0,0:13:01.60,0:13:05.60,Default,,0000,0000,0020,,Tôi đã thu thập nhiều thông tin về các cửa tiệm khác trước khi lên kế hoạch khai trương nó.
Dialogue: 0,0:13:05.70,0:13:06.70,Default,,0000,0000,0020,,Vậy thì sao...
Dialogue: 0,0:13:06.83,0:13:08.73,Default,,0000,0000,0020,,Khi gặp anh ban nãy.
Dialogue: 0,0:13:10.70,0:13:13.33,Default,,0000,0000,0020,,Tôi chợt nảy ra ý định thu lượm thêm một ít thông tin từ anh.
Dialogue: 0,0:13:24.56,0:13:27.10,Default,,0000,0000,0020,,Thái độ đó là sao hả,\Nchẳng phải cô đã gạt gẫm tôi nãy giờ sao?
Dialogue: 0,0:13:27.60,0:13:30.07,Default,,0000,0000,0020,,Cô tưởng rằng là chủ tịch Oute thì muốn phỉnh phờ ai cũng được sao?
Dialogue: 0,0:13:30.18,0:13:30.88,Default,,0000,0000,0020,,Hả?
Dialogue: 0,0:13:31.18,0:13:31.87,Default,,0000,0000,0020,,Đau đấy!
Dialogue: 0,0:13:32.37,0:13:33.90,Default,,0000,0000,0020,,Buông tôi ra!
Dialogue: 0,0:13:36.40,0:13:37.37,Default,,0000,0000,0020,,Đau quá...
Dialogue: 0,0:13:37.83,0:13:40.20,Default,,0000,0000,0020,,Tôi chỉ đùa anh một chút cho vui thôi mà.
Dialogue: 0,0:13:40.87,0:13:43.93,Default,,0000,0000,0020,,Đừng kích động thế chứ?
Dialogue: 0,0:13:46.66,0:13:48.93,Default,,0000,0000,0020,,Đừng hòng coi thường một thợ giặt ủi như tôi!
Dialogue: 0,0:13:50.37,0:13:51.33,Default,,0000,0000,0020,,Quay lại đây!
Dialogue: 0,0:13:51.43,0:13:52.73,Default,,0000,0000,0020,,Ko, buông tôi ra!
Dialogue: 0,0:13:52.83,0:13:53.83,Default,,0000,0000,0020,,Buông ra!
Dialogue: 0,0:13:54.13,0:13:55.10,Default,,0000,0000,0020,,Ko!
Dialogue: 0,0:13:55.20,0:13:59.67,Default,,0000,0000,0020,,Để xem ngực cô có đủ ngon để phỉnh phờ đàn ông ko nào!
Dialogue: 0,0:13:59.87,0:14:01.33,Default,,0000,0000,0020,,Này, dừng lại đi!
Dialogue: 0,0:14:20.70,0:14:21.67,Default,,0000,0000,0020,,Gì đây nhỉ?
Dialogue: 0,0:14:21.77,0:14:22.80,Default,,0000,0000,0020,,Đau tôi...
Dialogue: 0,0:14:23.00,0:14:27.54,Default,,0000,0000,0020,,Bộ ngực này mà cũng đòi phỉnh phờ tôi sao?
Dialogue: 0,0:14:27.66,0:14:29.10,Default,,0000,0000,0020,,Anh thật xấu xa...
Dialogue: 0,0:14:29.23,0:14:32.77,Default,,0000,0000,0020,,Để tôi làm một cái bánh kẹp sandwich với bộ ngực của cô nhé!
Dialogue: 0,0:14:33.30,0:14:35.13,Default,,0000,0000,0020,,Dừng lại đi! Làm ơn!
Dialogue: 0,0:14:39.30,0:14:41.07,Default,,0000,0000,0020,,Nhìn xem, hai đầu ngực cô đã cứng như vậy rồi.
Dialogue: 0,0:14:41.40,0:14:44.03,Default,,0000,0000,0020,,Hứng lên rồi phải ko, bà Chủ tịch?
Dialogue: 0,0:14:45.76,0:14:46.83,Default,,0000,0000,0020,,Ko phải...
Dialogue: 0,0:14:46.93,0:14:48.10,Default,,0000,0000,0020,,...như vậy...
Dialogue: 0,0:14:48.33,0:14:49.77,Default,,0000,0000,0020,,Vậy sao?
Dialogue: 0,0:14:52.40,0:14:54.70,Default,,0000,0000,0020,,Chẳng phải cô hứng lên vì tôi đối xử thô bạo với cô sao?
Dialogue: 0,0:14:54.93,0:14:57.67,Default,,0000,0000,0020,,Ko hiếm những kẻ dâm đãng giàu sang, phải ko nhỉ?
Dialogue: 0,0:14:58.43,0:15:00.17,Default,,0000,0000,0020,,Chúng ta như nhau cả thôi!
Dialogue: 0,0:15:03.53,0:15:06.37,Default,,0000,0000,0020,,Chỗ đó của cô ướt đẫm rồi kìa.
Dialogue: 0,0:15:06.53,0:15:07.97,Default,,0000,0000,0020,,Cô ả dâm đãng!
Dialogue: 0,0:15:08.66,0:15:10.50,Default,,0000,0000,0020,,Ướt hết cả rồi
Dialogue: 0,0:15:10.93,0:15:13.00,Default,,0000,0000,0020,,Cô luôn tỏ ra căm ghét đàn ông...
Dialogue: 0,0:15:13.10,0:15:16.33,Default,,0000,0000,0020,,...nhưng sự thật là cô luôn thèm khát thằng nhỏ của họ, phải ko nào?
Dialogue: 0,0:15:16.47,0:15:18.70,Default,,0000,0000,0020,,Ko phải... như vậy...
Dialogue: 0,0:15:18.83,0:15:21.63,Default,,0000,0000,0020,,Được lắm, tôi sẽ giúp cô thỏa nguyện!
Dialogue: 0,0:15:24.33,0:15:26.87,Default,,0000,0000,0020,,Ko ko ko ko!
Dialogue: 0,0:15:27.40,0:15:30.37,Default,,0000,0000,0020,,Đừng giả bộ nữa.\NTôi nhét nó vào bên trong đây!
Dialogue: 0,0:15:30.90,0:15:33.63,Default,,0000,0000,0020,,Coi kìa!\NNó vào hết bên trong rồi!
Dialogue: 0,0:15:34.83,0:15:36.30,Default,,0000,0000,0020,,Nào nào!\NKo, ko!
Dialogue: 0,0:15:36.73,0:15:37.87,Default,,0000,0000,0020,,Ko!
Dialogue: 0,0:15:38.26,0:15:40.93,Default,,0000,0000,0020,,Dù cô nói ko,\Nâm hộ của cô đang xoắn lấy...
Dialogue: 0,0:15:41.07,0:15:43.20,Default,,0000,0000,0020,,...thằng nhỏ của tôi như điên!
Dialogue: 0,0:15:46.87,0:15:48.67,Default,,0000,0000,0020,,Làm ơn tha cho tôi!
Dialogue: 0,0:15:48.87,0:15:52.03,Default,,0000,0000,0020,,Tôi ko cố ý cướp khách hàng của tiệm anh mà!
Dialogue: 0,0:15:53.07,0:15:55.07,Default,,0000,0000,0020,,Điều này phụ thuộc vào năng lực mỗi người.
Dialogue: 0,0:15:55.30,0:15:57.43,Default,,0000,0000,0020,,Ko cần phải tâng bốc nịnh nọt gì ai hết!
Dialogue: 0,0:15:57.80,0:16:00.07,Default,,0000,0000,0020,,Shin -siêu cấp giặt ủi này mới là đàn ông đích thực!
Dialogue: 0,0:16:04.26,0:16:06.80,Default,,0000,0000,0020,,Tôi ko chịu nổi nữa rồi!\NNếu anh làm mạnh như vậy...
Dialogue: 0,0:16:06.90,0:16:08.50,Default,,0000,0000,0020,,...Tôi... Tôi...
Dialogue: 0,0:16:10.93,0:16:12.23,Default,,0000,0000,0020,,Tôi cũng vậy...
Dialogue: 0,0:16:16.40,0:16:20.03,Default,,0000,0000,0020,,Nào, dùng mắt cô kiểm định chất lượng của chúng đi!
Dialogue: 0,0:16:20.26,0:16:21.23,Default,,0000,0000,0020,,Ko...
Dialogue: 0,0:16:32.73,0:16:35.47,Default,,0000,0000,0020,,Chủ tịch một hiệp hội lớn mà lóng ngóng vậy sao.
Dialogue: 0,0:16:36.40,0:16:37.67,Default,,0000,0000,0020,,Rõ là một con ngốc!
Dialogue: 0,0:16:39.93,0:16:45.17,Default,,0000,0000,0020,,Nghe này, nếu cô luôn ra vẻ trịch thượng như vậy, sẽ chả có thằng nào thèm động đến cô đâu.
Dialogue: 0,0:16:45.60,0:16:49.07,Default,,0000,0000,0020,,Phần phụ nữ trong cô đã được đánh thức khi làm chuyện đó với tôi, phải ko nào?
Dialogue: 0,0:16:49.23,0:16:51.00,Default,,0000,0000,0020,,Chúc cô ngày càng nữ tính nhé...
Dialogue: 0,0:16:51.10,0:16:56.24,Default,,0000,0000,0020,,Chỉ vậy mà đòi thỏa mãn tôi sao.
Dialogue: 0,0:16:56.33,0:16:57.30,Default,,0000,0000,0020,,Hả?
Dialogue: 0,0:16:57.53,0:17:01.60,Default,,0000,0000,0020,,Chưa làm tôi ra thì đừng mong được gọi là đàn ông nhé.
Dialogue: 0,0:17:05.23,0:17:08.50,Default,,0000,0000,0020,,Nào, bự lên nữa nào...
Dialogue: 0,0:17:08.63,0:17:11.93,Default,,0000,0000,0020,,...ko thì anh ko thỏa mãn nổi tôi đâu.
Dialogue: 0,0:17:12.80,0:17:14.07,Default,,0000,0000,0020,,Ngoan lắm.
Dialogue: 0,0:17:14.57,0:17:15.83,Default,,0000,0000,0020,,Ngoan lắm.
Dialogue: 0,0:17:17.07,0:17:19.13,Default,,0000,0000,0020,,Nếu cô làm vậy, tôi sẽ...
Dialogue: 0,0:17:20.10,0:17:22.03,Default,,0000,0000,0020,,Chưa được...
Dialogue: 0,0:17:22.86,0:17:26.60,Default,,0000,0000,0020,,Tôi chưa cho phép anh ra lúc này.
Dialogue: 0,0:17:29.03,0:17:31.80,Default,,0000,0000,0020,,Nếu anh muốn ra,\Nhãy ra ở bên trong tôi này.
Dialogue: 0,0:17:36.93,0:17:40.13,Default,,0000,0000,0020,,Này, nguồn sinh lực dồi dào trước đó của anh biến đâu rồi?
Dialogue: 0,0:17:40.33,0:17:42.30,Default,,0000,0000,0020,,Nhịp mạnh lên đi chứ!
Dialogue: 0,0:17:44.03,0:17:45.60,Default,,0000,0000,0020,,Cô Yuko...
Dialogue: 0,0:17:48.23,0:17:49.30,Default,,0000,0000,0020,,Cô Yuko...
Dialogue: 0,0:17:49.47,0:17:50.67,Default,,0000,0000,0020,,Cô Yuko...
Dialogue: 0,0:17:50.86,0:17:52.07,Default,,0000,0000,0020,,Phải, chơi tôi đi!
Dialogue: 0,0:17:52.17,0:17:53.90,Default,,0000,0000,0020,,Chơi tôi mạnh nữa lên!
Dialogue: 0,0:18:00.03,0:18:01.00,Default,,0000,0000,0020,,Phải rồi!
Dialogue: 0,0:18:01.10,0:18:03.23,Default,,0000,0000,0020,,Ngay chỗ đó!
Dialogue: 0,0:18:05.10,0:18:07.60,Default,,0000,0000,0020,,Tôi hết chịu nổi rồi, cô Yuko...
Dialogue: 0,0:18:07.70,0:18:09.43,Default,,0000,0000,0020,,Làm ơn hãy để tôi ra...
Dialogue: 0,0:18:10.03,0:18:11.77,Default,,0000,0000,0020,,Chưa được! Chỉ một chút nữa thôi!
Dialogue: 0,0:18:11.90,0:18:12.97,Default,,0000,0000,0020,,Một chút nữa...
Dialogue: 0,0:18:13.07,0:18:14.77,Default,,0000,0000,0020,,Một chút xíu nữa thôi...\NTôi sắp ra rồi!
Dialogue: 0,0:18:23.60,0:18:24.57,Default,,0000,0000,0020,,Tôi sắp ra rồi!
Dialogue: 0,0:18:24.67,0:18:26.13,Default,,0000,0000,0020,,Tôi sắp ra rồi!
Dialogue: 0,0:18:29.43,0:18:31.17,Default,,0000,0000,0020,,Tôi ra rồi!
Dialogue: 0,0:18:36.83,0:18:39.80,Default,,0000,0000,0020,,Cám ơn vì đã tin tưởng!
Dialogue: 0,0:18:44.23,0:18:46.27,Default,,0000,0000,0020,,Mới bắt đầu được một lúc thôi mà...
Dialogue: 0,0:18:52.33,0:18:54.50,Default,,0000,0000,0020,,Chà, có lẽ mình hành động hơi quá?
Dialogue: 0,0:18:55.13,0:18:58.30,Default,,0000,0000,0020,,Cô ta mà tìm cách bắt tiệm giặt ủi Asai phải đóng cửa thì hỏng hết...
Dialogue: 0,0:18:59.10,0:19:02.07,Default,,0000,0000,0020,,Ko! Mình phải cùng cô Sayaka cứu lấy cửa tiệm này!
Dialogue: 0,0:19:02.40,0:19:03.57,Default,,0000,0000,0020,,Quyết định vậy đi!
Dialogue: 0,0:19:04.63,0:19:06.63,Default,,0000,0000,0020,,Làm ơn hãy chờ tôi, cô Sayaka!
Dialogue: 0,0:19:15.13,0:19:16.30,Default,,0000,0000,0020,,Yutaro...
Dialogue: 0,0:19:16.87,0:19:19.27,Default,,0000,0000,0020,,Đã 6 năm trôi qua từ khi anh ra đi...
Dialogue: 0,0:19:19.96,0:19:23.80,Default,,0000,0000,0020,,Mùi của một người đàn ông khác đã quyến rũ em...
Dialogue: 0,0:19:25.00,0:19:26.03,Default,,0000,0000,0020,,Tha lỗi cho em...
Dialogue: 0,0:19:28.23,0:19:31.93,Default,,0000,0000,0020,,Anh ấy có mùi giống hệt anh, điều đó khiến trái tim em được an ủi...
Dialogue: 0,0:19:32.53,0:19:36.30,Default,,0000,0000,0020,,...nhưng em ko đủ can đảm để thổ lộ với anh ấy.
Dialogue: 0,0:19:37.27,0:19:38.27,Default,,0000,0000,0020,,Tôi về rồi đây!
Dialogue: 0,0:19:39.03,0:19:40.43,Default,,0000,0000,0020,,Công việc hôm nay coi như đã hoàn thành.
Dialogue: 0,0:19:40.73,0:19:42.43,Default,,0000,0000,0020,,Anh làm việc chăm chỉ ghê.
Dialogue: 0,0:19:42.70,0:19:45.53,Default,,0000,0000,0020,,Tôi sẽ phân loại chúng sau,\Nanh cứ đặt ở đó là được rồi.
Dialogue: 0,0:19:45.67,0:19:47.10,Default,,0000,0000,0020,,Vâng! Um...
Dialogue: 0,0:19:48.27,0:19:50.27,Default,,0000,0000,0020,,Người đại diện lâu tới vậy nhỉ...
Dialogue: 0,0:19:50.40,0:19:52.23,Default,,0000,0000,0020,,Sayaka... Sayaka!
Dialogue: 0,0:19:53.87,0:19:54.97,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:19:55.53,0:19:56.70,Default,,0000,0000,0020,,Buông tôi ra...
Dialogue: 0,0:19:57.00,0:19:58.97,Default,,0000,0000,0020,,Sao cô cứ chạy trốn tôi như vậy, Sayaka?
Dialogue: 0,0:19:59.40,0:20:01.07,Default,,0000,0000,0020,,Tôi ko hề chạy trốn anh...
Dialogue: 0,0:20:01.37,0:20:03.67,Default,,0000,0000,0020,,Vậy cô đứng lại nghe tôi nói điều này được ko?
Dialogue: 0,0:20:04.30,0:20:05.33,Default,,0000,0000,0020,,V-Vâng...
Dialogue: 0,0:20:07.40,0:20:10.70,Default,,0000,0000,0020,,Anh muốn trao cho em vật này đã lâu, chỉ là...
Dialogue: 0,0:20:11.43,0:20:12.47,Default,,0000,0000,0020,,Đ-Đây là...
Dialogue: 0,0:20:12.83,0:20:14.77,Default,,0000,0000,0020,,Em đồng ý làm vợ anh chứ?
Dialogue: 0,0:20:16.50,0:20:22.30,Default,,0000,0000,0020,,Shin, điều anh vừa nói khiến em rất vui, nhưng em là gái đã có chồng...
Dialogue: 0,0:20:22.70,0:20:25.00,Default,,0000,0000,0020,,Nhưng chồng của em đâu còn trên đời này nữa!
Dialogue: 0,0:20:25.37,0:20:27.83,Default,,0000,0000,0020,,Hay là em ghét anh?
Dialogue: 0,0:20:29.50,0:20:30.93,Default,,0000,0000,0020,,Vậy tại sao?
Dialogue: 0,0:20:31.17,0:20:33.97,Default,,0000,0000,0020,,Em ko thể phản bội anh ấy được...
Dialogue: 0,0:20:34.27,0:20:35.63,Default,,0000,0000,0020,,Em ko hề phản bội ai hết!
Dialogue: 0,0:20:35.83,0:20:39.87,Default,,0000,0000,0020,,Chắc hẳn chồng em trên kia cũng luôn cầu mong cho em được hạnh phúc!
Dialogue: 0,0:20:39.96,0:20:40.93,Default,,0000,0000,0020,,Nhưng mà...
Dialogue: 0,0:20:41.53,0:20:42.60,Default,,0000,0000,0020,,Sayaka!
Dialogue: 0,0:20:44.96,0:20:46.70,Default,,0000,0000,0020,,Mùi mồ hôi của anh...
Dialogue: 0,0:20:46.90,0:20:49.60,Default,,0000,0000,0020,,Nó khiến em như phát điên...
Dialogue: 0,0:20:50.67,0:20:51.97,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:20:54.30,0:20:57.40,Default,,0000,0000,0020,,Hôm nay, anh sẽ chiều em tới bến.
Dialogue: 0,0:20:58.37,0:20:59.53,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:21:11.46,0:21:11.46,Default,,0000,0000,0020,,Đây là lần cuối cùng...
Dialogue: 0,0:21:12.93,0:21:14.43,Default,,0000,0000,0020,,Là lần cuối cùng...\NSayaka...
Dialogue: 0,0:21:15.90,0:21:17.73,Default,,0000,0000,0020,,Sayaka!
Dialogue: 0,0:21:18.06,0:21:19.17,Default,,0000,0000,0020,,Sayaka!
Dialogue: 0,0:21:19.33,0:21:23.33,Default,,0000,0000,0020,,Linh hồn và thể xác chúng ta sẽ hòa hợp làm một.
Dialogue: 0,0:21:24.16,0:21:25.40,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:21:28.60,0:21:31.07,Default,,0000,0000,0020,,Shin, của anh đang bự lên nữa kìa...
Dialogue: 0,0:21:31.20,0:21:34.20,Default,,0000,0000,0020,,Đó là minh chứng cho tình yêu anh dành cho em.
Dialogue: 0,0:21:35.70,0:21:37.30,Default,,0000,0000,0020,,Sayaka, anh yêu em...
Dialogue: 0,0:21:37.43,0:21:38.57,Default,,0000,0000,0020,,Sayaka...
Dialogue: 0,0:21:38.66,0:21:39.80,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:21:40.00,0:21:41.20,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:21:41.83,0:21:43.13,Default,,0000,0000,0020,,Sayaka!
Dialogue: 0,0:21:53.20,0:21:56.73,Default,,0000,0000,0020,,Sayaka, anh sẽ ko để em rời xa anh bất cứ giây phút nào...
Dialogue: 0,0:21:56.87,0:21:58.03,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:21:58.13,0:22:00.97,Default,,0000,0000,0020,,Anh muốn được ở bên em mãi mãi...
Dialogue: 0,0:22:05.50,0:22:06.63,Default,,0000,0000,0020,,Mùi hương này...
Dialogue: 0,0:22:08.50,0:22:12.54,Default,,0000,0000,0020,,Em ko thể sống thiếu mùi hương của anh, Shin...
Dialogue: 0,0:22:13.90,0:22:15.07,Default,,0000,0000,0020,,Sayaka...
Dialogue: 0,0:22:15.77,0:22:16.77,Default,,0000,0000,0020,,Đừng rời xa em...
Dialogue: 0,0:22:16.90,0:22:18.97,Default,,0000,0000,0020,,Shin, đừng bao giờ rời xa em!
Dialogue: 0,0:22:19.73,0:22:20.80,Default,,0000,0000,0020,,Sayaka...
Dialogue: 0,0:22:21.73,0:22:23.00,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:22:26.20,0:22:30.20,Default,,0000,0000,0020,,Shin đang ra ở bên trong em...
Dialogue: 0,0:22:41.73,0:22:45.17,Default,,0000,0000,0020,,Em nghĩ mình nên thành thật với anh hơn nữa...
Dialogue: 0,0:22:45.30,0:22:46.27,Default,,0000,0000,0020,,Uhm...
Dialogue: 0,0:22:48.50,0:22:49.67,Default,,0000,0000,0020,,Sayaka...
Dialogue: 0,0:22:51.27,0:22:52.53,Default,,0000,0000,0020,,Vậy thì...
Dialogue: 0,0:22:55.70,0:22:58.07,Default,,0000,0000,0020,,Này, có ai ở đây ko?
Dialogue: 0,0:23:01.06,0:23:03.27,Default,,0000,0000,0020,,Có lẽ là do vắng khách đây mà...
Dialogue: 0,0:23:03.47,0:23:05.77,Default,,0000,0000,0020,,Nhưng mà ko ai coi hàng thế này thì thật bất cẩn quá...
Dialogue: 0,0:23:07.00,0:23:08.20,Default,,0000,0000,0020,,Chào quý khách...
Dialogue: 0,0:23:09.20,0:23:10.27,Default,,0000,0000,0020,,...Cô Yuko?
Dialogue: 0,0:23:11.76,0:23:13.27,Default,,0000,0000,0020,,Cô tới đây làm gì thế?
Dialogue: 0,0:23:13.60,0:23:15.63,Default,,0000,0000,0020,,Bộ dạng đó của anh là sao hả...
Dialogue: 0,0:23:15.73,0:23:16.90,Default,,0000,0000,0020,,...anh yêu!
Dialogue: 0,0:23:17.00,0:23:18.77,Default,,0000,0000,0020,,A-Anh yêu?
Dialogue: 0,0:23:19.13,0:23:22.20,Default,,0000,0000,0020,,Em kết anh rồi đó.
Dialogue: 0,0:23:22.33,0:23:23.37,Default,,0000,0000,0020,,Hả?
Dialogue: 0,0:23:23.70,0:23:28.10,Default,,0000,0000,0020,,Chưa người đàn ông nào đối xử thô bạo với em như anh.
Dialogue: 0,0:23:28.63,0:23:32.94,Default,,0000,0000,0020,,Vậy nên anh đã để lại cho em ấn tượng rất sâu sắc.
Dialogue: 0,0:23:34.33,0:23:36.33,Default,,0000,0000,0020,,Ehhhhhhh?
Dialogue: 0,0:23:36.43,0:23:37.57,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:23:37.70,0:23:39.60,Default,,0000,0000,0020,,Ai vậy?
Dialogue: 0,0:23:39.90,0:23:41.17,Default,,0000,0000,0020,,Um, cô ấy là...
Dialogue: 0,0:23:41.26,0:23:44.20,Default,,0000,0000,0020,,Là người rất quan trọng đối với anh ấy.
Dialogue: 0,0:23:44.30,0:23:45.27,Default,,0000,0000,0020,,Oái...
Dialogue: 0,0:23:45.37,0:23:46.40,Default,,0000,0000,0020,,Thật hả...
Dialogue: 0,0:23:46.50,0:23:48.43,Default,,0000,0000,0020,,Tôi là bà chủ cửa tiệm này.
Dialogue: 0,0:23:48.70,0:23:51.70,Default,,0000,0000,0020,,Chúng tôi đã đính hôn cùng nhau.
Dialogue: 0,0:23:51.97,0:23:53.20,Default,,0000,0000,0020,,Sayaka!
Dialogue: 0,0:23:53.30,0:23:56.97,Default,,0000,0000,0020,,Anh ấy và tôi cũng đính ước với nhau rồi!
Dialogue: 0,0:23:57.07,0:23:59.10,Default,,0000,0000,0020,,Ko, tôi ko hề đính ước gì với cô cả!
Dialogue: 0,0:24:03.97,0:24:05.53,Default,,0000,0000,0020,,Shin!
Dialogue: 0,0:24:05.87,0:24:08.90,Default,,0000,0000,0020,,Tôi mang đồ tới cho anh nè\Ndạo này thân thể tôi cứ buồn bực sao sao ấy!
Dialogue: 0,0:24:12.23,0:24:15.97,Default,,0000,0000,0020,,À há, hai cô đang giành nhau Shin đấy hả?
Dialogue: 0,0:24:19.03,0:24:21.57,Default,,0000,0000,0020,,Cho tôi tham gia với nào?
Dialogue: 0,0:24:21.76,0:24:23.77,Default,,0000,0000,0020,,Cô Masayo, cả cô cũng thế sao?
Dialogue: 0,0:24:25.23,0:24:27.20,Default,,0000,0000,0020,,Um... chuyện đó...
Dialogue: 0,0:24:27.50,0:24:32.50,Default,,0000,0000,0020,,Làm thế này thật xấu hổ, nhưng vì anh thích điều này, vậy nên em đã cố.
Dialogue: 0,0:24:41.26,0:24:45.44,Default,,0000,0000,0020,,Để đến được đây, em đã ra những hai lần đấy.
Dialogue: 0,0:24:48.86,0:24:53.20,Default,,0000,0000,0020,,Này, Shin,\NTất cả chuyện này là sao hả?
Dialogue: 0,0:24:53.33,0:24:55.87,Default,,0000,0000,0020,,Anh yêu, giải thích xem nào?
Dialogue: 0,0:24:56.83,0:24:59.57,Default,,0000,0000,0020,,Uh, chuyện này...
Dialogue: 0,0:24:59.70,0:25:04.34,Default,,0000,0000,0020,,Tất cả chỉ là chiều ý khách hàng thôi mà...
Dialogue: 0,0:25:07.90,0:25:09.73,Default,,0000,0000,0020,,Sayaka...
Dialogue: 0,0:25:10.00,0:25:11.20,Default,,0000,0000,0020,,Shin, gã ngốc!
Dialogue: 0,0:25:11.33,0:25:12.97,Default,,0000,0000,0020,,Dâm tặc! Trăng hoa!
Dialogue: 0,0:25:13.10,0:25:14.13,Default,,0000,0000,0020,,Em ghét anh!
Dialogue: 0,0:25:18.73,0:25:23.44,Default,,0000,0000,0020,,Cứ tưởng sau chuyện này, tình cảm của tôi với Sayaka coi như đi tong, nhưng...
Dialogue: 0,0:25:25.86,0:25:28.83,Default,,0000,0000,0020,,Tám tháng sau...
Dialogue: 0,0:25:34.83,0:25:36.37,Default,,0000,0000,0020,,Xong xuôi!
Dialogue: 0,0:25:36.67,0:25:40.60,Default,,0000,0000,0020,,Đừng làm gì quá sức nhé, Sayaka.
Dialogue: 0,0:25:41.00,0:25:42.13,Default,,0000,0000,0020,,Ko sao đâu anh.
Dialogue: 0,0:25:43.03,0:25:44.50,Default,,0000,0000,0020,,Tất cả đều ổn mà.
Dialogue: 0,0:25:44.93,0:25:45.90,Default,,0000,0000,0020,,Cũng phải.
Dialogue: 0,0:25:46.07,0:25:49.00,Default,,0000,0000,0020,,Có vẻ như buổi tối hôm đó ko phải thời điểm an toàn nên...
Dialogue: 0,0:25:49.13,0:25:50.73,Default,,0000,0000,0020,,Uhm, cô ấy đã mang thai.
Dialogue: 0,0:25:51.50,0:25:55.50,Default,,0000,0000,0020,,Một tháng sau đó chúng tôi quyết định làm đám cưới.
Dialogue: 0,0:25:56.40,0:25:58.40,Default,,0000,0000,0020,,Sayaka, anh đi nhé!
Dialogue: 0,0:25:58.57,0:25:59.93,Default,,0000,0000,0020,,Chúc may mắn...
Dialogue: 0,0:26:00.30,0:26:01.53,Default,,0000,0000,0020,,...Ông xã!
Dialogue: 0,0:26:03.63,0:26:05.13,Default,,0000,0000,0020,,Mình sẽ làm việc thật chăm chỉ!
Dialogue: 0,0:26:07.17,0:26:09.87,Default,,0000,0000,0020,,Phải rồi.\NNhịp mông mạnh nữa lên nào, Shin!
Dialogue: 0,0:26:10.86,0:26:12.80,Default,,0000,0000,0020,,Được rồi, cứ để chuyện này cho anh!
Dialogue: 0,0:26:13.27,0:26:15.30,Default,,0000,0000,0020,,Tuyệt quá! Shin, tuyệt quá đi mất!
Dialogue: 0,0:26:22.03,0:26:24.00,Default,,0000,0000,0020,,Tôi đã giữ bí mật chuyện này với vợ mình, Sayaka...
Dialogue: 0,0:26:24.63,0:26:26.23,Default,,0000,0000,0020,,...để duy trì sự tồn tại của cửa tiệm...
Dialogue: 0,0:26:26.33,0:26:28.93,Default,,0000,0000,0020,,...và để đổi lại việc cửa tiệm kình địch bị đóng cửa...
Dialogue: 0,0:26:29.53,0:26:31.40,Default,,0000,0000,0020,,Phải rồi...
Dialogue: 0,0:26:31.57,0:26:33.07,Default,,0000,0000,0020,,Liếm sạch nó đi.
Dialogue: 0,0:26:34.90,0:26:38.90,Default,,0000,0000,0020,,Tôi vẫn tiếp tục chiều chuộng khách hàng của mình.
Dialogue: 0,0:26:39.27,0:26:40.27,Default,,0000,0000,0020,,Giải băng này khiến em...
Dialogue: 0,0:26:40.36,0:26:42.50,Default,,0000,0000,0020,,...cảm thấy như mình đang bị cưỡng bức, tuyệt quá đi mất!
Dialogue: 0,0:26:43.90,0:26:46.50,Default,,0000,0000,0020,,Khi anh ra,\Nhãy ra lên mặt em nhé!
Dialogue: 0,0:26:46.80,0:26:48.07,Default,,0000,0000,0020,,Luôn sẵn lòng phục vụ em!
Dialogue: 0,0:27:06.36,0:27:07.57,Default,,0000,0000,0020,,Shin...
Dialogue: 0,0:27:07.90,0:27:09.60,Default,,0000,0000,0020,,Anh làm chuyện này ngày một cừ hơn đấy.
Dialogue: 0,0:27:10.00,0:27:11.30,Default,,0000,0000,0020,,Cám ơn em vì lời khen tặng.
Dialogue: 0,0:27:11.77,0:27:13.63,Default,,0000,0000,0020,,Cũng là nhờ vào sự hỗ trợ của em nữa.
Dialogue: 0,0:27:14.73,0:27:16.83,Default,,0000,0000,0020,,Anh còn vài việc cần làm, anh đi trước nhé.
Dialogue: 0,0:27:16.96,0:27:17.93,Default,,0000,0000,0020,,Sao cơ?
Dialogue: 0,0:27:18.17,0:27:20.07,Default,,0000,0000,0020,,Anh định bỏ em lại đây sao?
Dialogue: 0,0:27:20.20,0:27:23.60,Default,,0000,0000,0020,,Masayo, anh sẽ dành cho em một màn chăm sóc đặc biệt.
Dialogue: 0,0:27:24.56,0:27:25.60,Default,,0000,0000,0020,,Gì thế này?
Dialogue: 0,0:27:28.23,0:27:31.97,Default,,0000,0000,0020,,Chăm sóc kiểu này thì kì quá!
Dialogue: 0,0:27:33.80,0:27:35.50,Default,,0000,0000,0020,,Nhưng điều này thực sự rất tuyệt.
Dialogue: 0,0:27:36.06,0:27:39.37,Default,,0000,0000,0020,,Từ từ tận hưởng nó đi nhé, Masayo.
Dialogue: 0,0:27:43.53,0:27:47.00,Default,,0000,0000,0020,,Shin, anh đúng là số một!
Dialogue: 0,0:27:48.83,0:27:51.43,Default,,0000,0000,0020,,Công việc giặt ủi quả là ko dễ dàng chút nào!
