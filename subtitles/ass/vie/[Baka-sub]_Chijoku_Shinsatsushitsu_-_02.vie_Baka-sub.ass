Dialogue: 0,0:00:11.31,0:00:12.51,Default,,0000,0000,0000,,Mau lên nào!
Dialogue: 0,0:00:26.90,0:00:30.39,Default,,0000,0000,0000,,Được rồi, anh thích nơi nào?
Dialogue: 0,0:00:30.83,0:00:34.74,Default,,0000,0000,0000,,Số một hay số hai?
Dialogue: 0,0:00:35.81,0:00:36.93,Default,,0000,0000,0000,,S-Số hai!
Dialogue: 0,0:00:37.07,0:00:39.27,Default,,0000,0000,0000,,Phiền anh đóng cửa lại được ko?
Dialogue: 0,0:00:39.54,0:00:40.41,Default,,0000,0000,0000,,T-Tất nhiên rồi!
Dialogue: 0,0:00:42.95,0:00:45.01,Default,,0000,0000,0000,,Anh chưa thử hậu môn bao giờ, phải ko nào?
Dialogue: 0,0:00:45.48,0:00:48.04,Default,,0000,0000,0000,,Ko sao đâu, bình tĩnh lại nào.
Dialogue: 0,0:00:49.02,0:00:51.89,Default,,0000,0000,0000,,Nơi đó rất vừa vặn đó!
Dialogue: 0,0:00:52.92,0:00:53.85,Default,,0000,0000,0000,,Vào trong rồi!
Dialogue: 0,0:00:55.66,0:00:56.63,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:00:56.66,0:00:59.89,Default,,0000,0000,0000,,Mạnh nữa lên, hành hạ tôi đi!
Dialogue: 0,0:01:01.26,0:01:05.36,Default,,0000,0000,0000,,Ra hết vào trong hậu môn tôi đi nào.
Dialogue: 0,0:01:05.44,0:01:06.27,Default,,0000,0000,0000,,V-Vâng!
Dialogue: 0,0:01:10.31,0:01:13.67,Default,,0000,0000,0000,,Đóng cửa lại nào?
Dialogue: 0,0:01:21.58,0:01:27.15,Default,,0000,0000,0000,,Vậy là mấy kẻ cặn bã đó đã tìm được nguồn vui trời ban cho chúng.
Dialogue: 0,0:01:28.19,0:01:31.25,Default,,0000,0000,0000,,Thứ cô ta cần lúc này chính là sự "điều trị" thường xuyên...
Dialogue: 0,0:01:48.91,0:01:49.45,Default,,0000,0000,0000,,Đừng bước vào!
Dialogue: 0,0:01:49.45,0:01:50.21,Default,,0000,0000,0000,,Dr. Shameless... Phần 2\NĐừng bước vào!
Dialogue: 0,0:01:50.21,0:01:50.58,Default,,0000,0000,0000,,Dr. Shameless... Phần 2
Dialogue: 0,0:01:50.58,0:01:51.85,Default,,0000,0000,0000,,Dr. Shameless... Phần 2\NXấu hổ lắm!
Dialogue: 0,0:01:51.85,0:01:53.54,Default,,0000,0000,0000,,Dr. Shameless... Phần 2
Dialogue: 0,0:01:54.15,0:01:58.05,Default,,0000,0000,0000,,Bệnh án Mẹ và Con gái\NThứ có thể chiếm hữu
Dialogue: 0,0:01:58.19,0:01:59.99,Default,,0000,0000,0000,,B-Bác sĩ!
Dialogue: 0,0:02:00.09,0:02:04.42,Default,,0000,0000,0000,,Làm ơn giúp tôi với! Con bé vừa bị té ngã ngoài kia...
Dialogue: 0,0:02:06.53,0:02:09.12,Default,,0000,0000,0000,,E rằng con bé đã bị gãy xương ở đâu đó.
Dialogue: 0,0:02:09.30,0:02:11.60,Default,,0000,0000,0000,,Con bé đang rất đau đớn!
Dialogue: 0,0:02:14.34,0:02:16.83,Default,,0000,0000,0000,,Phòng khám
Dialogue: 0,0:02:17.84,0:02:19.00,Default,,0000,0000,0000,,Đau ko em?
Dialogue: 0,0:02:19.11,0:02:20.34,Default,,0000,0000,0000,,Ko ạ.
Dialogue: 0,0:02:20.81,0:02:21.74,Default,,0000,0000,0000,,Còn chỗ này thì sao?
Dialogue: 0,0:02:22.35,0:02:23.68,Default,,0000,0000,0000,,Ko đau chút nào ạ.
Dialogue: 0,0:02:24.95,0:02:27.21,Default,,0000,0000,0000,,Tình hình tài chính của bệnh viện đã khá hơn rất nhiều...
Dialogue: 0,0:02:27.28,0:02:31.91,Default,,0000,0000,0000,,...và bệnh nhân thì ngày càng hài lòng với cung cách phục vụ ở đây.
Dialogue: 0,0:02:32.86,0:02:36.09,Default,,0000,0000,0000,,Bác sĩ Ishida quả là một thiên tài...
Dialogue: 0,0:02:47.54,0:02:49.84,Default,,0000,0000,0000,,Toàn bộ xương và dây chằng đều ổn.
Dialogue: 0,0:02:50.81,0:02:53.04,Default,,0000,0000,0000,,Có lẽ ko cần thiết phải chụp X quang nữa.
Dialogue: 0,0:02:54.08,0:02:56.57,Default,,0000,0000,0000,,Cô bé chỉ cần dùng thuốc giảm đau và gạc là ổn.
Dialogue: 0,0:02:57.65,0:02:59.58,Default,,0000,0000,0000,,Em nên tránh các hoạt động mạnh ở vùng này trong một thời gian.
Dialogue: 0,0:02:59.75,0:03:01.31,Default,,0000,0000,0000,,Cám ơn Bác sĩ!
Dialogue: 0,0:03:01.99,0:03:04.82,Default,,0000,0000,0000,,May mà con chỉ bị thương nhẹ, Tomomi.
Dialogue: 0,0:03:05.39,0:03:07.32,Default,,0000,0000,0000,,Thật ra thì, Cô Kinoshita...
Dialogue: 0,0:03:07.59,0:03:08.32,Default,,0000,0000,0000,,Gì thế Bác sĩ?
Dialogue: 0,0:03:08.76,0:03:10.75,Default,,0000,0000,0000,,Trông sắc mặt cô nhợt nhạt quá.
Dialogue: 0,0:03:10.83,0:03:11.69,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:03:12.83,0:03:14.42,Default,,0000,0000,0000,,Để tôi khám qua cho cô nhé.
Dialogue: 0,0:03:17.77,0:03:21.67,Default,,0000,0000,0000,,Nagatsuka, phiền cô băng vết thương lại cho cô bé.
Dialogue: 0,0:03:22.54,0:03:23.80,Default,,0000,0000,0000,,Theo tôi nào.
Dialogue: 0,0:03:32.02,0:03:36.88,Default,,0000,0000,0000,,B-Bác sĩ, anh định khám chỗ nào vậy?
Dialogue: 0,0:03:37.12,0:03:38.49,Default,,0000,0000,0000,,Cô đừng lo.
Dialogue: 0,0:03:39.89,0:03:41.75,Default,,0000,0000,0000,,Hãy tin ở tôi.
Dialogue: 0,0:03:53.30,0:03:54.60,Default,,0000,0000,0000,,Mùi hương này...
Dialogue: 0,0:03:54.70,0:03:56.50,Default,,0000,0000,0000,,Lạ thật...
Dialogue: 0,0:03:58.84,0:04:00.74,Default,,0000,0000,0000,,Để tôi kiểm tra kĩ hơn.
Dialogue: 0,0:04:05.68,0:04:06.94,Default,,0000,0000,0000,,Dễ thương thật...
Dialogue: 0,0:04:07.38,0:04:10.82,Default,,0000,0000,0000,,Thật khó mà tin được cô là gái một con.
Dialogue: 0,0:04:17.69,0:04:19.32,Default,,0000,0000,0000,,C-Có vấn đề gì sao?
Dialogue: 0,0:04:19.96,0:04:22.83,Default,,0000,0000,0000,,Uhm, có vẻ như mới xuất hiện trong thời gian gần đây...
Dialogue: 0,0:04:22.93,0:04:25.26,Default,,0000,0000,0000,,...một biến thể của hội chứng STD rất phổ biến với phụ nữ trong độ tuổi này...
Dialogue: 0,0:04:22.93,0:04:26.96,Note,,0000,0000,0000,,{\i1}STD : Sexually Transmitted Disease- Bệnh truyền nhiễm qua đường sinh dục
Dialogue: 0,0:04:25.70,0:04:26.96,Default,,0000,0000,0000,,STD?
Dialogue: 0,0:04:33.48,0:04:38.44,Default,,0000,0000,0000,,Dù đó là công việc, nhưng đúng là mình đang dần mất hứng với phụ nữ trẻ.
Dialogue: 0,0:04:38.85,0:04:42.41,Default,,0000,0000,0000,,Đúng lúc thật, ngay khi mình đang thèm muốn phụ nữ trưởng thành...
Dialogue: 0,0:04:42.59,0:04:44.68,Default,,0000,0000,0000,,...thì cô ta lại tự động dẫn xác tới,\Nnhư một mồi lửa.
Dialogue: 0,0:04:44.79,0:04:47.19,Default,,0000,0000,0000,,Một cơ hội ko thể tốt hơn...
Dialogue: 0,0:05:12.28,0:05:13.51,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:05:14.28,0:05:15.34,Default,,0000,0000,0000,,Um, ko...
Dialogue: 0,0:05:17.02,0:05:18.54,Default,,0000,0000,0000,,Um, Bác sĩ?
Dialogue: 0,0:05:19.22,0:05:20.38,Default,,0000,0000,0000,,Um...
Dialogue: 0,0:05:22.16,0:05:25.42,Default,,0000,0000,0000,,Anh đang khám cho tôi thật sao?
Dialogue: 0,0:05:25.70,0:05:27.22,Default,,0000,0000,0000,,Một năm trước...
Dialogue: 0,0:05:27.30,0:05:30.43,Default,,0000,0000,0000,,...khi tôi làm việc tại L.A, với tư cách là thành viên của tổ chức y khoa đứng đầu nước Mĩ.
Dialogue: 0,0:05:31.07,0:05:33.19,Default,,0000,0000,0000,,Liệu pháp này đã được áp dụng thành công với nhiều bệnh nhân...
Dialogue: 0,0:05:33.34,0:05:36.74,Default,,0000,0000,0000,,...và trở nên nổi tiếng tới mức lọt và danh sách đề cử Nobel Y học.
Dialogue: 0,0:05:38.37,0:05:42.33,Default,,0000,0000,0000,,Hiện giờ tại Nhật Bản, tôi là người duy nhất có đủ khả năng làm điều này.
Dialogue: 0,0:05:46.62,0:05:48.95,Default,,0000,0000,0000,,Tôi làm đau cô ư?
Dialogue: 0,0:05:52.66,0:05:55.09,Default,,0000,0000,0000,,K-Ko, ko hẳn vậy...
Dialogue: 0,0:06:07.80,0:06:10.71,Default,,0000,0000,0000,,Ko có dấu hiệu viêm nhiễm nào cả.
Dialogue: 0,0:06:10.94,0:06:13.84,Default,,0000,0000,0000,,Tuy nhiên, để chắc chắn cô sẽ ko mắc phải căn bệnh này nữa.
Dialogue: 0,0:06:15.45,0:06:18.35,Default,,0000,0000,0000,,Phiền cô giữ nguyên giải băng đó, nếu để ánh sáng chiếu vào mắt, kết quả sẽ bị sai lệch.
Dialogue: 0,0:06:18.45,0:06:19.81,Default,,0000,0000,0000,,Vâng ạ...
Dialogue: 0,0:06:20.98,0:06:22.38,Default,,0000,0000,0000,,Đặt tay và đầu gối xuống nào.
Dialogue: 0,0:06:23.59,0:06:24.35,Default,,0000,0000,0000,,Vâng, thưa bác sĩ.
Dialogue: 0,0:06:26.22,0:06:27.99,Default,,0000,0000,0000,,Tôi tiến hành đây.
Dialogue: 0,0:06:28.49,0:06:32.39,Default,,0000,0000,0000,,Dù đơn giản,\Nnhưng tôi vẫn muốn xét nghiệm thật kĩ lưỡng.
Dialogue: 0,0:06:32.53,0:06:35.79,Default,,0000,0000,0000,,Nói cách khác, ko thể cẩu thả trong chuyện này được.
Dialogue: 0,0:06:37.50,0:06:40.37,Default,,0000,0000,0000,,Nếu cảm thấy khó chịu, hãy báo cho tôi ngay.
Dialogue: 0,0:06:41.14,0:06:41.90,Default,,0000,0000,0000,,Vâng, thưa Bác sĩ.
Dialogue: 0,0:06:42.61,0:06:44.16,Default,,0000,0000,0000,,Tôi đưa dụng cụ vào trong đây...
Dialogue: 0,0:06:45.94,0:06:47.74,Default,,0000,0000,0000,,L-Là thứ gì vậy?
Dialogue: 0,0:06:49.25,0:06:50.91,Default,,0000,0000,0000,,Cố thư giãn đi nào.
Dialogue: 0,0:06:51.18,0:06:53.21,Default,,0000,0000,0000,,Sắp khó chịu hơn rồi đấy.
Dialogue: 0,0:06:55.02,0:06:56.28,Default,,0000,0000,0000,,K-Ko!
Dialogue: 0,0:07:04.03,0:07:06.26,Default,,0000,0000,0000,,Tôi còn phải lấy thêm một vài vật mẫu nữa...
Dialogue: 0,0:07:13.40,0:07:14.60,Default,,0000,0000,0000,,Cô ko sao chứ?
Dialogue: 0,0:07:15.87,0:07:17.74,Default,,0000,0000,0000,,Trông cô có vẻ rất đau đớn.
Dialogue: 0,0:07:18.34,0:07:20.40,Default,,0000,0000,0000,,Ko, tôi vẫn ổn.
Dialogue: 0,0:07:20.91,0:07:23.24,Default,,0000,0000,0000,,Làm ơn, đừng dừng chuyện đó lại!
Dialogue: 0,0:07:25.08,0:07:27.71,Default,,0000,0000,0000,,Việc xét nghiệm sẽ kết thúc sau vài phút nữa.
Dialogue: 0,0:07:28.18,0:07:30.62,Default,,0000,0000,0000,,Nếu có điều gì thất thố, mong cô bỏ quá cho.
Dialogue: 0,0:08:06.12,0:08:08.65,Default,,0000,0000,0000,,Cô bỏ băng bịt mắt được rồi đó.
Dialogue: 0,0:08:10.73,0:08:13.70,Default,,0000,0000,0000,,Kết quả xét nghiệm sẽ có sau một tuần.
Dialogue: 0,0:08:14.63,0:08:18.03,Default,,0000,0000,0000,,Tôi sẽ cố gắng báo cho cô sớm nhất có thể.
Dialogue: 0,0:08:19.64,0:08:20.66,Default,,0000,0000,0000,,Cám ơn bác sĩ.
Dialogue: 0,0:08:32.58,0:08:34.91,Default,,0000,0000,0000,,Mẹ, tình hình thế nào rồi?
Dialogue: 0,0:08:35.22,0:08:38.85,Default,,0000,0000,0000,,K-Ko có gì đâu, bác sĩ chỉ hỏi thăm sức khỏe và khám sơ qua cho mẹ thôi.
Dialogue: 0,0:08:40.22,0:08:43.92,Default,,0000,0000,0000,,Chỉ vậy thôi ư? Vậy mà sắc mặt mẹ nhìn khá hẳn lên đấy.
Dialogue: 0,0:08:44.03,0:08:45.39,Default,,0000,0000,0000,,Oh, con thấy vậy sao?
Dialogue: 0,0:08:46.53,0:08:49.43,Default,,0000,0000,0000,,Mà Mẹ này, mẹ thấy bác sĩ đẹp trai ko?
Dialogue: 0,0:08:49.50,0:08:53.16,Default,,0000,0000,0000,,Vừa đẹp trai lại vừa phong độ. Chả bù với bố già nhà mình!
Dialogue: 0,0:08:54.00,0:08:54.66,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:08:55.00,0:08:55.66,Default,,0000,0000,0000,,Oh.
Dialogue: 0,0:09:09.39,0:09:10.88,Default,,0000,0000,0000,,Cô vẫn chưa ăn trưa à?
Dialogue: 0,0:09:11.42,0:09:12.08,Default,,0000,0000,0000,,Vâng ạ...
Dialogue: 0,0:09:16.69,0:09:17.85,Default,,0000,0000,0000,,Bác sĩ này?
Dialogue: 0,0:09:19.86,0:09:24.70,Default,,0000,0000,0000,,Từ khi anh đến, tình hình bệnh viện ngày một tốt hơn.
Dialogue: 0,0:09:25.17,0:09:26.93,Default,,0000,0000,0000,,Em thực sự ko biết phải cám ơn anh thế nào.
Dialogue: 0,0:09:27.64,0:09:29.33,Default,,0000,0000,0000,,Cô ko cần phải cảm ơn tôi đâu.
Dialogue: 0,0:09:29.41,0:09:33.17,Default,,0000,0000,0000,,Tôi chỉ cố gắng hoàn thành tốt công việc của mình mà thôi.
Dialogue: 0,0:09:33.48,0:09:34.53,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:09:38.78,0:09:42.01,Default,,0000,0000,0000,,Giám đốc vẫn hào hoa như xưa nhỉ.
Dialogue: 0,0:09:42.08,0:09:42.71,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:09:43.12,0:09:44.31,Default,,0000,0000,0000,,Oh, là cô ấy.
Dialogue: 0,0:09:45.02,0:09:48.32,Default,,0000,0000,0000,,Trưởng bộ phận thương mại hãng dược phẩm M.M.
Dialogue: 0,0:09:48.52,0:09:50.89,Default,,0000,0000,0000,,Chỉ thuần túy là quan hệ công việc thôi mà.
Dialogue: 0,0:09:58.03,0:09:59.06,Default,,0000,0000,0000,,Papa!
Dialogue: 0,0:10:07.04,0:10:08.27,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:10:08.34,0:10:10.04,Default,,0000,0000,0000,,Bác sĩ, anh đúng là người tuyệt vời nhất!
Dialogue: 0,0:10:34.27,0:10:35.83,Default,,0000,0000,0000,,Oh, em quên chưa nói với anh...
Dialogue: 0,0:10:35.97,0:10:37.87,Default,,0000,0000,0000,,Cô bé tên Tomomi đã ở đây chiều hôm qua?
Dialogue: 0,0:10:37.97,0:10:39.77,Default,,0000,0000,0000,,Thông tin về cô bé đã được đăng tải trên một tạp chí!
Dialogue: 0,0:10:40.01,0:10:42.48,Default,,0000,0000,0000,,Cô bé đang nắm trong tay một gia tài kếch xù.
Dialogue: 0,0:10:42.55,0:10:44.04,Default,,0000,0000,0000,,Thật khó tin!
Dialogue: 0,0:10:44.61,0:10:45.77,Default,,0000,0000,0000,,Cô nói rõ hơn xem nào?
Dialogue: 0,0:10:46.28,0:10:50.74,Default,,0000,0000,0000,,Uhm, cô bé đã tạo ra một món đồ chơi tên là Mole\NAlien, một sản phẩm đầy triển vọng.
Dialogue: 0,0:10:50.89,0:10:53.29,Default,,0000,0000,0000,,Nghe nói chúng đã được bày bán rộng rãi ở nhiều nơi.
Dialogue: 0,0:10:54.02,0:10:57.02,Default,,0000,0000,0000,,Ước gì tôi trở nên giàu có như cô bé!
Dialogue: 0,0:10:57.19,0:10:58.52,Default,,0000,0000,0000,,Hiểu rồi...
Dialogue: 0,0:10:58.96,0:11:00.69,Default,,0000,0000,0000,,Lại thế rồi!
Dialogue: 0,0:11:00.76,0:11:02.09,Default,,0000,0000,0000,,Mất hứng quá đi.
Dialogue: 0,0:11:03.07,0:11:05.53,Default,,0000,0000,0000,,Bệnh nhân lại gọi rồi.
Dialogue: 0,0:11:15.38,0:11:17.44,Default,,0000,0000,0000,,Mọi hôm anh làm chuyện này giỏi lắm mà?
Dialogue: 0,0:11:18.85,0:11:20.94,Default,,0000,0000,0000,,Mạnh nữa lên nào!
Dialogue: 0,0:11:21.02,0:11:22.99,Default,,0000,0000,0000,,Coi nào, tôi sẽ giúp anh hồi phục hoàn toàn!
Dialogue: 0,0:11:26.46,0:11:28.95,Default,,0000,0000,0000,,Thật ko dễ để...
Dialogue: 0,0:11:30.16,0:11:32.56,Default,,0000,0000,0000,,...tái hòa nhập cộng đồng, phải ko nào?
Dialogue: 0,0:11:41.74,0:11:42.50,Default,,0000,0000,0000,,Ow, nóng quá!
Dialogue: 0,0:11:45.34,0:11:49.57,Default,,0000,0000,0000,,Anh sao vậy? Tiếp tục liếm đi nào!
Dialogue: 0,0:11:58.69,0:12:00.49,Default,,0000,0000,0000,,Phải, anh tìm đúng chỗ rồi đó!
Dialogue: 0,0:12:03.86,0:12:04.99,Default,,0000,0000,0000,,Tiếp tục nào!
Dialogue: 0,0:12:05.76,0:12:07.99,Default,,0000,0000,0000,,Đút lưỡi của anh vào sâu trong hậu môn tôi nào!
Dialogue: 0,0:12:13.94,0:12:16.27,Default,,0000,0000,0000,,Chức năng của anh hồi phục nhanh thật đấy!
Dialogue: 0,0:12:16.54,0:12:19.47,Default,,0000,0000,0000,,Để thưởng công, tôi sẽ làm cho anh thật dễ chịu!
Dialogue: 0,0:12:44.90,0:12:46.03,Default,,0000,0000,0000,,Em đúng là một thiên thần...
Dialogue: 0,0:12:46.20,0:12:50.16,Default,,0000,0000,0000,,Chắc chắn là vậy, một thiên thần trong trang phục y tá!
Dialogue: 0,0:12:59.42,0:13:00.68,Default,,0000,0000,0000,,Là em hả...
Dialogue: 0,0:13:07.42,0:13:08.32,Default,,0000,0000,0000,,Bác sĩ!
Dialogue: 0,0:13:08.79,0:13:12.59,Default,,0000,0000,0000,,Chân em có vẻ khá hơn rồi đấy, nhưng vẫn phải hết sức thận trọng.
Dialogue: 0,0:13:13.63,0:13:16.46,Default,,0000,0000,0000,,Xe của anh đậu ngay gần đây.
Dialogue: 0,0:13:16.60,0:13:17.73,Default,,0000,0000,0000,,Để anh đưa em về nhà nhé?
Dialogue: 0,0:13:33.02,0:13:35.78,Default,,0000,0000,0000,,Ngày nào cũng vậy...
Dialogue: 0,0:14:45.65,0:14:47.42,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:14:49.33,0:14:52.09,Default,,0000,0000,0000,,N-Nữa đi, Bác sĩ!
Dialogue: 0,0:15:03.74,0:15:05.54,Default,,0000,0000,0000,,Tới nhà em rồi, Bác sĩ.
Dialogue: 0,0:15:06.78,0:15:08.64,Default,,0000,0000,0000,,Nhà em đẹp thật đấy.
Dialogue: 0,0:15:14.45,0:15:16.75,Default,,0000,0000,0000,,Mẹ, chúng ta có khách này!
Dialogue: 0,0:15:16.82,0:15:18.65,Default,,0000,0000,0000,,Con đã mời được Bác sĩ Ishida ghé chơi nhà ta!
Dialogue: 0,0:15:19.19,0:15:20.82,Default,,0000,0000,0000,,Bác sĩ Ishida?
Dialogue: 0,0:15:21.02,0:15:21.96,Default,,0000,0000,0000,,Chào cô.
Dialogue: 0,0:15:22.63,0:15:26.03,Default,,0000,0000,0000,,Con tình cờ gặp bác sĩ tại nhà ga, vậy nên bác sĩ đã đưa con về nhà.
Dialogue: 0,0:15:26.10,0:15:27.65,Default,,0000,0000,0000,,Cám ơn Bác sĩ!
Dialogue: 0,0:15:29.43,0:15:32.33,Default,,0000,0000,0000,,Ah, dù nhà cửa có hơi bề bộn nhưng xin bác sĩ cứ tự nhiên như ở nhà mình vậy!
Dialogue: 0,0:15:33.24,0:15:36.47,Default,,0000,0000,0000,,Bác sĩ, anh qua chơi phòng em luôn nhé.
Dialogue: 0,0:15:36.67,0:15:40.11,Default,,0000,0000,0000,,Em muốn cho anh thấy những mẫu Mole\NAlien do chính em tạo ra!
Dialogue: 0,0:15:40.34,0:15:41.64,Default,,0000,0000,0000,,Anh đang rất nóng lòng được xem chúng đây.
Dialogue: 0,0:15:41.71,0:15:42.77,Default,,0000,0000,0000,,Xin phép cô nhé?
Dialogue: 0,0:15:44.45,0:15:45.50,Default,,0000,0000,0000,,Cô Kinoshita?
Dialogue: 0,0:15:45.78,0:15:46.41,Default,,0000,0000,0000,,Vâng, Bác sĩ?
Dialogue: 0,0:15:47.22,0:15:50.91,Default,,0000,0000,0000,,Mục đích chính của tôi khi tới đây là để xem xét lại lần nữa vết thương của cô bé.
Dialogue: 0,0:15:51.59,0:15:54.45,Default,,0000,0000,0000,,Phiền chị chờ ở bên ngoài một lúc.
Dialogue: 0,0:15:55.93,0:15:57.65,Default,,0000,0000,0000,,Um, vâng ạ...
Dialogue: 0,0:15:59.66,0:16:00.89,Default,,0000,0000,0000,,Bác sĩ thấy chúng có đẹp ko?
Dialogue: 0,0:17:09.90,0:17:11.23,Default,,0000,0000,0000,,Đừng kháng cự.
Dialogue: 0,0:17:11.93,0:17:14.06,Default,,0000,0000,0000,,Chuyện đó sẽ chấm dứt ngay bây giờ thôi...
Dialogue: 0,0:17:25.18,0:17:26.34,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:17:26.62,0:17:28.71,Default,,0000,0000,0000,,Anh đang làm gì con gái tôi vậy?
Dialogue: 0,0:17:30.32,0:17:33.48,Default,,0000,0000,0000,,Mẹ, sao mẹ lại vào đây! Mẹ làm con xấu hổ quá.
Dialogue: 0,0:17:33.56,0:17:35.52,Default,,0000,0000,0000,,Bác sĩ đang trị bệnh cho con!
Dialogue: 0,0:17:36.13,0:17:37.56,Default,,0000,0000,0000,,T-Trị bệnh?
Dialogue: 0,0:17:39.16,0:17:44.00,Default,,0000,0000,0000,,Trong mọi phương thức trị bệnh, ko gì có thể sánh với việc tác động theo chiều hướng tích cực lên tâm sinh lí bệnh nhân...
Dialogue: 0,0:17:50.84,0:17:53.31,Default,,0000,0000,0000,,Cô đã hiểu sai ý tôi trong chuyện này.
Dialogue: 0,0:17:54.91,0:17:58.37,Default,,0000,0000,0000,,Có vẻ như cô mới là người cần được giải tỏa căng thẳng ở đây.
Dialogue: 0,0:18:09.02,0:18:09.75,Default,,0000,0000,0000,,Um...
Dialogue: 0,0:18:17.20,0:18:18.89,Default,,0000,0000,0000,,Giữa hai mẹ con.
Dialogue: 0,0:18:19.17,0:18:21.64,Default,,0000,0000,0000,,Hai người giống nhau một cách kì lạ.
Dialogue: 0,0:18:22.81,0:18:26.00,Default,,0000,0000,0000,,Nào, để xem bên trong hai người ra sao...
Dialogue: 0,0:18:35.08,0:18:37.25,Default,,0000,0000,0000,,Nhiệt độ, sự ẩm ướt, cảm giác đụng chạm...
Dialogue: 0,0:18:37.59,0:18:39.49,Default,,0000,0000,0000,,Thật khó để tìm ra điểm khác nhau giữa hai người.
Dialogue: 0,0:18:41.09,0:18:42.46,Default,,0000,0000,0000,,Sâu hơn nữa nào...
Dialogue: 0,0:18:56.74,0:19:00.23,Default,,0000,0000,0000,,Cô Kinoshita, sinh lực của cô ko thua gì một phụ nữ trẻ...
Dialogue: 0,0:19:00.91,0:19:04.64,Default,,0000,0000,0000,,...nhưng dù ướt đến mức này,\Ncô vẫn ko thể sánh bằng con gái mình.
Dialogue: 0,0:19:05.01,0:19:07.21,Default,,0000,0000,0000,,Lượng dâm thủy chính là sự khác biệt rõ rệt nhất.
Dialogue: 0,0:19:08.72,0:19:09.91,Default,,0000,0000,0000,,Oh, xấu hổ quá...
Dialogue: 0,0:19:11.85,0:19:16.31,Default,,0000,0000,0000,,Ước gì mẹ trẻ lại bằng tuổi con, Tomomi!
Dialogue: 0,0:19:16.69,0:19:18.66,Default,,0000,0000,0000,,Cô đừng buồn.
Dialogue: 0,0:19:19.90,0:19:23.89,Default,,0000,0000,0000,,Nếu phụ nữ chăm sóc tốt cơ thể của mình...
Dialogue: 0,0:19:24.17,0:19:27.69,Default,,0000,0000,0000,,...sắc đẹp của họ sẽ ít chịu tác động của tuổi tác...
Dialogue: 0,0:19:29.61,0:19:34.44,Default,,0000,0000,0000,,Như lúc này đây, cô còn quyến rũ hơn lúc trẻ bội phần...
Dialogue: 0,0:19:40.45,0:19:43.61,Default,,0000,0000,0000,,Cơ thể tôi đang rộn lên...
Dialogue: 0,0:19:50.06,0:19:52.29,Default,,0000,0000,0000,,Bác sĩ, cả em nữa...
Dialogue: 0,0:19:52.43,0:19:53.72,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:19:53.80,0:19:55.96,Default,,0000,0000,0000,,Với tư cách bác sĩ,tôi sẽ chăm sóc cả hai người.
Dialogue: 0,0:19:56.70,0:19:57.89,Default,,0000,0000,0000,,Tôi ra rồi...
Dialogue: 0,0:19:58.43,0:19:59.46,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:20:00.24,0:20:03.00,Default,,0000,0000,0000,,Em ko chịu nổi nữa rồi, em phát điên lên mất!
Dialogue: 0,0:20:04.34,0:20:07.90,Default,,0000,0000,0000,,Nhu cầu sinh lí là một phần tất yếu của tự nhiên.
Dialogue: 0,0:20:08.64,0:20:10.81,Default,,0000,0000,0000,,Cố kìm nén điều đó sẽ gây hại cho bản thân .
Dialogue: 0,0:20:17.42,0:20:20.45,Default,,0000,0000,0000,,Nhân tiện, để tôi kiểm tra vùng ruột của hai người luôn thể.
Dialogue: 0,0:20:21.49,0:20:24.82,Default,,0000,0000,0000,,Nguy cơ ung thư đường ruột của bệnh nhân ngày nay đang tăng cao...
Dialogue: 0,0:20:29.33,0:20:30.59,Default,,0000,0000,0000,,Xin phép!
Dialogue: 0,0:20:32.83,0:20:36.00,Default,,0000,0000,0000,,Có lẽ Bác sĩ Ishida vẫn chưa về...
Dialogue: 0,0:20:44.15,0:20:45.74,Default,,0000,0000,0000,,Bác sĩ Ishida...
Dialogue: 0,0:20:51.99,0:20:53.18,Default,,0000,0000,0000,,Tôi về rồi đây?
Dialogue: 0,0:20:57.96,0:20:59.55,Default,,0000,0000,0000,,Chà, mệt chết mất.
Dialogue: 0,0:20:59.86,0:21:02.92,Default,,0000,0000,0000,,Đến kiệt sức với mấy trò nhảy nhót của sếp mất thôi...
Dialogue: 0,0:21:16.14,0:21:17.87,Default,,0000,0000,0000,,Mẹ con nó đâu cả rồi?
Dialogue: 0,0:21:33.96,0:21:35.40,Default,,0000,0000,0000,,Oh Trời ơi!
Dialogue: 0,0:21:35.96,0:21:37.43,Default,,0000,0000,0000,,Chuyện gì thế này?
Dialogue: 0,0:21:37.57,0:21:39.43,Default,,0000,0000,0000,,Mày là thằng quái nào?
Dialogue: 0,0:21:40.14,0:21:41.30,Default,,0000,0000,0000,,Ông ta là ai?
Dialogue: 0,0:21:43.37,0:21:45.07,Default,,0000,0000,0000,,Mặc kệ ông ta đi.
Dialogue: 0,0:21:45.54,0:21:46.67,Default,,0000,0000,0000,,Ông bố vô dụng!
Dialogue: 0,0:21:47.01,0:21:50.21,Default,,0000,0000,0000,,Mặc kệ là thế nào?\NCon vừa gọi ai là kẻ vô dụng hả?
Dialogue: 0,0:21:50.31,0:21:52.68,Default,,0000,0000,0000,,Hai mẹ con học đâu ra kiểu ăn nói đó hả?\NTôi là chủ của cái nhà này đấy!
Dialogue: 0,0:21:53.12,0:21:55.58,Default,,0000,0000,0000,,Ông thôi ngay đi, sự thật là ông đã bị công ty sa thải từ lâu...
Dialogue: 0,0:21:55.65,0:21:58.21,Default,,0000,0000,0000,,...vậy mà sáng nào ông cũng giả bộ tới công sở làm việc!
Dialogue: 0,0:21:58.39,0:22:00.18,Default,,0000,0000,0000,,Ông tưởng mẹ con tôi ko biết chuyện đó sao?
Dialogue: 0,0:22:00.66,0:22:03.96,Default,,0000,0000,0000,,Chưa kể toàn bộ chi phí sinh hoạt của nhà này đều do một tay mẹ con tôi lo liệu!
Dialogue: 0,0:22:04.13,0:22:07.43,Default,,0000,0000,0000,,Một khi ông tiếp tục lợi dụng Tomomi\Nđể thanh toán nợ nần cho ông...
Dialogue: 0,0:22:08.20,0:22:09.66,Default,,0000,0000,0000,,...thì chuyện này tất yếu sẽ xảy ra, đồ vô dụng!
Dialogue: 0,0:22:09.87,0:22:11.42,Default,,0000,0000,0000,,Hiểu rồi...
Dialogue: 0,0:22:11.77,0:22:15.36,Default,,0000,0000,0000,,Nếu đã như vậy, sao cô ko bỏ quách ông ta đi?
Dialogue: 0,0:22:15.84,0:22:19.50,Default,,0000,0000,0000,,Khi đó hai người có thể thoải mái sử dụng tiền bạc theo cách của mình.
Dialogue: 0,0:22:20.51,0:22:23.41,Default,,0000,0000,0000,,Số tiền đó có thể giúp ích cho cộng đồng rất nhiều!
Dialogue: 0,0:22:23.98,0:22:27.24,Default,,0000,0000,0000,,Ví dụ như, nếu hai người tài trợ số tiền đó cho bệnh viện của tôi...
Dialogue: 0,0:22:27.45,0:22:30.08,Default,,0000,0000,0000,,...rất nhiều bệnh nhân nghèo sẽ được cứu!
Dialogue: 0,0:22:30.19,0:22:31.41,Default,,0000,0000,0000,,Em sẽ tài trợ cho bệnh viện của anh!
Dialogue: 0,0:22:31.49,0:22:34.22,Default,,0000,0000,0000,,Chỉ cần làm Bác sĩ vui,\Nmuốn em tài trợ bao nhiêu cũng được!
Dialogue: 0,0:22:34.26,0:22:36.72,Default,,0000,0000,0000,,Hai người mất trí rồi sao?
Dialogue: 0,0:22:36.76,0:22:39.82,Default,,0000,0000,0000,,Này, các người làm gì vậy?
Dialogue: 0,0:22:39.86,0:22:42.73,Default,,0000,0000,0000,,Thả tôi ra!
Dialogue: 0,0:23:20.57,0:23:23.60,Default,,0000,0000,0000,,Bác sĩ, làm ơn điều trị cho cả tôi nữa!
Dialogue: 0,0:23:23.64,0:23:24.90,Default,,0000,0000,0000,,Bác sĩ!
Dialogue: 0,0:23:34.52,0:23:37.18,Default,,0000,0000,0000,,Thật sâu, sâu bên trong!
Dialogue: 0,0:24:26.94,0:24:30.67,Default,,0000,0000,0000,,Bụng em đang nóng ran lên!
Dialogue: 0,0:24:44.02,0:24:46.51,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:24:51.89,0:24:54.52,Default,,0000,0000,0000,,Việc trị bệnh hôm nay đến đây thôi nhỉ...
Dialogue: 0,0:24:54.80,0:24:56.96,Default,,0000,0000,0000,,T-Tôi sắp...
Dialogue: 0,0:24:57.93,0:24:58.76,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:24:58.93,0:25:01.63,Default,,0000,0000,0000,,Tuyệt quá đi mất, tôi chết mất!
Dialogue: 0,0:25:01.70,0:25:04.50,Default,,0000,0000,0000,,Dùng từ đó trước mặt một bác sĩ thật ko hay chút nào.
Dialogue: 0,0:25:06.14,0:25:08.61,Default,,0000,0000,0000,,Nhưng tôi ko thể kìm nén nổi!
Dialogue: 0,0:25:16.99,0:25:20.42,Default,,0000,0000,0000,,Cũng ko trách cô được, khi phải sống với một "nhà cung cấp" thế kia...
Dialogue: 0,0:25:21.16,0:25:23.65,Default,,0000,0000,0000,,Làm ơn cho tôi tham gia với!
Dialogue: 0,0:25:28.83,0:25:33.43,Default,,0000,0000,0000,,Từ bây giờ Bác sĩ sẽ là "nhà cung cấp" cho mẹ con tôi.
Dialogue: 0,0:25:48.42,0:25:50.78,Default,,0000,0000,0000,,Bắt quả tang, bắt quả tang!
Dialogue: 0,0:25:58.43,0:25:59.65,Default,,0000,0000,0000,,Biết ngay mà!
Dialogue: 0,0:25:59.73,0:26:01.53,Default,,0000,0000,0000,,Ko, cô nhầm rồi!
Dialogue: 0,0:26:01.80,0:26:07.79,Default,,0000,0000,0000,,Sao cũng được, bác sĩ rất ghét nhân viên làm chuyện mờ ám sau lưng anh ấy.
Dialogue: 0,0:26:08.00,0:26:12.63,Default,,0000,0000,0000,,Nếu biết chuyện này, chắc chắn anh ấy sẽ rời khỏi bệnh viện.
Dialogue: 0,0:26:12.71,0:26:13.87,Default,,0000,0000,0000,,Ko, ko thể như vậy...
Dialogue: 0,0:26:14.31,0:26:18.30,Default,,0000,0000,0000,,Nếu cô muốn anh ấy ở lại, phải ngoan ngoãn nghe lời tôi!
Dialogue: 0,0:26:18.38,0:26:19.01,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:26:19.41,0:26:20.47,Default,,0000,0000,0000,,Cô làm gì vậy?
Dialogue: 0,0:26:20.55,0:26:21.64,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:26:21.85,0:26:25.08,Default,,0000,0000,0000,,Để tôi giúp cô được sung sướng nhé!
Dialogue: 0,0:26:35.90,0:26:39.63,Default,,0000,0000,0000,,Ông chủ thực sự của gia đình phải hội tụ đủ các tố chất...
Dialogue: 0,0:26:40.10,0:26:44.94,Default,,0000,0000,0000,,Bự, cứng và ẩm ướt...
Dialogue: 0,0:26:45.14,0:26:49.01,Default,,0000,0000,0000,,Trích sổ tay cá nhân của Shinji
