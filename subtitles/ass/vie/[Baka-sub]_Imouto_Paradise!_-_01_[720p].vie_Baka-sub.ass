Dialogue: 0,0:00:01.39,0:00:04.33,Imouto Paradise Dialogue,,0000,0000,0000,,Nhìn như vậy làm em xấu hổ lắm.
Dialogue: 0,0:00:04.33,0:00:07.25,Imouto Paradise Dialogue,,0000,0000,0000,,Quả nhiên là ngực của em nhỏ hơn của Rio-chan.
Dialogue: 0,0:00:07.59,0:00:11.05,Imouto Paradise Dialogue,,0000,0000,0000,,Hừm, Aniki đúng là đồ biến thái!
Dialogue: 0,0:00:11.05,0:00:13.74,Imouto Paradise Dialogue,,0000,0000,0000,,Bọn em chỉ làm như vầy lần này thôi đấy nhé.
Dialogue: 0,0:00:13.74,0:00:17.20,Imouto Paradise Dialogue,,0000,0000,0000,,Aya! Tuyệt quá!
Dialogue: 0,0:00:17.20,0:00:22.40,Imouto Paradise Dialogue,,0000,0000,0000,,Em sẽ làm như trong Ero game mà anh thích, anh cứ ra thoải mái đi nhé.
Dialogue: 0,0:00:22.62,0:00:26.25,Imouto Paradise Dialogue,,0000,0000,0000,,Bọn em cosplay như những nhân vật Anime mà anh thích đấy.
Dialogue: 0,0:00:26.25,0:00:28.90,Imouto Paradise Dialogue,,0000,0000,0000,,Hãy ra thật nhiều trên mặt em!
Dialogue: 0,0:00:29.42,0:00:32.88,Imouto Paradise Dialogue,,0000,0000,0000,,Không! Đừng ra trên trang phục của em...
Dialogue: 0,0:00:32.88,0:00:35.68,Imouto Paradise Dialogue,,0000,0000,0000,,mà hãy bắn hết lên mặt của em!
Dialogue: 0,0:00:36.59,0:00:40.77,Imouto Paradise Dialogue,,0000,0000,0000,,Tuyệt quá, nó ra thật nhiều trên mặt em.
Dialogue: 0,0:00:41.12,0:00:42.61,Imouto Paradise Dialogue,,0000,0000,0000,,Anh ra đây!
Dialogue: 0,0:00:42.61,0:00:43.30,Imouto Paradise Dialogue,,0000,0000,0000,,Anh cứ ra đi!
Dialogue: 0,0:00:43.30,0:00:45.99,Imouto Paradise Dialogue,,0000,0000,0000,,Hãy cho em tinh dịch của anh!
Dialogue: 0,0:00:48.77,0:00:51.29,Imouto Paradise Dialogue,,0000,0000,0000,,Nii-san, ra nhiều tinh dịch quá đi mất.
Dialogue: 0,0:00:51.29,0:00:53.36,Imouto Paradise Dialogue,,0000,0000,0000,,Nhưng anh vẫn còn cứng như thế này.
Dialogue: 0,0:00:54.44,0:00:56.58,Imouto Paradise Dialogue,,0000,0000,0000,,Nó vẫn còn lớn quá chừng.
Dialogue: 0,0:00:56.58,0:00:58.83,Imouto Paradise Dialogue,,0000,0000,0000,,Thằng nhỏ của anh rất là hứng tình.
Dialogue: 0,0:00:58.94,0:01:00.06,Imouto Paradise Dialogue,,0000,0000,0000,,Đến lúc trừng phạt rồi.
Dialogue: 0,0:01:00.06,0:01:03.33,Imouto Paradise Dialogue,,0000,0000,0000,,Không! Đừng đâm vào em sâu như vậy!
Dialogue: 0,0:01:03.56,0:01:05.38,Imouto Paradise Dialogue,,0000,0000,0000,,Em sẽ ra mất!
Dialogue: 0,0:01:05.52,0:01:08.24,Imouto Paradise Dialogue,,0000,0000,0000,,Em xin lỗi vì mình thật dâm đãng, Nii-san!
Dialogue: 0,0:01:08.24,0:01:09.71,Imouto Paradise Dialogue,,0000,0000,0000,,Em là một con điếm biến thái!
Dialogue: 0,0:01:09.71,0:01:12.38,Imouto Paradise Dialogue,,0000,0000,0000,,Quả nhiên em là một con điếm cosplay biến thái!
Dialogue: 0,0:01:12.38,0:01:14.56,Imouto Paradise Dialogue,,0000,0000,0000,,Mạnh nữa lên! Hãy trừng phạt em đi!
Dialogue: 0,0:01:14.56,0:01:17.35,Imouto Paradise Dialogue,,0000,0000,0000,,Hãy phạt em mạnh hơn nữa! Đâm sâu hơn nữa!
Dialogue: 0,0:01:18.22,0:01:19.63,Imouto Paradise Dialogue,,0000,0000,0000,,Rio, em đúng là biến thái.
Dialogue: 0,0:01:19.73,0:01:21.79,Imouto Paradise Dialogue,,0000,0000,0000,,Ăn mặc như vầy, hứng tình như vầy.
Dialogue: 0,0:01:21.81,0:01:24.39,Imouto Paradise Dialogue,,0000,0000,0000,,Không, là Aniki biến thái thì có!
Dialogue: 0,0:01:25.04,0:01:26.69,Imouto Paradise Dialogue,,0000,0000,0000,,Không được! Đừng đâm sâu như vậy!
Dialogue: 0,0:01:26.69,0:01:28.70,Imouto Paradise Dialogue,,0000,0000,0000,,Em sẽ vỡ mất!
Dialogue: 0,0:01:29.39,0:01:32.97,Imouto Paradise Dialogue,,0000,0000,0000,,Thằng nhỏ thật dâm đãng, làm ơn hãy trao nó cho Aya!
Dialogue: 0,0:01:33.24,0:01:35.52,Imouto Paradise Dialogue,,0000,0000,0000,,Em muốn tinh địch của anh ở bên trong em!
Dialogue: 0,0:01:36.84,0:01:38.15,Imouto Paradise Dialogue,,0000,0000,0000,,Anh ra đây! Anh ra đây!
Dialogue: 0,0:01:38.15,0:01:40.39,Imouto Paradise Dialogue,,0000,0000,0000,,Hãy trừng phạt em vì em là một cô gái hư!
Dialogue: 0,0:01:42.19,0:01:44.67,Imouto Paradise Dialogue,,0000,0000,0000,,Em ra đây!
Dialogue: 0,0:01:54.00,0:01:54.84,Imouto Paradise Dialogue,,0000,0000,0000,,Ơ?
Dialogue: 0,0:01:56.07,0:01:58.30,Imouto Paradise Dialogue,,0000,0000,0000,,Thật là một giấc mơ đẹp.
