Dialogue: 0,0:00:45.00,0:00:55.00,Baka,,0000,0000,0000,,{\clip(83,29,708,76)\fad(2000,0)\pos(415,44)}Baka-sub trân trọng giới thiệu
Dialogue: 0,0:01:00.00,0:01:01.00,EN,,0000,0000,0000,,Không thể tin nổi.
Dialogue: 0,0:01:01.50,0:01:04.00,EN,,0000,0000,0000,,Quan hệ loạn luân như vầy.
Dialogue: 0,0:01:05.00,0:01:08.00,EN,,0000,0000,0000,,Onii-chan! Em yêu anh... Em yêu anh!
Dialogue: 0,0:01:08.50,0:01:10.00,EN,,0000,0000,0000,,Không được... Em lại sắp ra nữa rồi!
Dialogue: 0,0:01:11.00,0:01:14.00,EN,,0000,0000,0000,,Kazuhisa chết bầm... lại xem phim như thế này.
Dialogue: 0,0:01:22.60,0:01:24.00,EN,,0000,0000,0000,,Nhưng... cũng không tệ...
Dialogue: 0,0:01:48.70,0:01:51.50,EN,,0000,0000,0000,,Không... Cảm giác kỳ lạ này là sao?
Dialogue: 0,0:02:15.00,0:02:16.00,EN,,0000,0000,0000,,Akane-san?
Dialogue: 0,0:02:17.00,0:02:21.00,EN,,0000,0000,0000,,Em đang làm gì trong phòng của anh vậy?
Dialogue: 0,0:02:27.00,0:02:29.00,EN,,0000,0000,0000,,Mừng anh đã về, Ba-Kazuhisa.
Dialogue: 0,0:02:29.30,0:02:30.50,EN,,0000,0000,0000,,"Mừng" cái đầu em!
Dialogue: 0,0:02:31.00,0:02:35.00,EN,,0000,0000,0000,,Nhìn thế nào đi chăng nữa thì anh cũng là một tên cuồng Loli-Em gái!
Dialogue: 0,0:02:35.60,0:02:38.00,EN,,0000,0000,0000,,Biến thái gì đâu!
Dialogue: 0,0:02:38.50,0:02:41.00,EN,,0000,0000,0000,,Em không muốn như vầy!
Dialogue: 0,0:02:41.00,0:02:42.50,EN,,0000,0000,0000,,Dễ thương lắm, Manami.
Dialogue: 0,0:02:43.00,0:02:44.00,EN,,0000,0000,0000,,Xấu hổ quá!!
Dialogue: 0,0:02:44.70,0:02:46.00,EN,,0000,0000,0000,,"Xấu hổ quá!!"
Dialogue: 0,0:02:46.70,0:02:47.50,EN,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:02:47.60,0:02:48.50,EN,,0000,0000,0000,,AKANE!!
Dialogue: 0,0:02:49.00,0:02:50.00,EN,,0000,0000,0000,,Trả điều khiển đây!!
Dialogue: 0,0:02:51.70,0:02:53.00,EN,,0000,0000,0000,,Đừng có mơ!
Dialogue: 0,0:02:55.00,0:02:57.00,EN,,0000,0000,0000,,Nếu muốn thì nhào vô!
Dialogue: 0,0:02:57.00,0:02:57.80,EN,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:02:58.00,0:02:59.50,EN,,0000,0000,0000,,Sao thế? Cố gắng lên chứ!
Dialogue: 0,0:03:07.00,0:03:08.00,EN,,0000,0000,0000,,Cưng cứ mơ đi!
Dialogue: 0,0:03:12.00,0:03:15.00,EN,,0000,0000,0000,,Đúng rồi! Công tắc TV!
Dialogue: 0,0:03:15.70,0:03:18.00,EN,,0000,0000,0000,,Sao chúng ta không xem cùng nhau?
Dialogue: 0,0:03:22.00,0:03:22.70,EN,,0000,0000,0000,,Akane-san.
Dialogue: 0,0:03:23.00,0:03:25.50,EN,,0000,0000,0000,,Thả anh ra!
Dialogue: 0,0:03:26.00,0:03:28.00,EN,,0000,0000,0000,,Em sẽ phát điên mất!
Dialogue: 0,0:03:30.00,0:03:32.00,EN,,0000,0000,0000,,Mấy cảnh phim này khập khiễng quá đi!
Dialogue: 0,0:03:32.70,0:03:35.00,EN,,0000,0000,0000,,Ngực cô ta to thật đấy!
Dialogue: 0,0:03:35.70,0:03:36.50,EN,,0000,0000,0000,,Xem lại mình đi!
Dialogue: 0,0:03:42.00,0:03:46.00,EN,,0000,0000,0000,,Mà này, lúc nào anh cũng hứng khi xem những phim như vầy ư?
Dialogue: 0,0:03:46.20,0:03:47.00,EN,,0000,0000,0000,,Im đê!
Dialogue: 0,0:03:47.80,0:03:50.50,EN,,0000,0000,0000,,Nếu có thời gian xem phim sao không đi kiếm bạn gái đi?
Dialogue: 0,0:03:50.70,0:03:51.70,EN,,0000,0000,0000,,Im đê!!
Dialogue: 0,0:03:52.00,0:03:56.00,EN,,0000,0000,0000,,Tại vì thế mà anh vẫn còn zin đấy.
Dialogue: 0,0:03:56.50,0:03:57.80,EN,,0000,0000,0000,,O... nii-chan...
Dialogue: 0,0:03:58.80,0:03:59.70,EN,,0000,0000,0000,,Anh bảo IM rồi mà!!
Dialogue: 0,0:04:00.00,0:04:03.00,EN,,0000,0000,0000,,A ha... Cáu rồi ư!
Dialogue: 0,0:04:05.00,0:04:07.00,EN,,0000,0000,0000,,Sợ quá! Sợ quá!
Dialogue: 0,0:04:11.00,0:04:14.00,EN,,0000,0000,0000,,Rồi rồi! Không phiền anh nữa!
Dialogue: 0,0:04:16.00,0:04:17.00,EN,,0000,0000,0000,,Cứ tự nhiên!
Dialogue: 0,0:04:23.00,0:04:25.00,EN,,0000,0000,0000,,Chờ đã, anh đang làm gì vậy!?
Dialogue: 0,0:04:28.00,0:04:29.50,EN,,0000,0000,0000,,Nghiêm túc đấy chứ?
Dialogue: 0,0:04:31.00,0:04:34.00,EN,,0000,0000,0000,,Chờ đã... Bình tĩnh lại! Kazuhisa!
Dialogue: 0,0:04:34.70,0:04:35.50,EN,,0000,0000,0000,,Tiêu rồi!
Dialogue: 0,0:04:36.00,0:04:37.00,EN,,0000,0000,0000,,Anh ấy phát cuồng rồi!
Dialogue: 0,0:04:41.00,0:04:42.00,EN,,0000,0000,0000,,Anh làm gì vậy?
Dialogue: 0,0:04:51.00,0:04:52.00,EN,,0000,0000,0000,,Lưỡi của anh ấy...
Dialogue: 0,0:04:54.00,0:04:56.00,EN,,0000,0000,0000,,Không được... Chúng ta là anh em mà.
Dialogue: 0,0:04:58.00,0:05:01.00,EN,,0000,0000,0000,,Không được... Tại vì bộ phim lúc nãy...
Dialogue: 0,0:05:07.00,0:05:08.00,EN,,0000,0000,0000,,Này này!
Dialogue: 0,0:05:08.30,0:05:10.50,EN,,0000,0000,0000,,Nếu cứ như vầy, thì sẽ nguy mất...
Dialogue: 0,0:05:14.00,0:05:15.50,EN,,0000,0000,0000,,Tỉnh lại đi! Đồ ngốc!
Dialogue: 0,0:05:16.00,0:05:18.00,EN,,0000,0000,0000,,Anh biết mình đang làm gì không hả!?
Dialogue: 0,0:05:27.30,0:05:29.50,EN,,0000,0000,0000,,Nè...! Có nghe không hả?
Dialogue: 0,0:05:32.00,0:05:35.50,EN,,0000,0000,0000,,Không! Mình đang cảm thấy nó.
Dialogue: 0,0:05:39.50,0:05:40.50,EN,,0000,0000,0000,,Chờ đã!
Dialogue: 0,0:05:42.50,0:05:44.00,EN,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:05:44.00,0:05:45.50,EN,,0000,0000,0000,,Đây là... chỗ đó của Akane...
Dialogue: 0,0:05:46.50,0:05:47.30,EN,,0000,0000,0000,,Đừng nhìn...!
Dialogue: 0,0:05:49.50,0:05:50.50,EN,,0000,0000,0000,,Không thể nào!!
Dialogue: 0,0:06:15.50,0:06:18.00,EN,,0000,0000,0000,,K-Không được...! Dừng lại đi!
Dialogue: 0,0:06:20.00,0:06:24.00,EN,,0000,0000,0000,,Ở đây? Hay ở đây?
Dialogue: 0,0:06:25.00,0:06:27.00,EN,,0000,0000,0000,,Không được... Không được!
Dialogue: 0,0:06:28.00,0:06:30.00,EN,,0000,0000,0000,,Chúng ta là anh em đấy! Đồ ngốc!
Dialogue: 0,0:06:36.00,0:06:39.00,EN,,0000,0000,0000,,Dù chúng ta là anh em...
Dialogue: 0,0:06:41.00,0:06:42.00,EN,,0000,0000,0000,,Cũng chả sao.
Dialogue: 0,0:06:42.50,0:06:47.50,EN,,0000,0000,0000,,Dù có làm gì... thì quan hệ của chúng ta... cũng không thay đổi!
Dialogue: 0,0:06:48.50,0:06:51.00,EN,,0000,0000,0000,,Bây giờ nó thay đổi rồi!
Dialogue: 0,0:06:52.00,0:06:56.00,EN,,0000,0000,0000,,Vì Kazuhisa... đã vào bên trong em!
Dialogue: 0,0:06:58.00,0:07:02.00,EN,,0000,0000,0000,,Mình không thể chịu được... thứ cứng và mập như vầy!
Dialogue: 0,0:07:05.00,0:07:06.50,EN,,0000,0000,0000,,Tuyệt quá... Akane!
Dialogue: 0,0:07:43.50,0:07:45.50,EN,,0000,0000,0000,,A!! Bự quá!
Dialogue: 0,0:07:46.00,0:07:49.50,EN,,0000,0000,0000,,Đồ ngốc! Đừng có nhìn chằm chằm như vậy!
Dialogue: 0,0:07:55.60,0:07:56.50,EN,,0000,0000,0000,,Không được!
Dialogue: 0,0:08:00.00,0:08:01.50,EN,,0000,0000,0000,,Không! Đừng cử động!
Dialogue: 0,0:08:17.60,0:08:21.30,EN,,0000,0000,0000,,Thế nào hả, Akane? Anh không còn là trai tơ nữa rồi!
Dialogue: 0,0:08:24.00,0:08:27.00,EN,,0000,0000,0000,,Anh nói gì vậy? Em vẫn còn chưa ra mà.
Dialogue: 0,0:08:29.00,0:08:31.00,EN,,0000,0000,0000,,Nó chả sướng gì cả.
Dialogue: 0,0:08:36.00,0:08:39.00,EN,,0000,0000,0000,,Chết tiệt!! Em dám giỡn mặt với anh!
Dialogue: 0,0:08:40.00,0:08:43.50,EN,,0000,0000,0000,,Sao? Thế nào hả!?
Dialogue: 0,0:08:49.00,0:08:50.00,EN,,0000,0000,0000,,Không sướng...
Dialogue: 0,0:08:53.00,0:08:56.00,EN,,0000,0000,0000,,Thế sao nó lại thắt chặt lấy anh?
Dialogue: 0,0:08:57.00,0:08:59.00,EN,,0000,0000,0000,,Không có!
Dialogue: 0,0:09:00.00,0:09:04.00,EN,,0000,0000,0000,,Tư thế này khiến anh có thể thấy tất cả hậu môn quyến rũ của Akane!!
Dialogue: 0,0:09:09.00,0:09:10.00,EN,,0000,0000,0000,,Xấu hổ không!?
Dialogue: 0,0:09:11.00,0:09:13.70,EN,,0000,0000,0000,,Bên trong Akane sướng lắm!
Dialogue: 0,0:09:14.30,0:09:15.70,EN,,0000,0000,0000,,Không đời nào có chuyện đó!
Dialogue: 0,0:09:19.00,0:09:23.00,EN,,0000,0000,0000,,Ôi ôi...! Sao thế Akane? Sướng lắm phải không?
Dialogue: 0,0:09:28.00,0:09:30.50,EN,,0000,0000,0000,,Sướng lắm đúng không?
Dialogue: 0,0:09:31.50,0:09:33.00,EN,,0000,0000,0000,,Nói với anh nó sướng lắm đi!
Dialogue: 0,0:09:34.00,0:09:35.00,EN,,0000,0000,0000,,Không... Không!
Dialogue: 0,0:09:37.00,0:09:39.00,EN,,0000,0000,0000,,Không ư? Nếu em không chịu nói, anh sẽ dừng lại!
Dialogue: 0,0:09:41.00,0:09:41.70,EN,,0000,0000,0000,,Không!
Dialogue: 0,0:09:43.00,0:09:44.30,EN,,0000,0000,0000,,"Không" cái gì!?
Dialogue: 0,0:09:44.70,0:09:48.00,EN,,0000,0000,0000,,Em muốn ra ư? Hay em muốn dừng lại?
Dialogue: 0,0:09:53.70,0:09:54.80,EN,,0000,0000,0000,,Nhanh nói đi nào!
Dialogue: 0,0:09:54.90,0:09:56.70,EN,,0000,0000,0000,,Nói "Thằnh nhỏ của Onii-chan sướng lắm".
Dialogue: 0,0:10:09.00,0:10:10.80,EN,,0000,0000,0000,,O... Onii-chan!!
Dialogue: 0,0:10:12.00,0:10:14.00,EN,,0000,0000,0000,,Tha cho em...
Dialogue: 0,0:10:15.70,0:10:18.70,EN,,0000,0000,0000,,Onii-chan!! Tha cho em...
Dialogue: 0,0:10:20.30,0:10:21.00,EN,,0000,0000,0000,,AKANE!!
Dialogue: 0,0:10:36.00,0:10:37.00,EN,,0000,0000,0000,,E-Em sắp ra...
Dialogue: 0,0:10:45.00,0:10:47.50,EN,,0000,0000,0000,,T-Tuyệt quá!! Em ra đây...
Dialogue: 0,0:10:49.00,0:10:51.00,EN,,0000,0000,0000,,Onii-chan... Anh sắp làm Akane ra...
Dialogue: 0,0:10:55.00,0:11:00.00,EN,,0000,0000,0000,,Anh sắp làm em ra với thằng nhỏ của mình...
Dialogue: 0,0:11:01.00,0:11:03.00,EN,,0000,0000,0000,,Em ra đây... En ra đây!!
Dialogue: 0,0:11:06.50,0:11:07.00,EN,,0000,0000,0000,,AKANE!!
Dialogue: 0,0:11:07.70,0:11:08.50,EN,,0000,0000,0000,,ONII-CHAN!!
Dialogue: 0,0:11:22.00,0:11:24.00,EN,,0000,0000,0000,,Anh đang nói gì vậy hả?
Dialogue: 0,0:11:26.00,0:11:28.30,EN,,0000,0000,0000,,Nếu muốn thì hãy cầu xin đi nào!
Dialogue: 0,0:11:31.00,0:11:32.00,EN,,0000,0000,0000,,Làm ơn!!
Dialogue: 0,0:11:32.30,0:11:34.50,EN,,0000,0000,0000,,Cầu xin cho đàng hoàng vào!
Dialogue: 0,0:11:40.40,0:11:42.00,EN,,0000,0000,0000,,Anh về rồi đây.
Dialogue: 0,0:11:44.00,0:11:46.00,EN,,0000,0000,0000,,Ba-Kazuhisa...
Dialogue: 0,0:11:47.00,0:11:51.00,EN,,0000,0000,0000,,Rõ ràng là anh đang muốn huấn luyện em mà! Ba-Kazuhisa!!
Dialogue: 0,0:11:51.70,0:11:53.50,EN,,0000,0000,0000,,Hợp quá đi mất!
Dialogue: 0,0:12:09.40,0:12:11.00,EN,,0000,0000,0000,,Ấm quá.
Dialogue: 0,0:12:13.00,0:12:14.70,EN,,0000,0000,0000,,Là bánh bao thịt.
Dialogue: 0,0:12:15.70,0:12:17.00,EN,,0000,0000,0000,,Mềm quá.
Dialogue: 0,0:12:22.00,0:12:25.00,EN,,0000,0000,0000,,Cả một đống bánh bao thịt... Sướng quá!
Dialogue: 0,0:12:26.00,0:12:29.00,EN,,0000,0000,0000,,Nhưng không hẳn là mình thích bánh bao thịt.
Dialogue: 0,0:12:46.00,0:12:47.50,EN,,0000,0000,0000,,Là ngực...
Dialogue: 0,0:12:51.00,0:12:52.00,EN,,0000,0000,0000,,Mềm quá.
Dialogue: 0,0:12:52.70,0:12:53.60,EN,,0000,0000,0000,,Ngực...
Dialogue: 0,0:12:57.00,0:12:59.00,EN,,0000,0000,0000,,Này... Yasuda!
Dialogue: 0,0:13:00.00,0:13:03.00,EN,,0000,0000,0000,,Em... Em xin lỗi!!
Dialogue: 0,0:13:05.70,0:13:10.00,EN,,0000,0000,0000,,Cậu mơ cái quái gì vậy hả... Yasuda!?
Dialogue: 0,0:13:13.70,0:13:14.50,EN,,0000,0000,0000,,Yumiko-san!
Dialogue: 0,0:13:14.60,0:13:15.00,EN,,0000,0000,0000,,Ừm.
Dialogue: 0,0:13:19.50,0:13:21.60,EN,,0000,0000,0000,,M-Mình nhớ ra rồi...
Dialogue: 0,0:13:23.30,0:13:28.00,EN,,0000,0000,0000,,Mình đoán buổi tiệc đã bị Yumiko-san quậy tung cả lên.
Dialogue: 0,0:13:30.00,0:13:33.70,EN,,0000,0000,0000,,Chị ấy uống quá nhiều...
Dialogue: 0,0:13:34.00,0:13:36.50,EN,,0000,0000,0000,,Nên mình phải cõng chị ấy về...
Dialogue: 0,0:13:38.00,0:13:38.70,EN,,0000,0000,0000,,Ơ...
Dialogue: 0,0:13:42.00,0:13:43.00,EN,,0000,0000,0000,,Nước đây ạ...
Dialogue: 0,0:13:44.00,0:13:45.00,EN,,0000,0000,0000,,C-Cảm ơn!
Dialogue: 0,0:13:46.00,0:13:47.00,EN,,0000,0000,0000,,Đưa đây cho chị!
Dialogue: 0,0:13:52.20,0:13:54.50,EN,,0000,0000,0000,,Hừ... Onee-chan!!
Dialogue: 0,0:14:00.40,0:14:01.00,EN,,0000,0000,0000,,Yumiko-san...
Dialogue: 0,0:14:08.30,0:14:11.00,EN,,0000,0000,0000,,Yumiko-san!! Có người đang nhìn đấy!
Dialogue: 0,0:14:13.00,0:14:14.00,EN,,0000,0000,0000,,Không sao đâu!
Dialogue: 0,0:14:14.70,0:14:16.00,EN,,0000,0000,0000,,Đây là em gái tôi, Aki-chan.
Dialogue: 0,0:14:18.00,0:14:22.00,EN,,0000,0000,0000,,Em là em gái của chỉ... hân hạnh được biết anh!
Dialogue: 0,0:14:23.20,0:14:24.30,EN,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:14:26.00,0:14:29.00,EN,,0000,0000,0000,,O-NI-I-CHAN ư?
Dialogue: 0,0:14:31.00,0:14:34.80,EN,,0000,0000,0000,,Tôi muốn cậu lấy đi trinh tiết của con bé!
Dialogue: 0,0:14:35.80,0:14:37.50,EN,,0000,0000,0000,,Yasuda-kun!
Dialogue: 0,0:14:43.00,0:14:45.00,EN,,0000,0000,0000,,C-Chị nói gì vậy!?
Dialogue: 0,0:14:45.40,0:14:47.00,EN,,0000,0000,0000,,Chờ... Yumiko-san!
Dialogue: 0,0:14:47.40,0:14:49.00,EN,,0000,0000,0000,,Em chuẩn bị rồi chứ, Aki-chan?
Dialogue: 0,0:14:49.10,0:14:50.50,EN,,0000,0000,0000,,Hãy nhìn cho kỹ vào!
Dialogue: 0,0:14:52.00,0:14:55.00,EN,,0000,0000,0000,,Đây là hôn gợi tình đấy.
Dialogue: 0,0:15:07.00,0:15:08.00,EN,,0000,0000,0000,,Onee-chan.
Dialogue: 0,0:15:08.50,0:15:10.50,EN,,0000,0000,0000,,Thằng nhỏ của Onii-chan... trông lạ quá.
Dialogue: 0,0:15:11.50,0:15:14.50,EN,,0000,0000,0000,,Khi tắm em đã thấy của bố rồi... Nhưng cái này khác quá.
Dialogue: 0,0:15:15.00,0:15:17.00,EN,,0000,0000,0000,,Đấy gọi là chứng hẹp bao qui đầu.
Dialogue: 0,0:15:17.50,0:15:19.00,EN,,0000,0000,0000,,Hẹp bao qui đầu...
Dialogue: 0,0:15:20.00,0:15:22.80,EN,,0000,0000,0000,,Nghĩa là cậu ta có một thằng nhỏ rất đặc biệt!
Dialogue: 0,0:15:24.00,0:15:25.50,EN,,0000,0000,0000,,C-Cái đó...
Dialogue: 0,0:15:25.80,0:15:28.00,EN,,0000,0000,0000,,Hãy hôn nó đi!
Dialogue: 0,0:15:34.00,0:15:38.00,EN,,0000,0000,0000,,Rồi đưa nó vào trong miệng.
Dialogue: 0,0:15:38.10,0:15:39.50,EN,,0000,0000,0000,,Nhẹ nhàng thôi đấy, Aki-chan.
Dialogue: 0,0:15:49.00,0:15:49.80,EN,,0000,0000,0000,,Onee-chan!
Dialogue: 0,0:15:50.00,0:15:51.70,EN,,0000,0000,0000,,Bao qui đầu co lại rồi này...
Dialogue: 0,0:15:52.00,0:15:55.00,EN,,0000,0000,0000,,Tiếp theo hãy liếm nó đi!
Dialogue: 0,0:16:01.00,0:16:03.00,EN,,0000,0000,0000,,Nó có mùi lạ quá.
Dialogue: 0,0:16:03.50,0:16:04.30,EN,,0000,0000,0000,,Không sao đâu!
Dialogue: 0,0:16:04.70,0:16:06.30,EN,,0000,0000,0000,,Em sẽ dần thích nó thôi.
Dialogue: 0,0:16:10.00,0:16:14.20,EN,,0000,0000,0000,,Sau đó, hãy bú nó đi! Aki-chan!
Dialogue: 0,0:16:25.00,0:16:28.00,EN,,0000,0000,0000,,Sướng lắm không? Yasuda-kun!?
Dialogue: 0,0:16:28.50,0:16:30.00,EN,,0000,0000,0000,,Sướng lắm!
Dialogue: 0,0:16:31.00,0:16:36.00,EN,,0000,0000,0000,,Aki-chan, không chỉ làm với thằng nhỏ... mà hãy liếm núm vú của cậu ta nữa.
Dialogue: 0,0:16:52.20,0:16:55.50,EN,,0000,0000,0000,,Em đã chuẩn bị xong rồi phải không, Aki-chan!?
Dialogue: 0,0:17:01.00,0:17:04.00,EN,,0000,0000,0000,,Ô? Nó đã ướt như thế này rồi...
Dialogue: 0,0:17:10.00,0:17:14.00,EN,,0000,0000,0000,,Dâm quá! Có một vết bẩn thật lớn trên quần lót chỉ vì em bú nó.
Dialogue: 0,0:17:17.00,0:17:18.50,EN,,0000,0000,0000,,Yu... Yumiko-san?
Dialogue: 0,0:17:20.00,0:17:22.30,EN,,0000,0000,0000,,Yasuda-kun giữ im lặng chút nào.
Dialogue: 0,0:17:26.00,0:17:26.80,EN,,0000,0000,0000,,Onee-chan!
Dialogue: 0,0:18:06.00,0:18:08.00,EN,,0000,0000,0000,,O... Onee-chan... Em không thể...
Dialogue: 0,0:18:14.00,0:18:16.00,EN,,0000,0000,0000,,Dù cho chị chạm thế nào... nó cũng không lớn lên đâu.
Dialogue: 0,0:18:18.80,0:18:20.80,EN,,0000,0000,0000,,Nhưng nó đã lớn lên rồi này.
Dialogue: 0,0:18:27.00,0:18:29.50,EN,,0000,0000,0000,,Nó sướng lắm phải không?
Dialogue: 0,0:18:30.00,0:18:31.50,EN,,0000,0000,0000,,Trông nó lớn lên cứ như dương vật vậy.
Dialogue: 0,0:18:35.00,0:18:41.00,EN,,0000,0000,0000,,Dâm thủy đang trào ra từ chỗ này của Aki-chan... như muốn nói "Em thèm muốn dương vật".
Dialogue: 0,0:18:48.00,0:18:49.00,EN,,0000,0000,0000,,Onee-chan... Em...
Dialogue: 0,0:18:50.00,0:18:51.00,EN,,0000,0000,0000,,Nào, bắt đầu thôi.
Dialogue: 0,0:18:57.00,0:19:00.00,EN,,0000,0000,0000,,Onee-chan... Nó không vừa đâu.
Dialogue: 0,0:19:01.00,0:19:04.00,EN,,0000,0000,0000,,Không sau đâu! Nó đã ướt lắm rồi.
Dialogue: 0,0:19:05.80,0:19:08.00,EN,,0000,0000,0000,,Tư thế này sẽ giúp em không bị đau.
Dialogue: 0,0:19:08.80,0:19:10.00,EN,,0000,0000,0000,,Nào, Aki-chan.
Dialogue: 0,0:19:11.00,0:19:16.00,EN,,0000,0000,0000,,O... Onii-chan... Em muốn thằng nhỏ của anh biến em thành phụ nữ. Onii-chan.!
Dialogue: 0,0:19:30.00,0:19:33.00,EN,,0000,0000,0000,,Thằng nhỏ của anh ấy vào rồi.
Dialogue: 0,0:19:34.00,0:19:37.30,EN,,0000,0000,0000,,Cảm giác thằng nhỏ đầu tiên của em thế nào, Aki-chan?
Dialogue: 0,0:19:38.00,0:19:39.00,EN,,0000,0000,0000,,Em không biết.
Dialogue: 0,0:19:41.50,0:19:43.00,EN,,0000,0000,0000,,Nó có đau không?
Dialogue: 0,0:19:44.00,0:19:46.50,EN,,0000,0000,0000,,Nó đang lấp đầy bên trong em.
Dialogue: 0,0:19:48.00,0:19:50.60,EN,,0000,0000,0000,,Vậy thì hãy di chuyển đi, Yasuda-kun!
Dialogue: 0,0:19:51.00,0:19:52.60,EN,,0000,0000,0000,,Anh di chuyển đây, Aki-chan.
Dialogue: 0,0:20:03.70,0:20:06.00,EN,,0000,0000,0000,,Sướng lắm... Aki-chan!
Dialogue: 0,0:20:27.00,0:20:29.00,EN,,0000,0000,0000,,Yasuda-kun... Tôi nữa!
Dialogue: 0,0:20:30.00,0:20:33.00,EN,,0000,0000,0000,,Thôi nào, ở đây này. Đừng đứng nhìn như vậy!
Dialogue: 0,0:20:37.00,0:20:39.00,EN,,0000,0000,0000,,Phải... Chỗ đó... Chỗ đó!
Dialogue: 0,0:20:47.00,0:20:49.00,EN,,0000,0000,0000,,Là chỗ đó... Âm vật của tôi!
Dialogue: 0,0:20:53.70,0:20:58.50,EN,,0000,0000,0000,,Sướng quá... Âm vật của tôi sướng quá.
Dialogue: 0,0:21:04.50,0:21:07.00,EN,,0000,0000,0000,,Aki-chan! Đừng... Đừng chơi với núm vú của chị.
Dialogue: 0,0:21:09.00,0:21:12.00,EN,,0000,0000,0000,,Onee-chan... chỗ đó của Aki nóng quá.
Dialogue: 0,0:21:14.00,0:21:16.00,EN,,0000,0000,0000,,Aki-chan... Onee-chan cũng nóng lắm.
Dialogue: 0,0:21:32.00,0:21:33.50,EN,,0000,0000,0000,,Thằng nhỏ sướng quá đi.
Dialogue: 0,0:21:34.00,0:21:35.00,EN,,0000,0000,0000,,Em yêu nó!
Dialogue: 0,0:21:43.00,0:21:47.00,EN,,0000,0000,0000,,Thằng nhỏ vào rồi. Đừng làm đột ngột như vậy.
Dialogue: 0,0:21:49.00,0:21:53.00,EN,,0000,0000,0000,,Em không thể dừng lại được... sau khi liếm âm hộ của chị.
Dialogue: 0,0:22:13.00,0:22:13.90,EN,,0000,0000,0000,,Nó...
Dialogue: 0,0:22:21.00,0:22:23.50,EN,,0000,0000,0000,,Dâm thủy của chúng tôi đang hòa vào nhau.
Dialogue: 0,0:22:24.00,0:22:27.00,EN,,0000,0000,0000,,Dâm thủy của Onee-chan và Aki-chan đang hòa vào nhau.
Dialogue: 0,0:22:40.00,0:22:43.00,EN,,0000,0000,0000,,Tiếp theo hãy làm như vầy... Yasuda-kun!
Dialogue: 0,0:22:48.00,0:22:53.00,EN,,0000,0000,0000,,Yasuda-kun... Âm hộ của chúng tôi nóng quá.
Dialogue: 0,0:22:56.00,0:22:58.00,EN,,0000,0000,0000,,Onii-chan!! Onee-chan!!
Dialogue: 0,0:23:24.70,0:23:28.00,EN,,0000,0000,0000,,Yu... Yumiko-san!! Aki-chan!!
Dialogue: 0,0:23:31.70,0:23:35.00,EN,,0000,0000,0000,,Cậu sắp ra phải không? Cậu sắp ra rồi đúng không?
Dialogue: 0,0:23:35.70,0:23:37.00,EN,,0000,0000,0000,,Cứ ra đi!
Dialogue: 0,0:23:42.00,0:23:43.00,EN,,0000,0000,0000,,ONEE-CHAN!!
Dialogue: 0,0:23:43.10,0:23:44.50,EN,,0000,0000,0000,,AKI-CHAN!!
Dialogue: 0,0:24:00.00,0:24:01.00,EN,,0000,0000,0000,,Ra là vậy...
Dialogue: 0,0:24:01.30,0:24:03.50,EN,,0000,0000,0000,,Aki-chan tìm được bạn trai rồi ư?
Dialogue: 0,0:24:04.00,0:24:07.50,EN,,0000,0000,0000,,Con bé nói "Nhờ Onii-chan mà em đã có dũng khí để thổ lộ".
Dialogue: 0,0:24:08.00,0:24:09.00,EN,,0000,0000,0000,,Vậy à...
Dialogue: 0,0:24:09.70,0:24:12.50,EN,,0000,0000,0000,,Onee-chan! Onii-chan!
Dialogue: 0,0:24:14.00,0:24:14.70,EN,,0000,0000,0000,,Anh biết không...
Dialogue: 0,0:24:16.00,0:24:17.50,EN,,0000,0000,0000,,Bạn trai của em...
Dialogue: 0,0:24:18.00,0:24:22.00,EN,,0000,0000,0000,,Cậu ấy không bị hẹp bao qui đầu giống như Onii-chan!
Dialogue: 0,0:24:29.00,0:24:33.00,EN,,0000,0000,0000,,Mình... không muốn sống nữa.
Dialogue: 0,0:24:30.00,0:24:32.50,EN,,0000,0000,0000,,Em giỏi lắm! Aki!
Dialogue: 0,0:24:36.00,0:24:38.00,EN,,0000,0000,0000,,Ơ? Onii-chan bị làm sao thế?
Dialogue: 0,0:24:38.00,0:24:40.00,EN,,0000,0000,0000,,
