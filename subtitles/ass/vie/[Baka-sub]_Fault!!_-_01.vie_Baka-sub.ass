Dialogue: 0,0:00:39.10,0:00:40.07,Default,,0000,0000,0000,,Để tớ đỡ cho!
Dialogue: 0,0:00:44.57,0:00:46.43,Default,,0000,0000,0000,,Cậu...
Dialogue: 0,0:00:47.51,0:00:48.53,Default,,0000,0000,0000,,Mio, thơm!
Dialogue: 0,0:00:51.79,0:00:52.36,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:00:55.73,0:00:57.43,Default,,0000,0000,0000,,Home run nhé Shuu-niichan!
Dialogue: 0,0:00:59.16,0:01:04.17,Default,,0000,0000,0000,,Trận đấu kết thúc, chiến thắng thuộc về Mio-chan và Kei-chan!
Dialogue: 0,0:01:05.37,0:01:09.21,Default,,0000,0000,0000,,Này! Shuuichi! Sao cậu noob vậy hả!
Dialogue: 0,0:01:10.20,0:01:13.36,Default,,0000,0000,0000,,Từ lúc đến đây là cậu mất tập trung lắm đấy.
Dialogue: 0,0:01:13.96,0:01:16.14,Default,,0000,0000,0000,,Đây không phải thi đấu cá nhân đâu!
Dialogue: 0,0:01:19.06,0:01:19.99,Default,,0000,0000,0000,,Xin lỗi!
Dialogue: 0,0:01:20.40,0:01:21.29,Default,,0000,0000,0000,,Mình xin lỗi!
Dialogue: 0,0:01:21.88,0:01:26.41,Default,,0000,0000,0000,,Sugiyama-kun... tại phát đánh của mình quá nhẹ.
Dialogue: 0,0:01:26.88,0:01:31.03,Default,,0000,0000,0000,,Không, là lỗi tại tớ đã làm vướng chân cậu.
Dialogue: 0,0:01:31.37,0:01:33.30,Default,,0000,0000,0000,,K-không phải vậy đâu!
Dialogue: 0,0:01:33.87,0:01:38.02,Default,,0000,0000,0000,,Sau khi chắn quả đó xong thì em nên làm gì?
Dialogue: 0,0:01:38.72,0:01:41.80,Default,,0000,0000,0000,,Đã vậy tốt nhất là em chú ý vào điểm công của họ.
Dialogue: 0,0:01:42.52,0:01:46.14,Default,,0000,0000,0000,,Lúc đó, ta có thể di chuyển đối lập.
Dialogue: 0,0:01:46.88,0:01:50.72,Default,,0000,0000,0000,,Ừ... như thế có khi hay hơn.
Dialogue: 0,0:01:51.04,0:01:56.03,Default,,0000,0000,0000,,Chúng ta có thể dùng cách đó để dương đông kích tây.
Dialogue: 0,0:01:56.18,0:02:00.76,Default,,0000,0000,0000,,S-Sugiyama-kun, chúng mình\N cùng nghĩ chiến thuật gì đi nhé!
Dialogue: 0,0:02:02.84,0:02:04.23,Default,,0000,0000,0000,,Sugiyama-kun?
Dialogue: 0,0:02:06.12,0:02:07.86,Default,,0000,0000,0000,,Ớ... xin lỗi, chuyện gì cơ?
Dialogue: 0,0:02:10.72,0:02:13.90,Default,,0000,0000,0000,,Không sao... lần sau chúng mình cùng cố gắng nhé!
Dialogue: 0,0:02:18.26,0:02:24.10,Title,,0000,0000,0000,,{\fad(500,1200)\an3\pos(232,66)}Baka-sub present
Dialogue: 0,0:02:18.26,0:02:24.10,Credits,,0000,0000,0000,,{\fad(500,1200)\an3}Translator: Frozen \N Editor: Black Cat \N QC: Dark Knight
Dialogue: 0,0:02:26.02,0:02:32.44,Title,,0000,0000,0000,,{\fad(150,600)\pos(283,224)}Episode 1
Dialogue: 0,0:02:26.02,0:02:32.44,Title,,0000,0000,0000,,{\fad(150,600)\pos(491,275)}"Hãy làm điều anh muốn..."
Dialogue: 0,0:02:33.84,0:02:38.45,Default,,0000,0000,0000,,Mình thực sự nên nói chuyện thêm với Sugiyama-kun...
Dialogue: 0,0:02:46.53,0:02:48.88,Default,,0000,0000,0000,,Giờ.. thì anh có chạy đằng trời.
Dialogue: 0,0:02:53.86,0:02:54.81,Default,,0000,0000,0000,,Mio-san?
Dialogue: 0,0:03:11.80,0:03:15.40,Default,,0000,0000,0000,,Chỗ này của Shuu-niichan thật tuyệt!
Dialogue: 0,0:03:16.08,0:03:18.80,Default,,0000,0000,0000,,Làm thế này sẽ khiến anh cương lên chứ?
Dialogue: 0,0:03:19.70,0:03:22.00,Default,,0000,0000,0000,,Mio... dừng lại đi...
Dialogue: 0,0:03:22.70,0:03:24.01,Default,,0000,0000,0000,,Cho dù chúng ta chỉ là anh em họ...
Dialogue: 0,0:03:24.15,0:03:25.44,Default,,0000,0000,0000,,... nhưng việc này...
Dialogue: 0,0:03:26.24,0:03:29.14,Default,,0000,0000,0000,,Đừng nói dối, Shuu-niichan.
Dialogue: 0,0:03:29.67,0:03:32.26,Default,,0000,0000,0000,,Nhìn em thôi cũng đủ khiến anh cương lên rồi phải không?
Dialogue: 0,0:03:33.25,0:03:35.99,Default,,0000,0000,0000,,Và xuất như điên với con cu cương cứng của anh?
Dialogue: 0,0:03:36.73,0:03:37.72,Default,,0000,0000,0000,,Việc đó...
Dialogue: 0,0:03:39.07,0:03:44.75,Default,,0000,0000,0000,,Nè, anh muốn em làm nữa phải không?\N Anh đang muốn em làm những việc dâm đãng với anh à!
Dialogue: 0,0:03:45.27,0:03:47.30,Default,,0000,0000,0000,,Không phải... nếu em nói vậy...
Dialogue: 0,0:03:47.49,0:03:49.01,Default,,0000,0000,0000,,Vậy...
Dialogue: 0,0:03:49.25,0:03:52.81,Default,,0000,0000,0000,,Em có một dịch vụ nhỏ cho anh...
Dialogue: 0,0:03:55.69,0:03:58.69,Default,,0000,0000,0000,,Anh muốn thấy chỗ này của em phải không nào?
Dialogue: 0,0:04:03.87,0:04:07.53,Default,,0000,0000,0000,,A thật tuyệt! Nó thật cương cứng!
Dialogue: 0,0:04:08.03,0:04:10.36,Default,,0000,0000,0000,,Anh dâm thật đấy!
Dialogue: 0,0:04:10.79,0:04:14.83,Default,,0000,0000,0000,,Anh nứng lên chỉ vì cô em bé bỏng động vào nó.
Dialogue: 0,0:04:16.15,0:04:16.95,Default,,0000,0000,0000,,Anh...
Dialogue: 0,0:04:17.38,0:04:19.12,Default,,0000,0000,0000,,Không phải là tên dâm tặc...
Dialogue: 0,0:04:20.78,0:04:24.84,Default,,0000,0000,0000,,Này, chất nhờn này gọi là "xuất trước" phải không nào?
Dialogue: 0,0:04:25.13,0:04:28.36,Default,,0000,0000,0000,,Điều đó nghĩa là anh đang hứng!
Dialogue: 0,0:04:31.29,0:04:33.54,Default,,0000,0000,0000,,Anh dâm quá đi à!
Dialogue: 0,0:04:34.20,0:04:38.48,Default,,0000,0000,0000,,Nhưng, em lại thích anh như thế!
Dialogue: 0,0:04:39.72,0:04:43.22,Default,,0000,0000,0000,,Vì vậy, em sẽ chơi với anh thêm.
Dialogue: 0,0:04:43.44,0:04:46.91,Default,,0000,0000,0000,,Em sẽ cọ con trym ướt át của anh.
Dialogue: 0,0:04:48.15,0:04:50.32,Default,,0000,0000,0000,,Em cũng đang rất kích thích đúng không nào?
Dialogue: 0,0:04:50.72,0:04:52.96,Default,,0000,0000,0000,,A-anh nói gì vậy hả?
Dialogue: 0,0:04:53.37,0:04:55.72,Default,,0000,0000,0000,,Còn lâu em mới thấy kích thích!
Dialogue: 0,0:04:58.29,0:05:01.49,Default,,0000,0000,0000,,Anh sắp xuất rồi chứ gì?
Dialogue: 0,0:05:01.62,0:05:05.96,Default,,0000,0000,0000,,Anh sắp xuất vì cô em gái đang nghịch thằng em của anh phải không nào?
Dialogue: 0,0:05:06.24,0:05:08.98,Default,,0000,0000,0000,,Cứ như thật... chỉ thế mà đòi...
Dialogue: 0,0:05:08.62,0:05:10.49,Default,,0000,0000,0000,,Nói dối!
Dialogue: 0,0:05:10.85,0:05:11.87,Default,,0000,0000,0000,,Đã vậy...
Dialogue: 0,0:05:13.57,0:05:15.85,Default,,0000,0000,0000,,Cho anh biết thế nào là dâm thật...
Dialogue: 0,0:05:16.83,0:05:21.53,Default,,0000,0000,0000,,Cho em thấy anh dâm đến thế nào nào!
Dialogue: 0,0:05:22.17,0:05:23.35,Default,,0000,0000,0000,,M-Mio!
Dialogue: 0,0:05:28.43,0:05:33.39,Default,,0000,0000,0000,,Thật tuyệt... nó thật nóng và đặc...
Dialogue: 0,0:05:34.56,0:05:36.55,Default,,0000,0000,0000,,Nó vẫn còn cương này...
Dialogue: 0,0:05:46.88,0:05:49.08,Default,,0000,0000,0000,,Được rồi, hãy làm những gì anh muốn!
Dialogue: 0,0:05:49.55,0:05:54.87,Default,,0000,0000,0000,,Bộ ngực bé nhỏ của em có lẽ không làm anh vừa lòng, nhưng...
Dialogue: 0,0:05:58.38,0:06:03.44,Default,,0000,0000,0000,,Ở dưới này em đang cảm thấy rất nóng...
Dialogue: 0,0:06:04.04,0:06:07.04,Default,,0000,0000,0000,,Mio... Đừng đi xa hơn nữa...
Dialogue: 0,0:06:08.22,0:06:11.41,Default,,0000,0000,0000,,Anh ghét em đến thế sao?
Dialogue: 0,0:06:12.28,0:06:14.52,Default,,0000,0000,0000,,Hay... anh cảm thấy hối hận?
Dialogue: 0,0:06:15.21,0:06:16.15,Default,,0000,0000,0000,,Việc đó...
Dialogue: 0,0:06:17.44,0:06:21.55,Default,,0000,0000,0000,,Ngay từ đầu em đã không cảm thấy hối hận vì việc này.
Dialogue: 0,0:06:22.30,0:06:23.16,Default,,0000,0000,0000,,Vì vậy...
Dialogue: 0,0:06:23.56,0:06:24.77,Default,,0000,0000,0000,,Xin anh đừng...
Dialogue: 0,0:06:26.44,0:06:28.03,Default,,0000,0000,0000,,Căm ghét em...
Dialogue: 0,0:06:31.47,0:06:32.17,Default,,0000,0000,0000,,Mio...
Dialogue: 0,0:06:38.57,0:06:39.67,Default,,0000,0000,0000,,Đau lắm phải không nào?
Dialogue: 0,0:06:40.02,0:06:41.11,Default,,0000,0000,0000,,Em không nên cố!
Dialogue: 0,0:06:41.56,0:06:44.11,Default,,0000,0000,0000,,K-Không sao mà!
Dialogue: 0,0:06:44.38,0:06:47.63,Default,,0000,0000,0000,,E-Em không thấy đau gì cả!
Dialogue: 0,0:06:47.86,0:06:49.47,Default,,0000,0000,0000,,Đến lúc anh di chuyển rồi đấy.
Dialogue: 0,0:06:49.97,0:06:53.25,Default,,0000,0000,0000,,Em sẽ làm anh thấy sướng!
Dialogue: 0,0:06:56.70,0:07:01.86,Default,,0000,0000,0000,,Shuu-niichan... Anh thật dâm... làm mạnh thế này...
Dialogue: 0,0:07:01.93,0:07:05.53,Default,,0000,0000,0000,,Là do em dám khiêu khích anh!
Dialogue: 0,0:07:07.13,0:07:08.65,Default,,0000,0000,0000,,Shuu-niichan...
Dialogue: 0,0:07:12.97,0:07:15.23,Default,,0000,0000,0000,,Giống như... nó to...
Dialogue: 0,0:07:15.38,0:07:17.23,Default,,0000,0000,0000,,Mio... Mio!
Dialogue: 0,0:07:18.61,0:07:22.13,Default,,0000,0000,0000,,Vậy anh thực sự thích thân thể của em...!
Dialogue: 0,0:07:22.92,0:07:26.66,Default,,0000,0000,0000,,Là bởi vì... bởi vì anh là một tên dâm đãng!
Dialogue: 0,0:07:27.24,0:07:29.96,Default,,0000,0000,0000,,Em cũng rất dâm đấy!
Dialogue: 0,0:07:30.16,0:07:32.93,Default,,0000,0000,0000,,Làm việc này với anh trai mình.
Dialogue: 0,0:07:35.66,0:07:38.26,Default,,0000,0000,0000,,Đúng vậy! Em thật hư hỏng!
Dialogue: 0,0:07:38.61,0:07:42.22,Default,,0000,0000,0000,,Em thấy nỗi loạn khi chúng ta làm tình!
Dialogue: 0,0:07:43.20,0:07:45.86,Default,,0000,0000,0000,,Vậy... mạnh nữa đi anh...
Dialogue: 0,0:07:50.04,0:07:53.51,Default,,0000,0000,0000,,Shuu-niichan... đang bên trong em...
Dialogue: 0,0:07:55.37,0:07:57.56,Default,,0000,0000,0000,,Thật tuyệt... thật là tuyệt!
Dialogue: 0,0:07:59.59,0:08:05.09,Default,,0000,0000,0000,,Thật sướng... Mạnh nữa... mạnh nữa đi anh!
Dialogue: 0,0:08:05.20,0:08:06.60,Default,,0000,0000,0000,,Hãy làm mạnh nữa vào...
Dialogue: 0,0:08:06.97,0:08:09.04,Default,,0000,0000,0000,,Mio... Mio!
Dialogue: 0,0:08:09.97,0:08:12.68,Default,,0000,0000,0000,,Em không... giữ nổi nữa rồi...
Dialogue: 0,0:08:12.86,0:08:15.11,Default,,0000,0000,0000,,Em sắp ra rồi... em sắp ra rồi!
Dialogue: 0,0:08:16.76,0:08:20.03,Default,,0000,0000,0000,,Được rồi, thoải mái đi em!
Dialogue: 0,0:08:23.97,0:08:26.62,Default,,0000,0000,0000,,Của em...
Dialogue: 0,0:08:26.69,0:08:29.72,Default,,0000,0000,0000,,Nó đang ra...
Dialogue: 0,0:08:30.92,0:08:33.00,Default,,0000,0000,0000,,Nó ra rồi... ra rồi...
Dialogue: 0,0:08:36.03,0:08:38.69,Default,,0000,0000,0000,,Mio... Anh cũng...
Dialogue: 0,0:08:42.19,0:08:44.50,Default,,0000,0000,0000,,Không sao... Chúng ta hãy làm thêm lần nữa nhé!
Dialogue: 0,0:08:46.34,0:08:47.11,Default,,0000,0000,0000,,Mio...
Dialogue: 0,0:08:53.99,0:08:58.54,Default,,0000,0000,0000,,Ơ.. a? Họ vừa... hôn nhau... ư?
Dialogue: 0,0:09:02.49,0:09:07.97,Default,,0000,0000,0000,,Điều này là bình thường khi họ là anh em chứ?
Dialogue: 0,0:09:14.43,0:09:16.60,Default,,0000,0000,0000,,Nhưng.. sao...?
Dialogue: 0,0:09:18.14,0:09:22.06,Default,,0000,0000,0000,,sao tim mình... lại thấy đau thế này?
Dialogue: 0,0:09:24.05,0:09:26.25,Default,,0000,0000,0000,,Nào, vạch nó ra nào!
Dialogue: 0,0:09:26.56,0:09:31.75,Default,,0000,0000,0000,,Việc này... thật xấu hổ!
Dialogue: 0,0:09:31.80,0:09:34.57,Default,,0000,0000,0000,,Thật là một cơ thể dâm đãng!
Dialogue: 0,0:09:34.67,0:09:39.46,Default,,0000,0000,0000,,không, đừng nhìn cơ thể dâm đãng của em như vậy...
Dialogue: 0,0:09:41.34,0:09:44.05,Default,,0000,0000,0000,,Em thật sự đang rất ướt.
Dialogue: 0,0:09:44.34,0:09:49.04,Default,,0000,0000,0000,,Không phải, chỉ là em bị vã mồ hôi.
Dialogue: 0,0:09:49.21,0:09:50.74,Default,,0000,0000,0000,,Thật vậy sao?
Dialogue: 0,0:09:55.13,0:09:57.75,Default,,0000,0000,0000,,Anh không chạm vào nó sao?
Dialogue: 0,0:09:57.93,0:10:03.33,Default,,0000,0000,0000,,Anh thôi. Nếu em muốn đến như vậy hãy tự đi mà làm!
Dialogue: 0,0:10:03.71,0:10:06.42,Default,,0000,0000,0000,,Đừng đùa em như thế...
Dialogue: 0,0:10:06.80,0:10:12.02,Default,,0000,0000,0000,,Hãy chạm vào em... chạm vào sâu trong em!
Dialogue: 0,0:10:12.41,0:10:16.32,Default,,0000,0000,0000,,Có vẻ như anh phải phạt cô em hư hỏng của mình rồi!
Dialogue: 0,0:10:21.93,0:10:23.99,Default,,0000,0000,0000,,Ô, em lại lên đỉnh rồi sao?
Dialogue: 0,0:10:24.90,0:10:26.70,Default,,0000,0000,0000,,Em không...
Dialogue: 0,0:10:26.88,0:10:30.56,Default,,0000,0000,0000,,Anh đột nhiên đút nó vào làm em thấy bất ngờ!
Dialogue: 0,0:10:32.72,0:10:34.80,Default,,0000,0000,0000,,Thật là... sướng...
Dialogue: 0,0:10:34.92,0:10:38.08,Default,,0000,0000,0000,,Nó thật khác... so với lúc trước!
Dialogue: 0,0:10:39.28,0:10:42.66,Default,,0000,0000,0000,,Anh không hiểu, giải thích cho anh xem!
Dialogue: 0,0:10:43.01,0:10:47.97,Default,,0000,0000,0000,,Con trym của anh... chọc vào xâu... trong em...
Dialogue: 0,0:10:50.47,0:10:55.98,Default,,0000,0000,0000,,Làm... âm hộ của em... thấy... thật sướng...
Dialogue: 0,0:10:56.77,0:10:57.73,Default,,0000,0000,0000,,Mio...
Dialogue: 0,0:10:59.10,0:11:04.40,Default,,0000,0000,0000,,Shuu-niichan... Em vỡ tung ra mất...
Dialogue: 0,0:11:04.57,0:11:07.01,Default,,0000,0000,0000,,Sướng chứ, Mio?!
Dialogue: 0,0:11:07.01,0:11:09.21,Default,,0000,0000,0000,,Sướng quá đi...
Dialogue: 0,0:11:09.62,0:11:13.16,Default,,0000,0000,0000,,Hãy đút nó thêm vào trong...
Dialogue: 0,0:11:13.47,0:11:16.00,Default,,0000,0000,0000,,Đút nó vào cơ thể dâm đãng của em...
Dialogue: 0,0:11:18.10,0:11:20.94,Default,,0000,0000,0000,,Có ai dám nghĩ đây là em gái mình chứ...
Dialogue: 0,0:11:21.31,0:11:22.77,Default,,0000,0000,0000,,Em xin lỗi...
Dialogue: 0,0:11:23.01,0:11:26.48,Default,,0000,0000,0000,,Em thật là hư hỏng... em là một cô em hư hỏng...
Dialogue: 0,0:11:27.94,0:11:31.99,Default,,0000,0000,0000,,Anh cũng rất hư hỏng, vậy là thật tuyệt, phải không!?
Dialogue: 0,0:11:34.45,0:11:35.40,Default,,0000,0000,0000,,Mio...
Dialogue: 0,0:11:35.59,0:11:42.14,Default,,0000,0000,0000,,Em ra rồi, em ra rồi...
Dialogue: 0,0:11:59.62,0:12:01.18,Default,,0000,0000,0000,,Geez...
Dialogue: 0,0:12:01.70,0:12:03.04,Default,,0000,0000,0000,,Hmm...
Dialogue: 0,0:12:03.12,0:12:05.25,Default,,0000,0000,0000,,Thật là lạ...
Dialogue: 0,0:12:06.01,0:12:11.72,Default,,0000,0000,0000,,Cứ nghĩ Shuu-chan với Ai-chan sẽ là cặp đôi hoàn hảo, ai dè...
Dialogue: 0,0:12:12.96,0:12:16.23,Default,,0000,0000,0000,,Không biết có vấn đề gì nữa...
Dialogue: 0,0:12:18.39,0:12:23.15,Default,,0000,0000,0000,,H-hay là... vấn đề giới tính?
Dialogue: 0,0:12:26.72,0:12:28.97,Default,,0000,0000,0000,,E-em không được... Shuu-chan...
Dialogue: 0,0:12:29.41,0:12:31.95,Default,,0000,0000,0000,,Cô là giáo viên của em...
Dialogue: 0,0:12:32.94,0:12:36.68,Default,,0000,0000,0000,,Sensei... Em không kiềm chế được nữa rồi!
Dialogue: 0,0:12:39.00,0:12:40.37,Default,,0000,0000,0000,,Đó là...
Dialogue: 0,0:12:40.55,0:12:42.18,Default,,0000,0000,0000,,Được chứ phải không nào?
Dialogue: 0,0:12:44.12,0:12:45.75,Default,,0000,0000,0000,,Cô hiểu rồi...
Dialogue: 0,0:12:45.91,0:12:51.66,Default,,0000,0000,0000,,Vậy... cô sẽ dùng ngực của cô...
Dialogue: 0,0:12:51.79,0:12:53.98,Default,,0000,0000,0000,,Hãy để ngực cô chăm sóc cái của em...
Dialogue: 0,0:12:55.45,0:12:56.99,Default,,0000,0000,0000,,Em đau à?
Dialogue: 0,0:12:57.30,0:13:01.10,Default,,0000,0000,0000,,Sensei... Hãy di chuyển nó lên xuống!
Dialogue: 0,0:13:02.24,0:13:03.38,Default,,0000,0000,0000,,Như thế này sao?
Dialogue: 0,0:13:05.26,0:13:08.34,Default,,0000,0000,0000,,Sensei, cô thật tuyệt!
Dialogue: 0,0:13:09.99,0:13:14.15,Default,,0000,0000,0000,,Cô chưa từng làm việc này...
Dialogue: 0,0:13:14.34,0:13:16.37,Default,,0000,0000,0000,,Không hay rồi... em...
Dialogue: 0,0:13:16.63,0:13:20.41,Default,,0000,0000,0000,,Em thấy sướng chứ? em thấy sướng chứ?
Dialogue: 0,0:13:21.31,0:13:24.77,Default,,0000,0000,0000,,Không, đừng có đẩy nó...
Dialogue: 0,0:13:25.14,0:13:28.18,Default,,0000,0000,0000,,Xin lỗi, sensei nhưng hông em không dừng được!
Dialogue: 0,0:13:29.11,0:13:35.46,Default,,0000,0000,0000,,Thật tuyệt... Trym của Shuu-chan trở nên nóng thế này!
Dialogue: 0,0:13:35.50,0:13:36.75,Default,,0000,0000,0000,,Cô cũng...
Dialogue: 0,0:13:38.12,0:13:43.96,Default,,0000,0000,0000,,Không... nếu em làm việc với đầu vú của cô.
Dialogue: 0,0:13:46.08,0:13:48.50,Default,,0000,0000,0000,,Nó sẽ làm cô cảm thấy kỳ lạ...
Dialogue: 0,0:13:50.21,0:13:53.32,Default,,0000,0000,0000,,Ngực cô đang rất nóng...
Dialogue: 0,0:13:53.57,0:13:55.02,Default,,0000,0000,0000,,Sensei...
Dialogue: 0,0:13:56.85,0:14:01.98,Default,,0000,0000,0000,,Cô sẽ làm em thấy sướng với ngực cô vậy nên...
Dialogue: 0,0:14:03.13,0:14:07.36,Default,,0000,0000,0000,,Em có thể... xuất bất kỳ lúc nào...
Dialogue: 0,0:14:07.51,0:14:10.17,Default,,0000,0000,0000,,Sensei, cô đang làm gì vậy ?
Dialogue: 0,0:14:15.20,0:14:17.80,Default,,0000,0000,0000,,Mặt cô đỏ lên rồi kìa...
Dialogue: 0,0:14:18.38,0:14:19.51,Default,,0000,0000,0000,,Có sao không ạ?
Dialogue: 0,0:14:20.19,0:14:22.74,Default,,0000,0000,0000,,À không có việc gì đâu...
Dialogue: 0,0:14:23.11,0:14:26.77,Default,,0000,0000,0000,,Chỉ là... cô đang suy nghĩ linh tinh ấy mà!
Dialogue: 0,0:14:54.18,0:14:55.48,Default,,0000,0000,0000,,À này...
Dialogue: 0,0:14:58.70,0:15:02.66,Default,,0000,0000,0000,,Nếu cậu muốn... tớ sẽ nhặt bóng hộ cho!
Dialogue: 0,0:15:02.97,0:15:04.23,Default,,0000,0000,0000,,Saeki-san.
Dialogue: 0,0:15:04.90,0:15:12.34,Default,,0000,0000,0000,,Mọi người nói hoãn buổi tập chiều nay thì trời quá nóng, nhưng tớ lại chẳng thấy cậu đâu nên...
Dialogue: 0,0:15:13.06,0:15:15.77,Default,,0000,0000,0000,,Vậy à. Thấy tớ chỗ này kỳ lạ lắm phải không?
Dialogue: 0,0:15:17.02,0:15:21.64,Default,,0000,0000,0000,,M-Mình xin lỗi... mình đã làm phiền cậu!
Dialogue: 0,0:15:21.87,0:15:24.28,Default,,0000,0000,0000,,không không, không có phiền gì mà!
Dialogue: 0,0:15:25.23,0:15:25.85,Default,,0000,0000,0000,,Cám ơn cậu!
Dialogue: 0,0:15:27.98,0:15:29.42,Default,,0000,0000,0000,,Dù sao chúng ta cũng là đồng đội mà.
Dialogue: 0,0:15:30.36,0:15:32.70,Default,,0000,0000,0000,,Cậu muốn đánh bóng ở đây một chút chứ?
Dialogue: 0,0:15:33.79,0:15:36.17,Default,,0000,0000,0000,,Mình thấy cũng gần quen tay rồi...
Dialogue: 0,0:15:37.55,0:15:38.60,Default,,0000,0000,0000,,Ừh!
Dialogue: 0,0:15:57.13,0:16:00.44,Default,,0000,0000,0000,,Cậu đánh hay hơn nhiều rùi đó!
Dialogue: 0,0:16:00.70,0:16:02.53,Default,,0000,0000,0000,,Không, chỉ là ăn may thui!
Dialogue: 0,0:16:02.76,0:16:04.64,Default,,0000,0000,0000,,Cậu cứ...
Dialogue: 0,0:16:13.29,0:16:15.73,Default,,0000,0000,0000,,Saeki-san! Chúng ta phải dọn đến đống bóng này!
Dialogue: 0,0:16:16.56,0:16:17.48,Default,,0000,0000,0000,,Ừ!
Dialogue: 0,0:16:30.18,0:16:32.41,Default,,0000,0000,0000,,Đáng sợ quá... như sắp có bão vậy.
Dialogue: 0,0:16:33.18,0:16:35.87,Default,,0000,0000,0000,,Tự nhiên mưa thế này thì chắc còn lâu mới tạnh.
Dialogue: 0,0:16:47.84,0:16:49.43,Default,,0000,0000,0000,,X-Xin lỗi!
Dialogue: 0,0:16:49.80,0:16:51.80,Default,,0000,0000,0000,,Đợi đã!
Dialogue: 0,0:16:52.39,0:16:55.23,Default,,0000,0000,0000,,Nếu là cậu... thì không sao...
Dialogue: 0,0:16:57.41,0:17:05.90,Default,,0000,0000,0000,,Mình... thực sự đã rất, rất vui khi được đánh đôi cùng cậu...
Dialogue: 0,0:17:06.74,0:17:08.39,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:17:08.96,0:17:16.36,Default,,0000,0000,0000,,Umm...Đã từ lâu... Mình vẫn... luôn...
Dialogue: 0,0:17:16.43,0:17:19.90,Default,,0000,0000,0000,,Xin lỗi... Bây giờ... Mình...
Dialogue: 0,0:17:20.43,0:17:24.18,Default,,0000,0000,0000,,Nếu là vì Mio-san, thì mình biết rồi!
Dialogue: 0,0:17:24.60,0:17:26.33,Default,,0000,0000,0000,,Nhưng mình không quan tâm!
Dialogue: 0,0:17:28.38,0:17:32.24,Default,,0000,0000,0000,,Lúc này... hãy nhìn mình tớ thôi!
Dialogue: 0,0:17:32.79,0:17:34.24,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:18:13.63,0:18:16.76,Default,,0000,0000,0000,,Saeki-san... Mình không kiềm được nữa rồi...!
Dialogue: 0,0:18:23.86,0:18:25.30,Default,,0000,0000,0000,,Mình thấy xấu hổ quá!
Dialogue: 0,0:18:25.48,0:18:27.66,Default,,0000,0000,0000,,Phía dưới này ấm thật!
Dialogue: 0,0:18:28.11,0:18:31.92,Default,,0000,0000,0000,,Cậu đừng có nói ra chứ...
Dialogue: 0,0:18:33.54,0:18:35.78,Default,,0000,0000,0000,,Đừng liếm mình ở chỗ đó...
Dialogue: 0,0:18:38.07,0:18:40.64,Default,,0000,0000,0000,,Cậu dâm thật đấy...!
Dialogue: 0,0:18:40.82,0:18:44.05,Default,,0000,0000,0000,,Cơ thể cậu làm trả lời tớ rồi đấy.
Dialogue: 0,0:18:44.55,0:18:49.10,Default,,0000,0000,0000,,Đừng nói vậy... thật xấu hổ!
Dialogue: 0,0:18:50.74,0:18:52.63,Default,,0000,0000,0000,,Chỗ đó...
Dialogue: 0,0:18:54.02,0:18:58.45,Default,,0000,0000,0000,,Hãy... làm... thật nhẹ nhàng!
Dialogue: 0,0:18:58.52,0:19:00.45,Default,,0000,0000,0000,,Saeki-san, cậu cảm nhận được nó chứ?
Dialogue: 0,0:19:01.42,0:19:04.15,Default,,0000,0000,0000,,Mình không biết...
Dialogue: 0,0:19:04.21,0:19:06.79,Default,,0000,0000,0000,,Cậu thật là...
Dialogue: 0,0:19:07.45,0:19:10.04,Default,,0000,0000,0000,,Chỗ này... ướt nhoẹt hết cả rồi!
Dialogue: 0,0:19:10.36,0:19:15.08,Default,,0000,0000,0000,,Cũng vì tên dâm đãng như cậu làm tớ phát điên...
Dialogue: 0,0:19:17.23,0:19:22.29,Default,,0000,0000,0000,,Tớ phát điên lên mất... tớ...
Dialogue: 0,0:19:22.62,0:19:26.20,Default,,0000,0000,0000,,Vậy cậu muốn tớ làm gì? Tớ sẽ làm theo ý cậu!
Dialogue: 0,0:19:28.70,0:19:32.48,Default,,0000,0000,0000,,Hãy làm... theo ý cậu...
Dialogue: 0,0:19:35.96,0:19:36.95,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:19:38.34,0:19:40.94,Default,,0000,0000,0000,,Mình... muốn cậu...
Dialogue: 0,0:19:42.81,0:19:47.13,Default,,0000,0000,0000,,Vậy... xin hãy làm tình với tớ...
Dialogue: 0,0:19:53.25,0:19:56.35,Default,,0000,0000,0000,,Tư thế này xấu hổ thật...!
Dialogue: 0,0:19:56.43,0:19:57.75,Default,,0000,0000,0000,,Sugiyama-kun...
Dialogue: 0,0:20:01.28,0:20:04.54,Default,,0000,0000,0000,,Nó thật to và nóng...
Dialogue: 0,0:20:05.00,0:20:07.06,Default,,0000,0000,0000,,Nó sẽ khít vào trong tớ chứ?
Dialogue: 0,0:20:07.09,0:20:08.33,Default,,0000,0000,0000,,Hãy cứ thoải mái đi nào!
Dialogue: 0,0:20:11.97,0:20:13.31,Default,,0000,0000,0000,,Chỉ một chút nữa thôi...
Dialogue: 0,0:20:20.25,0:20:22.18,Default,,0000,0000,0000,,Nó vào trong rồi!
Dialogue: 0,0:20:27.06,0:20:29.29,Default,,0000,0000,0000,,Ổn cả rồi... cậu có thể di chuyển!
Dialogue: 0,0:20:29.75,0:20:31.98,Default,,0000,0000,0000,,Được rồi!
Dialogue: 0,0:20:32.32,0:20:38.98,Default,,0000,0000,0000,,Không sao mà... mình sẽ cố giữ nếu nó làm cậu thấy sướng!
Dialogue: 0,0:20:39.12,0:20:40.47,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:20:45.98,0:20:51.32,Default,,0000,0000,0000,,Sugiyama-kun... đang bên trong mình...
Dialogue: 0,0:20:53.00,0:20:54.65,Default,,0000,0000,0000,,Thật là sướng...
Dialogue: 0,0:20:58.90,0:21:00.56,Default,,0000,0000,0000,,Mình rất vui...
Dialogue: 0,0:21:00.72,0:21:03.48,Default,,0000,0000,0000,,Mình thấy vui hơn rồi...
Dialogue: 0,0:21:04.46,0:21:07.64,Default,,0000,0000,0000,,Mình cũng sẽ làm cậu thấy sướng...
Dialogue: 0,0:21:10.93,0:21:13.65,Default,,0000,0000,0000,,Mình sẽ... lại phát điên lên mất!
Dialogue: 0,0:21:13.95,0:21:17.25,Default,,0000,0000,0000,,không sao, hãy để mình thấy cậu điên lên!
Dialogue: 0,0:21:19.08,0:21:25.54,Default,,0000,0000,0000,,Đây là lần đầu tiên của mình... sao mình lại thấy...
Dialogue: 0,0:21:29.99,0:21:31.13,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:21:33.61,0:21:36.24,Default,,0000,0000,0000,,Lúc này trông cậu dễ thương quá!
Dialogue: 0,0:21:36.31,0:21:41.25,Default,,0000,0000,0000,,Thật tuyệt... cả cơ thể mình đều cảm thấy...
Dialogue: 0,0:21:43.24,0:21:48.39,Default,,0000,0000,0000,,Mình cảm thấy cơ thể mình nóng rang lên...
Dialogue: 0,0:21:50.45,0:21:51.64,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:21:59.00,0:22:03.38,Default,,0000,0000,0000,,Không sao mà... cậu cứ làm mạnh hơn nữa...
Dialogue: 0,0:22:09.90,0:22:12.56,Default,,0000,0000,0000,,Hãy cứ làm mạnh vào...
Dialogue: 0,0:22:20.15,0:22:21.77,Default,,0000,0000,0000,,Mình rút nó ra đây!
Dialogue: 0,0:22:23.05,0:22:25.00,Default,,0000,0000,0000,,Chờ đã, Sugiyama...
Dialogue: 0,0:22:25.04,0:22:26.22,Default,,0000,0000,0000,,Hãy nhấc chân cậu lên!
Dialogue: 0,0:22:26.85,0:22:28.22,Default,,0000,0000,0000,,Ư-ừ...
Dialogue: 0,0:22:41.18,0:22:43.55,Default,,0000,0000,0000,,Nó đang vào trong mình...
Dialogue: 0,0:22:46.08,0:22:49.59,Default,,0000,0000,0000,,Saeki-san... Mình sắp...
Dialogue: 0,0:22:51.37,0:22:55.06,Default,,0000,0000,0000,,Hãy xuất... hãy xuất vào bên trong mình...
Dialogue: 0,0:22:57.72,0:22:58.94,Default,,0000,0000,0000,,Hử? Nhưng...
Dialogue: 0,0:23:00.14,0:23:05.80,Default,,0000,0000,0000,,Xin cậu... đây là lần đầu của mình... mình muốn cậu...
Dialogue: 0,0:23:23.74,0:23:27.15,Default,,0000,0000,0000,,Nó nóng thật!
Dialogue: 0,0:23:28.01,0:23:29.97,Default,,0000,0000,0000,,Xin lỗi, mình...
Dialogue: 0,0:23:31.14,0:23:32.26,Default,,0000,0000,0000,,... không sao mà!
Dialogue: 0,0:23:32.53,0:23:34.43,Default,,0000,0000,0000,,Cậu không cần xin lỗi!
Dialogue: 0,0:23:34.65,0:23:36.03,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:23:36.16,0:23:42.36,Default,,0000,0000,0000,,Mình rất vui... lần đầu tiên của mình là với cậu!
Dialogue: 0,0:24:13.67,0:24:17.52,Default,,0000,0000,0000,,Mình không biết có việc gì mà các cậu lại bảo mình chuẩn bị một bộ Yukata.
Dialogue: 0,0:24:18.03,0:24:21.15,Default,,0000,0000,0000,,Hóa ra là vì thế này.
Dialogue: 0,0:24:21.75,0:24:26.59,Default,,0000,0000,0000,,Mình có nghe nói chỗ này\N có tổ chức lễ hội nên mới đặt hội trại ở đây luôn.
Dialogue: 0,0:24:27.42,0:24:29.27,Default,,0000,0000,0000,,Quả không ngoài kỳ vọng của cô!
Dialogue: 0,0:24:30.26,0:24:34.85,Default,,0000,0000,0000,,Thì, luyện tập nhiều cũng cần có chút thời gian thư giãn phải không nào!
Dialogue: 0,0:24:46.07,0:24:46.94,Default,,0000,0000,0000,,À.. à này...
Dialogue: 0,0:24:47.45,0:24:48.42,Default,,0000,0000,0000,,Dạ?
Dialogue: 0,0:24:48.83,0:24:51.67,Default,,0000,0000,0000,,Trông cậu rất xinh... trong bộ yukata đó...!
Dialogue: 0,0:24:53.15,0:24:54.94,Default,,0000,0000,0000,,Cám ơn cậu rất nhiều!
Dialogue: 0,0:25:04.30,0:25:07.72,Default,,0000,0000,0000,,Hai người cứ thập thà thập thụt gì thế hả?
Dialogue: 0,0:25:07.83,0:25:08.88,Default,,0000,0000,0000,,M-Mio?
Dialogue: 0,0:25:09.63,0:25:13.01,Default,,0000,0000,0000,,Nếu là về chuyện đánh đôi thì để lúc khác mà bàn!
Dialogue: 0,0:25:13.30,0:25:14.90,Default,,0000,0000,0000,,Nhé?
Dialogue: 0,0:25:15.15,0:25:18.58,Default,,0000,0000,0000,,Mio... Em... không mặc áo trong sao!?
Dialogue: 0,0:25:19.01,0:25:23.40,Default,,0000,0000,0000,,Á~~ anh nhìn trộm em nhé! Xấu quá đi!
Dialogue: 0,0:25:23.92,0:25:27.63,Default,,0000,0000,0000,,Ồ! Lại thêm một tình huống táo bạo nữa kìa!
Dialogue: 0,0:25:27.99,0:25:32.41,Default,,0000,0000,0000,,Đừng trẻ con nữa, Mio.\N Em không cần phải ôm chặt thế đâu!
Dialogue: 0,0:25:32.60,0:25:37.04,Default,,0000,0000,0000,,Nhưng không mặc áo lót chẳng phải hấp dẫn hơn sao?
Dialogue: 0,0:26:03.04,0:26:04.28,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:26:05.57,0:26:08.58,Default,,0000,0000,0000,,Ơ kìa! Ai-chan đi đâu mất rồi?
Dialogue: 0,0:26:10.97,0:26:16.46,Default,,0000,0000,0000,,Lạ thật, vừa lúc nẫy còn thấy ở ngay đây mà!
Dialogue: 0,0:26:17.21,0:26:17.97,Default,,0000,0000,0000,,Xin lỗi...
Dialogue: 0,0:26:22.42,0:26:24.14,Default,,0000,0000,0000,,Kìa, Saeki-san?
Dialogue: 0,0:26:24.50,0:26:29.01,Default,,0000,0000,0000,,Xin lỗi, mình mải nhìn mấy gian hàng nên đến muộn.
Dialogue: 0,0:26:29.50,0:26:32.78,Default,,0000,0000,0000,,Geez, đừng có mà để lạc đấy!
Dialogue: 0,0:26:33.27,0:26:34.97,Default,,0000,0000,0000,,Được rồi, chúng ta đi nào.
Dialogue: 0,0:26:36.25,0:26:40.35,Default,,0000,0000,0000,,Ơ... Saeki-san... cậu vừa...
Dialogue: 0,0:26:41.33,0:26:47.15,Default,,0000,0000,0000,,Ừh... mình cũng vừa... cởi đồ lót ra xong...
Dialogue: 0,0:26:48.81,0:26:52.66,Default,,0000,0000,0000,,Đúng là có hơi "mát mẻ" nhưng...
Dialogue: 0,0:26:53.61,0:26:56.17,Default,,0000,0000,0000,,Nó cũng thật tuyệt phải không nào...?
Dialogue: 0,0:26:56.32,0:26:57.23,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:26:59.73,0:27:03.02,Default,,0000,0000,0000,,{\i1}Mình không lo dù có bị coi là một cô gái hư hỏng!{\i0}
Dialogue: 0,0:27:03.53,0:27:11.21,Default,,0000,0000,0000,,{\i1}Việc này thật xấu hổ,\N nhưng mình nhất quyết không từ bỏ Sugiyama-kun.{\i0}
Dialogue: 0,0:27:11.21,0:27:13.21,Default,,0000,0000,0000,,
