Dialogue: 0,0:00:00.00,0:00:05.00,Default,,0000,0000,0000,,
Dialogue: 0,0:00:23.73,0:00:25.95,Script,,0000,0000,0000,,Let's Fall In Love, The Ero-manga: Page 2!
Dialogue: 0,0:00:25.79,0:00:30.50,Script,,0000,0000,0000,,{\pos(422,459)}{TS}{\fad(700,700)}Chỉ là một sự kiện lớn và khủng
Dialogue: 0,0:00:34.84,0:00:36.54,Script,,0000,0000,0000,,Con chào mẹ.
Dialogue: 0,0:00:36.54,0:00:38.09,Script,,0000,0000,0000,,Chào con, Hiro.
Dialogue: 0,0:00:39.41,0:00:40.12,Script,,0000,0000,0000,,Hử?
Dialogue: 0,0:00:40.44,0:00:42.59,Script,,0000,0000,0000,,Chị đâu rồi ạ? Vẫn đang ngủ sao?
Dialogue: 0,0:00:43.04,0:00:45.95,Script,,0000,0000,0000,,Hôm này là "ngày ấy" của Azura nên con bé chắc nghỉ ở nhà.
Dialogue: 0,0:00:45.95,0:00:49.04,Script,,0000,0000,0000,,Vậy sao, hôm nay là "ngày ấy" ...
Dialogue: 0,0:00:49.04,0:00:51.73,Script,,0000,0000,0000,,Mà con cũng nghỉ ở nhà nốt luôn đi.
Dialogue: 0,0:00:51.73,0:00:53.49,Script,,0000,0000,0000,,Hả? Con làm gì mà cũng phải nghỉ?
Dialogue: 0,0:00:53.49,0:00:57.90,Script,,0000,0000,0000,,Cũng chịu thôi. Làm sao để Azura ở nhà một mình được chứ.
Dialogue: 0,0:00:57.90,0:01:00.21,Script,,0000,0000,0000,,Mẹ còn phải đi làm nên không nghỉ được hôm nay,
Dialogue: 0,0:01:00.72,0:01:04.01,Script,,0000,0000,0000,,Con phải chăm sóc chị chứ Hiro.
Dialogue: 0,0:01:04.31,0:01:06.27,Script,,0000,0000,0000,,Ế? Đ-đợi đã nào... Mẹ!
Dialogue: 0,0:01:06.27,0:01:07.59,Script,,0000,0000,0000,,Mẹ đi làm đây~!
Dialogue: 0,0:01:13.17,0:01:14.49,Script,,0000,0000,0000,,Sao lại là mình chứ?
Dialogue: 0,0:01:15.97,0:01:17.79,Script,,0000,0000,0000,,Tháng nào vào thời điểm này,
Dialogue: 0,0:01:18.26,0:01:20.41,Script,,0000,0000,0000,,{\i1}Chị gái cùng mẹ khác cha của tôi...{\i0}
Dialogue: 0,0:01:23.27,0:01:24.80,Script,,0000,0000,0000,,{\i1}... lại lên cơn hứng tình.{\i0}
Dialogue: 0,0:01:26.93,0:01:28.73,Script,,0000,0000,0000,,Chị à, chị đang làm gì vậy?
Dialogue: 0,0:01:28.89,0:01:31.57,Script,,0000,0000,0000,,Ăn sáng thôi nào! Quả chuối đáng yêu của chị...
Dialogue: 0,0:01:32.20,0:01:34.32,Script,,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:01:36.82,0:01:38.71,Script,,0000,0000,0000,,N-này- chị!
Dialogue: 0,0:01:50.05,0:01:51.29,Script,,0000,0000,0000,,Ngon ghê!
Dialogue: 0,0:02:18.79,0:02:21.14,Script,,0000,0000,0000,,Mới sáng... mà đã... làm thế này.
Dialogue: 0,0:02:22.34,0:02:25.13,Script,,0000,0000,0000,,Tuyệt lắm, cứ phóng hết ra đi.
Dialogue: 0,0:02:28.20,0:02:31.85,Script,,0000,0000,0000,,Hãy phóng hết tất cả vào miệng chị này!
Dialogue: 0,0:02:35.63,0:02:36.88,Script,,0000,0000,0000,,Em ra đây.
Dialogue: 0,0:02:37.55,0:02:38.62,Script,,0000,0000,0000,,Chị!
Dialogue: 0,0:02:51.57,0:02:54.39,Script,,0000,0000,0000,,Tinh dịch của Hiro thật mặn mà và đầy hương vị.
Dialogue: 0,0:02:54.39,0:02:55.65,Script,,0000,0000,0000,,Cám ơn về bữa ăn.
Dialogue: 0,0:02:56.00,0:02:58.64,Script,,0000,0000,0000,,À em... xin lỗi, em cạn tinh rồi.
Dialogue: 0,0:02:58.64,0:03:00.63,Script,,0000,0000,0000,,Đã quá, chị muốn làm lần nữa với em.
Dialogue: 0,0:03:03.32,0:03:07.06,Script,,0000,0000,0000,,Lần này chị muốn em thưởng thức nó...
Dialogue: 0,0:03:07.06,0:03:08.20,Script,,0000,0000,0000,,Nào em.
Dialogue: 0,0:03:08.76,0:03:11.20,Script,,0000,0000,0000,,Dạ... vâng...
Dialogue: 0,0:03:20.40,0:03:24.75,Script,,0000,0000,0000,,Chị vừa mới dậy xong... mà đã có thể nhịp ra nhịp vào trong cái ấy của chị dễ dàng thế này rồi sao?
Dialogue: 0,0:03:26.42,0:03:29.82,Script,,0000,0000,0000,,Sướng quá! Cây xúc xích của Hiro thật là to!
Dialogue: 0,0:03:31.33,0:03:34.50,Script,,0000,0000,0000,,Nữa đi, nữa đi, chị muốn được nếm thêm món sữa chuối của Hiro!
Dialogue: 0,0:03:34.50,0:03:35.82,Script,,0000,0000,0000,,Chị!
Dialogue: 0,0:03:37.24,0:03:39.85,Script,,0000,0000,0000,,Ôi! Chị sướng quá... Hiro.
Dialogue: 0,0:03:40.17,0:03:41.59,Script,,0000,0000,0000,,Hiro!
Dialogue: 0,0:03:43.67,0:03:45.05,Script,,0000,0000,0000,,Chị!
Dialogue: 0,0:04:02.19,0:04:03.50,Script,,0000,0000,0000,,Nữa đi em.
Dialogue: 0,0:04:51.36,0:04:53.67,Script,,0000,0000,0000,,Chị ấy mệt đến nỗi thiếp đi rồi.
Dialogue: 0,0:04:57.11,0:04:59.92,Script,,0000,0000,0000,,Mà mình còn mệt hơn ấy chứ...
Dialogue: 0,0:04:59.92,0:05:01.85,Script,,0000,0000,0000,,A, đói chết đi mất.
Dialogue: 0,0:05:04.81,0:05:06.54,Script,,0000,0000,0000,,Xin cám ơn quý khách rất nhiều.
Dialogue: 0,0:05:07.35,0:05:10.06,Script,,0000,0000,0000,,Nhanh lên nhanh lên...vốn đã định về ngay sau khi mua hộp cơm trưa.
Dialogue: 0,0:05:10.06,0:05:11.94,Script,,0000,0000,0000,,Mà tự dưng lại bị quấn vào đống sách đó.
Dialogue: 0,0:05:13.49,0:05:15.09,Script,,0000,0000,0000,,Em về rồi đây.
Dialogue: 0,0:05:15.39,0:05:18.03,Script,,0000,0000,0000,,Chị, thức ăn mua xong rồi này.
Dialogue: 0,0:05:19.00,0:05:19.96,Script,,0000,0000,0000,,Hả?
Dialogue: 0,0:05:21.08,0:05:22.70,Script,,0000,0000,0000,,Đang trong phòng mình sao?
Dialogue: 0,0:05:35.45,0:05:36.60,Script,,0000,0000,0000,,Hiro...
Dialogue: 0,0:05:37.78,0:05:38.89,Script,,0000,0000,0000,,Hiro...
Dialogue: 0,0:05:43.01,0:05:47.27,Script,,0000,0000,0000,,Hiro, nữa đi... hãy làm chị thấy sướng hơn nữa đi...
Dialogue: 0,0:06:03.68,0:06:06.04,Script,,0000,0000,0000,,Xin đừng rời xa chị!
Dialogue: 0,0:06:28.44,0:06:30.13,Script,,0000,0000,0000,,Hiro, chị đang ra,
Dialogue: 0,0:06:30.13,0:06:33.24,Script,,0000,0000,0000,,chị đang ra!
Dialogue: 0,0:06:42.04,0:06:43.01,Script,,0000,0000,0000,,À...
Dialogue: 0,0:06:43.31,0:06:44.71,Script,,0000,0000,0000,,C-Chị này...
Dialogue: 0,0:06:46.66,0:06:48.09,Script,,0000,0000,0000,,Em mua cho chị hộp cơm này...
Dialogue: 0,0:06:57.71,0:06:59.66,Script,,0000,0000,0000,,Có chuyện gì sao?
Dialogue: 0,0:06:59.66,0:07:00.81,Script,,0000,0000,0000,,Đừng khóc mà.
Dialogue: 0,0:07:00.81,0:07:04.10,Script,,0000,0000,0000,,Lúc... chị tỉnh dậy... em đã đi đâu rồi.
Dialogue: 0,0:07:04.76,0:07:09.04,Script,,0000,0000,0000,,Chị tưởng em ghét chị nên bỏ chị mà đi.
Dialogue: 0,0:07:09.54,0:07:11.63,Script,,0000,0000,0000,,Đồ ngốc, sao lại thế được chứ?
Dialogue: 0,0:07:12.01,0:07:14.88,Script,,0000,0000,0000,,Thật sao? Em có yêu chị không?
Dialogue: 0,0:07:14.88,0:07:15.59,Script,,0000,0000,0000,,Vâng.
Dialogue: 0,0:07:15.91,0:07:17.12,Script,,0000,0000,0000,,Em yêu chị sao?
Dialogue: 0,0:07:17.90,0:07:20.49,Script,,0000,0000,0000,,Em... yêu chị.
Dialogue: 0,0:07:22.15,0:07:23.32,Script,,0000,0000,0000,,Thật là tốt quá.
Dialogue: 0,0:07:23.74,0:07:25.81,Script,,0000,0000,0000,,Thôi bỏ đi, cơm hộp của chị đây.
Dialogue: 0,0:07:25.81,0:07:27.62,Script,,0000,0000,0000,,Chị đang đói mà phải không?
Dialogue: 0,0:07:27.62,0:07:29.82,Script,,0000,0000,0000,,Ừ, chị đói rã cả bướm ra rồi đây.
Dialogue: 0,0:07:29.82,0:07:30.76,Script,,0000,0000,0000,,Vậy nên...
Dialogue: 0,0:07:30.76,0:07:31.38,Script,,0000,0000,0000,,Ế?
Dialogue: 0,0:07:31.47,0:07:35.47,Script,,0000,0000,0000,,Phải "ăn" thêm thằng nhỏ của em thì chị mới no được.
Dialogue: 0,0:07:35.47,0:07:38.40,Script,,0000,0000,0000,,Mình biết ngay là sẽ thế này màaaa.
Dialogue: 0,0:07:40.00,0:07:41.63,Script,,0000,0000,0000,,Chị nhìn xem.
Dialogue: 0,0:07:41.98,0:07:44.17,Script,,0000,0000,0000,,Chỗ đặc biệt của hai chị em mình đang cọ xát nhau này...
Dialogue: 0,0:07:44.17,0:07:45.81,Script,,0000,0000,0000,,Chị ướt thật đấy.
Dialogue: 0,0:07:46.29,0:07:48.11,Script,,0000,0000,0000,,Không, đấy là...
Dialogue: 0,0:07:49.20,0:07:51.35,Script,,0000,0000,0000,,... ôi, nó cứng quá...
Dialogue: 0,0:07:52.68,0:07:54.68,Script,,0000,0000,0000,,Chị muốn dương vật của em.
Dialogue: 0,0:07:54.68,0:07:58.06,Script,,0000,0000,0000,,Sao lại của em nào. Đống đồ chơi kia còn không đủ à?
Dialogue: 0,0:08:00.86,0:08:02.12,Script,,0000,0000,0000,,Không!
Dialogue: 0,0:08:02.82,0:08:04.45,Script,,0000,0000,0000,,Chị chỉ muốn của em.
Dialogue: 0,0:08:04.45,0:08:06.68,Script,,0000,0000,0000,,Phải là của em.
Dialogue: 0,0:08:06.68,0:08:07.57,Script,,0000,0000,0000,,Chị...
Dialogue: 0,0:08:17.10,0:08:19.07,Script,,0000,0000,0000,,Á, ôi dương vật của em thật tuyệt vời.
Dialogue: 0,0:08:20.67,0:08:24.41,Script,,0000,0000,0000,,Em vừa mới đút vào mà chị đã ra rồi.
Dialogue: 0,0:08:25.54,0:08:28.52,Script,,0000,0000,0000,,Âm hộ của chị cũng sướng lắm.
Dialogue: 0,0:08:28.52,0:08:30.82,Script,,0000,0000,0000,,Nó thật ướt và khít.
Dialogue: 0,0:08:49.59,0:08:50.79,Script,,0000,0000,0000,,Ôi Hiro.
Dialogue: 0,0:08:51.18,0:08:52.22,Script,,0000,0000,0000,,Làm tình với em thật sướng quá.
Dialogue: 0,0:08:55.28,0:08:59.17,Script,,0000,0000,0000,,Em... em... sắp ra rồi.
Dialogue: 0,0:09:00.59,0:09:02.09,Script,,0000,0000,0000,,Cứ cho nó ra đi Hiro.
Dialogue: 0,0:09:02.09,0:09:05.46,Script,,0000,0000,0000,,Cho chị nếm món sữa chuối của em đi!
Dialogue: 0,0:09:05.46,0:09:07.96,Script,,0000,0000,0000,,Em không giữ nổi nữa rồi!
Dialogue: 0,0:09:15.07,0:09:17.24,Script,,0000,0000,0000,,Nhìn xem sữa của em vương vãi khắp người chị này...
Dialogue: 0,0:09:21.51,0:09:23.45,Script,,0000,0000,0000,,Em cho chị hết sữa chuối rồi đấy.
Dialogue: 0,0:09:23.45,0:09:25.07,Script,,0000,0000,0000,,Sao em dám xuất vào trong chị chứ!
Dialogue: 0,0:09:25.28,0:09:26.76,Script,,0000,0000,0000,,Geez...
Dialogue: 0,0:09:28.93,0:09:30.39,Script,,0000,0000,0000,,Chị làm gì vậy?
Dialogue: 0,0:09:30.39,0:09:33.21,Script,,0000,0000,0000,,Chị vẫn chưa thấy no.
Dialogue: 0,0:09:33.21,0:09:36.35,Script,,0000,0000,0000,,Hả? E-Em vừa ra xong mà.
Dialogue: 0,0:09:36.76,0:09:38.42,Script,,0000,0000,0000,,Đợi đã nào, chị!
Dialogue: 0,0:09:44.19,0:09:45.76,Script,,0000,0000,0000,,Nó lại cương lên gòi nè!
Dialogue: 0,0:09:58.18,0:10:02.14,Script,,0000,0000,0000,,C-Chị đừng nắc nhanh quá.
Dialogue: 0,0:10:05.24,0:10:07.92,Script,,0000,0000,0000,,Vậy mới phê chứ.
Dialogue: 0,0:10:08.94,0:10:11.56,Script,,0000,0000,0000,,Chị ôi, em lại sắp ra rồi...
Dialogue: 0,0:10:13.33,0:10:15.06,Script,,0000,0000,0000,,Cứ xuất hết ra đi, Hiro.
Dialogue: 0,0:10:15.42,0:10:18.45,Script,,0000,0000,0000,,Lần này hãy xuất vào trong chị.
Dialogue: 0,0:10:19.78,0:10:22.04,Script,,0000,0000,0000,,Chị, không được.
Dialogue: 0,0:10:25.23,0:10:27.54,Script,,0000,0000,0000,,Em phải xuất vào trong chị.
Dialogue: 0,0:10:28.14,0:10:30.69,Script,,0000,0000,0000,,Đây là lệnh của chị.
Dialogue: 0,0:10:31.07,0:10:33.15,Script,,0000,0000,0000,,Không được.
Dialogue: 0,0:10:42.84,0:10:44.77,Script,,0000,0000,0000,,Chị... em đang ra!
Dialogue: 0,0:11:06.38,0:11:07.36,Script,,0000,0000,0000,,Chị...
Dialogue: 0,0:11:07.59,0:11:10.65,Script,,0000,0000,0000,,Hiro, chị yêu em nhiều lắm.
Dialogue: 0,0:11:14.62,0:11:15.65,Script,,0000,0000,0000,,Tụi mình "dùng bữa" tiếp đi nào!
Dialogue: 0,0:11:21.59,0:11:22.79,Script,,0000,0000,0000,,Mẹ về rồi đây.
Dialogue: 0,0:11:23.44,0:11:25.33,Script,,0000,0000,0000,,Mẹ có mang về một ít cá trình này.
Dialogue: 0,0:11:25.87,0:11:30.64,Script,,0000,0000,0000,,So với buổi sáng, trông con thảm quá vậy!
Dialogue: 0,0:11:31.13,0:11:32.90,Script,,0000,0000,0000,,Vậy, con thế nào rồi?
Dialogue: 0,0:11:32.90,0:11:35.82,Script,,0000,0000,0000,,Từ lần thứ 20 thì con đã hết nhớ nổi rồi.
Dialogue: 0,0:11:36.11,0:11:38.30,Script,,0000,0000,0000,,Chà, của quý của con mình kinh thật.
Dialogue: 0,0:11:38.30,0:11:41.31,Script,,0000,0000,0000,,Con bé giống hệt mẹ hồi trẻ.
Dialogue: 0,0:11:43.54,0:11:44.91,Script,,0000,0000,0000,,"Giống mẹ hồi còn trẻ..."
Dialogue: 0,0:11:44.91,0:11:48.36,Script,,0000,0000,0000,,Vậy mẹ cũng cảm thấy bị kích thích sao?
Dialogue: 0,0:11:48.61,0:11:50.11,Script,,0000,0000,0000,,Ủa, mẹ chưa nói con à?
Dialogue: 0,0:11:50.11,0:11:52.40,Script,,0000,0000,0000,,Đây là truyền thống của gia đình ta.
Dialogue: 0,0:11:52.40,0:11:55.09,Script,,0000,0000,0000,,Dù rằng mẹ đã mãn kinh lâu rồi.
Dialogue: 0,0:11:55.51,0:11:59.53,Script,,0000,0000,0000,,Vậy chắc chị cũng sẽ sớm hết thôi phải không?
Dialogue: 0,0:11:59.81,0:12:04.07,Script,,0000,0000,0000,,Sau khi mẹ sinh Azura thì tự nhiên nó hết.
Dialogue: 0,0:12:04.28,0:12:06.32,Script,,0000,0000,0000,,Sau khi... sinh?
Dialogue: 0,0:12:06.97,0:12:09.02,Script,,0000,0000,0000,,Nhìn bố con xem này.
Dialogue: 0,0:12:09.02,0:12:11.13,Script,,0000,0000,0000,,Là bố và cũng là bác.
Dialogue: 0,0:12:13.61,0:12:15.00,Script,,0000,0000,0000,,Toi mình rồi.
Dialogue: 0,0:12:15.00,0:12:17.66,Script,,0000,0000,0000,,Mình phải nhanh chóng kiếm ra con ngựa nào để thế mạng thôi.
Dialogue: 0,0:12:17.66,0:12:18.68,Script,,0000,0000,0000,,Không sao đâu con.
Dialogue: 0,0:12:19.80,0:12:23.92,Script,,0000,0000,0000,,Khi nào con còn tình yêu, con có thể dễ dàng vượt qua những chướng ngại trên đường đời!
Dialogue: 0,0:12:23.92,0:12:27.93,Script,,0000,0000,0000,,Đừng có chuyển chủ đề rồi nói mấy thứ vớ vẩn như thế!
Dialogue: 0,0:12:30.09,0:12:33.10,Script,,0000,0000,0000,,Bụng mình đang đầy ắp sữa.
Dialogue: 0,0:12:35.03,0:12:36.27,Script,,0000,0000,0000,,Hiro...
Dialogue: 0,0:12:41.62,0:12:44.61,Script,,0000,0000,0000,,Let's Fall In Love: The Ero-manga!
Dialogue: 0,0:12:49.82,0:12:53.92,Script,,0000,0000,0000,,Lâu rồi mới thấy chị lên phòng em chơi nhỉ, Mayo-nee.
Dialogue: 0,0:12:53.92,0:12:55.85,Script,,0000,0000,0000,,À,ừ... đúng vậy.
Dialogue: 0,0:12:56.39,0:12:57.69,Script,,0000,0000,0000,,Chị có việc gì sao?
Dialogue: 0,0:12:58.02,0:13:00.35,Script,,0000,0000,0000,,Ngoài chuyện cho mượn tiền ra thì việc gì em cũng giúp.
Dialogue: 0,0:13:01.26,0:13:03.15,Script,,0000,0000,0000,,Thật ra là...
Dialogue: 0,0:13:03.55,0:13:06.03,Script,,0000,0000,0000,,Chị có chút chuyện muốn nhờ em.
Dialogue: 0,0:13:06.03,0:13:06.90,Script,,0000,0000,0000,,Nhờ em?
Dialogue: 0,0:13:06.90,0:13:08.96,Script,,0000,0000,0000,,Đúng vậy... ngực của chị...
Dialogue: 0,0:13:12.27,0:13:17.01,Script,,0000,0000,0000,,Em có thể... vuốt ve ngực của chị không?
Dialogue: 0,0:13:18.56,0:13:21.01,Script,,0000,0000,0000,,À, ngực! Ý chị là matxa vú chứ gì.
Dialogue: 0,0:13:21.01,0:13:21.85,Script,,0000,0000,0000,,Đúng đúng.
Dialogue: 0,0:13:21.85,0:13:22.95,Script,,0000,0000,0000,,Vậy thôi sao?
Dialogue: 0,0:13:26.06,0:13:27.83,Script,,0000,0000,0000,,V-V-Vú?!
Dialogue: 0,0:13:30.62,0:13:32.36,Script,,0000,0000,0000,,Wa! Satoru-kun!
Dialogue: 0,0:13:33.27,0:13:36.18,Script,,0000,0000,0000,,Em bị shock đến nổi hồn xiêu phách tán rồi.
Dialogue: 0,0:13:36.18,0:13:37.80,Script,,0000,0000,0000,,Này, Satoru-kun?
Dialogue: 0,0:13:40.19,0:13:41.47,Script,,0000,0000,0000,,Em bình tĩnh lại chưa?
Dialogue: 0,0:13:41.69,0:13:43.57,Script,,0000,0000,0000,,Dạ... rồi.
Dialogue: 0,0:13:44.31,0:13:47.48,Script,,0000,0000,0000,,Mà sao, đột nhiên chị lại nhờ chuyện đó?
Dialogue: 0,0:13:47.48,0:13:49.41,Script,,0000,0000,0000,,Là vì... à...
Dialogue: 0,0:13:50.66,0:13:53.71,Script,,0000,0000,0000,,Em nghĩ ngực của chị thế nào, Satoru-kun?
Dialogue: 0,0:13:56.08,0:13:57.62,Script,,0000,0000,0000,,Nếu chị hỏi em thì...
Dialogue: 0,0:13:59.61,0:14:05.12,Script,,0000,0000,0000,,Chị biết, trông chúng thật đồ sộ và khó coi phải không?
Dialogue: 0,0:14:05.12,0:14:06.71,Script,,0000,0000,0000,,K-Không... không phải.
Dialogue: 0,0:14:07.14,0:14:10.32,Script,,0000,0000,0000,,Là con trai nên em không hiểu chứ...
Dialogue: 0,0:14:10.32,0:14:12.21,Script,,0000,0000,0000,,... ngực bự làm chị bị nặng vai lắm.
Dialogue: 0,0:14:12.21,0:14:14.13,Script,,0000,0000,0000,,Và khi chạy chúng cứ cọ vào áo còn đau hơn nữa.
Dialogue: 0,0:14:14.42,0:14:17.15,Script,,0000,0000,0000,,Bạn khác giới thì nhìn chị với ánh mắt kỳ lạ,
Dialogue: 0,0:14:17.15,0:14:19.30,Script,,0000,0000,0000,,Còn bạn đồng giới thì lại ghen tị với chị.
Dialogue: 0,0:14:19.36,0:14:22.54,Script,,0000,0000,0000,,{\an8}Ngoài ra còn rất nhiều rắc rối khác nữa.
Dialogue: 0,0:14:19.71,0:14:22.54,Script,,0000,0000,0000,,A... sự lãng mạn thế là đã cuốn theo chiều gió.
Dialogue: 0,0:14:23.01,0:14:25.48,Script,,0000,0000,0000,,Nghe có vẻ chị gặp nhiều rắc rối quá nhỉ?
Dialogue: 0,0:14:25.48,0:14:28.11,Script,,0000,0000,0000,,Chị muốn nói điều này với bạn chị lắm nhưng...
Dialogue: 0,0:14:35.51,0:14:37.74,Script,,0000,0000,0000,,Cậu muốn chết hả!?
Dialogue: 0,0:14:37.74,0:14:41.35,Script,,0000,0000,0000,,Tìm một thằng mà bóp cái "huyền thoại Cameron" chết dịch đó đi!
Dialogue: 0,0:14:41.35,0:14:43.93,Script,,0000,0000,0000,,Bóp đến chừng nào nó bể ra thì thôi!
Dialogue: 0,0:14:43.93,0:14:47.06,Script,,0000,0000,0000,,Và cô ấy đã cho chị một lời khuyên quý báu.
Dialogue: 0,0:14:47.06,0:14:48.93,Script,,0000,0000,0000,,Cái đó mà cho là lời khuyên sao?
Dialogue: 0,0:14:49.42,0:14:51.17,Script,,0000,0000,0000,,Chị cũng đã hỏi rất nhiều người khác,
Dialogue: 0,0:14:51.51,0:14:53.50,Script,,0000,0000,0000,,và họ đều nói y hệt vây.
Dialogue: 0,0:14:53.50,0:14:55.21,Script,,0000,0000,0000,,Mà nghe nói nó hiệu nghiệm lắm.
Dialogue: 0,0:14:57.60,0:15:01.59,Script,,0000,0000,0000,,Con gái mà được con trai matxa ngực sẽ làm ngực nhỏ đi.
Dialogue: 0,0:15:01.59,0:15:04.74,Script,,0000,0000,0000,,Ờ thì dĩ nhiên là mấy chị nhà ta ghen với chị cũng phải.
Dialogue: 0,0:15:04.74,0:15:07.49,Script,,0000,0000,0000,,Mayo-nee, chị nên kiếm bạn tốt hơn mà chơi đi.
Dialogue: 0,0:15:08.20,0:15:11.65,Script,,0000,0000,0000,,Vì vậy chị mới muốn thử xem sao...
Dialogue: 0,0:15:12.19,0:15:15.49,Script,,0000,0000,0000,,... mà chỉ có em là người khác giới mà chị gần gũi thôi.
Dialogue: 0,0:15:17.41,0:15:22.21,Script,,0000,0000,0000,,Vậy thì... thế nhé, Satoru-kun.
Dialogue: 0,0:15:23.96,0:15:26.93,Script,,0000,0000,0000,,Chị muốn em... vuốt ve... ngực chị...
Dialogue: 0,0:15:27.93,0:15:31.29,Script,,0000,0000,0000,,Nếu chị đã có lòng thì em xin nhận vậy...
Dialogue: 0,0:15:34.63,0:15:37.77,Script,,0000,0000,0000,,Dạ... thật sự không sao chứ ạ?
Dialogue: 0,0:15:38.40,0:15:39.25,Script,,0000,0000,0000,,Ừ.
Dialogue: 0,0:15:40.40,0:15:42.13,Script,,0000,0000,0000,,Em cứ làm đi.
Dialogue: 0,0:15:45.99,0:15:48.34,Script,,0000,0000,0000,,Này, sao em lại cởi áo chị ra thế?
Dialogue: 0,0:15:48.34,0:15:52.50,Script,,0000,0000,0000,,Thuốc đắng dã tật mà, làm trực tiếp sẽ có hiệu quả hơn.
Dialogue: 0,0:15:52.50,0:15:56.11,Script,,0000,0000,0000,,Uhm... Nghe... có vẻ hợp lý..
Dialogue: 0,0:15:56.78,0:15:59.93,Script,,0000,0000,0000,,Vậy thì... em bắt đầu đây, Mayo-nee.
Dialogue: 0,0:15:59.93,0:16:01.18,Script,,0000,0000,0000,,Uhm.
Dialogue: 0,0:16:07.54,0:16:09.16,Script,,0000,0000,0000,,Thế nào, Mayo-nee?
Dialogue: 0,0:16:09.43,0:16:10.74,Script,,0000,0000,0000,,Được chứ ạ?
Dialogue: 0,0:16:14.59,0:16:17.40,Script,,0000,0000,0000,,Chị không biết, nhưng...
Dialogue: 0,0:16:19.11,0:16:21.76,Script,,0000,0000,0000,,... cơ thể chị... đang nóng lên.
Dialogue: 0,0:16:22.63,0:16:25.25,Script,,0000,0000,0000,,Đầu chị... bắt đầu trống rỗng.
Dialogue: 0,0:16:30.04,0:16:34.97,Script,,0000,0000,0000,,Chị muốn em vuốt ve nó... lâu hơn chút nữa.
Dialogue: 0,0:16:35.33,0:16:36.53,Script,,0000,0000,0000,,Mayo-nee...
Dialogue: 0,0:16:39.96,0:16:44.08,Script,,0000,0000,0000,,E-Em đâu cần phải bóp 2 đầu vú chị chứ?
Dialogue: 0,0:16:44.43,0:16:47.38,Script,,0000,0000,0000,,Nhưng chẳng phải nó cũng là ngực sao?
Dialogue: 0,0:16:47.72,0:16:50.69,Script,,0000,0000,0000,,Ờ thì đúng là... vậy.
Dialogue: 0,0:16:54.22,0:16:59.49,Script,,0000,0000,0000,,S-Satoru-kun, sau khi em bóp chỗ đó...
Dialogue: 0,0:16:59.49,0:17:01.24,Script,,0000,0000,0000,,... chị cảm thấy khó thở quá.
Dialogue: 0,0:17:02.97,0:17:04.77,Script,,0000,0000,0000,,Khó thở lắm.
Dialogue: 0,0:17:05.66,0:17:08.81,Script,,0000,0000,0000,,{\i1}Mayo-nee, mới khúc dạo đầu thôi mà chị ấy đã tuyệt thế này.{\i0}
Dialogue: 0,0:17:08.81,0:17:10.34,Script,,0000,0000,0000,,Chị thật là đáng yêu.
Dialogue: 0,0:17:17.92,0:17:23.88,Script,,0000,0000,0000,,Này... không yêu nhau mà làm viêc này là sai trái đúng không?
Dialogue: 0,0:17:24.29,0:17:26.25,Script,,0000,0000,0000,,Hả, e-e-em xin lỗi!
Dialogue: 0,0:17:26.25,0:17:28.16,Script,,0000,0000,0000,,Chỉ vì chị quá dễ thương nên...
Dialogue: 0,0:17:28.16,0:17:31.76,Script,,0000,0000,0000,,Đ-Đồ ngốc! Người như chị thì có điểm gì dễ thương chứ?
Dialogue: 0,0:17:31.76,0:17:34.10,Script,,0000,0000,0000,,Gương mặt gợi tình đó của chị...
Dialogue: 0,0:17:34.56,0:17:36.53,Script,,0000,0000,0000,,Tuy có vẻ rất nghiêm nghị nhưng kì thực lại rất đáng yêu. Rồi tính cách của chị...
Dialogue: 0,0:17:37.20,0:17:39.74,Script,,0000,0000,0000,,Và bộ ngực bụ bẫm của chị cũng thật đáng yêu.
Dialogue: 0,0:17:40.62,0:17:42.67,Script,,0000,0000,0000,,Vì thế, Mayo-nee...
Dialogue: 0,0:17:43.04,0:17:44.99,Script,,0000,0000,0000,,Em vẫn luôn yêu chị.
Dialogue: 0,0:17:48.31,0:17:51.18,Script,,0000,0000,0000,,X-xin lỗi, em không cố ý làm chị thấy khó xử đâu.
Dialogue: 0,0:17:51.62,0:17:52.95,Script,,0000,0000,0000,,À...
Dialogue: 0,0:18:00.85,0:18:02.49,Script,,0000,0000,0000,,Bây giờ thì hòa nhé.
Dialogue: 0,0:18:03.10,0:18:05.70,Script,,0000,0000,0000,,{\i1}Mayo-nee, chị thật sự đáng yêu quá.{\i0}
Dialogue: 0,0:18:06.12,0:18:09.16,Script,,0000,0000,0000,,Vậy chúng ta sẽ làm gì tiếp theo đây nà?
Dialogue: 0,0:18:09.16,0:18:10.53,Script,,0000,0000,0000,,... "tiếp theo" nghĩa là...
Dialogue: 0,0:18:10.53,0:18:13.63,Script,,0000,0000,0000,,Em đã tỏ tình với chị và chúng ta cũng đã hôn nhau rồi.
Dialogue: 0,0:18:14.58,0:18:16.32,Script,,0000,0000,0000,,Vậy mà em không muốn làm tới luôn sao?
Dialogue: 0,0:18:17.43,0:18:18.25,Script,,0000,0000,0000,,Làm... chuyện ấy đấy.
Dialogue: 0,0:18:19.65,0:18:21.66,Script,,0000,0000,0000,,Chị bạo thật đấy, Mayo-nee.
Dialogue: 0,0:18:21.66,0:18:23.42,Script,,0000,0000,0000,,Không phải thế à?
Dialogue: 0,0:18:23.42,0:18:27.65,Script,,0000,0000,0000,,Chị nghe nhỏ bạn nói sau đoạn xoa bóp ngực sẽ là đoạn đi theo "tiếng gọi nơi hoang dã" đấy.
Dialogue: 0,0:18:29.51,0:18:33.92,Script,,0000,0000,0000,,Hay là em không muốn vuốt ve âu yếm chị?
Dialogue: 0,0:18:34.33,0:18:36.82,Script,,0000,0000,0000,,Tất nhiên em muốn chứ!
Dialogue: 0,0:18:36.82,0:18:37.87,Script,,0000,0000,0000,,Chị nín đi mà.
Dialogue: 0,0:18:38.65,0:18:44.34,Script,,0000,0000,0000,,Nhưng... chị không có kinh nghiệm gì trong việc này cả.
Dialogue: 0,0:18:44.58,0:18:47.61,Script,,0000,0000,0000,,Nên em cứ làm gì tuỳ thích cũng được.
Dialogue: 0,0:18:48.51,0:18:52.02,Script,,0000,0000,0000,,Vậy... xin tuân lệnh quý cô hehehe.
Dialogue: 0,0:18:53.91,0:18:55.63,Script,,0000,0000,0000,,Em muốn bắt đầu từ chỗ đó sao?
Dialogue: 0,0:18:55.63,0:18:58.04,Script,,0000,0000,0000,,Mayo-nee, chị ướt thật đấy.
Dialogue: 0,0:18:58.45,0:19:00.32,Script,,0000,0000,0000,,Chỉ mới xoa ngực mà đã ướt thế này rồi sao?
Dialogue: 0,0:19:00.72,0:19:02.57,Script,,0000,0000,0000,,Em không ngờ chị lại dâm như thế đấy.
Dialogue: 0,0:19:02.57,0:19:04.25,Script,,0000,0000,0000,,V-Vậy à?
Dialogue: 0,0:19:04.57,0:19:07.69,Script,,0000,0000,0000,,Em ghét con gái...  đồi trụy sao?
Dialogue: 0,0:19:08.12,0:19:12.98,Script,,0000,0000,0000,,Em yêu chị vì chị thật là dễ thương, ngực to và còn gợi tình nữa.
Dialogue: 0,0:19:13.37,0:19:14.40,Script,,0000,0000,0000,,Đồ ngốc.
Dialogue: 0,0:19:28.69,0:19:30.29,Script,,0000,0000,0000,,Mayo-nee, em hứng lắm rồi...
Dialogue: 0,0:19:42.11,0:19:44.06,Script,,0000,0000,0000,,E-Em cứ làm đi.
Dialogue: 0,0:19:44.51,0:19:46.60,Script,,0000,0000,0000,,Vậy, em vào nhé.
Dialogue: 0,0:19:53.51,0:19:55.46,Script,,0000,0000,0000,,Mayo-nee, chị không sao chứ?
Dialogue: 0,0:19:57.23,0:20:00.01,Script,,0000,0000,0000,,Chị không sao...
Dialogue: 0,0:20:01.38,0:20:05.91,Script,,0000,0000,0000,,Bên trong chị ướt và khít quá Mayo-nee, em sướng điên lên mất.
Dialogue: 0,0:20:13.50,0:20:14.48,Script,,0000,0000,0000,,Đây là...
Dialogue: 0,0:20:15.77,0:20:17.10,Script,,0000,0000,0000,,Nó thật là mạnh mẽ.
Dialogue: 0,0:20:30.45,0:20:32.45,Script,,0000,0000,0000,,Đầu vú chị... đừng mà!
Dialogue: 0,0:20:32.45,0:20:35.55,Script,,0000,0000,0000,,Đầu vú của Mayo-nee mềm và ngon thật đấy.
Dialogue: 0,0:20:36.14,0:20:37.40,Script,,0000,0000,0000,,Không.
Dialogue: 0,0:20:40.85,0:20:42.16,Script,,0000,0000,0000,,Đồ ngốc.
Dialogue: 0,0:20:44.14,0:20:47.08,Script,,0000,0000,0000,,Em yêu mọi thứ của Mayo-nee.
Dialogue: 0,0:20:48.94,0:20:49.92,Script,,0000,0000,0000,,Không!
Dialogue: 0,0:21:01.77,0:21:02.88,Script,,0000,0000,0000,,Chi đang ra!
Dialogue: 0,0:21:04.35,0:21:06.00,Script,,0000,0000,0000,,A, không!
Dialogue: 0,0:21:06.00,0:21:07.29,Script,,0000,0000,0000,,Chị sắp ra rồi!
Dialogue: 0,0:21:08.93,0:21:10.16,Script,,0000,0000,0000,,Chị đang ra!
Dialogue: 0,0:21:29.37,0:21:32.33,Script,,0000,0000,0000,,Thật lòng em luôn muốn làm điều này với chị.
Dialogue: 0,0:21:33.86,0:21:34.73,Script,,0000,0000,0000,,Ngốc quá.
Dialogue: 0,0:21:38.23,0:21:40.02,Script,,0000,0000,0000,,Mayo-nee~!
Dialogue: 0,0:21:40.76,0:21:43.84,Script,,0000,0000,0000,,Geez, em cứ như là trẻ con ấy.
Dialogue: 0,0:21:45.21,0:21:48.04,Script,,0000,0000,0000,,Em thích ngực chị thế sao?
Dialogue: 0,0:21:48.04,0:21:50.82,Script,,0000,0000,0000,,Vâng, chúng to và mềm lắm.
Dialogue: 0,0:21:52.14,0:21:54.12,Script,,0000,0000,0000,,Thế là có vấn đề đấy.
Dialogue: 0,0:21:54.12,0:21:55.08,Script,,0000,0000,0000,,Sao ạ?
Dialogue: 0,0:21:55.62,0:21:58.98,Script,,0000,0000,0000,,Em thích ngực to đúng không?
Dialogue: 0,0:22:00.07,0:22:03.60,Script,,0000,0000,0000,,Vậy có nghĩa em không thích các cô gái ngực nhỏ.
Dialogue: 0,0:22:05.23,0:22:09.46,Script,,0000,0000,0000,,Nói vậy là chị phải giữ bộ ngực này cho em rồi.
Dialogue: 0,0:22:09.46,0:22:11.87,Script,,0000,0000,0000,,Thế này thì chị không thể làm ngực nhỏ được.
Dialogue: 0,0:22:13.14,0:22:16.77,Script,,0000,0000,0000,,Vì vậy chị không cho em xoa ngực chị nữa.
Dialogue: 0,0:23:52.28,0:23:54.75,Script,,0000,0000,0000,,Thức tỉnh đi, bộ ngực ái tình!
Dialogue: 0,0:23:54.75,0:23:55.96,Script,,0000,0000,0000,,Bóp này, bóp này~!
Dialogue: 0,0:23:57.38,0:23:59.82,Script,,0000,0000,0000,,Em đang làm gì thế?
Dialogue: 0,0:23:59.82,0:24:01.32,Script,,0000,0000,0000,,Cứ "bóp, bóp" cái gì đấy...
Dialogue: 0,0:24:01.32,0:24:03.18,Script,,0000,0000,0000,,em có còn là trẻ con nữa đâu.
Dialogue: 0,0:24:03.46,0:24:06.52,Script,,0000,0000,0000,,Chẳng phải sau đó chúng ta cũng đã "chơi" với nhau hơi nhiều đó sao?
Dialogue: 0,0:24:33.48,0:24:35.23,Script,,0000,0000,0000,,Thế này được chưa, nya~!
Dialogue: 0,0:24:36.50,0:24:38.01,Script,,0000,0000,0000,,Uhm.
Dialogue: 0,0:24:40.38,0:24:41.22,Script,,0000,0000,0000,,Nya!
Dialogue: 0,0:24:41.22,0:24:42.12,Script,,0000,0000,0000,,Đau em!
Dialogue: 0,0:24:42.12,0:24:43.39,Script,,0000,0000,0000,,Nyah!
Dialogue: 0,0:24:45.21,0:24:50.49,Script,,0000,0000,0000,,A! Em ghét không được động vào ngực chị! Đáng ghét! Đáng ghét! Đáng ghét! Đáng ghét! Đáng ghét!
Dialogue: 0,0:24:51.49,0:24:53.99,Script,,0000,0000,0000,,Geez, thật chứng nào tật nấy.
Dialogue: 0,0:24:54.40,0:24:56.20,Script,,0000,0000,0000,,Hôm nay là ngoại lệ vậy.
Dialogue: 0,0:25:00.04,0:25:02.57,Script,,0000,0000,0000,,Vú của Mayo-nee tuyệt quá.
Dialogue: 0,0:25:04.69,0:25:09.39,Script,,0000,0000,0000,,Em có bú mạnh thế nào đi nữa thì chị cũng không thấy sướng đâu.
Dialogue: 0,0:25:11.52,0:25:15.03,Script,,0000,0000,0000,,Nhưng chỗ này ướt hết cả rồi.
Dialogue: 0,0:25:15.54,0:25:18.05,Script,,0000,0000,0000,,Mayo-nee, em giao hết "cục cưng" của mình cho chị xử lý đấy.
Dialogue: 0,0:25:18.05,0:25:19.36,Script,,0000,0000,0000,,Dùng ngực của chị nhé.
Dialogue: 0,0:25:19.36,0:25:22.30,Script,,0000,0000,0000,,Geez, em thật sự thích nó vậy sao?
Dialogue: 0,0:25:32.33,0:25:34.92,Script,,0000,0000,0000,,Cái của em đang co giật này.
Dialogue: 0,0:25:35.33,0:25:37.18,Script,,0000,0000,0000,,Sướng quá chị ơi.
Dialogue: 0,0:25:37.34,0:25:39.37,Script,,0000,0000,0000,,Em sẽ làm chị thấy sướng theo luôn nhé.
Dialogue: 0,0:25:53.25,0:25:56.20,Script,,0000,0000,0000,,Em đang ra, Mayo-nee. Em đang ra trên ngực chị.
Dialogue: 0,0:26:02.77,0:26:05.28,Script,,0000,0000,0000,,Tuyệt thật, nhiều thế này...
Dialogue: 0,0:26:05.28,0:26:08.39,Script,,0000,0000,0000,,Nó giống như sữa chị vừa phun ra vậy.
Dialogue: 0,0:26:08.39,0:26:09.40,Script,,0000,0000,0000,,Đồ ngốc.
Dialogue: 0,0:26:10.44,0:26:12.31,Script,,0000,0000,0000,,Em làm nhé, Mayo-nee.
Dialogue: 0,0:26:12.73,0:26:13.55,Script,,0000,0000,0000,,Ừh.
Dialogue: 0,0:26:19.52,0:26:20.49,Script,,0000,0000,0000,,Em cho nó vào lỗ của chị đây!
Dialogue: 0,0:26:21.66,0:26:25.18,Script,,0000,0000,0000,,Tuyệt quá, Mayo-nee. Vú chị tung lên tung xuống này.
Dialogue: 0,0:26:27.19,0:26:31.47,Script,,0000,0000,0000,,Bởi vì... em làm tình nghệ thuật quá.
Dialogue: 0,0:26:31.47,0:26:34.07,Script,,0000,0000,0000,,Hông chị cũng đang tự chuyển động theo.
Dialogue: 0,0:26:34.07,0:26:36.57,Script,,0000,0000,0000,,Chị sướng quá, sướng điên lên mất!
Dialogue: 0,0:26:39.58,0:26:42.61,Script,,0000,0000,0000,,Mayo-nee, E-Em... sắp ra rồi.
Dialogue: 0,0:26:44.91,0:26:46.85,Script,,0000,0000,0000,,Chị cũng thế...
Dialogue: 0,0:26:46.85,0:26:48.89,Script,,0000,0000,0000,,Chị không giữ được nữa, chị đang ra.
Dialogue: 0,0:26:53.60,0:26:56.79,Script,,0000,0000,0000,,Ra rồi! Ra rồi! Ra rồi! Ra rồi! Ra rồi!
Dialogue: 0,0:27:07.71,0:27:11.54,Script,,0000,0000,0000,,Nó tràn ngập... trong chị...
Dialogue: 0,0:27:12.03,0:27:15.93,Script,,0000,0000,0000,,Vậy thì, sau đó chị có thể cho ra sữa đúng không?
Dialogue: 0,0:27:15.93,0:27:19.65,Script,,0000,0000,0000,,Đồ ngốc, nếu chị có thai thì làm sao đây-
Dialogue: 0,0:27:20.25,0:27:21.96,Script,,0000,0000,0000,,Em biết rồi, Mayo-nee.
Dialogue: 0,0:27:22.14,0:27:23.45,Script,,0000,0000,0000,,Em sẽ chịu trách nhiệm cho!
Dialogue: 0,0:27:23.69,0:27:28.19,Script,,0000,0000,0000,,Vậy trước khi sinh, ngoài ngực chị ra, còn những món khác thì thôi nhé!
Dialogue: 0,0:27:30.70,0:27:33.38,Script,,0000,0000,0000,,Mình mới chỉ bắt đầu cất bước trên con đường này...
Dialogue: 0,0:27:33.38,0:27:36.89,Script,,0000,0000,0000,,Con đường dài bất tận... hướng tới bộ ngực xa xăm!
