Dialogue: 0,0:00:58.03,0:01:01.08,Default,,0000,0000,0000,,Thôi nào, đủ rồi đấy.
Dialogue: 0,0:01:04.25,0:01:05.93,Default,,0000,0000,0000,,Thôi đi, Makoto-san!
Dialogue: 0,0:01:07.17,0:01:09.32,Default,,0000,0000,0000,,Con chỉ đùa một chút thôi mà.
Dialogue: 0,0:01:09.43,0:01:11.36,Default,,0000,0000,0000,,Dì không thích kiểu đùa giỡn đó đâu.
Dialogue: 0,0:01:12.24,0:01:14.01,Default,,0000,0000,0000,,Vậy thì dì phải nói ngay từ đầu chứ.
Dialogue: 0,0:01:14.02,0:01:15.99,Default,,0000,0000,0000,,Hừ, dì biết làm sao được!
Dialogue: 0,0:01:16.34,0:01:18.84,Default,,0000,0000,0000,,Cơ thể dì nó tự hành động đấy chứ.
Dialogue: 0,0:01:25.40,0:01:27.17,Default,,0000,0000,0000,,Cứ xem như dì không nói gì cả.
Dialogue: 0,0:01:34.11,0:01:39.78,game 1,,0000,0000,0000,,{\fad(0,300)\pos(176,43)}Natsuki
Dialogue: 0,0:01:34.11,0:01:39.78,game 1,,0000,0000,0000,,{\fad(0,300)\pos(177,92)}Makoto
Dialogue: 0,0:01:34.11,0:01:39.78,game 1,,0000,0000,0000,,{\fad(0,300)\pos(177,147)}Yuna
Dialogue: 0,0:01:34.11,0:01:39.78,game 1,,0000,0000,0000,,{\fad(0,1021)\pos(175,203)}Misaki{TS}
