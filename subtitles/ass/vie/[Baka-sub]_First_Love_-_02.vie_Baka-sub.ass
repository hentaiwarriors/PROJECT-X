Dialogue: 0,0:01:25.45,0:01:27.80,Default,,0000,0000,0000,,Mùa hè sắp đến rồi.
Dialogue: 0,0:01:51.82,0:01:53.05,Default,,0000,0000,0000,,Gì hả?
Dialogue: 0,0:01:54.31,0:01:57.83,Default,,0000,0000,0000,,Ơ... Tớ có thể chứ?
Dialogue: 0,0:01:58.50,0:02:00.22,Default,,0000,0000,0000,,Không hẳn là không!
Dialogue: 0,0:02:16.85,0:02:17.85,Default,,0000,0000,0000,,Cô ấy ướt rồi...
Dialogue: 0,0:02:19.94,0:02:22.11,Default,,0000,0000,0000,,Cô ấy cũng muốn mà phải không?
Dialogue: 0,0:02:27.86,0:02:29.66,Default,,0000,0000,0000,,Còn chờ gì nữa...
Dialogue: 0,0:02:30.65,0:02:32.32,Default,,0000,0000,0000,,Có được không đây?
Dialogue: 0,0:02:39.35,0:02:41.49,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:02:41.49,0:02:42.83,Default,,0000,0000,0000,,Cậu không sao chứ?
Dialogue: 0,0:02:43.65,0:02:45.38,Default,,0000,0000,0000,,Không sao cả!
Dialogue: 0,0:04:23.42,0:04:26.34,Default,,0000,0000,0000,,Tớ muốn hôn cậu...
Dialogue: 0,0:05:28.39,0:05:29.55,Default,,0000,0000,0000,,Gì hả?
Dialogue: 0,0:05:29.55,0:05:31.80,Default,,0000,0000,0000,,Không có gì...
Dialogue: 0,0:05:45.75,0:05:48.05,Default,,0000,0000,0000,,Hoàng hôn thật đẹp.
Dialogue: 0,0:05:50.11,0:05:53.35,Default,,0000,0000,0000,,Tên cô ấy là Aoba Chinatsu.
Dialogue: 0,0:05:53.35,0:05:58.07,Default,,0000,0000,0000,,Hiện tại chúng tôi đang là người yêu của nhau.
Dialogue: 0,0:06:11.33,0:06:17.06,Default,,0000,0000,0000,,À, tớ sẽ ngồi kế bên cậu, tên tớ là Sahara Satoshi. Hân hạnh được biết cậu.
Dialogue: 0,0:06:19.02,0:06:20.23,Default,,0000,0000,0000,,Tôi cũng vậy.
Dialogue: 0,0:06:26.56,0:06:29.46,Default,,0000,0000,0000,,Mình đã làm sai điều gì ư?
Dialogue: 0,0:06:29.46,0:06:31.92,Default,,0000,0000,0000,,Ấn tượng đầu tiên cũng là ấn tượng tồi tệ nhất.
Dialogue: 0,0:06:33.35,0:06:35.29,Default,,0000,0000,0000,,Nè, Aoba-san!
Dialogue: 0,0:06:36.05,0:06:42.15,Default,,0000,0000,0000,,Nhưng tôi hi vọng chúng tôi sẽ thân thiết hơn nên tôi vẫn cố nói chuyện với cô ấy.
Dialogue: 0,0:06:43.25,0:06:48.14,Default,,0000,0000,0000,,Ơ, cậu có thể cho tớ coi chung được không?
Dialogue: 0,0:06:50.52,0:06:51.68,Default,,0000,0000,0000,,Cảm ơn.
Dialogue: 0,0:06:52.66,0:06:55.70,Default,,0000,0000,0000,,Thế là tôi dần quen với việc mỗi ngày của mình đều trôi qua như thế...
Dialogue: 0,0:07:05.36,0:07:07.04,Default,,0000,0000,0000,,Cậu vừa nói gì?
Dialogue: 0,0:07:07.80,0:07:09.09,Default,,0000,0000,0000,,Tôi nói là...
Dialogue: 0,0:07:09.89,0:07:13.83,Default,,0000,0000,0000,,Tôi muốn cậu hẹn hò với tôi.
Dialogue: 0,0:07:13.83,0:07:16.34,Default,,0000,0000,0000,,Gì hả? Hổng muốn à?
Dialogue: 0,0:07:17.05,0:07:19.55,Default,,0000,0000,0000,,Không không, sao mà không muốn được chứ!
Dialogue: 0,0:07:23.12,0:07:27.06,Default,,0000,0000,0000,,Một ngày nọ, đột nhiên tôi bị cô ấy tỏ tình!
Dialogue: 0,0:07:45.53,0:07:49.22,Default,,0000,0000,0000,,Cái hôn của cô ấy thật tuyệt{a lot better}!
Dialogue: 0,0:07:54.47,0:07:55.35,Default,,0000,0000,0000,,Gì hả?
Dialogue: 0,0:07:55.92,0:07:58.24,Default,,0000,0000,0000,,Không, không có gì...
Dialogue: 0,0:08:10.71,0:08:12.09,Default,,0000,0000,0000,,Cô ấy ướt sũng rồi.
Dialogue: 0,0:08:50.10,0:08:53.80,Default,,0000,0000,0000,,Ơ... Cậu không cần phải...
Dialogue: 0,0:08:54.44,0:08:55.88,Default,,0000,0000,0000,,Hổng muốn à?
Dialogue: 0,0:08:56.63,0:08:59.27,Default,,0000,0000,0000,,T-Tớ vui lắm!
Dialogue: 0,0:08:59.27,0:09:01.94,Default,,0000,0000,0000,,Thế là được rồi.
Dialogue: 0,0:09:39.80,0:09:40.97,Default,,0000,0000,0000,,Nguy rồi!
Dialogue: 0,0:10:08.42,0:10:10.57,Default,,0000,0000,0000,,Cậu ổn chứ?
Dialogue: 0,0:10:12.22,0:10:14.99,Default,,0000,0000,0000,,Vậy tớ đút vào nhé.
Dialogue: 0,0:10:21.26,0:10:22.76,Default,,0000,0000,0000,,Aoba-san...
Dialogue: 0,0:11:46.15,0:11:52.08,Default,,0000,0000,0000,,Aoba-san, tớ yêu cậu nhiều lắm.
Dialogue: 0,0:12:02.56,0:12:06.17,Default,,0000,0000,0000,,Chúng tôi hẹn hò cũng được một khoảng thời gian,
Dialogue: 0,0:12:06.17,0:12:10.61,Default,,0000,0000,0000,,nhưng tôi chả bao giờ nhìn thấy cô ấy cười.
Dialogue: 0,0:12:10.61,0:12:12.61,Default,,0000,0000,0000,,Ngược lại...
Dialogue: 0,0:12:14.31,0:12:20.86,Default,,0000,0000,0000,,Gần đây thấy cô ấy có vẻ còn buồn hơn trước.
Dialogue: 0,0:12:22.41,0:12:25.25,Default,,0000,0000,0000,,Không. Không có gì cả.
Dialogue: 0,0:12:29.73,0:12:33.07,Default,,0000,0000,0000,,Tôi nghĩ chắc do tính cách cô ấy như thế,
Dialogue: 0,0:12:33.07,0:12:37.37,Default,,0000,0000,0000,,nhưng cứ bị nhìn như vậy thì tự tin của tôi nó cũng bay theo gió.
Dialogue: 0,0:12:41.00,0:12:44.40,Default,,0000,0000,0000,,Cô ấy có thật sự yêu mình không?
Dialogue: 0,0:12:45.57,0:12:48.79,Default,,0000,0000,0000,,Cứ như là cô ấy mệt mỏi vì phải hẹn hò với mình.
Dialogue: 0,0:12:48.79,0:12:51.04,Default,,0000,0000,0000,,Chắc mình đã làm cho cô thất vọng,
Dialogue: 0,0:12:51.04,0:12:55.45,Default,,0000,0000,0000,,nhưng vì cô ấy đã tỏ tình trước nên không thể trở mặt được...
Dialogue: 0,0:12:55.45,0:12:56.81,Default,,0000,0000,0000,,Chắc là vậy.
Dialogue: 0,0:12:56.81,0:13:01.02,Default,,0000,0000,0000,,Chết tiệt, mình phải làm sao đây?
Dialogue: 0,0:13:01.85,0:13:04.10,Default,,0000,0000,0000,,Nè! Nè, có nghe không hả?
Dialogue: 0,0:13:04.10,0:13:05.82,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:13:05.82,0:13:08.42,Default,,0000,0000,0000,,Đến nhà tôi rồi.
Dialogue: 0,0:13:11.05,0:13:15.44,Default,,0000,0000,0000,,Thế à! Vậy hẹn mai gặp lại!
Dialogue: 0,0:13:15.70,0:13:19.65,Default,,0000,0000,0000,,Nè, nếu được thì vào trong ngồi chơi một chút?
Dialogue: 0,0:13:32.21,0:13:37.43,Default,,0000,0000,0000,,Ơ... Chiaki-chan, phải không?
Dialogue: 0,0:13:37.84,0:13:39.61,Default,,0000,0000,0000,,Có gì không?
Dialogue: 0,0:13:40.91,0:13:44.17,Default,,0000,0000,0000,,Gần đây, Onee-chan trông rất là vui!
Dialogue: 0,0:13:44.17,0:13:46.47,Default,,0000,0000,0000,,Thì ra nguyên do là đây!
Dialogue: 0,0:13:47.44,0:13:49.96,Default,,0000,0000,0000,,Ở nhà chị ấy có cười sao?
Dialogue: 0,0:13:49.96,0:13:51.13,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:13:51.13,0:13:56.70,Default,,0000,0000,0000,,Nhưng khi chị ấy nhíu mài lại nhìn có vẻ rất giận phải không?
Dialogue: 0,0:13:57.45,0:14:02.70,Default,,0000,0000,0000,,Thật ra đó là lúc chị ấy đang rất vui nhưng mà ráng nhịn để không cười đấy.
Dialogue: 0,0:14:02.70,0:14:07.46,Default,,0000,0000,0000,,Chị ấy càng làm như vậy nghĩa là chị ấy càng vui!
Dialogue: 0,0:14:07.46,0:14:11.10,Default,,0000,0000,0000,,Chị em hay xấu hổ ấy mà!
Dialogue: 0,0:14:11.99,0:14:13.76,Default,,0000,0000,0000,,Ra là vậy...
Dialogue: 0,0:14:13.76,0:14:16.31,Default,,0000,0000,0000,,Cô ấy đang vui!
Dialogue: 0,0:14:16.31,0:14:17.47,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:14:24.10,0:14:27.48,Default,,0000,0000,0000,,Em cậu đi đâu rồi?
Dialogue: 0,0:14:27.48,0:14:30.36,Default,,0000,0000,0000,,Ờ, hôm nay nó có lớp thư pháp.
Dialogue: 0,0:14:32.09,0:14:34.29,Default,,0000,0000,0000,,Ra là vậy.
Dialogue: 0,0:14:34.29,0:14:37.49,Default,,0000,0000,0000,,Cô ấy đang rất vui.
Dialogue: 0,0:14:37.49,0:14:41.48,Default,,0000,0000,0000,,Nhưng càng vui thì lại càng làm vẻ mặt đó.
Dialogue: 0,0:14:41.48,0:14:43.77,Default,,0000,0000,0000,,Vượt quá mức xấu hổ luôn rồi...
Dialogue: 0,0:14:46.11,0:14:48.90,Default,,0000,0000,0000,,Đang nhìn cái gì vậy hả?
Dialogue: 0,0:14:48.90,0:14:54.51,Default,,0000,0000,0000,,Vậy có nghĩa là cô ấy chỉ xấu hổ vì quá vui mà thôi nhỉ?
Dialogue: 0,0:14:54.51,0:14:56.40,Default,,0000,0000,0000,,Chắc là thật?
Dialogue: 0,0:15:08.14,0:15:11.59,Default,,0000,0000,0000,,Gì hả? Tự nhiên...
Dialogue: 0,0:15:11.59,0:15:15.45,Default,,0000,0000,0000,,Tớ nghĩ chúng ta có thể tiếp tục chuyện lúc nãy.
Dialogue: 0,0:15:15.45,0:15:16.68,Default,,0000,0000,0000,,Cậu không thích à?
Dialogue: 0,0:15:17.53,0:15:20.10,Default,,0000,0000,0000,,Không hẳn.
Dialogue: 0,0:15:20.10,0:15:23.97,Default,,0000,0000,0000,,Cái này là vui đúng không? Hi vọng là vậy.
Dialogue: 0,0:15:36.21,0:15:39.75,Default,,0000,0000,0000,,Ngực của cậu đẹp thật đấy.
Dialogue: 0,0:15:40.78,0:15:44.52,Default,,0000,0000,0000,,Nó chỉ được cái lớn thôi... ngoài ra chả có gì.
Dialogue: 0,0:15:45.29,0:15:48.21,Default,,0000,0000,0000,,Cái này cũng là vui đúng không?
Dialogue: 0,0:15:49.37,0:15:55.14,Default,,0000,0000,0000,,Không không, không phải kích cỡ thôi đâu, cả hình dáng cũng rất là tuyệt.
Dialogue: 0,0:15:56.60,0:15:58.99,Default,,0000,0000,0000,,Cứ tiếp tục như vầy chắc không sao?
Dialogue: 0,0:15:58.99,0:16:06.60,Default,,0000,0000,0000,,Chúng vừa nặng, vừa mềm lại còn ấm nữa...
Dialogue: 0,0:16:06.60,0:16:07.58,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:16:09.28,0:16:13.78,Default,,0000,0000,0000,,Chúng thật là vừa tay, không thể chê vào đâu được!
Dialogue: 0,0:16:19.82,0:16:22.14,Default,,0000,0000,0000,,Tớ yêu chúng lắm.
Dialogue: 0,0:16:35.59,0:16:38.69,Default,,0000,0000,0000,,Cảm giác giống như là cô ấy đang thả lỏng...
Dialogue: 0,0:16:41.47,0:16:43.29,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:16:43.29,0:16:48.94,Default,,0000,0000,0000,,Nhưng khi chị ấy nhíu mài lại nhìn có vẻ rất giận phải không?
Dialogue: 0,0:16:48.94,0:16:54.41,Default,,0000,0000,0000,,Thật ra đó là lúc chị ấy đang rất vui nhưng mà ráng nhịn để không cười đấy.
Dialogue: 0,0:16:55.49,0:17:00.29,Default,,0000,0000,0000,,Chị ấy càng làm như vậy nghĩa là chị ấy càng vui!
Dialogue: 0,0:17:00.29,0:17:04.05,Default,,0000,0000,0000,,Chị em hay xấu hổ ấy mà!
Dialogue: 0,0:17:05.30,0:17:10.55,Default,,0000,0000,0000,,Đó giờ mình không hề hay biết, nhưng em ấy đã nói vậy thì...
Dialogue: 0,0:17:15.99,0:17:19.45,Default,,0000,0000,0000,,Vẻ mặt của cậu khi bị kích thích rất là đáng yêu.
Dialogue: 0,0:17:20.97,0:17:25.09,Default,,0000,0000,0000,,Đừng có nói những lời kì quặc như vậy! Đồ ngốc!
Dialogue: 0,0:17:27.66,0:17:30.59,Default,,0000,0000,0000,,Ôi ôi, đừng có che!
Dialogue: 0,0:17:31.74,0:17:35.12,Default,,0000,0000,0000,,Những lời kì quặc? Là khen cô ấy đáng yêu à?
Dialogue: 0,0:17:35.12,0:17:36.95,Default,,0000,0000,0000,,Hông được!
Dialogue: 0,0:17:37.67,0:17:39.42,Default,,0000,0000,0000,,Cho dù là vậy...
Dialogue: 0,0:17:42.83,0:17:44.31,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:17:46.35,0:17:49.05,Default,,0000,0000,0000,,Cái tớ muốn thấy chính là vẻ mặt này.
Dialogue: 0,0:17:55.15,0:17:59.84,Default,,0000,0000,0000,,K-Khoan đã... Tại sao lại thành ra như vầy...
Dialogue: 0,0:18:00.37,0:18:03.25,Default,,0000,0000,0000,,Không sao đâu, không sao đâu!
Dialogue: 0,0:18:03.25,0:18:06.28,Default,,0000,0000,0000,,Cô ấy lại trở lại bình thường rồi.
Dialogue: 0,0:18:08.70,0:18:10.65,Default,,0000,0000,0000,,Mình làm được rồi!
Dialogue: 0,0:18:11.20,0:18:12.94,Default,,0000,0000,0000,,Cô ấy thật đáng yêu!
Dialogue: 0,0:18:16.03,0:18:20.43,Default,,0000,0000,0000,,Quả nhiên ngực của cậu là số một!
Dialogue: 0,0:18:20.43,0:18:24.42,Default,,0000,0000,0000,,Đừng có chơi đùa với chúng!
Dialogue: 0,0:18:24.92,0:18:27.72,Default,,0000,0000,0000,,Cậu thật là đáng yêu!
Dialogue: 0,0:18:31.46,0:18:33.35,Default,,0000,0000,0000,,Cô ấy cứ như sắp tan chảy.
Dialogue: 0,0:18:35.25,0:18:37.72,Default,,0000,0000,0000,,Nhìn đầu ti của cậu cứng lên này!
Dialogue: 0,0:18:40.68,0:18:44.49,Default,,0000,0000,0000,,Tiếng rên của cậu khi cảm nhận nó cũng thật đáng yêu!
Dialogue: 0,0:18:47.35,0:18:48.59,Default,,0000,0000,0000,,Aoba-san...
Dialogue: 0,0:18:56.60,0:19:02.00,Default,,0000,0000,0000,,Miệng cậu... thật là tuyệt!
Dialogue: 0,0:19:08.85,0:19:10.47,Default,,0000,0000,0000,,Aoba-san...
Dialogue: 0,0:19:17.79,0:19:21.99,Default,,0000,0000,0000,,Tớ ra đây!
Dialogue: 0,0:19:27.17,0:19:30.70,Default,,0000,0000,0000,,Aoba-san, cậu đáng yêu quá đi...
Dialogue: 0,0:19:31.92,0:19:33.67,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:19:34.98,0:19:37.79,Default,,0000,0000,0000,,Tự nhiên cậu thổi kèn cho tớ làm tớ giật cả mình.
Dialogue: 0,0:19:38.52,0:19:43.25,Default,,0000,0000,0000,,Tôi nghĩ làm như vậy cậu sẽ rất vui.
Dialogue: 0,0:19:51.50,0:19:54.03,Default,,0000,0000,0000,,Dâm thủy của cậu ra nhầy nhụa...
Dialogue: 0,0:19:54.03,0:19:56.25,Default,,0000,0000,0000,,Đừng có nói vậy!
Dialogue: 0,0:19:58.58,0:20:01.36,Default,,0000,0000,0000,,Thật tuyệt, nó ướt sũng rồi!
Dialogue: 0,0:20:01.35,0:20:04.00,Default,,0000,0000,0000,,Đừng có nhìn chằm chằm như vậy!
Dialogue: 0,0:20:04.59,0:20:07.82,Default,,0000,0000,0000,,Chỉ chơi đùa với ngực thôi mà cậu đã như vầy...
Dialogue: 0,0:20:07.82,0:20:10.58,Default,,0000,0000,0000,,Im đê, đồ ngốc!
Dialogue: 0,0:20:10.58,0:20:12.73,Default,,0000,0000,0000,,Cậu đáng yêu chết đi được!
Dialogue: 0,0:20:14.13,0:20:16.47,Default,,0000,0000,0000,,Aoba-san, tớ đút vào nhé.
Dialogue: 0,0:20:28.40,0:20:29.76,Default,,0000,0000,0000,,Cậu không sao chứ?
Dialogue: 0,0:20:31.15,0:20:33.58,Default,,0000,0000,0000,,Tôi nghĩ mình ra mất rồi.
Dialogue: 0,0:20:34.00,0:20:37.85,Default,,0000,0000,0000,,Bây giờ vẻ mặt của cậu cực kì đáng yêu!
Dialogue: 0,0:20:45.42,0:20:47.13,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:20:47.13,0:20:50.79,Default,,0000,0000,0000,,Là thật đó! Cậu đáng yêu lắm!
Dialogue: 0,0:20:51.21,0:20:53.21,Default,,0000,0000,0000,,Nhưng tôi...
Dialogue: 0,0:21:04.26,0:21:05.69,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:21:14.64,0:21:19.22,Default,,0000,0000,0000,,Tôi không muốn cậu thấy vẻ mặt đáng xấu hổ này!
Dialogue: 0,0:21:22.14,0:21:24.27,Default,,0000,0000,0000,,Làm gì có chuyện đó!
Dialogue: 0,0:21:26.88,0:21:31.51,Default,,0000,0000,0000,,Vẻ mặt của cậu khi sung sướng cực kì đáng yêu.
Dialogue: 0,0:21:36.45,0:21:41.35,Default,,0000,0000,0000,,Nó hông kì quặc chứ? Có thật hông?
Dialogue: 0,0:21:41.35,0:21:43.50,Default,,0000,0000,0000,,Ừm, tớ thích lắm.
Dialogue: 0,0:21:44.31,0:21:48.56,Default,,0000,0000,0000,,Tớ yêu cậu và cả khuôn mặt đỏ bừng của cậu.
Dialogue: 0,0:22:53.40,0:22:57.99,Default,,0000,0000,0000,,Hãy cho tớ xem khuôn mặt đáng yêu của cậu nhiều hơn nữa!
Dialogue: 0,0:23:14.50,0:23:18.00,Default,,0000,0000,0000,,Tôi ra đây! Tôi ra đây!
Dialogue: 0,0:23:44.38,0:23:48.04,Default,,0000,0000,0000,,Satoshi... Tớ yêu cậu!
Dialogue: 0,0:00:00.00,0:00:00.00,Default,,0000,0000,0000,,{ED}
Dialogue: 0,0:26:01.60,0:26:05.76,Default,,0000,0000,0000,,Lúc nãy cậu đáng yêu ghê!
Dialogue: 0,0:26:11.08,0:26:13.83,Default,,0000,0000,0000,,Sao lại làm khuôn mặt đó rồi?
Dialogue: 0,0:26:14.79,0:26:18.20,Default,,0000,0000,0000,,Im đê! Bình thường tôi là vậy đó!
Dialogue: 0,0:26:18.20,0:26:23.45,Default,,0000,0000,0000,,Cho tớ xem vẻ mặt thành thật đáng yêu cậu thêm chút nữa cũng có sao đâu.
Dialogue: 0,0:26:23.45,0:26:26.09,Default,,0000,0000,0000,,Im đê! Im đê!
Dialogue: 0,0:26:29.58,0:26:31.10,Default,,0000,0000,0000,,Gì hả?
Dialogue: 0,0:26:31.10,0:26:36.24,Default,,0000,0000,0000,,Nhưng vẻ mặt lúc này cũng đáng yêu không kém.
Dialogue: 0,0:26:38.87,0:26:41.11,Default,,0000,0000,0000,,Hể, sao thế?
Dialogue: 0,0:26:42.28,0:26:44.50,Default,,0000,0000,0000,,Cho tớ xem vẻ mặt của cậu nào.
Dialogue: 0,0:26:44.50,0:26:46.05,Default,,0000,0000,0000,,Im đê!
Dialogue: 0,0:26:46.05,0:26:47.36,Default,,0000,0000,0000,,Aoba-san!
Dialogue: 0,0:26:47.36,0:26:51.12,Default,,0000,0000,0000,,Đây là vẻ mặt bình thường của tôi!
Dialogue: 0,0:26:51.12,0:26:53.12,Default,,0000,0000,0000,,
