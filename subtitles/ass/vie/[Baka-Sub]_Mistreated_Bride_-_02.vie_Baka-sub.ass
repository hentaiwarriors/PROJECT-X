Dialogue: 0,0:00:25.43,0:00:27.40,Default,,0000,0000,0000,,Này...Mitsuko.
Dialogue: 0,0:00:29.23,0:00:31.00,Default,,0000,0000,0000,,Anh tới công ty đây.
Dialogue: 0,0:00:32.84,0:00:35.40,Default,,0000,0000,0000,,Anh ta vẫn thường đi làm sớm thế sao?
Dialogue: 0,0:00:37.34,0:00:39.37,Default,,0000,0000,0000,,Gã khốn này!
Dialogue: 0,0:00:43.78,0:00:44.97,Default,,0000,0000,0000,,Ko cần ra tiễn đâu.
Dialogue: 0,0:00:45.38,0:00:46.54,Default,,0000,0000,0000,,Anh đi đây.
Dialogue: 0,0:00:51.12,0:00:55.28,Default,,0000,0000,0000,,Anh ta luôn tỏ vẻ là một người anh gương mẫu
Dialogue: 0,0:00:55.86,0:00:57.85,Default,,0000,0000,0000,,Mình hoàn toàn bị gạt sang một bên...
Dialogue: 0,0:01:04.60,0:01:07.03,Default,,0000,0000,0000,,Tất cả mọi người đều luôn kì vọng ở anh ta.
Dialogue: 0,0:01:07.47,0:01:11.30,Default,,0000,0000,0000,,Lão già thì đã coi thường mình từ lâu...
Dialogue: 0,0:01:11.81,0:01:14.47,Default,,0000,0000,0000,,Trong khi đó anh ta lại được lão tuyệt đối tin tưởng.
Dialogue: 0,0:01:15.88,0:01:21.61,Default,,0000,0000,0000,,Anh ta luôn chiếm lấy tất cả mọi thứ, trước khi mình có thể với tới chúng!
Dialogue: 0,0:01:22.62,0:01:25.31,Default,,0000,0000,0000,,Nhưng, còn chị ta...
Dialogue: 0,0:01:25.39,0:01:26.86,Default,,0000,0000,0000,,Chị ta đã thuộc về mình!
Dialogue: 0,0:01:41.84,0:01:42.86,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:01:47.04,0:01:49.84,Default,,0000,0000,0000,,Uống chúng đi, uống hết đi chị dâu!
Dialogue: 0,0:01:59.76,0:02:04.42,Default,,0000,0000,0000,,<i>Mistreated Bride Volume 2\NDoorway to New Pleasures</i>
Dialogue: 0,0:02:11.57,0:02:13.36,Default,,0000,0000,0000,,Kouji đâu?
Dialogue: 0,0:02:13.84,0:02:18.43,Default,,0000,0000,0000,,Cậu ấy bảo cảm thấy nhức đâu\Nnên sẽ nghỉ trong phòng ngày hôm nay.
Dialogue: 0,0:02:19.11,0:02:21.54,Default,,0000,0000,0000,,Kouji ốm hả mẹ?
Dialogue: 0,0:02:22.31,0:02:23.30,Default,,0000,0000,0000,,U, uhm...
Dialogue: 0,0:02:24.55,0:02:28.45,Default,,0000,0000,0000,,Sao cơ, nhưng con muốn chơi đùa với cậu.
Dialogue: 0,0:02:32.36,0:02:35.98,Default,,0000,0000,0000,,Chúng ta có cần mời bác sĩ ko?
Dialogue: 0,0:02:37.23,0:02:39.25,Default,,0000,0000,0000,,Ta ko nghĩ điều đó cần thiết.
Dialogue: 0,0:02:39.60,0:02:41.09,Default,,0000,0000,0000,,Um...
Dialogue: 0,0:02:41.70,0:02:46.53,Default,,0000,0000,0000,,Con sẽ lên xem cậu ấy thế nào, mẹ đừng lo việc đó.
Dialogue: 0,0:03:04.45,0:03:06.79,Default,,0000,0000,0000,,Này Mitsuko.
Dialogue: 0,0:03:08.36,0:03:10.48,Default,,0000,0000,0000,,Vâng, chuyện gì thế cha?
Dialogue: 0,0:03:11.76,0:03:16.03,Default,,0000,0000,0000,,Con có muốn...tới phòng ta một lúc ko?
Dialogue: 0,0:03:16.43,0:03:19.06,Default,,0000,0000,0000,,Um, Con còn phải chăm sóc Kouji...
Dialogue: 0,0:03:19.14,0:03:22.83,Default,,0000,0000,0000,,Hãy quên thằng Kouji đi.
Dialogue: 0,0:03:27.48,0:03:30.60,Default,,0000,0000,0000,,Thưa cha....làm ơn dừng lại.
Dialogue: 0,0:03:31.81,0:03:33.11,Default,,0000,0000,0000,,Chị dâu đấy à?
Dialogue: 0,0:03:33.42,0:03:35.18,Default,,0000,0000,0000,,Chị có thể mang cho em một chiếc khăn ướt được ko?
Dialogue: 0,0:03:35.48,0:03:36.85,Default,,0000,0000,0000,,Con phải đi đây.
Dialogue: 0,0:03:37.25,0:03:38.65,Default,,0000,0000,0000,,Xin lỗi cha.
Dialogue: 0,0:03:46.56,0:03:48.72,Default,,0000,0000,0000,,Chị tới để thay khăn cho em đây.
Dialogue: 0,0:03:48.80,0:03:51.10,Default,,0000,0000,0000,,Ko cần đâu, em chỉ giả vờ thế thôi.
Dialogue: 0,0:03:52.64,0:03:55.37,Default,,0000,0000,0000,,Cảm ơn vì đã giúp chị...
Dialogue: 0,0:03:57.01,0:03:58.80,Default,,0000,0000,0000,,Ko cần phải nói điều đó...
Dialogue: 0,0:04:02.85,0:04:05.51,Default,,0000,0000,0000,,Chị thuộc về em!
Dialogue: 0,0:04:06.92,0:04:08.14,Default,,0000,0000,0000,,Nên việc đó là đương nhiên.
Dialogue: 0,0:04:12.36,0:04:14.35,Default,,0000,0000,0000,,Để em chơi cửa sau của chị nào.
Dialogue: 0,0:04:14.72,0:04:17.56,Default,,0000,0000,0000,,T, tốt hơn là em nên dừng lại đi, Kouji.
Dialogue: 0,0:04:19.03,0:04:23.76,Default,,0000,0000,0000,,Em ko tới trường hôm nay là vì\Nem muốn thưởng thức cái hậu môn tuyệt vời của chị!
Dialogue: 0,0:04:24.93,0:04:27.03,Default,,0000,0000,0000,,Chúng ta ko thể làm thế này...
Dialogue: 0,0:04:27.37,0:04:29.17,Default,,0000,0000,0000,,Chúng ta làm chuyện đó sau được ko.
Dialogue: 0,0:04:29.24,0:04:30.93,Default,,0000,0000,0000,,Hứa với chị đi...
Dialogue: 0,0:04:34.98,0:04:36.57,Default,,0000,0000,0000,,Có gì đáng cười chứ?
Dialogue: 0,0:04:37.08,0:04:38.94,Default,,0000,0000,0000,,Làm ơn hãy nghe chị nói.
Dialogue: 0,0:04:39.68,0:04:44.12,Default,,0000,0000,0000,,Chị luôn luôn nói như thế vào những lúc như thế này!
Dialogue: 0,0:04:47.96,0:04:54.56,Default,,0000,0000,0000,,Trong suốt hai tháng nay, em chơi cửa sau của chị ko biết bao nhiêu lần mà ko ai phát hiện ra.
Dialogue: 0,0:04:54.66,0:04:59.40,Default,,0000,0000,0000,,Chị thậm chí còn uống tinh dịch của em sau những bữa ăn.
Dialogue: 0,0:04:59.50,0:05:06.60,Default,,0000,0000,0000,,Vào mỗi buổi sáng khi cái đó của em còn đang khô\Nchị bú nó ướt đẫm và uống hết những gì chảy ra.
Dialogue: 0,0:05:11.38,0:05:16.61,Default,,0000,0000,0000,,Nếu chị ko muốn anh ta biết chuyện này\Nthì chị phải nghe lời em!
Dialogue: 0,0:05:17.15,0:05:19.71,Default,,0000,0000,0000,,Nếu chị đã hiểu rồi, cho em thấy hai bầu vú của chị nào.
Dialogue: 0,0:05:25.83,0:05:27.80,Default,,0000,0000,0000,,Nó vẫn to như mọi khi nhỉ!
Dialogue: 0,0:05:27.86,0:05:29.96,Default,,0000,0000,0000,,Trông ngon thật.
Dialogue: 0,0:05:39.24,0:05:40.90,Default,,0000,0000,0000,,Hai đầu vú chị cứng lên rồi kìa.
Dialogue: 0,0:05:42.71,0:05:45.48,Default,,0000,0000,0000,,Em biết điểm yếu của chị mà!
Dialogue: 0,0:05:53.16,0:05:55.65,Default,,0000,0000,0000,,Vọt ra nào, vọt ra, coi xem cái gì đang vọt ra kìa!
Dialogue: 0,0:05:57.26,0:05:59.75,Default,,0000,0000,0000,,Chị dâu, hãy để em uống sữa của chị!
Dialogue: 0,0:06:14.01,0:06:17.71,Default,,0000,0000,0000,,Mọi thứ của chị đều thuộc về em.
Dialogue: 0,0:06:20.02,0:06:21.54,Default,,0000,0000,0000,,Kouji...
Dialogue: 0,0:06:21.62,0:06:22.55,Default,,0000,0000,0000,,Chị dâu!
Dialogue: 0,0:06:22.75,0:06:24.51,Default,,0000,0000,0000,,Mẹ!!!!
Dialogue: 0,0:06:30.76,0:06:32.39,Default,,0000,0000,0000,,Oh Aya.
Dialogue: 0,0:06:32.50,0:06:34.36,Default,,0000,0000,0000,,Con về rồi à, Achan.
Dialogue: 0,0:06:35.26,0:06:40.73,Default,,0000,0000,0000,,Mẹ biết ko, hôm nay con đã ăn hết bữa trưa đấy!
Dialogue: 0,0:06:41.27,0:06:43.47,Default,,0000,0000,0000,,Wow, con ăn cả ớt xanh nữa à?
Dialogue: 0,0:06:48.88,0:06:51.94,Default,,0000,0000,0000,,Ko phải lúc này...
Dialogue: 0,0:06:54.88,0:06:57.05,Default,,0000,0000,0000,,Giỏi lắm, Achan.
Dialogue: 0,0:06:57.55,0:06:59.25,Default,,0000,0000,0000,,Kouji thế nào rồi?
Dialogue: 0,0:07:00.86,0:07:03.38,Default,,0000,0000,0000,,Cậu ấy đỡ nhiều rồi.
Dialogue: 0,0:07:03.49,0:07:05.19,Default,,0000,0000,0000,,Giờ cậu ấy đang dùng bữa....!
Dialogue: 0,0:07:24.28,0:07:26.91,Default,,0000,0000,0000,,Cái này để chuẩn bị đây.
Dialogue: 0,0:07:30.69,0:07:35.06,Default,,0000,0000,0000,,Hmm, em đang phân vân xem nên ra vào hậu môn hay âm hộ của chị đây.
Dialogue: 0,0:07:37.59,0:07:39.79,Default,,0000,0000,0000,,Đừng ra vào âm....âm hộ.
Dialogue: 0,0:07:40.90,0:07:42.73,Default,,0000,0000,0000,,Chị ko muốn mang thai...
Dialogue: 0,0:07:44.53,0:07:49.94,Default,,0000,0000,0000,,Tại sao lại thế? là chị dâu, đáng nhẽ chị phải từ chối em, nhưng mà...
Dialogue: 0,0:07:51.37,0:07:53.24,Default,,0000,0000,0000,,Chị muốn chơi cửa sau hả!?
Dialogue: 0,0:07:53.84,0:07:57.21,Default,,0000,0000,0000,,Việc này thật đồi bại và hôi hám, nhưng em sẽ làm cho chị.
Dialogue: 0,0:07:58.88,0:08:00.78,Default,,0000,0000,0000,,Em xấu quá...
Dialogue: 0,0:08:03.25,0:08:08.19,Default,,0000,0000,0000,,Hậu môn mình sẽ bị thứ to lớn đó đâm vào.
Dialogue: 0,0:08:14.70,0:08:17.46,Default,,0000,0000,0000,,Mitsuko, chuyện gì thế?
Dialogue: 0,0:08:17.80,0:08:20.96,Default,,0000,0000,0000,,Ko, ko...ko có gì cả...
Dialogue: 0,0:08:23.91,0:08:25.64,Default,,0000,0000,0000,,Uhm, nếu con ổn thì...
Dialogue: 0,0:08:30.75,0:08:35.55,Default,,0000,0000,0000,,Mitsuko, để mẹ đưa những người làm vườn vào nhé?
Dialogue: 0,0:08:36.09,0:08:41.25,Default,,0000,0000,0000,,Hậu môn chị khít thật, và rất mềm, đúng là thứ tuyệt nhất!
Dialogue: 0,0:08:50.73,0:08:57.26,Default,,0000,0000,0000,,Cái đó của em đang ở trong hậu môn chị khi mẹ và Aya đang ở ngay đây!
Dialogue: 0,0:09:08.68,0:09:13.35,Default,,0000,0000,0000,,Và chị thì lại đang muốn nhiều hơn nữa!
Dialogue: 0,0:09:17.93,0:09:20.59,Default,,0000,0000,0000,,Tôi muốn chuyển cây hồng\Nnày tới phía đối diện phòng trà.
Dialogue: 0,0:09:29.91,0:09:33.27,Default,,0000,0000,0000,,Hãy để mẹ và Aya thấy chị ra.
Dialogue: 0,0:09:40.02,0:09:41.74,Default,,0000,0000,0000,,Em sẽ ra vào hậu môn chị!!
Dialogue: 0,0:09:41.82,0:09:44.34,Default,,0000,0000,0000,,Sẽ rất nhiều đấy!
Dialogue: 0,0:09:46.56,0:09:49.25,Default,,0000,0000,0000,,Chị ra đây...Chị ra đây!
Dialogue: 0,0:09:52.09,0:09:53.46,Default,,0000,0000,0000,,Em ra đây!
Dialogue: 0,0:09:57.37,0:09:59.86,Default,,0000,0000,0000,,Ra rồi...!
Dialogue: 0,0:10:15.52,0:10:19.21,Default,,0000,0000,0000,,lchiro luôn là người đàn ông của công việc.
Dialogue: 0,0:10:19.82,0:10:22.79,Default,,0000,0000,0000,,Từ khi sinh Aya, chúng tôi ngày càng xa cách....
Dialogue: 0,0:10:24.13,0:10:26.92,Default,,0000,0000,0000,,Có vẻ như anh ta làm điều này chỉ để tạo ra con cái.
Dialogue: 0,0:10:27.76,0:10:31.39,Default,,0000,0000,0000,,Ko lẽ anh ta ko xem mình là phụ nữ thực thụ?
Dialogue: 0,0:10:33.04,0:10:37.40,Default,,0000,0000,0000,,Cha và Kouji thì khác hẳn...
Dialogue: 0,0:10:39.84,0:10:45.21,Default,,0000,0000,0000,,Và chị thì lại đang muốn nhiều hơn nữa!
Dialogue: 0,0:11:22.32,0:11:25.58,Default,,0000,0000,0000,,Chị dâu, chị thuộc về em!
Dialogue: 0,0:11:27.89,0:11:31.12,Default,,0000,0000,0000,,Em chẳng phải tốt hơn anh ta hay lão già ấy sao?
Dialogue: 0,0:11:31.79,0:11:33.42,Default,,0000,0000,0000,,Chị ko muốn thật sao!?
Dialogue: 0,0:11:35.10,0:11:36.53,Default,,0000,0000,0000,,Chị dâu!
Dialogue: 0,0:11:36.70,0:11:40.03,Default,,0000,0000,0000,,Hãy nghĩ về em, chỉ một mình em!
Dialogue: 0,0:11:40.80,0:11:43.53,Default,,0000,0000,0000,,Em coi chị là gì đây?
Dialogue: 0,0:11:44.64,0:11:47.80,Default,,0000,0000,0000,,Tình nhân, nô lệ, hay....
Dialogue: 0,0:11:50.88,0:11:54.14,Default,,0000,0000,0000,,Hay là một người mẹ hết lòng yêu thương em?
Dialogue: 0,0:11:55.35,0:11:58.44,Default,,0000,0000,0000,,Ta sẽ là mẹ của con kể từ hôm nay, Kouji.
Dialogue: 0,0:12:00.22,0:12:02.78,Default,,0000,0000,0000,,Hãy làm bất cứ điều gì con muốn.
Dialogue: 0,0:12:06.26,0:12:09.72,Default,,0000,0000,0000,,Hãy ra vào bên trong hậu môn của mẹ.
Dialogue: 0,0:12:14.04,0:12:17.10,Default,,0000,0000,0000,,Chúng ta là một gia đình, đừng xấu hổ.
Dialogue: 0,0:12:17.21,0:12:18.17,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:12:24.78,0:12:28.91,Default,,0000,0000,0000,,Tuyệt lắm. Kouji, Kouji.
Dialogue: 0,0:12:29.55,0:12:31.02,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:12:31.12,0:12:33.88,Default,,0000,0000,0000,,Hãy ra vào bên trong mẹ!
Dialogue: 0,0:12:34.09,0:12:37.12,Default,,0000,0000,0000,,Mẹ... Mẹ!
Dialogue: 0,0:12:55.78,0:12:59.14,Default,,0000,0000,0000,,Oh Mitsuko, có vẻ trông con vẫn ổn lúc trong phòng tắm.
Dialogue: 0,0:13:00.25,0:13:03.15,Default,,0000,0000,0000,,Mẹ đừng nói thế, xấu hổ lắm ạ.
Dialogue: 0,0:13:04.99,0:13:10.62,Default,,0000,0000,0000,,Đó là vì Kouji đâm cái đó vào hậu môn mình mỗi ngày...
Dialogue: 0,0:13:10.76,0:13:13.35,Default,,0000,0000,0000,,Khiến cho chỗ đó ngày càng giãn ra...
Dialogue: 0,0:13:15.03,0:13:16.52,Default,,0000,0000,0000,,À đúng rồi.
Dialogue: 0,0:13:17.03,0:13:21.97,Default,,0000,0000,0000,,Những người làm vườn đã tới, con có thể lo chỗ quần áo chưa giặt kia chứ?
Dialogue: 0,0:13:22.54,0:13:23.70,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:13:24.27,0:13:29.28,Default,,0000,0000,0000,,Cậu ấy luôn gọi mình vào phòng tắm, sao hôm nay lại ko nhỉ?
Dialogue: 0,0:13:32.85,0:13:34.65,Default,,0000,0000,0000,,Quần lót của Kouji...
Dialogue: 0,0:13:35.32,0:13:36.98,Default,,0000,0000,0000,,Nó ướt hết cả rồi.
Dialogue: 0,0:13:37.05,0:13:41.25,Default,,0000,0000,0000,,Toàn mùi tinh dịch của cậu ấy...
Dialogue: 0,0:13:41.39,0:13:44.52,Default,,0000,0000,0000,,Đúng là thanh niên.
Dialogue: 0,0:13:45.59,0:13:46.96,Default,,0000,0000,0000,,Thật lãng phí...
Dialogue: 0,0:13:47.03,0:13:49.86,Default,,0000,0000,0000,,Này Mitsuko...
Dialogue: 0,0:13:50.73,0:13:51.56,Default,,0000,0000,0000,,Vâng!?
Dialogue: 0,0:13:54.20,0:13:57.50,Default,,0000,0000,0000,,Con giúp ta một việc được chứ?
Dialogue: 0,0:14:01.11,0:14:03.84,Default,,0000,0000,0000,,Cũng lâu rồi nhỉ.
Dialogue: 0,0:14:03.95,0:14:05.24,Default,,0000,0000,0000,,V, vâng...
Dialogue: 0,0:14:06.01,0:14:08.54,Default,,0000,0000,0000,,May quá, lão ta chưa thấy gì cả.
Dialogue: 0,0:14:10.02,0:14:12.11,Default,,0000,0000,0000,,Mình đành tạm quên Kouji đi vậy...
Dialogue: 0,0:14:12.29,0:14:14.31,Default,,0000,0000,0000,,Thế nên, hôm nay mình sẽ chiều lão ta một chút.
Dialogue: 0,0:14:23.90,0:14:28.50,Default,,0000,0000,0000,,Mitsuko, con làm tốt hơn lần trước đấy nhỉ!?
Dialogue: 0,0:14:40.02,0:14:44.08,Default,,0000,0000,0000,,Cha, có sướng ko?
Dialogue: 0,0:14:46.92,0:14:49.52,Default,,0000,0000,0000,,Sao, sao nó lại ko cứng lên?
Dialogue: 0,0:14:51.83,0:14:53.19,Default,,0000,0000,0000,,Cha...
Dialogue: 0,0:14:57.50,0:14:59.93,Default,,0000,0000,0000,,Lão ta ko cứng lên được...!
Dialogue: 0,0:15:01.80,0:15:04.33,Default,,0000,0000,0000,,Hôm nay ta ko được khỏe.
Dialogue: 0,0:15:04.77,0:15:06.30,Default,,0000,0000,0000,,Con có thể dừng lại, Mitsuko.
Dialogue: 0,0:15:07.14,0:15:08.54,Default,,0000,0000,0000,,Ta đi đây.
Dialogue: 0,0:15:11.78,0:15:13.27,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:15:13.35,0:15:17.22,Default,,0000,0000,0000,,Sau khi đã làm mình nứng thế này, thật ác độc.
Dialogue: 0,0:15:21.92,0:15:25.15,Default,,0000,0000,0000,,Cho đến khi Kouji quay về...
Dialogue: 0,0:15:29.50,0:15:31.66,Default,,0000,0000,0000,,Mình ko thể đợi lâu như vậy!
Dialogue: 0,0:15:33.47,0:15:37.63,Default,,0000,0000,0000,,Mình đang nghĩ gì thế này, có thể làm như vậy được sao!?
Dialogue: 0,0:15:38.27,0:15:40.21,Default,,0000,0000,0000,,Ko, mình ko thể làm thế!
Dialogue: 0,0:15:40.61,0:15:43.60,Default,,0000,0000,0000,,Thủ dâm bằng chiếc quần lót của cậu ấy!?
Dialogue: 0,0:15:45.85,0:15:48.15,Default,,0000,0000,0000,,Mùi của...Kouji
Dialogue: 0,0:15:57.03,0:16:00.86,Default,,0000,0000,0000,,Mùi của...Kouji!
Dialogue: 0,0:16:16.31,0:16:17.68,Default,,0000,0000,0000,,Con về rồi đây.
Dialogue: 0,0:16:26.76,0:16:30.85,Default,,0000,0000,0000,,Cái quái gì thế, nóng thế này mà còn làm vườn à.
Dialogue: 0,0:16:30.93,0:16:32.33,Default,,0000,0000,0000,,Chào em.
Dialogue: 0,0:16:41.30,0:16:42.79,Default,,0000,0000,0000,,Mừng em trở về.
Dialogue: 0,0:16:45.51,0:16:47.74,Default,,0000,0000,0000,,Gì thế này?
Dialogue: 0,0:16:48.58,0:16:50.31,Default,,0000,0000,0000,,Chị đang dụ dỗ em đấy hả?
Dialogue: 0,0:16:51.51,0:16:52.81,Default,,0000,0000,0000,,K, ko...
Dialogue: 0,0:16:53.72,0:16:57.12,Default,,0000,0000,0000,,Đúng là mùi của thanh niên trẻ...
Dialogue: 0,0:17:00.96,0:17:04.82,Default,,0000,0000,0000,,Chị mặc thế này vì thời tiết đang rất nóng nực.
Dialogue: 0,0:17:08.16,0:17:11.36,Default,,0000,0000,0000,,Vậy sao, hai đầu vú chị cứng lên rồi này!
Dialogue: 0,0:17:12.40,0:17:15.03,Default,,0000,0000,0000,,Đừng....kéo chúng như thế.
Dialogue: 0,0:17:17.84,0:17:22.37,Default,,0000,0000,0000,,Sữa và mồ hôi chị làm\Nchỗ đó nhìn trong suốt, lại nhàu nhĩ thế kia!
Dialogue: 0,0:17:31.39,0:17:37.22,Default,,0000,0000,0000,,Mùi của mồ hôi, nước từ âm hộ và hậu môn...
Dialogue: 0,0:17:37.29,0:17:39.52,Default,,0000,0000,0000,,Đừng có ngửi như thế...
Dialogue: 0,0:17:40.40,0:17:42.49,Default,,0000,0000,0000,,Mùi hương thật khiêu gợi!
Dialogue: 0,0:17:44.80,0:17:46.63,Default,,0000,0000,0000,,Để em chơi chị nào, chị dâu.
Dialogue: 0,0:17:46.70,0:17:50.47,Default,,0000,0000,0000,,Em sẽ đâm nó vào cửa sau chị!
Dialogue: 0,0:18:18.23,0:18:21.36,Default,,0000,0000,0000,,Có gì đó chảy ra từ âm hộ chị này!
Dialogue: 0,0:18:25.14,0:18:29.67,Default,,0000,0000,0000,,Em sẽ bấu cái mấu nhỏ kia khi chơi cửa sau của chị, chị sẽ ra rất nhiều lần!
Dialogue: 0,0:18:35.85,0:18:38.32,Default,,0000,0000,0000,,Em sẽ cho chị nước của em
Dialogue: 0,0:18:38.42,0:18:43.26,Default,,0000,0000,0000,,Đúng là vấn đề lớn khi có một người chị dâu dâm đãng như chị!
Dialogue: 0,0:18:43.43,0:18:48.12,Default,,0000,0000,0000,,Kouji....cho chị....nước....của em...!
Dialogue: 0,0:18:52.57,0:18:56.30,Default,,0000,0000,0000,,Kouji...nó thật ấm...uh.
Dialogue: 0,0:19:02.68,0:19:07.41,Default,,0000,0000,0000,,Cậu ta vẫn ra dù còn đang cứng, rất cứng.
Dialogue: 0,0:19:07.48,0:19:10.85,Default,,0000,0000,0000,,Cậu ta khiến mình ra liên tục...
Dialogue: 0,0:19:11.59,0:19:13.61,Default,,0000,0000,0000,,Chị có cái hậu môn tuyệt thật.
Dialogue: 0,0:19:13.72,0:19:16.59,Default,,0000,0000,0000,,Sao hậu môn của chị thích cái của em đến thế nhỉ, chị dâu!?
Dialogue: 0,0:19:23.16,0:19:26.00,Default,,0000,0000,0000,,Sau mỗi cú nhấp, mình tê dại dần đi.
Dialogue: 0,0:19:26.23,0:19:29.97,Default,,0000,0000,0000,,Bổn phận của một người vợ và người mẹ trong tôi dần dần biến mất...
Dialogue: 0,0:20:13.75,0:20:16.11,Default,,0000,0000,0000,,Ồ, có mùi gì đó thì phải.
Dialogue: 0,0:20:16.82,0:20:19.81,Default,,0000,0000,0000,,Hai anh nên nghỉ một chút\NTôi mang đồ uống tới đây.
Dialogue: 0,0:20:20.86,0:20:22.91,Default,,0000,0000,0000,,Cám ơn bà rất nhiều.
Dialogue: 0,0:20:23.63,0:20:25.96,Default,,0000,0000,0000,,Thật nóng và mệt!
Dialogue: 0,0:20:26.80,0:20:29.06,Default,,0000,0000,0000,,Nghỉ một chút đi.
Dialogue: 0,0:20:29.56,0:20:30.90,Default,,0000,0000,0000,,Vào đây nào.
Dialogue: 0,0:20:31.17,0:20:32.13,Default,,0000,0000,0000,,Cám ơn.
Dialogue: 0,0:20:32.50,0:20:34.06,Default,,0000,0000,0000,,Hôm nay nóng thật.
Dialogue: 0,0:20:34.40,0:20:35.20,Default,,0000,0000,0000,,Bà chắc chứ?
Dialogue: 0,0:20:35.37,0:20:36.74,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:20:37.37,0:20:39.24,Default,,0000,0000,0000,,Nhưng thưa bà...
Dialogue: 0,0:20:43.91,0:20:44.88,Default,,0000,0000,0000,,Thưa bà?
Dialogue: 0,0:20:48.55,0:20:50.54,Default,,0000,0000,0000,,Chúng ta ko thể làm thế!
Dialogue: 0,0:20:52.09,0:20:54.99,Default,,0000,0000,0000,,Thưa bà, sao bà lại...!
Dialogue: 0,0:21:20.05,0:21:24.38,Default,,0000,0000,0000,,Đây là cách để tôi cảm ơn hai anh vì đã làm việc chăm chỉ.
Dialogue: 0,0:21:25.32,0:21:28.69,Default,,0000,0000,0000,,Đừng lo gì cả, cứ ra hết đi.
Dialogue: 0,0:21:31.79,0:21:33.59,Default,,0000,0000,0000,,Tiểu thư, cô thật dâm đãng.
Dialogue: 0,0:21:33.66,0:21:37.89,Default,,0000,0000,0000,,Thằng đần này, ko được gọi là tiểu thư, đó là một lệnh bà, kể cả khi mày...
Dialogue: 0,0:21:40.67,0:21:43.40,Default,,0000,0000,0000,,Đừng lo, đó là sự thật.
Dialogue: 0,0:21:49.54,0:21:52.14,Default,,0000,0000,0000,,Nhìn chúng này.
Dialogue: 0,0:21:52.35,0:21:55.84,Default,,0000,0000,0000,,Tôi là một bà mẹ vì thế tôi ko còn trẻ nữa.
Dialogue: 0,0:21:55.95,0:21:58.54,Default,,0000,0000,0000,,Nhưng tôi vẫn là đàn bà.
Dialogue: 0,0:21:58.82,0:22:00.98,Default,,0000,0000,0000,,Tôi đã quá già rồi ư, phải ko?
Dialogue: 0,0:22:02.72,0:22:06.96,Default,,0000,0000,0000,,Làm ơn, ít nhất hãy cho tôi được tận hưởng khoái lạc...
Dialogue: 0,0:22:08.13,0:22:09.32,Default,,0000,0000,0000,,T, thưa bà!
Dialogue: 0,0:22:10.37,0:22:11.42,Default,,0000,0000,0000,,Ko thể nào...!?
Dialogue: 0,0:22:12.30,0:22:16.13,Default,,0000,0000,0000,,Ko thể như thế được, là mẹ ư!?
Dialogue: 0,0:22:16.24,0:22:18.00,Default,,0000,0000,0000,,Đang làm chuyện này!?
Dialogue: 0,0:22:24.75,0:22:27.38,Default,,0000,0000,0000,,Tôi có thể cảm thấy chúng, rất ấm...
Dialogue: 0,0:22:30.35,0:22:33.22,Default,,0000,0000,0000,,Mẹ cùng với hai gã đàn ông, như thế này...
Dialogue: 0,0:22:33.79,0:22:37.42,Default,,0000,0000,0000,,Kouji cứng lên vì nhìn thấy chuyện mẹ làm?
Dialogue: 0,0:22:38.03,0:22:40.22,Default,,0000,0000,0000,,Kouji, chúng ta làm nhé!
Dialogue: 0,0:22:40.30,0:22:41.92,Default,,0000,0000,0000,,với chị!
Dialogue: 0,0:22:43.83,0:22:45.63,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:22:48.44,0:22:50.46,Default,,0000,0000,0000,,Kouji cứng lên ghê quá...
Dialogue: 0,0:22:51.61,0:22:54.27,Default,,0000,0000,0000,,Sao...sướng thế này?
Dialogue: 0,0:22:56.95,0:22:59.07,Default,,0000,0000,0000,,Đáng sợ thật...!
Dialogue: 0,0:23:00.42,0:23:04.28,Default,,0000,0000,0000,,Cơ thể mình...đang tự di chuyển.....
Dialogue: 0,0:23:05.22,0:23:08.59,Default,,0000,0000,0000,,Thật đáng sợ, nhưng...nhưng....!
Dialogue: 0,0:23:10.59,0:23:12.86,Default,,0000,0000,0000,,Nó đâm vào sâu quá...!
Dialogue: 0,0:23:18.63,0:23:20.69,Default,,0000,0000,0000,,Bộ ngực lớn thật!
Dialogue: 0,0:23:22.87,0:23:24.07,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:23:26.27,0:23:31.54,Default,,0000,0000,0000,,Bầu ngực và hai đầu vú thật lớn, lại ko có áo ngực thảo nào nó chắc như vậy!
Dialogue: 0,0:23:34.48,0:23:37.75,Default,,0000,0000,0000,,Lệnh bà, Vẻ mặt bà cho thấy bà rất thích chuyện này...thật dâm đãng.
Dialogue: 0,0:23:47.66,0:23:50.22,Default,,0000,0000,0000,,Nhìn này, cái mẫu nhỏ đó lòi hết ra ngoài rồi.
Dialogue: 0,0:23:50.33,0:23:53.89,Default,,0000,0000,0000,,Thật khó thể tưởng tượng bà lại có mùi vị thế này.
Dialogue: 0,0:23:57.34,0:23:59.86,Default,,0000,0000,0000,,Đừng nói những điều đáng xấu hổ như vậy chứ...!
Dialogue: 0,0:24:11.32,0:24:15.45,Default,,0000,0000,0000,,Làm ơn đừng đùa giỡn với bà già này.
Dialogue: 0,0:24:17.49,0:24:20.69,Default,,0000,0000,0000,,Với cậu bé sực mùi của các anh....
Dialogue: 0,0:24:21.76,0:24:26.29,Default,,0000,0000,0000,,Hãy đâm nó vào bên trong bà già này và làm cho tôi gào khóc đi!
Dialogue: 0,0:24:32.21,0:24:34.40,Default,,0000,0000,0000,,Bên trong thật là tuyệt!
Dialogue: 0,0:24:36.21,0:24:38.94,Default,,0000,0000,0000,,Đang vào...sâu hơn...!!!
Dialogue: 0,0:24:39.05,0:24:42.14,Default,,0000,0000,0000,,Lệnh bà...Lệnh bà!!
Dialogue: 0,0:24:43.35,0:24:45.84,Default,,0000,0000,0000,,Chơi thế ko đẹp đâu, còn tôi thì sao!
Dialogue: 0,0:24:50.23,0:24:53.13,Default,,0000,0000,0000,,Anh có thể chơi hậu môn của tôi...
Dialogue: 0,0:24:54.20,0:24:56.86,Default,,0000,0000,0000,,Tôi đã rửa nó sạch sẽ...
Dialogue: 0,0:25:02.64,0:25:06.73,Default,,0000,0000,0000,,Tôi chưa bao giờ chơi kiểu này, đặc biệt là với hậu môn!
Dialogue: 0,0:25:10.51,0:25:13.31,Default,,0000,0000,0000,,Bà thích cảm giác này sao!?
Dialogue: 0,0:25:14.92,0:25:18.41,Default,,0000,0000,0000,,Tuyệt quá...tôi sắp ra rồi...tôi sắp ra rồi!
Dialogue: 0,0:25:23.79,0:25:24.99,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:25:26.93,0:25:28.90,Default,,0000,0000,0000,,Ra rồi!!!!
Dialogue: 0,0:25:30.21,0:25:33.90,Default,,0000,0000,0000,,Ra rồi!!!!
