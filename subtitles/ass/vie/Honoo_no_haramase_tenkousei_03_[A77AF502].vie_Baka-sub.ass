Dialogue: 0,0:00:43.88,0:00:46.17,Default,,0000,0000,0000,,Tagami-sama, mình sắp...
Dialogue: 0,0:00:51.16,0:00:53.67,Default,,0000,0000,0000,,Đừng ra bên trong!
Dialogue: 0,0:01:09.85,0:01:11.98,Default,,0000,0000,0000,,Dù mình không muốn...
Dialogue: 0,0:01:12.08,0:01:14.35,Default,,0000,0000,0000,,Nhưng chuyện này thật sự rất tuyệt.
Dialogue: 0,0:01:18.34,0:01:23.15,Default,,0000,0000,0000,,Bây giờ tới lượt cô, Reika.
Dialogue: 0,0:01:23.47,0:01:25.44,Default,,0000,0000,0000,,Sao lại là tôi?
Dialogue: 0,0:01:25.62,0:01:31.01,Default,,0000,0000,0000,,Hai thuộc hạ của cô đều làm cả rồi, cô muốn từ chối sao?
Dialogue: 0,0:01:31.36,0:01:35.27,Default,,0000,0000,0000,,Tất nhiên là tôi sẽ làm.
Dialogue: 0,0:01:35.49,0:01:39.22,Default,,0000,0000,0000,,Xem như hôm nay tôi chịu thiệt một lần.
Dialogue: 0,0:01:56.15,0:01:58.82,Default,,0000,0000,0000,,Như vậy không hay chút nào.
Dialogue: 0,0:02:03.08,0:02:06.20,Default,,0000,0000,0000,,"Thổi kèn" thì phải như thế này nè, rõ chưa?
Dialogue: 0,0:02:06.85,0:02:12.44,Default,,0000,0000,0000,,Nghe cho rõ đây, cô đã thua trong cuộc thi xạ kích.
Dialogue: 0,0:02:15.79,0:02:20.27,Default,,0000,0000,0000,,Vậy nên liệu mà chăm sóc khẩu đại pháo của tôi cho chu đáo.
Dialogue: 0,0:02:23.05,0:02:25.36,Default,,0000,0000,0000,,Có vẻ như cô không khoái chuyện này nhỉ?!
Dialogue: 0,0:02:25.48,0:02:29.13,Default,,0000,0000,0000,,Suzuka, Ayami, giúp tôi thuyết phục cô ta nào.
Dialogue: 0,0:02:30.61,0:02:35.94,Default,,0000,0000,0000,,Nếu cảm thấy thích thú, có lẽ cô ta sẽ làm khá hơn bây giờ.
Dialogue: 0,0:02:35.98,0:02:37.28,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:02:37.48,0:02:39.12,Default,,0000,0000,0000,,Reika-sama...
Dialogue: 0,0:02:40.52,0:02:42.93,Default,,0000,0000,0000,,Xin hãy tha lỗi cho tụi em, Reika-sama.
Dialogue: 0,0:03:01.66,0:03:03.43,Default,,0000,0000,0000,,Ayami, cả em nữa.
Dialogue: 0,0:03:03.69,0:03:06.45,Default,,0000,0000,0000,,V-Vâng ạ, Reika-sama.
Dialogue: 0,0:03:10.89,0:03:13.12,Default,,0000,0000,0000,,Cả hai cô đều làm rất tốt.
Dialogue: 0,0:03:13.26,0:03:16.40,Default,,0000,0000,0000,,Reika, đừng quên vấn đề chính nhé.
Dialogue: 0,0:03:18.39,0:03:22.16,Default,,0000,0000,0000,,T-Tất nhiên rồi, Tatsuya-san.
Dialogue: 0,0:03:23.60,0:03:26.84,Default,,0000,0000,0000,,Cô làm khá hơn nhiều rồi đó, Reika.
Dialogue: 0,0:03:34.50,0:03:36.85,Default,,0000,0000,0000,,Tôi sắp ra rồi, Reika!
Dialogue: 0,0:03:45.85,0:03:48.02,Default,,0000,0000,0000,,Chuyện thú vị giờ mới thật sự bắt đầu.
Dialogue: 0,0:03:51.37,0:03:57.41,Default,,0000,0000,0000,,Đại pháo của anh đang khuấy đảo bên trong tôi!
Dialogue: 0,0:03:58.06,0:04:00.06,Default,,0000,0000,0000,,Đúng là tiểu thư nhà giàu có khác.
Dialogue: 0,0:04:00.06,0:04:02.58,Default,,0000,0000,0000,,Âm hộ của cô được chăm sóc tốt thật.
Dialogue: 0,0:04:04.52,0:04:07.54,Default,,0000,0000,0000,,Tôi sẽ chết vì hoan lạc mất.
Dialogue: 0,0:04:07.62,0:04:09.37,Default,,0000,0000,0000,,Ayami, Suzuka.
Dialogue: 0,0:04:09.45,0:04:10.81,Default,,0000,0000,0000,,Có bọn em.
Dialogue: 0,0:04:10.99,0:04:12.61,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:04:13.58,0:04:16.81,Default,,0000,0000,0000,,Suzuka, lưỡi của em tuyệt quá.
Dialogue: 0,0:04:17.68,0:04:20.62,Default,,0000,0000,0000,,Bầu vú của ta bấn hết lên rồi!
Dialogue: 0,0:04:23.29,0:04:27.29,Default,,0000,0000,0000,,Cặp vú của Reika-sama quả là vĩ đại.
Dialogue: 0,0:04:29.21,0:04:32.11,Default,,0000,0000,0000,,Âm vật của Reika-sama thật khó tin.
Dialogue: 0,0:04:32.27,0:04:36.89,Default,,0000,0000,0000,,Này Reika, bản tính dâm đãng của cô đã bị thuộc hạ của mình đánh thức rồi hả?!
Dialogue: 0,0:04:37.46,0:04:41.44,Default,,0000,0000,0000,,Thằng nhỏ của anh quá sức tuyệt vời.
Dialogue: 0,0:04:42.79,0:04:45.45,Default,,0000,0000,0000,,Được lắm, tôi sẽ bắn cả vào bên trong cô.
Dialogue: 0,0:04:45.51,0:04:49.26,Default,,0000,0000,0000,,Đừng mà! Không! Đừng bắn vào trong!
Dialogue: 0,0:04:53.11,0:04:56.11,Default,,0000,0000,0000,,Ôi không... Tôi sắp...
Dialogue: 0,0:04:56.60,0:04:58.87,Default,,0000,0000,0000,,Hãy sinh con cho tôi!
Dialogue: 0,0:05:11.54,0:05:13.97,Default,,0000,0000,0000,,Giờ chỉ còn duy nhất một người.
Dialogue: 0,0:05:14.21,0:05:15.17,Default,,0000,0000,0000,,Ờ...
Dialogue: 0,0:05:15.27,0:05:19.22,Default,,0000,0000,0000,,Nhưng cô nàng này xem chừng khá rắc rối đấy.
Dialogue: 0,0:05:20.13,0:05:21.90,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:05:22.18,0:05:25.73,Default,,0000,0000,0000,,Tôi sẽ tự mình giải quyết chuyện này.
Dialogue: 0,0:05:27.94,0:05:29.92,Default,,0000,0000,0000,,Dù cho đó là Minase Sayaka.
Dialogue: 0,0:05:36.77,0:05:38.88,Default,,0000,0000,0000,,Mau vào trong đi!
Dialogue: 0,0:05:38.94,0:05:40.95,Default,,0000,0000,0000,,Đợi mình với!
