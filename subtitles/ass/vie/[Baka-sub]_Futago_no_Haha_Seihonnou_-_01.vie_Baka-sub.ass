Dialogue: 0,0:00:11.28,0:00:12.54,Default,,0000,0000,0000,,Im nào!
Dialogue: 0,0:00:15.11,0:00:17.27,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:00:17.58,0:00:18.51,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:00:18.95,0:00:20.28,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:00:22.29,0:00:24.65,Default,,0000,0000,0000,,Muốn tôi làm cô đau sao?
Dialogue: 0,0:00:30.60,0:00:32.32,Default,,0000,0000,0000,,Ngực cô ta bự chưa này!
Dialogue: 0,0:00:39.60,0:00:40.80,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:00:48.25,0:00:49.41,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:00:49.55,0:00:50.98,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:00:53.45,0:00:55.78,Default,,0000,0000,0000,,Giờ để tôi nhét nó vào bên trong nào!
Dialogue: 0,0:00:56.25,0:00:59.05,Default,,0000,0000,0000,,Ko, ko!
Dialogue: 0,0:00:59.26,0:01:00.28,Default,,0000,0000,0000,,Yên nào!
Dialogue: 0,0:01:00.86,0:01:03.95,Default,,0000,0000,0000,,Đừng nhét nó vào bên trong tôi!
Dialogue: 0,0:01:04.43,0:01:07.02,Default,,0000,0000,0000,,Ai đó giúp tôi với!
Dialogue: 0,0:01:29.19,0:01:31.02,Default,,0000,0000,0000,,Chà, cô ta khít quá!
Dialogue: 0,0:01:31.32,0:01:33.31,Default,,0000,0000,0000,,Nó cứ xoắn lấy thằng nhỏ của tao này !
Dialogue: 0,0:02:13.80,0:02:15.16,Default,,0000,0000,0000,,Đừng cắn mạnh như vậy.
Dialogue: 0,0:02:15.53,0:02:16.26,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:02:27.05,0:02:28.38,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:02:34.62,0:02:38.61,Default,,0000,0000,0000,,Hắn ta ra ở bên trong mình.
Dialogue: 0,0:02:38.72,0:02:40.32,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:02:40.43,0:02:42.09,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:02:56.84,0:03:00.91,credits,,0000,0000,0000,,{\pos(460,20)}Translated By Invincible_Dark_Thunder
Dialogue: 0,0:02:56.84,0:03:00.91,credits,,0000,0000,0000,,{\pos(82,20)}Baka-sub.net
Dialogue: 0,0:02:56.84,0:03:00.91,title,,0000,0000,0000,,Chị em song sinh
Dialogue: 0,0:03:02.24,0:03:05.85,title2,,0000,0000,0000,,{\pos(315,330)\fad(0,1000)}Vol 1: Dục vọng
Dialogue: 0,0:03:15.96,0:03:17.79,Default,,0000,0000,0000,,Mẹ phải đi đây, Minoru-san.
Dialogue: 0,0:03:20.80,0:03:22.79,Default,,0000,0000,0000,,Hôm nay mẹ lại trực đêm ở bệnh viện à?
Dialogue: 0,0:03:23.07,0:03:24.00,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:03:24.44,0:03:26.80,Default,,0000,0000,0000,,Mẹ để bữa tối của con trong tủ lạnh đấy.
Dialogue: 0,0:03:27.67,0:03:28.83,Default,,0000,0000,0000,,Con biết rồi ạ.
Dialogue: 0,0:03:28.94,0:03:30.37,Default,,0000,0000,0000,,À mẹ này...
Dialogue: 0,0:03:34.65,0:03:35.31,Default,,0000,0000,0000,,Túi gì đây?
Dialogue: 0,0:03:35.71,0:03:37.84,Default,,0000,0000,0000,,Ah, mẹ quên mất.
Dialogue: 0,0:03:40.99,0:03:45.65,Default,,0000,0000,0000,,Mấy cô y tá mới vào nhờ mẹ làm hộ ít cơm rang.
Dialogue: 0,0:03:45.89,0:03:47.52,Default,,0000,0000,0000,,Suýt nữa thì mẹ quên mang nó theo.
Dialogue: 0,0:03:48.86,0:03:51.06,Default,,0000,0000,0000,,Bắt đầu từ đêm nay, lịch làm việc sẽ rất dày đặc.
Dialogue: 0,0:03:51.36,0:03:53.35,Default,,0000,0000,0000,,Mẹ đã mua một ít đồ ăn dự trữ trong tủ lạnh...
Dialogue: 0,0:03:53.50,0:03:55.49,Default,,0000,0000,0000,,Mẹ sắp muộn giờ làm rồi kìa.
Dialogue: 0,0:03:56.07,0:03:57.23,Default,,0000,0000,0000,,Con nói phải.
Dialogue: 0,0:04:02.47,0:04:04.81,Default,,0000,0000,0000,,Ko thể tin được mẹ lại là y tá chuyên khoa thần kinh.
Dialogue: 0,0:05:15.55,0:05:17.45,Default,,0000,0000,0000,,Cô ta lại làm chuyện đó nữa rồi...
Dialogue: 0,0:05:17.62,0:05:20.68,Default,,0000,0000,0000,,Đành rằng chồng cô ta hay đi công tác xa nhưng...
Dialogue: 0,0:05:22.95,0:05:24.44,Default,,0000,0000,0000,,Mail nhầm ư?
Dialogue: 0,0:05:24.66,0:05:25.92,sign,,0000,0000,0000,,{\pos(360,317)\fr-2\fs20}Gặp
Dialogue: 0,0:05:24.66,0:05:25.92,sign,,0000,0000,0000,,{\pos(360,413)\fr-2\fs20}Ko gặp
Dialogue: 0,0:05:46.44,0:05:48.14,sign,,0000,0000,0000,,{\pos(315,199)\fr8}Gặp
Dialogue: 0,0:05:55.95,0:05:56.89,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:06:09.73,0:06:11.40,Default,,0000,0000,0000,,Cô là ai?
Dialogue: 0,0:06:13.07,0:06:14.83,Default,,0000,0000,0000,,Tên tôi là Akane.
Dialogue: 0,0:06:14.94,0:06:16.13,sign,,0000,0000,0000,,{\fs60\b1\pos(320,237)\fr14}Từ Akane
Dialogue: 0,0:06:26.92,0:06:30.18,Default,,0000,0000,0000,,Thế này có gọi là hẹn hò ko nhỉ?
Dialogue: 0,0:06:30.52,0:06:32.35,Default,,0000,0000,0000,,Đây là lần đầu tiên tôi làm chuyện này.
Dialogue: 0,0:06:36.76,0:06:38.99,Default,,0000,0000,0000,,Ko hẳn là vậy.
Dialogue: 0,0:06:39.36,0:06:43.27,Default,,0000,0000,0000,,Một người bạn của cậu đã cho tôi địa chỉ mail cậu thường dùng.
Dialogue: 0,0:06:46.60,0:06:48.77,Default,,0000,0000,0000,,Tại sao lại là tôi?
Dialogue: 0,0:06:53.48,0:06:55.04,Default,,0000,0000,0000,,Tôi chỉ muốn gặp cậu...
Dialogue: 0,0:06:55.48,0:06:56.78,Default,,0000,0000,0000,,Đừng hỏi vì sao.
Dialogue: 0,0:07:30.72,0:07:32.81,Default,,0000,0000,0000,,Nhẹ nhàng thôi nhé.
Dialogue: 0,0:07:46.40,0:07:47.59,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:07:47.70,0:07:48.46,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:07:48.57,0:07:49.62,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:07:49.77,0:07:51.83,Default,,0000,0000,0000,,Nữa, nữa...
Dialogue: 0,0:07:52.17,0:07:53.54,Default,,0000,0000,0000,,Nữa đi...
Dialogue: 0,0:08:02.38,0:08:04.68,Default,,0000,0000,0000,,Ko phải chỗ đó, Minoru-kun...
Dialogue: 0,0:08:04.82,0:08:06.72,Default,,0000,0000,0000,,Thấp hơn chút nữa...
Dialogue: 0,0:08:06.82,0:08:09.84,Default,,0000,0000,0000,,Phải, cao hơn chút nữa nào...
Dialogue: 0,0:08:18.90,0:08:20.92,Default,,0000,0000,0000,,Cửa ko khóa...
Dialogue: 0,0:08:22.37,0:08:25.06,Default,,0000,0000,0000,,Mình lại quên chưa khóa cửa ư?
Dialogue: 0,0:08:25.70,0:08:28.26,Default,,0000,0000,0000,,Minoru-san, mẹ để quên một thứ.
Dialogue: 0,0:08:28.84,0:08:32.07,Default,,0000,0000,0000,,Thấy vé tháng mẹ để cạnh TV đâu ko?
Dialogue: 0,0:08:37.98,0:08:39.35,Default,,0000,0000,0000,,Con có ở đó ko?
Dialogue: 0,0:08:43.35,0:08:44.95,Default,,0000,0000,0000,,Chắc nó đang tắm?
Dialogue: 0,0:08:45.19,0:08:46.59,Default,,0000,0000,0000,,Minoru-san...
Dialogue: 0,0:08:54.10,0:08:55.93,Default,,0000,0000,0000,,Dừng lại.
Dialogue: 0,0:08:56.07,0:08:57.43,Default,,0000,0000,0000,,Ko phải lúc này...
Dialogue: 0,0:09:14.89,0:09:16.62,Default,,0000,0000,0000,,Tệ thật.
Dialogue: 0,0:09:16.75,0:09:19.02,Default,,0000,0000,0000,,Rõ ràng là mình đã cất nó vào...
Dialogue: 0,0:09:20.93,0:09:22.72,Default,,0000,0000,0000,,Oh, đây rồi.
Dialogue: 0,0:09:24.06,0:09:26.62,Default,,0000,0000,0000,,Thôi, mẹ đi làm đây, Minoru-san.
Dialogue: 0,0:09:40.85,0:09:42.78,Default,,0000,0000,0000,,Nhỏ tiếng đi nào.
Dialogue: 0,0:09:43.45,0:09:45.44,Default,,0000,0000,0000,,Cô ấy đi rồi mà.
Dialogue: 0,0:09:45.55,0:09:47.25,Default,,0000,0000,0000,,Hàng xóm sẽ nghe thấy mất.
Dialogue: 0,0:09:48.82,0:09:51.62,Default,,0000,0000,0000,,Nhét nó vào trong hậu môn tôi đi nào.
Dialogue: 0,0:09:55.46,0:09:56.59,Default,,0000,0000,0000,,Khít quá!
Dialogue: 0,0:10:33.23,0:10:34.16,Default,,0000,0000,0000,,Của cô...
Dialogue: 0,0:10:34.30,0:10:35.46,Default,,0000,0000,0000,,Khít qua đi mất!
Dialogue: 0,0:10:35.73,0:10:37.76,Default,,0000,0000,0000,,Phải, tuyệt lắm...
Dialogue: 0,0:10:39.14,0:10:40.76,Default,,0000,0000,0000,,Tôi thấy hứng lên rồi...
Dialogue: 0,0:10:42.81,0:10:44.21,Default,,0000,0000,0000,,Phải, phải...
Dialogue: 0,0:10:44.34,0:10:46.17,Default,,0000,0000,0000,,Hậu môn tôi rát quá...
Dialogue: 0,0:10:46.54,0:10:48.14,Default,,0000,0000,0000,,Chơi tôi mạnh nữa lên...
Dialogue: 0,0:10:54.09,0:10:56.11,Default,,0000,0000,0000,,Mạnh nữa lên nào!
Dialogue: 0,0:10:58.29,0:11:00.15,Default,,0000,0000,0000,,Ko, chuyện này ko giống như lúc trước.
Dialogue: 0,0:11:00.29,0:11:01.52,Default,,0000,0000,0000,,Hãy giúp tôi quên đi...
Dialogue: 0,0:11:01.63,0:11:05.49,Default,,0000,0000,0000,,Chuyện đồi bại của những gã đàn ông đó...
Dialogue: 0,0:11:05.96,0:11:08.30,Default,,0000,0000,0000,,Chơi tôi mạnh nữa lên nào!
Dialogue: 0,0:11:08.87,0:11:10.70,Default,,0000,0000,0000,,Chuyện đồi bại của những gã đàn ông?
Dialogue: 0,0:11:12.10,0:11:13.83,Default,,0000,0000,0000,,Nữa đi, nữa đi...
Dialogue: 0,0:11:14.27,0:11:17.80,Default,,0000,0000,0000,,Những gã đàn ông, huh?
Dialogue: 0,0:11:19.98,0:11:21.10,Default,,0000,0000,0000,,Đừng rút nó ra!
Dialogue: 0,0:11:45.67,0:11:47.73,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:11:49.64,0:11:52.70,Default,,0000,0000,0000,,Tôi ra rồi, tôi ra rồi, tôi ra rồi!
Dialogue: 0,0:11:58.22,0:11:59.11,Default,,0000,0000,0000,,Nóng quá...
Dialogue: 0,0:11:59.22,0:12:01.78,Default,,0000,0000,0000,,Cậu đang ra bên trong tôi!
Dialogue: 0,0:12:22.47,0:12:25.41,Default,,0000,0000,0000,,Cậu muốn liên lạc qua mail hay điện thoại?
Dialogue: 0,0:12:25.58,0:12:27.54,Default,,0000,0000,0000,,Chỉ được chọn một ư?
Dialogue: 0,0:12:28.21,0:12:30.51,Default,,0000,0000,0000,,Tôi nghĩ như vậy thì tiện cho hai chúng ta hơn.
Dialogue: 0,0:12:30.88,0:12:33.25,Default,,0000,0000,0000,,Hay là cậu ko muốn gặp tôi nữa?
Dialogue: 0,0:12:34.75,0:12:36.12,Default,,0000,0000,0000,,Sao cũng được.
Dialogue: 0,0:12:36.72,0:12:38.75,Default,,0000,0000,0000,,Tùy cô quyết định.
Dialogue: 0,0:12:39.49,0:12:40.96,Default,,0000,0000,0000,,Tên tôi là Akane.
Dialogue: 0,0:12:41.06,0:12:42.75,Default,,0000,0000,0000,,Tôi sẽ liên lạc với cậu sau.
Dialogue: 0,0:13:03.21,0:13:05.24,Default,,0000,0000,0000,,Mẹ cậu đi vắng rồi hả?
Dialogue: 0,0:13:19.26,0:13:21.56,Default,,0000,0000,0000,,Muốn sang ăn tối cùng tôi ko?
Dialogue: 0,0:13:22.07,0:13:23.43,Default,,0000,0000,0000,,Thật sao?
Dialogue: 0,0:13:23.53,0:13:24.40,Default,,0000,0000,0000,,Tất nhiên rồi.
Dialogue: 0,0:13:24.90,0:13:30.20,Default,,0000,0000,0000,,Chồng tôi đang đi công tác xa, ăn một mình mãi buồn lắm.
Dialogue: 0,0:13:30.34,0:13:33.17,Default,,0000,0000,0000,,Cậu thèm ăn món gì thì cứ bảo tôi.
Dialogue: 0,0:13:34.11,0:13:36.31,Default,,0000,0000,0000,,Tôi sẽ trổ tài nấu ăn cho cậu xem.
Dialogue: 0,0:13:46.59,0:13:47.65,Default,,0000,0000,0000,,Cám ơn cô vì bữa tối.
Dialogue: 0,0:13:47.83,0:13:49.12,Default,,0000,0000,0000,,Đồ ăn ngon lắm.
Dialogue: 0,0:13:49.26,0:13:52.59,Default,,0000,0000,0000,,Có lẽ tay nghề của tôi lại tiến bộ thêm rồi.
Dialogue: 0,0:13:53.76,0:13:57.50,Default,,0000,0000,0000,,Nhưng tôi hơi bất ngờ về chuyện cô mang thai đó.
Dialogue: 0,0:13:58.84,0:13:59.93,Default,,0000,0000,0000,,Thật hả?
Dialogue: 0,0:14:05.78,0:14:08.47,Default,,0000,0000,0000,,Đây là lần đầu tiên, vậy nên tôi cũng ko có nhiều kinh nghiệm.
Dialogue: 0,0:14:09.01,0:14:11.41,Default,,0000,0000,0000,,Hình như em bé đang quẫy bên trong.
Dialogue: 0,0:14:11.72,0:14:12.74,Default,,0000,0000,0000,,Cậu cảm thấy ko?
Dialogue: 0,0:14:13.12,0:14:14.55,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:14:16.09,0:14:18.06,Default,,0000,0000,0000,,Áp tai cậu vào gần hơn xem nào.
Dialogue: 0,0:14:18.42,0:14:19.41,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:14:21.19,0:14:22.25,Default,,0000,0000,0000,,Thế nào?
Dialogue: 0,0:14:22.79,0:14:24.12,Default,,0000,0000,0000,,Nghe thấy gì chưa?
Dialogue: 0,0:14:24.50,0:14:27.83,Default,,0000,0000,0000,,Uhm, để xem sao...
Dialogue: 0,0:14:28.00,0:14:30.23,Default,,0000,0000,0000,,Cậu nghe thấy rồi chứ?
Dialogue: 0,0:14:31.40,0:14:32.16,Default,,0000,0000,0000,,Phải.
Dialogue: 0,0:14:32.30,0:14:36.14,Default,,0000,0000,0000,,Đêm nào tôi cũng nghe cô rên rỉ khi thủ dâm.
Dialogue: 0,0:14:37.88,0:14:38.93,Default,,0000,0000,0000,,Cậu làm tôi xấu hổ đấy.
Dialogue: 0,0:14:41.58,0:14:42.94,Default,,0000,0000,0000,,Ướt hết rồi nè.
Dialogue: 0,0:14:43.38,0:14:44.97,Default,,0000,0000,0000,,Cô ko mặc quần lót sao?
Dialogue: 0,0:14:45.08,0:14:47.74,Default,,0000,0000,0000,,Mặc thế này thoải mái hơn nhiều chứ.
Dialogue: 0,0:14:48.25,0:14:50.69,Default,,0000,0000,0000,,Tiếp tục đi nào.
Dialogue: 0,0:14:57.50,0:14:58.62,Default,,0000,0000,0000,,Minoru-kun...
Dialogue: 0,0:15:31.00,0:15:34.80,Default,,0000,0000,0000,,Ko, tôi phát điên lên mất...
Dialogue: 0,0:15:36.23,0:15:38.46,Default,,0000,0000,0000,,Nhét nó vào bên trong tôi đi!
Dialogue: 0,0:15:44.68,0:15:46.34,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:15:48.91,0:15:50.81,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:15:51.92,0:15:52.47,Default,,0000,0000,0000,,Ko thể tưởng tượng nổi...
Dialogue: 0,0:16:14.44,0:16:15.17,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:16:16.01,0:16:17.50,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:16:23.25,0:16:24.44,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:16:27.62,0:16:28.31,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:16:30.39,0:16:31.35,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:16:32.76,0:16:37.52,Default,,0000,0000,0000,,Làm thế này liệu có ảnh hưởng tới em bé ko nhỉ?
Dialogue: 0,0:16:55.55,0:16:56.81,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:16:57.28,0:16:58.31,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:17:11.36,0:17:16.16,Default,,0000,0000,0000,,Sao tự nhiên con lại đòi đi mua sắm cùng mẹ thế?
Dialogue: 0,0:17:16.80,0:17:17.83,Default,,0000,0000,0000,,Ko có gì đâu ạ.
Dialogue: 0,0:17:18.14,0:17:21.57,Default,,0000,0000,0000,,Tại dạo này mẹ vất vả vì công việc nhiều quá, nên con nghĩ mình nên phụ mẹ mấy chuyện thế này.
Dialogue: 0,0:17:22.01,0:17:24.13,Default,,0000,0000,0000,,Hôm nay con trai mẹ lạ ghê nhỉ...
Dialogue: 0,0:17:25.54,0:17:26.41,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:17:38.69,0:17:39.95,Default,,0000,0000,0000,,Đã lâu ko gặp.
Dialogue: 0,0:17:40.06,0:17:40.79,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:17:42.39,0:17:43.42,Default,,0000,0000,0000,,Trông chị tuyệt lắm.
Dialogue: 0,0:18:01.48,0:18:02.60,Default,,0000,0000,0000,,Cô ấy là ai vậy?
Dialogue: 0,0:18:02.78,0:18:04.58,Default,,0000,0000,0000,,Bạn của mẹ à?
Dialogue: 0,0:18:05.02,0:18:07.81,Default,,0000,0000,0000,,Uhm, cũng gần như vậy...
Dialogue: 0,0:18:13.99,0:18:17.95,sign,,0000,0000,0000,,{\an8\i1\c&H000000&\b1\shad2\4c&H313131&}Akane đây. Tôi muốn gặp cậu. Tôi đang đứng ở...{\i0}
Dialogue: 0,0:18:21.90,0:18:24.46,Default,,0000,0000,0000,,Xin lỗi mẹ, con phải đi gặp bạn ngay bây giờ.
Dialogue: 0,0:18:56.53,0:18:58.93,Default,,0000,0000,0000,,Ko ngờ họ lại có một căn phòng thế này đấy.
Dialogue: 0,0:19:01.97,0:19:04.44,Default,,0000,0000,0000,,Ra là cô cũng thích nơi này.
Dialogue: 0,0:19:05.04,0:19:08.17,Default,,0000,0000,0000,,Cậu cho rằng nơi này sẽ khiến tôi sợ hãi sao?
Dialogue: 0,0:19:10.01,0:19:12.18,Default,,0000,0000,0000,,Trò chơi nào mà chả cần đến sự đau đớn thể xác.
Dialogue: 0,0:19:12.82,0:19:15.58,Default,,0000,0000,0000,,Cô và mẹ tôi gặp nhau ko hẳn là do tình cờ, phải ko?
Dialogue: 0,0:19:15.99,0:19:20.51,Default,,0000,0000,0000,,Nếu cô ko cho tôi biết mối quan hệ của hai người, chuyện này sẽ kết thúc rất nhanh.
Dialogue: 0,0:19:20.93,0:19:25.45,Default,,0000,0000,0000,,Tôi đã nói với cậu rồi mà, tôi quen mẹ cậu lâu rồi, tất cả chỉ là do tình cờ.
Dialogue: 0,0:19:26.00,0:19:27.69,Default,,0000,0000,0000,,Bắt đầu thôi nhỉ?
Dialogue: 0,0:19:38.31,0:19:40.21,Default,,0000,0000,0000,,Nếu cô muốn nói gì thì cứ ra hiệu nhé.
Dialogue: 0,0:20:06.84,0:20:10.83,Default,,0000,0000,0000,,Tôi đang nóng lòng muốn biết mối quan hệ giữa cô và mẹ tôi là thế nào.
Dialogue: 0,0:20:28.23,0:20:30.52,Default,,0000,0000,0000,,Sao, chịu nói chưa hả?
Dialogue: 0,0:20:36.27,0:20:38.86,Default,,0000,0000,0000,,Hiểu rồi, cô ko chịu nói chứ gì?!
Dialogue: 0,0:20:48.01,0:20:50.45,Default,,0000,0000,0000,,Xem cô chịu đựng được bao lâu.
Dialogue: 0,0:20:58.86,0:21:00.19,Default,,0000,0000,0000,,Khát lắm phải ko?
Dialogue: 0,0:21:19.71,0:21:21.70,Default,,0000,0000,0000,,Thoải mái hơn rồi chứ?
Dialogue: 0,0:21:22.75,0:21:24.21,Default,,0000,0000,0000,,Làm ơn cho tôi vào phòng tắm.
Dialogue: 0,0:21:25.28,0:21:26.94,Default,,0000,0000,0000,,Chịu nói chưa hả?
Dialogue: 0,0:21:29.75,0:21:32.22,Default,,0000,0000,0000,,Có vẻ như tôi sẽ phải dùng tới biện pháp cuối cùng.
Dialogue: 0,0:21:37.90,0:21:41.76,Default,,0000,0000,0000,,Muốn tôi nhét nó vào đâu nào?
Dialogue: 0,0:21:53.68,0:21:55.87,Default,,0000,0000,0000,,Cô thích chơi bằng hậu môn lắm phải ko?
Dialogue: 0,0:22:08.16,0:22:09.32,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:22:12.56,0:22:15.46,Default,,0000,0000,0000,,Để tôi giúp cô thỏa mãn với lỗ còn lại nhé.
Dialogue: 0,0:22:22.34,0:22:23.50,Default,,0000,0000,0000,,Tôi tè ra mất!
Dialogue: 0,0:22:30.65,0:22:31.81,Default,,0000,0000,0000,,Tôi tè ra mất!
Dialogue: 0,0:22:31.95,0:22:33.71,Default,,0000,0000,0000,,Tôi tè ra mất!
Dialogue: 0,0:22:44.83,0:22:46.09,Default,,0000,0000,0000,,Bỏ nó ra khỏi hậu môn tôi đi mà!
Dialogue: 0,0:22:46.20,0:22:49.22,Default,,0000,0000,0000,,Bỏ nó ra, bỏ ra!
Dialogue: 0,0:22:50.73,0:22:54.50,Default,,0000,0000,0000,,Vậy hãy nói cho tôi biết mối quan hệ giữa hai người.
Dialogue: 0,0:22:58.84,0:23:01.00,Default,,0000,0000,0000,,Nếu cậu nhất quyết muốn biết như vậy, tôi sẽ nói.
Dialogue: 0,0:23:01.45,0:23:04.90,Default,,0000,0000,0000,,Tôi là chị ruột của mẹ cậu!
Dialogue: 0,0:23:10.82,0:23:12.48,Default,,0000,0000,0000,,Ko thể nào...
Dialogue: 0,0:23:31.84,0:23:33.33,Default,,0000,0000,0000,,Con về rồi à.
Dialogue: 0,0:23:33.44,0:23:35.24,Default,,0000,0000,0000,,Để mẹ hâm lại bữa tối cho con.
Dialogue: 0,0:23:36.88,0:23:39.11,Default,,0000,0000,0000,,Con vừa gặp Akane-san.
Dialogue: 0,0:23:41.39,0:23:43.44,Default,,0000,0000,0000,,Chị ấy cho con biết mọi chuyện rồi sao?
Dialogue: 0,0:23:43.95,0:23:45.08,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:23:52.86,0:23:54.39,Default,,0000,0000,0000,,Mẹ xin lỗi.
Dialogue: 0,0:23:54.80,0:23:57.49,Default,,0000,0000,0000,,Mẹ ko định giấu con chuyện này.
Dialogue: 0,0:23:58.10,0:24:01.94,Default,,0000,0000,0000,,Mẹ định kể cho con biết tất cả vào một ngày nào đó.
Dialogue: 0,0:24:04.54,0:24:08.57,Default,,0000,0000,0000,,Ngày đó cuối cùng đã đến.
Dialogue: 0,0:24:08.95,0:24:13.85,Default,,0000,0000,0000,,Sự thật thì con là con ruột của Akane.
Dialogue: 0,0:24:15.75,0:24:18.15,Default,,0000,0000,0000,,Ko, chuyện này ko giống như lúc trước.
Dialogue: 0,0:24:18.29,0:24:19.35,Default,,0000,0000,0000,,Hãy giúp tôi quên đi...
Dialogue: 0,0:24:19.49,0:24:23.79,Default,,0000,0000,0000,,Chuyện đồi bại của những gã đàn ông đó...
Dialogue: 0,0:24:25.40,0:24:29.66,Default,,0000,0000,0000,,Akane đã bị cưỡng bức và sau đó mang thai, mình chính là con ruột cô ta?
Dialogue: 0,0:24:29.77,0:24:34.43,Default,,0000,0000,0000,,Vậy mình đã làm chuyện đó với mẹ ruột của mình, Akane...
Dialogue: 0,0:24:40.14,0:24:41.58,Default,,0000,0000,0000,,Con làm gì vậy?
Dialogue: 0,0:24:44.78,0:24:45.65,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:24:54.19,0:24:56.09,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:25:32.13,0:25:33.10,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:25:33.83,0:25:34.99,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:25:35.60,0:25:36.76,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:25:46.98,0:25:49.10,Default,,0000,0000,0000,,Làm ơn, dừng lại đi...
Dialogue: 0,0:25:49.98,0:25:50.81,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:25:52.15,0:25:54.67,Default,,0000,0000,0000,,Mẹ cảm thấy nhục nhã chứ?
Dialogue: 0,0:25:55.12,0:25:57.81,Default,,0000,0000,0000,,Khi bị chính đứa trẻ mình nuôi nấng nên người lột trần...
Dialogue: 0,0:25:57.99,0:26:01.05,Default,,0000,0000,0000,,Bô ngực thì bị phơi ra\Nvà miệng thì bú thằng nhỏ của con...
Dialogue: 0,0:26:02.29,0:26:04.95,Default,,0000,0000,0000,,Mẹ sẽ trở thành món đồ chơi cho con!
Dialogue: 0,0:26:10.40,0:26:12.06,Default,,0000,0000,0000,,Giờ thì...
Dialogue: 0,0:26:22.28,0:26:23.27,Default,,0000,0000,0000,,Cái quái gì đây?
Dialogue: 0,0:26:23.75,0:26:26.84,Default,,0000,0000,0000,,Mẹ vẫn chưa trở thành đàn bà ư!
Dialogue: 0,0:26:26.98,0:26:28.78,Default,,0000,0000,0000,,Một trinh nữ cơ đấy!
Dialogue: 0,0:26:32.59,0:26:33.72,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:26:38.80,0:26:40.29,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:26:43.90,0:26:45.23,Default,,0000,0000,0000,,Thú vị thật!
Dialogue: 0,0:26:45.37,0:26:46.77,Default,,0000,0000,0000,,Con định làm gì?
Dialogue: 0,0:26:49.01,0:26:52.77,Default,,0000,0000,0000,,Con sẽ chơi với nơi này một chút!
Dialogue: 0,0:26:53.48,0:26:54.94,Default,,0000,0000,0000,,Ko, dừng lại đi!
Dialogue: 0,0:26:58.18,0:27:00.58,Default,,0000,0000,0000,,Đừng tự biến mình thành một bà mẹ tồi, ka-san...
Dialogue: 0,0:27:00.78,0:27:03.48,Default,,0000,0000,0000,,Với con thì thứ này gọi là tình cảm gia đình.
Dialogue: 0,0:27:17.13,0:27:18.76,Default,,0000,0000,0000,,Vậy nên ko lí do gì con phải dừng lại cả.
Dialogue: 0,0:27:18.90,0:27:20.46,Default,,0000,0000,0000,,Mẹ cảm thấy thích thú phải ko?
Dialogue: 0,0:27:24.04,0:27:26.51,Default,,0000,0000,0000,,Nếu mẹ ko thích, con sẽ dừng lại.
Dialogue: 0,0:27:44.16,0:27:45.72,Default,,0000,0000,0000,,Con ra đây!
Dialogue: 0,0:27:46.36,0:27:47.59,Default,,0000,0000,0000,,Nóng quá!
Dialogue: 0,0:27:47.76,0:27:53.13,Default,,0000,0000,0000,,Tinh dịch của con đang tràn ngập bên trong mẹ!
Dialogue: 0,0:27:55.31,0:27:56.50,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:27:57.14,0:27:59.73,Default,,0000,0000,0000,,Dù là một trinh nữ nhưng xem ra mẹ rất thỏa mãn.
Dialogue: 0,0:28:00.31,0:28:04.30,Default,,0000,0000,0000,,Chắc hẳn mẹ luôn thủ dâm khi ko có con bên cạnh.
Dialogue: 0,0:28:04.92,0:28:08.18,Default,,0000,0000,0000,,Con sẽ chơi hậu môn của mẹ, coi như đó là quà tặng của con!
