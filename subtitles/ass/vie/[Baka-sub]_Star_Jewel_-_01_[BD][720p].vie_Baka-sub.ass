Dialogue: 0,0:00:37.19,0:00:38.38,Default,,0000,0000,0000,,Đáng yêu thật!
Dialogue: 0,0:01:07.53,0:01:09.44,Default,,0000,0000,0000,,Trông cô dễ thương quá trời luôn.
Dialogue: 0,0:01:10.00,0:01:11.04,Default,,0000,0000,0000,,Megumi...
Dialogue: 0,0:01:58.37,0:02:01.45,Default,,0000,0000,0000,,Cô cũng phải làm gì đó cho tôi mới được, Megumi.
Dialogue: 0,0:02:07.35,0:02:09.90,Default,,0000,0000,0000,,Phải, mau liếm nó đi.
Dialogue: 0,0:02:16.27,0:02:18.78,Default,,0000,0000,0000,,Khi cô làm như vậy, tôi...
Dialogue: 0,0:02:23.19,0:02:25.02,Default,,0000,0000,0000,,Dâm thủy đang trào ra....
Dialogue: 0,0:02:32.43,0:02:35.16,Default,,0000,0000,0000,,Thứ đó... không lẽ cô...
Dialogue: 0,0:02:35.16,0:02:36.39,Default,,0000,0000,0000,,là đàn ông?
Dialogue: 0,0:02:36.39,0:02:38.05,Default,,0000,0000,0000,,Tôi không phải đàn ông.
Dialogue: 0,0:02:38.05,0:02:39.52,Default,,0000,0000,0000,,Và cũng chẳng phải đàn bà.
Dialogue: 0,0:02:40.83,0:02:44.35,Default,,0000,0000,0000,,Chúng tôi là những sinh vật đã thất bại trong việc chọn lựa giới tính của riêng mình.
Dialogue: 0,0:02:45.39,0:02:49.44,Default,,0000,0000,0000,,Theo cách gọi của nhân loại, chúng tôi chính là...
Dialogue: 0,0:02:50.50,0:02:51.26,Default,,0000,0000,0000,,Thần.
Dialogue: 0,0:02:55.70,0:02:58.27,Default,,0000,0000,0000,,Tinh hạch của cô sẽ thuộc về tôi.
Dialogue: 0,0:03:13.03,0:03:16.59,Default,,0000,0000,0000,,Không biết đóa hoa nở trong trong khoái lạc của cô sẽ có màu gì nhỉ?
Dialogue: 0,0:03:43.05,0:03:45.19,Default,,0000,0000,0000,,Thật thần kì.
Dialogue: 0,0:03:45.19,0:03:47.35,Default,,0000,0000,0000,,Ánh sáng tinh khiết vô cùng.
Dialogue: 0,0:03:47.35,0:03:48.31,Default,,0000,0000,0000,,Màu sắc của nó thật rực rỡ.
Dialogue: 0,0:03:48.85,0:03:52.31,Default,,0000,0000,0000,,Chứng tỏ trái tim cô cực kì thanh khiết.
Dialogue: 0,0:04:14.65,0:04:16.03,Default,,0000,0000,0000,,Bị phát hiện rồi sao?
Dialogue: 0,0:04:24.30,0:04:26.77,Default,,0000,0000,0000,,Ngươi đi quá giới hạn rồi đó, ma thần.
Dialogue: 0,0:04:27.05,0:04:28.62,Default,,0000,0000,0000,,Bẻ gẫy nghiền nát! 
Dialogue: 0,0:05:15.07,0:05:16.51,Default,,0000,0000,0000,,Mình không thể rơi vào tay bọn chúng được.
Dialogue: 0,0:05:17.28,0:05:18.55,Default,,0000,0000,0000,,Chính là chỗ này.
Dialogue: 0,0:05:20.65,0:05:21.02,Default,,0000,0000,0000,,Mở ra!
Dialogue: 0,0:06:04.26,0:06:07.49,Default,,0000,0000,0000,,Mau trả tinh hạch lại cho cô ấy.
Dialogue: 0,0:06:07.49,0:06:09.86,Default,,0000,0000,0000,,Cô nghĩ mình là ai hả?
Dialogue: 0,0:06:09.86,0:06:11.18,Default,,0000,0000,0000,,Vậy thì không còn cách nào cả.
Dialogue: 0,0:06:31.17,0:06:35.43,Default,,0000,0000,0000,,Liệu tinh hạch của cô có đủ sức tiếp nhận cơn khoái lạc do tôi mang đến không hả?
Dialogue: 0,0:06:38.35,0:06:39.44,Default,,0000,0000,0000,,Vậy thì tốt.
Dialogue: 0,0:06:41.05,0:06:42.91,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:06:54.95,0:06:57.55,Default,,0000,0000,0000,,Mau cho tôi thấy hình dạng thật của cô đi nào.
Dialogue: 0,0:07:06.66,0:07:09.57,Default,,0000,0000,0000,,Tên cô là gì nhỉ?
Dialogue: 0,0:07:11.49,0:07:13.69,Default,,0000,0000,0000,,Tôi không bao giờ xưng danh trước kẻ thù cả.
Dialogue: 0,0:07:15.10,0:07:16.04,Default,,0000,0000,0000,,Đáng yêu lắm.
Dialogue: 0,0:07:18.38,0:07:20.05,Default,,0000,0000,0000,,Tôi sẽ không trao nó cho cô đâu.
Dialogue: 0,0:07:21.69,0:07:22.95,Default,,0000,0000,0000,,Vậy thì tôi buộc phải xuống tay vậy.
Dialogue: 0,0:07:23.50,0:07:24.60,Default,,0000,0000,0000,,Tên tôi là Natsumi.
Dialogue: 0,0:07:28.15,0:07:29.35,Default,,0000,0000,0000,,Jinno Natsumi.
Dialogue: 0,0:07:30.29,0:07:32.42,Default,,0000,0000,0000,,Đây là nghĩa vụ của tôi.
Dialogue: 0,0:07:39.57,0:07:42.83,Default,,0000,0000,0000,,Không biết tinh hạch của cô sẽ có màu gì nhỉ?
Dialogue: 0,0:08:01.59,0:08:02.99,Default,,0000,0000,0000,,Xin lỗi đã để cô chờ lâu.
Dialogue: 0,0:08:04.15,0:08:05.26,Default,,0000,0000,0000,,Cám ơn anh.
Dialogue: 0,0:08:29.95,0:08:31.99,Default,,0000,0000,0000,,Đây là điều tất yếu sẽ phải xảy ra.
Dialogue: 0,0:08:31.99,0:08:35.45,Default,,0000,0000,0000,,Trái tim thánh thiện của cô sẽ đồng hành với sự cô đơn.
Dialogue: 0,0:08:35.45,0:08:36.39,Default,,0000,0000,0000,,Cô là ai?
Dialogue: 0,0:08:36.39,0:08:37.76,Default,,0000,0000,0000,,Lapis đâu rồi?
Dialogue: 0,0:08:37.76,0:08:38.77,Default,,0000,0000,0000,,Lapis?
Dialogue: 0,0:08:40.92,0:08:42.56,Default,,0000,0000,0000,,Lapis đâu mất rồi?
Dialogue: 0,0:08:43.20,0:08:44.84,Default,,0000,0000,0000,,Đó là tên của cô ta ư?
Dialogue: 0,0:08:44.84,0:08:45.89,Default,,0000,0000,0000,,Lapis...
Dialogue: 0,0:08:46.75,0:08:48.77,Default,,0000,0000,0000,,sẽ không bao giờ quay về nữa.
Dialogue: 0,0:08:48.77,0:08:50.14,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:08:50.14,0:08:52.72,Default,,0000,0000,0000,,Bởi vì cô ta đã tìm cách hãm hại cô.
Dialogue: 0,0:08:54.00,0:08:54.90,Default,,0000,0000,0000,,Không đâu!
Dialogue: 0,0:08:54.90,0:08:57.48,Default,,0000,0000,0000,,Cô ấy sẽ không đời nào làm chuyện xấu xa đó.
Dialogue: 0,0:08:58.10,0:09:01.70,Default,,0000,0000,0000,,Cô ta đã cố cướp đi báu vật vô giá của cô.
Dialogue: 0,0:09:02.59,0:09:03.71,Default,,0000,0000,0000,,Không phải đâu!
Dialogue: 0,0:09:03.71,0:09:05.19,Default,,0000,0000,0000,,Tôi tình nguyện tặng nó cho cô ấy!
Dialogue: 0,0:09:05.19,0:09:07.73,Default,,0000,0000,0000,,Tặng cho cô ấy tất cả!
Dialogue: 0,0:09:07.73,0:09:08.79,Default,,0000,0000,0000,,Phải.
Dialogue: 0,0:09:09.74,0:09:12.83,Default,,0000,0000,0000,,Tất cả chỉ là ảo giác mà thôi.
Dialogue: 0,0:09:12.83,0:09:14.86,Default,,0000,0000,0000,,Cô hãy quên hết mọi chuyện đi.
Dialogue: 0,0:10:05.45,0:10:06.95,Default,,0000,0000,0000,,Tôi tình nguyện tặng nó cho cô ấy!
Dialogue: 0,0:10:06.95,0:10:08.95,Default,,0000,0000,0000,,Tặng cho cô ấy tất cả!
Dialogue: 0,0:10:14.31,0:10:15.93,Default,,0000,0000,0000,,Làm tốt lắm, Natsumi.
Dialogue: 0,0:10:17.41,0:10:18.60,Default,,0000,0000,0000,,Reiko!
Dialogue: 0,0:10:18.95,0:10:21.25,Default,,0000,0000,0000,,Chắc là cô vừa mới kết thúc trận chiến.
Dialogue: 0,0:10:21.25,0:10:23.26,Default,,0000,0000,0000,,Đã thu hồi được tinh hạch của nhân loại chưa?
Dialogue: 0,0:10:23.97,0:10:26.41,Default,,0000,0000,0000,,Rồi. Tinh hạch đã được hoàn lại cho chủ nhân của nó.
Dialogue: 0,0:10:26.41,0:10:28.59,Default,,0000,0000,0000,,Ma thần đã trốn thoát.
Dialogue: 0,0:10:28.95,0:10:30.11,Default,,0000,0000,0000,,Hiểu rồi.
Dialogue: 0,0:10:30.11,0:10:32.95,Default,,0000,0000,0000,,Máy bay của tôi đang chuẩn bị hạ thấp độ cao.
Dialogue: 0,0:10:33.46,0:10:35.88,Default,,0000,0000,0000,,Cô đã đoạt được tinh hạch của cô ta.
Dialogue: 0,0:10:35.88,0:10:38.93,Default,,0000,0000,0000,,Tên cô ta là Lapis phải không?
Dialogue: 0,0:10:40.74,0:10:41.97,Default,,0000,0000,0000,,Lapis...
Dialogue: 0,0:10:43.70,0:10:48.38,Default,,0000,0000,0000,,Dạo này dường như bọn chúng xuất hiện thường xuyên hơn thì phải?
Dialogue: 0,0:10:48.38,0:10:51.58,Default,,0000,0000,0000,,Có vẻ như bọn chúng đang âm mưu chuyện gì đó!
Dialogue: 0,0:10:51.58,0:10:52.59,Default,,0000,0000,0000,,Mei...
Dialogue: 0,0:10:52.59,0:10:54.85,Default,,0000,0000,0000,,Cô đã phát hiện ra điều gì sao, Mei?
Dialogue: 0,0:10:58.28,0:10:59.91,Default,,0000,0000,0000,,Cũng không hẳn.
Dialogue: 0,0:10:59.91,0:11:02.18,Default,,0000,0000,0000,,Nhưng chúng ta vẫn phải chuẩn bị sẵn sàng.
Dialogue: 0,0:11:02.59,0:11:06.00,Default,,0000,0000,0000,,Tôi không thích bị phục kích bất ngờ đâu.
Dialogue: 0,0:11:07.65,0:11:08.66,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:11:09.72,0:11:12.15,Default,,0000,0000,0000,,Các cô phải luôn đề cao cảnh giác.
Dialogue: 0,0:11:13.90,0:11:16.45,Default,,0000,0000,0000,,Natsumi, nếu phát hiện ra bọn chúng,
Dialogue: 0,0:11:16.45,0:11:18.82,Default,,0000,0000,0000,,đừng ngần ngại đoạt lấy tinh hạch.
Dialogue: 0,0:11:19.85,0:11:22.94,Default,,0000,0000,0000,,Tôi cảm thấy hơi thất vọng.
Dialogue: 0,0:11:22.94,0:11:25.76,Default,,0000,0000,0000,,Điều này giống như chúng ta đang tàn sát đồng loại vậy.
Dialogue: 0,0:11:27.98,0:11:31.56,Default,,0000,0000,0000,,Quê hương của các thần nữ nằm tại một tinh cầu khác.
Dialogue: 0,0:11:31.56,0:11:34.14,Default,,0000,0000,0000,,Chúng tôi chính là những người bảo vệ được gửi tới từ Thần giới.
Dialogue: 0,0:11:46.30,0:11:52.47,Default,,0000,0000,0000,,Các ma thần luôn tìm cách thoát khỏi cảnh lưu đày tại chốn tối tăm hắc ám, Trầm Luân giới,
Dialogue: 0,0:11:52.47,0:11:56.90,Default,,0000,0000,0000,,và nghĩa vụ của chúng tôi chính là ngăn cản họ quay về.
Dialogue: 0,0:12:01.77,0:12:07.82,Default,,0000,0000,0000,,Trong lúc ngưng kết tinh hạch của chính mình, các ma thần thu gom tinh hạch đánh cắp từ tay nhân loại.
Dialogue: 0,0:12:07.82,0:12:12.67,Default,,0000,0000,0000,,Nhằm tìm cách khởi động chiếc phi thuyền được giấu trong Đại không gian.
Dialogue: 0,0:12:14.54,0:12:18.44,Default,,0000,0000,0000,,Họ định dùng nó để quay về quê hương của chúng tôi, Thần giới.
Dialogue: 0,0:12:18.44,0:12:20.16,Default,,0000,0000,0000,,Đó cũng là cách duy nhất.
Dialogue: 0,0:12:21.14,0:12:24.11,Default,,0000,0000,0000,,Họ phải khởi động Thái Dương Thần Hạm.
Dialogue: 0,0:12:38.20,0:12:41.81,Default,,0000,0000,0000,,Luudi nè, tình trạng của Lapis ra sao rồi?
Dialogue: 0,0:12:42.67,0:12:47.31,Default,,0000,0000,0000,,Cô ấy đã mất tinh hạch và rơi vào hôn mê.
Dialogue: 0,0:12:48.20,0:12:52.96,Default,,0000,0000,0000,,Phải mất 2000 năm để ngưng kết lại nó.
Dialogue: 0,0:13:10.85,0:13:14.70,Default,,0000,0000,0000,,Còn Thần nữ đánh bại cô ấy thì sao?
Dialogue: 0,0:13:18.37,0:13:20.50,Default,,0000,0000,0000,,Cô ta đã biến mất.
Dialogue: 0,0:13:21.50,0:13:27.19,Default,,0000,0000,0000,,Chắc hẳn cô ta đã giả trang thành nhân loại.
Dialogue: 0,0:13:27.80,0:13:28.97,Default,,0000,0000,0000,,Ra vậy.
Dialogue: 0,0:13:30.36,0:13:33.75,Default,,0000,0000,0000,,Răng nanh của mày đã cùng đi rồi sao, Lyktaito?
Dialogue: 0,0:13:37.18,0:13:40.31,Default,,0000,0000,0000,,Jirokon-sama, xin hãy ra lệnh.
Dialogue: 0,0:13:40.85,0:13:44.81,Default,,0000,0000,0000,,Cô muốn ta ra lệnh thế nào, Luudi?
Dialogue: 0,0:13:49.34,0:13:52.05,Default,,0000,0000,0000,,"Báo thù cho Lapis."
Dialogue: 0,0:13:54.18,0:13:59.18,Default,,0000,0000,0000,,Mục đích của chúng ta không phải là gây chiến với các thần nữ.
Dialogue: 0,0:13:59.18,0:14:02.21,Default,,0000,0000,0000,,Hơn nữa, chúng ta cần phải thu thập đủ số tinh hạch.
Dialogue: 0,0:14:02.21,0:14:03.03,Default,,0000,0000,0000,,Tôi phản đối!
Dialogue: 0,0:14:03.03,0:14:07.31,Default,,0000,0000,0000,,Tôi không muốn mất thêm một người bạn nào nữa.
Dialogue: 0,0:14:14.84,0:14:17.86,Default,,0000,0000,0000,,Cô ghét tôi đến vậy sao?
Dialogue: 0,0:14:21.56,0:14:24.31,Default,,0000,0000,0000,,Liệu cô có cơ may chiến thắng không?
Dialogue: 0,0:14:38.74,0:14:39.81,Default,,0000,0000,0000,,Jirokon-sama!
Dialogue: 0,0:14:41.30,0:14:43.46,Default,,0000,0000,0000,,Cô phải chiến thắng trở về.
Dialogue: 0,0:14:43.46,0:14:45.43,Default,,0000,0000,0000,,Đó là điều kiện để cô xuất chiến.
Dialogue: 0,0:14:46.25,0:14:47.12,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:15:09.42,0:15:10.63,Default,,0000,0000,0000,,Tôi sẽ không tha thứ cho cô ta.
Dialogue: 0,0:15:10.63,0:15:14.88,Default,,0000,0000,0000,,Thôi sẽ không tha thứ cho thần nữ đã đẩy cô vào tình cảnh này!
Dialogue: 0,0:15:17.06,0:15:19.56,Default,,0000,0000,0000,,Lapis... Lapis...
Dialogue: 0,0:15:24.68,0:15:25.45,Default,,0000,0000,0000,,Lapis...
Dialogue: 0,0:15:37.79,0:15:39.05,Default,,0000,0000,0000,,Lapis...
Dialogue: 0,0:15:39.05,0:15:44.56,Default,,0000,0000,0000,,Hãy chia sẽ trái tim và sức mạnh của cô cùng tôi!
Dialogue: 0,0:16:28.77,0:16:30.85,Default,,0000,0000,0000,,Chuyện gì vậy, Reiko?
Dialogue: 0,0:16:30.85,0:16:33.68,Default,,0000,0000,0000,,Khi tập trung tinh thần, tôi đã cảm thấy sóng năng lượng dao động.
Dialogue: 0,0:16:34.57,0:16:38.12,Default,,0000,0000,0000,,Một tinh hạch đang được ngưng kết ở gần đây.
Dialogue: 0,0:16:38.91,0:16:40.03,Default,,0000,0000,0000,,Không thể nào.
Dialogue: 0,0:16:40.03,0:16:41.68,Default,,0000,0000,0000,,Tôi vừa trả nó lại cách đây vài giờ mà.
Dialogue: 0,0:16:41.68,0:16:43.30,Default,,0000,0000,0000,,Không lẽ là kẻ khác?
Dialogue: 0,0:16:45.05,0:16:47.05,Default,,0000,0000,0000,,Cần phải truy lùng nó ngay lập tức.
Dialogue: 0,0:16:48.06,0:16:50.65,Default,,0000,0000,0000,,Reiko, cô giúp tôi một tay nhé?
Dialogue: 0,0:16:50.65,0:16:51.68,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:17:01.89,0:17:04.43,Default,,0000,0000,0000,,Ơ? Vừa có ai ở đây ư?
Dialogue: 0,0:17:28.55,0:17:30.05,Default,,0000,0000,0000,,Tôi sẽ tới đó!
Dialogue: 0,0:17:30.05,0:17:32.60,Default,,0000,0000,0000,,Natsumi vẫn chưa hồi phục phải không?
Dialogue: 0,0:18:30.32,0:18:33.05,Default,,0000,0000,0000,,Mình sẽ mất nốt cô ấy ư?
Dialogue: 0,0:18:34.24,0:18:37.21,Default,,0000,0000,0000,,Nếu tình yêu chính là tội lỗi...
Dialogue: 0,0:18:37.21,0:18:41.07,Default,,0000,0000,0000,,thì vũ trụ này cũng sinh ra trong tội lỗi.
Dialogue: 0,0:18:41.72,0:18:44.20,Default,,0000,0000,0000,,Nếu điều đó là chân lý,
Dialogue: 0,0:18:44.20,0:18:47.57,Default,,0000,0000,0000,,thì tôi sẽ không còn vướng bận bởi gánh nặng tri thức trên vai nữa...
Dialogue: 0,0:19:52.42,0:19:54.52,Default,,0000,0000,0000,,Ta sẽ báo thù cho Lapis!
Dialogue: 0,0:21:15.13,0:21:18.10,Default,,0000,0000,0000,,Điều này là dành cho Lapis!
Dialogue: 0,0:21:36.16,0:21:39.10,Default,,0000,0000,0000,,Cô đã khơi dậy thú tính bên trong ta.
Dialogue: 0,0:21:39.10,0:21:43.51,Default,,0000,0000,0000,,Tinh hạch của cô sẽ thuộc về ta!
Dialogue: 0,0:21:50.55,0:21:53.55,Default,,0000,0000,0000,,Đừng cố chống cự.
Dialogue: 0,0:22:04.72,0:22:06.31,Default,,0000,0000,0000,,Thật thần kì.
Dialogue: 0,0:22:06.31,0:22:10.31,Default,,0000,0000,0000,,Sức mạnh của ta đang tăng lên.
Dialogue: 0,0:22:10.56,0:22:17.29,Default,,0000,0000,0000,,Ta có thể cảm nhận được tinh hạch bên trong cơ thể cô.
Dialogue: 0,0:22:17.29,0:22:18.70,Default,,0000,0000,0000,,Sức mạnh của ta...
Dialogue: 0,0:22:27.05,0:22:32.22,Default,,0000,0000,0000,,Ta muốn trông thấy cô bị nhấn chìm trong dục vọng.
Dialogue: 0,0:22:48.36,0:22:50.71,Default,,0000,0000,0000,,Một đóa hoa khổng lồ.
Dialogue: 0,0:22:50.71,0:22:53.70,Default,,0000,0000,0000,,Đây là điểm báo cho sự thức tỉnh của Thái Dương Thần Hạm.
Dialogue: 0,0:22:54.27,0:22:56.47,Default,,0000,0000,0000,,Thái Dương Thần Hạm?
Dialogue: 0,0:22:56.48,0:22:57.31,Default,,0000,0000,0000,,Đừng mà.
Dialogue: 0,0:22:57.31,0:22:58.88,Default,,0000,0000,0000,,Trừ chuyện này ra.
Dialogue: 0,0:23:09.10,0:23:11.92,Default,,0000,0000,0000,,Tôi sẽ không chịu thua cô đâu.
Dialogue: 0,0:23:22.05,0:23:23.56,Default,,0000,0000,0000,,Chuyện gì vậy!
Dialogue: 0,0:24:04.33,0:24:07.60,Default,,0000,0000,0000,,Đừng nhìn ta!
Dialogue: 0,0:24:11.70,0:24:13.12,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:24:30.64,0:24:33.10,Default,,0000,0000,0000,,Cô cũng cảm thấy phải không?
Dialogue: 0,0:24:33.10,0:24:34.06,Default,,0000,0000,0000,,Tôi sắp ra rồi.
Dialogue: 0,0:25:07.58,0:25:08.95,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:25:08.95,0:25:10.30,Default,,0000,0000,0000,,Cơ thể cô thật tuyệt.
Dialogue: 0,0:25:11.52,0:25:15.98,Default,,0000,0000,0000,,Tôi sẽ trả lại nó.
Dialogue: 0,0:25:21.10,0:25:21.88,Default,,0000,0000,0000,,Natsumi.
Dialogue: 0,0:25:23.14,0:25:24.76,Default,,0000,0000,0000,,Tôi vẫn ổn.
Dialogue: 0,0:25:24.76,0:25:26.64,Default,,0000,0000,0000,,Tôi đã đoạt lại tinh hạch.
Dialogue: 0,0:25:26.64,0:25:27.77,Default,,0000,0000,0000,,Vậy thì tốt!
Dialogue: 0,0:25:27.77,0:25:29.71,Default,,0000,0000,0000,,Tôi vừa định tới để giải quyết cô ta.
Dialogue: 0,0:25:29.71,0:25:31.99,Default,,0000,0000,0000,,Đừng để cô ta chạy thoát nha
Dialogue: 0,0:25:31.99,0:25:32.60,Default,,0000,0000,0000,,Rõ rồi.
Dialogue: 0,0:25:42.39,0:25:43.25,Default,,0000,0000,0000,,Thành công rồi!
Dialogue: 0,0:25:47.55,0:25:50.21,Default,,0000,0000,0000,,Đó không phải là tinh hạch của nhân loại.
Dialogue: 0,0:25:50.70,0:25:53.45,Default,,0000,0000,0000,,Đó là tinh hạch của chúng ta, tinh hạch của ma thần
Dialogue: 0,0:26:01.92,0:26:07.73,Default,,0000,0000,0000,,Đây chính là tinh hạch... của em gái ta, Sapphire.
Dialogue: 0,0:26:10.10,0:26:15.60,Default,,0000,0000,0000,,Tinh hạch của ma thần chính là độc dược cực mạnh với linh hồn thần nữ.
Dialogue: 0,0:26:15.60,0:26:18.69,Default,,0000,0000,0000,,Linh hồn thanh khiết của cô sẽ phải chịu đựng sự dày vò vĩnh cửu,
Dialogue: 0,0:26:19.76,0:26:23.93,Default,,0000,0000,0000,,hoặc là cô sẽ nhập ma, trở thành một trong chúng ta.
Dialogue: 0,0:26:38.00,0:26:39.31,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:26:39.31,0:26:43.18,Default,,0000,0000,0000,,Đôi cánh của cô ta bị sao vậy?
Dialogue: 0,0:26:45.18,0:26:46.50,Default,,0000,0000,0000,,Natsumi!
Dialogue: 0,0:26:53.52,0:26:55.61,Default,,0000,0000,0000,,Nhà ngươi đừng hòng chạy thoát.
Dialogue: 0,0:27:00.03,0:27:00.82,Default,,0000,0000,0000,,Mei!
Dialogue: 0,0:27:00.82,0:27:02.16,Default,,0000,0000,0000,,Chăm sóc Natsumi dùm tôi!
Dialogue: 0,0:27:02.99,0:27:04.97,Default,,0000,0000,0000,,Tôi cũng sẽ đuổi theo cô ta!
Dialogue: 0,0:27:05.60,0:27:07.52,Default,,0000,0000,0000,,Xin lỗi, cho tôi mượn nó một lúc nhé.
Dialogue: 0,0:27:08.95,0:27:10.68,Default,,0000,0000,0000,,Hừ!
Dialogue: 0,0:27:10.68,0:27:12.00,Default,,0000,0000,0000,,Natsumi!
Dialogue: 0,0:27:12.37,0:27:14.62,Default,,0000,0000,0000,,Natsumi, cánh của cô bị làm sao vậy?
Dialogue: 0,0:27:15.60,0:27:17.84,Default,,0000,0000,0000,,Đừng lại gần tôi!
Dialogue: 0,0:27:17.84,0:27:19.60,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:27:45.75,0:27:48.60,Default,,0000,0000,0000,,Ngươi đã gây họa lớn rồi.
Dialogue: 0,0:27:48.60,0:27:50.96,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:27:50.96,0:27:54.08,Default,,0000,0000,0000,,Ngươi đã phá hoại kế hoạch của chúng ta.
Dialogue: 0,0:27:55.45,0:27:57.60,Default,,0000,0000,0000,,Ta sẽ không giao tinh hạch cho cô đâu.
Dialogue: 0,0:27:58.00,0:27:59.68,Default,,0000,0000,0000,,Ta cần gì phải chiếm lấy nó chứ.
Dialogue: 0,0:28:00.25,0:28:02.95,Default,,0000,0000,0000,,Nó vốn luôn luôn thuộc về ta.
Dialogue: 0,0:28:02.95,0:28:04.05,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:28:04.05,0:28:05.06,Default,,0000,0000,0000,,Cô định làm gì ta?
Dialogue: 0,0:28:05.06,0:28:06.64,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:28:06.64,0:28:09.60,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:28:11.85,0:28:12.97,Default,,0000,0000,0000,,Khốn kiếp!
Dialogue: 0,0:28:13.57,0:28:15.71,Default,,0000,0000,0000,,Cô là ai?
Dialogue: 0,0:28:24.83,0:28:27.13,Default,,0000,0000,0000,,Cô làm gì vậy, Natsumi?
Dialogue: 0,0:28:27.13,0:28:28.57,Default,,0000,0000,0000,,Tôi xin lỗi, Mei.
Dialogue: 0,0:28:28.57,0:28:30.89,Default,,0000,0000,0000,,Tôi đã quên mất.
Dialogue: 0,0:28:30.89,0:28:33.45,Default,,0000,0000,0000,,Chuyện gì đã xảy ra?
Dialogue: 0,0:28:33.45,0:28:35.63,Default,,0000,0000,0000,,Tôi... Tôi...
Dialogue: 0,0:28:44.50,0:28:46.88,Default,,0000,0000,0000,,Đừng mà, Natsumi!
Dialogue: 0,0:28:53.92,0:28:55.52,Default,,0000,0000,0000,,Ngừng lại.
Dialogue: 0,0:28:56.20,0:29:01.38,Default,,0000,0000,0000,,Thần nữ bị cấm quan hệ cùng nhau.
Dialogue: 0,0:29:01.38,0:29:03.35,Default,,0000,0000,0000,,Tôi không thể ngừng lại được... 
Dialogue: 0,0:29:16.85,0:29:19.00,Default,,0000,0000,0000,,Tôi xin lỗi.
Dialogue: 0,0:30:20.22,0:30:21.77,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:30:21.77,0:30:24.54,Default,,0000,0000,0000,,Nếu đi quá giới hạn, Thần nữ sẽ...
Dialogue: 0,0:30:25.32,0:30:27.95,Default,,0000,0000,0000,,Natsumi!
Dialogue: 0,0:30:27.95,0:30:31.63,Default,,0000,0000,0000,,Tôi đã không còn là Thần nữ nữa rồi.
Dialogue: 0,0:30:47.64,0:30:49.70,Default,,0000,0000,0000,,R... Reiko...
Dialogue: 0,0:30:53.40,0:30:55.20,Default,,0000,0000,0000,,Để tôi giúp cô một tay.
Dialogue: 0,0:30:57.37,0:30:58.60,Default,,0000,0000,0000,,Mau ra đi nào.
Dialogue: 0,0:30:58.60,0:30:59.66,Default,,0000,0000,0000,,Natsumi.
Dialogue: 0,0:31:18.06,0:31:19.65,Default,,0000,0000,0000,,Sớm thôi mà.
Dialogue: 0,0:31:48.65,0:31:49.68,Default,,0000,0000,0000,,Đó là thứ gì vậy...
Dialogue: 0,0:31:57.62,0:32:00.16,Default,,0000,0000,0000,,Reiko, đó là thứ gì vậy?
Dialogue: 0,0:32:00.16,0:32:03.54,Default,,0000,0000,0000,,Hãy mang nó về phi thuyền mẹ.
Dialogue: 0,0:32:03.54,0:32:04.27,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:32:18.23,0:32:19.30,Default,,0000,0000,0000,,Cuối cùng...
Dialogue: 0,0:32:20.83,0:32:23.41,Default,,0000,0000,0000,,Ngày nó thức tỉnh đã tới.
Dialogue: 0,0:32:42.20,0:32:44.69,Default,,0000,0000,0000,,Tới lúc rồi ư?
Dialogue: 0,0:32:52.75,0:32:56.29,Default,,0000,0000,0000,,Tôi sẽ không buông xuôi đâu!
