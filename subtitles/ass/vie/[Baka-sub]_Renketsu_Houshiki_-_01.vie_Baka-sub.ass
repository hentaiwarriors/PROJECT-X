Dialogue: 0,0:00:01.42,0:00:05.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net !
Dialogue: 0,0:00:06.51,0:00:08.82,Default,,0000,0000,0000,,Em yêu an
Dialogue: 0,0:00:09.00,0:00:10.36,Default,,0000,0000,0000,,Keisuke!
Dialogue: 0,0:00:22.82,0:00:24.84,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:00:43.13,0:00:46.25,Default,,0000,0000,0000,,Cha tôi được chẩn đoán là mắc bệnh ung thư.
Dialogue: 0,0:00:46.97,0:00:48.85,Default,,0000,0000,0000,,Thời gian của ông ko còn nhiều...
Dialogue: 0,0:00:51.60,0:00:56.31,Default,,0000,0000,0000,,Vì vậy... Mẹ muốn tôi kết hôn sớm...
Dialogue: 0,0:00:57.28,0:00:59.46,Default,,0000,0000,0000,,Bà đã nhiều lần khóc cầu xin tôi.
Dialogue: 0,0:01:02.00,0:01:04.47,Default,,0000,0000,0000,,Chúng ta thật sự rất ngạc nhiên.
Dialogue: 0,0:01:04.87,0:01:07.35,Default,,0000,0000,0000,,Ko ngờ cậu ấy lại quyết định nhanh đến vậy.
Dialogue: 0,0:01:07.62,0:01:10.40,Default,,0000,0000,0000,,Từ trước đến giờ tôi chưa hề có bạn gái.
Dialogue: 0,0:01:10.67,0:01:12.76,Default,,0000,0000,0000,,Tôi luôn thất bại trong những chuyện như thế này.
Dialogue: 0,0:01:12.88,0:01:15.14,Default,,0000,0000,0000,,Keisuke tốt số thật đấy.
Dialogue: 0,0:01:15.30,0:01:18.74,Default,,0000,0000,0000,,Có lẽ hơi khó tin, nhưng đám cưới này đã được sắp đặt từ trước.
Dialogue: 0,0:01:19.13,0:01:22.04,Default,,0000,0000,0000,,Gia đình hai bên đã đồng ý chuyện này trước cả khi tôi biết mình sắp kết hôn.
Dialogue: 0,0:01:25.94,0:01:27.84,Default,,0000,0000,0000,,Chị ấy ko hợp...
Dialogue: 0,0:01:28.05,0:01:30.11,Default,,0000,0000,0000,,Vợ chưa cưới của Onii-chan ấy.
Dialogue: 0,0:01:30.59,0:01:35.40,Default,,0000,0000,0000,,Con nghĩ vậy sao? Mẹ thấy cô ấy tuyệt đấy chứ.
Dialogue: 0,0:01:35.55,0:01:38.60,Default,,0000,0000,0000,,Với một người trầm tính như Keisuke, ko ai thích hợp hơn cô ấy đâu.
Dialogue: 0,0:01:38.84,0:01:43.29,Default,,0000,0000,0000,,Nhưng mà Keisuke mới 29 tuổi. Quá sớm để kết hôn
Dialogue: 0,0:01:43.41,0:01:46.04,Default,,0000,0000,0000,,Misaki, con thôi phàn nàn đi.
Dialogue: 0,0:01:46.16,0:01:48.46,Default,,0000,0000,0000,,Giúp mẹ chuẩn bị bữa tối nào.
Dialogue: 0,0:01:57.83,0:02:01.94,Default,,0000,0000,0000,,Con bé và Keisuke hơn kém nhau tận 10 tuổi.
Dialogue: 0,0:02:03.30,0:02:06.90,Default,,0000,0000,0000,,Có lẽ do con bé sinh ra khá muộn nên tôi có hơi nuông chiều nó.
Dialogue: 0,0:02:07.05,0:02:10.95,Default,,0000,0000,0000,,Lúc nhỏ dù đã tự đi được nhưng con bé luôn bám lấy anh trai của nó.
Dialogue: 0,0:02:11.31,0:02:13.21,Default,,0000,0000,0000,,Mẹ, đừng kể chuyện đó ở đây...
Dialogue: 0,0:02:13.31,0:02:17.60,Default,,0000,0000,0000,,Oh, Tomoko-san cũng sắp là người nhà chúng ta rồi, việc gì mà con phải xấu hổ chứ.
Dialogue: 0,0:02:20.08,0:02:22.68,Default,,0000,0000,0000,,Hai anh em nó từ nhỏ đã luôn quấn quít bên nhau
Dialogue: 0,0:02:22.98,0:02:27.91,Default,,0000,0000,0000,,Con bé ko còn anh chị em nào khác nên chắc hơi ghen tị khi anh trai mình kết hôn.
Dialogue: 0,0:02:40.60,0:02:45.80,Default,,0000,0000,0000,,{\be1\c&H54B9B9&\fad(500,0)\fs40\pos(320,330)\bord6.5\3c&H782AB5&}Renketsu Houshiki
Dialogue: 0,0:02:43.53,0:02:45.89,Default,,0000,0000,0000,,1st. [ Hold ]
Dialogue: 0,0:03:54.94,0:03:56.60,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:04:23.87,0:04:26.19,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:04:39.68,0:04:42.03,Default,,0000,0000,0000,,Em yêu anh...
Dialogue: 0,0:04:59.75,0:05:02.20,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:05:19.31,0:05:21.60,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:05:25.41,0:05:27.50,Default,,0000,0000,0000,,Cái của anh đang chà xát...
Dialogue: 0,0:05:27.71,0:05:29.61,Default,,0000,0000,0000,,Chỗ đó của em.
Dialogue: 0,0:05:29.92,0:05:31.82,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:05:58.33,0:06:00.87,Default,,0000,0000,0000,,Tuyệt quá đi mất.
Dialogue: 0,0:06:05.22,0:06:07.73,Default,,0000,0000,0000,,Em... Em...
Dialogue: 0,0:06:11.75,0:06:13.54,Default,,0000,0000,0000,,Em...
Dialogue: 0,0:06:13.69,0:06:17.01,Default,,0000,0000,0000,,Onii-chan... Onii-chan -
Dialogue: 0,0:06:17.74,0:06:21.52,Default,,0000,0000,0000,,Em... Em sắp... Em sắp ra...
Dialogue: 0,0:06:31.19,0:06:34.27,Default,,0000,0000,0000,,Anh ra trước cả khi kịp nhét nó vào bên trong em sao.
Dialogue: 0,0:06:41.92,0:06:43.74,Default,,0000,0000,0000,,Misaki...
Dialogue: 0,0:06:45.55,0:06:48.69,Default,,0000,0000,0000,,muốn trở thành vợ của anh.
Dialogue: 0,0:06:50.11,0:06:53.95,Default,,0000,0000,0000,,Onii-chan... anh rất yêu Misaki, phải ko?
Dialogue: 0,0:06:55.07,0:06:56.40,Default,,0000,0000,0000,,Em đùa à...
Dialogue: 0,0:06:56.95,0:06:58.09,Default,,0000,0000,0000,,Chúng ta là anh em ruột mà.
Dialogue: 0,0:06:58.22,0:07:01.39,Default,,0000,0000,0000,,Anh luôn... nhìn trộm lúc em tắm.
Dialogue: 0,0:07:01.72,0:07:04.08,Default,,0000,0000,0000,,Ko yêu em thì tại sao anh lại làm vậy chứ?
Dialogue: 0,0:07:13.39,0:07:16.72,Default,,0000,0000,0000,,Anh ko biết... trong đầu mình nghĩ gì lúc đó nữa.
Dialogue: 0,0:07:19.83,0:07:24.42,Default,,0000,0000,0000,,Anh là người rõ hơn ai hết, Misaki chỉ quan tâm đến một người đàn ông duy nhất...
Dialogue: 0,0:07:25.94,0:07:28.47,Default,,0000,0000,0000,,Từ khi em được sinh ra trên đời...
Dialogue: 0,0:07:31.92,0:07:36.27,Default,,0000,0000,0000,,Em nên kiếm cho mình... một cậu bạn trai.
Dialogue: 0,0:07:47.10,0:07:48.15,Default,,0000,0000,0000,,Keisuke...
Dialogue: 0,0:07:48.34,0:07:51.27,Default,,0000,0000,0000,,Ngày mai chúng ta sẽ gặp bố mẹ vợ của con.
Dialogue: 0,0:07:51.42,0:07:55.05,Default,,0000,0000,0000,,Có nhiều thứ cần chuẩn bị, vì vậy chúng ta phải ra ngoài một chút.
Dialogue: 0,0:07:55.23,0:07:55.92,Default,,0000,0000,0000,,Vâng ạ...
Dialogue: 0,0:07:56.10,0:07:58.19,Default,,0000,0000,0000,,Ngày mai con ko được phép đến muộn đâu đấy.
Dialogue: 0,0:07:58.37,0:08:01.64,Default,,0000,0000,0000,,Đây là lần đầu tiên chúng ta gặp mặt cha mẹ của Tomoko.
Dialogue: 0,0:08:01.88,0:08:03.57,Default,,0000,0000,0000,,Con biết rồi ạ...
Dialogue: 0,0:08:04.05,0:08:06.23,Default,,0000,0000,0000,,Này, Misaki đâu rồi nhỉ? Nó về phòng rồi sao?
Dialogue: 0,0:08:06.53,0:08:10.43,Default,,0000,0000,0000,,Dạo này Misaki quanh quẩn ở cửa hàng cả ngày với cái điện thoại trên tay.
Dialogue: 0,0:08:10.67,0:08:14.36,Default,,0000,0000,0000,,Có lẽ con bé đã có bạn trai, thấy nó dùng điện thoại ở cửa hàng suốt.
Dialogue: 0,0:08:14.85,0:08:18.11,Default,,0000,0000,0000,,Bạn trai nó là ai? Sống ở đâu?
Dialogue: 0,0:08:18.53,0:08:23.19,Default,,0000,0000,0000,,Làm gì mà ông hỏi han kĩ thế? Đây là lần đầu con bé có bạn trai mà.
Dialogue: 0,0:08:27.12,0:08:31.68,Default,,0000,0000,0000,,Huh? Đi chơi ư? Ngày mai mình có việc bận mất rồi.
Dialogue: 0,0:08:46.32,0:08:49.61,Default,,0000,0000,0000,,Xin lỗi cậu , tuần sau ta gặp nhau nhé.
Dialogue: 0,0:09:01.37,0:09:05.54,Default,,0000,0000,0000,,Anh khuyên em nên kiếm bạn trai, vì vậy em...
Dialogue: 0,0:09:26.85,0:09:32.63,Default,,0000,0000,0000,,Em gái của anh có vẻ ko vui với chuyện anh sắp kết hôn?
Dialogue: 0,0:09:32.84,0:09:36.86,Default,,0000,0000,0000,,Cô ấy trẻ và xinh đẹp hơn em tưởng nhiều...
Dialogue: 0,0:09:39.34,0:09:41.27,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:09:42.15,0:09:43.39,Default,,0000,0000,0000,,Hình như..
Dialogue: 0,0:09:43.54,0:09:46.50,Default,,0000,0000,0000,,Cô ấy ko thích em lắm thì phải?
Dialogue: 0,0:09:47.77,0:09:52.31,Default,,0000,0000,0000,,Mặc kệ nó đi, em có thể dời ngày cưới lên sớm hơn được ko?
Dialogue: 0,0:09:52.79,0:09:56.66,Default,,0000,0000,0000,,Anh muốn... Anh muốn chúng ta kết hôn càng sớm càng tốt.
Dialogue: 0,0:10:01.62,0:10:04.94,Default,,0000,0000,0000,,Cậu ấy để ý em cũng khá lâu rồi.
Dialogue: 0,0:10:08.21,0:10:10.84,Default,,0000,0000,0000,,Cậu ấy là thành viên của đội bóng đá nam trường em.
Dialogue: 0,0:10:10.99,0:10:13.13,Default,,0000,0000,0000,,Các thành viên đội bóng nam của trường được phái nữ vô cùng ái mộ.
Dialogue: 0,0:10:13.65,0:10:16.73,Default,,0000,0000,0000,,Vậy ra một trong những gã khó chịu anh gặp chủ nhật tuần trước là bạn trai của em à?
Dialogue: 0,0:10:17.21,0:10:19.96,Default,,0000,0000,0000,,Những gã ồn ào và thích khoe mẽ!
Dialogue: 0,0:10:20.72,0:10:23.71,Default,,0000,0000,0000,,Anh yêu chị ấy thật lòng ư, Onii-chan?
Dialogue: 0,0:10:26.95,0:10:31.33,Default,,0000,0000,0000,,Anh là lí do duy nhất khiến Misaki tồn tại trên cõi đời này, vì vậy em ko thể yêu bất kì người đàn ông nào khác.
Dialogue: 0,0:10:31.69,0:10:35.84,Default,,0000,0000,0000,,Anh có tin ko? Misaki được sinh ra trên cõi đời này là vì Thượng đế thấy anh quá cô đơn.
Dialogue: 0,0:10:35.99,0:10:38.92,Default,,0000,0000,0000,,Misaki được sinh ra để dành cho anh.
Dialogue: 0,0:10:39.70,0:10:41.19,Default,,0000,0000,0000,,Đừng nói những điều lạ lùng như vậy.
Dialogue: 0,0:10:41.37,0:10:45.18,Default,,0000,0000,0000,,Ngày mai anh sẽ cầu hôn chị ấy, phải ko?
Dialogue: 0,0:10:45.45,0:10:48.11,Default,,0000,0000,0000,,Có cách nào giúp Misaki ngăn chuyện đó lại ko.
Dialogue: 0,0:10:48.20,0:10:49.65,Default,,0000,0000,0000,,Misaki...
Dialogue: 0,0:11:01.98,0:11:04.22,Default,,0000,0000,0000,,Ko đâu, em đùa đấy.
Dialogue: 0,0:11:08.00,0:11:13.11,Default,,0000,0000,0000,,Misaki ko hề muốn gây rắc rối cho anh, Onii-chan.
Dialogue: 0,0:11:15.10,0:11:20.06,Default,,0000,0000,0000,,Cả nhà đều ngủ hết rồi.
Dialogue: 0,0:11:25.65,0:11:28.43,Default,,0000,0000,0000,,Khoan... Misa...
Dialogue: 0,0:12:35.75,0:12:38.60,Default,,0000,0000,0000,,Thằng nhỏ của anh cứng lên rồi nè...
Dialogue: 0,0:12:48.00,0:12:49.93,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:13:29.83,0:13:33.37,Default,,0000,0000,0000,,Onii-chan... sướng ko anh?
Dialogue: 0,0:14:03.84,0:14:06.50,Default,,0000,0000,0000,,Onii-chan, đây là...
Dialogue: 0,0:15:04.45,0:15:06.45,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:15:10.07,0:15:12.22,Default,,0000,0000,0000,,Đừng, Onii-chan!!
Dialogue: 0,0:15:12.46,0:15:14.97,Default,,0000,0000,0000,,Xấu hổ quá!
Dialogue: 0,0:15:19.87,0:15:21.62,Default,,0000,0000,0000,,Nó có màu hồng thật đẹp.
Dialogue: 0,0:15:21.77,0:15:23.68,Default,,0000,0000,0000,,Đừng, Onii-chan!!
Dialogue: 0,0:15:23.83,0:15:25.55,Default,,0000,0000,0000,,Như vậy xấu hổ lắm!
Dialogue: 0,0:15:25.76,0:15:27.27,Default,,0000,0000,0000,,Đừng nhìn mà!
Dialogue: 0,0:15:27.46,0:15:29.45,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:15:51.07,0:15:52.88,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:16:03.49,0:16:05.30,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:16:06.09,0:16:09.66,Default,,0000,0000,0000,,Em làm nó trào hết ra ngoài rồi!
Dialogue: 0,0:16:12.68,0:16:15.31,Default,,0000,0000,0000,,Để em lau sạch nó dùm anh.
Dialogue: 0,0:16:29.12,0:16:32.27,Default,,0000,0000,0000,,Em đừng... rên to như vậy.
Dialogue: 0,0:16:32.39,0:16:33.51,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:16:33.75,0:16:34.90,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:16:35.11,0:16:38.04,Default,,0000,0000,0000,,Anh định nhét nó vào như vậy sao?
Dialogue: 0,0:16:38.22,0:16:42.67,Default,,0000,0000,0000,,Anh sẽ nhét nó vào trong Misaki sao?
Dialogue: 0,0:16:52.10,0:16:53.76,Default,,0000,0000,0000,,Onii-ch...
Dialogue: 0,0:17:02.41,0:17:06.58,Default,,0000,0000,0000,,Của anh... đang nằm trọn... bên trong em.
Dialogue: 0,0:17:29.64,0:17:31.70,Default,,0000,0000,0000,,Ko, đừng nhét nó vào hậu môn của em!
Dialogue: 0,0:17:31.76,0:17:32.94,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:17:34.54,0:17:37.23,Default,,0000,0000,0000,,Đừng... Em ra mất... Em...
Dialogue: 0,0:17:38.20,0:17:39.95,Default,,0000,0000,0000,,Em ra mất!
Dialogue: 0,0:18:12.78,0:18:15.47,Default,,0000,0000,0000,,Đừng... chơi ngực của em như vậy.
Dialogue: 0,0:18:16.07,0:18:18.07,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:18:19.46,0:18:23.00,Default,,0000,0000,0000,,Nó vào... sâu bên trong em.
Dialogue: 0,0:18:27.89,0:18:29.50,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:18:30.40,0:18:33.46,Default,,0000,0000,0000,,Onii-chan, đừng làm mạnh như vậy...
Dialogue: 0,0:18:35.45,0:18:36.93,Default,,0000,0000,0000,,Em...
Dialogue: 0,0:18:38.63,0:18:40.59,Default,,0000,0000,0000,,Tan chảy ra mất!
Dialogue: 0,0:18:46.85,0:18:48.39,Default,,0000,0000,0000,,Ngừng lại đi mà!
Dialogue: 0,0:18:48.54,0:18:50.72,Default,,0000,0000,0000,,Em ra mất! Em sắp ra rồi!
Dialogue: 0,0:18:50.81,0:18:53.86,Default,,0000,0000,0000,,Em thích ko hả?
Dialogue: 0,0:18:58.30,0:19:00.39,Default,,0000,0000,0000,,Thích lắm!
Dialogue: 0,0:19:05.83,0:19:08.46,Default,,0000,0000,0000,,Em ra mất! Tuyệt quá!
Dialogue: 0,0:19:08.55,0:19:11.39,Default,,0000,0000,0000,,Em thích chuyên này như vậy sao?
Dialogue: 0,0:19:11.48,0:19:14.45,Default,,0000,0000,0000,,Vâng! Em yêu cái đó của anh!
Dialogue: 0,0:19:14.78,0:19:18.29,Default,,0000,0000,0000,,Em yêu cả những động tác dũng mãnh của anh!
Dialogue: 0,0:19:18.38,0:19:21.88,Default,,0000,0000,0000,,Onii-chan! Onii-chan!
Dialogue: 0,0:19:22.79,0:19:24.76,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:19:25.27,0:19:28.29,Default,,0000,0000,0000,,Em yêu anh! Em yêu anh, em yêu anh vô cùng!
Dialogue: 0,0:19:28.81,0:19:29.71,Default,,0000,0000,0000,,Vô cùng!
Dialogue: 0,0:19:31.07,0:19:32.83,Default,,0000,0000,0000,,Này, còn anh thì sao?
Dialogue: 0,0:19:32.98,0:19:34.64,Default,,0000,0000,0000,,Nói cho em biết đi!
Dialogue: 0,0:19:37.09,0:19:41.65,Default,,0000,0000,0000,,Tuyệt quá... Thằng nhỏ của anh tuyệt quá đi mất!
Dialogue: 0,0:19:43.65,0:19:45.70,Default,,0000,0000,0000,,Tuyệt quá. Nữa đi anh.
Dialogue: 0,0:19:45.70,0:19:46.76,Default,,0000,0000,0000,,Anh sắp ra rồi!
Dialogue: 0,0:19:46.85,0:19:48.55,Default,,0000,0000,0000,,Anh sẽ ra vào bên trong em.
Dialogue: 0,0:19:48.64,0:19:49.72,Default,,0000,0000,0000,,Ra đi anh!
Dialogue: 0,0:19:49.78,0:19:54.50,Default,,0000,0000,0000,,Ra hết tất cả đi anh. Ra hết tất cả vào bên trong em!
Dialogue: 0,0:19:55.74,0:19:58.37,Default,,0000,0000,0000,,Misaki cũng sắp ra rồi!
Dialogue: 0,0:20:03.81,0:20:06.77,Default,,0000,0000,0000,,Misaki sẽ ko ra cho đến lúc anh ra!
Dialogue: 0,0:20:06.99,0:20:10.95,Default,,0000,0000,0000,,Onii-chan! Onii-chan! Onii-chan!!
Dialogue: 0,0:20:16.36,0:20:19.80,Default,,0000,0000,0000,,Onii-chan... Em yêu anh vô cùng...
Dialogue: 0,0:20:20.44,0:20:22.67,Default,,0000,0000,0000,,Keisuke? Con dậy chưa vậy?
Dialogue: 0,0:20:22.92,0:20:27.27,Default,,0000,0000,0000,,Chúng ta đi trước đây. Con phải có mặt ở đó trước 3:30, rõ chưa?
Dialogue: 0,0:20:27.45,0:20:30.59,Default,,0000,0000,0000,,Ko được phép đến muộn đâu đấy, con nghe rõ chứ?
Dialogue: 0,0:20:30.90,0:20:33.04,Default,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:20:51.66,0:20:54.23,Default,,0000,0000,0000,,Onii-chan... nữa đi
Dialogue: 0,0:21:12.88,0:21:15.18,Default,,0000,0000,0000,,Thôi nào, hãy còn sớm mà anh...
Dialogue: 0,0:21:15.30,0:21:17.48,Default,,0000,0000,0000,,Nhưng mà, anh cần phải đến sớm...
Dialogue: 0,0:21:31.29,0:21:33.38,Default,,0000,0000,0000,,Misaki...
Dialogue: 0,0:21:35.56,0:21:38.13,Default,,0000,0000,0000,,Đây là lúc... mình phải...
Dialogue: 0,0:21:42.45,0:21:45.26,Default,,0000,0000,0000,,Lại nữa rồi. Anh luôn như vậy.
Dialogue: 0,0:21:45.62,0:21:48.25,Default,,0000,0000,0000,,Và Misaki cũng giống anh.
Dialogue: 0,0:21:51.49,0:21:53.27,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:21:56.32,0:21:59.26,Default,,0000,0000,0000,,Onii-chan! Tuyệt quá đi mất.
Dialogue: 0,0:22:03.07,0:22:05.03,Default,,0000,0000,0000,,Mình ko thể đến trễ được...
Dialogue: 0,0:22:05.21,0:22:07.93,Default,,0000,0000,0000,,Mình làm chuyện này cả tiếng đồng hồ rồi.
Dialogue: 0,0:22:08.26,0:22:10.20,Default,,0000,0000,0000,,Mình phải rời khỏi đây ngay lập tức...
Dialogue: 0,0:22:10.41,0:22:13.16,Default,,0000,0000,0000,,Phải rời khỏi nhà trước 2:30 may ra mới kịp...
Dialogue: 0,0:22:15.31,0:22:17.88,Default,,0000,0000,0000,,M-Misaki, đủ rồi-
Dialogue: 0,0:22:18.00,0:22:20.57,Default,,0000,0000,0000,,Onii-chan, anh sao vậy?
Dialogue: 0,0:22:23.02,0:22:25.22,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:22:26.83,0:22:30.72,Default,,0000,0000,0000,,Tuyệt quá... Em mệt lắm rồi!
Dialogue: 0,0:22:30.85,0:22:34.26,Default,,0000,0000,0000,,Âm hộ của Misaki sắp ko chịu nổi nữa rồi!
Dialogue: 0,0:22:36.20,0:22:38.31,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:22:39.10,0:22:40.91,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:22:42.64,0:22:45.93,Default,,0000,0000,0000,,Nốt lần này thôi nhé... anh kiệt sức rồi.
Dialogue: 0,0:22:58.32,0:23:00.29,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:23:02.83,0:23:04.91,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:23:08.15,0:23:09.84,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:23:11.75,0:23:13.29,Default,,0000,0000,0000,,Này...!
Dialogue: 0,0:23:16.10,0:23:18.82,Default,,0000,0000,0000,,Anh định bỏ em lại đây sao?
Dialogue: 0,0:23:26.65,0:23:28.28,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:23:31.85,0:23:35.60,Default,,0000,0000,0000,,Nghe nói cha Tomoko là người rất qui củ.
Dialogue: 0,0:23:36.02,0:23:38.17,Default,,0000,0000,0000,,Mình ko thể làm mất lòng ông ấy được.
Dialogue: 0,0:23:38.65,0:23:40.13,Default,,0000,0000,0000,,Phải khẩn trương lên thôi...
Dialogue: 0,0:23:40.34,0:23:43.70,Default,,0000,0000,0000,,Cần phải gây ấn tượng tốt với ông ấy.
Dialogue: 0,0:23:48.78,0:23:53.22,Default,,0000,0000,0000,,Onii-chan, anh chắc là mình muốn tới đó chứ?
Dialogue: 0,0:23:54.58,0:23:56.30,Default,,0000,0000,0000,,Tất nhiên rồi, anh...
Dialogue: 0,0:23:56.55,0:24:00.99,Default,,0000,0000,0000,,Nếu anh tới đó, mọi chuyện sẽ đúng như sắp đặt của cha mẹ...
Dialogue: 0,0:24:01.26,0:24:03.32,Default,,0000,0000,0000,,Và tháng tới anh sẽ buộc phải kết hôn...
Dialogue: 0,0:24:04.56,0:24:07.73,Default,,0000,0000,0000,,Em ko muốn anh đi chút nào... Misaki ko hề muốn điều đó!
Dialogue: 0,0:24:07.94,0:24:10.51,Default,,0000,0000,0000,,Onii-chan... Này, Onii-chan!
Dialogue: 0,0:24:10.63,0:24:13.02,Default,,0000,0000,0000,,Đủ rồi! Anh phải đi ngay bây giờ!
Dialogue: 0,0:24:13.14,0:24:15.98,Default,,0000,0000,0000,,Em xin lỗi, em chỉ muốn cho anh biết suy nghĩ của mình...
Dialogue: 0,0:24:16.19,0:24:19.43,Default,,0000,0000,0000,,Em biết anh ko muốn tới đó chút nào.
Dialogue: 0,0:24:21.64,0:24:25.75,Default,,0000,0000,0000,,Misaki muốn được ở bên anh mãi mãi.
Dialogue: 0,0:24:26.56,0:24:29.37,Default,,0000,0000,0000,,Misaki muốn trở thành vợ của anh!
Dialogue: 0,0:24:29.56,0:24:31.19,Default,,0000,0000,0000,,Chúng ta ko thể. Ko có cách nào...
Dialogue: 0,0:24:31.31,0:24:32.58,Default,,0000,0000,0000,,Thật vậy sao?
Dialogue: 0,0:24:32.79,0:24:37.57,Default,,0000,0000,0000,,Em chưa bao giờ coi mình là thành viên của gia đình này. Sự chênh lệch tuổi tác ko phải vấn đề lớn.
Dialogue: 0,0:24:37.90,0:24:41.89,Default,,0000,0000,0000,,Vì vậy, nếu chúng ta thực sự quyết tâm...
Dialogue: 0,0:24:44.04,0:24:45.91,Default,,0000,0000,0000,,Chủ nhân số máy này hiện đang vắng nhà.
Dialogue: 0,0:24:46.12,0:24:50.56,Default,,0000,0000,0000,,Làm ơn để lại tên và lời nhắn sau tiếng chuông.
Dialogue: 0,0:24:50.78,0:24:53.56,Default,,0000,0000,0000,,Xin chào? Mẹ đây. Misaki, con có ở đó ko?
Dialogue: 0,0:24:53.77,0:24:57.46,Default,,0000,0000,0000,,Keisuke đã rời khỏi nhà chưa? Nếu nó chưa thì nhắc nó khẩn trương lên!
Dialogue: 0,0:24:58.52,0:25:01.90,Default,,0000,0000,0000,,Em tin anh sẽ mang lại hạnh phúc cho em...
Dialogue: 0,0:25:07.74,0:25:09.46,Default,,0000,0000,0000,,Em yêu anh...
Dialogue: 0,0:25:10.40,0:25:12.00,Default,,0000,0000,0000,,Mình phải đi ngay bây giờ.
Dialogue: 0,0:25:12.33,0:25:14.14,Default,,0000,0000,0000,,Mọi người đang đợi mình.
Dialogue: 0,0:25:15.20,0:25:17.29,Default,,0000,0000,0000,,Chúng ta ko thể tiếp tục như vậy được.
Dialogue: 0,0:25:19.04,0:25:20.28,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:25:20.43,0:25:21.58,Default,,0000,0000,0050,,Dừng lại!
Dialogue: 0,0:25:20.46,0:25:23.27,Default,,0000,0000,0000,,Đừng bỏ em, Onii-chan!
Dialogue: 0,0:25:21.55,0:25:23.51,Default,,0000,0000,0000,,Đừng cản anh!
Dialogue: 0,0:25:24.33,0:25:25.84,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:25:28.38,0:25:30.23,Default,,0000,0000,0000,,Em xin lỗi...
Dialogue: 0,0:25:34.85,0:25:37.60,Default,,0000,0000,0000,,Xin chào!? Keisuke!? Con có ở đó ko vậy!?
Dialogue: 0,0:25:37.69,0:25:39.39,Default,,0000,0000,0000,,Sắp tới giờ rồi!
Dialogue: 0,0:25:39.48,0:25:41.77,Default,,0000,0000,0000,,Này! Keisuke!!
Dialogue: 0,0:25:43.41,0:25:46.79,Default,,0000,0000,0000,,Nhưng mà, em yêu anh vô cùng, Onii-chan...
Dialogue: 0,0:25:48.51,0:25:49.87,Default,,0000,0000,0000,,Tha lỗi cho em...
Dialogue: 0,0:25:51.36,0:25:52.63,Default,,0000,0000,0000,,Tha lỗi cho em...
Dialogue: 0,0:25:54.41,0:25:56.92,Default,,0000,0000,0000,,Tha lỗi cho em...
Dialogue: 0,0:25:58.88,0:26:01.48,Default,,0000,0000,0000,,Onii-chan...
