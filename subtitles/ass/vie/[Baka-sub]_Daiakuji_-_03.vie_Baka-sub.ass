Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:11.92,0:00:13.44,Default,,0000,0000,0000,,Gã này khó chơi đấy.
Dialogue: 0,0:00:13.86,0:00:15.49,Default,,0000,0000,0000,,Ta sẽ cho ngươi thấy sức mạnh thật sự là như thế nào!
Dialogue: 0,0:00:20.02,0:00:21.05,Default,,0000,0000,0000,,Hắn mạnh lắm đấy.
Dialogue: 0,0:00:21.05,0:00:21.77,Default,,0000,0000,0000,,Tao biết
Dialogue: 0,0:00:29.67,0:00:31.01,Default,,0000,0000,0000,,Tha lỗi cho ta vì quá đường đột.
Dialogue: 0,0:00:31.99,0:00:33.74,Default,,0000,0000,0000,,Trò đó chả có gì đáng sợ cả.
Dialogue: 0,0:00:55.64,0:01:01.14,Default,,0000,0000,0000,,DAIAKUJI 3
Dialogue: 0,0:03:58.29,0:04:00.69,Default,,0000,0000,0000,,Đây là máu của thánh... Nako
Dialogue: 0,0:04:19.01,0:04:24.02,Default,,0000,0000,0000,,Tình trạng cậu bé ko được ổn cho lắm, giờ hãy xem sức mạnh của những giọt máu này.
Dialogue: 0,0:04:24.02,0:04:26.29,Default,,0000,0000,0000,,Thật kì diệu! Đúng là phép màu!
Dialogue: 0,0:04:33.21,0:04:35.52,Default,,0000,0000,0000,,Quần áo của người đây ạ.
Dialogue: 0,0:04:35.94,0:04:37.63,Default,,0000,0000,0000,,Cảm ơn, Haruka-san.
Dialogue: 0,0:04:37.98,0:04:43.70,Default,,0000,0000,0000,,Nako-sama , Xin người đừng gọi tên tôi một cách thân mật như vậy. Việc này liên quan đến sự tôn nghiêm của người.
Dialogue: 0,0:04:44.27,0:04:46.45,Default,,0000,0000,0000,,Xin người thứ lỗi.
Dialogue: 0,0:04:55.54,0:04:57.55,Default,,0000,0000,0000,,Xong rồi đây, Nako-sama.
Dialogue: 0,0:04:57.89,0:05:02.93,Default,,0000,0000,0000,,Yoko-san, ko có ai ở đây cả, làm ơn đừng gọi tôi như vậy.
Dialogue: 0,0:05:03.66,0:05:05.94,Default,,0000,0000,0000,,Vâng, tha lỗi cho tôi Yume.
Dialogue: 0,0:05:13.41,0:05:15.09,Default,,0000,0000,0000,,Tình hình Osaka thế nào rồi?
Dialogue: 0,0:05:15.70,0:05:17.65,Default,,0000,0000,0000,,Akuji đã thôn tính Wakame-gumi.
Dialogue: 0,0:05:18.32,0:05:22.11,Default,,0000,0000,0000,,Đánh bại thống lĩnh Nogi của khu Ashiya, và việc này vẫn đang tiếp diễn.
Dialogue: 0,0:05:22.43,0:05:23.96,Default,,0000,0000,0000,,Nếu hắn tiếp tục, mục tiêu tiếp theo sẽ là...
Dialogue: 0,0:05:24.43,0:05:29.63,Default,,0000,0000,0000,,Tôi sẽ gửi yêu cầu nghị hòa tới Akuji.
Dialogue: 0,0:05:29.63,0:05:33.14,Default,,0000,0000,0000,,Chuyện này ko thể được. Cô là vị thánh của Nako.
Dialogue: 0,0:05:33.14,0:05:36.87,Default,,0000,0000,0000,,Ko đúng! Cô biết rõ điều này hơn ai hết mà, Yoko-san.
Dialogue: 0,0:05:38.23,0:05:43.11,Default,,0000,0000,0000,,Tôi chẳng phải thần thánh gì cả. Tôi là một con quái vật, nửa đàn ông nửa phụ nữ.
Dialogue: 0,0:05:43.61,0:05:47.36,Default,,0000,0000,0000,,Tôi đã mất hết kí ức trước đây, và cô đã tìm thấy tôi ở bờ biển.
Dialogue: 0,0:05:47.55,0:05:50.92,Default,,0000,0000,0000,,Nhưng máu của cô có thể chữa bệnh cho rất nhiều người...
Dialogue: 0,0:05:51.31,0:05:55.91,Default,,0000,0000,0000,,Vả lại, có rất nhiều người tin tưởng ở cô và đã được cô cứu sống.
Dialogue: 0,0:06:00.32,0:06:04.33,Default,,0000,0000,0000,,Nhưng, điều này giống như tôi đang lừa gạt họ.
Dialogue: 0,0:06:05.12,0:06:05.78,Default,,0000,0000,0000,,Yoko-san
Dialogue: 0,0:06:06.60,0:06:09.51,Default,,0000,0000,0000,,Tôi hiểu mà. Hãy tin ở tôi.
Dialogue: 0,0:06:09.87,0:06:10.89,Default,,0000,0000,0000,,Cám ơn cô
Dialogue: 0,0:06:20.61,0:06:23.69,Default,,0000,0000,0000,,Bây giờ, là phát biểu của thủ lĩnh.
Dialogue: 0,0:06:30.96,0:06:32.33,Default,,0000,0000,0000,,Sat-chan thật dễ thương.
Dialogue: 0,0:06:32.33,0:06:34.40,Default,,0000,0000,0000,,Cô ấy trong chả giống một thủ lĩnh tẹo nào cả.
Dialogue: 0,0:06:34.55,0:06:37.85,Default,,0000,0000,0000,,Tôi là Takeda Satsu. Vào lúc này, tôi muốn nói vài lời với tất cả các hội viên ở đây.
Dialogue: 0,0:06:38.34,0:06:43.10,Default,,0000,0000,0000,,Tất cả mọi người cần luôn chuẩn bị tâm lý ko còn gì để mất, hãy sẵn sàng hi sinh tính mạng của mình.
Dialogue: 0,0:06:43.62,0:06:46.46,Default,,0000,0000,0000,,Nếu ko, chúng ta sẽ ko bao giờ có thể hoàn thành việc thôn tính Osaka.
Dialogue: 0,0:06:46.46,0:06:48.17,Default,,0000,0000,0000,,Cô đừng dọa tôi chứ!
Dialogue: 0,0:06:48.17,0:06:50.23,Default,,0000,0000,0000,,Cô hi sinh luôn tại đây để làm gương cho mọi người được đấy.
Dialogue: 0,0:06:51.22,0:06:52.40,Default,,0000,0000,0000,,Tôi xin rút lại những lời vừa nói...
Dialogue: 0,0:06:52.69,0:06:54.89,Default,,0000,0000,0000,,Cô ấy dứt khoát là dì của Akuji.
Dialogue: 0,0:06:55.55,0:06:56.68,Default,,0000,0000,0000,,Tôi sẽ ra ngoài một chút đây.
Dialogue: 0,0:06:57.11,0:06:57.82,Default,,0000,0000,0000,,Đi đâu vậy?
Dialogue: 0,0:06:58.14,0:07:00.22,Default,,0000,0000,0000,,Đi dạo một chút, với cô ta.
Dialogue: 0,0:07:06.96,0:07:08.60,Default,,0000,0000,0000,,Tamanegi đã rất ấn tượng về cô.
Dialogue: 0,0:07:09.07,0:07:13.59,Default,,0000,0000,0000,,Ko phụ nữ nào có thể cưỡng lại ông ta trong suốt 10 năm, cho tới khi cô tới, Kikko.
Dialogue: 0,0:07:13.83,0:07:15.36,Default,,0000,0000,0000,,Đừng gọi tôi theo lối xách mé như vậy.
Dialogue: 0,0:07:15.61,0:07:20.48,Default,,0000,0000,0000,,Tên tôi là Nogi Kikuko, cháu gái của thủ lĩnh Nogi vĩ đại.
Dialogue: 0,0:07:20.83,0:07:21.99,Default,,0000,0000,0000,,Với hạng người như anh...
Dialogue: 0,0:07:32.16,0:07:34.31,Default,,0000,0000,0000,,Dù có xảy ra chuyện gì đi chăng nữa, tôi sẽ ko bao giờ đầu hàng.
Dialogue: 0,0:07:34.63,0:07:36.53,Default,,0000,0000,0000,,Kể cả khi Ashiya có ra sao đi chăng nữa.
Dialogue: 0,0:07:39.02,0:07:41.93,Default,,0000,0000,0000,,Chuyện gì thế nhỉ? Tôi cũng ko biết chuyện gì xảy ra với cô ta cả?
Dialogue: 0,0:07:42.55,0:07:45.16,Default,,0000,0000,0000,,Sao lại buộc dây cô ta nhỉ? Shh, đừng có nhìn. Trông thật bệnh hoạn.
Dialogue: 0,0:07:51.68,0:07:56.91,Default,,0000,0000,0000,,Này này! Nếu cô ko bước nhanh lên, cô sẽ tạo ra một vũng nước trên đường mất.
Dialogue: 0,0:08:00.63,0:08:04.58,Default,,0000,0000,0000,,Đừng có lạnh nhạt như thế. Chẳng phải cô đang chờ để được giải quyết sao?
Dialogue: 0,0:08:04.93,0:08:09.29,Default,,0000,0000,0000,,Cảm giác của việc đi tè sau khi đã vượt qua giới hạn bình thường.
Dialogue: 0,0:08:09.38,0:08:11.02,Default,,0000,0000,0000,,Tôi sẽ lo việc khảo sát ở đây.
Dialogue: 0,0:08:11.78,0:08:17.70,Default,,0000,0000,0000,,Đã một tuần từ khi cô tới đây, cô đã được huấn luyện bởi tôi và Tamanegi,
Dialogue: 0,0:08:18.30,0:08:22.05,Default,,0000,0000,0000,,Chắc hẳn sẽ rất thú vị khi khảo sát xem công sức huấn luyện của tôi sẽ có kết quả thế nào.
Dialogue: 0,0:08:22.61,0:08:23.87,Default,,0000,0000,0000,,Ai mà thèm...
Dialogue: 0,0:08:25.61,0:08:26.69,Default,,0000,0000,0000,,Chúng ta sẽ làm chuyện đó ở đây.
Dialogue: 0,0:08:29.58,0:08:31.74,Default,,0000,0000,0000,,Ở đây ư? Anh đùa đấy à...
Dialogue: 0,0:08:31.74,0:08:37.49,Default,,0000,0000,0000,,Có gì ngạc nhiên đâu, đây chỉ là một căn nhà. Có điều tôi ko biết nó thuộc về ai cả.
Dialogue: 0,0:08:42.76,0:08:43.82,Default,,0000,0000,0000,,Mạng sống của Akuji...
Dialogue: 0,0:08:44.39,0:08:48.46,Default,,0000,0000,0000,,Giáo phái Nako cần phải bảo tồn vị thế của mình ở Osaka.
Dialogue: 0,0:08:49.08,0:08:52.44,Default,,0000,0000,0000,,Chúng ta ko cho phép Akuji, kẻ bầy tôi của ác quỉ, sống sót.
Dialogue: 0,0:08:52.82,0:08:55.29,Default,,0000,0000,0000,,Đây là lệnh của Nako-sama.
Dialogue: 0,0:08:55.80,0:08:56.78,Default,,0000,0000,0000,,Nếu Nako-sama đã ra lệnh như vậy...
Dialogue: 0,0:08:57.23,0:08:59.11,Default,,0000,0000,0000,,Nene-sama, Nene-sama!
Dialogue: 0,0:08:59.36,0:09:00.94,Default,,0000,0000,0000,,Ta tin tưởng ở cô.
Dialogue: 0,0:09:01.18,0:09:01.51,Default,,0000,0000,0000,,Vâng thưa chủ nhân
Dialogue: 0,0:09:05.84,0:09:07.94,Default,,0000,0000,0000,,Nene-sama, Nene-sama
Dialogue: 0,0:09:09.55,0:09:11.93,Default,,0000,0000,0000,,Nene-sama, cô ở đây à.
Dialogue: 0,0:09:12.21,0:09:15.60,Default,,0000,0000,0000,,Tới đúng lúc đấy Yoko. Tôi có thể nói chuyện với cô một lúc ko?
Dialogue: 0,0:09:16.26,0:09:21.35,Default,,0000,0000,0000,,Thật như thế, tôi đã nghĩ tới việc gửi một sứ giả đến để nghị hòa với Akuji.
Dialogue: 0,0:09:21.37,0:09:22.48,Default,,0000,0000,0000,,Thật chứ?
Dialogue: 0,0:09:22.81,0:09:23.28,Default,,0000,0000,0000,,Quả thật là như vậy
Dialogue: 0,0:09:27.66,0:09:30.25,Default,,0000,0000,0000,,Ko có ai ở đây cả. Mọi việc có vẻ thuận lợi đấy nhỉ.
Dialogue: 0,0:09:30.80,0:09:33.31,Default,,0000,0000,0000,,Cô có thể gỡ những thứ gắn ở hai đầu vú và âm hộ xuống rồi đó.
Dialogue: 0,0:09:33.59,0:09:35.94,Default,,0000,0000,0000,,Nhưng chỉ khi cô cởi hết y phục ra.
Dialogue: 0,0:09:36.01,0:09:37.00,Default,,0000,0000,0000,,Ai, ai thèm làm...
Dialogue: 0,0:09:39.98,0:09:43.14,Default,,0000,0000,0000,,Tôi có thể làm cho nó rung mạnh hơn nữa. Cô có làm hay ko nào?
Dialogue: 0,0:09:43.14,0:09:44.77,Default,,0000,0000,0000,,Anh là tên khốn!
Dialogue: 0,0:09:49.00,0:09:50.12,Default,,0000,0000,0000,,Thế này đã được chưa?
Dialogue: 0,0:09:50.54,0:09:52.27,Default,,0000,0000,0000,,Được rồi, cô có thể gỡ chúng xuống.
Dialogue: 0,0:10:08.99,0:10:12.09,Default,,0000,0000,0000,,Cảm thấy thế nào? Khi gỡ tất cả những thứ cô thường mang bên mình.
Dialogue: 0,0:10:12.65,0:10:17.13,Default,,0000,0000,0000,,Hãy thôi tỏ ra cứng đầu và cố thư giãn đi. Tôi sẽ làm cô sung sướng.
Dialogue: 0,0:10:17.13,0:10:17.89,Default,,0000,0000,0000,,Ko, tôi ko muốn điều đó.
Dialogue: 0,0:10:18.17,0:10:21.42,Default,,0000,0000,0000,,Thế nhưng chỗ này của cô lại nói khác đấy.
Dialogue: 0,0:10:21.42,0:10:24.34,Default,,0000,0000,0000,,Tôi ko phải một dâm nữ nhơ bẩn.
Dialogue: 0,0:10:24.34,0:10:26.98,Default,,0000,0000,0000,,Đương nhiên là cô ko phải rồi. Nhìn tôi này.
Dialogue: 0,0:10:28.21,0:10:30.18,Default,,0000,0000,0000,,Cô ko hề nhơ bẩn.
Dialogue: 0,0:10:30.76,0:10:31.89,Default,,0000,0000,0000,,Vì cô là dâm nữ của tôi.
Dialogue: 0,0:10:42.26,0:10:44.04,Default,,0000,0000,0000,,Cô có những đường cong hoàn hảo đấy chứ.
Dialogue: 0,0:10:45.91,0:10:47.36,Default,,0000,0000,0000,,Đừng nhìn tôi.
Dialogue: 0,0:10:50.48,0:10:53.30,Default,,0000,0000,0000,,Hey, xui thật! Xin lỗi vì đã làm phiền cô, chuyện này sẽ ko...
Dialogue: 0,0:10:58.65,0:10:59.11,Default,,0000,0000,0000,,Chết tiệt
Dialogue: 0,0:10:59.58,0:11:01.04,Default,,0000,0000,0000,,Cô là đứa quái nào?
Dialogue: 0,0:11:02.29,0:11:05.56,Default,,0000,0000,0000,,Ta là cận vệ của thánh Nako-sama, Toki Haruka.
Dialogue: 0,0:11:06.16,0:11:08.04,Default,,0000,0000,0000,,Ta tới đây để lấy mạng ngươi!
Dialogue: 0,0:11:17.55,0:11:18.28,Default,,0000,0000,0000,,Cơ may của mình đây rồi.
Dialogue: 0,0:11:19.27,0:11:22.09,Default,,0000,0000,0000,,Ghê thật! Một sát thủ có khuôn mặt dễ thương ư.
Dialogue: 0,0:11:23.53,0:11:25.95,Default,,0000,0000,0000,,Làm ơn hãy ban cho tôi sức mạnh, Nako-sama!
Dialogue: 0,0:11:27.28,0:11:27.75,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:11:36.44,0:11:37.72,Default,,0000,0000,0000,,May cho anh là tôi tới kịp đấy.
Dialogue: 0,0:11:37.93,0:11:39.64,Default,,0000,0000,0000,,Tôi đã bao giờ nói là cô nợ tôi một lần chưa?
Dialogue: 0,0:11:39.70,0:11:42.20,Default,,0000,0000,0000,,Anh đừng coi thường giáo phái Nako.
Dialogue: 0,0:11:42.59,0:11:48.84,Default,,0000,0000,0000,,Tôi chả ngán kẻ nào hết. Mặc kệ chúng là ai, tôi cũng sẽ thôn tính bằng được Osaka.
Dialogue: 0,0:11:50.08,0:11:51.16,Default,,0000,0000,0000,,Vết thương của cô thế nào rồi?
Dialogue: 0,0:11:51.79,0:11:53.33,Default,,0000,0000,0000,,Tôi ko cần anh lo lắng cho tôi.
Dialogue: 0,0:11:53.80,0:11:55.23,Default,,0000,0000,0000,,Cô thật cứng đầu.
Dialogue: 0,0:11:57.03,0:11:58.26,Default,,0000,0000,0000,,Cô cử động được rồi hả.
Dialogue: 0,0:11:59.41,0:12:00.66,Default,,0000,0000,0000,,Nako-sama...
Dialogue: 0,0:12:00.90,0:12:06.70,Default,,0000,0000,0000,,Quên thánh Nako-sama gì đó đi, bởi vì từ hôm nay, ta sẽ là vị thánh của cô.
Dialogue: 0,0:12:23.91,0:12:27.22,Default,,0000,0000,0000,,Mình đang nghĩ gì thế này? Mình đang cố bỏ trốn mà.
Dialogue: 0,0:12:57.45,0:12:58.48,Default,,0000,0000,0000,,Tỉnh rồi hả?
Dialogue: 0,0:13:01.82,0:13:03.55,Default,,0000,0000,0000,,Đừng nhìn ta với ánh mắt dữ tợn như thế.
Dialogue: 0,0:13:03.93,0:13:06.24,Default,,0000,0000,0000,,Cô chưa hề có kinh nghiệm với đàn ông, đúng ko?
Dialogue: 0,0:13:07.97,0:13:11.02,Default,,0000,0000,0000,,Đây chắc chắn là lần đầu tiên một người đàn ông ngắm cô khỏa thân, đúng chứ?
Dialogue: 0,0:13:12.21,0:13:13.48,Default,,0000,0000,0000,,Cô có thân hình tuyệt đấy.
Dialogue: 0,0:13:14.60,0:13:17.73,Default,,0000,0000,0000,,Hôm nay chính là cơ hội cho cô phô bày thân thể mình cho mọi người đấy!
Dialogue: 0,0:13:20.27,0:13:21.92,Default,,0000,0000,0000,,Đừng có kích động như vậy.
Dialogue: 0,0:13:27.86,0:13:29.69,Default,,0000,0000,0000,,Oh, cô quay lại rồi à.
Dialogue: 0,0:13:30.73,0:13:31.15,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:13:35.82,0:13:36.70,Default,,0000,0000,0000,,Đẹp quá...
Dialogue: 0,0:13:43.94,0:13:44.89,Default,,0000,0000,0000,,Yume!
Dialogue: 0,0:13:45.31,0:13:46.20,Default,,0000,0000,0000,,Chuyện gì thế?
Dialogue: 0,0:13:55.12,0:13:56.16,Default,,0000,0000,0000,,Yoko-san...
Dialogue: 0,0:14:18.36,0:14:20.01,Default,,0000,0000,0000,,Tại sao mình lại làm điều này chứ...?
Dialogue: 0,0:14:22.69,0:14:24.48,Default,,0000,0000,0000,,Komiya-sama! Komiya-sama!
Dialogue: 0,0:14:25.20,0:14:26.80,Default,,0000,0000,0000,,Haruka đã rở vào tay Akuji?
Dialogue: 0,0:14:27.08,0:14:31.02,Default,,0000,0000,0000,,Vâng, Tsukigase-sama đã tới để thương lượng với Akuji.
Dialogue: 0,0:14:31.35,0:14:32.08,Default,,0000,0000,0000,,Ko thể như vậy được.
Dialogue: 0,0:14:33.31,0:14:38.72,Default,,0000,0000,0000,,Và Yoko-sama cần phải ở đây để bảo vệ Nako-sama.
Dialogue: 0,0:14:42.78,0:14:43.26,Default,,0000,0000,0000,,Yume...
Dialogue: 0,0:14:49.51,0:14:50.18,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:14:52.90,0:14:53.98,Default,,0000,0000,0000,,Yoko-san
Dialogue: 0,0:15:03.82,0:15:09.23,Default,,0000,0000,0000,,Akuji đã từ chối yêu cầu nghị hòa, và con bắt giữ người của chúng ta.
Dialogue: 0,0:15:09.42,0:15:11.40,Default,,0000,0000,0000,,Điều này thật ko thể tha thứ!
Dialogue: 0,0:15:11.46,0:15:12.92,Default,,0000,0000,0000,,Đúng như vậy! Ko thể tha thứ được!
Dialogue: 0,0:15:12.92,0:15:14.00,Default,,0000,0000,0000,,Phá hủy Akuji-gumi! Tiêu diệt Akuji!
Dialogue: 0,0:15:14.00,0:15:19.41,Default,,0000,0000,0000,,Linh hồn của chúng ta là một. Chúng ta phải giải cứu anh em của mình, và tiêu diệt ác quỉ!
Dialogue: 0,0:15:21.72,0:15:23.12,Default,,0000,0000,0000,,Ko phải e sợ bất cứ điều gì cả.
Dialogue: 0,0:15:23.60,0:15:25.32,Default,,0000,0000,0000,,Nào mọi người, hãy cầm lấy vũ khí!
Dialogue: 0,0:15:37.28,0:15:38.95,Default,,0000,0000,0000,,Cô cảm thấy thế nào?
Dialogue: 0,0:15:39.70,0:15:43.58,Default,,0000,0000,0000,,Làm ơn buông tôi ra! Tôi có thể nhìn thấu tâm hồn cô cô, sức mạnh của Nako-sama sẽ...
Dialogue: 0,0:15:47.17,0:15:50.60,Default,,0000,0000,0000,,Tốt, tốt! Vậy là cô có hứng thú với tôi.
Dialogue: 0,0:15:51.15,0:15:52.05,Default,,0000,0000,0000,,Quan sát đi
Dialogue: 0,0:15:55.14,0:16:01.23,Default,,0000,0000,0000,,Trông rất đẹp phải ko? Một tuần trước đây, cô ta cũng cứng đầu hệt như cô.
Dialogue: 0,0:16:02.67,0:16:03.38,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:16:04.58,0:16:09.63,Default,,0000,0000,0000,,Tôi rất thích việc ngắm nhìn những cô gái cứng đầu như cô sa ngã bởi chính dục vọng của mình.
Dialogue: 0,0:16:09.63,0:16:12.82,Default,,0000,0000,0000,,Chỉ  nhìn thôi đã khiến chỗ đó của tôi ướt hết cả.
Dialogue: 0,0:16:13.23,0:16:15.49,Default,,0000,0000,0000,,Tôi sẽ ko bao giờ sa ngã bởi...
Dialogue: 0,0:16:18.54,0:16:20.32,Default,,0000,0000,0000,,Cô vẫn là trinh nữ đúng ko?
Dialogue: 0,0:16:21.66,0:16:24.51,Default,,0000,0000,0000,,Vậy chắc chắn trước đây cô chưa bao giờ dùng thứ này.
Dialogue: 0,0:16:30.68,0:16:33.27,Default,,0000,0000,0000,,Ko, cô phải nhìn về hướng này chứ.
Dialogue: 0,0:16:33.63,0:16:36.30,Default,,0000,0000,0000,,Koharu-san, tôi giao cô ta cho cô đấy.
Dialogue: 0,0:16:36.95,0:16:40.12,Default,,0000,0000,0000,,Ok, giờ nói câu đó lại một lần nữa xem nào.
Dialogue: 0,0:16:41.06,0:16:45.47,Default,,0000,0000,0000,,Vâng. Tôi sẽ ko bao giờ bỏ trốn nữa.
Dialogue: 0,0:16:45.47,0:16:48.32,Default,,0000,0000,0000,,Chủ nhân Akuji... Chủ nhân Akuji...
Dialogue: 0,0:16:55.61,0:16:56.94,Default,,0000,0000,0000,,Thành tâm hơn chút nữa xem nào.
Dialogue: 0,0:16:57.05,0:16:58.30,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:16:58.42,0:17:00.83,Default,,0000,0000,0000,,Tôi thề sẽ trung thành với chủ nhân Akuji.
Dialogue: 0,0:17:01.08,0:17:05.86,Default,,0000,0000,0000,,Làm ơn hãy sử dụng tôi mọi lúc ngài cần. Vì thế làm ơn...
Dialogue: 0,0:17:07.00,0:17:09.78,Default,,0000,0000,0000,,Tốt lắm. Cô đã nắm được rồi đó.
Dialogue: 0,0:17:11.23,0:17:14.39,Default,,0000,0000,0000,,Ta ghi nhận sự trung thành của cô.
Dialogue: 0,0:17:15.10,0:17:16.08,Default,,0000,0000,0000,,Tới đây nào.
Dialogue: 0,0:17:19.30,0:17:20.11,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:17:21.93,0:17:22.81,Default,,0000,0000,0000,,Ngửi nó đi.
Dialogue: 0,0:17:24.46,0:17:25.28,Default,,0000,0000,0000,,Nó có nặng mùi ko?
Dialogue: 0,0:17:25.96,0:17:26.92,Default,,0000,0000,0000,,Vâng, một chút ạ.
Dialogue: 0,0:17:27.99,0:17:28.83,Default,,0000,0000,0000,,Cô ko thích à?
Dialogue: 0,0:17:29.26,0:17:30.08,Default,,0000,0000,0000,,Ko ạ
Dialogue: 0,0:17:30.45,0:17:34.43,Default,,0000,0000,0000,,Đây chính là mùi của ta. Mùi hương của khoái lạc.
Dialogue: 0,0:17:34.82,0:17:35.31,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:17:48.70,0:17:50.37,Default,,0000,0000,0000,,Thấy thế nào? Điều này thật tuyệt vời phải ko?
Dialogue: 0,0:17:51.29,0:17:55.15,Default,,0000,0000,0000,,Phụ nữ được tạo ra để làm những chuyện như thế này.
Dialogue: 0,0:17:55.15,0:17:58.76,Default,,0000,0000,0000,,Sâu thẳm bên trong tâm hồn, chúng ta luôn luôn nghĩ tới điều đó.
Dialogue: 0,0:17:59.36,0:18:01.09,Default,,0000,0000,0000,,Chúng ta luôn luôn thèm muốn chuyện này.
Dialogue: 0,0:18:01.89,0:18:07.09,Default,,0000,0000,0000,,Điều đó đã làm nên phụ nữ, chứ ko phải là giới tính của cô. Hiểu chứ?
Dialogue: 0,0:18:07.50,0:18:11.96,Default,,0000,0000,0000,,Cô đang nghĩ tới chuyện đó, phải ko? .
Dialogue: 0,0:18:12.03,0:18:13.69,Default,,0000,0000,0000,,Ko, cô nhầm rồi!
Dialogue: 0,0:18:14.84,0:18:21.94,Default,,0000,0000,0000,,Đừng lo. Tôi sẽ giúp cô cảm nhận được điều này.
Dialogue: 0,0:18:26.60,0:18:28.90,Default,,0000,0000,0000,,Sau chuyện này, hãy nhìn lại chính bản thân mình.
Dialogue: 0,0:18:34.18,0:18:35.49,Default,,0000,0000,0000,,Tôi sẽ đâm vào sâu hơn!
Dialogue: 0,0:18:39.21,0:18:41.69,Default,,0000,0000,0000,,Tan ra! Tôi đang tan chảy ra!
Dialogue: 0,0:18:42.00,0:18:44.70,Default,,0000,0000,0000,,Tốt đấy. Hãy tan chảy cùng những cảm xúc mà cô chưa bao giờ cảm nhận được trước kia!
Dialogue: 0,0:18:45.17,0:18:45.78,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:19:02.66,0:19:05.39,Default,,0000,0000,0000,,Ở đây! Đúng rồi! Tuyệt quá!
Dialogue: 0,0:19:12.77,0:19:14.83,Default,,0000,0000,0000,,Sâu hơn nữa! Sâu hơn nữa!
Dialogue: 0,0:19:16.84,0:19:19.26,Default,,0000,0000,0000,,Nói rõ ràng xem nào. Cô muốn tôi làm gì?
Dialogue: 0,0:19:19.68,0:19:26.60,Default,,0000,0000,0000,,Vâng, chủ nhân Akuji. Làm ơn hãy ngắm nhìn tôi. Hãy ngắm nhìn tôi khi tôi ra.
Dialogue: 0,0:19:26.69,0:19:29.09,Default,,0000,0000,0000,,Nói hay lắm. Chúng ta tiếp tục nào!
Dialogue: 0,0:19:30.00,0:19:31.01,Default,,0000,0000,0000,,Chủ nhân Akuji!
Dialogue: 0,0:19:35.49,0:19:36.99,Default,,0000,0000,0000,,Tôi ra rồi, Tôi ra rồi!
Dialogue: 0,0:19:40.21,0:19:42.52,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:19:47.38,0:19:50.32,Default,,0000,0000,0000,,Được, được, hình như cô cũng ra rồi.
Dialogue: 0,0:19:51.20,0:19:54.40,Default,,0000,0000,0000,,Nhưng cô ko thể thỏa mãn chỉ với thứ đồ chơi này.
Dialogue: 0,0:19:55.14,0:19:56.12,Default,,0000,0000,0000,,Nhìn về phía trước đi
Dialogue: 0,0:19:58.54,0:19:59.77,Default,,0000,0000,0000,,Ta sẽ chơi cô ngay bây giờ.
Dialogue: 0,0:20:00.98,0:20:02.23,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:20:09.45,0:20:11.16,Default,,0000,0000,0000,,Đừng mất tự chủ như thế.
Dialogue: 0,0:20:11.32,0:20:12.47,Default,,0000,0000,0000,,Ko, dừng lại đi!
Dialogue: 0,0:20:12.77,0:20:13.98,Default,,0000,0000,0000,,Đau quá... Đau quá...
Dialogue: 0,0:20:14.36,0:20:15.55,Default,,0000,0000,0000,,Đau quá
Dialogue: 0,0:20:20.37,0:20:22.70,Default,,0000,0000,0000,,Sâu quá... oh tôi...
Dialogue: 0,0:20:23.62,0:20:24.70,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:20:38.98,0:20:44.05,Default,,0000,0000,0000,,Tôi ko chịu được nữa... nóng quá, rát quá...
Dialogue: 0,0:20:55.86,0:20:58.63,Default,,0000,0000,0000,,Sâu quá, sấu quá... Nóng quá...
Dialogue: 0,0:21:00.55,0:21:03.14,Default,,0000,0000,0000,,Cảm giác này thật tồi tệ...
Dialogue: 0,0:21:05.80,0:21:08.70,Default,,0000,0000,0000,,Ra rồi, ra rồi...Có thứ gì đó đang ra!
Dialogue: 0,0:21:19.38,0:21:23.75,Default,,0000,0000,0000,,Cô ta ra nhiều như vậy chỉ sau hai lần. Cô ta sẽ có giá rất cao đấy.
Dialogue: 0,0:21:37.20,0:21:39.25,Default,,0000,0000,0000,,Màu hồng tuyệt đẹp!
Dialogue: 0,0:21:39.50,0:21:41.82,Default,,0000,0000,0000,,Đất nước chúng tôi ko hề có màu lông này.
Dialogue: 0,0:21:42.12,0:21:45.13,Default,,0000,0000,0000,,Xấu hổ quá... Tôi ko muốn như thế này.
Dialogue: 0,0:21:45.38,0:21:46.65,Default,,0000,0000,0000,,Awww...
Dialogue: 0,0:21:46.75,0:21:47.54,Default,,0000,0000,0000,,Nozomi
Dialogue: 0,0:21:49.84,0:21:51.24,Default,,0000,0000,0000,,Vâng, thưa chủ nhân.
Dialogue: 0,0:21:52.09,0:21:53.24,Default,,0000,0000,0000,,Các vị cứ thỏa mái đi.
Dialogue: 0,0:21:53.33,0:21:54.89,Default,,0000,0000,0000,,Tuyệt vời!
Dialogue: 0,0:22:08.17,0:22:09.28,Default,,0000,0000,0000,,Ngon quá!
Dialogue: 0,0:22:11.49,0:22:13.40,Default,,0000,0000,0000,,Nhìn cặp mông cô ta này!
Dialogue: 0,0:22:14.12,0:22:17.05,Default,,0000,0000,0000,,Thật là nhẵn nhụi, giống như hai trái trứng luộc vậy!
Dialogue: 0,0:22:18.62,0:22:20.17,Default,,0000,0000,0000,,Hey, vui chứ các ngài?
Dialogue: 0,0:22:20.56,0:22:21.77,Default,,0000,0000,0000,,Anh là ai?
Dialogue: 0,0:22:22.13,0:22:23.72,Default,,0000,0000,0000,,Các vị đừng lo.
Dialogue: 0,0:22:24.31,0:22:26.82,Default,,0000,0000,0000,,Anh ta là người mang tới cho các vị một món quà.
Dialogue: 0,0:22:27.49,0:22:29.85,Default,,0000,0000,0000,,Akuji, anh đã chuẩn bị chúng chưa?
Dialogue: 0,0:22:30.00,0:22:32.04,Default,,0000,0000,0000,,Dĩ nhiên. Vào đi.
Dialogue: 0,0:22:37.93,0:22:40.18,Default,,0000,0000,0000,,Oh! những thiếu nữ Japanese!
Dialogue: 0,0:22:40.18,0:22:41.61,Default,,0000,0000,0000,,Đẹp quá
Dialogue: 0,0:22:42.50,0:22:43.68,Default,,0000,0000,0000,,Gì cơt...?
Dialogue: 0,0:22:45.44,0:22:47.09,Default,,0000,0000,0000,,Mùi thơm quá...
Dialogue: 0,0:22:50.15,0:22:51.15,Default,,0000,0000,0000,,Please...
Dialogue: 0,0:22:52.94,0:22:55.07,Default,,0000,0000,0000,,Tôi là một phụ nữ dâm đãng.
Dialogue: 0,0:22:55.71,0:23:01.17,Default,,0000,0000,0000,,Làm ơn hãy chơi chúng tôi với những cái đó của các ngài.
Dialogue: 0,0:23:01.91,0:23:04.39,Default,,0000,0000,0000,,Đây chả phải tiểu thư Kikuko của nhà Ashiya đây sao?
Dialogue: 0,0:23:04.56,0:23:06.47,Default,,0000,0000,0000,,Oh, ngài biết cô ta à.
Dialogue: 0,0:23:06.94,0:23:11.61,Default,,0000,0000,0000,,Hey, đừng lo. Ko ai biết cô ta ở đây đâu.
Dialogue: 0,0:23:11.77,0:23:15.15,Default,,0000,0000,0000,,Kikuko, Kikuko ko thể đợi lâu hơn nữa.
Dialogue: 0,0:23:15.35,0:23:18.92,Default,,0000,0000,0000,,Làm ơn... nhanh lên.
Dialogue: 0,0:23:34.61,0:23:35.49,Default,,0000,0000,0000,,Đúng rồi!
Dialogue: 0,0:23:37.16,0:23:40.08,Default,,0000,0000,0000,,Thật tuyệt vời! Tuyệt lắm, Akuji-kun!
Dialogue: 0,0:23:40.50,0:23:43.40,Default,,0000,0000,0000,,Nếu ngài thích, tôi sẽ nhượng lại một cô cho ngài.
Dialogue: 0,0:23:43.40,0:23:47.30,Default,,0000,0000,0000,,Làm ơn, làm ơn đi Akuji! Tôi sẽ mua với bất cứ giá nào.
Dialogue: 0,0:23:47.30,0:23:48.58,Default,,0000,0000,0000,,Haruka được chứ?
Dialogue: 0,0:23:50.47,0:23:55.02,Default,,0000,0000,0000,,Được, tôi ko thể để một thành viên của Nogi ở trong nhà mình.
Dialogue: 0,0:23:58.07,0:24:00.93,Default,,0000,0000,0000,,Vả lại, cô ấy cũng ko muốn thế...
Dialogue: 0,0:24:08.02,0:24:09.12,Default,,0000,0000,0000,,Lời to rồi...
Dialogue: 0,0:24:11.09,0:24:14.04,Default,,0000,0000,0000,,Đại ca! Nguy rồi!
Dialogue: 0,0:24:14.23,0:24:15.81,Default,,0000,0000,0000,,Ở ngoài kia đang...
Dialogue: 0,0:24:15.81,0:24:17.09,Default,,0000,0000,0000,,Ra đây mau Akuji!
Dialogue: 0,0:24:17.09,0:24:18.03,Default,,0000,0000,0000,,Haruka-san!
Dialogue: 0,0:24:18.03,0:24:20.58,Default,,0000,0000,0000,,Ra khỏi đó ngay! Trả Haruka-san lại cho chúng ta!
Dialogue: 0,0:24:21.67,0:24:23.96,Default,,0000,0000,0000,,Chúng ta làm gì đây, Sat-chan?
Dialogue: 0,0:24:28.05,0:24:29.09,Default,,0000,0000,0000,,Giết hết!
Dialogue: 0,0:24:29.21,0:24:30.23,Default,,0000,0000,0000,,Hmmm...
Dialogue: 0,0:24:32.49,0:24:33.55,Default,,0000,0000,0000,,Ý kiến hay đấy!
Dialogue: 0,0:24:33.55,0:24:34.70,Default,,0000,0000,0000,,Ko được làm thế !
Dialogue: 0,0:24:35.08,0:24:36.70,Default,,0000,0000,0000,,Hey, xin lỗi vì đã bắt các người đợi lâu.
Dialogue: 0,0:24:38.02,0:24:39.11,Default,,0000,0000,0000,,Hắn là Akuji.
Dialogue: 0,0:24:39.11,0:24:40.56,Default,,0000,0000,0000,,Trả Haruka-san lại cho chúng ta!
Dialogue: 0,0:24:41.16,0:24:42.42,Default,,0000,0000,0000,,Vậy, ngươi là Akuji...
Dialogue: 0,0:24:43.06,0:24:44.91,Default,,0000,0000,0000,,Hãy thả Toki Haruka ra!
Dialogue: 0,0:24:46.80,0:24:47.73,Default,,0000,0000,0000,,Haruka...?
Dialogue: 0,0:24:48.77,0:24:50.26,Default,,0000,0000,0000,,Oh, cô gái đó hả.
Dialogue: 0,0:24:50.60,0:24:52.10,Default,,0000,0000,0000,,Cô ta ko còn ở đây nữa rồi.
Dialogue: 0,0:24:53.25,0:24:56.68,Default,,0000,0000,0000,,Cô ta đang phục vụ các quý ông ở Ouime rồi.
Dialogue: 0,0:24:59.22,0:25:00.66,Default,,0000,0000,0000,,Sao ngươi dám...
Dialogue: 0,0:25:02.01,0:25:04.40,Default,,0000,0000,0000,,Các người đã thấy rồi đó, Akuji là tên ác quỉ.
Dialogue: 0,0:25:05.28,0:25:08.30,Default,,0000,0000,0000,,Thương lượng với hắn là vô nghĩa. Mọi người!
Dialogue: 0,0:25:11.39,0:25:13.30,Default,,0000,0000,0000,,Huh, thú vị đấy...
Dialogue: 0,0:25:14.79,0:25:16.78,Default,,0000,0000,0000,,Vào cả đây nào!
