Dialogue: 0,0:00:33.93,0:00:36.16,Default,,0000,0000,0000,,Nè, Naoto-senpai.
Dialogue: 0,0:00:36.23,0:00:37.72,Default,,0000,0000,0000,,Naoto-senpai!
Dialogue: 0,0:00:37.80,0:00:38.70,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:00:38.87,0:00:39.99,Default,,0000,0000,0000,,Ngoan lắm.
Dialogue: 0,0:00:41.47,0:00:42.87,Default,,0000,0000,0000,,Hỏi anh một câu...
Dialogue: 0,0:00:43.47,0:00:45.10,Default,,0000,0000,0000,,Anh phải thành thật trả lời đấy nhé.
Dialogue: 0,0:00:46.41,0:00:51.28,Default,,0000,0000,0000,,Senpai đang hẹn hò với ai đó đúng không?
Dialogue: 0,0:01:00.26,0:01:01.78,Default,,0000,0000,0000,,Em biết ngay mà...
Dialogue: 0,0:01:03.26,0:01:05.56,Default,,0000,0000,0000,,N-Nè, Chika-chan...
Dialogue: 0,0:01:06.19,0:01:08.99,Default,,0000,0000,0000,,Anh sẽ rất mừng nếu em chịu leo xuống khỏi người...
Dialogue: 0,0:01:12.20,0:01:13.50,Default,,0000,0000,0000,,C-Chika-chan?
Dialogue: 0,0:01:13.77,0:01:14.76,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:01:16.04,0:01:18.47,Default,,0000,0000,0000,,Hừm! Naoto-senpai, anh im đi!
Dialogue: 0,0:01:25.98,0:01:27.85,Default,,0000,0000,0000,,Đúng rồi, cứ làm vậy đi!
Dialogue: 0,0:01:42.13,0:01:43.83,Default,,0000,0000,0000,,C-Chika-chan?
Dialogue: 0,0:01:48.64,0:01:50.00,Default,,0000,0000,0000,,Nè, Senpai...
Dialogue: 0,0:01:50.27,0:01:52.54,Default,,0000,0000,0000,,Ngực của em thế nào?
Dialogue: 0,0:01:55.64,0:01:58.08,Default,,0000,0000,0000,,Ý em là sao?
Dialogue: 0,0:01:59.61,0:02:03.61,Default,,0000,0000,0000,,Em không còn là Chika mà Naoto-senpai từng biết nữa đâu...
Dialogue: 0,0:02:03.82,0:02:04.48,Default,,0000,0000,0000,,Đúng không?
Dialogue: 0,0:02:09.89,0:02:10.92,Default,,0000,0000,0000,,Senpai.
Dialogue: 0,0:02:11.69,0:02:12.92,Default,,0000,0000,0000,,Naoto-kun...
Dialogue: 0,0:02:19.83,0:02:23.79,Default,,0000,0000,0000,,Lúc này, trong đầu anh đang so sánh chúng với "hàng" của Aya-neechan, đúng không?
Dialogue: 0,0:02:27.11,0:02:28.10,Default,,0000,0000,0000,,Nói dối!
Dialogue: 0,0:02:28.28,0:02:32.24,Default,,0000,0000,0000,,Em thừa biết anh đang nghĩ gì!
Dialogue: 0,0:02:36.55,0:02:41.39,Default,,0000,0000,0000,,Thế giữa em và Aya-neechan, anh thích ngực của ai hơn?
Dialogue: 0,0:02:49.76,0:02:50.75,Default,,0000,0000,0000,,Senpai.
Dialogue: 0,0:02:51.70,0:02:52.89,Default,,0000,0000,0000,,Naoto-kun...
Dialogue: 0,0:02:56.87,0:02:59.64,Default,,0000,0000,0000,,Này, tỉnh lại đi...
Dialogue: 0,0:03:00.01,0:03:04.50,Default,,0000,0000,0000,,K-Không, đó không phải là thứ có thể so sánh...
Dialogue: 0,0:03:04.98,0:03:07.92,Default,,0000,0000,0000,,Rõ ràng anh đang so sánh nó trong đầu.
Dialogue: 0,0:03:08.12,0:03:12.21,Default,,0000,0000,0000,,Nè, em đã yêu anh từ rất lâu rồi...
Dialogue: 0,0:03:12.85,0:03:18.02,Default,,0000,0000,0000,,Khi chúng ta còn nhỏ... từ cái lúc em chơi đùa cùng với anh và Aya-neechan...
Dialogue: 0,0:03:20.43,0:03:21.52,Default,,0000,0000,0000,,Chika-chan...
Dialogue: 0,0:03:22.20,0:03:24.17,Default,,0000,0000,0000,,Thế nên, thế nên...
Dialogue: 0,0:03:27.67,0:03:29.36,Default,,0000,0000,0000,,Xin lỗi... Anh xin lỗi...
Dialogue: 0,0:03:29.64,0:03:33.84,Default,,0000,0000,0000,,Dĩ nhiên, đối với anh, em là một người vô cùng quan trọng.
Dialogue: 0,0:03:34.88,0:03:38.71,Default,,0000,0000,0000,,Tình cảm của em... chỉ là tình cảm đơn phương thôi sao?
Dialogue: 0,0:03:40.58,0:03:46.89,Default,,0000,0000,0000,,Anh cũng rất vui khi được em quan tâm như vậy, nhưng mà...
Dialogue: 0,0:03:47.22,0:03:48.21,Default,,0000,0000,0000,,Nhưng mà?
Dialogue: 0,0:03:49.19,0:03:52.79,Default,,0000,0000,0000,,''Vì đang hẹn hò với Aya-neechan, nên anh không thể.'', phải không?
Dialogue: 0,0:03:53.29,0:03:56.32,Default,,0000,0000,0000,,Cho dù là vậy, em vẫn không muốn từ bỏ, Naoto-senpai...
Dialogue: 0,0:03:56.40,0:03:59.42,Default,,0000,0000,0000,,Em càng không muốn trao anh cho Aya-neechan!
Dialogue: 0,0:04:01.17,0:04:02.83,Default,,0000,0000,0000,,Hơn thế nữa, Senpai...
Dialogue: 0,0:04:03.17,0:04:07.47,Default,,0000,0000,0000,,Không phải dương vật của anh đang cứng lên vì ngực của em sao?
Dialogue: 0,0:04:20.46,0:04:25.32,Default,,0000,0000,0000,,Vì vậy nếu chỉ dùng một từ khóa tìm kiếm, thì sẽ không đủ để...
Dialogue: 0,0:04:26.23,0:04:28.20,Default,,0000,0000,0000,,Naoto-kun, em có nghe không...
Dialogue: 0,0:04:38.77,0:04:39.87,Default,,0000,0000,0000,,Ayane-san...
Dialogue: 0,0:04:40.98,0:04:42.50,Default,,0000,0000,0000,,Núm vú của chị cứng lên rồi nè.
Dialogue: 0,0:04:42.68,0:04:46.31,Default,,0000,0000,0000,,Tại vì trước đó chúng cọ xát với tay của Naoto-kun.
Dialogue: 0,0:04:47.85,0:04:51.38,Default,,0000,0000,0000,,Không phải chị cố tình làm vậy vì cảm thấy sướng sao?
Dialogue: 0,0:04:54.39,0:04:57.05,Default,,0000,0000,0000,,Mà này, hôm nay chị không mặc áo ngực à?
Dialogue: 0,0:04:58.13,0:04:59.42,Default,,0000,0000,0000,,Có khi nào...
Dialogue: 0,0:04:59.53,0:05:00.69,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:05:01.50,0:05:03.33,Default,,0000,0000,0000,,Đừng có tưởng bở.
Dialogue: 0,0:05:03.67,0:05:06.43,Default,,0000,0000,0000,,Chị không mong chờ chuyện này hay cái gì khác...
Dialogue: 0,0:05:07.07,0:05:08.66,Default,,0000,0000,0000,,Thật không?
Dialogue: 0,0:05:11.91,0:05:15.00,Default,,0000,0000,0000,,Naoto-kun, em xấu lắm...
Dialogue: 0,0:05:24.22,0:05:26.28,Default,,0000,0000,0000,,Chị chỉ cần kẹp nó ở chính giữa như vầy là được à?
Dialogue: 0,0:05:27.02,0:05:28.15,Default,,0000,0000,0000,,Vâng, vâng!
Dialogue: 0,0:05:28.66,0:05:32.35,Default,,0000,0000,0000,,Không ngờ em lại sung sướng như vậy, xấu hổ ghê ta...
Dialogue: 0,0:05:33.03,0:05:36.90,Default,,0000,0000,0000,,Chị luôn nghĩ ngực lớn như vầy thật là vướng víu...
Dialogue: 0,0:05:37.23,0:05:38.96,Default,,0000,0000,0000,,Không đúng!
Dialogue: 0,0:05:39.77,0:05:42.40,Default,,0000,0000,0000,,Ayane-san, ngực chị là tuyệt nhấtl!
Dialogue: 0,0:05:43.17,0:05:47.30,Default,,0000,0000,0000,,Nó là thứ mà tất cả đàn ông trên thế giới đều mỏi mắt kiếm tìm!
Dialogue: 0,0:05:48.58,0:05:49.54,Default,,0000,0000,0000,,Cảm ơn em.
Dialogue: 0,0:06:28.92,0:06:31.82,Default,,0000,0000,0000,,Naoto-kun, thằng nhỏ của em đang bự lên kìa.
Dialogue: 0,0:06:42.90,0:06:46.63,Default,,0000,0000,0000,,Ayane-san, âm hộ của chị đang siết chặt lấy em.
Dialogue: 0,0:06:47.14,0:06:48.36,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:06:50.27,0:06:53.47,Default,,0000,0000,0000,,Ayane-san, chị càng ngày cang dâm đó nha.
Dialogue: 0,0:06:53.64,0:06:57.01,Default,,0000,0000,0000,,Ai bảo em suốt ngày cứ đòi làm chuyện đó với chị chứ?
Dialogue: 0,0:06:59.08,0:07:00.21,Default,,0000,0000,0000,,Ngực chị...
Dialogue: 0,0:07:01.18,0:07:02.17,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:07:08.89,0:07:11.36,Default,,0000,0000,0000,,Chất dịch dơ dáy đó đang trào ra!
Dialogue: 0,0:07:15.70,0:07:16.72,Default,,0000,0000,0000,,Em cũng vậy...
Dialogue: 0,0:07:22.17,0:07:24.47,Default,,0000,0000,0000,,Ngực chị đang bị siết chặt...
Dialogue: 0,0:07:30.68,0:07:32.37,Default,,0000,0000,0000,,C-Chị...
Dialogue: 0,0:07:32.85,0:07:34.11,Default,,0000,0000,0000,,Chị ra đây!
Dialogue: 0,0:07:35.45,0:07:36.35,Default,,0000,0000,0000,,Chị ra đây!
Dialogue: 0,0:07:36.42,0:07:38.44,Default,,0000,0000,0000,,Chị ra đây!
Dialogue: 0,0:07:40.12,0:07:41.92,Default,,0000,0000,0000,,Chị ra đây!
Dialogue: 0,0:07:56.54,0:07:58.66,Default,,0000,0000,0000,,Này, Naoto-kun... đó, đó...
Dialogue: 0,0:07:59.01,0:07:59.94,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:08:00.24,0:08:01.67,Default,,0000,0000,0000,,Còn cái này thì sao?
Dialogue: 0,0:08:03.65,0:08:07.05,Default,,0000,0000,0000,,Chẳng phải họ đang đề nghị chị một công việc tương đối bận rộn sao!
Dialogue: 0,0:08:08.35,0:08:09.21,Default,,0000,0000,0000,,Khoan đã...
Dialogue: 0,0:08:09.82,0:08:12.22,Default,,0000,0000,0000,,Đừng nói vậy chứ.
Dialogue: 0,0:08:12.42,0:08:14.72,Default,,0000,0000,0000,,Cái này không được, cái kia cũng không được.
Dialogue: 0,0:08:14.82,0:08:16.22,Default,,0000,0000,0000,,Kết quả là chả làm nên trò chống gì.
Dialogue: 0,0:08:16.29,0:08:17.22,Default,,0000,0000,0000,,Đau!
Dialogue: 0,0:08:17.29,0:08:19.56,Default,,0000,0000,0000,,Đau lắm, Ayane-san!
Dialogue: 0,0:08:20.70,0:08:21.99,Default,,0000,0000,0000,,Bởi vì...
Dialogue: 0,0:08:22.06,0:08:25.66,Default,,0000,0000,0000,,Em chẳng phải mẫu người làm việc chăm chỉ.
Dialogue: 0,0:08:26.30,0:08:29.79,Default,,0000,0000,0000,,Vậy đến khi em tốt nghiệp...
Dialogue: 0,0:08:31.21,0:08:32.54,Default,,0000,0000,0000,,Ayane-san...
Dialogue: 0,0:08:40.28,0:08:42.84,Default,,0000,0000,0000,,Bây giờ em muốn làm thêm một lần nữa, có được không?
Dialogue: 0,0:08:52.43,0:08:53.45,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:08:54.03,0:08:57.62,Default,,0000,0000,0000,,Tôi muốn xem tài liệu hướng dẫn thảo luận nhóm...
Dialogue: 0,0:08:58.10,0:08:59.16,Default,,0000,0000,0000,,Vâng, vâng!
Dialogue: 0,0:09:05.24,0:09:06.57,Default,,0000,0000,0000,,T-To quá...
Dialogue: 0,0:09:09.48,0:09:11.47,Default,,0000,0000,0000,,Em học năm ba phải không?
Dialogue: 0,0:09:12.11,0:09:13.05,Default,,0000,0000,0000,,H?
Dialogue: 0,0:09:13.25,0:09:14.87,Default,,0000,0000,0000,,Không, chắc là F?
Dialogue: 0,0:09:17.69,0:09:20.31,Default,,0000,0000,0000,,Đầy là lần đầu em sử dụng phòng này đúng không?
Dialogue: 0,0:09:23.86,0:09:27.35,Default,,0000,0000,0000,,H-Họ nói đó là...
Dialogue: 0,0:09:27.43,0:09:30.69,Default,,0000,0000,0000,,thung lũng chỉ có ở rừng rậm Châu Phi...
Dialogue: 0,0:09:40.07,0:09:42.24,Default,,0000,0000,0000,,Trước khi sử dụng phòng này...
Dialogue: 0,0:09:42.54,0:09:43.53,Default,,0000,0000,0000,,Cứu tôi với!
Dialogue: 0,0:09:43.61,0:09:44.77,Default,,0000,0000,0000,,Đây, đây!
Dialogue: 0,0:09:45.18,0:09:47.67,Default,,0000,0000,0000,,Em cần điền vào đây...
Dialogue: 0,0:09:49.68,0:09:51.17,Default,,0000,0000,0000,,Tương đối phức tạp, đúng không?
Dialogue: 0,0:09:51.59,0:09:54.25,Default,,0000,0000,0000,,Cái thủ tục bắt điền vào ô...
Dialogue: 0,0:09:54.32,0:10:00.39,Default,,0000,0000,0000,,sinh viên tìm kiếm việc làm đúng là có vấn đề. Nhưng luật là luật, chị xin lỗi.
Dialogue: 0,0:10:05.60,0:10:07.36,Default,,0000,0000,0000,,Em biết điền thế nào rồi chứ?
Dialogue: 0,0:10:08.27,0:10:13.21,Note,,0000,0000,0000,,{\i1}Naoto Sudo{\i0}
Dialogue: 0,0:10:08.90,0:10:11.24,Default,,0000,0000,0000,,Viết tên em vào đây...
Dialogue: 0,0:10:15.88,0:10:16.84,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:10:17.48,0:10:18.84,Default,,0000,0000,0000,,Naoto-kun?
Dialogue: 0,0:10:27.36,0:10:30.81,Default,,0000,0000,0000,,Không lẽ chị là Ayane-san ư?
Dialogue: 0,0:10:34.93,0:10:36.42,Default,,0000,0000,0000,,Cảm ơn về bữa ăn!
Dialogue: 0,0:10:40.84,0:10:42.39,Default,,0000,0000,0000,,Naoto-kun, ngon không?
Dialogue: 0,0:10:42.47,0:10:43.56,Default,,0000,0000,0000,,Ngon lắm!
Dialogue: 0,0:10:43.74,0:10:44.73,Default,,0000,0000,0000,,Vậy thì tốt.
Dialogue: 0,0:10:51.91,0:10:52.78,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:10:54.92,0:10:56.11,Default,,0000,0000,0000,,Em không sao chứ?
Dialogue: 0,0:10:58.95,0:11:00.89,Default,,0000,0000,0000,,Em vừa bị nghẹn ấy mà.
Dialogue: 0,0:11:00.96,0:11:04.52,Default,,0000,0000,0000,,Thôi nào, cơm dính đầy hai bên mép thế kia.
Dialogue: 0,0:11:08.53,0:11:09.93,Default,,0000,0000,0000,,C-Chị xin lỗi!
Dialogue: 0,0:11:10.00,0:11:11.52,Default,,0000,0000,0000,,Đó chỉ là thói quen thôi.
Dialogue: 0,0:11:14.70,0:11:16.10,Default,,0000,0000,0000,,Ơ-Ơ!
Dialogue: 0,0:11:16.17,0:11:17.93,Default,,0000,0000,0000,,Em nói trước đi!
Dialogue: 0,0:11:16.17,0:11:17.93,Default,,0000,0000,0000,,Chị nói trước đi!
Dialogue: 0,0:11:24.38,0:11:25.54,Default,,0000,0000,0000,,Ơ-Ơ...
Dialogue: 0,0:11:25.61,0:11:28.41,Default,,0000,0000,0000,,Đ-Đã từ rất lâu rồi...
Dialogue: 0,0:11:31.99,0:11:34.61,Default,,0000,0000,0000,,Em luôn yên chị, Ayane-san.
Dialogue: 0,0:11:34.89,0:11:35.69,Default,,0000,0000,0000,,Vì vậy...
Dialogue: 0,0:11:35.76,0:11:36.92,Default,,0000,0000,0000,,Vì vậy...
Dialogue: 0,0:11:38.09,0:11:39.15,Default,,0000,0000,0000,,Naoto-kun?
Dialogue: 0,0:11:41.86,0:11:43.42,Default,,0000,0000,0000,,Hãy hẹn hò với em.
Dialogue: 0,0:11:49.94,0:11:51.91,Default,,0000,0000,0000,,Hoan hô!
Dialogue: 0,0:12:05.95,0:12:07.75,Default,,0000,0000,0000,,N-Naoto-kun?
Dialogue: 0,0:12:10.22,0:12:12.92,Default,,0000,0000,0000,,Tắt đèn đi em?
Dialogue: 0,0:12:13.56,0:12:17.19,Default,,0000,0000,0000,,Vậy thì làm sao em ngắm nhìn ngực chị được.
Dialogue: 0,0:12:30.65,0:12:31.74,Default,,0000,0000,0000,,Naoto-kun...
Dialogue: 0,0:12:32.01,0:12:33.84,Default,,0000,0000,0000,,Đừng sờ nắn ngực chị mãi như vậy...
Dialogue: 0,0:13:50.42,0:13:52.48,Default,,0000,0000,0000,,Ngực chị... tuyệt quá!
Dialogue: 0,0:14:11.75,0:14:12.91,Default,,0000,0000,0000,,Naoto-kun!
Dialogue: 0,0:14:13.15,0:14:14.17,Default,,0000,0000,0000,,Naoto-kun!
Dialogue: 0,0:14:14.45,0:14:15.38,Default,,0000,0000,0000,,Naoto-kun!
Dialogue: 0,0:14:15.45,0:14:16.31,Default,,0000,0000,0000,,Naoto-kun!
Dialogue: 0,0:14:17.32,0:14:18.51,Default,,0000,0000,0000,,Naoto-kun!
Dialogue: 0,0:14:19.82,0:14:20.81,Default,,0000,0000,0000,,Tệ quá!
Dialogue: 0,0:14:21.06,0:14:22.35,Default,,0000,0000,0000,,Em sắp ra.
Dialogue: 0,0:14:22.62,0:14:23.72,Default,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:14:25.36,0:14:26.52,Default,,0000,0000,0000,,Bắn vào bên trong chị!
Dialogue: 0,0:14:27.36,0:14:29.33,Default,,0000,0000,0000,,Chị sắp ra rồi. Chị sắp ra rồi!
Dialogue: 0,0:14:31.00,0:14:32.76,Default,,0000,0000,0000,,Ngực chị. Ngực chị...
Dialogue: 0,0:14:32.93,0:14:33.96,Default,,0000,0000,0000,,Chị ra rồi!
Dialogue: 0,0:14:34.24,0:14:35.57,Default,,0000,0000,0000,,Chị ra rồi!
Dialogue: 0,0:14:44.41,0:14:47.87,Default,,0000,0000,0000,,Ayane-san, ngực chị thật sự rất bự và quyến rũ.
Dialogue: 0,0:14:47.95,0:14:50.68,Default,,0000,0000,0000,,Đừng nói như vậy!
Dialogue: 0,0:15:11.34,0:15:12.33,Default,,0000,0000,0000,,Gì vậy? Gì vậy?
Dialogue: 0,0:15:12.41,0:15:16.04,Default,,0000,0000,0000,,Đây, Chika-chan quyết tâm vượt qua kì kiểm tra để nhập học trường chúng ta.
Dialogue: 0,0:15:16.61,0:15:19.74,Default,,0000,0000,0000,,Lần này, rút kinh nghiệm từ thất bại năm vừa rồi...
Dialogue: 0,0:15:19.81,0:15:23.11,Default,,0000,0000,0000,,Em sẽ đến Tokyo sớm một chút để chuẩn bị trước...
Dialogue: 0,0:15:23.18,0:15:27.38,Default,,0000,0000,0000,,vậy nên em dự định ở nhờ căn hộ của anh một tuần.
Dialogue: 0,0:15:28.56,0:15:32.12,Default,,0000,0000,0000,,Anh phải chiếu cố em cho tốt vào nha, Senpai.
Dialogue: 0,0:15:36.43,0:15:37.86,Default,,0000,0000,0000,,Nó gợi lại thật nhiều kỉ niệm.
Dialogue: 0,0:15:38.07,0:15:42.06,Default,,0000,0000,0000,,Nhưng mà, thật ra năm vừa rồi em ấy cũng quyết tâm như thế.
Dialogue: 0,0:15:42.74,0:15:45.97,Default,,0000,0000,0000,,Em ấy cũng tới Tokyo sớm một ngày, nhưng...
Dialogue: 0,0:15:46.04,0:15:48.20,Default,,0000,0000,0000,,Kết quả bài thi lại không được tốt cho lắm.
Dialogue: 0,0:15:48.44,0:15:51.97,Default,,0000,0000,0000,,Mặc dù kiểm tra thử em ấy được A++.
Dialogue: 0,0:15:56.42,0:15:59.65,Default,,0000,0000,0000,,Chờ em với, Naoto-niichan!
Dialogue: 0,0:16:15.20,0:16:16.36,Default,,0000,0000,0000,,Chika-chan!
Dialogue: 0,0:16:17.00,0:16:18.49,Default,,0000,0000,0000,,Naoto-senpai!
Dialogue: 0,0:16:19.91,0:16:21.97,Default,,0000,0000,0000,,Senpai... Ai vậy?
Dialogue: 0,0:16:22.71,0:16:24.20,Default,,0000,0000,0000,,Em thử đoán xem cô ấy là ai?
Dialogue: 0,0:16:24.75,0:16:26.94,Default,,0000,0000,0000,,Chika-chan, thực ra hai người quen nhau đấy.
Dialogue: 0,0:16:40.03,0:16:41.12,Default,,0000,0000,0000,,Aya-neechan?
Dialogue: 0,0:16:41.60,0:16:42.65,Default,,0000,0000,0000,,Đúng rồi!
Dialogue: 0,0:16:43.60,0:16:46.09,Default,,0000,0000,0000,,Chika-chan, đã lâu không gặp.
Dialogue: 0,0:16:47.77,0:16:49.26,Default,,0000,0000,0000,,Nhớ hồi xưa ghê ta!
Dialogue: 0,0:16:51.77,0:16:53.17,Default,,0000,0000,0000,,Nhưng tại sao?
Dialogue: 0,0:16:53.61,0:16:58.01,Default,,0000,0000,0000,,Ayane-san là nhân viên văn phòng giới thiệu việc làm của trường anh.
Dialogue: 0,0:16:58.45,0:17:02.78,Default,,0000,0000,0000,,À, anh cũng mới chỉ phát hiện ra điều đó sau khi bắt đầu năm học.
Dialogue: 0,0:17:03.18,0:17:04.58,Default,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:17:05.42,0:17:08.72,Default,,0000,0000,0000,,Từ năm sau làm ơn hãy chiếu cố đến em.
Dialogue: 0,0:17:09.86,0:17:13.76,Default,,0000,0000,0000,,Mà em cũng đã thi đậu đâu. Nói thế hơi sớm thì phải?
Dialogue: 0,0:17:18.30,0:17:23.29,Default,,0000,0000,0000,,Thôi, bao tử của em lại bắt đầu réo rồi, hai người đi ăn trưa cùng em chứ?
Dialogue: 0,0:17:23.70,0:17:26.70,Default,,0000,0000,0000,,Dĩ nhiên là... Naoto-senpai chủ chi!
Dialogue: 0,0:17:28.71,0:17:32.30,Default,,0000,0000,0000,,Với một sinh viên nghèo như anh, điều đó thật là nhẫn tâm...
Dialogue: 0,0:17:32.38,0:17:33.94,Default,,0000,0000,0000,,Có sao đâu.
Dialogue: 0,0:17:34.55,0:17:36.68,Default,,0000,0000,0000,,Xem như là ăn mừng cuộc hội nhộ của ba chúng ta!
Dialogue: 0,0:17:37.25,0:17:39.52,Default,,0000,0000,0000,,Ơ-Ơ, chị xin lỗi...
Dialogue: 0,0:17:40.05,0:17:43.55,Default,,0000,0000,0000,,Chị có chút chuyện phải làm.
Dialogue: 0,0:17:44.33,0:17:47.66,Default,,0000,0000,0000,,Vậy nên chị phải đi bây giờ.
Dialogue: 0,0:17:48.80,0:17:50.06,Default,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:17:50.23,0:17:51.86,Default,,0000,0000,0000,,Ể, tiếc quá...
Dialogue: 0,0:17:52.73,0:17:54.06,Default,,0000,0000,0000,,Chị xin lỗi.
Dialogue: 0,0:17:54.57,0:17:56.80,Default,,0000,0000,0000,,Lần tới chị sẽ khao.
Dialogue: 0,0:17:56.97,0:18:00.37,Default,,0000,0000,0000,,Ể? Có nghĩa là lần này em phải khao thật sao?
Dialogue: 0,0:18:00.44,0:18:03.64,Default,,0000,0000,0000,,Thôi nào, Aya-neechan cũng đã nói vậy rồi.
Dialogue: 0,0:18:05.15,0:18:08.08,Default,,0000,0000,0000,,V-Vậy chị phải đi đây.
Dialogue: 0,0:18:11.82,0:18:13.84,Default,,0000,0000,0000,,Hẹn gặp lại!
Dialogue: 0,0:18:14.72,0:18:17.35,Default,,0000,0000,0000,,Vậy, chúng ta cũng đi thôi.
Dialogue: 0,0:18:35.94,0:18:37.88,Default,,0000,0000,0000,,Nhanh lên, hãy trả lời em đi.
Dialogue: 0,0:18:42.82,0:18:46.31,Default,,0000,0000,0000,,E rằng anh không thể đáp lại...
Dialogue: 0,0:18:47.76,0:18:48.98,Default,,0000,0000,0000,,tình cảm của em được.
Dialogue: 0,0:18:52.86,0:18:54.92,Default,,0000,0000,0000,,V-Vậy nên...
Dialogue: 0,0:18:55.33,0:18:58.70,Default,,0000,0000,0000,,Anh rất vui khí biết Chika-chan...
Dialogue: 0,0:19:00.03,0:19:03.03,Default,,0000,0000,0000,,Anh vẫn luôn xem em như...
Dialogue: 0,0:19:04.37,0:19:05.53,Default,,0000,0000,0000,,C-Chika-chan?
Dialogue: 0,0:19:05.61,0:19:06.77,Default,,0000,0000,0000,,Đừng nói nữa!
Dialogue: 0,0:19:08.08,0:19:11.67,Default,,0000,0000,0000,,Nếu anh còn nói như vậy, em sẽ trở thành một kẻ đánh thương hại!
Dialogue: 0,0:19:11.78,0:19:13.25,Default,,0000,0000,0000,,Em là một đứa khờ khạo!
Dialogue: 0,0:19:13.51,0:19:16.81,Default,,0000,0000,0000,,Em không biết Aya-neechan yêu anh nhiều tới mức nào, nhưng...
Dialogue: 0,0:19:16.88,0:19:20.45,Default,,0000,0000,0000,,từ nhỏ em đã muốn...
Dialogue: 0,0:19:20.79,0:19:22.78,Default,,0000,0000,0000,,làm điều này, điều kia... {mến anh, yêu anh}
Dialogue: 0,0:19:22.99,0:19:25.52,Default,,0000,0000,0000,,Em muốn cùng anh làm chuyện nọ, chuyện kia... {Em muốn anh mến em, anh yêu em}
Dialogue: 0,0:19:26.06,0:19:28.59,Default,,0000,0000,0000,,Em muốn làm điều này. Em muốn làm điều kia... {Em muốn mến anh, em muốn yêu anh}
Dialogue: 0,0:19:28.66,0:19:30.63,Default,,0000,0000,0000,,Em đã phải suy nghĩ rất nhiều!
Dialogue: 0,0:19:31.43,0:19:32.16,Default,,0000,0000,0000,,Chika-chan...
Dialogue: 0,0:19:33.20,0:19:34.43,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:19:34.84,0:19:36.13,Default,,0000,0000,0000,,Sai thời điểm ư?
Dialogue: 0,0:19:36.70,0:19:41.51,Default,,0000,0000,0000,,Nếu là như vậy, em sẽ không thỏa hiệp đâu!
Dialogue: 0,0:19:54.99,0:19:56.25,Default,,0000,0000,0000,,Senpai...
Dialogue: 0,0:20:23.05,0:20:24.24,Default,,0000,0000,0000,,Sướng không?
Dialogue: 0,0:20:25.22,0:20:26.41,Default,,0000,0000,0000,,Thật chứ?
Dialogue: 0,0:20:26.49,0:20:29.51,Default,,0000,0000,0000,,Nếu đúng là vậy, em vui lắm.
Dialogue: 0,0:20:29.96,0:20:33.83,Default,,0000,0000,0000,,Vì anh, em đã cố gắng học hỏi nhiều thứ.
Dialogue: 0,0:20:34.13,0:20:35.03,Default,,0000,0000,0000,,Vậy nên...
Dialogue: 0,0:20:35.10,0:20:36.12,Default,,0000,0000,0000,,Vậy nên...
Dialogue: 0,0:20:44.44,0:20:45.63,Default,,0000,0000,0000,,Em yêu anh!
Dialogue: 0,0:20:55.88,0:20:57.61,Default,,0000,0000,0000,,Naoto-senpai...
Dialogue: 0,0:20:58.22,0:20:59.98,Default,,0000,0000,0000,,đút nó vào đây đúng không?
Dialogue: 0,0:21:01.59,0:21:04.58,Default,,0000,0000,0000,,Em đã muốn làm chuyện này từ lâu lắm rồi.
Dialogue: 0,0:21:10.06,0:21:11.36,Default,,0000,0000,0000,,Nó đang đâm vào...
Dialogue: 0,0:21:12.80,0:21:15.50,Default,,0000,0000,0000,,Dương vật nóng hổi của Senpai...
Dialogue: 0,0:21:16.54,0:21:18.84,Default,,0000,0000,0000,,đang đâm vào bên trong em.
Dialogue: 0,0:21:28.02,0:21:32.48,Default,,0000,0000,0000,,Chúng ta đang làm chuyện này, em thật sự rất vui.
Dialogue: 0,0:21:35.52,0:21:38.58,Default,,0000,0000,0000,,Naoto-senpai, dương vật của anh...
Dialogue: 0,0:21:38.73,0:21:40.45,Default,,0000,0000,0000,,sướng quá.
Dialogue: 0,0:21:51.54,0:21:53.94,Default,,0000,0000,0000,,Nè, anh có thể chơi đùa với ngực em được không?
Dialogue: 0,0:21:54.68,0:21:55.51,Default,,0000,0000,0000,,Nhé?
Dialogue: 0,0:21:55.91,0:21:59.28,Default,,0000,0000,0000,,Nhé? Em chăm sóc bộ ngực kĩ như vậy là vì anh đó...
Dialogue: 0,0:21:59.35,0:22:01.37,Default,,0000,0000,0000,,Bởi vì anh rất yêu ngực.
Dialogue: 0,0:22:01.45,0:22:03.88,Default,,0000,0000,0000,,Vậy nên anh cũng sẽ yêu em, Naoto-senpai.
Dialogue: 0,0:22:04.18,0:22:05.21,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:22:06.12,0:22:09.28,Default,,0000,0000,0000,,Nhưng là anh làm em đau.
Dialogue: 0,0:22:10.89,0:22:11.95,Default,,0000,0000,0000,,Được rồi...
Dialogue: 0,0:22:16.30,0:22:19.03,Default,,0000,0000,0000,,Naoto-senpai, anh dâm quá đi...
Dialogue: 0,0:22:19.53,0:22:21.13,Default,,0000,0000,0000,,Sướng quá!
Dialogue: 0,0:22:25.37,0:22:26.86,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:22:27.47,0:22:30.27,Default,,0000,0000,0000,,Naoto-senpai, em nghĩ anh rất đặc biệt.
Dialogue: 0,0:22:36.72,0:22:38.84,Default,,0000,0000,0000,,Em thật dâm đãng...
Dialogue: 0,0:22:39.42,0:22:42.29,Default,,0000,0000,0000,,Senpai, em yêu "thằng nhỏ" của anh.
Dialogue: 0,0:22:43.69,0:22:46.32,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:22:47.03,0:22:50.26,Default,,0000,0000,0000,,Đây là lần đầu tiên em có cảm giác này.
Dialogue: 0,0:23:09.52,0:23:10.92,Default,,0000,0000,0000,,Dâm dãng?
Dialogue: 0,0:23:11.05,0:23:13.02,Default,,0000,0000,0000,,Em là một cô gái dâm đãng đúng không?
Dialogue: 0,0:23:13.35,0:23:17.38,Default,,0000,0000,0000,,Nhưng em trở nên dâm đãng là vì anh, Naoto-senpai!
Dialogue: 0,0:23:18.29,0:23:20.09,Default,,0000,0000,0000,,E-Em ra đây!
Dialogue: 0,0:23:20.76,0:23:22.82,Default,,0000,0000,0000,,Mặc dù em vẫn muốn tiếp tục chuyện này!
Dialogue: 0,0:23:40.35,0:23:42.14,Default,,0000,0000,0000,,Em... Em...
Dialogue: 0,0:23:42.75,0:23:44.68,Default,,0000,0000,0000,,muốn sinh em bé.
Dialogue: 0,0:24:10.08,0:24:13.07,Default,,0000,0000,0000,,Em ra rồi, em ra rồi, em ra rồi!
Dialogue: 0,0:24:26.39,0:24:28.33,Default,,0000,0000,0000,,Mình phải làm sao đây?
