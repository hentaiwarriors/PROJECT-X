Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:45.19,0:00:47.60,Default,,0000,0000,0000,,Takakura xinh đẹp: Bị cướp đi trinh tiết
Dialogue: 0,0:00:47.60,0:00:49.86,Default,,0000,0000,0000,,trong lúc học bài ở nhà.
Dialogue: 0,0:00:53.45,0:00:56.16,Default,,0000,0000,0000,,Sensei, em làm xong rồi nè.
Dialogue: 0,0:01:00.17,0:01:02.48,Default,,0000,0000,0000,,Có vẻ đúng đấy.
Dialogue: 0,0:01:03.51,0:01:04.20,Default,,0000,0000,0000,,Oh, để thầy xem kĩ lại.
Dialogue: 0,0:01:08.65,0:01:10.33,Default,,0000,0000,0000,,Ah, em làm tốt đấy chứ.
Dialogue: 0,0:01:10.85,0:01:12.35,Default,,0000,0000,0000,,Em ko chỉ xinh đep mà còn học rất tốt.
Dialogue: 0,0:01:21.41,0:01:22.05,Default,,0000,0000,0000,,Yumi?
Dialogue: 0,0:01:24.54,0:01:25.20,Default,,0000,0000,0000,,Em sao thế?
Dialogue: 0,0:01:25.97,0:01:27.20,Default,,0000,0000,0000,,Trông em có vẻ hơi khó chịu đấy.
Dialogue: 0,0:01:30.99,0:01:33.90,Default,,0000,0000,0000,,Nếu cảm thấy không thoải mái thì để thầy bóp vai cho em nhé.
Dialogue: 0,0:01:36.67,0:01:39.85,Default,,0000,0000,0000,,Không! Em không sao đâu mà.
Dialogue: 0,0:01:44.84,0:01:45.70,Default,,0000,0000,0000,,Sensei?
Dialogue: 0,0:01:46.75,0:01:49.25,Default,,0000,0000,0000,,Thầy làm gì vậy? Hãy buông em ra đi.
Dialogue: 0,0:01:49.83,0:01:51.53,Default,,0000,0000,0000,,Em thật đáng yêu, Yumi.
Dialogue: 0,0:01:52.35,0:01:54.23,Default,,0000,0000,0000,,Tôi đã muốn làm việc này từ lâu lắm rồi.
Dialogue: 0,0:01:57.62,0:01:59.62,Default,,0000,0000,0000,,Đừng! Buông em ra.
Dialogue: 0,0:02:04.57,0:02:06.42,Default,,0000,0000,0000,,Không! Dừng lại! Sensei.
Dialogue: 0,0:02:07.03,0:02:08.97,Default,,0000,0000,0000,,Ai đó làm ơn... ah!
Dialogue: 0,0:02:09.33,0:02:12.51,Default,,0000,0000,0000,,Em đang phí sức đấy, không ai có thể giúp em lúc này đâu.
Dialogue: 0,0:02:13.18,0:02:15.06,Default,,0000,0000,0000,,Bắt đầu cảm thấy lạ rồi phải ko? Đây chính là sức mạnh của "Quyển sổ" đó.
Dialogue: 0,0:02:15.98,0:02:18.04,Default,,0000,0000,0000,,Sensei, tại sao thầy lại đối xử với em như vậy?
Dialogue: 0,0:02:18.96,0:02:21.14,Default,,0000,0000,0000,,Em đã tin thầy...
Dialogue: 0,0:02:23.63,0:02:24.73,Default,,0000,0000,0000,,Không chỉ có lần này đâu.
Dialogue: 0,0:02:25.32,0:02:28.09,Default,,0000,0000,0000,,Tôi đã từng hưởng thụ cảm giác này nhiều lần trước đây.
Dialogue: 0,0:02:28.35,0:02:29.29,Default,,0000,0000,0000,,Sao có thể như vậy được...?
Dialogue: 0,0:02:31.19,0:02:32.18,Default,,0000,0000,0000,,Em không để ý sao?
Dialogue: 0,0:02:32.93,0:02:37.76,Default,,0000,0000,0000,,Trong suốt buổi học đầu óc tôi luôn tràn ngập hình ảnh cơ thể trần trụi của em.
Dialogue: 0,0:02:38.52,0:02:39.53,Default,,0000,0000,0000,,Xinh đẹp... và gợi tình... Ah!
Dialogue: 0,0:02:40.50,0:02:42.79,Default,,0000,0000,0000,,Ngực em... chúng thật tuyệt!
Dialogue: 0,0:02:44.75,0:02:47.35,Default,,0000,0000,0000,,Không... đừng nhìn! Ah!
Dialogue: 0,0:02:52.00,0:02:53.92,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:02:53.84,0:02:55.99,Default,,0000,0000,0000,,Sensei, đừng vậy mà!
Dialogue: 0,0:03:06.75,0:03:08.11,Default,,0000,0000,0000,,Em bắt đầu thấy thích thú rồi phải ko.
Dialogue: 0,0:03:09.77,0:03:10.55,Default,,0000,0000,0000,,Em không có...
Dialogue: 0,0:03:21.60,0:03:24.33,Default,,0000,0000,0000,,Sao nào, em vẫn muốn thầy tiếp tục phải ko?
Dialogue: 0,0:03:25.51,0:03:28.25,Default,,0000,0000,0000,,Không, làm ơn dừng lại.
Dialogue: 0,0:03:28.97,0:03:30.14,Default,,0000,0000,0000,,Em không thành thật chút nào cả.
Dialogue: 0,0:03:31.25,0:03:32.21,Default,,0000,0000,0000,,Tiếp theo là dưới này.
Dialogue: 0,0:03:33.98,0:03:36.10,Default,,0000,0000,0000,,Không, không phải chỗ đó... không!
Dialogue: 0,0:03:40.29,0:03:43.70,Default,,0000,0000,0000,,Rất ướt át, em đúng là một cô bé hư.
Dialogue: 0,0:03:44.58,0:03:46.97,Default,,0000,0000,0000,,Và còn rất nhạy cảm nữa.
Dialogue: 0,0:03:47.50,0:03:49.43,Default,,0000,0000,0000,,Thật là dâm đãng.
Dialogue: 0,0:03:50.25,0:03:54.35,Default,,0000,0000,0000,,Xin thầy đừng nói những lời như vậy.
Dialogue: 0,0:03:55.71,0:03:56.50,Default,,0000,0000,0000,,Tại sao không?
Dialogue: 0,0:03:56.95,0:04:00.74,Default,,0000,0000,0000,,Chỗ của em đang rất ướt, có vẻ như nó đang mời gọi thầy.
Dialogue: 0,0:04:03.17,0:04:06.22,Default,,0000,0000,0000,,Không, xin thầy hãy tha cho em.
Dialogue: 0,0:04:06.94,0:04:08.92,Default,,0000,0000,0000,,Em chưa có kinh nghiệm trong chuyện này.
Dialogue: 0,0:04:09.76,0:04:13.80,Default,,0000,0000,0000,,Một người đàn ông sẽ cảm thấy phấn khích hơn khi nghe thấy những lời đó.
Dialogue: 0,0:04:14.74,0:04:17.39,Default,,0000,0000,0000,,Yumi, giờ học đến rồi.
Dialogue: 0,0:04:18.21,0:04:19.04,Default,,0000,0000,0000,,Sao có thể...?
Dialogue: 0,0:04:19.43,0:04:22.25,Default,,0000,0000,0000,,Không, đừng, ah!
Dialogue: 0,0:04:36.42,0:04:37.57,Default,,0000,0000,0000,,Vào bên trong rồi.
Dialogue: 0,0:04:38.00,0:04:40.82,Default,,0000,0000,0000,,Yumi, cảm giác trở thành đàn bà như thế nào?
Dialogue: 0,0:04:41.68,0:04:44.85,Default,,0000,0000,0000,,Đau quá... xin thầy hãy rút nó ra đi.
Dialogue: 0,0:04:45.70,0:04:48.76,Default,,0000,0000,0000,,Đừng lo , em sẽ cảm thấy thích thú ngay bây giờ thôi.
Dialogue: 0,0:04:49.80,0:04:51.75,Default,,0000,0000,0000,,Đau quá, làm ơn dừng lại đi.
Dialogue: 0,0:05:01.54,0:05:02.93,Default,,0000,0000,0000,,Vài phút trước, em vẫn còn là một trinh nữ.
Dialogue: 0,0:05:03.30,0:05:05.41,Default,,0000,0000,0000,,Giờ thì em đã cảm thấy sướng rồi sao?
Dialogue: 0,0:05:05.70,0:05:06.65,Default,,0000,0000,0000,,Không phải như vậy...
Dialogue: 0,0:05:07.28,0:05:08.42,Default,,0000,0000,0000,,Nhưng tại sao?
Dialogue: 0,0:05:31.63,0:05:35.78,Default,,0000,0000,0000,,Em sẽ bước qua cánh cửa tới thiên đường hoan lạc nhanh thôi.
Dialogue: 0,0:05:54.27,0:05:56.66,Default,,0000,0000,0000,,Thật tuyệt vời...
Dialogue: 0,0:05:57.95,0:05:58.59,Default,,0000,0000,0000,,Tôi sắp ra rồi.
Dialogue: 0,0:05:58.98,0:06:00.72,Default,,0000,0000,0000,,Tôi sẽ ra bên trong em.
Dialogue: 0,0:06:02.36,0:06:03.98,Default,,0000,0000,0000,,Không, em xin thầy!
Dialogue: 0,0:06:04.21,0:06:07.64,Default,,0000,0000,0000,,Đừng ra...bên trong!
Dialogue: 0,0:06:09.09,0:06:11.21,Default,,0000,0000,0000,,Không, ah!
Dialogue: 0,0:06:15.76,0:06:18.77,Default,,0000,0000,0000,,Thầy thật sự đã... ra bên trong sao?
Dialogue: 0,0:06:18.33,0:06:21.43,Default,,0000,0000,0000,,Em sẽ có thai mất...
Dialogue: 0,0:06:38.31,0:06:39.34,Default,,0000,0000,0000,,Yumi, nghe đây!
Dialogue: 0,0:06:39.84,0:06:41.74,Default,,0000,0000,0000,,Không được phép kể chuyện này với gia đình.
Dialogue: 0,0:06:42.40,0:06:44.06,Default,,0000,0000,0000,,Tốt hơn cả là họ ko nên biết chúng ta đã làm chuyện gì với nhau.
Dialogue: 0,0:06:44.88,0:06:47.09,Default,,0000,0000,0000,,Tiết lộ mọi chuyện không phải là một ý kiến hay, phải không nào?
Dialogue: 0,0:06:59.43,0:07:01.57,Default,,0000,0000,0000,,Tan học rồi sao, sensei?
Dialogue: 0,0:07:03.16,0:07:05.66,Default,,0000,0000,0000,,Buổi học tốt chứ ạ?
Dialogue: 0,0:07:06.47,0:07:10.39,Default,,0000,0000,0000,,Đúng vậy, Yumi rất hứng thú với môn học này.
Dialogue: 0,0:07:11.23,0:07:13.99,Default,,0000,0000,0000,,Cái gì, lại là anh hả?
Dialogue: 0,0:07:16.41,0:07:19.91,Default,,0000,0000,0000,,Shiori, em vẫn ầm ĩ như vậy.
Dialogue: 0,0:07:21.52,0:07:24.03,Default,,0000,0000,0000,,Đừng nói với tôi bằng cái giọng đó.
Dialogue: 0,0:07:24.59,0:07:25.85,Default,,0000,0000,0000,,Gọi tôi là cô Shiori, rõ chưa?
Dialogue: 0,0:07:28.37,0:07:30.20,Default,,0000,0000,0000,,Đừng có xáp lại gần...
Dialogue: 0,0:07:30.80,0:07:33.11,Default,,0000,0000,0000,,và nhìn em gái tôi bằng đôi mắt dâm đãng ấy.
Dialogue: 0,0:07:34.92,0:07:35.88,Default,,0000,0000,0000,,Con bé ngạo mạn!
Dialogue: 0,0:07:36.51,0:07:39.17,Default,,0000,0000,0000,,Mình ngờ rằng cô ta vẫn còn là trinh nữ.
Dialogue: 0,0:07:39.76,0:07:42.56,Default,,0000,0000,0000,,Shiori, con ăn nói kiểu gì vậy?
Dialogue: 0,0:07:42.96,0:07:44.76,Default,,0000,0000,0000,,Mong thầy bỏ quá cho, sensei.
Dialogue: 0,0:07:46.24,0:07:48.38,Default,,0000,0000,0000,,Không sao! Không sao!
Dialogue: 0,0:07:50.82,0:07:51.92,Default,,0000,0000,0000,,Vậy tôi xin phép.
Dialogue: 0,0:07:52.39,0:07:53.46,Default,,0000,0000,0000,,Hẹn gặp lại Shiori-chan.
Dialogue: 0,0:07:54.55,0:07:57.40,Default,,0000,0000,0000,,Sensei là một người thầy tốt, rất mong thầy sẽ quay lại.
Dialogue: 0,0:07:57.92,0:07:59.57,Default,,0000,0000,0000,,Mẹ đâu cần phải khách sáo với hắn như vậy!
Dialogue: 0,0:08:01.51,0:08:03.28,Default,,0000,0000,0000,,Cô ta quả thực rất ngạo mạn.
Dialogue: 0,0:08:04.14,0:08:06.32,Default,,0000,0000,0000,,Mình sẽ dùng "quyển sổ đó" để trừng phạt cô ta.
Dialogue: 0,0:08:07.05,0:08:09.39,Default,,0000,0000,0000,,Ah! Đưa cả bà mẹ Misako đó vào trong "danh sách" luôn mới được.
Dialogue: 0,0:08:13.32,0:08:15.02,Default,,0000,0000,0000,,Mọi chuyện ngày càng trở nên thú vị.
Dialogue: 0,0:08:15.04,0:08:19.54,Default,,0000,0000,0000,,Shiori: Thác loạn trong máy chụp ảnh.
Dialogue: 0,0:08:29.24,0:08:31.64,Default,,0000,0000,0000,,Shiori, chúng ta vào trong nào.
Dialogue: 0,0:08:33.80,0:08:35.06,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:08:40.18,0:08:43.09,Default,,0000,0000,0000,,Khởi động máy giúp thầy nhé.
Dialogue: 0,0:08:43.76,0:08:44.38,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:08:45.52,0:08:48.47,Default,,0000,0000,0000,,"Quyển sổ" đã khiến cô ta trở nên ngoan ngoãn như vậy.
Dialogue: 0,0:08:49.98,0:08:51.92,Default,,0000,0000,0000,,Cứ như vậy thì thật mất hứng.
Dialogue: 0,0:08:55.57,0:08:58.12,Default,,0000,0000,0000,,Huh. Tôi đang ở đâu đây?
Dialogue: 0,0:09:02.89,0:09:03.87,Default,,0000,0000,0000,,Tại sao anh dám...? Ah!
Dialogue: 0,0:09:04.58,0:09:05.79,Default,,0000,0000,0000,,Chỉ là chụp ảnh thôi mà.
Dialogue: 0,0:09:07.56,0:09:09.67,Default,,0000,0000,0000,,Nhưng những tấm ảnh bình thường thì nhàm chán vô cùng.
Dialogue: 0,0:09:10.68,0:09:13.07,Default,,0000,0000,0000,,Tôi muốn chụp ảnh con người dâm đãng bên trong em.
Dialogue: 0,0:09:14.07,0:09:16.98,Default,,0000,0000,0000,,Không được... Buông tôi ra. Tôi...
Dialogue: 0,0:09:18.78,0:09:20.81,Default,,0000,0000,0000,,Thấy kích thích rồi sao? Tôi chưa cưỡng ép gì em đâu nhé.
Dialogue: 0,0:09:22.15,0:09:23.79,Default,,0000,0000,0000,,Sự thật là em đang rất thích thú, phải không nào?
Dialogue: 0,0:09:25.86,0:09:27.91,Default,,0000,0000,0000,,Đồ ngốc, dĩ nhiên là tôi không...
Dialogue: 0,0:09:30.35,0:09:33.14,Default,,0000,0000,0000,,Hả! Cơ thể mình đang tự chuyển động...
Dialogue: 0,0:09:34.25,0:09:35.48,Default,,0000,0000,0000,,Điều gì đang xảy ra thế này?
Dialogue: 0,0:09:36.38,0:09:37.60,Default,,0000,0000,0000,,Không!?
Dialogue: 0,0:09:42.06,0:09:43.66,Default,,0000,0000,0000,,Có vẻ như em muốn làm "chuyện đó".
Dialogue: 0,0:09:43.84,0:09:44.49,Default,,0000,0000,0000,,Sao có thể?
Dialogue: 0,0:09:45.72,0:09:48.87,Default,,0000,0000,0000,,Khôngggggggggggggg!!!
Dialogue: 0,0:09:55.35,0:09:58.50,Default,,0000,0000,0000,,Đau quá, làm ơn rút nó ra!
Dialogue: 0,0:09:59.25,0:10:03.81,Default,,0000,0000,0000,,Thật tuyệt! Cơ thể em cho thấy rằng em muốn tiếp tục chuyện này.
Dialogue: 0,0:10:06.96,0:10:11.57,Default,,0000,0000,0000,,Không, tôi không muốn.
Dialogue: 0,0:10:16.70,0:10:17.84,Default,,0000,0000,0000,,Đừng...
Dialogue: 0,0:10:23.65,0:10:26.14,Default,,0000,0000,0000,,Shiori, cơ thể em có phản ứng khác hẳn những lời nói của em đấy.
Dialogue: 0,0:10:32.88,0:10:35.72,Default,,0000,0000,0000,,Cái của anh đang cọ sát bên trong.
Dialogue: 0,0:10:41.37,0:10:42.91,Default,,0000,0000,0000,,Shiori, sướng ko nào?
Dialogue: 0,0:10:43.37,0:10:44.67,Default,,0000,0000,0000,,Rên to nữa lên nào!
Dialogue: 0,0:10:44.22,0:10:47.52,Default,,0000,0000,0000,,Đau quá. Sensei, làm ơn rút nó ra. Tha cho em!
Dialogue: 0,0:10:45.97,0:10:49.06,Default,,0000,0000,0000,,Không, việc này thật tồi tệ... thật xấu xa...
Dialogue: 0,0:10:49.37,0:10:51.38,Default,,0000,0000,0000,,... thật tuyệt, nóng quá, ah!
Dialogue: 0,0:10:52.30,0:10:53.85,Default,,0000,0000,0000,,Em vẫn muốn tiếp tục chứ...?
Dialogue: 0,0:10:54.51,0:10:57.37,Default,,0000,0000,0000,,...tiếp tục chụp những bức ảnh nghệ thuật này?
Dialogue: 0,0:10:58.57,0:11:03.28,Default,,0000,0000,0000,,Em không thể làm gì khác được.
Dialogue: 0,0:11:03.52,0:11:05.65,Default,,0000,0000,0000,,Tôi sẽ ra bên trong em, Shiori.
Dialogue: 0,0:11:06.13,0:11:10.27,Default,,0000,0000,0000,,Tôi sẽ biến em thành một con đàn bà dâm đãng.
Dialogue: 0,0:11:11.15,0:11:13.24,Default,,0000,0000,0000,,Đừng mà...
Dialogue: 0,0:11:15.37,0:11:17.15,Default,,0000,0000,0000,,Tuyệt quá đi mất... sâu hơn nữa đi anh... ah!
Dialogue: 0,0:11:20.91,0:11:23.94,Default,,0000,0000,0000,,Sensei, cái của anh đang ở sâu trong em.
Dialogue: 0,0:11:28.19,0:11:31.14,Default,,0000,0000,0000,,Không, đừng chụp nữa mà!
Dialogue: 0,0:11:32.66,0:11:33.39,Default,,0000,0000,0000,,Xấu hổ quá.
Dialogue: 0,0:11:33.80,0:11:37.25,Default,,0000,0000,0000,,Đừng chụp ảnh lúc hai chân của em dang ra như vậy.
Dialogue: 0,0:11:38.05,0:11:39.27,Default,,0000,0000,0000,,Em sắp ra chưa hả?
Dialogue: 0,0:11:39.73,0:11:42.35,Default,,0000,0000,0000,,Tuyệt quá, tôi cũng sắp ra rồi!!!
Dialogue: 0,0:11:43.11,0:11:46.48,Default,,0000,0000,0000,,Đừng... ah... chụp ảnh lúc em đang ra...
Dialogue: 0,0:11:47.82,0:11:48.43,Default,,0000,0000,0000,,Đừng mà...
Dialogue: 0,0:11:49.46,0:11:51.24,Default,,0000,0000,0000,,Không... đừng làm mạnh như vậy!
Dialogue: 0,0:11:51.54,0:11:54.08,Default,,0000,0000,0000,,Đừng ra bên trong em.
Dialogue: 0,0:12:03.57,0:12:07.36,Default,,0000,0000,0000,,Sensei, bên trong em đầy ắp tinh dịch của anh.
Dialogue: 0,0:12:08.44,0:12:09.30,Default,,0000,0000,0000,,Nó thật nóng...
Dialogue: 0,0:12:09.66,0:12:13.43,Default,,0000,0000,0000,,Chúng trào hết ra ngoài rồi.
Dialogue: 0,0:12:18.14,0:12:19.83,Default,,0000,0000,0000,,Như vậy là đủ rồi.
Dialogue: 0,0:12:20.75,0:12:22.98,Default,,0000,0000,0000,,Em sẽ mang trong mình giọt máu của tôi, Shiori.
Dialogue: 0,0:12:28.30,0:12:30.26,Default,,0000,0000,0000,,Misako: Trong phòng khách, khi đang nghe điện thoại của chồng.
Dialogue: 0,0:12:39.61,0:12:40.49,Default,,0000,0000,0000,,Vậy à...
Dialogue: 0,0:12:41.50,0:12:42.60,Default,,0000,0000,0000,,Anh vất vả quá.
Dialogue: 0,0:12:43.55,0:12:45.91,Default,,0000,0000,0000,,Khi nào anh về thăm nhà?
Dialogue: 0,0:12:46.74,0:12:49.16,Default,,0000,0000,0000,,Vâng, em hiểu.
Dialogue: 0,0:12:52.48,0:12:55.25,Default,,0000,0000,0000,,Không có cách nào quay về vào ngày thứ bảy ạ.
Dialogue: 0,0:12:57.33,0:12:58.03,Default,,0000,0000,0000,,Vậy là...
Dialogue: 0,0:12:59.95,0:13:02.01,Default,,0000,0000,0000,,Sensei, thầy đang làm gì vậy?
Dialogue: 0,0:13:05.64,0:13:07.22,Default,,0000,0000,0000,,Ah? Chuyện gì thế em?
Dialogue: 0,0:13:07.73,0:13:09.59,Default,,0000,0000,0000,,Không, không có gì đâu ạ.
Dialogue: 0,0:13:13.74,0:13:14.73,Default,,0000,0000,0000,,Làm ơn dừng lại.
Dialogue: 0,0:13:15.06,0:13:16.31,Default,,0000,0000,0000,,Buổi tối tốt lành cô Misako.
Dialogue: 0,0:13:16.76,0:13:18.80,Default,,0000,0000,0000,,Tôi đang gặp chút rắc rối với nó.
Dialogue: 0,0:13:21.09,0:13:23.40,Default,,0000,0000,0000,,Liệu cô có thể giúp tôi xoa dịu nó bằng miệng của cô được không?
Dialogue: 0,0:13:25.61,0:13:27.89,Default,,0000,0000,0000,,Tôi không thể làm làm một việc như vậy được.
Dialogue: 0,0:13:30.26,0:13:31.76,Default,,0000,0000,0000,,Alo? Có ai nghe máy đó không?
Dialogue: 0,0:13:32.58,0:13:35.72,Default,,0000,0000,0000,,Em xin lỗi, có một chút...
Dialogue: 0,0:13:37.85,0:13:40.27,Default,,0000,0000,0000,,Tôi muốn cô...
Dialogue: 0,0:13:44.43,0:13:46.66,Default,,0000,0000,0000,,Xin đừng, hãy dừng việc này lại.
Dialogue: 0,0:13:47.49,0:13:48.41,Default,,0000,0000,0000,,Vô ích thôi.
Dialogue: 0,0:13:49.02,0:13:50.46,Default,,0000,0000,0000,,Chỉ có một cách duy nhất để xoa dịu nó mà thôi.
Dialogue: 0,0:13:50.81,0:13:53.24,Default,,0000,0000,0000,,Đó là làm cho một người đàn ông ra.
Dialogue: 0,0:13:53.93,0:13:54.68,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:13:56.07,0:13:58.17,Default,,0000,0000,0000,,Em sao vậy?
Dialogue: 0,0:13:59.79,0:14:01.72,Default,,0000,0000,0000,,Anh đừng lo, em ổn mà.
Dialogue: 0,0:14:02.32,0:14:04.15,Default,,0000,0000,0000,,Chỉ là em đang xem xét một vài thứ.
Dialogue: 0,0:14:05.99,0:14:08.12,Default,,0000,0000,0000,,Thôi nào, làm đi.
Dialogue: 0,0:14:09.05,0:14:10.85,Default,,0000,0000,0000,,Tôi không thể làm việc này.
Dialogue: 0,0:14:11.11,0:14:13.17,Default,,0000,0000,0000,,Nếu cô còn chần chừ như vậy thì tôi sẽ nói to lên cho anh ta nghe thấy đó.
Dialogue: 0,0:14:15.22,0:14:16.02,Default,,0000,0000,0000,,Nếu như vậy thì...
Dialogue: 0,0:14:16.75,0:14:17.87,Default,,0000,0000,0000,,Thôi được.
Dialogue: 0,0:14:27.51,0:14:30.08,Default,,0000,0000,0000,,Ah? Anh không nghe thấy gì cả.
Dialogue: 0,0:14:31.86,0:14:35.09,Default,,0000,0000,0000,,Em xin lỗi, tại em đang ăn kem.
Dialogue: 0,0:14:42.17,0:14:45.16,Default,,0000,0000,0000,,Xin lỗi vì tiếng ồn, anh yêu!
Dialogue: 0,0:14:55.76,0:14:58.77,Default,,0000,0000,0000,,Misako, cô làm điều này tuyệt thật đấy.
Dialogue: 0,0:15:03.79,0:15:04.62,Default,,0000,0000,0000,,Sâu quá...
Dialogue: 0,0:15:05.08,0:15:07.34,Default,,0000,0000,0000,,Tôi không thể dừng lại được.
Dialogue: 0,0:15:24.05,0:15:26.44,Default,,0000,0000,0000,,Nuốt hết đi, đừng để rớt giọt nào cả.
Dialogue: 0,0:15:30.98,0:15:33.25,Default,,0000,0000,0000,,Mới rồi anh nghe thấy vài tiếng động lạ.
Dialogue: 0,0:15:34.13,0:15:37.33,Default,,0000,0000,0000,,Em xin lỗi, em cảm thấy không được khoẻ.
Dialogue: 0,0:15:44.63,0:15:46.21,Default,,0000,0000,0000,,Làm ơn... dừng lại...
Dialogue: 0,0:15:53.84,0:15:56.78,Default,,0000,0000,0000,,Hãy tha cho tôi, tôi không muốn chuyện này...
Dialogue: 0,0:15:58.45,0:16:01.21,Default,,0000,0000,0000,,Chỗ đó của cô ướt lắm rồi. Có thật là cô không muốn chuyện đó chứ?
Dialogue: 0,0:16:02.00,0:16:06.11,Default,,0000,0000,0000,,Chẳng phải cô đang khẩu dâm qua điện thoại với chồng mình sao.
Dialogue: 0,0:16:06.92,0:16:09.72,Default,,0000,0000,0000,,Tôi hoàn toàn không muốn.
Dialogue: 0,0:16:12.49,0:16:14.14,Default,,0000,0000,0000,,Anh yêu, em xin lỗi!
Dialogue: 0,0:16:15.34,0:16:17.26,Default,,0000,0000,0000,,Chúng tôi vừa mới bàn về chuyến công tác của anh ấy.
Dialogue: 0,0:16:20.94,0:16:21.57,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:16:26.96,0:16:28.24,Default,,0000,0000,0000,,Tôi sẽ ra bên trong cô.
Dialogue: 0,0:16:29.89,0:16:31.08,Default,,0000,0000,0000,,Không, dừng lại.
Dialogue: 0,0:16:32.02,0:16:33.11,Default,,0000,0000,0000,,Không thể...
Dialogue: 0,0:16:33.81,0:16:35.77,Default,,0000,0000,0000,,Anh ấy sẽ nghe thấy mất.
Dialogue: 0,0:16:37.04,0:16:39.07,Default,,0000,0000,0000,,Hãy tha thứ cho em...
Dialogue: 0,0:16:44.47,0:16:45.84,Default,,0000,0000,0000,,Làm ơn... Xin đừng...
Dialogue: 0,0:16:46.83,0:16:52.00,Default,,0000,0000,0000,,Em rên lên mất rồi, anh ta đã ra bên trong em...
Dialogue: 0,0:16:54.08,0:16:55.76,Default,,0000,0000,0000,,Anh yêu! Xin anh tha thứ cho em!
Dialogue: 0,0:16:56.41,0:17:00.02,Default,,0000,0000,0000,,Anh ta đã ra hết bên trong em rồi.
Dialogue: 0,0:17:21.88,0:17:27.41,Default,,0000,0000,0000,,Shiori: đi dạo và khỏa thân ngoài công viên.
Dialogue: 0,0:17:47.07,0:17:50.67,Default,,0000,0000,0000,,Shiori, nghe kĩ những gì tôi sắp nói và làm theo, rõ chứ?
Dialogue: 0,0:17:51.83,0:17:54.37,Default,,0000,0000,0000,,Vâng, thưa chủ nhân.
Dialogue: 0,0:17:56.68,0:17:58.71,Default,,0000,0000,0000,,Cởi áo choàng ra.
Dialogue: 0,0:18:00.07,0:18:01.67,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:18:06.25,0:18:08.71,Default,,0000,0000,0000,,Tốt lắm, em cảm thấy thế nào?
Dialogue: 0,0:18:09.16,0:18:13.36,Default,,0000,0000,0000,,Khỏa thân như vậy ngượng lắm ạ.
Dialogue: 0,0:18:15.67,0:18:20.81,Default,,0000,0000,0000,,Bây giờ, hãy "tưới" cây đi.
Dialogue: 0,0:18:22.49,0:18:23.77,Default,,0000,0000,0000,,Tưới cây...?
Dialogue: 0,0:18:25.55,0:18:29.04,Default,,0000,0000,0000,,Nếu chủ nhân đã yêu cầu như vậy, em sẽ làm.
Dialogue: 0,0:18:37.41,0:18:38.44,Default,,0000,0000,0000,,Đúng là một tư thế thú vị.
Dialogue: 0,0:18:47.15,0:18:50.00,Default,,0000,0000,0000,,Để tôi ngắm nó thật kĩ nào, Shiori.
Dialogue: 0,0:18:50.91,0:18:51.50,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:18:54.42,0:18:55.64,Default,,0000,0000,0000,,Ah, như vậy thật ngượng quá.
Dialogue: 0,0:19:11.37,0:19:14.12,Default,,0000,0000,0000,,Chỗ này... Ah!
Dialogue: 0,0:19:14.64,0:19:18.13,Default,,0000,0000,0000,,Shiori, tôi có bảo em thủ dâm đâu nhỉ?
Dialogue: 0,0:19:19.14,0:19:20.47,Default,,0000,0000,0000,,Vâng... em xin lỗi.
Dialogue: 0,0:19:20.77,0:19:23.43,Default,,0000,0000,0000,,Hừ! Em dám làm vậy ngay trước mặt tôi sao.
Dialogue: 0,0:19:23.81,0:19:27.71,Default,,0000,0000,0000,,Shiori, em thật là dâm đãng.
Dialogue: 0,0:19:28.01,0:19:30.75,Default,,0000,0000,0000,,Em không phải loại người đó.
Dialogue: 0,0:19:31.96,0:19:36.15,Default,,0000,0000,0000,,Thật sao, đáng lẽ ra tôi phải đoán ra từ trước chứ nhỉ!
Dialogue: 0,0:19:36.15,0:19:37.27,Default,,0000,0000,0000,,Đúng là em có nhiều vấn đề thật đấy.
Dialogue: 0,0:19:40.07,0:19:44.69,Default,,0000,0000,0000,,Xin lỗi đã khiến chủ nhân phải quở trách.
Dialogue: 0,0:19:45.27,0:19:46.34,Default,,0000,0000,0000,,Tôi hiểu.
Dialogue: 0,0:19:46.87,0:19:48.66,Default,,0000,0000,0000,,Được rồi, "tưới" cây nhanh lên nào.
Dialogue: 0,0:19:49.55,0:19:50.14,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:20:00.00,0:20:03.75,Default,,0000,0000,0000,,Nó không ngừng tuôn ra, điều này thật tuyệt.
Dialogue: 0,0:20:07.79,0:20:11.01,Default,,0000,0000,0000,,Tôi được chứng kiến một cảnh tượng thú vị, Shiori.
Dialogue: 0,0:20:14.59,0:20:21.38,Default,,0000,0000,0000,,Làm việc này ở nơi công cộng, em thấy thoả mãn chứ?
Dialogue: 0,0:20:21.13,0:20:22.13,Default,,0000,0000,0000,,Em đang đang làm gì thế này?
Dialogue: 0,0:20:28.55,0:20:35.79,Default,,0000,0000,0000,,Em không quan tâm nếu có ai đó nhìn thấy.
Dialogue: 0,0:20:36.21,0:20:43.88,Default,,0000,0000,0000,,Vì đã ngoan ngoãn nghe lời, đây là phần thưởng dành cho em.
Dialogue: 0,0:20:47.72,0:20:51.46,Default,,0000,0000,0000,,Xin hãy trao nó cho em...
Dialogue: 0,0:20:52.13,0:20:53.47,Default,,0000,0000,0000,,Tốt thôi...
Dialogue: 0,0:21:09.26,0:21:11.24,Default,,0000,0000,0000,,Sướng ko hả?
Dialogue: 0,0:21:12.43,0:21:15.59,Default,,0000,0000,0000,,Nó thật tuyệt... nó vào sâu bên trong...
Dialogue: 0,0:21:17.39,0:21:21.98,Default,,0000,0000,0000,,Nếu em rên to như vậy, mọi người sẽ nghe thấy hết đó.
Dialogue: 0,0:21:23.34,0:21:24.32,Default,,0000,0000,0000,,Không sao cả.
Dialogue: 0,0:21:24.62,0:21:28.33,Default,,0000,0000,0000,,Mặc dù điều này là nhục nhã, nhưng em lại thấy phấn khích vô cùng.
Dialogue: 0,0:21:28.74,0:21:32.78,Default,,0000,0000,0000,,Thật là nhục nhã nhưng cũng... thật tuyệt.
Dialogue: 0,0:21:32.84,0:21:34.46,Default,,0000,0000,0000,,Shiori, em đúng là một con đàn bà dâm đãng.
Dialogue: 0,0:21:35.80,0:21:37.89,Default,,0000,0000,0000,,Tôi nên ra ở đâu đây? Bên trong, phải ko nào?
Dialogue: 0,0:21:38.54,0:21:41.14,Default,,0000,0000,0000,,Ở bên trong! Em muốn được mang trong mình giọt máu của chủ nhân...
Dialogue: 0,0:21:41.42,0:21:42.91,Default,,0000,0000,0000,,Làm ơn cho em...
Dialogue: 0,0:21:42.21,0:21:46.18,Default,,0000,0000,0000,,Làm ơn cho em tinh dịch của chủ nhân.
Dialogue: 0,0:21:47.04,0:21:50.63,Default,,0000,0000,0000,,Nhận lấy phần thưởng của em này.
Dialogue: 0,0:21:51.91,0:21:54.77,Default,,0000,0000,0000,,Hãy ra đi, lấp đầy bên trong em.
Dialogue: 0,0:22:05.10,0:22:09.13,Default,,0000,0000,0000,,Bên trong em tràn ngập tinh dịch của chủ nhân.
Dialogue: 0,0:22:11.95,0:22:13.22,Default,,0000,0000,0000,,Tuyệt quá đi mất.
Dialogue: 0,0:22:14.11,0:22:18.22,Default,,0000,0000,0000,,Mình sẽ bị chủ nhân làm cho mang thai giữa chốn công cộng.
Dialogue: 0,0:22:25.49,0:22:31.06,Default,,0000,0000,0000,,Ni: tại bể bơi thành phố.
Dialogue: 0,0:23:01.04,0:23:03.06,Default,,0000,0000,0000,,Cái của thầy đang ở sâu trong em.
Dialogue: 0,0:23:03.57,0:23:05.56,Default,,0000,0000,0000,,Nhiều người đang chứng kiến chúng ta...
Dialogue: 0,0:23:06.06,0:23:09.62,Default,,0000,0000,0000,,Tiếng rên của em thật dâm đãng.
Dialogue: 0,0:23:10.28,0:23:12.12,Default,,0000,0000,0000,,Nhưng em không thể kiềm chế lại được.
Dialogue: 0,0:23:13.02,0:23:16.96,Default,,0000,0000,0000,,Ah, đúng rồi, em muốn được mọi người ngắm nhìn mà.
Dialogue: 0,0:23:23.61,0:23:24.30,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:23:25.56,0:23:26.82,Default,,0000,0000,0000,,Đừng làm như vậy...
Dialogue: 0,0:23:27.39,0:23:31.21,Default,,0000,0000,0000,,Em đoán xem bọn họ đang nghĩ gì nào?
Dialogue: 0,0:23:31.83,0:23:34.81,Default,,0000,0000,0000,,Có lẽ họ muốn xem em thủ dâm đấy.
Dialogue: 0,0:23:35.41,0:23:36.72,Default,,0000,0000,0000,,Không phải như vậy.
Dialogue: 0,0:23:37.41,0:23:40.45,Default,,0000,0000,0000,,Em ko muốn bị họ trông thấy như vậy.
Dialogue: 0,0:23:43.91,0:23:45.54,Default,,0000,0000,0000,,Thầy chắc rằng họ rất muốn thấy...
Dialogue: 0,0:23:45.99,0:23:49.26,Default,,0000,0000,0000,,... em vật vã trong sung sướng và khoái lạc.
Dialogue: 0,0:23:49.57,0:23:53.30,Default,,0000,0000,0000,,Không, em không có dâm đãng như thế.
Dialogue: 0,0:23:53.96,0:23:57.82,Default,,0000,0000,0000,,Ngoan nào, ngồi lên thành bể và quay mặt lại.
Dialogue: 0,0:23:58.93,0:23:59.51,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:24:18.45,0:24:20.34,Default,,0000,0000,0000,,Vẻ mặt em thật dâm đãng.
Dialogue: 0,0:24:20.78,0:24:22.37,Default,,0000,0000,0000,,Ham muốn của em cũng rất mãnh liệt.
Dialogue: 0,0:24:27.80,0:24:31.60,Default,,0000,0000,0000,,Đến lúc phải ra rồi.
Dialogue: 0,0:24:34.76,0:24:38.33,Default,,0000,0000,0000,,Tốt thôi, hãy để bọn họ chứng kiến.
Dialogue: 0,0:24:39.05,0:24:41.68,Default,,0000,0000,0000,,Không, đừng làm vậy...
Dialogue: 0,0:24:44.02,0:24:46.72,Default,,0000,0000,0000,,Ni, trước mặt mọi người thầy sẽ làm cho em có thai.
Dialogue: 0,0:24:47.50,0:24:48.99,Default,,0000,0000,0000,,Xin đừng nhin tôi.
Dialogue: 0,0:24:49.33,0:24:52.55,Default,,0000,0000,0000,,Mọi người, làm ơn đừng nhìn.
Dialogue: 0,0:25:01.54,0:25:02.22,Default,,0000,0000,0000,,Thầy ra rồi...
Dialogue: 0,0:25:02.65,0:25:04.51,Default,,0000,0000,0000,,Em đã mang thai rồi sao?
Dialogue: 0,0:25:05.21,0:25:08.61,Default,,0000,0000,0000,,Trước mặt bao nhiêu người... mang thai sao.
Dialogue: 0,0:25:14.84,0:25:17.47,Default,,0000,0000,0000,,Vào trong phòng tắm nào.
Dialogue: 0,0:25:18.48,0:25:19.19,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:25:41.10,0:25:44.48,Default,,0000,0000,0000,,Nếu em còn kêu to như vậy, chúng ta sẽ bị phát hiện đấy.
Dialogue: 0,0:25:44.89,0:25:48.93,Default,,0000,0000,0000,,Không, nếu bị nhìn thấy trong lúc như này, chúng ta sẽ...
Dialogue: 0,0:25:49.84,0:25:53.01,Default,,0000,0000,0000,,Nhưng thật sướng quá, em không thể ngừng mình lại.
Dialogue: 0,0:25:53.32,0:25:55.49,Default,,0000,0000,0000,,Sẽ bị phát hiện, chúng ta sẽ bị phát hiện...
Dialogue: 0,0:25:57.04,0:25:59.26,Default,,0000,0000,0000,,Em muốn bị phát hiện, phải không?
Dialogue: 0,0:26:00.15,0:26:03.60,Default,,0000,0000,0000,,Sensei, sự thực là em muốn mọi người chứng kiến thầy làm chuyện này với em.
Dialogue: 0,0:26:04.11,0:26:06.05,Default,,0000,0000,0000,,Em muốn mọi người chứng kiến giây phút em mang thai.
Dialogue: 0,0:26:07.42,0:26:09.32,Default,,0000,0000,0000,,Hãy khiến em có thai đi!
Dialogue: 0,0:26:09.98,0:26:10.62,Default,,0000,0000,0000,,Hãy khiến em có thai đi!
Dialogue: 0,0:26:11.06,0:26:13.94,Default,,0000,0000,0000,,Hãy cho em thật nhiều tinh dịch nhờn và ấm áp của thầy.
Dialogue: 0,0:26:14.22,0:26:16.71,Default,,0000,0000,0000,,Hãy khiến em có thai đi!
Dialogue: 0,0:26:24.02,0:26:28.39,Default,,0000,0000,0000,,Em rất hạnh phúc được mang trong mình giọt máu của thầy, sensei.
Dialogue: 0,0:27:15.87,0:27:18.76,Default,,0000,0000,0000,,Cô ta đã trở thành nô lệ trung thành của mình.
Dialogue: 0,0:27:20.70,0:27:21.44,Default,,0000,0000,0000,,Bây giờ...
Dialogue: 0,0:27:23.00,0:27:24.06,Default,,0000,0000,0000,,tới lượt hai người bọn họ...
