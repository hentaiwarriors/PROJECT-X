Dialogue: 0,0:00:39.17,0:00:44.23,Default,,0000,0000,0000,,Tinh dịch của con đang tràn ngập bên trong mẹ!
Dialogue: 0,0:00:57.95,0:01:01.79,Default,,0000,0000,0000,,Chà, có vẻ như mẹ chưa từng thủ dâm với nơi này...
Dialogue: 0,0:01:01.89,0:01:04.22,Default,,0000,0000,0000,,Nhưng mẹ ngất đi thế này khiến con bất ngờ đấy.
Dialogue: 0,0:01:06.93,0:01:10.33,Default,,0000,0000,0000,,Giờ làm cách nào để khiến nó thoải mái đây nhỉ?
Dialogue: 0,0:01:11.77,0:01:14.76,Default,,0000,0000,0000,,À, tự nhiên lại thấy đói bụng.
Dialogue: 0,0:01:15.47,0:01:20.00,Default,,0000,0000,0000,,Hẳn cô ta sẽ rất vui mừng chuẩn bị bữa tối cho con.
Dialogue: 0,0:01:20.64,0:01:22.70,Default,,0000,0000,0000,,Mẹ xài thứ này thế chỗ thằng nhỏ của con một lúc nhé.
Dialogue: 0,0:01:32.82,0:01:36.99,credits,,0000,0000,0000,,{\pos(460,20)}Translated By Invincible_Dark_Thunder
Dialogue: 0,0:01:32.82,0:01:36.99,credits,,0000,0000,0000,,{\pos(82,20)}Baka-sub.net
Dialogue: 0,0:01:32.82,0:01:36.99,title,,0000,0000,0000,,Chị em song sinh
Dialogue: 0,0:01:38.09,0:01:41.79,title2,,0000,0000,0000,,{\pos(315,330)\fad(0,1000)}Vol 2: Bản Concerto hoan lạc
Dialogue: 0,0:01:50.04,0:01:51.97,Default,,0000,0000,0000,,Chuyện gì vậy? Cậu thỏa mãn rồi sao?
Dialogue: 0,0:01:53.11,0:01:55.91,Default,,0000,0000,0000,,Ko, cô muốn ra ban công hóng gió chứ?
Dialogue: 0,0:01:56.01,0:01:57.74,Default,,0000,0000,0000,,Đêm nay trời nhiều sao lắm đấy.
Dialogue: 0,0:01:57.98,0:01:59.17,Default,,0000,0000,0000,,Ý cậu là?
Dialogue: 0,0:01:59.35,0:02:01.78,Default,,0000,0000,0000,,Chúng ta ko thể cứ như vậy mà ra ngoài đó được.
Dialogue: 0,0:02:02.32,0:02:03.84,Default,,0000,0000,0000,,Cô ko biết sao?
Dialogue: 0,0:02:03.95,0:02:07.55,Default,,0000,0000,0000,,Giờ này thì làm gì còn ai ở ngoài đó nữa.
Dialogue: 0,0:02:08.86,0:02:10.48,Default,,0000,0000,0000,,Phải, nhưng mà...
Dialogue: 0,0:02:11.33,0:02:13.09,Default,,0000,0000,0000,,Thôi nào.
Dialogue: 0,0:02:13.23,0:02:14.39,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:02:15.13,0:02:16.96,Default,,0000,0000,0000,,Cậu đang ép buộc tôi.
Dialogue: 0,0:02:33.31,0:02:34.68,Default,,0000,0000,0000,,Chuyện này là sao...
Dialogue: 0,0:02:35.18,0:02:36.95,Default,,0000,0000,0000,,Cậu ta đã nhét thứ này vào bên trong mình...
Dialogue: 0,0:02:45.23,0:02:46.78,Default,,0000,0000,0000,,Cảm giác tuyệt quá...
Dialogue: 0,0:02:56.44,0:02:58.80,Default,,0000,0000,0000,,Sao vậy? Trông cô có vẻ ko thoải mái.
Dialogue: 0,0:02:58.91,0:03:03.50,Default,,0000,0000,0000,,Chúng ta đâu thể to tiếng ở nơi này được.
Dialogue: 0,0:03:04.14,0:03:05.94,Default,,0000,0000,0000,,Đừng lo lắng về điều đó.
Dialogue: 0,0:03:06.28,0:03:10.44,Default,,0000,0000,0000,,Có nhiều đôi mới cưới sống ở tòa nhà này.
Dialogue: 0,0:03:10.62,0:03:12.28,Default,,0000,0000,0000,,Vậy nên chẳng ai quan tâm đến tiếng rên của cô đâu.
Dialogue: 0,0:03:12.45,0:03:13.82,Default,,0000,0000,0000,,Gì thế này?
Dialogue: 0,0:03:13.92,0:03:15.05,Default,,0000,0000,0000,,Thật đáng xấu hổ...
Dialogue: 0,0:03:16.06,0:03:18.49,Default,,0000,0000,0000,,Sao hả? Thấy sướng ko?
Dialogue: 0,0:03:18.83,0:03:21.19,Default,,0000,0000,0000,,Sướng, sướng lắm!
Dialogue: 0,0:03:22.23,0:03:23.82,Default,,0000,0000,0000,,Đây là sự thật sao?
Dialogue: 0,0:03:23.93,0:03:26.09,Default,,0000,0000,0000,,Cậu ta cũng làm chuyện này với Yuko-chan ư?
Dialogue: 0,0:03:26.20,0:03:28.69,Default,,0000,0000,0000,,Tại sao cậu ta lại làm như vậy?
Dialogue: 0,0:03:31.00,0:03:33.77,Default,,0000,0000,0000,,Mẹ tỉnh rồi à.
Dialogue: 0,0:03:33.91,0:03:35.31,Default,,0000,0000,0000,,Vậy thì...
Dialogue: 0,0:03:49.52,0:03:52.01,Default,,0000,0000,0000,,Khoan đã, tôi vừa nghe tiếng ai đó...
Dialogue: 0,0:03:53.16,0:03:54.18,Default,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:03:54.29,0:03:56.29,Default,,0000,0000,0000,,Có lẽ ai đó đang nhìn trộm chúng ta.
Dialogue: 0,0:03:56.70,0:03:58.89,Default,,0000,0000,0000,,Chắc hẳn người đó đang thủ dâm trong lúc nhìn trộm chúng ta.
Dialogue: 0,0:04:01.94,0:04:04.17,Default,,0000,0000,0000,,Để tôi chăm sóc bộ ngực cô kĩ càng chút nữa nhé.
Dialogue: 0,0:04:04.57,0:04:07.51,Default,,0000,0000,0000,,Ko, đừng bóp mạnh như vậy!
Dialogue: 0,0:04:11.38,0:04:14.40,Default,,0000,0000,0000,,Chẳng phải mông cô đang nhịp mạnh hơn sao.
Dialogue: 0,0:04:14.85,0:04:16.61,Default,,0000,0000,0000,,Thật khó tin...
Dialogue: 0,0:04:16.75,0:04:18.31,Default,,0000,0000,0000,,Nước của cô đang tuôn ra ko ngớt này.
Dialogue: 0,0:04:18.45,0:04:20.39,Default,,0000,0000,0000,,Đừng nói vậy chứ.
Dialogue: 0,0:04:22.69,0:04:24.12,Default,,0000,0000,0000,,Ko, làm ơn dừng lại đi...
Dialogue: 0,0:04:24.36,0:04:27.05,Default,,0000,0000,0000,,Ai đó làm ơn đánh thức tôi khỏi cơn ác mộng này!
Dialogue: 0,0:04:49.02,0:04:52.18,Default,,0000,0000,0000,,Chúa ơi, nó còn bự hơn cả lúc trước
Dialogue: 0,0:04:52.32,0:04:53.38,Default,,0000,0000,0000,,Tôi sắp ra rồi...
Dialogue: 0,0:04:54.35,0:04:55.75,Default,,0000,0000,0000,,Hứng thú thật đấy!
Dialogue: 0,0:04:59.99,0:05:02.15,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:05:02.26,0:05:04.13,Default,,0000,0000,0000,,Tôi ra rồi, tôi ra rồi!
Dialogue: 0,0:05:04.56,0:05:05.96,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:05:28.52,0:05:29.68,Default,,0000,0000,0000,,Sao hả?
Dialogue: 0,0:05:29.82,0:05:32.95,Default,,0000,0000,0000,,Mẹ nghĩ gì khi chứng kiến\NYuko-san và con làm chuyện đó?
Dialogue: 0,0:05:36.80,0:05:38.99,Default,,0000,0000,0000,,Nói gì đi chứ?
Dialogue: 0,0:05:45.67,0:05:48.73,Default,,0000,0000,0000,,Ah, mẹ thỏa mãn trong tư thế này lâu rồi hả?
Dialogue: 0,0:05:57.82,0:05:59.88,Default,,0000,0000,0000,,Trông mẹ bơ phờ quá đấy.
Dialogue: 0,0:06:00.59,0:06:01.75,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:06:02.16,0:06:04.72,Default,,0000,0000,0000,,Tại sao con lại làm chuyện tồi bại đó với Yuko-chan?
Dialogue: 0,0:06:05.33,0:06:06.72,Default,,0000,0000,0000,,Tồi bại?
Dialogue: 0,0:06:06.86,0:06:08.56,Default,,0000,0000,0000,,Cô ta tự nguyện đấy chứ.
Dialogue: 0,0:06:09.56,0:06:13.47,Default,,0000,0000,0000,,Mẹ thừa nhận, sự thật đó đã vượt quá sức chịu đựng của con.
Dialogue: 0,0:06:14.07,0:06:17.50,Default,,0000,0000,0000,,Nhưng tại sao con lại trút giận dữ của mình lên đầu người khác?
Dialogue: 0,0:06:17.97,0:06:20.13,Default,,0000,0000,0000,,Lúc này con cảm thấy cô đơn lắm phải ko?
Dialogue: 0,0:06:20.34,0:06:21.86,Default,,0000,0000,0000,,Mẹ đang ám chỉ điều gì?
Dialogue: 0,0:06:22.14,0:06:27.17,Default,,0000,0000,0000,,Con ko thể lấy lại những gì đã mất trong quá khứ,\Nnhưng tương lai luôn rộng mở với con.
Dialogue: 0,0:06:27.61,0:06:30.88,Default,,0000,0000,0000,,Cám ơn lời khuyên của mẹ, có điều...
Dialogue: 0,0:06:34.72,0:06:36.71,Default,,0000,0000,0000,,Ko, con định làm...
Dialogue: 0,0:06:37.42,0:06:40.95,Default,,0000,0000,0000,,Mẹ đang cố giải thích vì sao âm hộ mẹ lại ướt đẫm đấy à?!
Dialogue: 0,0:06:41.80,0:06:43.89,Default,,0000,0000,0000,,Đó là bởi vì...
Dialogue: 0,0:06:45.77,0:06:48.03,Default,,0000,0000,0000,,Mẹ vẫn chưa hiểu ra sao?
Dialogue: 0,0:06:49.87,0:06:51.30,Default,,0000,0000,0000,,Minoru-san...
Dialogue: 0,0:07:04.45,0:07:08.08,Default,,0000,0000,0000,,Vừa bị con cướp mất đời con gái mà mẹ vẫn thích thú thế này sao?
Dialogue: 0,0:07:08.19,0:07:10.09,Default,,0000,0000,0000,,Ko phải...
Dialogue: 0,0:07:11.69,0:07:13.35,Default,,0000,0000,0000,,Thái độ của mẹ khá hơn rồi đó.
Dialogue: 0,0:07:16.60,0:07:20.03,Default,,0000,0000,0000,,Sao mẹ phải kiềm chế lâu như vậy chứ? Thư giãn đi nào.
Dialogue: 0,0:07:20.13,0:07:23.30,Default,,0000,0000,0000,,Sao mẹ ko rên thật to giống Yuko-san?
Dialogue: 0,0:07:37.72,0:07:39.69,Default,,0000,0000,0000,,Thật cứng đầu.
Dialogue: 0,0:07:39.79,0:07:41.85,Default,,0000,0000,0000,,Xem mẹ chịu đựng được bao lâu.
Dialogue: 0,0:07:53.63,0:07:55.46,Default,,0000,0000,0000,,Con thích cảm giác này.
Dialogue: 0,0:07:56.20,0:07:58.40,Default,,0000,0000,0000,,Làm ơn dừng lại!
Dialogue: 0,0:08:00.74,0:08:02.87,Default,,0000,0000,0000,,Oh, mẹ thấy thích thú rồi hả?
Dialogue: 0,0:08:02.94,0:08:05.00,Default,,0000,0000,0000,,Vậy thì con phải làm chuyện này nhiệt tình hơn nữa mới được!
Dialogue: 0,0:08:23.23,0:08:29.07,Default,,0000,0000,0000,,Mẹ ko thấy xấu hổ khi là người thỏa mãn trước tiên\Nsau khi đã cố tỏ ra ko có gì sao?
Dialogue: 0,0:08:30.50,0:08:32.17,Default,,0000,0000,0000,,Mẹ đang nghe con nói đấy chứ?
Dialogue: 0,0:08:32.27,0:08:33.64,Default,,0000,0000,0000,,Ahm...
Dialogue: 0,0:08:44.89,0:08:47.91,Default,,0000,0000,0000,,Chuyện gì đây? Con tưởng mẹ vẫn còn giữ được chút tỉnh táo chứ...
Dialogue: 0,0:08:48.02,0:08:50.18,Default,,0000,0000,0000,,...của mẹ khít đến mức này rồi sao.
Dialogue: 0,0:08:50.32,0:08:51.92,Default,,0000,0000,0000,,Mẹ đích thị là một con đàn bà dâm đãng.
Dialogue: 0,0:08:55.60,0:08:57.32,Default,,0000,0000,0000,,Dù mẹ đang cố để ko rên to lên...
Dialogue: 0,0:08:57.43,0:08:59.92,Default,,0000,0000,0000,,Nhưng cơ thể mẹ vẫn biết cần phải khít lại ở đâu.
Dialogue: 0,0:09:11.51,0:09:13.04,Default,,0000,0000,0000,,Ok, con ra đây!
Dialogue: 0,0:09:51.82,0:09:54.38,Default,,0000,0000,0000,,Con muốn mẹ mặc thứ này trong cuộc gặp mặt tay ba hôm nay.
Dialogue: 0,0:09:55.19,0:09:57.16,Default,,0000,0000,0000,,Mẹ ko thể mặc thứ này khi tới đó được.
Dialogue: 0,0:09:57.99,0:10:03.40,Default,,0000,0000,0000,,Buổi gặp mặt tay ba hôm nay là để tìm cách giúp con cải thiện điểm số!
Dialogue: 0,0:10:03.60,0:10:06.12,Default,,0000,0000,0000,,Mẹ cũng có trách nhiệm trong chuyện này!
Dialogue: 0,0:10:14.74,0:10:16.30,Default,,0000,0000,0000,,Mẹ làm gì vậy?
Dialogue: 0,0:10:16.64,0:10:19.64,Default,,0000,0000,0000,,Mặc như vậy sẽ khiến người khác chú ý đấy.
Dialogue: 0,0:10:19.75,0:10:21.15,Default,,0000,0000,0000,,Hãy quay ngược nó lại.
Dialogue: 0,0:10:22.58,0:10:25.31,Default,,0000,0000,0000,,Cở quần lót mẹ ra và giấu nó vào bên trong âm hộ mẹ.
Dialogue: 0,0:10:25.42,0:10:26.85,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:10:28.89,0:10:31.79,Default,,0000,0000,0000,,Con biết là mẹ thích xài đồ chơi mà.
Dialogue: 0,0:10:46.24,0:10:48.90,sign,,0000,0000,0000,,{\an8\i1\b1\c&HFFFFFF&}Phòng tư vấn{\i0}
Dialogue: 0,0:10:54.85,0:10:57.21,Default,,0000,0000,0000,,Từ giờ tới lúc kết quả cậu ấy được cải thiện...
Dialogue: 0,0:10:57.32,0:11:00.62,Default,,0000,0000,0000,,Ngoài nỗ lực của cậu ấy còn cần tới sự hỗ trợ từ phía gia đình, đặc biệt là từ chị.
Dialogue: 0,0:11:01.22,0:11:03.31,Default,,0000,0000,0000,,Cậu ấy có thể cải thiện điểm số qua bài kiểm tra bổ trợ.
Dialogue: 0,0:11:03.56,0:11:05.99,Default,,0000,0000,0000,,Cám ơn thầy rất nhiều.
Dialogue: 0,0:11:06.66,0:11:09.82,Default,,0000,0000,0000,,Nhận tiện, trông chị ko được khỏe.
Dialogue: 0,0:11:10.06,0:11:11.66,Default,,0000,0000,0000,,Hôm qua chị lại trực đêm à?
Dialogue: 0,0:11:11.87,0:11:14.83,Default,,0000,0000,0000,,Ko, ko hẳn vậy.
Dialogue: 0,0:11:15.47,0:11:17.70,Default,,0000,0000,0000,,Làm nghề này thật khó mà giữ được nhan sắc.
Dialogue: 0,0:11:18.30,0:11:21.47,Default,,0000,0000,0000,,Nếu chị ko được khỏe, ta có thể bàn tiếp chuyện này vào hôm khác.
Dialogue: 0,0:11:21.78,0:11:24.64,Default,,0000,0000,0000,,Ko, thật sự ko có vấn đề gì đâu ạ.
Dialogue: 0,0:11:24.78,0:11:26.51,Default,,0000,0000,0000,,Xin thầy bỏ quá cho.
Dialogue: 0,0:11:32.32,0:11:34.98,Default,,0000,0000,0000,,Này, điện thoại chị reo kìa.
Dialogue: 0,0:11:35.06,0:11:37.46,Default,,0000,0000,0000,,Chị cứ nghe điện thoại nếu muốn.
Dialogue: 0,0:11:37.72,0:11:39.92,Default,,0000,0000,0000,,Chắc là điện thoại từ bệnh viện.
Dialogue: 0,0:11:40.09,0:11:41.79,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:11:43.20,0:11:45.39,Default,,0000,0000,0000,,Kusunoki-san?
Dialogue: 0,0:11:45.90,0:11:47.89,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:12:05.45,0:12:08.72,Default,,0000,0000,0000,,Chị ko nghe điện thoại sao?
Dialogue: 0,0:12:08.92,0:12:10.48,Default,,0000,0000,0000,,Chị cứ tự nhiên, tôi ko phiền đâu.
Dialogue: 0,0:12:12.89,0:12:13.82,Default,,0000,0000,0000,,Thôi chết...
Dialogue: 0,0:12:14.63,0:12:16.89,Default,,0000,0000,0000,,Xin lỗi thầy, là điện thoại của em.
Dialogue: 0,0:12:17.10,0:12:18.69,Default,,0000,0000,0000,,Kusunoki...
Dialogue: 0,0:12:18.93,0:12:21.59,Default,,0000,0000,0000,,Em muốn thầy từ điểm bài bổ trợ hả?
Dialogue: 0,0:12:23.20,0:12:24.47,Default,,0000,0000,0000,,Thành thật xin lỗi thầy ạ.
Dialogue: 0,0:12:24.90,0:12:28.67,Default,,0000,0000,0000,,Lẽ ra con phải nhấc máy ngay chứ.
Dialogue: 0,0:12:35.55,0:12:38.28,Default,,0000,0000,0000,,Này, mẹ giận con đấy à?
Dialogue: 0,0:12:38.38,0:12:40.48,Default,,0000,0000,0000,,Con là người gây ra tất cả mọi chuyện.
Dialogue: 0,0:12:41.12,0:12:44.02,Default,,0000,0000,0000,,Đừng nặng lời với con như vậy.
Dialogue: 0,0:12:44.12,0:12:45.85,Default,,0000,0000,0000,,Nhìn xem chúng ta đang ở đâu nào.
Dialogue: 0,0:12:46.13,0:12:48.89,Default,,0000,0000,0000,,Đáng lẽ ra con ko nên làm chuyện đó ở nơi công cộng như thế.
Dialogue: 0,0:12:58.20,0:12:59.93,Default,,0000,0000,0000,,Mẹ nghĩ mình là ai chứ?
Dialogue: 0,0:13:00.31,0:13:02.30,Default,,0000,0000,0000,,Con đã nhân nhượng với mẹ đủ lắm rồi!
Dialogue: 0,0:13:05.88,0:13:09.34,Default,,0000,0000,0000,,Mẹ định ra vẻ nhân từ và độ lượng đến khi nào hả?
Dialogue: 0,0:13:13.09,0:13:16.39,Default,,0000,0000,0000,,Thầy giáo con đẹp trai chứ hả?
Dialogue: 0,0:13:24.20,0:13:28.46,Default,,0000,0000,0000,,Mẹ định quyến rũ ông ta dù đang giữ trong mình món đồ chơi đó sao.
Dialogue: 0,0:13:28.74,0:13:30.29,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:13:31.10,0:13:32.13,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:13:32.94,0:13:34.46,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:13:41.15,0:13:45.48,Default,,0000,0000,0000,,Có có thể điềm nhiên bàn về kết quả học tập của con dù âm hộ đã ướt đẫm thế này ư?
Dialogue: 0,0:13:45.65,0:13:46.35,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:13:52.43,0:13:53.95,Default,,0000,0000,0000,,Con định làm gì?
Dialogue: 0,0:13:54.36,0:13:57.63,Default,,0000,0000,0000,,Nơi này là trường học. Là nơi để học tập và...
Dialogue: 0,0:13:57.96,0:14:00.76,Default,,0000,0000,0000,,Nếu mẹ nói vậy, hãy coi đây là một phần của buổi học.
Dialogue: 0,0:14:00.90,0:14:03.66,Default,,0000,0000,0000,,sao cũng được, chúng ta sẽ cùng nhau làm những điều thú vị ở nơi này.
Dialogue: 0,0:14:10.11,0:14:11.47,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:14:14.05,0:14:15.64,Default,,0000,0000,0000,,Con định làm gì?
Dialogue: 0,0:14:16.48,0:14:17.51,Default,,0000,0000,0000,,Trông ngon thật đấy.
Dialogue: 0,0:14:18.45,0:14:21.65,Default,,0000,0000,0000,,Mẹ vẫn điềm nhiên trò chuyện với thầy giáo trong tình trạng này khiến con bất ngờ đấy.
Dialogue: 0,0:14:22.76,0:14:23.81,Default,,0000,0000,0000,,Đừng nói nữa.
Dialogue: 0,0:14:26.49,0:14:31.45,Default,,0000,0000,0000,,Mà thế này, chiếc ghế sẽ ướt sũng mất.
Dialogue: 0,0:14:31.60,0:14:33.53,Default,,0000,0000,0000,,Nó sẽ bốc mùi ngay bây giờ thôi.
Dialogue: 0,0:14:34.67,0:14:35.57,Default,,0000,0000,0000,,Gì cơ?
Dialogue: 0,0:14:39.57,0:14:40.66,Default,,0000,0000,0000,,Ouch...
Dialogue: 0,0:14:41.77,0:14:43.40,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:14:43.54,0:14:45.60,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:14:55.32,0:14:56.88,Default,,0000,0000,0000,,Nơi này là trường học.
Dialogue: 0,0:15:07.17,0:15:10.36,Default,,0000,0000,0000,,Dù mẹ kêu đau, con cũng sẽ ko dừng lại lúc này đâu!
Dialogue: 0,0:15:10.64,0:15:13.40,Default,,0000,0000,0000,,Tốt hơn cả là mẹ nên cầu nguyện để nó dần dễ chịu hơn đi!
Dialogue: 0,0:15:14.64,0:15:16.37,Default,,0000,0000,0000,,Con cảm thấy cái đồ chơi đó rồi!
Dialogue: 0,0:15:43.00,0:15:44.73,Default,,0000,0000,0000,,Chà, no thật.
Dialogue: 0,0:15:45.87,0:15:48.20,Default,,0000,0000,0000,,Mẹ con ta đi dạo một lúc nhé.
Dialogue: 0,0:16:03.46,0:16:05.98,Default,,0000,0000,0000,,Ha, mình thích nơi này.
Dialogue: 0,0:16:06.16,0:16:09.25,Default,,0000,0000,0000,,Với mình, đây là nơi tuyệt vời nhất.
Dialogue: 0,0:16:12.77,0:16:16.57,Default,,0000,0000,0000,,Mẹ quỳ xuống và quay mông về phía này đi!
Dialogue: 0,0:16:17.30,0:16:19.29,Default,,0000,0000,0000,,Ko! Con định làm gì?
Dialogue: 0,0:16:19.54,0:16:21.47,Default,,0000,0000,0000,,Định làm gì ư?
Dialogue: 0,0:16:21.84,0:16:24.00,Default,,0000,0000,0000,,Mẹ thừa biết con muốn gì mà.
Dialogue: 0,0:16:24.78,0:16:26.90,Default,,0000,0000,0000,,Ko, chúng ta ko thể làm chuyện đó ở đây được!
Dialogue: 0,0:16:27.28,0:16:28.51,Default,,0000,0000,0000,,Đừng chống cự.
Dialogue: 0,0:16:28.65,0:16:30.78,Default,,0000,0000,0000,,Mẹ ko muốn con làm rách quần áo mẹ chứ.
Dialogue: 0,0:16:33.55,0:16:35.65,Default,,0000,0000,0000,,Ko, chúng ta ko thể làm nó ở đây được!
Dialogue: 0,0:16:37.49,0:16:40.08,Default,,0000,0000,0000,,Nhìn xem này, mẹ ướt đẫm cả rồi!
Dialogue: 0,0:16:40.19,0:16:43.89,Default,,0000,0000,0000,,Cái đồ chơi bị tắt rồi mà,\Ngiờ nó chỉ nằm yên bên trong mẹ thôi...
Dialogue: 0,0:16:44.53,0:16:48.36,Default,,0000,0000,0000,,Chắc mẹ đang lo có ai đó nhìn thấy chúng ta?
Dialogue: 0,0:16:48.63,0:16:50.40,Default,,0000,0000,0000,,Tất nhiên rồi.
Dialogue: 0,0:16:50.70,0:16:54.33,Default,,0000,0000,0000,,Dù nơi này nằm khuất trong công viên, ai đó vẫn có thể tình cờ thấy chúng ta!
Dialogue: 0,0:16:54.84,0:16:57.71,Default,,0000,0000,0000,,Nhưng mẹ thích vậy phải ko?
Dialogue: 0,0:16:58.38,0:16:59.85,Default,,0000,0000,0000,,Tại sao lại...
Dialogue: 0,0:17:00.45,0:17:02.31,Default,,0000,0000,0000,,Tại sao là sao?
Dialogue: 0,0:17:02.48,0:17:04.31,Default,,0000,0000,0000,,Mẹ thích điều đó mà?
Dialogue: 0,0:17:04.42,0:17:06.65,Default,,0000,0000,0000,,Mẹ ko thích gì cả...
Dialogue: 0,0:17:06.75,0:17:08.08,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:17:08.19,0:17:10.99,Default,,0000,0000,0000,,Vậy mẹ giải thích chỗ nước này thế nào đây?
Dialogue: 0,0:17:11.39,0:17:14.09,Default,,0000,0000,0000,,Ko, nếu con làm mạnh như vậy...
Dialogue: 0,0:17:15.26,0:17:19.06,Default,,0000,0000,0000,,Dù nó có bị xước hay toạc ra cũng ko liên quan tới con.
Dialogue: 0,0:17:19.67,0:17:21.06,Default,,0000,0000,0000,,Con thật độc ác!
Dialogue: 0,0:17:21.63,0:17:24.10,Default,,0000,0000,0000,,Mà trông mẹ có bất mãn gì đâu nhỉ.
Dialogue: 0,0:17:33.08,0:17:35.60,Default,,0000,0000,0000,,Thằng nhỏ của con bị hậu môn mẹ làm bẩn rồi.
Dialogue: 0,0:17:35.85,0:17:37.78,Default,,0000,0000,0000,,Liếm sạch nó dùm con nào mẹ.
Dialogue: 0,0:17:39.65,0:17:42.18,Default,,0000,0000,0000,,Đừng hiểu sai ý con.
Dialogue: 0,0:17:42.29,0:17:44.88,Default,,0000,0000,0000,,Hãy làm nó vì tình cảm giữa hai mẹ con ta.
Dialogue: 0,0:17:46.36,0:17:49.85,Default,,0000,0000,0000,,Dù con và mẹ ko có quan hệ ruột thịt, con vẫn muốn hai chúng ta yêu thương và gần gũi nhau như trước.
Dialogue: 0,0:17:49.96,0:17:53.92,Default,,0000,0000,0000,,Con muốn chúng ta gần gũi nhau như một gia đình thực thụ.
Dialogue: 0,0:18:31.64,0:18:33.57,Default,,0000,0000,0000,,Mẹ đúng là một người đàn bà hư hỏng.
Dialogue: 0,0:18:33.71,0:18:35.64,Default,,0000,0000,0000,,Mẹ lại cảm thấy sung sướng nữa sao?
Dialogue: 0,0:18:35.74,0:18:38.90,Default,,0000,0000,0000,,Ko phải như vậy...
Dialogue: 0,0:18:39.25,0:18:40.68,Default,,0000,0000,0000,,Thật hả?
Dialogue: 0,0:18:46.02,0:18:49.04,Default,,0000,0000,0000,,Con thừa biết mẹ bị chứng bạo dâm.
Dialogue: 0,0:19:06.54,0:19:09.80,Default,,0000,0000,0000,,Hiểu rồi, mẹ kích thích lắm rồi phải ko?
Dialogue: 0,0:19:10.58,0:19:13.70,Default,,0000,0000,0000,,Ko, ko!
Dialogue: 0,0:19:14.28,0:19:16.41,Default,,0000,0000,0000,,Vì vậy mà âm hộ mẹ đột nhiên khít lại như vậy.
Dialogue: 0,0:19:17.15,0:19:19.85,Default,,0000,0000,0000,,Con sắp ra rồi đây...
Dialogue: 0,0:19:19.99,0:19:22.15,Default,,0000,0000,0000,,Con sẽ ra vào bên trong mẹ, hãy đón nhận nó.
Dialogue: 0,0:19:22.36,0:19:23.62,Default,,0000,0000,0000,,Ko, bỏ ra đi!
Dialogue: 0,0:19:24.89,0:19:26.19,Default,,0000,0000,0000,,Con ra đây, Tomoe!
Dialogue: 0,0:19:26.29,0:19:27.73,Default,,0000,0000,0000,,Con sẽ ra vào bên trong!
Dialogue: 0,0:19:31.03,0:19:33.36,Default,,0000,0000,0000,,Ra vào bên trong mẹ...
Dialogue: 0,0:19:33.47,0:19:36.56,Default,,0000,0000,0000,,Dừng lại! Đừng làm thế!
Dialogue: 0,0:19:37.87,0:19:40.60,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:19:52.85,0:19:54.75,Default,,0000,0000,0000,,Oh, tin nhắn của Akane.
Dialogue: 0,0:19:55.66,0:20:00.22,Default,,0000,0000,0000,,Mình đối xử tàn nhẫn với cô ta như thế mà cô ta vẫn còn hứng thú đến vậy sao...
Dialogue: 0,0:20:07.63,0:20:11.26,Default,,0000,0000,0000,,Sựu thật đúng là Akane đã mang thai sau khi bị cưỡng bức.
Dialogue: 0,0:20:11.40,0:20:13.80,Default,,0000,0000,0000,,Nhưng cô ta đã bỏ cái thai ngay sau đó.
Dialogue: 0,0:20:14.11,0:20:16.87,Default,,0000,0000,0000,,Akane căm ghét chính đứa trẻ trong bụng cô ta...
Dialogue: 0,0:20:16.98,0:20:19.41,Default,,0000,0000,0000,,Vì vậy cô ta quyết định bỏ nó đi dù rất day dứt khi làm thế.
Dialogue: 0,0:20:19.51,0:20:23.81,Default,,0000,0000,0000,,Sau đó cô ta nhận được lời đề nghị mang thai hộ và đã đồng ý.
Dialogue: 0,0:20:27.39,0:20:31.19,Default,,0000,0000,0000,,Theo lẽ tự nhiên, mình được sinh ra bởi Akane...
Dialogue: 0,0:20:31.32,0:20:33.55,Default,,0000,0000,0000,,Nhưng là bằng phương pháp thụ tinh nhân tạo...
Dialogue: 0,0:20:33.66,0:20:37.19,Default,,0000,0000,0000,,...Vậy nên mình hoàn toàn ko có quan hệ huyết thống với Akane hay Tomoe.
Dialogue: 0,0:20:42.13,0:20:47.33,Default,,0000,0000,0000,,Tomoe nghĩ rằng vì cô ấy mà tôi bị cưỡng bức.
Dialogue: 0,0:20:47.74,0:20:49.00,Default,,0000,0000,0000,,Nói vậy là sao?
Dialogue: 0,0:20:49.54,0:20:52.60,Default,,0000,0000,0000,,Một gã đã tìm cách hẹn hò với\NTomoe, nhưng cô ấy phớt lờ hắn ta.
Dialogue: 0,0:20:52.95,0:20:55.44,Default,,0000,0000,0000,,Và hắn ta là bạn thân với những kẻ đã cưỡng bức tôi.
Dialogue: 0,0:20:56.98,0:20:59.01,Default,,0000,0000,0000,,Chuyện tôi bị cưỡng bức chưa bao giờ lộ ra.
Dialogue: 0,0:20:59.25,0:21:02.69,Default,,0000,0000,0000,,Chỉ khi cặp vợ chồng đã nhờ tôi mang thai hộ...
Dialogue: 0,0:21:02.79,0:21:05.45,Default,,0000,0000,0000,,...tức là cha mẹ ruột của cậu...
Dialogue: 0,0:21:05.69,0:21:09.59,Default,,0000,0000,0000,,Khi Tomoe biết tin họ đã qua đời trong một tai nạn giao thông...
Dialogue: 0,0:21:09.80,0:21:12.23,Default,,0000,0000,0000,,...cô ấy đã quyết định nhận nuôi cậu.
Dialogue: 0,0:21:13.50,0:21:16.43,Default,,0000,0000,0000,,Vậy ra cô ta làm vậy để chuộc lại lỗi lầm của mình.
Dialogue: 0,0:21:16.67,0:21:19.07,Default,,0000,0000,0000,,Ko khó để hình dung điều đó với một người phụ nữ nhân hậu như cô ta.
Dialogue: 0,0:21:19.81,0:21:22.93,Default,,0000,0000,0000,,Tôi và cậu ko có chung huyết thống.
Dialogue: 0,0:21:23.08,0:21:23.40,Default,,0000,0000,0000,,Phải ko nhỉ?
Dialogue: 0,0:21:23.94,0:21:26.97,Default,,0000,0000,0000,,Cô sinh ra tôi nhưng chúng ta lại ko chung huyết thống?
Dialogue: 0,0:21:27.18,0:21:30.64,Default,,0000,0000,0000,,Vậy điều gì đã khiến cô tới gặp tôi?
Dialogue: 0,0:21:30.95,0:21:34.78,Default,,0000,0000,0000,,Cô muốn thỏa mãn dục vọng bằng chính con đẻ của mình?
Dialogue: 0,0:21:35.09,0:21:37.65,Default,,0000,0000,0000,,Tôi tưởng rằng suy nghĩ của chúng ta giống nhau.
Dialogue: 0,0:21:43.36,0:21:44.12,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:21:49.07,0:21:51.00,Default,,0000,0000,0000,,Cậu ko nhẹ nhàng hơn được sao?
Dialogue: 0,0:21:51.24,0:21:53.21,Default,,0000,0000,0000,,Loại người như cô ko đáng để tôi nhẹ tay.
Dialogue: 0,0:21:53.31,0:21:55.33,Default,,0000,0000,0000,,Tôi biết cô thích bị đối xử thô bạo thế này.
Dialogue: 0,0:21:55.51,0:21:56.27,Default,,0000,0000,0000,,Nào.
Dialogue: 0,0:21:56.94,0:21:58.57,Default,,0000,0000,0000,,Ko phải như vậy!
Dialogue: 0,0:22:08.09,0:22:11.22,Default,,0000,0000,0000,,Cô thích thú khi bị đối xử thô bạo?
Dialogue: 0,0:22:11.32,0:22:17.16,Default,,0000,0000,0000,,Nếu cậu tiếp tục làm vậy, tôi sẽ chết mất!
Dialogue: 0,0:22:24.60,0:22:27.00,Default,,0000,0000,0000,,Ko! Ngực tôi rách ra mất...
Dialogue: 0,0:22:27.11,0:22:28.04,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:22:29.71,0:22:31.07,Default,,0000,0000,0000,,Đừng dừng lại!
Dialogue: 0,0:22:31.98,0:22:33.50,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:22:33.98,0:22:35.67,Default,,0000,0000,0000,,Cô và Tomoe đúng là chị em song sinh.
Dialogue: 0,0:22:35.78,0:22:37.34,Default,,0000,0000,0000,,Phản ứng của hai người giống hệt nhau!
Dialogue: 0,0:22:38.48,0:22:41.32,Default,,0000,0000,0000,,Đừng dập tôi mạnh như vậy!
Dialogue: 0,0:22:41.49,0:22:42.75,Default,,0000,0000,0000,,Bớt rên rỉ đi nào!
Dialogue: 0,0:22:52.73,0:22:55.43,Default,,0000,0000,0000,,Tôi ra mất, tôi ra mất!
Dialogue: 0,0:23:09.42,0:23:11.28,Default,,0000,0000,0000,,Hệt như hai quả banh nước vậy.
Dialogue: 0,0:23:17.72,0:23:18.78,Default,,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:23:19.02,0:23:22.15,Default,,0000,0000,0000,,Cô giãy giụa ko ngừng,\Nnhưng vẫn bú nó điên cuồng.
Dialogue: 0,0:23:27.10,0:23:31.90,Default,,0000,0000,0000,,Phải, dùng cả lưỡi và miệng của cô đi.
Dialogue: 0,0:23:36.44,0:23:39.14,Default,,0000,0000,0000,,Này, ai cho cô nghỉ hả?
Dialogue: 0,0:23:40.85,0:23:43.41,Default,,0000,0000,0000,,Nếu cô thấy mỏi miệng...
Dialogue: 0,0:23:43.55,0:23:45.78,Default,,0000,0000,0000,,...thì vẫn còn một cách khác đấy.
Dialogue: 0,0:23:45.89,0:23:49.62,Default,,0000,0000,0000,,Gật qua gật lại đầu của cô đi,\Nsẽ có cảm giác khác lạ đó.
Dialogue: 0,0:23:52.22,0:23:54.32,Default,,0000,0000,0000,,Cô làm tệ quá.
Dialogue: 0,0:23:54.53,0:23:57.86,Default,,0000,0000,0000,,Thôi quên đi, tôi sẽ bắt cô làm điều này.
Dialogue: 0,0:23:59.20,0:24:01.89,Default,,0000,0000,0000,,Sao hả? Thích bị ép buộc như vậy ko?
Dialogue: 0,0:24:04.17,0:24:05.83,Default,,0000,0000,0000,,Cô vẫn ko có phản ứng gì sao.
Dialogue: 0,0:24:05.97,0:24:08.44,Default,,0000,0000,0000,,Đúng là con đàn bà dâm đãng.
Dialogue: 0,0:24:10.04,0:24:11.84,Default,,0000,0000,0000,,Được rồi, tôi ra đây!
Dialogue: 0,0:24:12.28,0:24:14.14,Default,,0000,0000,0000,,Nuốt hết chúng đi!
Dialogue: 0,0:24:16.18,0:24:18.51,Default,,0000,0000,0000,,Nào, nuốt chửng hết đi.
Dialogue: 0,0:24:23.22,0:24:25.59,Default,,0000,0000,0000,,Tốt lắm, ngoan ngoãn như vậy là tốt.
Dialogue: 0,0:24:25.76,0:24:28.12,Default,,0000,0000,0000,,Tôi có quà cho cô đây.
Dialogue: 0,0:24:30.36,0:24:32.35,Default,,0000,0000,0000,,Đừng bất ngờ với nó nhé.
Dialogue: 0,0:24:52.08,0:24:53.42,Default,,0000,0000,0000,,Bỏ nó ra!
Dialogue: 0,0:24:55.09,0:24:58.52,Default,,0000,0000,0000,,Sao thế? Tôi mới chỉ nhét phần đầu vào thôi mà.
Dialogue: 0,0:25:02.09,0:25:03.36,Default,,0000,0000,0000,,Bỏ nó ra đi...
Dialogue: 0,0:25:03.50,0:25:05.36,Default,,0000,0000,0000,,Bỏ nó ra đi mà!
Dialogue: 0,0:25:05.90,0:25:07.96,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:25:08.50,0:25:09.99,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:25:18.31,0:25:20.54,Default,,0000,0000,0000,,Vào dễ dàng hơn rồi đấy!
Dialogue: 0,0:25:25.65,0:25:27.48,Default,,0000,0000,0000,,Trông hay chưa này.
Dialogue: 0,0:25:27.82,0:25:30.48,Default,,0000,0000,0000,,Âm hộ cô đang sửi bọt kìa.
Dialogue: 0,0:25:30.76,0:25:32.78,Default,,0000,0000,0000,,Tôi sẽ giúp cô biến ước mơ của mình thành hiện thực.
Dialogue: 0,0:25:34.36,0:25:36.23,Default,,0000,0000,0000,,Mát mẻ dễ chịu nhỉ.
Dialogue: 0,0:25:36.33,0:25:38.59,Default,,0000,0000,0000,,Chà, khít thật đấy.
Dialogue: 0,0:25:38.70,0:25:41.19,Default,,0000,0000,0000,,Có lẽ tôi ko nên di chuyển nhiều.
Dialogue: 0,0:25:43.37,0:25:47.00,Default,,0000,0000,0000,,Cô nghĩ gì vậy? Nhịp mông đi nào.
Dialogue: 0,0:25:47.31,0:25:49.00,Default,,0000,0000,0000,,Tôi phát điên lên mất...
Dialogue: 0,0:25:49.17,0:25:50.33,Default,,0000,0000,0000,,Cơ thể tôi đang run lên...
Dialogue: 0,0:25:50.44,0:25:52.38,Default,,0000,0000,0000,,Cô say rồi.
Dialogue: 0,0:26:04.99,0:26:05.85,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:26:07.43,0:26:09.05,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:26:12.43,0:26:14.13,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:26:20.64,0:26:22.73,Default,,0000,0000,0000,,Cô ko còn cơ hội hưởng niềm hoan lạc này lần thứ hai đâu.
Dialogue: 0,0:26:22.91,0:26:26.31,Default,,0000,0000,0000,,Ko! Đừng chơi mạnh như vậy!
Dialogue: 0,0:26:28.85,0:26:31.87,Default,,0000,0000,0000,,Chóng mặt quá!
Dialogue: 0,0:26:46.23,0:26:50.76,Default,,0000,0000,0000,,Chúng ta chưa xong đâu.\NSự trả thù của tôi mới chỉ bắt đầu thôi!
