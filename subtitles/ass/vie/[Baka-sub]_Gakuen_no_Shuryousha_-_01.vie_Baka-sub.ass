Dialogue: 0,0:00:13.84,0:00:15.07,Default,,0000,0000,0000,,<i>Bố mẹ tôi vừa qua đời...</i>
Dialogue: 0,0:00:16.41,0:00:17.67,Default,,0000,0000,0000,,<i>Họ mất...</i>
Dialogue: 0,0:00:19.01,0:00:20.28,Default,,0000,0000,0000,,<i>trong một vụ tai nạn...</i>
Dialogue: 0,0:00:22.15,0:00:26.75,Default,,0000,0000,0000,,<i>Giờ tôi sống với anh trai cùng cha khác mẹ từ cuộc hôn nhân trước đó...</i>
Dialogue: 0,0:00:28.56,0:00:29.82,Default,,0000,0000,0000,,<i>Đã sáu năm không gặp.</i>
Dialogue: 0,0:00:31.56,0:00:34.72,Default,,0000,0000,0000,,<i>Hay khi hay tin, anh ấy đã lập tức trở về.</i>
Dialogue: 0,0:00:36.56,0:00:42.53,Default,,0000,0000,0000,,<i>Vì vài lí do mà anh ấy căm ghét người cha của mình nên ghét luôn cả gia đình mới.</i>
Dialogue: 0,0:00:44.21,0:00:47.51,Default,,0000,0000,0000,,<i>Nên sau khi tốt nghiệp, anh ấy đã bỏ nhà ra đi...</i>
Dialogue: 0,0:00:48.24,0:00:50.80,Default,,0000,0000,0000,,<i>...và không bao giờ trở lại.</i>
Dialogue: 0,0:00:52.45,0:00:56.18,Default,,0000,0000,0000,,<i>Tôi luôn dựa dẫm vào bố và anh trai mình...</i>
Dialogue: 0,0:00:56.92,0:00:58.65,Default,,0000,0000,0000,,<i>...Tôi rất yêu quý họ.</i>
Dialogue: 0,0:01:00.82,0:01:06.19,Default,,0000,0000,0000,,<i>Mặc dù anh ấy luôn tìm cách xa lánh chúng tôi...</i>
Dialogue: 0,0:01:06.76,0:01:09.53,Default,,0000,0000,0000,,<i>...Nhưng tôi vẫn nhớ khi còn nhỏ, anh ấy rất tốt với tôi .</i>
Dialogue: 0,0:01:35.89,0:01:37.29,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:01:49.14,0:01:50.66,Default,,0000,0000,0000,,Bây giờ chỉ còn hai chúng ta...
Dialogue: 0,0:01:52.54,0:01:54.44,Default,,0000,0000,0000,,Chúng ta hãy cùng nhau cố gắng nhé!
Dialogue: 0,0:01:57.88,0:01:58.97,Default,,0000,0000,0000,,Chỉ có hai chúng ta.
Dialogue: 0,0:01:59.25,0:01:59.84,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:02:02.58,0:02:05.25,Default,,0000,0000,0000,,Nghĩ là bây giờ anh là người giám hộ của em.
Dialogue: 0,0:02:05.99,0:02:09.25,Default,,0000,0000,0000,,Em phải nghe theo những gì anh bảo.
Dialogue: 0,0:02:24.94,0:02:27.17,Default,,0000,0000,0000,,Đau quá! Dừng lại đi, Onii-chan!
Dialogue: 0,0:02:36.89,0:02:40.69,Default,,0000,0000,0000,,Không! Dừng lại đi, Onii-chan! Làm ơn!
Dialogue: 0,0:02:41.49,0:02:44.58,Default,,0000,0000,0000,,Không! Không được! Onii-chan!
Dialogue: 0,0:02:46.09,0:02:47.58,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:02:54.04,0:02:56.00,Default,,0000,0000,0000,,D-Dừng lại đi!
Dialogue: 0,0:02:56.17,0:02:56.87,Default,,0000,0000,0000,,Im đê!!
Dialogue: 0,0:03:34.94,0:03:37.43,Default,,0000,0000,0000,,Onii-chan... Anh tàn nhẫn lắm...
Dialogue: 0,0:03:50.19,0:03:53.18,Default,,0000,0000,0000,,<i>Tôi lúc nào cũng yêu anh trai của mình.</i>
Dialogue: 0,0:03:54.10,0:03:57.00,Default,,0000,0000,0000,,<i>Tôi không hề để mắt đến bất cứ người con trai nào khác...</i>
Dialogue: 0,0:03:57.67,0:03:59.60,Default,,0000,0000,0000,,<i>...Bởi vì tôi rất yêu anh ấy.</i>
Dialogue: 0,0:04:00.60,0:04:01.63,Default,,0000,0000,0000,,<i>Vậy mà...</i>
Dialogue: 0,0:04:14.88,0:04:17.72,Default,,0000,0000,0000,,Dù bị hiếp nhưng em lại cảm thấy nó ư?
Dialogue: 0,0:04:17.72,0:04:19.48,Default,,0000,0000,0000,,Không phải vậy...
Dialogue: 0,0:04:22.59,0:04:25.42,Default,,0000,0000,0000,,Đừng có xạo! Âm hộ em ướt đẫm rồi này!
Dialogue: 0,0:04:30.50,0:04:32.99,Default,,0000,0000,0000,,Bây giờ em đã trở thành một người phụ nữ...
Dialogue: 0,0:04:46.65,0:04:51.02,Default,,0000,0000,0000,,<i>Nhưng có lẽ tôi đã luôn chờ đợi giây phút này...</i>
Dialogue: 0,0:04:52.05,0:04:55.42,Default,,0000,0000,0000,,<i>Giây phút chúng tôi có thể làm tình với nhau...</i>
Dialogue: 0,0:04:56.52,0:05:01.72,Default,,0000,0000,0000,,Onii-chan... Em... Em... cảm thấy sướng lắm!
Dialogue: 0,0:05:08.64,0:05:12.30,Default,,0000,0000,0000,,Thằng nhỏ của anh đang lấp đầy em...
Dialogue: 0,0:05:13.91,0:05:17.54,Default,,0000,0000,0000,,Nó chạm đến mọi ngóc ngách của em...
Dialogue: 0,0:05:21.35,0:05:22.58,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:05:30.26,0:05:31.92,Default,,0000,0000,0000,,Anh sắp ra đây.
Dialogue: 0,0:05:39.93,0:05:43.53,Default,,0000,0000,0000,,Vâng! Em cảm thấy như mình sắp phát điên rồi!
Dialogue: 0,0:06:03.69,0:06:04.68,Default,,0000,0000,0000,,Em vui lắm.
Dialogue: 0,0:06:05.49,0:06:09.93,Default,,0000,0000,0000,,Bây giờ chúng ta thể được ở bên nhau.
Dialogue: 0,0:06:11.23,0:06:15.60,Default,,0000,0000,0000,,Và làm tình như vầy mỗi ngày.
Dialogue: 0,0:06:18.07,0:06:19.20,Default,,0000,0000,0000,,Đừng có nhầm.
Dialogue: 0,0:06:20.04,0:06:22.20,Default,,0000,0000,0000,,Anh rảnh rỗi nên mới làm tình với em thôi.
Dialogue: 0,0:06:27.68,0:06:30.41,Default,,0000,0000,0000,,Ngày mai em sẽ chuyển đến học viện Takamine.
Dialogue: 0,0:06:31.15,0:06:32.21,Default,,0000,0000,0000,,Đấy là trường nội trú...
Dialogue: 0,0:06:32.65,0:06:34.99,Default,,0000,0000,0000,,...rồi anh cũng sẽ bán căn nhà này luôn.
Dialogue: 0,0:06:35.96,0:06:39.62,Default,,0000,0000,0000,,Không được...! Vất vả lắm chúng ta mới được ở bên nhau...
Dialogue: 0,0:06:40.13,0:06:45.29,Default,,0000,0000,0000,,Anh sẽ làm giáo viên ở đó, nên chúng ta vẫn có thể gặp nhau mỗi ngày.
Dialogue: 0,0:06:45.83,0:06:49.93,Default,,0000,0000,0000,,Em sẽ báo cáo mọi thứ về học sinh ở đó cho anh.
Dialogue: 0,0:06:52.14,0:06:55.74,Default,,0000,0000,0000,,Dù là một trường uy tín nhưng họ cũng có vấn đề của riêng mình.
Dialogue: 0,0:06:56.84,0:06:59.11,Default,,0000,0000,0000,,Onii-chan, anh định làm gì...
Dialogue: 0,0:06:59.68,0:07:02.48,Default,,0000,0000,0000,,Em chỉ cần nghe lời anh là đủ!
Dialogue: 0,0:07:03.05,0:07:06.85,Default,,0000,0000,0000,,Đừng nghĩ đến những chuyện đó! Tại sao chúng ta không thể sống vui vẻ cùng nhau?
Dialogue: 0,0:07:07.09,0:07:10.96,Default,,0000,0000,0000,,Em chỉ còn có anh mà thôi!
Dialogue: 0,0:07:12.83,0:07:13.76,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:07:17.03,0:07:20.73,Default,,0000,0000,0000,,Em phải nghe lời anh! Em mà dám cãi lại thì anh sẽ không bỏ qua đâu!
Dialogue: 0,0:07:27.94,0:07:32.04,Default,,0000,0000,0000,,{\pos(309,40)\c&H123834&\3c&H83BEB7&}Học viện Takamine
Dialogue: 0,0:07:36.02,0:07:37.42,Default,,0000,0000,0000,,{\pos(329,40)\c&H563540&\3c&HB2867B&\4c&HE1D4D3&}Phòng hiệu trưởng
Dialogue: 0,0:07:37.42,0:07:44.23,Default,,0000,0000,0000,,Thật sự xin lỗi, đáng lẽ ra ta nên nói chuyện trực tiếp với cậu...
Dialogue: 0,0:07:45.49,0:07:49.55,Default,,0000,0000,0000,,Không thể nào khác được, tôi nghe nói ngài đang bận tranh cử mà.
Dialogue: 0,0:07:50.10,0:07:54.83,Default,,0000,0000,0000,,Vâng, ta nghĩ nó sẽ ổn thôi, nhưng vì đây là lần đầu tiên ta tranh cử nên...
Dialogue: 0,0:07:55.27,0:07:57.93,Default,,0000,0000,0000,,...việc bỏ phiếu cũng không làm ta bận tâm lắm.
Dialogue: 0,0:07:58.71,0:08:01.83,Default,,0000,0000,0000,,Chỉ mong nó như vầy đến ngày bầu cử thôi.
Dialogue: 0,0:08:05.31,0:08:07.04,Default,,0000,0000,0000,,Elena đến muộn quá...
Dialogue: 0,0:08:12.52,0:08:16.12,Default,,0000,0000,0000,,Không! Dừng lại đi! Làm ơn!
Dialogue: 0,0:09:32.33,0:09:34.36,Default,,0000,0000,0000,,Bố ơi, con là Elena đây.
Dialogue: 0,0:09:34.80,0:09:36.06,Default,,0000,0000,0000,,Con vào đi.
Dialogue: 0,0:09:38.41,0:09:41.43,Default,,0000,0000,0000,,Con đến muộn vậy. Lần đầu gặp mặt giáo viên mới như vầy thì thất lễ quá.
Dialogue: 0,0:09:42.21,0:09:42.97,Default,,0000,0000,0000,,Con xin lỗi.
Dialogue: 0,0:09:44.25,0:09:47.37,Default,,0000,0000,0000,,Đây là con gái ta Elena. Con bé chỉ là học sinh năm ba...
Dialogue: 0,0:09:47.78,0:09:50.55,Default,,0000,0000,0000,,...nhưng con bé sẽ giúp ta trong cuộc chạy đua bầy cử.
Dialogue: 0,0:09:50.95,0:09:53.72,Default,,0000,0000,0000,,Hân hạnh được gặp thầy. Em là Takamine Elena.
Dialogue: 0,0:09:55.52,0:09:58.15,Default,,0000,0000,0000,,Tôi là Shuichi Kano. Hân hạnh được gặp em.
Dialogue: 0,0:09:59.99,0:10:03.76,Default,,0000,0000,0000,,Ta phải đi đây, mọi chuyện còn lại trưởng khoa sẽ nói cho cậu sau.
Dialogue: 0,0:10:03.90,0:10:05.20,Default,,0000,0000,0000,,Bố có quên gì không?
Dialogue: 0,0:10:05.63,0:10:09.63,Default,,0000,0000,0000,,Không... À, ta quên nụ hôn tạm biệt.
Dialogue: 0,0:10:13.34,0:10:14.71,Default,,0000,0000,0000,,Xấu hổ quá.
Dialogue: 0,0:10:15.14,0:10:18.08,Default,,0000,0000,0000,,Xin thứ lỗi. Ta quên còn có anh ở đây. Tạm biệt.
Dialogue: 0,0:10:24.19,0:10:26.15,Default,,0000,0000,0000,,Đã để thầy thấy cảnh xấu hổ như vậy.
Dialogue: 0,0:10:26.69,0:10:29.71,Default,,0000,0000,0000,,Trong mắt bố em em lúc nào cũng chỉ là một đứa trẻ.
Dialogue: 0,0:10:30.39,0:10:32.79,Default,,0000,0000,0000,,Tôi chắc người cha nào cũng vậy thôi.
Dialogue: 0,0:10:38.07,0:10:42.20,Default,,0000,0000,0000,,Vâng, đây là phòng hiệu trưởng. Bố tôi hiện không có ở đây.
Dialogue: 0,0:10:43.80,0:10:52.84,Default,,0000,0000,0000,,Ơ? Vâng, thế thì không hay chút nào. \NVâng, tôi sẽ đến đó ngay.
Dialogue: 0,0:10:54.82,0:10:57.78,Default,,0000,0000,0000,,Xin lỗi thầy. Em có chuyện quan trọng cần phải làm.
Dialogue: 0,0:10:58.22,0:10:59.69,Default,,0000,0000,0000,,Em sẽ nhờ người khác đến tiếp chuyện với thầy.
Dialogue: 0,0:11:00.22,0:11:00.92,Default,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:11:03.39,0:11:09.19,Default,,0000,0000,0000,,Em là Takamine ạ. Có thể cử giáo viên nào đó đến đây được không ạ?
Dialogue: 0,0:11:13.03,0:11:16.70,Default,,0000,0000,0000,,Tuyệt quá. Anh tốt nghiệp đại học Mỹ à?
Dialogue: 0,0:11:17.47,0:11:20.46,Default,,0000,0000,0000,,Tôi cũng muốn mà không làm được.
Dialogue: 0,0:11:21.91,0:11:24.81,Default,,0000,0000,0000,,Giáo viên tiếng Nhật thì cần gì phải ra nước ngoài.
Dialogue: 0,0:11:25.25,0:11:29.74,Default,,0000,0000,0000,,Nói cũng phải. Nhưng tôi vẫn thấy ngưỡng mộ.
Dialogue: 0,0:11:53.81,0:11:54.71,Default,,0000,0000,0000,,Anh sao thế?
Dialogue: 0,0:11:56.14,0:11:58.27,Default,,0000,0000,0000,,Không có gì, chúng ta đi thôi
Dialogue: 0,0:11:58.75,0:12:02.48,Default,,0000,0000,0000,,Ơ? Kano-sensei. Anh biết phòng tập thể dục ở đâu à?
Dialogue: 0,0:12:03.58,0:12:08.11,Default,,0000,0000,0000,,Vâng. Tôi đến hơi sớm nên có dạo quanh một vòng.
Dialogue: 0,0:12:10.19,0:12:12.35,Default,,0000,0000,0000,,Thể dục thật là phiền phức!
Dialogue: 0,0:12:12.49,0:12:14.52,Default,,0000,0000,0000,,Em đã nói với giáo viên là mình đã đến ngày!
Dialogue: 0,0:12:14.60,0:12:15.36,Default,,0000,0000,0000,,Chị cũng vậy!
Dialogue: 0,0:12:21.94,0:12:23.40,Default,,0000,0000,0000,,Này, ba em kia!
Dialogue: 0,0:12:24.81,0:12:25.74,Default,,0000,0000,0000,,Tưởng gì...
Dialogue: 0,0:12:26.07,0:12:27.04,Default,,0000,0000,0000,,Hóa ra là Haruko-chan.
Dialogue: 0,0:12:27.11,0:12:28.91,Default,,0000,0000,0000,,Cô làm em giật cả mình!
Dialogue: 0,0:12:29.21,0:12:30.47,Default,,0000,0000,0000,,Chúng là chị em sinh ba.
Dialogue: 0,0:12:33.88,0:12:36.28,Default,,0000,0000,0000,,Haruko-chan đi với đàn ông kìa!
Dialogue: 0,0:12:36.68,0:12:38.12,Default,,0000,0000,0000,,Bạn trai cô hả?
Dialogue: 0,0:12:38.12,0:12:40.18,Default,,0000,0000,0000,,Em biết là cô có mà!
Dialogue: 0,0:12:41.29,0:12:43.45,Default,,0000,0000,0000,,Không phải! Đây là...
Dialogue: 0,0:12:43.52,0:12:44.55,Default,,0000,0000,0000,,Đừng có chối!
Dialogue: 0,0:12:44.76,0:12:47.92,Default,,0000,0000,0000,,Em nghĩ việc kết hôn hợp với cô hơn!
Dialogue: 0,0:12:48.03,0:12:49.66,Default,,0000,0000,0000,,Nè, hai người đã làm chuyện đó chưa?
Dialogue: 0,0:12:49.76,0:12:54.06,Default,,0000,0000,0000,,Dĩ nhiên rồi. Ngực cô bự như vầy mà!
Dialogue: 0,0:13:00.44,0:13:02.17,Default,,0000,0000,0000,,Ôi, Haruko-chan đáng yêu quá!
Dialogue: 0,0:13:05.81,0:13:07.34,Default,,0000,0000,0000,,Tôi là Kano, giáo viên mới.
Dialogue: 0,0:13:08.05,0:13:10.04,Default,,0000,0000,0000,,Tôi sẽ có buổi giới thiệu vào ngày mai.
Dialogue: 0,0:13:10.38,0:13:11.61,Default,,0000,0000,0000,,Sensei!?
Dialogue: 0,0:13:12.25,0:13:15.98,Default,,0000,0000,0000,,Thật không may tôi mới gặp Misaka-Sensei hôm nay...
Dialogue: 0,0:13:16.62,0:13:18.75,Default,,0000,0000,0000,,Chả có gì giống vói câu chuyện ngọt ngào của các em đâu.
Dialogue: 0,0:13:20.23,0:13:22.72,Default,,0000,0000,0000,,Nhưng Haruko-chan đổ rồi đấy!
Dialogue: 0,0:13:27.10,0:13:30.07,Default,,0000,0000,0000,,Tôi không cần phải vào trong.
Dialogue: 0,0:13:31.07,0:13:32.37,Default,,0000,0000,0000,,Chúng ta đi chỗ khác thôi!
Dialogue: 0,0:13:33.47,0:13:34.46,Default,,0000,0000,0000,,V-Vâng.
Dialogue: 0,0:13:36.14,0:13:41.45,Default,,0000,0000,0000,,Tất cả phòng trong KTX đều là phòng đôi, ngoại trừ phòng của con gái hiệu trưởng.
Dialogue: 0,0:13:46.05,0:13:46.85,Default,,0000,0000,0000,,Đây là em gái tôi.
Dialogue: 0,0:13:47.19,0:13:49.32,Default,,0000,0000,0000,,Hân hạnh được gặp cô. Em là Kano Noelle.
Dialogue: 0,0:13:49.96,0:13:52.69,Default,,0000,0000,0000,,Đây là Misaka-sensei. Cô ấy đang dẫn anh đi dạo một vòng.
Dialogue: 0,0:13:53.23,0:13:53.72,Default,,0000,0000,0000,,Chào em.
Dialogue: 0,0:13:57.26,0:13:59.96,Default,,0000,0000,0000,,Đó là bạn cùng phòng của em. Takamine Shirone.
Dialogue: 0,0:14:00.23,0:14:00.79,Default,,0000,0000,0000,,Takamine?
Dialogue: 0,0:14:01.64,0:14:03.26,Default,,0000,0000,0000,,Em ấy là cháu gái của hiệu trưởng.
Dialogue: 0,0:14:07.21,0:14:09.84,Default,,0000,0000,0000,,Bố mẹ đã qua đời khi  em ấy còn nhỏ...
Dialogue: 0,0:14:10.11,0:14:12.08,Default,,0000,0000,0000,,...nên hiệu trưởng đã nhận nuôi em ấy.
Dialogue: 0,0:14:13.85,0:14:18.84,Default,,0000,0000,0000,,Em ấy lớn lên cùng với Elena, nhưng tính cách hai người hoàn toàn khác biệt nhau.
Dialogue: 0,0:14:20.29,0:14:21.78,Default,,0000,0000,0000,,Dưới sảnh là quán ăn tự phục vụ.
Dialogue: 0,0:14:22.59,0:14:24.52,Default,,0000,0000,0000,,Điều tra gia đình Takamine từ cô ấy.
Dialogue: 0,0:14:36.80,0:14:41.83,Default,,0000,0000,0000,,Hôm nay chúng ta chỉ học đến đây thôi. Tiết sau sẽ học ở phòng thực hành, đừng có quên đấy nhé.
Dialogue: 0,0:14:45.55,0:14:46.10,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:14:47.38,0:14:48.68,Default,,0000,0000,0000,,Bọn em xin lỗi!
Dialogue: 0,0:15:15.08,0:15:16.27,Default,,0000,0000,0000,,Cảm ơn anh nhiều lắm.
Dialogue: 0,0:15:16.94,0:15:18.61,Default,,0000,0000,0000,,Đây không phải là lần đầu nhỉ?
Dialogue: 0,0:15:19.65,0:15:21.24,Default,,0000,0000,0000,,Làm ơn đừng nói với ai!
Dialogue: 0,0:15:21.78,0:15:25.55,Default,,0000,0000,0000,,Bị học sinh của mình bắt nạt như vầy... Xấu hổ lắm.
Dialogue: 0,0:15:27.35,0:15:31.62,Default,,0000,0000,0000,,Tôi biết rồi. Dù là một trường có uy tín nhưng có nhiều phền toái nhỉ.
Dialogue: 0,0:15:32.33,0:15:35.02,Default,,0000,0000,0000,,Nếu được cô có thể cho tôi hỏi vài chuyện được không?
Dialogue: 0,0:15:36.06,0:15:37.43,Default,,0000,0000,0000,,Cô đã dạy ở đây lâu hơn tôi mà...
Dialogue: 0,0:15:39.97,0:15:41.46,Default,,0000,0000,0000,,Nếu được tôi sẽ giúp anh.
Dialogue: 0,0:15:51.28,0:15:54.41,Default,,0000,0000,0000,,Không... Dừng lại đi... Làm ơn...
Dialogue: 0,0:15:56.02,0:15:59.54,Default,,0000,0000,0000,,Dừng lại ư? Thế cô nghĩ tôi kêu cô đến đây giờ này...
Dialogue: 0,0:15:59.99,0:16:02.46,Default,,0000,0000,0000,,...là chỉ để nói chuyện về học viện thôi sao?
Dialogue: 0,0:16:15.44,0:16:19.17,Default,,0000,0000,0000,,Cô thật sự vẫn chưa đi tắm ư?
Dialogue: 0,0:16:19.91,0:16:24.90,Default,,0000,0000,0000,,Tôi có thể ngửi thấy mùi hôi trong này. Dù là một lão già biến thái cũng phải chết vì nó.
Dialogue: 0,0:16:27.31,0:16:29.68,Default,,0000,0000,0000,,Cô có khóc cũng vô ích thôi.
Dialogue: 0,0:16:33.05,0:16:34.28,Default,,0000,0000,0000,,Cô chả cần phải phản kháng làm gì.
Dialogue: 0,0:16:49.67,0:16:50.66,Default,,0000,0000,0000,,Cô cảm thấy nó chứ?
Dialogue: 0,0:16:52.67,0:16:53.70,Default,,0000,0000,0000,,Đừng có xạo.
Dialogue: 0,0:16:56.44,0:17:00.38,Default,,0000,0000,0000,,Tuy không còn trinh, nhưng cô vẫn thiếu kinh nghiệm nhỉ.
Dialogue: 0,0:17:01.92,0:17:05.18,Default,,0000,0000,0000,,Nó ướt đẫm rồi... Cô đã chuẩn bị xong rồi...
Dialogue: 0,0:17:14.83,0:17:20.70,Default,,0000,0000,0000,,Thư giãn đi... Hãy để tôi tận hưởng nó nào....
Dialogue: 0,0:17:42.09,0:17:47.79,Default,,0000,0000,0000,,Tuyệt quá. Như vầy mà vẫn vừa! Không chừng tôi có thể đút của nắm tay vào ấy chứ.
Dialogue: 0,0:17:56.80,0:18:03.44,Default,,0000,0000,0000,,Dừng môi và lưỡi của cô nhiều hơn nữa. Phải... Là vậy đó.
Dialogue: 0,0:18:06.88,0:18:09.91,Default,,0000,0000,0000,,Tôi không có hướng thú với những gì cô đã nói với tôi.
Dialogue: 0,0:18:10.62,0:18:12.28,Default,,0000,0000,0000,,Từ bây giờ...
Dialogue: 0,0:18:13.05,0:18:17.92,Default,,0000,0000,0000,,Với tư cách là một giáo viên nữ, cô hãy điều tra sự thật về ngôi trường này.
Dialogue: 0,0:18:18.56,0:18:22.29,Default,,0000,0000,0000,,Cô hãy tìm ra vụ bê bối nhất trong ngôi trường này.
Dialogue: 0,0:18:26.13,0:18:27.60,Default,,0000,0000,0000,,Nhanh hơn nữa!
Dialogue: 0,0:18:28.24,0:18:30.14,Default,,0000,0000,0000,,Nhanh nữa. Nhanh nữa!
Dialogue: 0,0:18:55.43,0:18:57.80,Default,,0000,0000,0000,,Sướng quá! Lần đầu tiên tôi sướng như vậy!
Dialogue: 0,0:18:58.33,0:19:02.46,Default,,0000,0000,0000,,Bên trong tôi đang bị thiêu đốt... Tôi cảm thấy như mình sắp phát điên...
Dialogue: 0,0:19:11.28,0:19:11.90,Default,,0000,0000,0000,,Phải!
Dialogue: 0,0:19:14.15,0:19:18.48,Default,,0000,0000,0000,,Tuyệt quá... Tôi chết mất!
Dialogue: 0,0:19:19.79,0:19:21.55,Default,,0000,0000,0000,,Cô còn muốn nữa không?
Dialogue: 0,0:19:22.39,0:19:23.25,Default,,0000,0000,0000,,Tôi muốn!
Dialogue: 0,0:19:23.32,0:19:27.35,Default,,0000,0000,0000,,Vậy thì hãy nghe lời tôi! Cô sẽ là việc cho tôi!
Dialogue: 0,0:19:27.93,0:19:31.99,Default,,0000,0000,0000,,Vâng! Tôi sẽ làm mọi thứ! Tôi sẽ là nô lệ của anh!
Dialogue: 0,0:19:32.70,0:19:34.83,Default,,0000,0000,0000,,Cô ngoan lắm! Tôi sắp ra đây!
Dialogue: 0,0:19:50.12,0:19:50.64,Default,,0000,0000,0000,,Chẳng có gì ư?
Dialogue: 0,0:19:51.02,0:19:56.22,Default,,0000,0000,0000,,Em không thể moi được gì từ Shirone. Bạn ấy chỉ chào em một tiếng rồi thôi.
Dialogue: 0,0:19:57.22,0:19:59.16,Default,,0000,0000,0000,,Anh biết rồi. Tự anh sẽ nghĩ cách khác.
Dialogue: 0,0:20:00.83,0:20:02.89,Default,,0000,0000,0000,,Onii-chan, rốt cuộc anh định làm gì vậy?
Dialogue: 0,0:20:04.26,0:20:08.93,Default,,0000,0000,0000,,Hãy nói em biết đi. Nếu biết nhiều hơn em có thể sẽ giúp được anh.
Dialogue: 0,0:20:10.80,0:20:14.80,Default,,0000,0000,0000,,Đừng bắt anh phải nói lại, em chỉ cần làm theo những gì anh bảo.
Dialogue: 0,0:20:37.36,0:20:38.49,Default,,0000,0000,0000,,Rốt cuộc cô...?
Dialogue: 0,0:20:42.47,0:20:45.17,Default,,0000,0000,0000,,Tôi mình biết không thể chạy thoát.
Dialogue: 0,0:20:46.84,0:20:47.93,Default,,0000,0000,0000,,Cứ làm những gì thầy muốn.
Dialogue: 0,0:20:49.44,0:20:51.47,Default,,0000,0000,0000,,Đây không phải mục đích của tôi.
Dialogue: 0,0:20:52.65,0:20:58.88,Default,,0000,0000,0000,,Tôi chỉ muốn lấy thông tin về gia đình Takamine từ cô thôi.
Dialogue: 0,0:21:00.19,0:21:03.18,Default,,0000,0000,0000,,Tôi không có gì để nói cả.
Dialogue: 0,0:21:10.00,0:21:15.20,Default,,0000,0000,0000,,Chỉ thế thôi ư? Nếu vậy thì tôi đi đây...
Dialogue: 0,0:21:24.21,0:21:27.77,Default,,0000,0000,0000,,Chờ đã. Tôi không thể để cô đi như vậy được...
Dialogue: 0,0:21:28.75,0:21:30.77,Default,,0000,0000,0000,,Tôi sẽ hỏi cơ thể cô...
Dialogue: 0,0:21:31.42,0:21:34.18,Default,,0000,0000,0000,,Cô không ngờ là lần đầu của mình sẽ như vầy phải không?
Dialogue: 0,0:21:41.86,0:21:44.30,Default,,0000,0000,0000,,Đây là cách thầy đã làm với Noelle-san ư?
Dialogue: 0,0:21:46.40,0:21:47.16,Default,,0000,0000,0000,,Cô...
Dialogue: 0,0:21:56.04,0:21:57.77,Default,,0000,0000,0000,,Như vầy thầy sẽ dễ chơi tôi hơn.
Dialogue: 0,0:21:58.61,0:22:03.14,Default,,0000,0000,0000,,Tôi cứ nghĩ cô còn trinh... Không ngờ...
Dialogue: 0,0:22:05.49,0:22:08.82,Default,,0000,0000,0000,,Sao thế? Không chơi tôi à?
Dialogue: 0,0:22:52.57,0:22:55.56,Default,,0000,0000,0000,,Ai đã huấn luyện cô như vầy?
Dialogue: 0,0:22:57.70,0:22:59.70,Default,,0000,0000,0000,,<i>Hiệu trưởng đã nhận nuôi em ấy.</i>
Dialogue: 0,0:23:00.21,0:23:00.80,Default,,0000,0000,0000,,Không lẽ nào...!
Dialogue: 0,0:23:02.18,0:23:08.38,Default,,0000,0000,0000,,Là ai cũng được. Tôi đã làm thầy ra... Bây giờ đến lượt thầy.
Dialogue: 0,0:23:26.40,0:23:27.73,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:23:28.47,0:23:30.96,Default,,0000,0000,0000,,Không thể tin được. Ông ta đã chơi cô ta trong suốt khoảng thời gian đó...
Dialogue: 0,0:23:33.47,0:23:33.96,Default,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:23:38.78,0:23:41.27,Default,,0000,0000,0000,,Phải... Sướng quá.
Dialogue: 0,0:23:51.36,0:23:54.99,Default,,0000,0000,0000,,Chết tiệt! Chết tiệt! Chết tiệt! Chết tiệt!
Dialogue: 0,0:24:13.91,0:24:16.01,Default,,0000,0000,0000,,Phải! Sướng quá!
Dialogue: 0,0:24:16.55,0:24:21.35,Default,,0000,0000,0000,,Nó lớn hơn và cứng hơn của ông ấy nhiều... Tuyệt vời!
Dialogue: 0,0:24:23.99,0:24:24.85,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:25:04.40,0:25:06.03,Default,,0000,0000,0000,,Em có ghét ông ta không?
Dialogue: 0,0:25:08.74,0:25:09.63,Default,,0000,0000,0000,,Em ghét mà đúng không?
Dialogue: 0,0:25:16.41,0:25:18.84,Default,,0000,0000,0000,,Tôi có thể giúp em trả thù.
Dialogue: 0,0:25:20.65,0:25:23.51,Default,,0000,0000,0000,,Vậy nên em có thể giúp tôi chứ?
Dialogue: 0,0:25:46.01,0:25:48.57,Default,,0000,0000,0000,,{\pos(309,40)\c&H123834&\3c&H83BEB7&}Kiến trúc xá học viện Takamine
Dialogue: 0,0:26:04.99,0:26:07.98,Default,,0000,0000,0000,,<i>Cuối cùng, thời cơ để thực hiện kế hoạch của mình đã đến.</i>
Dialogue: 0,0:26:11.93,0:26:13.70,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:26:20.87,0:26:24.28,Default,,0000,0000,0000,,<i>Onii-chan... Rốt cuộc anh đang nghĩ gì vậy?</i>
Dialogue: 0,0:26:25.58,0:26:28.31,Default,,0000,0000,0000,,<i>Anh làm em sợ lắm...</i>
Dialogue: 0,0:26:28.31,0:26:30.31,Default,,0000,0000,0000,,
