Dialogue: 0,0:00:06.71,0:00:08.14,Default,,0000,0000,0000,,Kia rồi!
Dialogue: 0,0:00:11.68,0:00:13.61,Default,,0000,0000,0000,,Mở cửa! Mau mở cửa!
Dialogue: 0,0:00:15.15,0:00:18.02,Default,,0000,0000,0000,,Trông ngươi thật đáng ngờ, mau khai báo danh tính?
Dialogue: 0,0:00:18.52,0:00:21.51,Default,,0000,0000,0000,,Đồ ngu! Các người không nhận ra ta sao?
Dialogue: 0,0:00:21.89,0:00:22.58,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:00:23.36,0:00:26.66,Default,,0000,0000,0000,,Nói mới nhớ, ta đã xa nhà suốt 8 năm trời.
Dialogue: 0,0:00:28.53,0:00:32.86,Default,,0000,0000,0000,,Ta là hoàng tử của Asteria, Distol El Asteria!
Dialogue: 0,0:00:33.67,0:00:36.50,Default,,0000,0000,0000,,Ngài chính là vị hoàng tử được phái đi làm con tin tại Bastarauge?
Dialogue: 0,0:00:37.10,0:00:41.84,Default,,0000,0000,0000,,Ta có chuyện cần bẩm báo với vua cha. Báo với ông ấy rằng ta đã trở về.
Dialogue: 0,0:00:42.88,0:00:44.71,Default,,0000,0000,0000,,Tuân lệnh!
Dialogue: 0,0:00:46.31,0:00:48.28,Default,,0000,0000,0000,,Hoàng tử Distol đã quay về.
Dialogue: 0,0:00:48.35,0:00:49.82,Default,,0000,0000,0000,,Chẳng phải hoàng tử đang làm con tin sao?
Dialogue: 0,0:00:49.88,0:00:51.32,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:00:52.29,0:00:55.19,Default,,0000,0000,0000,,Distol! Là con phải không, Distol?
Dialogue: 0,0:00:55.72,0:00:55.82,Default,,0000,0000,0000,,Phụ hoàng, con về rồi đây.
Dialogue: 0,0:00:55.82,0:00:57.59,Default,,0000,0000,0000,,Phụ hoàng, con về rồi đây.
Dialogue: 0,0:00:57.59,0:01:01.29,Default,,0000,0000,0000,,Phải rồi, đúng là Distol. Ta rất mừng vì con đã trở về bình an.
Dialogue: 0,0:01:02.53,0:01:05.52,Default,,0000,0000,0000,,Tâu Bệ hạ, hạ thần thấy có điều gì đó không ổn.
Dialogue: 0,0:01:06.07,0:01:10.33,Default,,0000,0000,0000,,Bastarauge chưa hề thông báo cho chúng ta về việc thả con tin về nước.
Dialogue: 0,0:01:10.90,0:01:13.77,Default,,0000,0000,0000,,Việc này can hệ rất lớn tới hòa ước? Đó chính là vấn đề.
Dialogue: 0,0:01:14.17,0:01:18.04,Default,,0000,0000,0000,,Phải rồi! Hoàng nhi, con tự động trốn về sao?
Dialogue: 0,0:01:18.55,0:01:23.01,Default,,0000,0000,0000,,Phụ hoàng thật hồ đồ, suốt 8 năm mà người vẫn chẳng thay đổi chút nào.
Dialogue: 0,0:01:23.52,0:01:26.65,Default,,0000,0000,0000,,Nhưng nếu hòa ước bị phá vỡ, vương quốc Asteria sẽ...
Dialogue: 0,0:01:27.25,0:01:31.85,Default,,0000,0000,0000,,Bastarauge vẫn luôn âm thầm xây dựng lực lượng để thôn tính vương quốc ta.
Dialogue: 0,0:01:32.06,0:01:33.08,Default,,0000,0000,0000,,Cái gì!?
Dialogue: 0,0:01:33.16,0:01:35.03,Default,,0000,0000,0000,,Thế này không ổn!
Dialogue: 0,0:01:35.60,0:01:38.53,Default,,0000,0000,0000,,Chúng ta phải gửi thư xin lỗi ngay lập tức.
Dialogue: 0,0:01:38.60,0:01:41.12,Default,,0000,0000,0000,,Ngân khố vương quốc không đủ để cống nạp cho họ.
Dialogue: 0,0:01:41.50,0:01:44.27,Default,,0000,0000,0000,,Vậy nên chúng ta cần cử đi một con tin thay thế.
Dialogue: 0,0:01:44.94,0:01:51.54,Default,,0000,0000,0000,,Không, các khanh không định bắt ta đi làm con tin đó chứ.
Dialogue: 0,0:01:51.61,0:01:56.11,Default,,0000,0000,0000,,Tâu bệ hạ, có lẽ đó là cách duy nhất để cứu Asteria.
Dialogue: 0,0:01:56.55,0:01:59.38,Default,,0000,0000,0000,,Phụ hoàng, chúng ta phải vùng lên thôi!
Dialogue: 0,0:01:59.45,0:02:02.62,Default,,0000,0000,0000,,Con nắm rất rõ tình hình của Bastarauge!
Dialogue: 0,0:02:03.59,0:02:07.82,Default,,0000,0000,0000,,Hãy giao quân đội cho con, con sẽ đánh cho chúng không còn manh giáp!
Dialogue: 0,0:02:07.89,0:02:09.56,Default,,0000,0000,0000,,Thật sao?
Dialogue: 0,0:02:10.03,0:02:12.79,Default,,0000,0000,0000,,Mau triệu tập quân đội và người dân lại!
Dialogue: 0,0:02:12.87,0:02:15.84,Default,,0000,0000,0000,,Mau lên, nếu không, các người đừng mong giữ được cái đầu trên cổ!
Dialogue: 0,0:02:17.80,0:02:19.64,Default,,0000,0000,0000,,Hạ thần tuân lệnh, thưa Hoàng tử.
Dialogue: 0,0:02:19.74,0:02:23.23,Default,,0000,0000,0000,,Mau mau truyền lệnh, chúng ta không còn đường lùi nữa rồi!
Dialogue: 0,0:02:24.24,0:02:28.58,Default,,0000,0000,0000,,Một vương quốc yếu hèn... Mẫu hậu chết thật oan uổng quá.
