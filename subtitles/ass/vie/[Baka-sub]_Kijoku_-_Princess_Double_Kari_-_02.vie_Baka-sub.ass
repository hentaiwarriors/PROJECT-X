Dialogue: 0,0:02:34.29,0:02:39.95,Default,,0000,0000,0000,,Phần 2: {\bord4\shad2\c&H010CE3&}Bài học về sự vâng lời{\4c&H656565&}
Dialogue: 0,0:02:43.76,0:02:46.63,Default,,0000,0000,0000,,Hỡi bách tính Asteria, cám ơn sự có mặt của mọi người
Dialogue: 0,0:02:46.70,0:02:51.30,Default,,0000,0000,0000,,Ta là hoàng tử của Asterial, Distol!
Dialogue: 0,0:02:52.44,0:02:58.04,Default,,0000,0000,0000,,Mọi người hãy lắng nghe thật kĩ. Bastarauge đã phá vỡ hòa ước và nhăm nhe thôn tính vương quốc ta!
Dialogue: 0,0:02:58.85,0:03:01.84,Default,,0000,0000,0000,,Ta phải vội vã quay về để thông báo tin này.
Dialogue: 0,0:03:01.92,0:03:05.65,Default,,0000,0000,0000,,Ta xin thề! Nếu mọi người đi theo ta,
Dialogue: 0,0:03:05.72,0:03:11.16,Default,,0000,0000,0000,,Ta sẽ bắt Bastarauge phải trả giá cho những gì chúng đã gây ra với mọi người!
Dialogue: 0,0:03:11.63,0:03:15.89,Default,,0000,0000,0000,,Đã đến lúc chúng ta vùng lên để bảo vệ non sông nước nhà bằng chính đôi tay mình!
Dialogue: 0,0:03:15.96,0:03:17.40,Default,,0000,0000,0000,,Đi theo Hoàng tử!
Dialogue: 0,0:03:17.46,0:03:19.56,Default,,0000,0000,0000,,Chiến thắng sẽ thuộc về Asteria!
Dialogue: 0,0:03:20.47,0:03:24.60,Default,,0000,0000,0000,,Xin lỗi các người, ta buộc phải cứu Qoona.
Dialogue: 0,0:03:26.07,0:03:27.33,Default,,0000,0000,0000,,Tiến lên!
Dialogue: 0,0:03:27.74,0:03:29.33,Default,,0000,0000,0000,,Đi theo Hoàng tử!
Dialogue: 0,0:03:32.51,0:03:34.38,Default,,0000,0000,0000,,Thắng lợi muôn năm!
Dialogue: 0,0:03:34.68,0:03:39.52,Default,,0000,0000,0000,,Chiến tranh tiếp nỗi ngày này qua ngày khác, nhưng trong lòng Distol vẫn luôn canh cánh một chuyện.
Dialogue: 0,0:03:39.59,0:03:42.32,Default,,0000,0000,0000,,Tất nhiên đó là chuyện của Công chúa Qoona.
Dialogue: 0,0:03:42.82,0:03:48.92,Default,,0000,0000,0000,,Nàng vẫn bị giam cầm trong Tháp Tin lành, giải cứu nàng khỏi phong ấn ma thuật kia hiện giờ vẫn là điều bất khả thi.
Dialogue: 0,0:03:50.23,0:03:51.42,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:03:52.90,0:03:53.66,Default,,0000,0000,0000,,Eleanor hả?
Dialogue: 0,0:03:53.87,0:03:56.93,Default,,0000,0000,0000,,Vâng thưa Chủ nhân, ngài nghĩ gì vậy?
Dialogue: 0,0:03:57.87,0:04:03.17,Default,,0000,0000,0000,,Không có gì. Ngày mai chúng ta sẽ tiến công thành trì tiếp theo.
Dialogue: 0,0:04:03.24,0:04:05.21,Default,,0000,0000,0000,,Ta cần nghỉ sớm.
Dialogue: 0,0:04:11.28,0:04:12.58,Default,,0000,0000,0000,,Distol-sama.
Dialogue: 0,0:04:13.35,0:04:15.98,Default,,0000,0000,0000,,Thảo dân chỉ là một tên thợ máy tầm thường.
Dialogue: 0,0:04:16.09,0:04:20.86,Default,,0000,0000,0000,,Thảo dân chỉ tới đây để làm ăn, xin ngài mở lượng khoan dung
Dialogue: 0,0:04:20.93,0:04:22.76,Default,,0000,0000,0000,,Đổi lại thảo dân xin dâng ngài một vật.
Dialogue: 0,0:04:23.46,0:04:24.73,Default,,0000,0000,0000,,Là thứ gì?
Dialogue: 0,0:04:24.80,0:04:30.43,Default,,0000,0000,0000,,Nó có tên là Thuốc vắt sữa. Ngài thậm chí có thể vắt sữa từ một trinh nữ nếu xài nó.
Dialogue: 0,0:04:30.94,0:04:32.34,Default,,0000,0000,0000,,Ngươi tặng cho ta?
Dialogue: 0,0:04:32.41,0:04:36.21,Default,,0000,0000,0000,,Thảo dân tin ngài sẽ có chỗ để dùng nó.
Dialogue: 0,0:04:38.88,0:04:40.94,Default,,0000,0000,0000,,Thôi được, ta nhận.
Dialogue: 0,0:04:41.01,0:04:43.74,Default,,0000,0000,0000,,Hãy lấy giấy thông hành từ cận vệ của ta, muốn đi đâu tùy ngươi.
Dialogue: 0,0:04:43.82,0:04:45.34,Default,,0000,0000,0000,,Tạ ơn Hoàng tử.
Dialogue: 0,0:04:51.99,0:04:54.72,Default,,0000,0000,0000,,Bất cứ chuyện gì ngài cũng luôn quyết đoán như vậy.
Dialogue: 0,0:04:55.46,0:04:57.40,Default,,0000,0000,0000,,Eleanor hả? Hiện hình đi nào.
Dialogue: 0,0:04:59.37,0:05:01.49,Default,,0000,0000,0000,,Vâng... Xem chừng ngài đang rất mệt mỏi.
Dialogue: 0,0:05:02.40,0:05:03.56,Default,,0000,0000,0000,,Tất nhiên.
Dialogue: 0,0:05:03.64,0:05:09.54,Default,,0000,0000,0000,,Ta đang làm một việc mà không một người bình thường nào dám làm... nói đúng hơn là buộc phải làm.
Dialogue: 0,0:05:10.38,0:05:15.54,Default,,0000,0000,0000,,Chứng tỏ ngài là người duy nhất xứng đáng với sức mạnh này.
Dialogue: 0,0:05:16.78,0:05:19.98,Default,,0000,0000,0000,,Và cô thấy vui mừng vì điều đó?
Dialogue: 0,0:05:20.42,0:05:25.72,Default,,0000,0000,0000,,Không liên quan tới tôi, sử dụng quyền năng đó ra sao là tùy ở ngài.
Dialogue: 0,0:05:26.76,0:05:31.20,Default,,0000,0000,0000,,Cũng vậy cả thôi. Ta sẽ làm mọi thứ để cứu Qoona.
Dialogue: 0,0:05:31.63,0:05:33.12,Default,,0000,0000,0000,,Đúng là những lời vàng ngọc.
Dialogue: 0,0:05:33.73,0:05:36.13,Default,,0000,0000,0000,,Hôm nay, ta được tặng một món quà rất thú vị.
Dialogue: 0,0:05:36.20,0:05:37.33,Default,,0000,0000,0000,,Chà...
Dialogue: 0,0:05:37.70,0:05:40.10,Default,,0000,0000,0000,,Ta vừa nghĩ tới chuyện dùng nó với Elena.
Dialogue: 0,0:05:40.71,0:05:42.83,Default,,0000,0000,0000,,Chắc sẽ vui lắm đây.
Dialogue: 0,0:05:42.91,0:05:44.71,Default,,0000,0000,0000,,Để xem sao.
Dialogue: 0,0:05:47.98,0:05:50.64,Default,,0000,0000,0000,,Di, vào mà không gõ cửa, ngươi thật vô lễ.
Dialogue: 0,0:05:50.85,0:05:52.51,Default,,0000,0000,0000,,Nàng đâu phải thượng khách.
Dialogue: 0,0:05:52.59,0:05:56.39,Default,,0000,0000,0000,,Có vẻ như nàng vẫn chưa nhận ra vị thế của mình lúc này.
Dialogue: 0,0:05:57.09,0:05:58.25,Default,,0000,0000,0000,,Ngươi tới đây làm gì?
Dialogue: 0,0:05:58.33,0:06:00.92,Default,,0000,0000,0000,,Ta vừa được tặng một vài thứ rất đặc sắc.
Dialogue: 0,0:06:01.73,0:06:03.09,Default,,0000,0000,0000,,Đặc sắc!?
Dialogue: 0,0:06:03.43,0:06:04.92,Default,,0000,0000,0000,,Đừng kích động như vậy.
Dialogue: 0,0:06:06.03,0:06:07.16,Default,,0000,0000,0000,,Thứ gì kia?
Dialogue: 0,0:06:07.80,0:06:08.79,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:06:10.94,0:06:13.24,Default,,0000,0000,0000,,Ngươi muốn làm gì ta...!!?
Dialogue: 0,0:06:16.84,0:06:18.74,Default,,0000,0000,0000,,Xem ra nàng không còn sức để chống cự nữa rồi.
Dialogue: 0,0:06:20.81,0:06:23.41,Default,,0000,0000,0000,,Ngươi vừa cho ta uông thứ gì?
Dialogue: 0,0:06:23.48,0:06:25.64,Default,,0000,0000,0000,,Đừng sốt ruột, nàng sẽ sớm biết thôi.
Dialogue: 0,0:06:25.72,0:06:27.15,Default,,0000,0000,0000,,C, Cái gì?
Dialogue: 0,0:06:31.02,0:06:32.25,Default,,0000,0000,0000,,Chuyện gì vậy, Elena?
Dialogue: 0,0:06:33.06,0:06:34.46,Default,,0000,0000,0000,,Không, không gì hết...
Dialogue: 0,0:06:35.53,0:06:38.05,Default,,0000,0000,0000,,Sao thế, nàng đau ngực hả?
Dialogue: 0,0:06:39.30,0:06:41.03,Default,,0000,0000,0000,,Ta đã bảo là không sao...
Dialogue: 0,0:06:41.67,0:06:43.00,Default,,0000,0000,0000,,Hình như nàng đang rất đau đớn.
Dialogue: 0,0:06:44.24,0:06:45.93,Default,,0000,0000,0000,,Không, đừng đụng vào ta!
Dialogue: 0,0:06:50.61,0:06:51.51,Default,,0000,0000,0000,,Dừng...
Dialogue: 0,0:06:51.58,0:06:54.55,Default,,0000,0000,0000,,Ngực nàng bự ghê nhỉ, ta sẽ vắt sữa nàng như một con bò cái.
Dialogue: 0,0:06:55.65,0:06:56.77,Default,,0000,0000,0000,,Câm miệng.
Dialogue: 0,0:06:59.12,0:07:01.25,Default,,0000,0000,0000,,Sao? Có gì đáng cười chứ?
Dialogue: 0,0:07:04.56,0:07:06.25,Default,,0000,0000,0000,,Ngực của ta...
Dialogue: 0,0:07:07.53,0:07:09.32,Default,,0000,0000,0000,,Ngực của nàng làm sao, Elena?
Dialogue: 0,0:07:11.03,0:07:14.23,Default,,0000,0000,0000,,Đau quá, như sắp vỡ tung ra vậy. Đau quá.
Dialogue: 0,0:07:16.64,0:07:18.37,Default,,0000,0000,0000,,Nàng sẽ thấy dễ chịu ngay thôi.
Dialogue: 0,0:07:19.61,0:07:20.33,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:07:21.98,0:07:22.96,Default,,0000,0000,0000,,Cảm thấy rồi hả?
Dialogue: 0,0:07:24.61,0:07:27.74,Default,,0000,0000,0000,,Không, ta không có...
Dialogue: 0,0:07:30.18,0:07:32.58,Default,,0000,0000,0000,,Có vẻ như nàng phấn khích hơn hẳn mọi khi.
Dialogue: 0,0:07:34.89,0:07:36.91,Default,,0000,0000,0000,,Không, làm ơn đừng...
Dialogue: 0,0:07:40.59,0:07:41.86,Default,,0000,0000,0000,,Chuyện này là sao?
Dialogue: 0,0:07:42.60,0:07:44.79,Default,,0000,0000,0000,,Trò vui mới chỉ bắt đầu!
Dialogue: 0,0:07:45.63,0:07:46.83,Default,,0000,0000,0000,,Thế này...?
Dialogue: 0,0:07:51.40,0:07:52.74,Default,,0000,0000,0000,,Gì đây nhỉ?
Dialogue: 0,0:07:54.34,0:07:56.00,Default,,0000,0000,0000,,Ngươi nói cái gì...
Dialogue: 0,0:07:57.58,0:07:59.70,Default,,0000,0000,0000,,Bộ nàng không phải đàn bà sao?
Dialogue: 0,0:08:03.32,0:08:05.98,Default,,0000,0000,0000,,Có điều nàng lại chưa hề sinh con.
Dialogue: 0,0:08:06.59,0:08:10.55,Default,,0000,0000,0000,,Không thể nào, ngực của ta...
Dialogue: 0,0:08:14.03,0:08:19.73,Default,,0000,0000,0000,,Khi bị ta nắn bóp hai bầu vú, nàng bấn cả lên và phọt sữa tùm lum. Thật dâm đãng!
Dialogue: 0,0:08:22.44,0:08:25.50,Default,,0000,0000,0000,,Đây là... thứ thuốc ngươi cho ta uống... chính là nó.
Dialogue: 0,0:08:26.94,0:08:29.27,Default,,0000,0000,0000,,Thông minh lắm, để ta thưởng cho nàng.
Dialogue: 0,0:08:29.34,0:08:31.44,Default,,0000,0000,0000,,Ta sẽ nắn bóp hai bầu vú kia cho tới lúc nàng lên đỉnh!
Dialogue: 0,0:08:33.45,0:08:35.51,Default,,0000,0000,0000,,Không, không được!
Dialogue: 0,0:08:37.78,0:08:40.12,Default,,0000,0000,0000,,Không, ta ra mất.
Dialogue: 0,0:08:40.69,0:08:42.42,Default,,0000,0000,0000,,Đừng lo, ta sẽ giúp nàng ra.
Dialogue: 0,0:08:44.22,0:08:45.78,Default,,0000,0000,0000,,Ra rồi!!!
Dialogue: 0,0:08:48.76,0:08:50.06,Default,,0000,0000,0000,,Ngực của ta...!
Dialogue: 0,0:08:51.16,0:08:53.50,Default,,0000,0000,0000,,Ngày hôm nay vậy là đủ.
Dialogue: 0,0:09:07.95,0:09:09.38,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:09:09.88,0:09:10.87,Default,,0000,0000,0000,,Chấp nhận ta...
Dialogue: 0,0:09:11.82,0:09:13.79,Default,,0000,0000,0000,,Chấp nhận ta...!
Dialogue: 0,0:09:13.85,0:09:15.05,Default,,0000,0000,0000,,Cái gì!?
Dialogue: 0,0:09:15.49,0:09:18.51,Default,,0000,0000,0000,,Ta sẽ cho ngươi tận hưởng những khoái lạc vượt xa thế giới này.
Dialogue: 0,0:09:20.16,0:09:23.79,Default,,0000,0000,0000,,Ta nhắc lại, hãy chấp nhận ta!
Dialogue: 0,0:09:28.07,0:09:29.80,Default,,0000,0000,0000,,Là mơ ư...?
Dialogue: 0,0:09:29.87,0:09:31.13,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:09:31.20,0:09:32.07,Default,,0000,0000,0000,,Không có gì...
Dialogue: 0,0:09:34.64,0:09:35.54,Default,,0000,0000,0000,,Ngài muốn đi đâu?
Dialogue: 0,0:09:35.88,0:09:38.47,Default,,0000,0000,0000,,Đi kiểm tra lại một chuyện. Ta sẽ quay về sớm thôi.
Dialogue: 0,0:09:43.15,0:09:43.95,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:09:51.89,0:09:52.76,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:09:53.89,0:09:54.99,Default,,0000,0000,0000,,Nii-sama?
Dialogue: 0,0:09:55.06,0:09:56.62,Default,,0000,0000,0000,,Xin lỗi, anh không định làm phiền em giờ này.
Dialogue: 0,0:09:57.16,0:09:59.56,Default,,0000,0000,0000,,Em vừa nằm mơ thấy anh, Nii-sama.
Dialogue: 0,0:09:59.93,0:10:02.90,Default,,0000,0000,0000,,Anh sẽ không để em chịu khổ ở đây mãi đâu.
Dialogue: 0,0:10:03.60,0:10:06.00,Default,,0000,0000,0000,,Nhưng đó là nghĩa vụ của một Thánh nữ.
Dialogue: 0,0:10:06.51,0:10:10.34,Default,,0000,0000,0000,,Anh sẽ đưa em rời khỏi đây như đã hứa.
Dialogue: 0,0:10:10.98,0:10:14.57,Default,,0000,0000,0000,,Vâng, nhưng một khi phong ấn đã khởi động, trốn thoát khỏi đây là điều không tưởng.
Dialogue: 0,0:10:14.95,0:10:18.54,Default,,0000,0000,0000,,Vậy sao? Anh nghĩ mình đủ sức vô hiệu hóa nó.
Dialogue: 0,0:10:19.02,0:10:19.92,Default,,0000,0000,0000,,Nii-sama...
Dialogue: 0,0:10:20.72,0:10:23.69,Default,,0000,0000,0000,,Qoona, chúng ta sẽ thử xem. Nấp vào áo choàng của anh nào.
Dialogue: 0,0:10:26.79,0:10:27.69,Default,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:10:27.76,0:10:28.66,Default,,0000,0000,0000,,Nii-sama...
Dialogue: 0,0:10:28.73,0:10:29.82,Default,,0000,0000,0000,,Qoona, em không sao chứ?
Dialogue: 0,0:10:30.13,0:10:34.12,Default,,0000,0000,0000,,Em không sao, anh đã chắn hết ma thuật dùm em.
Dialogue: 0,0:10:34.40,0:10:37.13,Default,,0000,0000,0000,,Vẫn chưa được... Xin lỗi vì đã khiến em gặp nguy hiểm.
Dialogue: 0,0:10:37.94,0:10:41.00,Default,,0000,0000,0000,,Không, Nii-sama. Em hiểu tấm lòng của anh.
Dialogue: 0,0:10:43.84,0:10:47.90,Default,,0000,0000,0000,,Đừng lo, chắc chắn anh sẽ cứu được em. Chỉ cần làm theo đúng kế hoạch.
Dialogue: 0,0:10:48.41,0:10:53.22,Default,,0000,0000,0000,,Nii-sama, chỉ gặp lại anh là em thấy mãn nguyện lắm rồi.
Dialogue: 0,0:10:53.29,0:10:55.81,Default,,0000,0000,0000,,Hiện giờ thì vậy... nhưng em không thấy cô đơn sao, Qoona?
Dialogue: 0,0:10:55.89,0:10:59.69,Default,,0000,0000,0000,,Không, bởi vì anh vẫn luôn bên em, có điều...
Dialogue: 0,0:10:59.76,0:11:00.73,Default,,0000,0000,0000,,Sao em?
Dialogue: 0,0:11:00.79,0:11:04.89,Default,,0000,0000,0000,,Em muốn được gắn bó cùng anh cả về thể xác lẫn tâm hồn.
Dialogue: 0,0:11:05.40,0:11:06.33,Default,,0000,0000,0000,,Qoona?
Dialogue: 0,0:11:06.40,0:11:11.20,Default,,0000,0000,0000,,Em biết mình là một Thánh nữ, nhưng em không thể tự lừa dối chính bản thân mình.
Dialogue: 0,0:11:11.70,0:11:15.07,Default,,0000,0000,0000,,Qoona, vậy em có biết gắn bó về thể xác là thế nào không?
Dialogue: 0,0:11:15.27,0:11:24.58,Default,,0000,0000,0000,,Vâng, là một Thánh nữ, em sẵn sàng sống trọn đời trong tòa tháp này...
Dialogue: 0,0:11:24.65,0:11:30.25,Default,,0000,0000,0000,,Nhưng... Nhưng... Em vẫn không thể quên bàn tay dịu dàng của Nii-sama.
Dialogue: 0,0:11:30.56,0:11:31.58,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:11:32.49,0:11:35.39,Default,,0000,0000,0000,,Em xin lỗi... Em không xứng là một Thánh nữ.
Dialogue: 0,0:11:36.23,0:11:38.99,Default,,0000,0000,0000,,Qoona, em quan tâm tới anh nhiều vậy sao...
Dialogue: 0,0:11:39.07,0:11:41.16,Default,,0000,0000,0000,,Anh là người duy nhất.
Dialogue: 0,0:11:41.23,0:11:43.33,Default,,0000,0000,0000,,Qoona, em nghĩ kĩ chưa?
Dialogue: 0,0:11:43.40,0:11:49.24,Default,,0000,0000,0000,,Vâng, nhưng em chưa làm điều này bao giờ. Làm ơn hãy dạy em, Nii-sama.
Dialogue: 0,0:11:56.25,0:11:58.41,Default,,0000,0000,0000,,Nii-sama, đó là...
Dialogue: 0,0:11:58.48,0:12:00.82,Default,,0000,0000,0000,,Đừng lo, thư giãn đi em.
Dialogue: 0,0:12:00.89,0:12:02.32,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:12:03.72,0:12:09.32,Default,,0000,0000,0000,,Nii-sama, cảm giác này lạ quá. Tim em đập rộn ràng và cơ thể nóng bừng lên...
Dialogue: 0,0:12:09.40,0:12:10.69,Default,,0000,0000,0000,,Qoona, chứng tỏ em đang phấn khích.
Dialogue: 0,0:12:10.76,0:12:11.89,Default,,0000,0000,0000,,Phấn khích?
Dialogue: 0,0:12:11.96,0:12:13.96,Default,,0000,0000,0000,,Đúng vậy, anh sẽ làm em sung sướng hơn nữa.
Dialogue: 0,0:12:21.44,0:12:23.24,Default,,0000,0000,0000,,Qoona, chúng ta sẽ cùng nhau hòa làm một.
Dialogue: 0,0:12:28.88,0:12:30.51,Default,,0000,0000,0000,,Nhắm mặt lại em, sẽ dễ chịu hơn một chút.
Dialogue: 0,0:12:31.05,0:12:32.14,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:12:35.76,0:12:37.09,Default,,0000,0000,0000,,Đau lắm phải không? Anh ngừng lại nhé?
Dialogue: 0,0:12:39.93,0:12:43.12,Default,,0000,0000,0000,,Nii-sama, xin anh... hãy làm tới cùng.
Dialogue: 0,0:12:49.24,0:12:53.04,Default,,0000,0000,0000,,Qoona, chúng ta cuối cùng đã thuộc về nhau.
Dialogue: 0,0:12:54.01,0:12:55.67,Default,,0000,0000,0000,,Với Nii-sama...
Dialogue: 0,0:12:55.74,0:12:59.30,Default,,0000,0000,0000,,Phải rồi, chúng ta đã hòa làm một.
Dialogue: 0,0:13:00.21,0:13:01.84,Default,,0000,0000,0000,,Em vui lắm...
Dialogue: 0,0:13:03.68,0:13:05.55,Default,,0000,0000,0000,,Rất vui.
Dialogue: 0,0:13:05.62,0:13:06.78,Default,,0000,0000,0000,,Anh làm đây.
Dialogue: 0,0:13:07.79,0:13:08.95,Default,,0000,0000,0000,,Khít quá!
Dialogue: 0,0:13:09.66,0:13:10.75,Default,,0000,0000,0000,,Em sợ!
Dialogue: 0,0:13:10.82,0:13:12.62,Default,,0000,0000,0000,,Đừng sợ, có anh ở đây rồi.
Dialogue: 0,0:13:12.69,0:13:15.13,Default,,0000,0000,0000,,Nii-sama!!
Dialogue: 0,0:13:18.66,0:13:19.79,Default,,0000,0000,0000,,Anh sẽ ra bên ngoài.
Dialogue: 0,0:13:19.87,0:13:23.17,Default,,0000,0000,0000,,Không, đừng anh! Đừng rút nó ra!
Dialogue: 0,0:13:23.24,0:13:24.29,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:13:24.37,0:13:26.90,Default,,0000,0000,0000,,Nii-sama, em ko muốn anh rút ra, xin anh đấy.
Dialogue: 0,0:13:27.14,0:13:28.00,Default,,0000,0000,0000,,Em chắc chứ?
Dialogue: 0,0:13:28.07,0:13:29.70,Default,,0000,0000,0000,,Chỉ dành riêng cho anh thôi.
Dialogue: 0,0:13:35.08,0:13:37.52,Default,,0000,0000,0000,,Anh đang ra bên trong em...
Dialogue: 0,0:13:43.49,0:13:45.05,Default,,0000,0000,0000,,Tinh dịch của Nii-sama...
Dialogue: 0,0:13:45.12,0:13:47.06,Default,,0000,0000,0000,,Đó là minh chứng cho tình yêu của đôi ta.
Dialogue: 0,0:13:48.06,0:13:49.72,Default,,0000,0000,0000,,Nii-sama...
Dialogue: 0,0:14:03.34,0:14:07.64,Default,,0000,0000,0000,,Tiến lên! Chỉ cần hạ được thành trì này, kinh đô Bastarauge sẽ ở ngay trước mặt chúng ta!!
Dialogue: 0,0:14:09.52,0:14:15.78,Default,,0000,0000,0000,,Noi theo sự quả cảm của Distol, binh sĩ Asteria chẳng mấy khó khăn san bằng mọi thành trì của Bastarauge.
Dialogue: 0,0:14:16.89,0:14:23.16,Default,,0000,0000,0000,,Tuy nhiên, dù cách kinh đô Bastarauge không còn bao xa, cuộc chiến vẫn tiếp diễn.
Dialogue: 0,0:14:23.23,0:14:27.29,Default,,0000,0000,0000,,Lúc này, họ phải đối phó với một kẻ địch hùng mạnh, loài Elf.
Dialogue: 0,0:14:27.37,0:14:30.13,Default,,0000,0000,0000,,Nghe đồn có một phù thủy sống trong ngôi làng này.
Dialogue: 0,0:14:31.17,0:14:35.63,Default,,0000,0000,0000,,Chỉ cần san bằng ngôi làng này, kinh đô coi như đã thuộc về chúng ta. Các anh em, theo ta!
Dialogue: 0,0:14:38.11,0:14:39.51,Default,,0000,0000,0000,,Đi theo Hoàng tử!
Dialogue: 0,0:14:39.58,0:14:41.57,Default,,0000,0000,0000,,Asteria muôn năm!
Dialogue: 0,0:14:41.65,0:14:43.11,Default,,0000,0000,0000,,Tấn công!!!
Dialogue: 0,0:14:47.95,0:14:49.65,Default,,0000,0000,0000,,Hoàng tử, có rắc rối!
Dialogue: 0,0:14:50.39,0:14:51.91,Default,,0000,0000,0000,,Chuyện gì vậy!?
Dialogue: 0,0:14:53.23,0:14:56.09,Default,,0000,0000,0000,,Hãy cẩn thận, chúng là tộc Elf. Rất khó chơi đấy.
Dialogue: 0,0:14:56.70,0:15:00.39,Default,,0000,0000,0000,,Ta biết, ta sẽ đích thân đối mặt với Yuno.
Dialogue: 0,0:15:00.93,0:15:04.30,Default,,0000,0000,0000,,Ngài không cần ra tay, cứ để cô ta cho tôi.
Dialogue: 0,0:15:04.74,0:15:07.54,Default,,0000,0000,0000,,Cô tự tin ma thuật của mình mạnh hơn loài Elf ư?
Dialogue: 0,0:15:08.07,0:15:10.57,Default,,0000,0000,0000,,Xin ngài đợi cho một lát, Chủ nhân.
Dialogue: 0,0:15:16.45,0:15:18.75,Default,,0000,0000,0000,,Tại sao mình phải bỏ chạy trước con người chứ?
Dialogue: 0,0:15:25.29,0:15:27.95,Default,,0000,0000,0000,,Mình cần nghỉ ngơi một chút...
Dialogue: 0,0:15:28.69,0:15:33.03,Default,,0000,0000,0000,,Tại sao cây cối không trò chuyện với mình nữa?
Dialogue: 0,0:15:33.10,0:15:35.36,Default,,0000,0000,0000,,Là do mình không thể tập trung ư?
Dialogue: 0,0:15:37.37,0:15:38.93,Default,,0000,0000,0000,,Rừng cây đang than khóc?
Dialogue: 0,0:15:47.08,0:15:49.21,Default,,0000,0000,0000,,Ma thuật của mình... đang dần phân tán.
Dialogue: 0,0:15:49.28,0:15:50.72,Default,,0000,0000,0000,,Chuyện gì vậy!?
Dialogue: 0,0:15:50.78,0:15:52.94,Default,,0000,0000,0000,,Ở yên đó.
Dialogue: 0,0:15:55.49,0:15:59.29,Default,,0000,0000,0000,,Tại sao ta không thể cảm nhận được ma thuật của ngươi? Ngươi là ai!?
Dialogue: 0,0:16:02.86,0:16:04.56,Default,,0000,0000,0000,,Dễ thương lắm, Yuno.
Dialogue: 0,0:16:05.40,0:16:07.33,Default,,0000,0000,0000,,Cô tưởng có thể trốn khỏi tay ta dễ dàng vậy sao?
Dialogue: 0,0:16:07.80,0:16:09.46,Default,,0000,0000,0000,,Ngươi muốn gì?
Dialogue: 0,0:16:10.07,0:16:15.51,Default,,0000,0000,0000,,Vẫn kiêu ngạo như xưa. Bây giờ cô là tù nhân của ta, ta muốn làm gì mà chẳng được.
Dialogue: 0,0:16:15.94,0:16:20.50,Default,,0000,0000,0000,,Vậy mau thả ta ra. Ta sẽ trở thành một tù nhân si ngốc của ngươi.
Dialogue: 0,0:16:20.58,0:16:23.78,Default,,0000,0000,0000,,Tù nhân mà dám ăn nói với chủ vậy sao, Yuno.
Dialogue: 0,0:16:23.85,0:16:27.55,Default,,0000,0000,0000,,Có nên trừng phạt cô ta một chút không, Eleanor?
Dialogue: 0,0:16:28.69,0:16:30.62,Default,,0000,0000,0000,,Tùy ngài quyết định, Chủ nhân.
Dialogue: 0,0:16:31.09,0:16:32.35,Default,,0000,0000,0000,,Trừng phạt?
Dialogue: 0,0:16:33.63,0:16:38.56,Default,,0000,0000,0000,,Dù là Elf, nhưng chắc cô cũng biết chuyện gì sẽ xảy đến với tù binh nữ.
Dialogue: 0,0:16:38.63,0:16:41.79,Default,,0000,0000,0000,,Không, đừng có hòng!
Dialogue: 0,0:16:52.18,0:16:53.04,Default,,0000,0000,0000,,Tại sao!?
Dialogue: 0,0:16:53.58,0:16:56.28,Default,,0000,0000,0000,,Vấn muốn kháng cự sao?
Dialogue: 0,0:16:56.88,0:16:58.85,Default,,0000,0000,0000,,Chắc chắn là do ngươi làm.
Dialogue: 0,0:17:00.09,0:17:02.08,Default,,0000,0000,0000,,Chủ nhân, hẹn gặp lại ngài sau.
Dialogue: 0,0:17:03.79,0:17:05.88,Default,,0000,0000,0000,,Ma thuật mạnh hơn cả loài Elf... ?
Dialogue: 0,0:17:07.76,0:17:10.02,Default,,0000,0000,0000,,Chúng ta bắt đầu vui đùa thôi nhỉ?
Dialogue: 0,0:17:10.50,0:17:11.62,Default,,0000,0000,0000,,S, Sao cơ!?
Dialogue: 0,0:17:19.87,0:17:22.77,Default,,0000,0000,0000,,Nghe nói tộc Elf các người rất thanh nhã.
Dialogue: 0,0:17:25.54,0:17:28.48,Default,,0000,0000,0000,,Lần đầu tiên ta cùng với một nàng Elf...
Dialogue: 0,0:17:28.55,0:17:29.84,Default,,0000,0000,0000,,D-Dừng lại!
Dialogue: 0,0:17:30.38,0:17:32.32,Default,,0000,0000,0000,,Buông ra, đừng động vào ta!
Dialogue: 0,0:17:33.82,0:17:37.41,Default,,0000,0000,0000,,Muốn ta dừng lại, sao không dùng ma thuật của cô đi?
Dialogue: 0,0:17:40.29,0:17:40.99,Default,,0000,0000,0000,,Không, không!
Dialogue: 0,0:17:41.66,0:17:44.22,Default,,0000,0000,0000,,Hai đầu vú của cô cứng lên rồi nè, Yuno.
Dialogue: 0,0:17:45.70,0:17:47.00,Default,,0000,0000,0000,,Sướng không hả?
Dialogue: 0,0:17:47.07,0:17:49.66,Default,,0000,0000,0000,,Câm miệng, con người thấp kém...!
Dialogue: 0,0:17:52.20,0:17:54.47,Default,,0000,0000,0000,,Dừng lại, đừng véo ta!
Dialogue: 0,0:17:56.64,0:17:59.11,Default,,0000,0000,0000,,Không... Không được!!
Dialogue: 0,0:18:00.41,0:18:03.94,Default,,0000,0000,0000,,Mùi hương thơm tho này là sao?
Dialogue: 0,0:18:05.32,0:18:08.58,Default,,0000,0000,0000,,Khi ướt đẫm, Elf thường tỏa ra mùi này sao.
Dialogue: 0,0:18:09.32,0:18:11.95,Default,,0000,0000,0000,,Không, ta không thể... không...
Dialogue: 0,0:18:14.26,0:18:15.75,Default,,0000,0000,0000,,Để ta xem nào.
Dialogue: 0,0:18:16.43,0:18:17.95,Default,,0000,0000,0000,,Không, cấm ngươi chạm vào chỗ đó!
Dialogue: 0,0:18:19.80,0:18:25.03,Default,,0000,0000,0000,,Chưa gì đã ướt đẫm như vậy rồi. Loài Elf quả là độc đáo.
Dialogue: 0,0:18:25.84,0:18:27.67,Default,,0000,0000,0000,,Đừng nói nữa!
Dialogue: 0,0:18:28.27,0:18:29.61,Default,,0000,0000,0000,,Ta... Ta...
Dialogue: 0,0:18:31.88,0:18:36.04,Default,,0000,0000,0000,,Dù cố gắng che giấu bản thân tới mức nào, điểm vẫn muôn đời là điếm.
Dialogue: 0,0:18:37.68,0:18:39.55,Default,,0000,0000,0000,,D-Dừng lại... xin ngươi!!
Dialogue: 0,0:18:40.19,0:18:42.35,Default,,0000,0000,0000,,Dừng... cái này hả!?
Dialogue: 0,0:18:44.26,0:18:45.35,Default,,0000,0000,0000,,Không!!!!!!
Dialogue: 0,0:18:48.73,0:18:51.56,Default,,0000,0000,0000,,Không, đau quá!
Dialogue: 0,0:18:55.60,0:18:57.97,Default,,0000,0000,0000,,Vậy ra đây là lần đầu của cô.
Dialogue: 0,0:19:02.07,0:19:02.87,Default,,0000,0000,0000,,Tuyệt thật!
Dialogue: 0,0:19:03.78,0:19:07.11,Default,,0000,0000,0000,,Thấy dương vật của con người ra sao, cô nàng Elf!
Dialogue: 0,0:19:07.61,0:19:10.64,Default,,0000,0000,0000,,Dừng lại đi, xin ngươi đó!
Dialogue: 0,0:19:11.15,0:19:14.81,Default,,0000,0000,0000,,Sao thế? Ta tưởng cô luôn khinh rẻ con người chứ?
Dialogue: 0,0:19:15.32,0:19:18.45,Default,,0000,0000,0000,,Đau quá... Làm ơn hãy làm nhẹ thôi...
Dialogue: 0,0:19:19.39,0:19:21.09,Default,,0000,0000,0000,,Ta sẽ làm cô sung sướng.
Dialogue: 0,0:19:27.23,0:19:28.83,Default,,0000,0000,0000,,Không, không...
Dialogue: 0,0:19:30.44,0:19:34.46,Default,,0000,0000,0000,,Cô nói không, nhưng âm vật cô đang nói có.
Dialogue: 0,0:19:36.41,0:19:39.28,Default,,0000,0000,0000,,Không, đừng động vào chỗ đó...
Dialogue: 0,0:19:39.35,0:19:41.01,Default,,0000,0000,0000,,Cô thích vậy mà, phải không!?
Dialogue: 0,0:19:41.91,0:19:45.51,Default,,0000,0000,0000,,Con người đang cưỡng bức mình, nhưng tại sao...?
Dialogue: 0,0:19:47.29,0:19:49.65,Default,,0000,0000,0000,,Cô bị con người mê hoặc rồi sao?
Dialogue: 0,0:19:51.49,0:19:53.05,Default,,0000,0000,0000,,Nữa... nữa đi.
Dialogue: 0,0:19:55.19,0:19:58.46,Default,,0000,0000,0000,,Nóng quá... Sướng quá đi mất.
Dialogue: 0,0:19:59.47,0:20:01.09,Default,,0000,0000,0000,,Đồ Elf dâm đãng.
Dialogue: 0,0:20:02.70,0:20:05.93,Default,,0000,0000,0000,,Nó đang đâm sâu vào bên trong ta...
Dialogue: 0,0:20:08.81,0:20:10.54,Default,,0000,0000,0000,,Cô thích thế này chứ?
Dialogue: 0,0:20:14.21,0:20:17.18,Default,,0000,0000,0000,,Không, dừng lại... đừng chạm vào tai ta...
Dialogue: 0,0:20:18.58,0:20:20.31,Default,,0000,0000,0000,,Dễ thương ghê, Yuno.
Dialogue: 0,0:20:21.42,0:20:25.29,Default,,0000,0000,0000,,Không... Ta sẽ... phát điên mất...
Dialogue: 0,0:20:26.73,0:20:30.49,Default,,0000,0000,0000,,Ta... tuyệt... quá đi mất...
Dialogue: 0,0:20:32.03,0:20:33.76,Default,,0000,0000,0000,,Nó đang khít lại!
Dialogue: 0,0:20:36.44,0:20:39.73,Default,,0000,0000,0000,,Ngay tại đó, sướng quá đi mất.
Dialogue: 0,0:20:43.31,0:20:45.50,Default,,0000,0000,0000,,Nó đang bự lên bên trong!!
Dialogue: 0,0:20:52.69,0:20:56.14,Default,,0000,0000,0000,,Ta ra rồi, ra rồi, ra rồi!!!
Dialogue: 0,0:20:59.96,0:21:03.66,Default,,0000,0000,0000,,Cảm giác bị con người cưỡng bức thế nào hả? Có thích không?
Dialogue: 0,0:21:04.36,0:21:07.80,Default,,0000,0000,0000,,Giờ hãy khiến ta thấy thoải mái đi... bú nó xem nào.
Dialogue: 0,0:21:08.87,0:21:10.60,Default,,0000,0000,0000,,Thứ này...?
Dialogue: 0,0:21:11.44,0:21:18.24,Default,,0000,0000,0000,,Đúng vậy, ta muốn xem tộc Elf làm chuyện này ra sao. Mau lên nào.
Dialogue: 0,0:21:21.25,0:21:26.65,Default,,0000,0000,0000,,Chết tiệt, cô làm tuyệt lắm. Tiếp tục đi.
Dialogue: 0,0:21:31.12,0:21:33.89,Default,,0000,0000,0000,,Ta sẽ đáp lễ cho cô.
Dialogue: 0,0:21:35.33,0:21:37.56,Default,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:21:39.63,0:21:43.26,Default,,0000,0000,0000,,Chơi Elf từ phía sau mới tuyệt làm sao.
Dialogue: 0,0:21:57.65,0:21:58.98,Default,,0000,0000,0000,,Ta ra đây!
Dialogue: 0,0:22:06.13,0:22:07.68,Default,,0000,0000,0000,,Cô làm khá lắm, Yuno.
Dialogue: 0,0:22:12.56,0:22:14.46,Default,,0000,0000,0000,,Có vẻ như ngài đang rất cao hứng, Chủ nhân.
Dialogue: 0,0:22:14.53,0:22:15.90,Default,,0000,0000,0000,,Cô nhìn lén chúng ta ư, Eleanor?
Dialogue: 0,0:22:15.97,0:22:19.06,Default,,0000,0000,0000,,Ồ không, tôi không việc gì phải làm như vậy.
Dialogue: 0,0:22:19.54,0:22:22.51,Default,,0000,0000,0000,,Thôi bỏ đi, nhưng giờ xử lí cô ta sao đây?
Dialogue: 0,0:22:23.14,0:22:26.63,Default,,0000,0000,0000,,Quyền năng của cô ta sẽ giúp ích cho ngài, Chủ nhân.
Dialogue: 0,0:22:27.25,0:22:31.08,Default,,0000,0000,0000,,Được rồi, hãy giam cô ta vào ngục.
Dialogue: 0,0:22:31.55,0:22:33.21,Default,,0000,0000,0000,,Tuân lệnh, Chủ nhân...
Dialogue: 0,0:22:33.92,0:22:40.38,Default,,0000,0000,0000,,Có lẽ do ta vừa ái ân với loài Elf, dục vọng đang trào dâng trong cơ thể ta.
Dialogue: 0,0:22:44.90,0:22:49.03,Default,,0000,0000,0000,,Distol, ngươi lại tới để làm nhục ta phải không?
Dialogue: 0,0:22:52.20,0:22:56.61,Default,,0000,0000,0000,,Dù ngươi dùng bất cứ thủ đoạn gì, ta cũng sẽ không đầu hàng đâu!
Dialogue: 0,0:22:57.28,0:23:01.30,Default,,0000,0000,0000,,Dù có bày ra mưu ma chước quỷ gì, ngươi cũng không thể làm vấy bẩn linh hồn ta!
Dialogue: 0,0:23:02.15,0:23:03.44,Default,,0000,0000,0000,,Ngươi làm gì vậy?
Dialogue: 0,0:23:03.52,0:23:06.48,Default,,0000,0000,0000,,Ta hỏi, ngươi đang làm cái gì vậy, Distol!?
Dialogue: 0,0:23:06.55,0:23:08.28,Default,,0000,0000,0000,,Nàng nhiều lời quá đó, Elena.
Dialogue: 0,0:23:08.52,0:23:09.45,Default,,0000,0000,0000,,Ngươi nói cái gì?
Dialogue: 0,0:23:10.16,0:23:14.72,Default,,0000,0000,0000,,Uhm, đây đâu phải lần đầu. Mỗi khi chúng ta ân ái với nhau...
Dialogue: 0,0:23:14.83,0:23:17.66,Default,,0000,0000,0000,,nàng luôn rên la như một con cún.
Dialogue: 0,0:23:18.60,0:23:22.16,Default,,0000,0000,0000,,Rên dữ như vậy, hẳn nàng rất khát nước.
Dialogue: 0,0:23:25.57,0:23:28.94,Default,,0000,0000,0000,,Đó là cái gì? Ngươi định làm gì ta?
Dialogue: 0,0:23:29.01,0:23:34.50,Default,,0000,0000,0000,,Nàng đừng lo, không phải thuốc độc đâu. Nó chỉ có thêm vài tác dụng phụ mà thôi.
Dialogue: 0,0:23:35.48,0:23:38.31,Default,,0000,0000,0000,,Dừng lại. Distol, làm ơn dừng lại.
Dialogue: 0,0:23:38.98,0:23:41.92,Default,,0000,0000,0000,,Ta tưởng nàng không thèm quan tâm.
Dialogue: 0,0:23:41.99,0:23:44.32,Default,,0000,0000,0000,,Ta không có nói...!
Dialogue: 0,0:23:47.29,0:23:49.49,Default,,0000,0000,0000,,Để xem nàng chống cự được bao lâu.
Dialogue: 0,0:23:51.56,0:23:53.03,Default,,0000,0000,0000,,Dừng lại, Distol!
Dialogue: 0,0:23:55.07,0:23:57.19,Default,,0000,0000,0000,,Nàng sẽ không đầu hàng sớm vậy đâu.
Dialogue: 0,0:23:57.27,0:23:58.20,Default,,0000,0000,0000,,C-Cái gì!?
Dialogue: 0,0:23:59.51,0:24:02.30,Default,,0000,0000,0000,,Ngươi tưởng có thể biến ta thành nô lệ với loại thuốc đó ư?
Dialogue: 0,0:24:04.11,0:24:06.37,Default,,0000,0000,0000,,Sao vậy, Elena?
Dialogue: 0,0:24:08.65,0:24:12.28,Default,,0000,0000,0000,,Dừng lại, đừng cắn chỗ đó...!!!
Dialogue: 0,0:24:14.29,0:24:15.81,Default,,0000,0000,0000,,Oh, gì thế này?
Dialogue: 0,0:24:15.89,0:24:18.98,Default,,0000,0000,0000,,Lạ quá, cơ thể ta đang nóng bừng lên...
Dialogue: 0,0:24:19.83,0:24:21.79,Default,,0000,0000,0000,,Dưới này thế nào hả?
Dialogue: 0,0:24:24.96,0:24:28.13,Default,,0000,0000,0000,,Dừng lại. Ta...
Dialogue: 0,0:24:28.20,0:24:31.17,Default,,0000,0000,0000,,Tuyệt thật, nàng ướt đẫm rồi.
Dialogue: 0,0:24:33.47,0:24:36.74,Default,,0000,0000,0000,,Tại sao... không giống mình ngày thường chút nào...
Dialogue: 0,0:24:42.28,0:24:44.94,Default,,0000,0000,0000,,Nàng muốn phải không, Elena?
Dialogue: 0,0:24:46.49,0:24:49.18,Default,,0000,0000,0000,,Ngu ngốc... Ta không đời nào...
Dialogue: 0,0:24:52.99,0:24:54.58,Default,,0000,0000,0000,,Nếu tiếp tục, ta...!
Dialogue: 0,0:24:55.39,0:24:57.49,Default,,0000,0000,0000,,Ta sẽ chiều lòng nàng, Elena!
Dialogue: 0,0:25:00.67,0:25:02.96,Default,,0000,0000,0000,,Không. Dừng lại...
Dialogue: 0,0:25:08.11,0:25:12.48,Default,,0000,0000,0000,,Ta vừa mới đâm vào mà nàng đã ra luôn rồi, tuy nhiên, cuộc vui mới chỉ bắt đầu, Elena!
Dialogue: 0,0:25:16.15,0:25:20.85,Default,,0000,0000,0000,,Làm ơn... đừng... mà...
Dialogue: 0,0:25:23.86,0:25:25.38,Default,,0000,0000,0000,,Càng lúc càng ướt hơn.
Dialogue: 0,0:25:26.63,0:25:27.59,Default,,0000,0000,0000,,Thấy không...
Dialogue: 0,0:25:29.03,0:25:30.49,Default,,0000,0000,0000,,Tuyệt quá đi...
Dialogue: 0,0:25:35.63,0:25:37.93,Default,,0000,0000,0000,,Ta ... lại ra rồi.!!
Dialogue: 0,0:25:40.54,0:25:44.13,Default,,0000,0000,0000,,Ra tới hai lần trong một hiệp, Công chúa gì mà dâm đãng vậy.
Dialogue: 0,0:25:45.28,0:25:48.51,Default,,0000,0000,0000,,Nếu tiếp tục, ta sẽ phát điên mất!
Dialogue: 0,0:25:49.52,0:25:52.54,Default,,0000,0000,0000,,Tất nhiên, đó chính là mục đích của ta.
Dialogue: 0,0:25:57.22,0:25:59.16,Default,,0000,0000,0000,,Nhận lấy này!
Dialogue: 0,0:26:25.72,0:26:28.74,Default,,0000,0000,0000,,Phải rồi, sa ngã trong vực thẳm hoan lạc.
Dialogue: 0,0:26:28.82,0:26:31.22,Default,,0000,0000,0000,,Ước mơ của ta đã trở thành hiện thực!
Dialogue: 0,0:26:31.29,0:26:35.66,Default,,0000,0000,0000,,Ta sẽ có cả Bastarauge và nàng!
