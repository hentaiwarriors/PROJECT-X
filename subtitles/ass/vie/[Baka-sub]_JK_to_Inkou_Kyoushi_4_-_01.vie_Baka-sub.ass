Dialogue: 0,0:00:19.28,0:00:20.83,Default,,0000,0000,0000,,Sensei.
Dialogue: 0,0:00:23.54,0:00:24.62,Default,,0000,0000,0000,,Tuyệt lắm phải không?
Dialogue: 0,0:00:25.12,0:00:26.88,Default,,0000,0000,0000,,Sướng lắm đúng không?
Dialogue: 0,0:00:34.47,0:00:37.89,Default,,0000,0000,0000,,Thằng nhỏ của Sensei lớn quá...
Dialogue: 0,0:00:44.60,0:00:48.69,Default,,0000,0000,0000,,Thằng nhỏ của Sensei thật là cứng.
Dialogue: 0,0:00:49.77,0:00:51.61,Default,,0000,0000,0000,,Sensei.
Dialogue: 0,0:00:57.16,0:00:58.28,Default,,0000,0000,0000,,Sensei.
Dialogue: 0,0:00:58.28,0:00:59.83,Default,,0000,0000,0000,,Hân hạnh được biết thầy.
Dialogue: 0,0:01:01.45,0:01:05.58,Default,,0000,0000,0000,,Ơ... Học sinh chuyển trường? Vào lúc này ư?
Dialogue: 0,0:01:05.71,0:01:06.92,Default,,0000,0000,0000,,Sao lại đột ngột vậy?
Dialogue: 0,0:01:06.92,0:01:11.38,Default,,0000,0000,0000,,Kể ra thì dài dòng lắm, chuyện còn lại nhờ cả vào thầy.
Dialogue: 0,0:01:11.38,0:01:12.50,Default,,0000,0000,0000,,V-Vâng...
Dialogue: 0,0:01:12.80,0:01:15.97,Default,,0000,0000,0000,,Trường nữ sinh này thật là đẹp.
Dialogue: 0,0:01:16.30,0:01:18.55,Default,,0000,0000,0000,,Em thật mừng vì đã chuyển đến đây.
Dialogue: 0,0:01:18.80,0:01:21.01,Default,,0000,0000,0000,,Vậy ư?
Dialogue: 0,0:01:21.68,0:01:26.52,Default,,0000,0000,0000,,Mà tại sao em lại chuyển trường vào lúc này? Và thầy hiệu phó cũng cư xử thật kì lạ.
Dialogue: 0,0:01:26.81,0:01:31.44,Default,,0000,0000,0000,,Trường cũ trước đây của em đầy kẻ phá hoại nhưng chỗ này thật tuyệt vời.
Dialogue: 0,0:01:31.44,0:01:32.52,Default,,0000,0000,0000,,Cả Sensei nữa.
Dialogue: 0,0:01:33.82,0:01:37.28,Default,,0000,0000,0000,,Sensei, mau dẫn em đi tham quan trường đi nào!
Dialogue: 0,0:01:46.62,0:01:50.08,Default,,0000,0000,0000,,Còn rất nhiều thứ bọn mình chưa biết, nên xin được mọi người chiếu cố.
Dialogue: 0,0:01:50.21,0:01:53.17,Default,,0000,0000,0000,,Bọn em là người mẫu ư? Thật là tuyệt.
Dialogue: 0,0:01:53.17,0:01:55.88,Default,,0000,0000,0000,,Hèn gì bọn em thật năng động.
Dialogue: 0,0:01:56.94,0:01:58.97,Default,,0000,0000,0000,,Cảm ơn thầy rất nhiều.
Dialogue: 0,0:01:58.97,0:02:03.43,Default,,0000,0000,0000,,Thế Sensei, thầy có biết người phục dịch cho bọn em là ai không ạ?
Dialogue: 0,0:02:04.68,0:02:06.73,Default,,0000,0000,0000,,Cái gì thế? Phục dịch gì?
Dialogue: 0,0:02:07.98,0:02:11.15,Default,,0000,0000,0000,,Ơ... Thế thầy không nghe thầy hiệu phó nói gì sao?
Dialogue: 0,0:02:11.94,0:02:14.69,Default,,0000,0000,0000,,Phải làm sao đây? Bây giờ mình sắp phải đi làm rồi.
Dialogue: 0,0:02:15.50,0:02:16.69,Default,,0000,0000,0000,,Mình phải đi hỏi mới được.
Dialogue: 0,0:02:16.93,0:02:19.24,Default,,0000,0000,0000,,Vậy thôi, em phải đi chụp ảnh đây.
Dialogue: 0,0:02:19.24,0:02:20.87,Default,,0000,0000,0000,,Gặp lại thầy sao, Sensei.
Dialogue: 0,0:02:21.07,0:02:22.62,Default,,0000,0000,0000,,Đi thôi, Satsuki.
Dialogue: 0,0:02:24.22,0:02:26.60,Default,,0000,0000,0000,,Ý bọn chúng là sao, phục dịch ư?
Dialogue: 0,0:02:27.55,0:02:28.33,Default,,0000,0000,0000,,Tuyệt lắm...!
Dialogue: 0,0:02:28.47,0:02:29.37,Default,,0000,0000,0000,,Cười lên nào!
Dialogue: 0,0:02:29.67,0:02:30.38,Default,,0000,0000,0000,,Như thế...
Dialogue: 0,0:02:30.81,0:02:32.00,Default,,0000,0000,0000,,Cô đang tỏa sáng!
Dialogue: 0,0:02:32.21,0:02:33.75,Default,,0000,0000,0000,,Cô cứ như một nữ thần!
Dialogue: 0,0:02:33.76,0:02:34.36,Default,,0000,0000,0000,,Tuyệt vời!
Dialogue: 0,0:02:34.46,0:02:35.34,Default,,0000,0000,0000,,Đẹp lắm.
Dialogue: 0,0:02:35.83,0:02:37.48,Default,,0000,0000,0000,,Hơ, mệt quá.
Dialogue: 0,0:02:37.67,0:02:39.01,Default,,0000,0000,0000,,Kĩ năng của bọn họ thật nghèo nàn.
Dialogue: 0,0:02:39.01,0:02:39.80,Default,,0000,0000,0000,,Thật là...
Dialogue: 0,0:02:40.43,0:02:42.26,Default,,0000,0000,0000,,Cậu vẫn ổn chứ, Satsuki?
Dialogue: 0,0:02:42.71,0:02:45.22,Default,,0000,0000,0000,,Ừm, tớ vẫn ổn.
Dialogue: 0,0:02:47.23,0:02:50.35,Default,,0000,0000,0000,,Sensei? Sao thế? Sao thầy lại ở đây?
Dialogue: 0,0:02:50.94,0:02:54.73,Default,,0000,0000,0000,,Thầy hiệu phó bảo thầy trông chừng bọn con gái các em.
Dialogue: 0,0:02:54.73,0:02:56.03,Default,,0000,0000,0000,,Nên thầy đến đây.
Dialogue: 0,0:02:56.43,0:02:57.48,Default,,0000,0000,0000,,Vậy ư...
Dialogue: 0,0:03:00.09,0:03:05.02,Default,,0000,0000,0000,,Thầy hiệu phó bảo là sẽ lập tức chỉ định một người phục dịch.
Dialogue: 0,0:03:05.45,0:03:09.50,Default,,0000,0000,0000,,Ồ, đúng vậy, thầy ấy bảo thầy sẽ là người phục dịch để trông chừng bọn em.
Dialogue: 0,0:03:09.50,0:03:12.32,Default,,0000,0000,0000,,Hả? Cái gì!? Em không hề nghe chuyện đó.
Dialogue: 0,0:03:12.96,0:03:14.88,Default,,0000,0000,0000,,Satsui, cậu có nghe gì về nó không?
Dialogue: 0,0:03:15.71,0:03:16.96,Default,,0000,0000,0000,,Tớ không nghe gì cả...
Dialogue: 0,0:03:19.30,0:03:22.01,Default,,0000,0000,0000,,Chờ đã, đừng có đùa như vậy chứ?
Dialogue: 0,0:03:24.35,0:03:26.26,Default,,0000,0000,0000,,Hiệu phó phải không?
Dialogue: 0,0:03:26.26,0:03:29.22,Default,,0000,0000,0000,,Ông biết thỏa thuận giữa chúng ta chứ?
Dialogue: 0,0:03:29.64,0:03:30.31,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:03:30.31,0:03:32.02,Default,,0000,0000,0000,,Không phù hợp gì cả.
Dialogue: 0,0:03:32.60,0:03:34.06,Default,,0000,0000,0000,,Một người trẻ tuổi, đẹp trai...
Dialogue: 0,0:03:34.06,0:03:34.65,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:03:35.93,0:03:37.30,Default,,0000,0000,0000,,Trẻ tuổi đẹp trai gì chứ...
Dialogue: 0,0:03:37.67,0:03:39.69,Default,,0000,0000,0000,,Hử? Đó là chuyện của ông, không phải sao?
Dialogue: 0,0:03:40.15,0:03:41.65,Default,,0000,0000,0000,,Tôi không chấp nhận chuyện đó.
Dialogue: 0,0:03:42.51,0:03:45.42,Default,,0000,0000,0000,,Cái gì chứ? Ông đang định sắp đặt tôi sao?
Dialogue: 0,0:03:45.41,0:03:47.16,Default,,0000,0000,0000,,Ông nghĩ bọn tôi sẽ bị lừa dễ dàng như vậy ư?
Dialogue: 0,0:03:47.45,0:03:48.29,Default,,0000,0000,0000,,Này, Tagai?
Dialogue: 0,0:03:48.29,0:03:51.21,Default,,0000,0000,0000,,Im đi! Câm miệng lại, tôi đang bàn chuyện quan trọng.
Dialogue: 0,0:03:51.41,0:03:52.00,Default,,0000,0000,0000,,Đồ phì lũ.
Dialogue: 0,0:03:52.75,0:03:55.99,Default,,0000,0000,0000,,Wakao, chuyện gì đã xảy ra với Takai vậy?
Dialogue: 0,0:03:56.71,0:03:59.00,Default,,0000,0000,0000,,Bây giờ tôi không thể rút lui được.
Dialogue: 0,0:04:00.02,0:04:02.60,Default,,0000,0000,0000,,Vậy đi, tôi sẽ cần thêm vài thứ tiện nghi nữa.
Dialogue: 0,0:04:04.80,0:04:06.10,Default,,0000,0000,0000,,Cái quái gì vậy?
Dialogue: 0,0:04:06.18,0:04:08.22,Default,,0000,0000,0000,,Mình đã bảo ông ta phải biết giữ lời vậy mà...
Dialogue: 0,0:04:08.22,0:04:10.98,Default,,0000,0000,0000,,Đồ đầu hói ngu ngốc, đừng có đùa với tôi.
Dialogue: 0,0:04:11.06,0:04:12.48,Default,,0000,0000,0000,,Này!
Dialogue: 0,0:04:12.64,0:04:14.69,Default,,0000,0000,0000,,Ông ta quá xem thường mình.
Dialogue: 0,0:04:15.23,0:04:19.23,Default,,0000,0000,0000,,Làm sao tôi có thể chịu đựng được gã phì lũ này chứ.
Dialogue: 0,0:04:19.23,0:04:21.24,Default,,0000,0000,0000,,Này, Tagai...
Dialogue: 0,0:04:21.24,0:04:22.53,Default,,0000,0000,0000,,Ông phiền quá đi!
Dialogue: 0,0:04:22.53,0:04:24.17,Default,,0000,0000,0000,,Ông dai quá đấy.
Dialogue: 0,0:04:24.15,0:04:26.13,Default,,0000,0000,0000,,Chuyện này thật là tồi tệ.
Dialogue: 0,0:04:27.33,0:04:28.37,Default,,0000,0000,0000,,Mệt thật.
Dialogue: 0,0:04:28.38,0:04:31.54,Default,,0000,0000,0000,,Mệt quá đi mất, tôi về nhà ngủ đây.
Dialogue: 0,0:04:31.56,0:04:32.66,Default,,0000,0000,0000,,Đi thôi nào!
Dialogue: 0,0:04:32.92,0:04:35.71,Default,,0000,0000,0000,,Tagai, em không định quay trở lại trường sao?
Dialogue: 0,0:04:35.71,0:04:37.75,Default,,0000,0000,0000,,Tôi không có tâm trạng đó.
Dialogue: 0,0:04:37.75,0:04:39.00,Default,,0000,0000,0000,,Tự mà về một mình đi.
Dialogue: 0,0:04:39.21,0:04:40.05,Default,,0000,0000,0000,,Nhưng cái đó..
Dialogue: 0,0:04:40.05,0:04:41.71,Default,,0000,0000,0000,,Ông ngoan cố quá nha.
Dialogue: 0,0:04:41.71,0:04:44.84,Default,,0000,0000,0000,,Cái gì? Muốn đi cùng tôi nhiều như vậy ư?
Dialogue: 0,0:04:44.84,0:04:45.79,Default,,0000,0000,0000,,Em nói gì vậy?
Dialogue: 0,0:04:47.30,0:04:50.26,Default,,0000,0000,0000,,Nhìn thấy áo tắm của tôi khiến ông hứng lắm ư?
Dialogue: 0,0:04:50.47,0:04:52.06,Default,,0000,0000,0000,,Kinh tởm.
Dialogue: 0,0:04:52.06,0:04:54.77,Default,,0000,0000,0000,,Đừng nhìn tôi bằng ánh mắt dê xồm đó.
Dialogue: 0,0:04:54.79,0:04:58.23,Default,,0000,0000,0000,,Tôi đoán là thứ đó đã có trông máu ông rồi.
Dialogue: 0,0:04:58.56,0:04:59.77,Default,,0000,0000,0000,,Con khốn!
Dialogue: 0,0:04:59.77,0:05:02.98,Default,,0000,0000,0000,,Bị tôi nhìn bằng ánh mắt kinh tởm làm ông nổi điên à.
Dialogue: 0,0:05:04.15,0:05:06.70,Default,,0000,0000,0000,,Satsuki, cậu mút ông ta đi.
Dialogue: 0,0:05:07.28,0:05:10.46,Default,,0000,0000,0000,,Cái gì? Mút?
Dialogue: 0,0:05:11.06,0:05:14.83,Default,,0000,0000,0000,,Phản ứng đó, ông vẫn còn zin à?
Dialogue: 0,0:05:14.83,0:05:19.33,Default,,0000,0000,0000,,Chờ đã! Cái gì vậy? Vả lại thầy không phải là còn zin.
Dialogue: 0,0:05:19.72,0:05:21.41,Default,,0000,0000,0000,,Cái gì mà thầy không phải là còn zin chứ.
Dialogue: 0,0:05:21.42,0:05:22.71,Default,,0000,0000,0000,,Sao lại nhoi lên vậy.
Dialogue: 0,0:05:22.71,0:05:24.21,Default,,0000,0000,0000,,Thật là kinh tờm.
Dialogue: 0,0:05:25.02,0:05:25.80,Default,,0000,0000,0000,,Em...
Dialogue: 0,0:05:25.80,0:05:27.93,Default,,0000,0000,0000,,Ông thật là kinh tởm.
Dialogue: 0,0:05:28.30,0:05:30.35,Default,,0000,0000,0000,,Bản mặt của ông đang nói lên điều đó đấy.
Dialogue: 0,0:05:30.68,0:05:33.27,Default,,0000,0000,0000,,Được người dễ thương như cậu ấy mút cho....
Dialogue: 0,0:05:33.39,0:05:34.85,Default,,0000,0000,0000,,hãy lấy làm biết ơn đi.
Dialogue: 0,0:05:34.85,0:05:37.31,Default,,0000,0000,0000,,Cũng thật đúng lúc, phải không Satsuki?
Dialogue: 0,0:05:37.94,0:05:38.85,Default,,0000,0000,0000,,Ừm...
Dialogue: 0,0:05:39.38,0:05:43.48,Default,,0000,0000,0000,,Vậy tớ về nhà đây, chuyện còn lại giao cho cậu đấy.
Dialogue: 0,0:05:45.69,0:05:50.03,Default,,0000,0000,0000,,Wakao, Takai đã nắm được điểm yếu gì của em ư?
Dialogue: 0,0:05:50.87,0:05:53.20,Default,,0000,0000,0000,,Không, không phải vậy.
Dialogue: 0,0:05:54.54,0:05:55.70,Default,,0000,0000,0000,,Sensei.
Dialogue: 0,0:05:57.08,0:06:00.04,Default,,0000,0000,0000,,Nè, chờ chút, Wakao.
Dialogue: 0,0:06:01.34,0:06:05.88,Default,,0000,0000,0000,,Nếu em không làm gì đó, Shizuka sẽ mắng em mất.
Dialogue: 0,0:06:05.90,0:06:06.67,Default,,0000,0000,0000,,Nhé?
Dialogue: 0,0:06:07.38,0:06:08.76,Default,,0000,0000,0000,,Thầy biết rồi. {Wakatta}
Dialogue: 0,0:06:09.21,0:06:12.48,Default,,0000,0000,0000,,Chỗ này nguy hiểm lắm, sao chúng ta về trưòng trước nhỉ?
Dialogue: 0,0:06:12.97,0:06:13.35,Default,,0000,0000,0000,,Nhé?
Dialogue: 0,0:06:14.96,0:06:15.35,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:06:19.29,0:06:25.10,Default,,0000,0000,0000,,Rốt cuộc, em chẳng chịu thay đổi ý định. Em thường làm chuyện như vầy lắm sao?
Dialogue: 0,0:06:27.88,0:06:30.51,Default,,0000,0000,0000,,Không có chuyện đó đâu.
Dialogue: 0,0:06:31.20,0:06:33.87,Default,,0000,0000,0000,,Đây là lần đầu tiên của em.
Dialogue: 0,0:06:38.29,0:06:42.13,Default,,0000,0000,0000,,Thế cứ làm như vầy, rủi chuyện gì xảy ra thì sao?
Dialogue: 0,0:06:44.92,0:06:50.38,Default,,0000,0000,0000,,Không, đến lúc đó, Shizuka sẽ bảo vệ em.
Dialogue: 0,0:06:53.14,0:06:58.73,Default,,0000,0000,0000,,Nếu có chuyện gì xảy ra với em, cậu ấy nói là sẽ nghỉ làm.
Dialogue: 0,0:06:58.73,0:07:01.02,Default,,0000,0000,0000,,Mặc dù em ấy bắt em làm chuyện như thế này ư?
Dialogue: 0,0:07:03.73,0:07:09.28,Default,,0000,0000,0000,,Cậu ấy nói là em phải có kinh nghiệm về những chuyện như vầy.
Dialogue: 0,0:07:10.90,0:07:13.12,Default,,0000,0000,0000,,Takai nói là sẽ bảo vệ em ư?
Dialogue: 0,0:07:13.12,0:07:15.58,Default,,0000,0000,0000,,Nếu em từ chối, em sẽ bị đá ra đúng không?
Dialogue: 0,0:07:16.88,0:07:17.44,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:07:17.91,0:07:22.04,Default,,0000,0000,0000,,Ôi, nó nóng quá.
Dialogue: 0,0:07:25.96,0:07:29.01,Default,,0000,0000,0000,,Thầy sắp ra rồi phải không?
Dialogue: 0,0:07:43.31,0:07:47.44,Default,,0000,0000,0000,,Của Sensei tuyệt lắm.
Dialogue: 0,0:07:48.61,0:07:50.53,Default,,0000,0000,0000,,Vậy ư... Nhưng mà...
Dialogue: 0,0:07:51.82,0:07:54.37,Default,,0000,0000,0000,,Nhưng chuyện vui mới bắt đầu mà thôi.
Dialogue: 0,0:08:00.20,0:08:02.87,Default,,0000,0000,0000,,Không... Sensei, thầy đang làm gì vậy?
Dialogue: 0,0:08:02.87,0:08:04.04,Default,,0000,0000,0000,,Làm ơn dừng lại đi!
Dialogue: 0,0:08:04.62,0:08:09.30,Default,,0000,0000,0000,,Sensei, nếu còn thầy còn tiếp tục, em sẽ mách với Shizuka đấy.
Dialogue: 0,0:08:09.55,0:08:10.96,Default,,0000,0000,0000,,Cũng chả sao.
Dialogue: 0,0:08:11.47,0:08:14.84,Default,,0000,0000,0000,,Chuyện này chỉ ảnh hưởng đến công việc của Takai,
Dialogue: 0,0:08:14.84,0:08:17.18,Default,,0000,0000,0000,,Nó hoàn toàn không ảnh hưởng gì đến thầy.
Dialogue: 0,0:08:17.18,0:08:19.61,Default,,0000,0000,0000,,Việc làm giáo viên của tôi chẳng bị ảnh hưởng gì.
Dialogue: 0,0:08:21.18,0:08:22.56,Default,,0000,0000,0000,,Không... không...
Dialogue: 0,0:08:22.98,0:08:26.31,Default,,0000,0000,0000,,Sensei, dừng lại đi, em cầu xin mà!
Dialogue: 0,0:08:26.31,0:08:29.07,Default,,0000,0000,0000,,Bây giờ chúng đã không thể dừng lại được.
Dialogue: 0,0:08:41.64,0:08:43.25,Default,,0000,0000,0000,,Không thể nào...
Dialogue: 0,0:08:43.82,0:08:46.70,Default,,0000,0000,0000,,Đây cũng là kinh nghiệm đấy.
Dialogue: 0,0:08:55.17,0:08:56.01,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:08:56.68,0:08:57.47,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:09:01.39,0:09:02.48,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:09:02.47,0:09:03.27,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:09:07.06,0:09:07.90,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:09:07.90,0:09:09.01,Default,,0000,0000,0000,,Đau lắm...!
Dialogue: 0,0:09:33.00,0:09:36.38,Default,,0000,0000,0000,,Không... Đau lắm... Sensei!
Dialogue: 0,0:09:36.76,0:09:38.80,Default,,0000,0000,0000,,Chỉ chút nữa là em sẽ quen với nó thôi.
Dialogue: 0,0:09:38.84,0:09:40.14,Default,,0000,0000,0000,,Không...!
Dialogue: 0,0:10:09.92,0:10:14.05,Default,,0000,0000,0000,,Không... Sensei... nếu cứ tiếp tục...
Dialogue: 0,0:10:17.99,0:10:20.73,Default,,0000,0000,0000,,Không... Không...
Dialogue: 0,0:10:23.97,0:10:29.39,Default,,0000,0000,0000,,Em sẽ không nói với Shizuka... nên làm ơn, Sensei...
Dialogue: 0,0:10:29.77,0:10:34.52,Default,,0000,0000,0000,,Để thể hiện lòng biết ơn, thầy sẽ xuất ra bên trong em.
Dialogue: 0,0:10:35.82,0:10:39.49,Default,,0000,0000,0000,,Sensei, chờ đã... thầy không thể xuất ra bên trong em.
Dialogue: 0,0:10:39.78,0:10:41.82,Default,,0000,0000,0000,,Em sẽ có thai mất.
Dialogue: 0,0:10:43.12,0:10:44.28,Default,,0000,0000,0000,,Không... Không...
Dialogue: 0,0:10:44.28,0:10:45.82,Default,,0000,0000,0000,,Làm ơn mà, Sensei...
Dialogue: 0,0:10:59.83,0:11:02.34,Default,,0000,0000,0000,,Không~!
Dialogue: 0,0:11:09.27,0:11:12.36,Default,,0000,0000,0000,,Nó đang co giật bên trong mình.
Dialogue: 0,0:11:24.72,0:11:27.63,Default,,0000,0000,0000,,Thầy thật tàn nhẫn, Sensei.
Dialogue: 0,0:11:28.04,0:11:32.04,Default,,0000,0000,0000,,Nếu em nói với bất kì ai, Takai cũng sẽ gặp rắc rối đấy.
Dialogue: 0,0:11:33.19,0:11:35.14,Default,,0000,0000,0000,,Còn thầy thì vẫn không sao.
Dialogue: 0,0:11:36.24,0:11:39.83,Default,,0000,0000,0000,,Vâng.. thầy nói phải.
Dialogue: 0,0:11:40.22,0:11:44.59,Default,,0000,0000,0000,,Nếu em ở cùng với một người đàn ông thì chuyện này chắc chắn sẽ xảy ra.
Dialogue: 0,0:11:46.02,0:11:46.81,Default,,0000,0000,0000,,Ừ...
Dialogue: 0,0:11:48.40,0:11:53.19,Default,,0000,0000,0000,,Vậy từ bây giờ nếu Takai có ham muốn thì em hãy thỏa mãn cô ấy.
Dialogue: 0,0:11:54.40,0:11:55.57,Default,,0000,0000,0000,,Em hiểu rồi.
Dialogue: 0,0:12:07.08,0:12:11.87,Default,,0000,0000,0000,,Xin lỗi, tớ có một số việc với Sensei, nên tớ đi trước đây.
Dialogue: 0,0:12:13.00,0:12:16.84,Default,,0000,0000,0000,,Phải biết ơn tôi vì đã đối xử tử tế với ông...
Dialogue: 0,0:12:17.38,0:12:19.84,Default,,0000,0000,0000,,Nhưng ai cho ông gọi tôi bằng tên vậy?
Dialogue: 0,0:12:20.13,0:12:22.34,Default,,0000,0000,0000,,Cô thật là...
Dialogue: 0,0:12:22.92,0:12:24.30,Default,,0000,0000,0000,,"Cô" gì chứ cái?
Dialogue: 0,0:12:24.30,0:12:26.06,Default,,0000,0000,0000,,Gọi tôi là Shizuka-sama.
Dialogue: 0,0:12:26.26,0:12:28.01,Default,,0000,0000,0000,,Ông thật là một tên phục dịch hỗn xược.
Dialogue: 0,0:12:28.01,0:12:30.06,Default,,0000,0000,0000,,Dù gì, thầy cũng là giáo viên.
Dialogue: 0,0:12:30.06,0:12:31.64,Default,,0000,0000,0000,,Ơ, Satsuki đâu rồi?
Dialogue: 0,0:12:31.64,0:12:34.31,Default,,0000,0000,0000,,Hình như Satsuki đi với bạn em ấy rồi?
Dialogue: 0,0:12:34.31,0:12:36.90,Default,,0000,0000,0000,,Cái gì? Cậu ấy thật là...
Dialogue: 0,0:12:36.90,0:12:38.06,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:12:38.06,0:12:42.27,Default,,0000,0000,0000,,Ông trở thành người yêu của cậu ấy chỉ vì Satsuki đã mút cho ông sao?
Dialogue: 0,0:12:42.28,0:12:43.65,Default,,0000,0000,0000,,Chúc mừng nha...
Dialogue: 0,0:12:43.78,0:12:45.82,Default,,0000,0000,0000,,Đồ còn zin kém cỏi.
Dialogue: 0,0:12:45.82,0:12:49.74,Default,,0000,0000,0000,,Hừm.. cô lúc nào cũng phàn nàn.
Dialogue: 0,0:12:49.74,0:12:51.29,Default,,0000,0000,0000,,Ông lại gọi là "cô"!
Dialogue: 0,0:12:51.29,0:12:52.75,Default,,0000,0000,0000,,Nói bao nhiêu lần thì ông mới hiểu hả?
Dialogue: 0,0:12:52.75,0:12:55.33,Default,,0000,0000,0000,,Ông chẳng có gì cả bên trong lẫn bên ngoài.
Dialogue: 0,0:12:55.33,0:12:57.83,Default,,0000,0000,0000,,Tôi cực kỳ căng thẳng đấy  biết không hả?
Dialogue: 0,0:12:57.96,0:12:59.79,Default,,0000,0000,0000,,Là lỗi của một kẻ phì lũ như ông!
Dialogue: 0,0:12:59.88,0:13:03.51,Default,,0000,0000,0000,,Cô...!!! Cô biết không hả? Ở đây tôi mới là nạn nhân đấy.
Dialogue: 0,0:13:03.51,0:13:05.13,Default,,0000,0000,0000,,Vì lỗi của cô mà...
Dialogue: 0,0:13:06.22,0:13:08.23,Default,,0000,0000,0000,,Tôi đã bảo là Shizuka-sama.
Dialogue: 0,0:13:08.72,0:13:10.97,Default,,0000,0000,0000,,Đầu gối của ông cũng là đồ bỏ đi như mà ông thôi.
Dialogue: 0,0:13:12.35,0:13:14.98,Default,,0000,0000,0000,,Cô dám láo xược đá giáo viên của mình.
Dialogue: 0,0:13:19.23,0:13:21.27,Default,,0000,0000,0000,,Tôi nghĩ nếu quỳ gối thì ông sẽ hiểu ra.
Dialogue: 0,0:13:21.65,0:13:23.32,Default,,0000,0000,0000,,Nếu còn nói "cô" một lần nữa...!
Dialogue: 0,0:13:23.32,0:13:24.53,Default,,0000,0000,0000,,Cô...
Dialogue: 0,0:13:26.49,0:13:29.45,Default,,0000,0000,0000,,Tôi vẫn chưa bình tĩnh lại dù cho ông có quỳ gối.
Dialogue: 0,0:13:29.86,0:13:31.41,Default,,0000,0000,0000,,Hừm, tôi đi vệ sinh đây.
Dialogue: 0,0:13:32.58,0:13:34.95,Default,,0000,0000,0000,,Tại sao mình lại trở thành đồ chơi của con khốn đó chứ?
Dialogue: 0,0:13:41.09,0:13:43.42,Default,,0000,0000,0000,,Cô ta nghĩ gì mà đi vệ sinh...?
Dialogue: 0,0:13:43.42,0:13:44.76,Default,,0000,0000,0000,,Cô ta không có ở đây ư?
Dialogue: 0,0:13:45.63,0:13:48.01,Default,,0000,0000,0000,,Cô khốn đó đi đâu rồi?
Dialogue: 0,0:13:53.60,0:13:56.35,Default,,0000,0000,0000,,Ở chỗ này...
Dialogue: 0,0:14:10.03,0:14:12.20,Default,,0000,0000,0000,,Không... mình không dừng lại được...
Dialogue: 0,0:14:20.79,0:14:22.59,Default,,0000,0000,0000,,Ông đang làm gì vậy?
Dialogue: 0,0:14:23.04,0:14:27.63,Default,,0000,0000,0000,,Cô đang nói gì vậy? Tôi ở đây để làm phục dịch cho cô theo lời của thầy hiệu phó mà.
Dialogue: 0,0:14:27.63,0:14:30.84,Default,,0000,0000,0000,,Vậy, thế thì chúng ta đi thôi.
Dialogue: 0,0:14:33.39,0:14:35.01,Default,,0000,0000,0000,,Cái gì!? Thả tôi ra!
Dialogue: 0,0:14:35.01,0:14:37.02,Default,,0000,0000,0000,,Nếu muốn đi, sao lại đến đây?
Dialogue: 0,0:14:37.02,0:14:39.89,Default,,0000,0000,0000,,Ông đang nói gì vậy? Tôi về nhà đây.
Dialogue: 0,0:14:39.89,0:14:41.01,Default,,0000,0000,0000,,Thả tôi ra.
Dialogue: 0,0:14:43.36,0:14:45.78,Default,,0000,0000,0000,,Không, ông đang làm gì vậy?
Dialogue: 0,0:14:45.78,0:14:48.69,Default,,0000,0000,0000,,Cô đang giả vờ gì vậy?
Dialogue: 0,0:14:48.69,0:14:50.53,Default,,0000,0000,0000,,Cái này là gì đây?
Dialogue: 0,0:14:52.66,0:14:54.70,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:14:54.70,0:14:57.75,Default,,0000,0000,0000,,Nếu ông còn tiếp tục ,tôi sẽ sa thải ông.
Dialogue: 0,0:14:57.75,0:14:59.41,Default,,0000,0000,0000,,Tôi chả quan tâm.
Dialogue: 0,0:14:59.41,0:15:02.04,Default,,0000,0000,0000,,Tôi có thể thôi làm giáo viên bất cứ lúc nào.
Dialogue: 0,0:15:02.04,0:15:06.17,Default,,0000,0000,0000,,Nhưng tôi sẽ nghỉ chỉ sau khi chơi đùa với một cô gái như cô.
Dialogue: 0,0:15:06.52,0:15:07.85,Default,,0000,0000,0000,,Chờ đã...!
Dialogue: 0,0:15:08.51,0:15:12.39,Default,,0000,0000,0000,,Cô cảm thấy sướng khi cọ xát nó vào cạnh bàn sao?
Dialogue: 0,0:15:12.51,0:15:17.35,Default,,0000,0000,0000,,Ai lại để người khác chờ đợi trong khi mình sướng một mình chứ?
Dialogue: 0,0:15:17.51,0:15:19.42,Default,,0000,0000,0000,,Cô không quan tâm dù nó là ở đâu ư?
Dialogue: 0,0:15:19.43,0:15:21.48,Default,,0000,0000,0000,,Nếu cô muốn bị đùa giỡn thì...
Dialogue: 0,0:15:23.07,0:15:25.48,Default,,0000,0000,0000,,Thì tôi sẽ để cô ra ở đây.
Dialogue: 0,0:15:27.07,0:15:29.24,Default,,0000,0000,0000,,Nè, ông đang đùa phải không?
Dialogue: 0,0:15:29.24,0:15:30.90,Default,,0000,0000,0000,,Nè dừng lại đi!
Dialogue: 0,0:15:30.90,0:15:32.25,Default,,0000,0000,0000,,Tôi không muốn.
Dialogue: 0,0:15:35.74,0:15:37.99,Default,,0000,0000,0000,,Thật sự đấy, tôi không có đùa đâu.
Dialogue: 0,0:15:37.99,0:15:42.12,Default,,0000,0000,0000,,Tôi bảo là dừng lại mà... Không được... Không được... Tại sao tôi phải làm chuyện này với ông chứ?
Dialogue: 0,0:15:42.12,0:15:44.36,Default,,0000,0000,0000,,Tôi không có đùa.
Dialogue: 0,0:15:45.21,0:15:47.71,Default,,0000,0000,0000,,Không... Không... nó thật sự đang vào trong.
Dialogue: 0,0:15:48.80,0:15:50.09,Default,,0000,0000,0000,,Tôi đã bảo là không được.
Dialogue: 0,0:15:50.38,0:15:52.30,Default,,0000,0000,0000,,Là lần đầu tiên của tôi đó.
Dialogue: 0,0:15:52.30,0:15:53.01,Default,,0000,0000,0000,,Nè!
Dialogue: 0,0:15:53.01,0:15:56.35,Default,,0000,0000,0000,,Tại sao một tên phì lũ như ông lại lấy đi lần đầu của tôi chứ?
Dialogue: 0,0:15:56.35,0:15:57.60,Default,,0000,0000,0000,,Thật là vô lý.
Dialogue: 0,0:15:57.89,0:16:00.18,Default,,0000,0000,0000,,Cô thật là có một cái miệng hôi thối và còn cư xử tồi tệ.
Dialogue: 0,0:16:00.18,0:16:02.27,Default,,0000,0000,0000,,Cô thật sự là một kẻ tồi tệ.
Dialogue: 0,0:16:02.60,0:16:05.02,Default,,0000,0000,0000,,Gọi người khác là phì lũ, là kinh tởm.
Dialogue: 0,0:16:05.45,0:16:07.86,Default,,0000,0000,0000,,Nè... Em xin lỗi mà.
Dialogue: 0,0:16:08.27,0:16:11.07,Default,,0000,0000,0000,,Em xin lỗi vì đã gọi thầy là phì lũ.
Dialogue: 0,0:16:12.05,0:16:14.93,Default,,0000,0000,0000,,Thầy không có mập, nó chỉ là mũm mĩm thôi.
Dialogue: 0,0:16:15.65,0:16:17.83,Default,,0000,0000,0000,,Em sẽ rút lại những lời đã nói.
Dialogue: 0,0:16:17.83,0:16:20.16,Default,,0000,0000,0000,,Em đã làm những chuyện không phải.
Dialogue: 0,0:16:20.19,0:16:21.53,Default,,0000,0000,0000,,Em thật sự xin lỗi.
Dialogue: 0,0:16:21.91,0:16:24.71,Default,,0000,0000,0000,,Chỉ là hiểu lầm thôi. Em đáng yêu mà, đúng không?
Dialogue: 0,0:16:25.01,0:16:27.25,Default,,0000,0000,0000,,Từ giờ trở đi em sẽ sửa đổi.
Dialogue: 0,0:16:27.29,0:16:29.09,Default,,0000,0000,0000,,Em đã nói thì em nhất định sẽ sửa.
Dialogue: 0,0:16:29.10,0:16:29.74,Default,,0000,0000,0000,,Vậy nên.
Dialogue: 0,0:16:30.55,0:16:32.59,Default,,0000,0000,0000,,Cái gì mà "vậy nên" chứ?
Dialogue: 0,0:16:32.59,0:16:35.26,Default,,0000,0000,0000,,Em đã được dạy thứ đó ở đâu vậy?
Dialogue: 0,0:16:35.38,0:16:37.76,Default,,0000,0000,0000,,Em đáng phải bị trừng phạt.
Dialogue: 0,0:16:39.26,0:16:40.64,Default,,0000,0000,0000,,Nó đang vào... Nó đang vào...
Dialogue: 0,0:16:40.64,0:16:42.73,Default,,0000,0000,0000,,Không phải em đã xin lỗi rồi sao?
Dialogue: 0,0:16:42.73,0:16:44.81,Default,,0000,0000,0000,,Em đã nói là sẽ rút lại những lời đó.
Dialogue: 0,0:16:46.23,0:16:50.36,Default,,0000,0000,0000,,Nè, em đã xin lỗi rồi, nên thầy hãy bỏ qua cho em.
Dialogue: 0,0:16:50.36,0:16:52.40,Default,,0000,0000,0000,,Thầy hiểu điều đó mà, đúng không?
Dialogue: 0,0:16:51.98,0:16:56.05,Default,,0000,0000,0000,,Dù có xin lỗi đi nữa cô vẫn thật là ồn ào.
Dialogue: 0,0:17:00.37,0:17:01.37,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:17:06.17,0:17:09.33,Default,,0000,0000,0000,,Nó đã vào bên trong tôi.
Dialogue: 0,0:17:11.30,0:17:12.21,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:17:13.21,0:17:14.17,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:17:14.17,0:17:15.60,Default,,0000,0000,0000,,Đau lắm.
Dialogue: 0,0:17:19.64,0:17:21.22,Default,,0000,0000,0000,,Làm ơn!
Dialogue: 0,0:17:22.77,0:17:24.89,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:17:25.70,0:17:28.49,Default,,0000,0000,0000,,Hãy bỏ qua cho em!
Dialogue: 0,0:17:32.90,0:17:34.19,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:17:34.19,0:17:36.43,Default,,0000,0000,0000,,Nó thật sự đã vào bên trong.
Dialogue: 0,0:17:39.03,0:17:42.74,Default,,0000,0000,0000,,Cô thật sự là một kẻ biết thái.
Dialogue: 0,0:17:43.54,0:17:46.08,Default,,0000,0000,0000,,Cái gì chứ?
Dialogue: 0,0:17:46.32,0:17:48.46,Default,,0000,0000,0000,,Đời nào tôi lại như vậy?
Dialogue: 0,0:17:50.92,0:17:53.22,Default,,0000,0000,0000,,Tôi đã bảo dừng lại.
Dialogue: 0,0:17:54.76,0:17:55.53,Default,,0000,0000,0000,,Thật đấy...
Dialogue: 0,0:18:11.36,0:18:13.40,Default,,0000,0000,0000,,Hãy dừng lại đi.
Dialogue: 0,0:18:14.16,0:18:15.66,Default,,0000,0000,0000,,Đồ biến thái.
Dialogue: 0,0:18:17.87,0:18:21.44,Default,,0000,0000,0000,,Cô để tiếng rên của mình còn lớn hơn cả lúc nãy.
Dialogue: 0,0:18:28.16,0:18:30.04,Default,,0000,0000,0000,,Mình đang nghĩ cái quái gì vậy?
Dialogue: 0,0:18:30.34,0:18:34.66,Default,,0000,0000,0000,,Không đời nào mình cảm thấy nó khi bị một tên phì lũ như ông ta làm như vầy.
Dialogue: 0,0:18:35.84,0:18:39.68,Default,,0000,0000,0000,,Không đời nào tôi cảm thấy nó chỉ với sự nhịp mông nghèo nàn này.
Dialogue: 0,0:18:41.78,0:18:44.98,Default,,0000,0000,0000,,Sao cô có thể phàn nàn khi chỉ mình tôi di chuyển chứ?
Dialogue: 0,0:18:53.31,0:18:58.32,Default,,0000,0000,0000,,Thật đáng buồn là tinh dịch của kẻ nghèo nàn này sẽ ra bên trong cô.
Dialogue: 0,0:19:00.61,0:19:01.24,Default,,0000,0000,0000,,Nè...
Dialogue: 0,0:19:02.56,0:19:04.34,Default,,0000,0000,0000,,Ông đùa phải không?
Dialogue: 0,0:19:04.34,0:19:07.12,Default,,0000,0000,0000,,Tôi không đồng ý chuyện đó.
Dialogue: 0,0:19:08.91,0:19:12.04,Default,,0000,0000,0000,,Chờ đã... Thật sự không thể...
Dialogue: 0,0:19:12.06,0:19:13.48,Default,,0000,0000,0000,,Chờ đã!
Dialogue: 0,0:19:15.14,0:19:16.77,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:19:16.75,0:19:18.94,Default,,0000,0000,0000,,Không được, không được...
Dialogue: 0,0:19:23.79,0:19:26.38,Default,,0000,0000,0000,,Ông không thể.
Dialogue: 0,0:19:33.03,0:19:36.23,Default,,0000,0000,0000,,Ông đang đùa phải không? Ông đã ra bên trong tôi.
Dialogue: 0,0:19:36.23,0:19:37.24,Default,,0000,0000,0000,,Không thể nào...
Dialogue: 0,0:19:42.95,0:19:45.63,Default,,0000,0000,0000,,Ông... dám đùa giỡn với tôi...
Dialogue: 0,0:19:45.70,0:19:47.62,Default,,0000,0000,0000,,Cô chẳng thành thật chút nào.
Dialogue: 0,0:19:47.62,0:19:49.24,Default,,0000,0000,0000,,Dù cô đã cảm nhận nó nhiều như vậy.
Dialogue: 0,0:19:49.51,0:19:51.98,Default,,0000,0000,0000,,Thêm lần nữa nào.
Dialogue: 0,0:19:53.71,0:19:54.67,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:19:54.67,0:19:55.79,Default,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:19:56.13,0:19:59.55,Default,,0000,0000,0000,,Ông vẫn tiếp tục ư? Không phải vừa mới ra sao?
Dialogue: 0,0:20:03.34,0:20:05.22,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:20:05.22,0:20:05.80,Default,,0000,0000,0000,,Nè!
Dialogue: 0,0:20:08.26,0:20:11.48,Default,,0000,0000,0000,,Tôi nghĩ cô không thể giữ thêm được nữa.
Dialogue: 0,0:20:11.48,0:20:13.02,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:20:13.02,0:20:14.56,Default,,0000,0000,0000,,Đừng đùa với tôi.
Dialogue: 0,0:20:14.56,0:20:17.90,Default,,0000,0000,0000,,Cô vẫn còn hăng hái nhỉ, thế tôi sẽ xuất tinh thêm một lần nữa.
Dialogue: 0,0:20:18.65,0:20:21.28,Default,,0000,0000,0000,,Không... Nó đã ra nhiều lắm rồi.
Dialogue: 0,0:20:21.27,0:20:22.82,Default,,0000,0000,0000,,Tôi không muốn.
Dialogue: 0,0:20:22.81,0:20:24.64,Default,,0000,0000,0000,,Không cần phải kiềm chế.
Dialogue: 0,0:20:26.55,0:20:27.99,Default,,0000,0000,0000,,Không... Tôi lại...
Dialogue: 0,0:20:33.04,0:20:36.92,Default,,0000,0000,0000,,{\fad(0,1000)}Không~!
Dialogue: 0,0:20:44.84,0:20:48.60,Default,,0000,0000,0000,,Ông lại xuất ra bên trong tôi...
Dialogue: 0,0:20:58.47,0:21:01.82,Default,,0000,0000,0000,,Tư thế đó có thể kiếm được rất nhiều tiền đấy, cô người mẫu.
Dialogue: 0,0:21:02.48,0:21:03.78,Default,,0000,0000,0000,,Đừng có đùa!
Dialogue: 0,0:21:03.78,0:21:06.19,Default,,0000,0000,0000,,Ông thật sự là một kẻ bẩn thỉu chết tiệt.
Dialogue: 0,0:21:07.43,0:21:10.88,Default,,0000,0000,0000,,Ông đã ăn phải thứ gì hôi thối trước khi đến đây ư.
Dialogue: 0,0:21:10.87,0:21:11.70,Default,,0000,0000,0000,,Thế?
Dialogue: 0,0:21:11.70,0:21:14.42,Default,,0000,0000,0000,,Tại sao cô lại cọ xát với cạnh bàn?
Dialogue: 0,0:21:15.37,0:21:16.62,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:21:16.71,0:21:18.17,Default,,0000,0000,0000,,Đó không phải là chuyện của ông.
Dialogue: 0,0:21:18.17,0:21:20.88,Default,,0000,0000,0000,,Chỉ là tôi muốn vui vẻ thôi.
Dialogue: 0,0:21:21.88,0:21:23.80,Default,,0000,0000,0000,,Tôi biết rồi.
Dialogue: 0,0:21:23.80,0:21:25.09,Default,,0000,0000,0000,,Là để giải tỏa căng thẳng.
Dialogue: 0,0:21:25.09,0:21:28.97,Default,,0000,0000,0000,,Khi tôi khó chịu, tôi làm vậy để giải tỏa căng thẳng...
Dialogue: 0,0:21:28.97,0:21:33.11,Default,,0000,0000,0000,,Tất cả các cô gái ở tuổi tôi điều làm như vậy mà.
Dialogue: 0,0:21:33.56,0:21:35.43,Default,,0000,0000,0000,,Nó chẳng phải là chuyện gì kì cục cả.
Dialogue: 0,0:21:35.43,0:21:36.89,Default,,0000,0000,0000,,Vậy ư...
Dialogue: 0,0:21:36.89,0:21:41.02,Default,,0000,0000,0000,,Đừng lo lắng, vì bây giờ cô có thể giải tỏa căng thẳng.
Dialogue: 0,0:21:41.02,0:21:42.02,Default,,0000,0000,0000,,Hả?!
Dialogue: 0,0:21:42.02,0:21:43.44,Default,,0000,0000,0000,,Ông đang nói gì vậy?
Dialogue: 0,0:21:43.44,0:21:45.37,Default,,0000,0000,0000,,Não ông mọc nấm rồi sao?
Dialogue: 0,0:21:45.57,0:21:49.68,Default,,0000,0000,0000,,Ông nên vào tù và ăn những thứ thật hôi thối.
Dialogue: 0,0:21:49.91,0:21:54.83,Default,,0000,0000,0000,,Nếu cô tố cáo tôi, tôi sẽ đem chuyện này đến nhà xuất bản.
Dialogue: 0,0:21:55.04,0:22:00.63,Default,,0000,0000,0000,,Từ thái độ của cô, việc cô thủ dâm, tất cả sẽ bị phơi bày.
Dialogue: 0,0:22:00.63,0:22:01.84,Default,,0000,0000,0000,,Cứ làm như ông muốn.
Dialogue: 0,0:22:02.17,0:22:03.17,Default,,0000,0000,0000,,Ê!?
Dialogue: 0,0:22:03.17,0:22:09.80,Default,,0000,0000,0000,,Thế thì tôi sẽ bán mình, đó chẳng là gì so với sự đáng yêu của tôi.
Dialogue: 0,0:22:10.64,0:22:14.22,Default,,0000,0000,0000,,Gì chứ? Ông định làm tôi sợ chỉ với nhiêu đó thôi sao?
Dialogue: 0,0:22:14.22,0:22:15.64,Default,,0000,0000,0000,,Đơn giản quá đấy.
Dialogue: 0,0:22:15.64,0:22:17.48,Default,,0000,0000,0000,,Quả nhiên rác rưởi chỉ là rác rưởi.
Dialogue: 0,0:22:17.48,0:22:19.60,Default,,0000,0000,0000,,Ông nên ngẫm lại ở trong tù đi.
Dialogue: 0,0:22:19.65,0:22:21.36,Default,,0000,0000,0000,,Vậy ư...
Dialogue: 0,0:22:21.36,0:22:25.11,Default,,0000,0000,0000,,Vậy không phải cô đã sướng lắm sao.
Dialogue: 0,0:22:25.11,0:22:28.32,Default,,0000,0000,0000,,Hả? Cái gì chứ? Sướng ư?
Dialogue: 0,0:22:28.32,0:22:31.70,Default,,0000,0000,0000,,Đời nào tôi lại cảm thấy sướng với ông cơ chứ. Đồ ngốc!
Dialogue: 0,0:22:31.70,0:22:33.70,Default,,0000,0000,0000,,Nhưng cơ thể cô rất thành thật.
Dialogue: 0,0:22:33.70,0:22:35.24,Default,,0000,0000,0000,,Cô đã ra trong lần đầu tiên.
Dialogue: 0,0:22:35.24,0:22:37.29,Default,,0000,0000,0000,,Thế nghĩa là chúng ta rất hợp đấy.
Dialogue: 0,0:22:37.41,0:22:37.79,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:22:38.75,0:22:41.25,Default,,0000,0000,0000,,Nó sướng hơn là làm một mình đúng không?
Dialogue: 0,0:22:41.50,0:22:43.25,Default,,0000,0000,0000,,Cô có thể giải tỏa nhiều căng thẳng hơn.
Dialogue: 0,0:22:43.25,0:22:45.59,Default,,0000,0000,0000,,Từ giờ cô sẽ cảm thấy sướng hơn nữa.
Dialogue: 0,0:22:45.59,0:22:49.05,Default,,0000,0000,0000,,Bị ông cưỡng ép làm tình thì đời nào mà sướng được cơ chứ.
Dialogue: 0,0:22:49.63,0:22:53.30,Default,,0000,0000,0000,,Với lại tôi có thể giải tỏa nhiều căng thẳng hơn nếu có bạn trai.
Dialogue: 0,0:22:53.51,0:22:54.47,Default,,0000,0000,0000,,Hể...
Dialogue: 0,0:22:54.47,0:22:58.23,Default,,0000,0000,0000,,Cô nghĩ mình có thể làm tình với bạn trai chỉ vì để giải tỏa căng thẳng thôi ư?
Dialogue: 0,0:22:58.23,0:23:00.19,Default,,0000,0000,0000,,Cô nghĩ cậu ta sẽ chịu sao.
Dialogue: 0,0:23:00.19,0:23:04.02,Default,,0000,0000,0000,,Bị cô đem làm vật để giải tỏa căng thẳng.
Dialogue: 0,0:23:04.02,0:23:04.68,Default,,0000,0000,0000,,Hứ..
Dialogue: 0,0:23:05.94,0:23:09.15,Default,,0000,0000,0000,,Cái đống đó là do ông gây ra.
Dialogue: 0,0:23:09.15,0:23:11.24,Default,,0000,0000,0000,,Hãy tự mình đi mà dọn lấy.
Dialogue: 0,0:23:12.86,0:23:14.57,Default,,0000,0000,0000,,Tên phì lũ đó...
Dialogue: 0,0:23:14.57,0:23:16.03,Default,,0000,0000,0000,,Nó vẫn còn đau.
Dialogue: 0,0:23:16.20,0:23:17.58,Default,,0000,0000,0000,,Mình nên làm gì đây?
Dialogue: 0,0:23:17.58,0:23:20.25,Default,,0000,0000,0000,,Dù để ông ta ăn thức ăn hôi thối cũng không làm cơ đau biến mất.
Dialogue: 0,0:23:20.25,0:23:21.08,Default,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:23:21.25,0:23:24.00,Default,,0000,0000,0000,,Ông ta nói gì chứ, công cụ để giải tỏa căng thẳng ư?
Dialogue: 0,0:23:24.00,0:23:25.79,Default,,0000,0000,0000,,Nó làm mình còn căng thẳng hơn ấy chứ.
Dialogue: 0,0:23:28.55,0:23:30.47,Default,,0000,0000,0000,,Không phải chứ?
Dialogue: 0,0:23:30.76,0:23:32.80,Default,,0000,0000,0000,,Đừng có đùa chứ?
Dialogue: 0,0:23:32.80,0:23:34.34,Default,,0000,0000,0000,,Tại sao hắn ta lại ở đây?
Dialogue: 0,0:23:35.14,0:23:37.32,Default,,0000,0000,0000,,Ở chỗ như vầy... Không thể nào!
Dialogue: 0,0:23:37.73,0:23:40.43,Default,,0000,0000,0000,,Hắn ta mất trí rồi, hắn ta mất trí rồi.
Dialogue: 0,0:23:48.57,0:23:50.11,Default,,0000,0000,0000,,Không... dừng lại đi...
Dialogue: 0,0:23:50.74,0:23:52.36,Default,,0000,0000,0000,,Dừng lại đi...
Dialogue: 0,0:23:52.36,0:23:54.41,Default,,0000,0000,0000,,Em đáng yêu lắm, Shizuka-chan.
Dialogue: 0,0:23:54.87,0:23:57.74,Default,,0000,0000,0000,,Em đang run rẩy vì cuối cùng cũng gặy được anh sao?
Dialogue: 0,0:23:57.74,0:23:59.75,Default,,0000,0000,0000,,Làm ơn, dừng lại đi!
Dialogue: 0,0:24:00.08,0:24:03.50,Default,,0000,0000,0000,,Chúng ta yêu nhau mà, có gì phải sợ cơ chứ?
Dialogue: 0,0:24:03.50,0:24:04.29,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:24:04.92,0:24:05.67,Default,,0000,0000,0000,,Làm ơn!
Dialogue: 0,0:24:08.50,0:24:10.26,Default,,0000,0000,0000,,Cái mùi hôi thối này là gì vậy?
Dialogue: 0,0:24:10.88,0:24:11.59,Default,,0000,0000,0000,,Cô...
Dialogue: 0,0:24:11.59,0:24:12.26,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:24:12.29,0:24:12.91,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:24:14.43,0:24:16.51,Default,,0000,0000,0000,,Cô thực sự không phải là Shizuka-chan.
Dialogue: 0,0:24:16.51,0:24:18.35,Default,,0000,0000,0000,,Đây là mùi của một con điếm.
Dialogue: 0,0:24:19.01,0:24:22.85,Default,,0000,0000,0000,,Con điếm, cô không phải là Shizuka-chan. Biến thái!
Dialogue: 0,0:24:24.14,0:24:25.56,Default,,0000,0000,0000,,Chờ đã?
Dialogue: 0,0:24:25.56,0:24:26.52,Default,,0000,0000,0000,,Biến thái ư!
Dialogue: 0,0:24:26.52,0:24:27.61,Default,,0000,0000,0000,,Nghĩa là sao chứ?
Dialogue: 0,0:24:30.28,0:24:32.69,Default,,0000,0000,0000,,Takai, không sao chứ?
Dialogue: 0,0:24:33.78,0:24:34.61,Default,,0000,0000,0000,,Sensei.
Dialogue: 0,0:24:35.16,0:24:39.62,Default,,0000,0000,0000,,Ơ... Đồ rác rưởi! Phì lũ, ông đã đi đâu...
Dialogue: 0,0:24:39.95,0:24:41.66,Default,,0000,0000,0000,,lúc tôi gặp nguy hiểm hả?
Dialogue: 0,0:24:42.70,0:24:44.00,Default,,0000,0000,0000,,Tôi xin lỗi.
Dialogue: 0,0:24:44.00,0:24:46.46,Default,,0000,0000,0000,,Ông là phục dịch của tôi mà.
Dialogue: 0,0:24:46.46,0:24:48.42,Default,,0000,0000,0000,,Nếu ông không ở bên cạnh tôi phải là sao chứ?
Dialogue: 0,0:24:48.42,0:24:50.05,Default,,0000,0000,0000,,Ơ... Chỉ là...
Dialogue: 0,0:24:50.05,0:24:52.26,Default,,0000,0000,0000,,Ông cho rằng tôi bị cưỡng hiếp cũng dược ư?
Dialogue: 0,0:24:52.26,0:24:53.76,Default,,0000,0000,0000,,Ông đúng là rác rưởi.
Dialogue: 0,0:24:53.76,0:24:54.38,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:24:54.38,0:24:54.97,Default,,0000,0000,0000,,Phì lũ!
Dialogue: 0,0:24:54.97,0:24:55.76,Default,,0000,0000,0000,,Biến thái!
Dialogue: 0,0:25:04.77,0:25:10.39,Default,,0000,0000,0000,,Đáng ra ông phải cứu tôi trước khi tôi bị cưỡng hiếp chứ.
Dialogue: 0,0:25:18.28,0:25:20.28,Default,,0000,0000,0000,,Không được... chỗ đó...
Dialogue: 0,0:25:26.00,0:25:32.05,Default,,0000,0000,0000,,Căng thẳng của tôi sẽ không được giải tỏa chỉ với như vầy.
Dialogue: 0,0:25:42.81,0:25:44.14,Default,,0000,0000,0000,,Nó vào rồi.
Dialogue: 0,0:25:44.14,0:25:46.56,Default,,0000,0000,0000,,Dương vật của ông đang ở bên trong tôi.
Dialogue: 0,0:25:50.56,0:25:51.69,Default,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:25:53.98,0:25:56.32,Default,,0000,0000,0000,,Tuy nó đau nhưng...
Dialogue: 0,0:25:56.32,0:25:57.03,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:25:58.41,0:26:00.87,Default,,0000,0000,0000,,Chúng ta thật sự rất hợp với nhau.
Dialogue: 0,0:26:00.87,0:26:02.91,Default,,0000,0000,0000,,Cô có thể cảm thấy nó đúng không?
Dialogue: 0,0:26:05.29,0:26:06.72,Default,,0000,0000,0000,,Không có chuyện đó...
Dialogue: 0,0:26:08.28,0:26:09.17,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:26:09.17,0:26:10.54,Default,,0000,0000,0000,,Nếu cứ như vậy...
Dialogue: 0,0:26:14.50,0:26:15.02,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:26:15.01,0:26:15.67,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:26:18.05,0:26:19.47,Default,,0000,0000,0000,,Không được, không được!
Dialogue: 0,0:26:31.94,0:26:32.73,Default,,0000,0000,0000,,Chuyện này!
Dialogue: 0,0:26:32.74,0:26:33.61,Default,,0000,0000,0000,,Chuyện này...
Dialogue: 0,0:26:34.93,0:26:36.99,Default,,0000,0000,0000,,Ông không được dùng nó cho chuyện khác.
Dialogue: 0,0:26:41.80,0:26:50.30,Default,,0000,0000,0000,,Dương vật này chỉ được dùng để làm cho tôi bình tĩnh mà thôi.
Dialogue: 0,0:26:51.62,0:26:57.80,Default,,0000,0000,0000,,Tôi sẽ bỏ qua chuyện đó cho ông. Ông hãy lấy làm biết ơn đi
Dialogue: 0,0:27:01.89,0:27:02.53,Default,,0000,0000,0000,,Bên trong tôi....
Dialogue: 0,0:27:02.52,0:27:03.98,Default,,0000,0000,0000,,Đâm sâu hơn nữa.
Dialogue: 0,0:27:03.98,0:27:05.00,Default,,0000,0000,0000,,Nữa đi!
Dialogue: 0,0:27:06.99,0:27:08.04,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:27:09.35,0:27:10.56,Default,,0000,0000,0000,,Tôi sắp ra rồi.
Dialogue: 0,0:27:12.71,0:27:13.90,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:27:13.89,0:27:14.98,Default,,0000,0000,0000,,Sâu hơn nữa!
Dialogue: 0,0:27:15.19,0:27:15.86,Default,,0000,0000,0000,,Nữa đi!
Dialogue: 0,0:27:19.55,0:27:20.20,Default,,0000,0000,0000,,Tôi sắp ra.
Dialogue: 0,0:27:20.20,0:27:21.75,Default,,0000,0000,0000,,Tôi sắp ra!
Dialogue: 0,0:27:22.32,0:27:23.16,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:27:24.74,0:27:25.87,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:27:26.46,0:27:27.55,Default,,0000,0000,0000,,Đẩy mạnh hơn nữa.
Dialogue: 0,0:27:27.99,0:27:28.63,Default,,0000,0000,0000,,Nữa đi!
Dialogue: 0,0:27:30.00,0:27:32.29,Default,,0000,0000,0000,,Hãy ra thật nhiều bên trong tôi.
Dialogue: 0,0:27:35.17,0:27:36.29,Default,,0000,0000,0000,,Không được...
Dialogue: 0,0:27:36.29,0:27:37.21,Default,,0000,0000,0000,,Chuyện này...
Dialogue: 0,0:27:38.13,0:27:38.84,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:27:48.39,0:27:51.60,Default,,0000,0000,0000,,{\fad(0,1000)}Tôi ra đây~!
Dialogue: 0,0:28:08.68,0:28:10.30,Default,,0000,0000,0000,,Sướng như vầy...
Dialogue: 0,0:28:10.62,0:28:15.75,Default,,0000,0000,0000,,{\fad(0,2200)}thật không thể nào.
