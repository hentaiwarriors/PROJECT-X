Dialogue: 0,0:05:20.81,0:05:22.60,Default,,0000,0000,0000,,Tuyệt quá, tuyệt quá!
Dialogue: 0,0:05:25.30,0:05:27.28,Default,,0000,0000,0000,,Cậu không thể đợi lâu hơn, phải không?
Dialogue: 0,0:05:27.28,0:05:28.96,Default,,0000,0000,0000,,Cứ nằm nguyên đấy một lúc nữa đi.
Dialogue: 0,0:05:30.77,0:05:34.35,Default,,0000,0000,0000,,Bự lên thế này rồi cơ à, cứ như sắp nhả đạn đến nơi rồi ấy.
Dialogue: 0,0:05:46.55,0:05:48.32,Default,,0000,0000,0000,,Nó đang vào bên trong...
Dialogue: 0,0:05:48.32,0:05:51.37,Default,,0000,0000,0000,,Một chút nữa thôi là vào bên trong...
Dialogue: 0,0:05:56.15,0:05:57.72,Default,,0000,0000,0000,,Khoan đã, Miharu!
Dialogue: 0,0:05:57.72,0:05:58.50,Default,,0000,0000,0000,,Gì hả?
Dialogue: 0,0:05:58.80,0:06:00.74,Default,,0000,0000,0000,,Chê mình còn trinh nên kĩ thuật kém hả?
Dialogue: 0,0:06:00.74,0:06:03.33,Default,,0000,0000,0000,,N-Nhưng mà cậu cho mình sự trinh trắng của cậu liệu có...
Dialogue: 0,0:06:03.33,0:06:04.51,Default,,0000,0000,0000,,Có vấn đề gì sao?
Dialogue: 0,0:06:04.81,0:06:08.71,Default,,0000,0000,0000,,Cậu nghĩ sao thì tùy, mình sẽ cho cậu đời con gái của mình,
Dialogue: 0,0:06:08.71,0:06:10.51,Default,,0000,0000,0000,,Và mình sẽ khiến cho cậu được sung sướng!
Dialogue: 0,0:06:14.43,0:06:17.08,Default,,0000,0000,0000,,Không, ai cho phép cậu sờ mó hả!
Dialogue: 0,0:06:18.12,0:06:23.52,Default,,0000,0000,0000,,Nếu tự mình không đè cậu xuống, thì cậu làm gì có gan làm chuyện nọ chuyện kia với mình.
Dialogue: 0,0:06:23.52,0:06:27.70,Default,,0000,0000,0000,,Vậy nên đừng hòng táy máy chỗ đó nhé...
Dialogue: 0,0:06:33.60,0:06:36.92,Default,,0000,0000,0000,,Mình là người chủ động trong chuyện này,
Dialogue: 0,0:06:36.92,0:06:39.94,Default,,0000,0000,0000,,vậy nên người có quyền quyết định làm thế này thế kia cũng là mình.
Dialogue: 0,0:06:48.78,0:06:50.14,Default,,0000,0000,0000,,K-Không!
Dialogue: 0,0:06:50.14,0:06:52.22,Default,,0000,0000,0000,,Cậu nhịp mông mạnh như vậy...
Dialogue: 0,0:06:53.28,0:06:58.44,Default,,0000,0000,0000,,Không, đây là... lần đầu tiên của mình, nhưng chuyện này thật sự rất tuyệt...
Dialogue: 0,0:07:00.27,0:07:01.38,Default,,0000,0000,0000,,Không, mình ra rồi...
Dialogue: 0,0:07:01.38,0:07:02.98,Default,,0000,0000,0000,,Là lần đầu tiên của mình, vậy mà{\fscx300}-
Dialogue: 0,0:07:04.59,0:07:08.74,Default,,0000,0000,0000,,Mình ra rồi!
Dialogue: 0,0:07:29.89,0:07:33.34,Default,,0000,0000,0000,,Trông xinh quá, cứ thể này khéo "đi" luôn mất.
Dialogue: 0,0:07:33.34,0:07:34.68,Default,,0000,0000,0000,,Chị em chỉ bị {\i1}choáng nhẹ{\i0} thôi .
Dialogue: 0,0:07:35.25,0:07:38.90,Default,,0000,0000,0000,,Nghe đâu Chị ấy từng muốn đóng vai chính của một vở bi kịch nào đó.
Dialogue: 0,0:07:39.83,0:07:41.61,Default,,0000,0000,0000,,Đừng giả nai nữa em ơi.
Dialogue: 0,0:07:41.61,0:07:43.62,Default,,0000,0000,0000,,Thế bình thường anh không thấy em dễ thương à?
Dialogue: 0,0:07:47.68,0:07:48.87,Default,,0000,0000,0000,,Hình như là có đấy.
Dialogue: 0,0:07:49.34,0:07:52.73,Default,,0000,0000,0000,,Bạn gái anh còn chưa tỉnh mà anh đã định "cưa" người khác rồi sao.
Dialogue: 0,0:07:52.73,0:07:53.88,Default,,0000,0000,0000,,Futaba-chan!
Dialogue: 0,0:07:53.88,0:07:56.49,Default,,0000,0000,0000,,Có lẽ anh không phải người thích trăng hoa nhỉ, Senpai?
Dialogue: 0,0:07:56.49,0:07:58.35,Default,,0000,0000,0000,,Chứng tỏ là anh vẫn còn "zin".
Dialogue: 0,0:07:58.35,0:07:59.65,Default,,0000,0000,0000,,Em đang nói cái quái gì vậy?!
Dialogue: 0,0:08:00.68,0:08:04.17,Default,,0000,0000,0000,,Lần đầu tiên em thấy anh có bộ mặt dễ thương như vậy đó, Senpai.
Dialogue: 0,0:08:04.17,0:08:06.67,Default,,0000,0000,0000,,Anh vẫn dễ bị "củ hành" như mọi khi nhỉ.
Dialogue: 0,0:08:06.67,0:08:08.28,Default,,0000,0000,0000,,Miệng lưỡi của em ghê gớm quá đấy.
Dialogue: 0,0:08:10.01,0:08:11.37,Default,,0000,0000,0000,,Fu... taba.
Dialogue: 0,0:08:11.87,0:08:13.58,Default,,0000,0000,0000,,Onee-chan, chị không sao chứ?
Dialogue: 0,0:08:13.58,0:08:14.39,Default,,0000,0000,0000,,Miharu.
Dialogue: 0,0:08:15.78,0:08:17.39,Default,,0000,0000,0000,,Anh sẽ đi gọi bác sĩ.
Dialogue: 0,0:08:18.28,0:08:20.42,Default,,0000,0000,0000,,Hả?  Chị đang ở đâu đây?
Dialogue: 0,0:08:20.74,0:08:21.99,Default,,0000,0000,0000,,Chị vẫn ổn chứ?
Dialogue: 0,0:08:21.99,0:08:26.19,Default,,0000,0000,0000,,Ban nãy chị vấp ngã và ngất đi,\Nvậy nên anh ấy đã đưa chị tới bệnh viện này.
Dialogue: 0,0:08:27.07,0:08:28.84,Default,,0000,0000,0000,,Không thể nào... sao lại?
Dialogue: 0,0:08:28.84,0:08:30.75,Default,,0000,0000,0000,,Hai người đang trên đường tới trường...
Dialogue: 0,0:08:30.75,0:08:33.98,Default,,0000,0000,0000,,Trường?  Sao chị lại...
Dialogue: 0,0:08:33.98,0:08:36.81,Default,,0000,0000,0000,,Futaba, sao em lại mặc bộ đồng phục này?
Dialogue: 0,0:08:41.01,0:08:42.16,Default,,0000,0000,0000,,Mất trí nhớ ư?!
Dialogue: 0,0:08:42.86,0:08:45.33,Default,,0000,0000,0000,,Họ nói rằng chỉ là mất trí nhớ tạm thời,
Dialogue: 0,0:08:45.93,0:08:48.59,Default,,0000,0000,0000,,nhưng nếu cứ tiếp tục thế này, sẽ không thể khôi phục trí nhớ được nữa?
Dialogue: 0,0:08:49.58,0:08:51.91,Default,,0000,0000,0000,,Không đời nào, cô ấy sẽ ổn thôi.
Dialogue: 0,0:08:51.91,0:08:55.22,Default,,0000,0000,0000,,Uhm, anh nói phải.  Chị ấy sẽ ổn thôi.
Dialogue: 0,0:08:55.66,0:09:00.65,Default,,0000,0000,0000,,Senpai này, một lúc nữa cha mẹ em sẽ tới đây.
Dialogue: 0,0:09:00.65,0:09:05.55,Default,,0000,0000,0000,,Em sẽ báo lại tình trạng của chị ấy cho anh sau, vậy nên anh cứ về nghỉ đi.
Dialogue: 0,0:09:05.55,0:09:06.57,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:09:06.57,0:09:10.33,Default,,0000,0000,0000,,Anh mà đổ bệnh nữa thì mọi chuyện coi như hỏng bét cả.
Dialogue: 0,0:09:10.33,0:09:12.31,Default,,0000,0000,0000,,Em nói cũng đúng.  Vậy nhé.
Dialogue: 0,0:09:20.99,0:09:24.16,Default,,0000,0000,0000,,Vậy nên, em và Onee-chan đã tới đây!
Dialogue: 0,0:09:24.16,0:09:25.88,Default,,0000,0000,0000,,Em nói "Vậy nên" là sao hả?!
Dialogue: 0,0:09:25.88,0:09:27.17,Default,,0000,0000,0000,,Em vẫn chưa cho anh một lời giải thích thỏa đáng đâu!
Dialogue: 0,0:09:27.17,0:09:29.48,Default,,0000,0000,0000,,Hừ, nói nãy giờ mà anh vẫn chưa nghe ra à.
Dialogue: 0,0:09:29.48,0:09:33.70,Default,,0000,0000,0000,,Em nói rồi đó, để giúp chị ấy sớm khôi phục lại trí nhớ, nơi này là lựa chọn tốt nhất.
Dialogue: 0,0:09:33.70,0:09:34.62,Default,,0000,0000,0000,,Em tự tiện quá đấy nhé!
Dialogue: 0,0:09:35.38,0:09:37.35,Default,,0000,0000,0000,,À, có lẽ anh cũng hiểu được phần nào.
Dialogue: 0,0:09:37.35,0:09:39.05,Default,,0000,0000,0000,,Nhưng còn vết thương của cô ấy thì sao?
Dialogue: 0,0:09:39.05,0:09:41.59,Default,,0000,0000,0000,,Vết thương bên ngoài thực sự không có gì nghiêm trọng,
Dialogue: 0,0:09:41.59,0:09:44.39,Default,,0000,0000,0000,,vậy nên chị ấy chỉ cần nghỉ ngơi vài bữa là ổn.
Dialogue: 0,0:09:44.97,0:09:48.69,Default,,0000,0000,0000,,Chà, đưa chị ấy tới đây khó thật. Onee-san vẫn còn đang xấu hổ.
Dialogue: 0,0:09:48.69,0:09:51.43,Default,,0000,0000,0000,,Dụ được chị ấy tới đây là cả một kì công.
Dialogue: 0,0:09:51.43,0:09:53.61,Default,,0000,0000,0000,,Hồi đó chị ấy hệt như thế này.
Dialogue: 0,0:09:53.61,0:09:55.69,Default,,0000,0000,0000,,K-Không, mình chỉ...
Dialogue: 0,0:09:55.69,0:09:59.40,Default,,0000,0000,0000,,Chà, chị ấy cứ như vậy mãi đến khi vào cấp ba...
Dialogue: 0,0:10:00.60,0:10:03.44,Default,,0000,0000,0000,,Onee-chan, chào Senpai đi kìa.
Dialogue: 0,0:10:03.76,0:10:06.50,Default,,0000,0000,0000,,Chị định để anh ấy ngắm lưng đến bao giờ nữa hả?!
Dialogue: 0,0:10:06.50,0:10:07.42,Default,,0000,0000,0000,,Hừ.
Dialogue: 0,0:10:10.81,0:10:11.42,Default,,0000,0000,0000,,Chết nè!
Dialogue: 0,0:10:17.09,0:10:19.28,Default,,0000,0000,0000,,Thôi nào, bà chị nhút nhát.
Dialogue: 0,0:10:19.83,0:10:21.20,Default,,0000,0000,0000,,Cậu ổn chứ, Miharu?
Dialogue: 0,0:10:23.26,0:10:26.07,Default,,0000,0000,0000,,Miharu... Miharu?
Dialogue: 0,0:10:26.07,0:10:28.54,Default,,0000,0000,0000,,Irifune-kun gọi mình bằng tên thật ư?
Dialogue: 0,0:10:28.54,0:10:30.55,Default,,0000,0000,0000,,Hừ, lại thế nữa rồi.
Dialogue: 0,0:10:30.55,0:10:32.91,Default,,0000,0000,0000,,Thôi nào, chào hỏi người ta đàng hoàng đi chứ.
Dialogue: 0,0:10:34.29,0:10:36.44,Default,,0000,0000,0000,,Làm ơn hãy chăm sóc hai chị em chúng em!
Dialogue: 0,0:10:36.44,0:10:37.55,Default,,0000,0000,0000,,Cả hai á?
Dialogue: 0,0:10:39.07,0:10:41.69,Default,,0000,0000,0000,,Mình đang ở trong nhà Irifune-kun...
Dialogue: 0,0:10:42.04,0:10:44.70,Default,,0000,0000,0000,,Và... cậu ấy chính là bạn trai mình sao?
Dialogue: 0,0:10:44.94,0:10:48.86,Default,,0000,0000,0000,,Có vẻ như chị ấy đã mất đi toàn bộ kí ức kể từ khi vào cấp ba.
Dialogue: 0,0:10:48.86,0:10:52.46,Default,,0000,0000,0000,,Vậy nên chắc cũng chẳng nhớ nổi chuyện chị ấy là bạn gái anh, hay lần rút thăm may mắn đó...
Dialogue: 0,0:10:52.46,0:10:56.46,Default,,0000,0000,0000,,Mình đang mặc bộ đồ này, và còn hẹn hò với Irifune-kun nữa...
Dialogue: 0,0:10:56.46,0:10:58.49,Default,,0000,0000,0000,,Mình... thật sự...
Dialogue: 0,0:10:56.46,0:10:59.08,Default,,0000,0000,0000,,Vậy là, cô ấy không hề nhớ gì về chuyện hai đứa hẹn hò?
Dialogue: 0,0:10:59.37,0:11:04.47,Default,,0000,0000,0000,,Em nghĩ chị ấy sẽ nhanh chóng khôi phục trí nhớ nếu được ở bên cạnh anh, vậy nên...
Dialogue: 0,0:11:04.72,0:11:06.71,Default,,0000,0000,0000,,Cha mẹ em đồng ý chuyện này sao?
Dialogue: 0,0:11:06.71,0:11:08.51,Default,,0000,0000,0000,,Chuyện nhỏ ấy mà.
Dialogue: 0,0:11:08.51,0:11:10.00,Default,,0000,0000,0000,,Mặc dù vậy...
Dialogue: 0,0:11:13.49,0:11:15.66,Default,,0000,0000,0000,,Vậy anh cũng biết hồi xưa chị ấy hay như vậy phải không?
Dialogue: 0,0:11:15.66,0:11:17.58,Default,,0000,0000,0000,,Lúc nào cũng thu lu một góc như vậy.
Dialogue: 0,0:11:17.58,0:11:19.22,Default,,0000,0000,0000,,Vậy à, anh cũng nghe nói qua.
Dialogue: 0,0:11:19.22,0:11:20.80,Default,,0000,0000,0000,,Chính là cái chủ đề sáng nay đây mà.
Dialogue: 0,0:11:20.80,0:11:23.14,Default,,0000,0000,0000,,Cô ấy nói không muốn cho anh thấy hình ảnh của mình trong quá khứ.
Dialogue: 0,0:11:23.14,0:11:24.87,Default,,0000,0000,0000,,Giờ thì anh thấy rồi đó.
Dialogue: 0,0:11:25.38,0:11:28.36,Default,,0000,0000,0000,,Onee-chan lúc nào cũng dễ thương thế này, nhỉ?
Dialogue: 0,0:11:28.88,0:11:31.42,Default,,0000,0000,0000,,Đúng là khác xa so với mọi khi.
Dialogue: 0,0:11:31.42,0:11:32.85,Default,,0000,0000,0000,,Cảm giác này cũng mới lạ đấy chứ{\fscx300}-
Dialogue: 0,0:11:32.85,0:11:35.93,Default,,0000,0000,0000,,Ấy không được, giúp cô ấy phục hồi trí nhớ mới là điều quan trọng lúc này.
Dialogue: 0,0:11:36.80,0:11:38.78,Default,,0000,0000,0000,,Chuyện đó không vội được đâu anh.
Dialogue: 0,0:11:38.78,0:11:43.85,Default,,0000,0000,0000,,Không... Futaba nói chuyện với Irifune-kun vui chưa kìa.
Dialogue: 0,0:11:40.90,0:11:43.02,Default,,0000,0000,0000,,{\an8}Trong trường hợp này, chọn thời điểm thích hợp là vô cùng quan trọng.
Dialogue: 0,0:11:43.02,0:11:46.47,Default,,0000,0000,0000,,{\an8}Vậy nên chúng ta cần kiên nhẫn cùng nhau theo dõi chị ấy.
Dialogue: 0,0:11:43.85,0:11:46.47,Default,,0000,0000,0000,,M-Mình cũng muốn được trò chuyện cùng cậu ấy!
Dialogue: 0,0:11:46.47,0:11:47.31,Default,,0000,0000,0000,,... không hẳn.
Dialogue: 0,0:11:46.47,0:11:48.74,Default,,0000,0000,0000,,{\an8}Hả?  Cùng nhau á?
Dialogue: 0,0:11:48.74,0:11:51.72,Default,,0000,0000,0000,,{\an8}Phải.  Cùng nhau.
Dialogue: 0,0:11:49.15,0:11:52.27,Default,,0000,0000,0000,,T-Trời, Futaba còn nắm tay cậu ấy nữa...
Dialogue: 0,0:11:52.95,0:11:56.27,Default,,0000,0000,0000,,Hình như chúng ta đã quên mất rằng còn một cô nàng mộng mơ đang ngồi kia nãy giờ.
Dialogue: 0,0:11:56.27,0:11:58.34,Default,,0000,0000,0000,,A-Anh hiểu rồi.
Dialogue: 0,0:11:58.34,0:12:01.82,Default,,0000,0000,0000,,Giờ thì anh biết con người thật của chị ấy rồi đó,
Dialogue: 0,0:12:01.82,0:12:02.78,Default,,0000,0000,0000,,Con người thật?!
Dialogue: 0,0:12:03.02,0:12:04.51,Default,,0000,0000,0000,,còn mỗi việc lên giường đi ngủ nữa thôi.
Dialogue: 0,0:12:04.51,0:12:05.98,Default,,0000,0000,0000,,{\fad(350,0)}À mà... chuyện gì vậy?
Dialogue: 0,0:12:05.98,0:12:08.38,Default,,0000,0000,0000,,Gia đình anh đi nghỉ cuối tuần hết rồi phải không?
Dialogue: 0,0:12:08.38,0:12:11.02,Default,,0000,0000,0000,,Em đang nghĩ đến chuyện biến anh thành nô lệ cho em.
Dialogue: 0,0:12:11.02,0:12:13.57,Default,,0000,0000,0000,,Đừng nói những điều như vậy ở đây, Futaba-chan!
Dialogue: 0,0:12:13.57,0:12:15.26,Default,,0000,0000,0000,,H-Hả?
Dialogue: 0,0:12:15.26,0:12:16.97,Default,,0000,0000,0000,,Ban nãy cô ấy xấu hổ lắm mà nhỉ...
Dialogue: 0,0:12:17.98,0:12:23.56,Default,,0000,0000,0000,,K-Không, Futaba, thế này thì chị không sao chợp mắt được...
Dialogue: 0,0:12:30.78,0:12:33.21,Default,,0000,0000,0000,,em nghĩ cứ nhét cho vài viên thuốc ngủ là yên chuyện, vậy nên...
Dialogue: 0,0:12:37.24,0:12:38.55,Default,,0000,0000,0000,,Thế này là sao?
Dialogue: 0,0:12:38.55,0:12:41.41,Default,,0000,0000,0000,,Hình như em cho chị ấy dùng hơi quá liều thì phải.
Dialogue: 0,0:12:41.41,0:12:43.65,Default,,0000,0000,0000,,Em đúng thật là...
Dialogue: 0,0:12:44.10,0:12:47.20,Default,,0000,0000,0000,,Giờ thì chị ấy thiếp đi rồi...
Dialogue: 0,0:12:47.61,0:12:48.83,Default,,0000,0000,0000,,Senpai.
Dialogue: 0,0:12:48.83,0:12:51.31,Default,,0000,0000,0000,,K-Khoan đã, chúng ta không thể.
Dialogue: 0,0:12:51.31,0:12:55.71,Default,,0000,0000,0000,,Oh?  Anh còn "zin" thì em làm gì mà chẳng được...
Dialogue: 0,0:12:57.00,0:12:58.59,Default,,0000,0000,0000,,Senpai...
Dialogue: 0,0:12:59.26,0:13:00.66,Default,,0000,0000,0000,,Làm chuyện đó với em nhé.
Dialogue: 0,0:13:01.63,0:13:02.95,Default,,0000,0000,0000,,Futaba-chan...
Dialogue: 0,0:13:05.64,0:13:07.74,Default,,0000,0000,0000,,Em biết thừa anh đang nghĩ gì trong đầu đấy nhé?
Dialogue: 0,0:13:07.74,0:13:11.96,Default,,0000,0000,0000,,Kiểu như "Futaba-chan thật dâm đãng", hay "Mình có nên thừa cơ lấn tới hay không".
Dialogue: 0,0:13:19.96,0:13:21.48,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:13:22.02,0:13:26.52,Default,,0000,0000,0000,,Lúc này em thực sự rất muốn làm điều đó cùng anh, Senpai.
Dialogue: 0,0:13:28.01,0:13:29.73,Default,,0000,0000,0000,,Nó bự lên rồi này...
Dialogue: 0,0:13:29.73,0:13:32.54,Default,,0000,0000,0000,,Cuối cùng em đã được sở hữu thằng nhỏ của anh, Senpai...
Dialogue: 0,0:13:34.13,0:13:35.17,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:13:35.17,0:13:38.56,Default,,0000,0000,0000,,Tuyệt hơn làm một mình rất nhiều...
Dialogue: 0,0:13:50.78,0:13:53.49,Default,,0000,0000,0000,,Tay của anh đang nóng bừng lên...
Dialogue: 0,0:13:55.29,0:13:59.43,Default,,0000,0000,0000,,Hơi thở của anh nữa... lâu lắm rồi...
Dialogue: 0,0:14:00.44,0:14:02.37,Default,,0000,0000,0000,,Em phấn khích lắm rồi đây...
Dialogue: 0,0:14:05.64,0:14:08.68,Default,,0000,0000,0000,,Senpai, anh cứ nằm yên vậy nhé,
Dialogue: 0,0:14:08.68,0:14:11.34,Default,,0000,0000,0000,,Vì em là người đã đè anh xuống.
Dialogue: 0,0:14:12.19,0:14:16.31,Default,,0000,0000,0000,,Thằng nhỏ của anh lại bự lên nữa rồi.
Dialogue: 0,0:14:16.80,0:14:19.75,Default,,0000,0000,0000,,Chưa gì đã "chào cờ" thế kia, thằng nhỏ của anh thiếu chung thủy quá đấy.
Dialogue: 0,0:14:20.49,0:14:23.74,Default,,0000,0000,0000,,Mỗi khi nhận được tín hiệu nó đều trở nên như vậy hả, Senpai.
Dialogue: 0,0:14:26.17,0:14:28.99,Default,,0000,0000,0000,,Lúc này em chỉ muốn đùa giỡn với anh càng lâu càng tốt.
Dialogue: 0,0:14:32.11,0:14:33.43,Default,,0000,0000,0000,,Khoan đã...
Dialogue: 0,0:14:33.43,0:14:37.48,Default,,0000,0000,0000,,Anh thích như vậy phải không, Senpai?
Dialogue: 0,0:14:39.72,0:14:44.80,Default,,0000,0000,0000,,Tuyệt quá... thằng nhỏ của anh đang nóng ran lên ở trong chân em...
Dialogue: 0,0:14:45.52,0:14:49.18,Default,,0000,0000,0000,,Không... Cảm giác này lạ quá,
Dialogue: 0,0:14:49.18,0:14:51.58,Default,,0000,0000,0000,,nhưng em thực sự rất phấn khích...
Dialogue: 0,0:14:51.58,0:14:54.19,Default,,0000,0000,0000,,Senpai, nước của anh rỉ ra nhiều quá...
Dialogue: 0,0:14:54.19,0:14:56.74,Default,,0000,0000,0000,,Anh sẽ ra nếu em tiếp tục làm vậy phải không?
Dialogue: 0,0:15:01.18,0:15:05.01,Default,,0000,0000,0000,,Vẻ mặt của anh đáng sợ quá đi, Senpai.
Dialogue: 0,0:15:06.63,0:15:08.15,Default,,0000,0000,0000,,Nhưng mà, không được.
Dialogue: 0,0:15:08.88,0:15:12.30,Default,,0000,0000,0000,,Chỉ mình anh được sung sướng thì bất công quá.
Dialogue: 0,0:15:13.73,0:15:16.47,Default,,0000,0000,0000,,Hãy làm cho âm hộ của em sung sướng đi.
Dialogue: 0,0:15:17.26,0:15:22.21,Default,,0000,0000,0000,,Dù bạn gái đang nằm ngủ bên cạnh, thằng nhỏ của anh lại chuẩn bị đâm vào trong em gái chị ấy, Senpai.
Dialogue: 0,0:15:26.02,0:15:28.07,Default,,0000,0000,0000,,Mặt anh nhìn chán đời quá.
Dialogue: 0,0:15:28.83,0:15:32.76,Default,,0000,0000,0000,,Không biết Onee-chan sẽ nghĩ sao khi nhìn thấy mặt anh bây giờ?
Dialogue: 0,0:15:34.72,0:15:38.65,Default,,0000,0000,0000,,Sao anh cứ làm ra vẻ mặt đó dù sắp được đâm nó vào bên trong em...
Dialogue: 0,0:15:41.29,0:15:44.16,Default,,0000,0000,0000,,Senpai, thằng nhỏ của anh vào hết bên trong em rồi!
Dialogue: 0,0:15:45.34,0:15:49.16,Default,,0000,0000,0000,,Em cảm thấy nó rồi, thằng nhỏ của anh đúng là tuyệt vời nhất!
Dialogue: 0,0:15:50.73,0:15:52.11,Default,,0000,0000,0000,,Tất cả mọi thứ...
Dialogue: 0,0:15:52.11,0:15:54.35,Default,,0000,0000,0000,,Lần tới em sẽ kể cho anh!
Dialogue: 0,0:15:56.44,0:15:58.24,Default,,0000,0000,0000,,Đã lâu lắm rồi...
Dialogue: 0,0:15:58.24,0:15:59.43,Default,,0000,0000,0000,,Bự quá...
Dialogue: 0,0:15:59.43,0:16:02.94,Default,,0000,0000,0000,,Không, anh làm em ra mất!
Dialogue: 0,0:16:05.91,0:16:09.06,Default,,0000,0000,0000,,Thằng nhỏ của anh tuyệt quá, Senpai!
Dialogue: 0,0:16:14.96,0:16:16.46,Default,,0000,0000,0000,,E-Em ra mất!
Dialogue: 0,0:16:16.46,0:16:20.45,Default,,0000,0000,0000,,Lâu rồi em mới được sở hữu thằng nhỏ của anh.  Em ra đây!
Dialogue: 0,0:16:21.96,0:16:23.93,Default,,0000,0000,0000,,Tuyệt quá đi mất, Senpai!
Dialogue: 0,0:16:39.82,0:16:43.33,Default,,0000,0000,0000,,Em... ra rồi...
Dialogue: 0,0:16:43.33,0:16:44.93,Default,,0000,0000,0000,,Em ra rồi!
Dialogue: 0,0:16:51.75,0:16:56.57,Default,,0000,0000,0000,,Anh bắn cả vào bên trong em rồi...
Dialogue: 0,0:16:57.68,0:17:03.14,Default,,0000,0000,0000,,Tinh dịch của Senpai bắn cả vào trong tử cung em dù Onee-chan đang nằm ngủ bên cạnh...
Dialogue: 0,0:17:04.63,0:17:06.89,Default,,0000,0000,0000,,Nó vẫn đang ra...
Dialogue: 0,0:17:06.89,0:17:09.22,Default,,0000,0000,0000,,Nhiều quá đi mất...
Dialogue: 0,0:17:11.16,0:17:15.35,Default,,0000,0000,0000,,Senpai.  Em biết mình không thể thay thế hình bóng của Onee-chan,
Dialogue: 0,0:17:15.35,0:17:17.86,Default,,0000,0000,0000,,nhưng từ giờ tới lúc chị ấy bình phục... được không anh?
Dialogue: 0,0:17:18.75,0:17:20.96,Default,,0000,0000,0000,,Ah, nó đang trào ra ngoài...
Dialogue: 0,0:17:21.96,0:17:23.35,Default,,0000,0000,0000,,Lãng phí quá...
Dialogue: 0,0:17:24.20,0:17:27.37,Default,,0000,0000,0000,,Biết đâu mùi vị của nó lại khiến chị ấy khôi phục trí nhớ thì sao?
Dialogue: 0,0:17:37.80,0:17:41.62,Default,,0000,0000,0000,,Trông mặt Onee-chan dâm chưa kìa, Senpai.
Dialogue: 0,0:17:46.99,0:17:50.12,Default,,0000,0000,0000,,Onee-chan, chị cứ an tâm nghỉ ngơi đi nhé?
Dialogue: 0,0:17:50.89,0:17:54.13,Default,,0000,0000,0000,,Tránh {\i1}cử động mạnh{\i0} cho tới khi trí nhớ của chị hồi phục hoàn toàn nhé.
Dialogue: 0,0:17:54.78,0:17:57.14,Default,,0000,0000,0000,,B-Bọn mình sẽ quay về sớm nhất có thể.
Dialogue: 0,0:17:57.85,0:17:59.17,Default,,0000,0000,0000,,Hai người đi đường cẩn thận.
Dialogue: 0,0:18:01.58,0:18:02.88,Default,,0000,0000,0000,,Sảng khoái quá!
Dialogue: 0,0:18:03.27,0:18:08.08,Default,,0000,0000,0000,,Từ lúc anh bắn nhiều như vậy vào trong em đêm qua, người em cứ lâng lâng sung sướng!
Dialogue: 0,0:18:08.08,0:18:09.29,Default,,0000,0000,0000,,Futaba-chan!
Dialogue: 0,0:18:11.37,0:18:15.91,Default,,0000,0000,0000,,Những lúc không kiềm chế nổi, anh cứ tìm đến em nhé?
Dialogue: 0,0:18:15.91,0:18:17.26,Default,,0000,0000,0000,,Kể cả trong giờ học.
Dialogue: 0,0:18:17.77,0:18:20.32,Default,,0000,0000,0000,,Ah, đợi em với!
Dialogue: 0,0:18:22.50,0:18:23.91,Default,,0000,0000,0000,,Hệt như một giấc mơ vậy...
Dialogue: 0,0:18:24.38,0:18:27.09,Default,,0000,0000,0000,,Không thể tin được mình là bạn gái của Irifune-kun...
Dialogue: 0,0:18:29.54,0:18:31.33,Default,,0000,0000,0000,,Trước đó, mình chỉ dám nhìn lén cậu ấy...
Dialogue: 0,0:18:32.35,0:18:36.62,Default,,0000,0000,0000,,Nhưng chuyện hai đứa yêu nhau là thật...
Dialogue: 0,0:18:39.11,0:18:42.82,Default,,0000,0000,0000,,Mình và cậu ấy vào chung một trường và sau đó hẹn hò cùng nhau...
Dialogue: 0,0:18:43.41,0:18:46.43,Default,,0000,0000,0000,,Mình đã thay đổi nhiều như vậy sao?
Dialogue: 0,0:18:46.82,0:18:51.81,Default,,0000,0000,0000,,Mình từng giống hệt như một con ngốc không bạn bè, giờ thì mình và cậu ấy đã...
Dialogue: 0,0:18:52.48,0:18:54.44,Default,,0000,0000,0000,,Thậm chí hai đứa đã tiến xa đến mức làm "chuyện đó" cùng nhau...
Dialogue: 0,0:18:55.41,0:18:58.84,Default,,0000,0000,0000,,Xấu hổ quá đi mất... điều đó là thật ư?
Dialogue: 0,0:18:58.84,0:19:01.72,Default,,0000,0000,0000,,Dường như mình đã trở thành một con người khác hoàn toàn...
Dialogue: 0,0:19:03.85,0:19:06.89,Default,,0000,0000,0000,,Nhưng mà, mùi hương này là thật.
Dialogue: 0,0:19:07.25,0:19:08.90,Default,,0000,0000,0000,,Là mùi hương của cậu ấy.
Dialogue: 0,0:19:08.90,0:19:11.46,Default,,0000,0000,0000,,Mùi hương của Ushio-kun.
Dialogue: 0,0:19:12.23,0:19:13.61,Default,,0000,0000,0000,,Mình mất tự chủ rồi.
Dialogue: 0,0:19:13.61,0:19:17.49,Default,,0000,0000,0000,,Từ sáng tới giờ, mùi hương của cậu ấy cứ tràn ngập xung quanh...
Dialogue: 0,0:19:17.86,0:19:23.48,Default,,0000,0000,0000,,Càng ngửi nhiều, cơ thể mình càng nóng bừng lên, khuôn mặt của Ushio-kun...
Dialogue: 0,0:19:25.42,0:19:27.10,Default,,0000,0000,0000,,Mình thật tồi tệ!
Dialogue: 0,0:19:27.35,0:19:30.97,Default,,0000,0000,0000,,Hệt như Ushio-kun đang làm chuyện đó với mình vậy...
Dialogue: 0,0:19:35.37,0:19:36.90,Default,,0000,0000,0000,,Không, mình không thể...
Dialogue: 0,0:19:41.87,0:19:42.66,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:19:43.70,0:19:48.07,Default,,0000,0000,0000,,Mình không thể... làm chuyện đáng xấu hổ này được...
Dialogue: 0,0:19:50.67,0:19:52.70,Default,,0000,0000,0000,,Nhưng mình không thể dừng lại!
Dialogue: 0,0:19:55.94,0:19:57.93,Default,,0000,0000,0000,,Không, không!
Dialogue: 0,0:19:58.50,0:20:04.63,Default,,0000,0000,0000,,Mình đang làm chuyện đáng xấu hổ này ngay trong phòng Ushio-kun...
Dialogue: 0,0:20:05.47,0:20:08.28,Default,,0000,0000,0000,,Không, mình sắp...
Dialogue: 0,0:20:09.61,0:20:10.60,Default,,0000,0000,0000,,Mình...!
Dialogue: 0,0:20:25.95,0:20:29.66,Default,,0000,0000,0000,,Không thể nào... Sao mình có thể làm chuyện đáng xấu hổ vậy chứ...
Dialogue: 0,0:20:29.66,0:20:30.93,Default,,0000,0000,0000,,Ushio-kun.
Dialogue: 0,0:20:33.98,0:20:34.75,Default,,0000,0000,0000,,Miharu.
Dialogue: 0,0:20:43.27,0:20:46.27,Default,,0000,0000,0000,,U-Uh, um, umm...
Dialogue: 0,0:20:48.44,0:20:49.53,Default,,0000,0000,0000,,Umm...
Dialogue: 0,0:20:52.12,0:20:53.14,Default,,0000,0000,0000,,Uh...
Dialogue: 0,0:20:53.51,0:20:58.44,Default,,0000,0000,0000,,Um, Ushio-kun... uh, umm... Mình...
Dialogue: 0,0:20:59.01,0:20:59.82,Default,,0000,0000,0000,,Miharu.
Dialogue: 0,0:21:01.56,0:21:03.44,Default,,0000,0000,0000,,Ushio-kun?
Dialogue: 0,0:21:07.44,0:21:10.83,Default,,0000,0000,0000,,Ôi không, mình đang hôn Ushio-kun!
Dialogue: 0,0:21:12.41,0:21:16.23,Default,,0000,0000,0000,,Hình như... trước kia, mình đã từng làm điều đó...
Dialogue: 0,0:21:16.23,0:21:19.10,Default,,0000,0000,0000,,Đúng rồi, dường như mình và cậu ấy thường xuyên làm điều đó...
Dialogue: 0,0:21:20.05,0:21:24.19,Default,,0000,0000,0000,,Không, Ushio-kun, mình, cậu biết đấy... ?
Dialogue: 0,0:21:24.19,0:21:26.09,Default,,0000,0000,0000,,Mình không quan tâm cậu ra sao, Miharu.
Dialogue: 0,0:21:30.48,0:21:31.43,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:21:35.49,0:21:38.12,Default,,0000,0000,0000,,Xấu hổ quá, Ushio-kun...
Dialogue: 0,0:21:38.12,0:21:41.60,Default,,0000,0000,0000,,Đây là lần đầu tiên mình làm chuyện này...
Dialogue: 0,0:21:44.85,0:21:47.53,Default,,0000,0000,0000,,M-Mình... umm...
Dialogue: 0,0:21:47.53,0:21:50.37,Default,,0000,0000,0000,,Vậy là tụi mình đã làm chuyện này trước kia rồi hả...
Dialogue: 0,0:21:50.37,0:21:53.06,Default,,0000,0000,0000,,X-Xin lỗi cậu, mình đường đột quá.
Dialogue: 0,0:21:53.68,0:21:58.93,Default,,0000,0000,0000,,M-Mình cũng vậy, mình cứ xấu hổ hoài như vậy khiến cậu khó xử lắm phải không.
Dialogue: 0,0:22:00.62,0:22:01.52,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:22:12.40,0:22:14.50,Default,,0000,0000,0000,,Tư thế này...
Dialogue: 0,0:22:17.51,0:22:18.76,Default,,0000,0000,0000,,Nó đang vào bên trong...
Dialogue: 0,0:22:24.84,0:22:27.01,Default,,0000,0000,0000,,Không... đau lắm.
Dialogue: 0,0:22:28.53,0:22:29.23,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:22:29.23,0:22:33.03,Default,,0000,0000,0000,,Nó đang vào sâu bên trong mình... tuyệt quá!
Dialogue: 0,0:22:35.19,0:22:37.21,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:22:37.21,0:22:39.68,Default,,0000,0000,0000,,Cảm giác này tuyệt như vậy sao?
Dialogue: 0,0:22:49.56,0:22:53.17,Default,,0000,0000,0000,,Không... mình không kiềm chế nổi nữa rồi...
Dialogue: 0,0:22:55.95,0:22:57.21,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:22:57.21,0:22:59.52,Default,,0000,0000,0000,,Tuyệt quá, tuyệt quá đi mất!
Dialogue: 0,0:23:02.57,0:23:05.67,Default,,0000,0000,0000,,Ushio-kun, tuyệt quá...
Dialogue: 0,0:23:07.52,0:23:08.98,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:23:11.67,0:23:15.99,Default,,0000,0000,0000,,Nó đang cọ sát ở sâu bên trong mình...
Dialogue: 0,0:23:19.97,0:23:23.70,Default,,0000,0000,0000,,Mình phải làm gì bây giờ?  Tuyệt quá đi mất!
Dialogue: 0,0:23:24.58,0:23:28.45,Default,,0000,0000,0000,,Cậu làm tuyệt lắm, Ushio-kun!
Dialogue: 0,0:23:29.63,0:23:33.57,Default,,0000,0000,0000,,Nó vẫn đâm sâu vào bên trong...
Dialogue: 0,0:23:33.57,0:23:35.83,Default,,0000,0000,0000,,Cậu làm mình phát điên lên mất!
Dialogue: 0,0:23:52.35,0:23:55.55,Default,,0000,0000,0000,,Không, Ushio-kun...
Dialogue: 0,0:23:55.55,0:23:58.57,Default,,0000,0000,0000,,Mình, mình...!
Dialogue: 0,0:24:20.67,0:24:21.80,Default,,0000,0000,0000,,Ahhhh...
Dialogue: 0,0:24:22.71,0:24:25.51,Default,,0000,0000,0000,,Tại sao chuyện này lại tuyệt đến vậy!
Dialogue: 0,0:24:26.62,0:24:29.56,Default,,0000,0000,0000,,{\fad(0,2000)}Ushio-kun...
Dialogue: 0,0:24:40.55,0:24:42.54,Default,,0000,0000,0000,,Senpai, anh thật xấu tính!
Dialogue: 0,0:24:43.33,0:24:46.07,Default,,0000,0000,0000,,Không ngờ anh lại lẻn về nhà trong giờ ăn trưa như vậy!
Dialogue: 0,0:24:46.07,0:24:47.11,Default,,0000,0000,0000,,Anh xin lỗi.
Dialogue: 0,0:24:47.11,0:24:51.58,Default,,0000,0000,0000,,Anh về nhà để lấy vài thứ mà anh bỏ quên, nhưng sau đó đã nảy sinh một số chuyện...
Dialogue: 0,0:24:51.59,0:24:54.78,Default,,0000,0000,0000,,Hừ, ít ra cũng phải nói với em một tiếng chứ!
Dialogue: 0,0:24:54.78,0:24:56.65,Default,,0000,0000,0000,,Em đã chạy đi tìm anh khắp nơi đó!
Dialogue: 0,0:24:56.65,0:24:57.55,Default,,0000,0000,0000,,U-Uhm.
Dialogue: 0,0:25:02.16,0:25:06.34,Default,,0000,0000,0000,,Ngay cả một người tốt bụng như em đây cũng không thể bỏ qua tội đó được.
Dialogue: 0,0:25:08.12,0:25:11.07,Default,,0000,0000,0000,,C-Chị về phòng đây.
Dialogue: 0,0:25:16.62,0:25:19.83,Default,,0000,0000,0000,,Anh vừa làm chuyện đó với Onee-chan phải không?
Dialogue: 0,0:25:19.83,0:25:21.74,Default,,0000,0000,0000,,Vậy là anh thừa nhận rồi.
Dialogue: 0,0:25:21.74,0:25:23.64,Default,,0000,0000,0000,,Lợi dụng lúc chị ấy mất trí nhớ để cưỡng bức...
Dialogue: 0,0:25:23.64,0:25:25.51,Default,,0000,0000,0000,,Anh không hề cưỡng bức cô ấy!
Dialogue: 0,0:25:26.61,0:25:28.07,Default,,0000,0000,0000,,Hử?
Dialogue: 0,0:25:28.07,0:25:29.84,Default,,0000,0000,0000,,G-Gì vậy?
Dialogue: 0,0:25:29.84,0:25:33.85,Default,,0000,0000,0000,,Để em nghĩ xem nên phạt anh thế nào.
Dialogue: 0,0:25:33.85,0:25:34.85,Default,,0000,0000,0000,,Phạt anh á?!
Dialogue: 0,0:25:36.00,0:25:38.84,Default,,0000,0000,0000,,Em sẽ làm cho thằng nhỏ của anh không thể thích bất kì ai ngoại trừ em.
Dialogue: 0,0:25:40.51,0:25:42.78,Default,,0000,0000,0000,,Giờ tắm cái đã!
Dialogue: 0,0:25:43.45,0:25:45.27,Default,,0000,0000,0000,,Muốn tắm cùng em không, Senpai?
Dialogue: 0,0:25:45.27,0:25:46.41,Default,,0000,0000,0000,,Tất nhiên là không.
Dialogue: 0,0:25:50.33,0:25:51.53,Default,,0000,0000,0000,,Ushio-kun.
Dialogue: 0,0:25:52.42,0:25:53.29,Default,,0000,0000,0000,,Miharu!
Dialogue: 0,0:25:53.90,0:25:55.70,Default,,0000,0000,0000,,Mình tưởng cậu ngủ rồi.
Dialogue: 0,0:25:55.70,0:25:58.23,Default,,0000,0000,0000,,Bỗng nhiên mình nghĩ tới điều đó,
Dialogue: 0,0:25:58.23,0:26:00.31,Default,,0000,0000,0000,,nhưng cậu còn đang nói chuyện với Futaba...
Dialogue: 0,0:26:02.99,0:26:05.95,Default,,0000,0000,0000,,Có vẻ cậu rất thân thiết với Futaba.
Dialogue: 0,0:26:06.88,0:26:09.29,Default,,0000,0000,0000,,À mà, cũng không có gì ngạc nhiên,
Dialogue: 0,0:26:09.29,0:26:12.30,Default,,0000,0000,0000,,vì con bé... là em gái người yêu cậu mà...
Dialogue: 0,0:26:13.05,0:26:16.18,Default,,0000,0000,0000,,Mình đang nói gì vậy...
Dialogue: 0,0:26:16.70,0:26:17.56,Default,,0000,0000,0000,,Miharu...
Dialogue: 0,0:26:18.30,0:26:21.87,Default,,0000,0000,0000,,Umm, xin lỗi vì đã nói những điều kì quặc đó.
Dialogue: 0,0:26:21.87,0:26:23.51,Default,,0000,0000,0000,,Cậu ghen tị phải không?
Dialogue: 0,0:26:23.51,0:26:28.30,Default,,0000,0000,0000,,Hả?  S-Sao mình phải ghen tị với em gái mình chứ?
Dialogue: 0,0:26:30.06,0:26:32.05,Default,,0000,0000,0000,,Mình xin lỗi, cứ xem như mình chưa nói gì cả{\fscx300}-
Dialogue: 0,0:26:33.82,0:26:34.94,Default,,0000,0000,0000,,Miharu.
Dialogue: 0,0:26:35.98,0:26:36.95,Default,,0000,0000,0000,,Cậu thật dễ thương.
Dialogue: 0,0:26:38.83,0:26:39.67,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:26:41.02,0:26:42.29,Default,,0000,0000,0000,,Senpai...
Dialogue: 0,0:26:43.90,0:26:47.43,Default,,0000,0000,0000,,Anh ấy vẫn làm chuyện đó với Onee-chan dù chị ấy đã trở lại hệt như trước kia...
Dialogue: 0,0:26:49.83,0:26:51.56,Default,,0000,0000,0000,,Có lẽ đã đến lúc tuyên chiến rồi đây.
Dialogue: 0,0:26:55.67,0:26:58.63,Default,,0000,0000,0000,,Nếu chúng ta làm chuyện đó ở đây, Futaba sẽ{\fscx300}-
Dialogue: 0,0:26:58.63,0:27:01.93,Default,,0000,0000,0000,,Dù nói vậy, nhưng cậu đang rất phấn khích, phải không Miharu?
Dialogue: 0,0:27:15.32,0:27:19.78,Default,,0000,0000,0000,,Không... con bé sẽ phát hiện ra chúng ta mất!
Dialogue: 0,0:27:21.80,0:27:24.46,Default,,0000,0000,0000,,Nó... đang vào sâu bên trong...
Dialogue: 0,0:27:27.89,0:27:31.89,Default,,0000,0000,0000,,Không... nó cứ cọ sát liên lục...
Dialogue: 0,0:27:32.70,0:27:34.38,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:27:42.19,0:27:46.76,Default,,0000,0000,0000,,Cậu làm tuyệt quá, Ushio-kun...
Dialogue: 0,0:27:46.76,0:27:49.51,Default,,0000,0000,0000,,Này, đừng làm chuyện này với bất kì ai khác nhé...
Dialogue: 0,0:27:56.12,0:27:57.84,Default,,0000,0000,0000,,Mình xin lỗi.
Dialogue: 0,0:27:58.73,0:28:01.39,Default,,0000,0000,0000,,Chắc chắn mình sẽ khôi phục lại toàn bộ kí ức của đôi ta...
Dialogue: 0,0:28:04.05,0:28:05.01,Default,,0000,0000,0000,,Vậy nên...
Dialogue: 0,0:28:06.06,0:28:10.87,Default,,0000,0000,0000,,Chỉ làm điều này với mình mình thôi nhé .  Chỉ mình mình thôi!
Dialogue: 0,0:28:10.87,0:28:11.77,Default,,0000,0000,0000,,Miharu!
Dialogue: 0,0:28:13.89,0:28:17.58,Default,,0000,0000,0000,,Không, nếu cậu làm mạnh như vậy, mình sẽ ra mất!
Dialogue: 0,0:28:19.63,0:28:22.44,Default,,0000,0000,0000,,Mình cảm thấy cậu, Ushio-kun...
Dialogue: 0,0:28:22.44,0:28:24.76,Default,,0000,0000,0000,,Làm cho mình sung sướng đến phát điên đi!
Dialogue: 0,0:28:25.18,0:28:28.74,Default,,0000,0000,0000,,Ra vào trong mình đi, Ushio-kun!
Dialogue: 0,0:28:28.74,0:28:31.76,Default,,0000,0000,0000,,Bên trong mình, thật nhiều vào!
Dialogue: 0,0:28:33.39,0:28:35.05,Default,,0000,0000,0000,,Tuyệt... quá...
Dialogue: 0,0:28:35.05,0:28:37.52,Default,,0000,0000,0000,,Thằng nhỏ của cậu lấp đầy bên trong mình!
Dialogue: 0,0:29:05.80,0:29:12.37,Default,,0000,0000,0000,,Bởi vì mình yêu Senpai cũng nhiều như Onee-chan yêu anh ấy.
