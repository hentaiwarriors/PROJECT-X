Dialogue: 0,0:00:29.73,0:00:32.15,Default,,0000,0000,0000,,Dùng cả đến "chiêu" này rồi mà cậu còn chưa chịu dậy sao...
Dialogue: 0,0:00:32.15,0:00:34.58,Default,,0000,0000,0000,,Cứng đầu nhỉ, đích thân {\i1}mình{\i0} ra tay mà còn vậy à...
Dialogue: 0,0:00:37.31,0:00:39.83,Default,,0000,0000,0000,,Ah, chịu dậy rồi hả!
Dialogue: 0,0:00:43.29,0:00:44.62,Default,,0000,0000,0000,,Miharu!
Dialogue: 0,0:00:45.12,0:00:49.19,Default,,0000,0000,0000,,Hừ, giờ mình còn phải dùng ngực để đánh thức cậu cơ đấy...
Dialogue: 0,0:00:52.06,0:00:54.15,Default,,0000,0000,0000,,Hết nhịn nổi nữa rồi, phải không?
Dialogue: 0,0:00:54.15,0:00:56.03,Default,,0000,0000,0000,,Vậy thì cậu ra lẹ đi!
Dialogue: 0,0:01:23.83,0:01:25.64,Default,,0000,0000,0000,,Mới sáng sớm mà cậu ra lắm ghê.
Dialogue: 0,0:01:27.39,0:01:28.31,Default,,0000,0000,0000,,Cậu!
Dialogue: 0,0:01:29.73,0:01:30.73,Default,,0000,0000,0000,,Hừ...
Dialogue: 0,0:01:30.73,0:01:32.70,Default,,0000,0000,0000,,Ái da~
Dialogue: 0,0:01:36.18,0:01:38.49,Default,,0000,0000,0000,,Sáng sớm mà đã ướt thế này là sao đây...
Dialogue: 0,0:01:38.49,0:01:40.12,Default,,0000,0000,0000,,Ah, cậu đang phà hơi vào chỗ đó...
Dialogue: 0,0:01:41.81,0:01:45.66,Default,,0000,0000,0000,,Mình chỉ dâm đãng thế này khi ở bên cậu, cậu biết mà?
Dialogue: 0,0:01:46.57,0:01:50.09,Default,,0000,0000,0000,,Vì cậu mà mình trở nên dâm đãng thế này, Ushio...
Dialogue: 0,0:01:50.09,0:01:51.42,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:01:51.42,0:01:54.57,Default,,0000,0000,0000,,Nè, mình muốn cậu...
Dialogue: 0,0:02:00.26,0:02:01.68,Default,,0000,0000,0000,,Đừng!
Dialogue: 0,0:02:01.89,0:02:05.34,Default,,0000,0000,0000,,Ushio, đừng giỡn mình như vậy!
Dialogue: 0,0:02:12.82,0:02:13.96,Default,,0000,0000,0000,,Nó vào bên trong rồi...
Dialogue: 0,0:02:19.88,0:02:23.37,Default,,0000,0000,0000,,Mới sáng sớm mà thằng nhỏ của cậu đã chui sâu vào bên trong mình thế này, Ushio...
Dialogue: 0,0:02:29.45,0:02:31.56,Default,,0000,0000,0000,,Cậu thô bạo quá, Ushio...
Dialogue: 0,0:02:34.57,0:02:35.89,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:02:38.99,0:02:39.72,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:02:40.60,0:02:43.71,Default,,0000,0000,0000,,Cậu đang cọ sát bên trong âm hộ mình...
Dialogue: 0,0:02:43.71,0:02:46.14,Default,,0000,0000,0000,,Tuyệt... quá đi mất!
Dialogue: 0,0:02:49.79,0:02:53.41,Default,,0000,0000,0000,,Tuyệt quá, nó vẫn đang vào sâu bên trong...
Dialogue: 0,0:02:53.41,0:02:57.61,Default,,0000,0000,0000,,Thằng nhỏ của cậu đang đâm vào tử cung mình, tuyệt quá đi mất!
Dialogue: 0,0:02:59.73,0:03:03.67,Default,,0000,0000,0000,,Phải, đâm mạnh vào đó nữa đi!
Dialogue: 0,0:03:04.86,0:03:05.70,Default,,0000,0000,0000,,Ushio!
Dialogue: 0,0:03:08.27,0:03:10.12,Default,,0000,0000,0000,,Mình sắp ra rồi!
Dialogue: 0,0:03:11.37,0:03:13.11,Default,,0000,0000,0000,,Hãy ra cùng lúc với mình...
Dialogue: 0,0:03:14.26,0:03:16.34,Default,,0000,0000,0000,,Mình muốn cậu ra cùng với mình, Ushio!
Dialogue: 0,0:03:17.82,0:03:20.09,Default,,0000,0000,0000,,Mình sắp ra rồi!
Dialogue: 0,0:03:20.09,0:03:21.32,Default,,0000,0000,0000,,Mình ra đây!
Dialogue: 0,0:03:26.36,0:03:30.10,Default,,0000,0000,0000,,Cậu ra bên trong tử cung mình nhiều quá...
Dialogue: 0,0:03:49.46,0:03:51.24,Default,,0000,0000,0000,,Hừ, cậu đang làm gì vậy hả?
Dialogue: 0,0:03:51.24,0:03:53.08,Default,,0000,0000,0000,,Muộn học bây giờ, nhanh lên nào!
Dialogue: 0,0:03:53.08,0:03:56.94,Default,,0000,0000,0000,,Hừ, toàn lợi dụng ông bà già vắng nhà để bắt nạt người ta thôi!
Dialogue: 0,0:03:57.50,0:03:59.38,Default,,0000,0000,0000,,"Bắt nạt" là sao, Miharu...
Dialogue: 0,0:04:00.96,0:04:03.24,Default,,0000,0000,0000,,Cậu ra bên trong mình nhiều như vậy,
Dialogue: 0,0:04:03.24,0:04:04.76,Default,,0000,0000,0000,,Nhỡ mình mang thai thì sao!
Dialogue: 0,0:04:05.40,0:04:08.18,Default,,0000,0000,0000,,Có cần phải nói điều này ngay trên đường không vậy trời?
Dialogue: 0,0:04:08.18,0:04:10.40,Default,,0000,0000,0000,,Chả ai thèm để ý đâu.
Dialogue: 0,0:04:10.40,0:04:11.94,Default,,0000,0000,0000,,Dù vậy...
Dialogue: 0,0:04:14.60,0:04:17.69,Default,,0000,0000,0000,,Nhìn cậu bây giờ chắc chả ai tin được là hồi xưa cậu từng như vậy.
Dialogue: 0,0:04:18.62,0:04:20.52,Default,,0000,0000,0000,,Chuyện hồi xưa thì liên quan gì ở đây hả?
Dialogue: 0,0:04:21.04,0:04:24.32,Default,,0000,0000,0000,,hay cậu muốn ám chỉ mình bây giờ không còn "hút hàng" nữa rồi chứ gì?
Dialogue: 0,0:04:24.32,0:04:25.99,Default,,0000,0000,0000,,K-Không...
Dialogue: 0,0:04:25.99,0:04:28.62,Default,,0000,0000,0000,,Vậy thì, đừng lo lắng gì cả.
Dialogue: 0,0:04:30.62,0:04:32.83,Default,,0000,0000,0000,,Mình không muốn cậu trong thấy hình ảnh của mình trước kia.
Dialogue: 0,0:04:33.26,0:04:34.33,Default,,0000,0000,0000,,Hồi đó...
Dialogue: 0,0:04:35.22,0:04:37.19,Default,,0000,0000,0000,,Mình không muốn nhớ lại quãng thời gian đó.
Dialogue: 0,0:04:38.13,0:04:41.87,Default,,0000,0000,0000,,Bởi vì lúc này mình đã có cậu, Ushio.
Dialogue: 0,0:04:41.87,0:04:43.50,Default,,0000,0000,0000,,M-Mình xin lỗi...
Dialogue: 0,0:04:43.50,0:04:44.34,Default,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:04:46.75,0:04:52.85,Default,,0000,0000,0000,,Tuy vậy, đôi khi mình vẫn lo không biết cậu có ghét con người của mình trước kia không.
Dialogue: 0,0:04:52.85,0:04:53.47,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:04:54.70,0:04:55.86,Default,,0000,0000,0000,,Không có gì đâu.
Dialogue: 0,0:04:56.79,0:04:57.65,Default,,0000,0000,0000,,Mình đi thôi nào!
Dialogue: 0,0:04:57.65,0:04:57.94,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:04:58.90,0:04:59.80,Default,,0000,0000,0000,,Miharu!
Dialogue: 0,0:05:03.07,0:05:05.20,Default,,0000,0000,0000,,Miharu, cậu không sao chứ?
Dialogue: 0,0:05:06.41,0:05:07.59,Default,,0000,0000,0000,,Miharu?
Dialogue: 0,0:05:07.59,0:05:09.35,Default,,0000,0000,0000,,Này, Miharu!
Dialogue: 0,0:05:09.68,0:05:11.31,Default,,0000,0000,0000,,Miharu!
