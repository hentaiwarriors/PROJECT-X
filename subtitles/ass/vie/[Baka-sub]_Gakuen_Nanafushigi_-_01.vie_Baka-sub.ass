Dialogue: 0,0:00:25.00,0:00:28.15,Default,,0000,0000,0000,,Thật chứ, làm sao cậu biết?
Dialogue: 0,0:00:28.00,0:00:29.92,Default,,0000,0000,0000,,Trên mạng cả đấy.
Dialogue: 0,0:00:30.03,0:00:32.62,Default,,0000,0000,0000,,Họ hướng dẫn cả cách thực hành.
Dialogue: 0,0:00:32.73,0:00:33.69,Default,,0000,0000,0000,,Thật á?
Dialogue: 0,0:00:34.23,0:00:36.66,Default,,0000,0000,0000,,Ok Reina,Kumi.
Dialogue: 0,0:00:36.97,0:00:37.93,Default,,0000,0000,0000,,Đưa ngón tay của hai cậu vào đây...
Dialogue: 0,0:00:39.27,0:00:40.23,Default,,0000,0000,0000,,Sẵn sàng chưa?
Dialogue: 0,0:00:40.34,0:00:41.30,Default,,0000,0000,0000,,Chúng ta sẽ bắt đầu.
Dialogue: 0,0:00:41.47,0:00:42.53,Default,,0000,0000,0000,,Tới luôn đi!
Dialogue: 0,0:00:42.87,0:00:44.46,Default,,0000,0000,0000,,Hỡi những linh hồn...
Dialogue: 0,0:00:44.57,0:00:46.27,Default,,0000,0000,0000,,Nếu nghe thấy ta, hãy đến đây...
Dialogue: 0,0:00:46.41,0:00:47.40,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:00:51.08,0:00:53.74,Default,,0000,0000,0000,,Nếu các cậu chỉ gọi linh hồn cho vui...
Dialogue: 0,0:00:53.85,0:00:55.25,Default,,0000,0000,0000,,...Các cậu sẽ bị nhập.
Dialogue: 0,0:00:55.62,0:00:57.08,Default,,0000,0000,0000,,Đáng.... sợ thế!
Dialogue: 0,0:00:57.19,0:00:58.74,Default,,0000,0000,0000,,Cậu nghĩ cậu là ai chứ?
Dialogue: 0,0:00:58.99,0:01:00.05,Default,,0000,0000,0000,,Bị nhập?
Dialogue: 0,0:01:00.12,0:01:01.28,Default,,0000,0000,0000,,Ý cậu là bởi một con hồ ly à!?
Dialogue: 0,0:01:01.42,0:01:02.65,Default,,0000,0000,0000,,Không có chuyện đó đâu.
Dialogue: 0,0:01:03.89,0:01:04.86,Default,,0000,0000,0000,,Tôi không đùa đâu!
Dialogue: 0,0:01:04.93,0:01:05.89,Default,,0000,0000,0000,,Đó là sự thật.
Dialogue: 0,0:01:05.96,0:01:06.95,Default,,0000,0000,0000,,Ôi! ớn quá.
Dialogue: 0,0:01:07.06,0:01:08.36,Default,,0000,0000,0000,,Đi mà lo chuyện của cậu đi!
Dialogue: 0,0:01:09.16,0:01:10.15,Default,,0000,0000,0000,,Được thôi.
Dialogue: 0,0:01:10.27,0:01:11.39,Default,,0000,0000,0000,,Thích bị H thì cứ việc!
Dialogue: 0,0:01:13.33,0:01:14.60,Default,,0000,0000,0000,,Ai vậy?
Dialogue: 0,0:01:14.70,0:01:16.00,Default,,0000,0000,0000,,Chẳng hiểu ý cô ta là gì.
Dialogue: 0,0:01:16.84,0:01:19.43,Default,,0000,0000,0000,,Có thể bạn ý muốn tham gia.
Dialogue: 0,0:01:20.01,0:01:22.70,Default,,0000,0000,0000,,Cứ như otaku chuyên đọc truyện tâm linh
Dialogue: 0,0:01:23.28,0:01:24.61,Default,,0000,0000,0000,,Cậu biết cô ấy?
Dialogue: 0,0:01:25.08,0:01:26.05,Default,,0000,0000,0000,,Không nhớ à?
Dialogue: 0,0:01:26.15,0:01:28.74,Default,,0000,0000,0000,,Học sinh mới của lớp C từ tháng truớc đó.
Dialogue: 0,0:01:28.85,0:01:31.75,Default,,0000,0000,0000,, Tên là Mia Tsunoda.
Dialogue: 0,0:01:31.82,0:01:35.45,Default,,0000,0000,0000,, Học sinh năm 3, người thành lập hội tâm linh hay cái gì đó.
Dialogue: 0,0:01:35.52,0:01:36.96,Default,,0000,0000,0000,,Một cô gái rất kỳ quái.
Dialogue: 0,0:01:39.83,0:01:41.52,Default,,0000,0000,0000,,Thôi, bắt đầu lại đi.
Dialogue: 0,0:01:41.63,0:01:44.26,Default,,0000,0000,0000,,Cái gì? Vẫn làm à? Mia mới bảo chúng ta có thể bị nhập...
Dialogue: 0,0:01:45.51,0:01:54.61,Default,,0000,0000,0000,,Sex Craft\NEp 1
Dialogue: 0,0:01:59.51,0:02:00.61,Default,,0000,0000,0000,,Đừng có linh tinh!
Dialogue: 0,0:02:00.68,0:02:03.52,Default,,0000,0000,0000,,Các cậu tính đùa với tớ đó hả?
Dialogue: 0,0:02:03.62,0:02:05.59,Default,,0000,0000,0000,,Không cần phải nóng vậy chứ.
Dialogue: 0,0:02:05.69,0:02:07.42,Default,,0000,0000,0000,,Chỉ là bói tình yêu thôi mà.
Dialogue: 0,0:02:10.56,0:02:14.12,Default,,0000,0000,0000,,Uhm, thế này không hay đâu, Tụi mình đâu thể dừng khi đã bắt đầu.
Dialogue: 0,0:02:14.23,0:02:18.53,Default,,0000,0000,0000,,Sau khi gọi được linh hồn, tụi mình sẽ cám ơn nó và đuổi nó đi.
Dialogue: 0,0:02:18.73,0:02:19.70,Default,,0000,0000,0000,,Cám ơn?
Dialogue: 0,0:02:19.80,0:02:24.00,Default,,0000,0000,0000,,Nói như vậy... Nhưng làm sao tớ cám ơn cái hồn đó được?
Dialogue: 0,0:02:24.14,0:02:25.83,Default,,0000,0000,0000,,Mari...
Dialogue: 0,0:02:27.14,0:02:28.87,Default,,0000,0000,0000,,Đồng xu...
Dialogue: 0,0:02:28.98,0:02:29.94,Default,,0000,0000,0000,,gì thế?
Dialogue: 0,0:02:30.04,0:02:31.74,Default,,0000,0000,0000,,Ta...
Dialogue: 0,0:02:32.98,0:02:34.28,Default,,0000,0000,0000,,...không...
Dialogue: 0,0:02:34.75,0:02:35.77,Default,,0000,0000,0000,,...tha...
Dialogue: 0,0:02:35.85,0:02:36.82,Default,,0000,0000,0000,,...thứ.
Dialogue: 0,0:02:36.92,0:02:37.89,Default,,0000,0000,0000,, Ta không tha thứ?
Dialogue: 0,0:02:37.99,0:02:39.01,Default,,0000,0000,0000,,Thế là thế nào?
Dialogue: 0,0:02:39.09,0:02:40.38,Default,,0000,0000,0000,,Đồng xu tự di chuyển!
Dialogue: 0,0:02:47.06,0:02:48.36,Default,,0000,0000,0000,,Chuyện gì thế này?
Dialogue: 0,0:03:30.21,0:03:31.87,Default,,0000,0000,0000,,Nghiêm túc chứ?
Dialogue: 0,0:03:31.97,0:03:33.80,Default,,0000,0000,0000,,Em đồng ý với 10,000 yen?
Dialogue: 0,0:03:34.81,0:03:35.78,Default,,0000,0000,0000,,Uh.
Dialogue: 0,0:03:35.88,0:03:39.61,Default,,0000,0000,0000,,Em sẽ đợi ở cầu vuợt trước trạm.
Dialogue: 0,0:03:39.71,0:03:41.91,Default,,0000,0000,0000,,Anh sẽ nhận ra đó là em...
Dialogue: 0,0:03:44.69,0:03:47.38,Default,,0000,0000,0000,,...với chiếc ô đỏ.
Dialogue: 0,0:03:52.33,0:03:53.66,Default,,0000,0000,0000,,...Em...
Dialogue: 0,0:03:54.16,0:03:56.29,Default,,0000,0000,0000,,Em có phải là Mari không?
Dialogue: 0,0:04:34.14,0:04:35.19,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:04:35.27,0:04:36.29,Default,,0000,0000,0000,,Y..y...
Dialogue: 0,0:04:36.77,0:04:39.33,Default,,0000,0000,0000,,Anh ra đây...
Dialogue: 0,0:04:40.51,0:04:44.95,Default,,0000,0000,0000,,Em...em cũng thế...
Dialogue: 0,0:04:48.75,0:04:50.22,Default,,0000,0000,0000,,Y...y...!
Dialogue: 0,0:05:11.51,0:05:14.48,Default,,0000,0000,0000,,Nó...lại cứng rồi.
Dialogue: 0,0:05:45.07,0:05:47.67,Default,,0000,0000,0000,,sướng quá!!
Dialogue: 0,0:06:06.09,0:06:07.36,Default,,0000,0000,0000,,Mari...
Dialogue: 0,0:06:22.78,0:06:25.75,Default,,0000,0000,0000,,đúng rồi...
Dialogue: 0,0:06:39.79,0:06:42.52,Default,,0000,0000,0000,,Mari hôm nay cũng không đến
Dialogue: 0,0:06:42.63,0:06:45.10,Default,,0000,0000,0000,,Cậu ấy cũng không trả lời điện thoại. Không biết cậu ấy tự sướng ở đâu...
Dialogue: 0,0:06:46.57,0:06:48.56,Default,,0000,0000,0000,,Có phải do linh hồn đó không?
Dialogue: 0,0:06:48.90,0:06:50.13,Default,,0000,0000,0000,,Gì cơ?
Dialogue: 0,0:06:50.24,0:06:53.17,Default,,0000,0000,0000,,Vì... Mari không đến trưòng sau ngày đó...
Dialogue: 0,0:06:53.27,0:06:56.07,Default,,0000,0000,0000,,...và cậu ấy đã cư xử rất lạ trên đưòng về nhà.
Dialogue: 0,0:07:01.38,0:07:05.15,Default,,0000,0000,0000,,Tớ đoán cô ấy đang rất vui vẻ \Ncùng với những người bạn ở nơi làm việc của mình.
Dialogue: 0,0:07:05.25,0:07:07.88,Default,,0000,0000,0000,,Có thể bây giờ họ đang chơi đùa với nhau.
Dialogue: 0,0:07:07.99,0:07:09.89,Default,,0000,0000,0000,,Hi vọng là cậu đúng...
Dialogue: 0,0:07:10.06,0:07:12.32,Default,,0000,0000,0000,,Cậu không nên quá lo lắng.
Dialogue: 0,0:07:12.43,0:07:14.05,Default,,0000,0000,0000,,Hey, Reina.
Dialogue: 0,0:07:14.16,0:07:17.46,Default,,0000,0000,0000,,Chúng ta qua nhà Mari để thăm cậu ấy không? Cho chắc ăn
Dialogue: 0,0:07:31.88,0:07:32.85,Default,,0000,0000,0000,,Cô!
Dialogue: 0,0:07:33.71,0:07:34.74,Default,,0000,0000,0000,,Kumi!
Dialogue: 0,0:07:34.85,0:07:36.32,Default,,0000,0000,0000,,Có chuyện gì vậy ạ?
Dialogue: 0,0:07:37.49,0:07:38.45,Default,,0000,0000,0000,,Mari...
Dialogue: 0,0:07:38.52,0:07:39.72,Default,,0000,0000,0000,,Cô nhận đc tin Mari bị thương rất nặng!
Dialogue: 0,0:07:39.72,0:07:41.05,Default,,0000,0000,0000,,Sao?
Dialogue: 0,0:07:42.79,0:07:44.78,Default,,0000,0000,0000,,Mại dâm?
Dialogue: 0,0:07:46.16,0:07:47.13,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:07:47.30,0:07:48.49,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:07:48.60,0:07:49.82,Default,,0000,0000,0000,,Mari không thể như thế...
Dialogue: 0,0:07:50.53,0:07:54.47,Default,,0000,0000,0000,,Có vẻ như người đàn ông đó đã chém em ấy ngay tại khách sạn.
Dialogue: 0,0:07:54.57,0:07:56.43,Default,,0000,0000,0000,,Nhưng.... tại sao?
Dialogue: 0,0:07:57.31,0:07:59.47,Default,,0000,0000,0000,,Chúng tôi sẽ điều tra chi tiết hơn...
Dialogue: 0,0:07:59.57,0:08:03.44,Default,,0000,0000,0000,,Theo lời khai của người đàn ông, Mari cứ nhất quyết không nhả ra.....
Dialogue: 0,0:08:03.54,0:08:07.07,Default,,0000,0000,0000,,cậu ta quá sợ hãi nên đã tặng cho em ấy một vết chém trên mặt...
Dialogue: 0,0:08:11.35,0:08:12.34,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:08:12.52,0:08:14.32,Default,,0000,0000,0000,,Nữa...
Dialogue: 0,0:08:16.49,0:08:17.79,Default,,0000,0000,0000,,đúng rồi...
Dialogue: 0,0:08:22.06,0:08:23.69,Default,,0000,0000,0000,,Em đang làm gì vậy?
Dialogue: 0,0:08:28.34,0:08:29.39,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:08:33.91,0:08:35.27,Default,,0000,0000,0000,,Dùng lại
Dialogue: 0,0:08:37.08,0:08:38.51,Default,,0000,0000,0000,,Mari, đừng, dừng lại!
Dialogue: 0,0:08:43.25,0:08:44.84,Default,,0000,0000,0000,,Mari, thôi đi!
Dialogue: 0,0:08:46.82,0:08:48.15,Default,,0000,0000,0000,,Mari!
Dialogue: 0,0:08:50.53,0:08:53.36,Default,,0000,0000,0000,,Mari, Mari, Mari!
Dialogue: 0,0:09:16.02,0:09:16.98,Default,,0000,0000,0000,,Mari!
Dialogue: 0,0:09:17.08,0:09:18.05,Default,,0000,0000,0000,,Mari!
Dialogue: 0,0:09:19.62,0:09:23.45,Default,,0000,0000,0000,,Mari! Mari!
Dialogue: 0,0:09:59.36,0:10:00.38,Default,,0000,0000,0000,,Xin chào?
Dialogue: 0,0:10:00.49,0:10:02.36,Default,,0000,0000,0000,,Trịnh trọng với tớ thế!
Dialogue: 0,0:10:02.46,0:10:03.93,Default,,0000,0000,0000,,Cậu đang ở đâu?
Dialogue: 0,0:10:04.30,0:10:05.27,Default,,0000,0000,0000,,Ở đâu...
Dialogue: 0,0:10:05.63,0:10:07.26,Default,,0000,0000,0000,,Cậu đột nhiên biến mất ở bệnh biện.
Dialogue: 0,0:10:07.37,0:10:08.67,Default,,0000,0000,0000,,Tớ lo cho cậu lắm đấy!
Dialogue: 0,0:10:08.77,0:10:10.50,Default,,0000,0000,0000,,Tớ đã gọi cậu qua điện thoại, sao cậu không bắt máy?.
Dialogue: 0,0:10:10.61,0:10:12.20,Default,,0000,0000,0000,,Tớ đang tìm cậu đó!
Dialogue: 0,0:10:14.58,0:10:15.54,Default,,0000,0000,0000,,gì cơ?
Dialogue: 0,0:10:16.34,0:10:17.31,Default,,0000,0000,0000,,Alo?
Dialogue: 0,0:10:17.41,0:10:18.84,Default,,0000,0000,0000,,Cậu có nghe không đấy, Kumi?
Dialogue: 0,0:10:20.38,0:10:21.71,Default,,0000,0000,0000,,Tớ sẽ gọi lại sau!
Dialogue: 0,0:10:22.75,0:10:23.72,Default,,0000,0000,0000,,Anh tới ngay đây...
Dialogue: 0,0:10:23.82,0:10:24.79,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:10:56.08,0:10:57.24,Default,,0000,0000,0000,,Kumi?
Dialogue: 0,0:10:57.35,0:10:59.38,Default,,0000,0000,0000,,Con về trễ đấy.
Dialogue: 0,0:11:02.46,0:11:05.52,Default,,0000,0000,0000,,Mình đã làm gì ở đó?
Dialogue: 0,0:11:07.43,0:11:09.26,Default,,0000,0000,0000,,Mình.... không nhớ gì hết...
Dialogue: 0,0:11:09.80,0:11:11.09,Default,,0000,0000,0000,,Kumi?
Dialogue: 0,0:11:11.73,0:11:14.22,Default,,0000,0000,0000,,Nếu cậu cầu cơ chỉ cho vui...
Dialogue: 0,0:11:14.34,0:11:16.13,Default,,0000,0000,0000,,...cậu sẽ bị nhập.
Dialogue: 0,0:11:27.85,0:11:28.87,Default,,0000,0000,0000,,Sếp!
Dialogue: 0,0:11:29.28,0:11:30.25,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:11:30.35,0:11:32.84,Default,,0000,0000,0000,,Bạn này có chuyện muốn nói với sếp.
Dialogue: 0,0:11:40.70,0:11:41.75,Default,,0000,0000,0000,,tớ chắc đó là.
Dialogue: 0,0:11:41.86,0:11:43.06,Default,,0000,0000,0000,,hồ ly đã nhập vào cậu.
Dialogue: 0,0:11:43.53,0:11:45.06,Default,,0000,0000,0000,,Hồ ly nhập vào mình?
Dialogue: 0,0:11:46.07,0:11:49.06,Default,,0000,0000,0000,,Con hồ ly nhập vào Mari đầu tiên...
Dialogue: 0,0:11:49.17,0:11:51.00,Default,,0000,0000,0000,,...rồi thoát khỏi xác cô ấy vì cô ấy đã quá sức...
Dialogue: 0,0:11:51.11,0:11:53.20,Default,,0000,0000,0000,,...và chuyển qua thân xác của cậu
Dialogue: 0,0:11:53.94,0:11:55.88,Default,,0000,0000,0000,,Tôi đã cảnh báo các cậu rồi
Dialogue: 0,0:11:58.25,0:12:00.34,Default,,0000,0000,0000,,Tớ nên làm gì đây?
Dialogue: 0,0:12:00.82,0:12:01.80,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:12:01.92,0:12:03.71,Default,,0000,0000,0000,,Sếp có thể trục xuất nó cho cậu.
Dialogue: 0,0:12:04.02,0:12:05.01,Default,,0000,0000,0000,,Thật á?
Dialogue: 0,0:12:05.82,0:12:06.84,Default,,0000,0000,0000,,Đúng không, sếp?
Dialogue: 0,0:12:07.22,0:12:08.38,Default,,0000,0000,0000,,Tất nhiên rồi...
Dialogue: 0,0:12:09.19,0:12:10.89,Default,,0000,0000,0000,,Cô ấy là pháp sư.
Dialogue: 0,0:12:10.99,0:12:12.55,Default,,0000,0000,0000,,Pháp sư...
Dialogue: 0,0:12:13.43,0:12:14.66,Default,,0000,0000,0000,,He he, nhưng lại rất bận rộn.
Dialogue: 0,0:12:15.13,0:12:16.10,Default,,0000,0000,0000,,Phải không sếp?
Dialogue: 0,0:12:16.20,0:12:17.66,Default,,0000,0000,0000,,Điều đó không có quan trọng
Dialogue: 0,0:12:17.77,0:12:18.75,Default,,0000,0000,0000,,Mia!
Dialogue: 0,0:12:20.40,0:12:21.46,Default,,0000,0000,0000,,Đừng lo
Dialogue: 0,0:12:21.57,0:12:23.50,Default,,0000,0000,0000,,Tớ sẽ trục xuất nó.
Dialogue: 0,0:12:29.18,0:12:30.58,Default,,0000,0000,0000,,Nhanh lên!
Dialogue: 0,0:12:30.64,0:12:32.64,Default,,0000,0000,0000,,Chuẩn bị xong hết rồi đó!
Dialogue: 0,0:12:33.65,0:12:35.21,Default,,0000,0000,0000,, À ừ...
Dialogue: 0,0:12:47.06,0:12:48.05,Default,,0000,0000,0000,,không...
Dialogue: 0,0:12:59.01,0:13:00.74,Default,,0000,0000,0000,,đừng...
Dialogue: 0,0:13:07.31,0:13:08.28,Default,,0000,0000,0000,,Mia...
Dialogue: 0,0:13:09.78,0:13:11.62,Default,,0000,0000,0000,,Mia...
Dialogue: 0,0:13:12.79,0:13:14.15,Default,,0000,0000,0000,,sao vậy?
Dialogue: 0,0:13:14.26,0:13:15.65,Default,,0000,0000,0000,,Cậu hơi ồn ào đó.
Dialogue: 0,0:13:15.82,0:13:16.92,Default,,0000,0000,0000,,Umm...
Dialogue: 0,0:13:17.02,0:13:21.29,Default,,0000,0000,0000,,Có thật làm thế này sẽ trục xuất được nó...
Dialogue: 0,0:13:21.40,0:13:23.73,Default,,0000,0000,0000,,Không tin thì thui.
Dialogue: 0,0:13:24.30,0:13:28.23,Default,,0000,0000,0000,,Quỷ và những linh hồn hút năng lượng của con người.
Dialogue: 0,0:13:28.34,0:13:30.80,Default,,0000,0000,0000,,Đối với con trai, là tinh dịch.
Dialogue: 0,0:13:30.91,0:13:33.64,Default,,0000,0000,0000,,Đối với con gái, là dâm thủy lúc họ cực khoái.
Dialogue: 0,0:13:33.74,0:13:39.87,Default,,0000,0000,0000,,Nên cậu phải đạt đc sự cực khoái để bãy con hồ ly đó.
Dialogue: 0,0:13:40.78,0:13:42.58,Default,,0000,0000,0000,, Ưm, nhưng...
Dialogue: 0,0:13:42.68,0:13:43.98,Default,,0000,0000,0000,,Yoshikawa...
Dialogue: 0,0:13:46.55,0:13:49.02,Default,,0000,0000,0000,,Đừng...
Dialogue: 0,0:14:25.09,0:14:26.65,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:15:32.76,0:15:35.89,Default,,0000,0000,0000,,Tớ ra rồi!
Dialogue: 0,0:15:49.04,0:15:55.21,Default,,0000,0000,0000,,Ta yêu cầu ngươi linh hồn kia. \NRời khỏi Kumi và biến đi.
Dialogue: 0,0:15:56.08,0:15:58.61,Default,,0000,0000,0000,,Ta... không...
Dialogue: 0,0:15:58.72,0:16:00.15,Default,,0000,0000,0000,,Ta không muốn?
Dialogue: 0,0:16:00.45,0:16:01.44,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:16:04.56,0:16:05.58,Default,,0000,0000,0000,,Đó là...
Dialogue: 0,0:16:08.83,0:16:12.23,Default,,0000,0000,0000,,Rin, hyo, tou, sha, kai, gìin, retsu, zai, zen!
Dialogue: 0,0:16:38.76,0:16:41.89,Default,,0000,0000,0000,,Tenchigen Myokougìinpentsuuriki!
Dialogue: 0,0:16:45.90,0:16:47.37,Default,,0000,0000,0000,,Nó chuồn rồi...
Dialogue: 0,0:16:53.51,0:16:54.47,Default,,0000,0000,0000,,Kumi!
Dialogue: 0,0:16:54.94,0:16:55.77,Default,,0000,0000,0000,,Kumi!
Dialogue: 0,0:17:00.05,0:17:03.38,Default,,0000,0000,0000,,Có vẻ các cậu triệu hồi đuợc hàng khủng rồi.
Dialogue: 0,0:17:04.08,0:17:05.05,Default,,0000,0000,0000,,Khủng?
Dialogue: 0,0:17:05.15,0:17:07.09,Default,,0000,0000,0000,,Tớ nghĩ đó là hồn hồ ly thật.
Dialogue: 0,0:17:07.89,0:17:10.02,Default,,0000,0000,0000,,Và nó hoàn toàn không yếu đâu.
Dialogue: 0,0:17:10.96,0:17:13.12,Default,,0000,0000,0000,,Nó chính xác là thứ gì?
Dialogue: 0,0:17:14.16,0:17:17.43,Default,,0000,0000,0000,,Cả ba cậu đều dính vào chuyện này, đúng không?
Dialogue: 0,0:17:17.83,0:17:19.13,Default,,0000,0000,0000,,Ai là người thứ ba?
Dialogue: 0,0:17:23.37,0:17:25.00,Default,,0000,0000,0000,,Sao vậy, Tachibana?
Dialogue: 0,0:17:31.25,0:17:33.08,Default,,0000,0000,0000,,- Tachibana!\N - Reina!
Dialogue: 0,0:17:33.21,0:17:35.04,Default,,0000,0000,0000,,- Tachibana!\N - Reina!
Dialogue: 0,0:17:39.85,0:17:40.84,Default,,0000,0000,0000,,Oh, Kumi.
Dialogue: 0,0:17:40.95,0:17:41.94,Default,,0000,0000,0000,,Chuyện gì mới xảy ra vậy?
Dialogue: 0,0:18:16.89,0:18:21.29,Default,,0000,0000,0420,,Bệnh xá
Dialogue: 0,0:18:27.10,0:18:28.23,Default,,0000,0000,0000,,Làm ơn...
Dialogue: 0,0:18:28.34,0:18:31.00,Default,,0000,0000,0000,,Tôi không quan tâm em sẽ làm gì. Chỉ cần tiếp tục như thế!
Dialogue: 0,0:18:32.61,0:18:33.60,Default,,0000,0000,0000,,Reina!
Dialogue: 0,0:18:35.54,0:18:37.07,Default,,0000,0000,0000,,Ngừng đi, Reina!
Dialogue: 0,0:18:39.11,0:18:39.74,Default,,0000,0000,0000,,Kumi!
Dialogue: 0,0:18:55.93,0:18:57.06,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:18:57.73,0:18:58.89,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:19:03.60,0:19:04.59,Default,,0000,0000,0000,,Reina...
Dialogue: 0,0:19:04.71,0:19:05.67,Default,,0000,0000,0000,,Làm ơn...
Dialogue: 0,0:19:09.14,0:19:10.11,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:19:11.45,0:19:13.57,Default,,0000,0000,0000,,Chuyện gì xảy ra với Reina?
Dialogue: 0,0:19:14.18,0:19:15.94,Default,,0000,0000,0000,,Linh hồn đó đã nhập vào cô ấy...
Dialogue: 0,0:19:16.05,0:19:17.61,Default,,0000,0000,0000,,...là darkini!
Dialogue: 0,0:19:17.72,0:19:18.74,Default,,0000,0000,0000,,Mia!
Dialogue: 0,0:19:18.82,0:19:19.81,Default,,0000,0000,0000,,Sếp!
Dialogue: 0,0:19:20.45,0:19:21.44,Default,,0000,0000,0000,,Gắng 1 chút
Dialogue: 0,0:19:21.56,0:19:23.02,Default,,0000,0000,0000,, Tôi sẽ cứu 2 nguời.
Dialogue: 0,0:19:24.39,0:19:26.42,Default,,0000,0000,0000,,Tham lam quá đấy, con cáo thành tinh kia.
Dialogue: 0,0:19:27.03,0:19:30.29,Default,,0000,0000,0000,,Trở thành linh hồn đã đủ sướng rồi.
Dialogue: 0,0:19:30.90,0:19:34.49,Default,,0000,0000,0000,,Nhưng ngươi lại muốn thêm sức mạnh. Đán khinh!
Dialogue: 0,0:19:36.70,0:19:39.04,Default,,0000,0000,0000,,Oh, tự ái rồi à.
Dialogue: 0,0:19:39.14,0:19:41.70,Default,,0000,0000,0000,,Sao không thử lấy năng lượng của ta
Dialogue: 0,0:20:03.36,0:20:05.52,Default,,0000,0000,0000,,Ta sẽ nhai hết, năng lượng của ngươi!
Dialogue: 0,0:20:34.86,0:20:37.99,Default,,0000,0000,0000,,Rin, hyo, tou, sha, kai, gìin, retsu, zai, zen!
Dialogue: 0,0:20:46.87,0:20:49.00,Default,,0000,0000,0000,,Yoshikawa! Đóng của!
Dialogue: 0,0:20:54.41,0:20:55.88,Default,,0000,0000,0000,,Ngươi không thể chạy nữa.
Dialogue: 0,0:20:55.95,0:20:58.65,Default,,0000,0000,0000,,Cái bãy này dành cho ngươi đó.
Dialogue: 0,0:21:03.42,0:21:10.66,Default,,0000,0000,0000,,Đòn này sẽ diệt linh hồn bám vào cô ấy.
Dialogue: 0,0:21:32.19,0:21:33.81,Default,,0000,0000,0000,,Tên cậu là Reina, phải không?
Dialogue: 0,0:21:34.39,0:21:35.79,Default,,0000,0000,0000,,Tớ sẽ giúp cậu thấy thoải mái...
Dialogue: 0,0:21:38.09,0:21:39.68,Default,,0000,0000,0000,,Mia, cậu định làm gì?
Dialogue: 0,0:21:46.83,0:21:47.82,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:21:47.93,0:21:50.10,Default,,0000,0000,0000,,Đấy là nước thánh làm từ trượng thần.
Dialogue: 0,0:21:50.17,0:21:54.61,Default,,0000,0000,0000,,Giờ, tớ cần các cậu giúp
Dialogue: 0,0:21:57.94,0:21:59.34,Default,,0000,0000,0000,,Nước thánh và...
Dialogue: 0,0:21:59.65,0:22:02.27,Default,,0000,0000,0000,,Kiếm của Fudo...
Dialogue: 0,0:22:03.05,0:22:05.92,Default,,0000,0000,0000,,Chúng ta sẽ ép nó ra...
Dialogue: 0,0:22:06.19,0:22:08.71,Default,,0000,0000,0000,,...và bãy hồn
Dialogue: 0,0:23:15.66,0:23:17.25,Default,,0000,0000,0000,,Ác linh, biến!
Dialogue: 0,0:23:17.36,0:23:18.76,Default,,0000,0000,0000,,Kaman!
Dialogue: 0,0:23:31.37,0:23:32.34,Default,,0000,0000,0000,,Reina!
Dialogue: 0,0:23:33.04,0:23:34.03,Default,,0000,0000,0000,,Reina!
Dialogue: 0,0:23:34.78,0:23:35.80,Default,,0000,0000,0000,,Reina!
Dialogue: 0,0:23:36.84,0:23:39.57,Default,,0000,0000,0000,,Kumi? Tớ...
Dialogue: 0,0:23:41.01,0:23:41.98,Default,,0000,0000,0000,,Sau?
Dialogue: 0,0:23:42.08,0:23:43.67,Default,,0000,0000,0000,, Sao tớ khỏa thân thế này?
Dialogue: 0,0:23:44.45,0:23:45.42,Default,,0000,0000,0000,,Mia!
Dialogue: 0,0:23:45.99,0:23:47.85,Default,,0000,0000,0000,,Yeah, cô ấy ổn rồi ^^
Dialogue: 0,0:23:49.82,0:23:50.98,Default,,0000,0000,0000,,Tạ ơn trời.
Dialogue: 0,0:23:51.22,0:23:52.28,Default,,0000,0000,0000,,Reina!
Dialogue: 0,0:23:53.63,0:23:54.59,Default,,0000,0000,0000,,Kumi?
Dialogue: 0,0:24:01.37,0:24:02.46,Default,,0000,0000,0000,,Hồ ly?
Dialogue: 0,0:24:03.24,0:24:07.23,Default,,0000,0000,0000,,Hehe, có thể nó là nguyên nhân \Ndẫn đến những việc nhứt đầu này...
Dialogue: 0,0:24:08.28,0:24:12.91,Default,,0000,0000,0000,,Cậu không bị nhập như Mari hay Reina...
Dialogue: 0,0:24:13.01,0:24:16.85,Default,,0000,0000,0000,,...vì cậu tôn trọng linh hồn.
Dialogue: 0,0:24:17.42,0:24:21.91,Default,,0000,0000,0000,, Có nghĩa là linh hồn đó không hề ghét tớ?
Dialogue: 0,0:24:22.46,0:24:25.02,Default,,0000,0000,0000,,Tớ nuôi nó nhé (có gì còn được nhập nữa)
Dialogue: 0,0:24:25.89,0:24:26.95,Default,,0000,0000,0000,,Nuôi con hồ ly này?
Dialogue: 0,0:24:27.06,0:24:28.12,Default,,0000,0000,0000,, Nó là quỷ đó
Dialogue: 0,0:24:28.20,0:24:29.42,Default,,0000,0000,0000,,Đừng làm thế!
Dialogue: 0,0:24:29.53,0:24:31.39,Default,,0000,0000,0000,,Chị nên gọi em là gì?
Dialogue: 0,0:24:32.57,0:24:33.56,Default,,0000,0000,0000,,Foxy.
Dialogue: 0,0:24:34.20,0:24:36.03,Default,,0000,0000,0000,,Yeah, chị sẽ gọi em là Foxy!
Dialogue: 0,0:24:49.62,0:24:51.24,Default,,0000,0000,0000,,Xin lỗi, tớ đến muộn!
Dialogue: 0,0:24:52.55,0:24:54.02,Default,,0000,0000,0000,,ok, bắt đầu thôi.
Dialogue: 0,0:24:54.42,0:24:55.85,Default,,0000,0000,0000,,Bắt đầu?
Dialogue: 0,0:24:56.22,0:24:57.88,Default,,0000,0000,0000,,Còn các thành viên khác?
Dialogue: 0,0:24:58.63,0:25:00.75,Default,,0000,0000,0000,,Huh? Tớ chưa nói gì với cậu à?
Dialogue: 0,0:25:00.86,0:25:04.52,Default,,0000,0000,0000,, Thành viên của hội có tớ và trợ lí, Yoshikawa.
Dialogue: 0,0:25:04.97,0:25:05.93,Default,,0000,0000,0000,, Ừh ^^.
Dialogue: 0,0:25:06.03,0:25:07.93,Default,,0000,0000,0000,,Nên, cậu sẽ là thành viên thứ 3.
Dialogue: 0,0:25:10.54,0:25:13.30,Default,,0000,0000,0000,,Vậy Foxy là thành viên thứ 4 nhé?.
Dialogue: 0,0:25:13.41,0:25:14.50,Default,,0000,0000,0000,,Được không, Foxy?
Dialogue: 0,0:25:18.08,0:25:19.44,Default,,0000,0000,0000,,Haizzzz...
