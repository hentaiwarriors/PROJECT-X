Dialogue: 0,0:00:03.07,0:00:05.03,HHH Triple Ecchi Dialogue,,0000,0000,0000,,S-Senpai!
Dialogue: 0,0:00:40.52,0:00:42.32,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chút nữa sẽ còn bắn ra nhiều hơn đó!
Dialogue: 0,0:00:42.32,0:00:43.53,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cố lên nào!
Dialogue: 0,0:00:45.26,0:00:49.86,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:00:55.27,0:00:58.28,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Coi nào, sao lại ngồi co ro một góc thế kia?!
Dialogue: 0,0:00:58.28,0:01:00.58,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Trò vui vẫn còn chưa bắt đầu mà.
Dialogue: 0,0:01:05.54,0:01:12.15,HHH Triple Ecchi Title,,0000,0000,0000,,{\fad(100,1000)\shad0\bord2\fs40\c&H7FAEFE&\3c&H99E8F0&\pos(341,75)}B{\c&H00D6FE&}a{\c&H2FBF8B&}k{\c&HE5C067&}a{\c&HC07C95&}-{\c&HC2CAFE&}s{\c&H20A0A9&}u{\c&H6A95FE&}b\N{\c&HFF3A00&\fs30}presents
Dialogue: 0,0:01:13.95,0:01:18.04,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nè, nếu ngày mai cậu rảnh thì ghé nhà tôi chơi nhé?
Dialogue: 0,0:01:19.19,0:01:20.72,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vâng!
Dialogue: 0,0:01:21.66,0:01:23.88,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Mình vừa được Senpai mời tới thăm nhà.{\i0}
Dialogue: 0,0:01:23.88,0:01:26.12,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Vậy có nghĩa là...?{\i0}
Dialogue: 0,0:01:33.01,0:01:34.29,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}To quá!{\i0}
Dialogue: 0,0:01:35.47,0:01:38.96,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Bố già, chẳng phải con đã từ chối rồi sao?!
Dialogue: 0,0:01:39.37,0:01:41.48,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ta không hiểu con đang phàn nàn điều gì?
Dialogue: 0,0:01:41.73,0:01:45.33,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Con không có ý định kết hôn với anh ta!
Dialogue: 0,0:01:46.41,0:01:48.55,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Kể cả khi con nói như vậy...
Dialogue: 0,0:01:48.55,0:01:49.92,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Ba đừng có tự tiện làm theo ý mình!
Dialogue: 0,0:01:49.92,0:01:53.68,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Sao con lại cứng đầu như vậy, cậu ấy là một người chồng lí tưởng.
Dialogue: 0,0:01:53.68,0:01:55.65,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Không phải lí tưởng đối với con, bố già!
Dialogue: 0,0:01:55.65,0:01:57.31,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Con dám to tiếng với ta phỏng?
Dialogue: 0,0:01:57.31,0:01:58.04,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Con không có ý đó!
Dialogue: 0,0:01:58.04,0:02:00.33,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Vậy sao con cứ khăng khăng làm trái ý ta!
Dialogue: 0,0:01:46.93,0:01:52.94,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Hóa ra mình chỉ là tấm bình phong để cô ấy từ chối hôn ước do ba mẹ chỉ định...{\i0}
Dialogue: 0,0:01:52.94,0:01:55.07,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Cơ mà, đúng là con nhà giàu có khác...{\i0}
Dialogue: 0,0:01:55.07,0:01:57.89,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Căn nhà rộng mênh mông, cầu thang cao chót vót.{\i0}
Dialogue: 0,0:01:57.89,0:02:00.33,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Khác hẳn mấy nhà bình dân...{\i0}
Dialogue: 0,0:02:02.59,0:02:05.08,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Trà hãy còn nóng, cháu cẩn thận nha.
Dialogue: 0,0:02:05.08,0:02:06.48,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:02:08.47,0:02:11.60,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vậy để ta tiếp chuyện chàng trai ngồi đây, tên cậu là gì nhỉ?
Dialogue: 0,0:02:11.60,0:02:13.36,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vâng!
Dialogue: 0,0:02:13.36,0:02:16.76,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Xin lỗi bác, cháu là "đồ chơi" của Shigure-san...
Dialogue: 0,0:02:16.76,0:02:19.29,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thôi được rồi, để con giới thiệu cậu ấy.
Dialogue: 0,0:02:19.29,0:02:21.19,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đây là Kita Komatsu-kun.
Dialogue: 0,0:02:21.40,0:02:24.86,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Bọn con đã hẹn hò cùng nhau một thời gian, cậu ấy chính là vị hôn phu do con lựa chọn.
Dialogue: 0,0:02:24.92,0:02:26.72,HHH Triple Ecchi Sign,,0000,0000,0000,,{\b1\4c&HEFF4F4&\c&H000000&\3c&HEFF4F4&\fs45\move(890,272,-175,272,0,1800)\fnUVN Nguyen Du}[Hôn phu được lựa chọn]
Dialogue: 0,0:02:26.99,0:02:27.98,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hả?
Dialogue: 0,0:02:27.98,0:02:30.02,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Có... thật không đấy?!
Dialogue: 0,0:02:30.02,0:02:32.67,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tất nhiên rồi, phải không Kita Komatsu-kun?
Dialogue: 0,0:02:32.67,0:02:36.02,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thứ lỗi cho ta, gia đình cậu xuất thân dòng dõi thế nào?
Dialogue: 0,0:02:36.02,0:02:37.87,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ba mẹ cậu làm nghề gì? Thu nhập hàng tháng bao nhiêu?
Dialogue: 0,0:02:37.87,0:02:40.40,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ba không thấy mình đường đột sao?!
Dialogue: 0,0:02:40.40,0:02:45.01,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Con nói cái gì vậy? Chuyện này rất quan trọng!
Dialogue: 0,0:02:45.03,0:02:48.49,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Ba đừng tưởng cao sang quyền quý đã là hay ho nhé?!
Dialogue: 0,0:02:48.49,0:02:50.96,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Ta chỉ lo lắng cho con mà thôi!
Dialogue: 0,0:02:50.96,0:02:54.10,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Thế việc gì ba phải truy hỏi gắt gao như vậy!
Dialogue: 0,0:02:42.23,0:02:45.20,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Ba cháu chỉ là một công chức quèn,{\i0}
Dialogue: 0,0:02:45.20,0:02:49.82,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Gia đình cháu vẫn còn món nợ 30 năm mới giả hết.{\i0}
Dialogue: 0,0:02:49.82,0:02:54.04,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Và... nhà cháu đang nuôi một con chuột cảnh.{\i0}
Dialogue: 0,0:02:54.48,0:02:55.53,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Shigure!
Dialogue: 0,0:02:56.75,0:02:59.25,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Có thật là con đang yêu và hẹn hò với cậu ấy không vậy?
Dialogue: 0,0:02:59.25,0:03:02.35,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ơ... tất nhiên rồi. Phải không, Kita Komatsu-kun?
Dialogue: 0,0:03:02.35,0:03:03.39,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ơ?
Dialogue: 0,0:03:03.39,0:03:05.05,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vừa nãy còn ở đây mà...
Dialogue: 0,0:03:05.05,0:03:09.85,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Con đúng là hết thuốc chữa, cậu ấy đã bỏ đi trong nước mắt.
Dialogue: 0,0:03:10.28,0:03:12.11,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ta thật không biết phải làm sao với con nữa?
Dialogue: 0,0:03:12.11,0:03:15.88,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Con chỉ biết đến bản thân mình và không bao giờ thèm để ý tới cảm xúc của cậu ấy.
Dialogue: 0,0:03:15.88,0:03:19.15,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ta đoán chắc suốt thời gian qua, con chỉ đùa giỡn với tình cảm của cậu ấy.
Dialogue: 0,0:03:19.15,0:03:21.96,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Bây giờ con còn dám vỗ ngực nói mình đang hẹn hò với người ta sao?
Dialogue: 0,0:03:22.98,0:03:24.51,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Mình vừa làm gì thế này...?{\i0}
Dialogue: 0,0:03:25.01,0:03:26.34,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Kita Komatsu-kun!
Dialogue: 0,0:03:26.34,0:03:27.80,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Này, Shigure!
Dialogue: 0,0:03:31.66,0:03:33.74,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hóa ra cậu rúc ở đây à.
Dialogue: 0,0:03:34.45,0:03:37.36,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Senpai, chị sắp kết hôn ư?
Dialogue: 0,0:03:37.67,0:03:39.68,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Đúng vậy...{\i0}
Dialogue: 0,0:03:39.68,0:03:42.42,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Chống đối bố già mãi cũng không phải ý hay.{\i0}
Dialogue: 0,0:03:43.46,0:03:46.79,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nhưng còn lâu tôi mới chịu kết hôn vào lúc này!
Dialogue: 0,0:03:47.50,0:03:51.96,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Cậu ta ăn mặc chải chuốt ghê nhỉ? Lại còn đeo cả cà vạt nữa.{\i0}
Dialogue: 0,0:03:51.96,0:03:55.64,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Là vậy sao? Cậu ta đã sửa soạn kĩ lưỡng khi tới đây.{\i0}
Dialogue: 0,0:03:58.55,0:04:01.40,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Còn định ăn vạ đến bao giờ nữa hả?
Dialogue: 0,0:04:03.10,0:04:06.15,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cởi quần áo ra nào. Giờ là lúc chúng ta "vui vẻ".
Dialogue: 0,0:04:06.15,0:04:09.62,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ơ? Bây giờ á? Như vậy e rằng không ổn đâu...
Dialogue: 0,0:04:09.94,0:04:11.44,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu nói gì kì vậy?
Dialogue: 0,0:04:11.44,0:04:15.28,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Một khi đã tới nước này rồi, chúng ta cứ vui chơi thỏa thích đã.
Dialogue: 0,0:04:16.23,0:04:19.37,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Thơm quá, đôi môi cô ấy thật mềm mại...{\i0}
Dialogue: 0,0:04:19.37,0:04:21.82,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Lưỡi của Senpai đang ngọ nguậy trên môi...{\i0}
Dialogue: 0,0:04:23.43,0:04:25.36,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Cô ấy chưa từng nút lưỡi vô trong...{\i0}
Dialogue: 0,0:04:26.49,0:04:30.74,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vì chưa từng hôn ai bao giờ nên khoản này tôi hoàn toàn mù tịt.
Dialogue: 0,0:04:30.74,0:04:33.15,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chắc là cậu cũng khoái vụ này phải không?
Dialogue: 0,0:04:33.72,0:04:36.09,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Phải công nhận là khoản này tôi chưa hề có kinh nghiệm gì cả.
Dialogue: 0,0:04:36.09,0:04:40.09,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vậy nên hôm nay tôi sẽ chiều theo ý cậu.
Dialogue: 0,0:04:41.80,0:04:43.83,HHH Triple Ecchi Dialogue,,0000,0000,0000,,S-Senpai!
Dialogue: 0,0:04:43.97,0:04:46.32,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vậy, cho em xem âm hộ được không?
Dialogue: 0,0:04:46.32,0:04:48.79,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hả? Được thôi...
Dialogue: 0,0:04:49.07,0:04:51.02,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chẳng phải cậu thấy suốt rồi sao?
Dialogue: 0,0:04:51.02,0:04:52.69,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Làm gì mà như sói đói vậy?
Dialogue: 0,0:04:53.17,0:04:55.10,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đừng bận tâm, em chỉ muốn chiêm ngưỡng nó thôi.
Dialogue: 0,0:04:55.10,0:04:57.43,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Biết ngay mà, cậu ta đang phởn.{\i0}
Dialogue: 0,0:04:58.12,0:05:00.71,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hé to hơn chút nữa được không, em muốn nhìn thật kĩ.
Dialogue: 0,0:05:00.71,0:05:03.40,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Mở ra mau nào!
Dialogue: 0,0:05:03.40,0:05:05.56,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hứ... cậu thô bạo vừa thôi.
Dialogue: 0,0:05:07.25,0:05:12.13,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Những lần trước, mình luôn là người ra lệnh,{\i0}
Dialogue: 0,0:05:12.13,0:05:15.58,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Bây giờ xấu hổ chết đi được...{\i0}
Dialogue: 0,0:05:19.79,0:05:23.56,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Không lẽ mình đã bắt đầu quen dần với điều này?{\i0}
Dialogue: 0,0:05:23.78,0:05:25.84,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thế này được chưa?
Dialogue: 0,0:05:25.84,0:05:28.39,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nếu chị không bỏ tay ra thì làm sao em nhìn được.
Dialogue: 0,0:05:28.59,0:05:30.69,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tôi biết rồi.
Dialogue: 0,0:05:30.69,0:05:33.50,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Không, sao mình lại có cảm giác háo hức như vậy!{\i0}
Dialogue: 0,0:05:35.22,0:05:39.39,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Xấu hổ chết đi được! Mình không thể chịu đựng thêm nữa.{\i0}
Dialogue: 0,0:05:40.50,0:05:43.12,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu dừng lại được chưa hả?
Dialogue: 0,0:05:43.12,0:05:44.05,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không, không được.
Dialogue: 0,0:05:44.05,0:05:45.15,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Khoan đã...
Dialogue: 0,0:05:46.40,0:05:50.04,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Oa, tuyệt quá. Em có thể nhìn vô bên trong Senpai rất rõ ràng.
Dialogue: 0,0:05:50.04,0:05:51.84,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:05:51.84,0:05:55.98,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Cậu ta đang nhìn... Cậu ta đang nhìn vào trong âm hộ mình...{\i0}
Dialogue: 0,0:05:55.98,0:05:58.26,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Âm hộ của chị mềm quá đi.
Dialogue: 0,0:05:58.26,0:06:01.30,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tình hình là em khó mà kiềm chế nổi nữa.
Dialogue: 0,0:06:01.30,0:06:05.32,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Những lần trước hệt như "tàu nhanh", giờ thì em có thể thoải mái chiêm ngưỡng bên trong rồi.
Dialogue: 0,0:06:05.32,0:06:07.21,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu đừng nói nữa.
Dialogue: 0,0:06:07.21,0:06:09.47,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em liếm nó được không?
Dialogue: 0,0:06:09.47,0:06:12.33,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu đừng hỏi tôi những điều ngu ngốc như vậy được không!
Dialogue: 0,0:06:15.18,0:06:17.67,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nó đang chà xát vô lưỡi em.
Dialogue: 0,0:06:17.67,0:06:19.31,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không!
Dialogue: 0,0:06:20.09,0:06:23.08,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Bên trong, lưỡi cậu ta đang ngọ nguậy bên trong mình.{\i0}
Dialogue: 0,0:06:23.77,0:06:26.19,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Nếu cậu liếm chỗ đó...{\i0}
Dialogue: 0,0:06:33.25,0:06:36.81,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Khoan đã, tôi muốn nghỉ một chút
Dialogue: 0,0:06:36.81,0:06:40.41,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không được, trò vui mới chỉ bắt đầu thôi mà.
Dialogue: 0,0:06:40.41,0:06:44.18,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Cậu ta thừa biết lúc này mình đang rất xấu hổ...{\i0}
Dialogue: 0,0:06:42.13,0:06:44.64,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}Em yêu chị... Em yêu chị, Senpai!
Dialogue: 0,0:06:45.38,0:06:47.88,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không, đừng làm vậy mà!
Dialogue: 0,0:06:48.61,0:06:52.24,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Trả lời em đi, Senpai, chị có yêu em không?
Dialogue: 0,0:06:52.24,0:06:54.12,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tại sao tôi phải trả lời câu hỏi ngớ ngẩn đó?
Dialogue: 0,0:06:54.12,0:06:57.05,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nếu không thì em sẽ đâm nó vào bên trong chị.
Dialogue: 0,0:06:57.05,0:06:59.96,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không... Khoan đã, nếu cậu đút vào lúc này...
Dialogue: 0,0:07:00.35,0:07:03.14,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Gương mặt thẹn thùng của chị thật dễ thương, Senpai.
Dialogue: 0,0:07:03.14,0:07:05.46,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hôm nay tôi hơi khó chịu.
Dialogue: 0,0:07:05.46,0:07:06.89,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không, khoan đã...
Dialogue: 0,0:07:07.79,0:07:10.39,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ở bên trong chị thật ấm áp...
Dialogue: 0,0:07:10.39,0:07:13.01,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không, nếu cậu đút vào lúc này
Dialogue: 0,0:07:13.98,0:07:15.85,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không! Tôi sắp...
Dialogue: 0,0:07:19.95,0:07:21.99,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chị khoái di chuyển nhịp nhàng như vậy phải không?
Dialogue: 0,0:07:21.99,0:07:24.75,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nói thật xem nào.
Dialogue: 0,0:07:24.84,0:07:29.83,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Dù cậu... có nói gì... chăng nữa...
Dialogue: 0,0:07:30.04,0:07:32.04,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Khó thở chết mất.{\i0}
Dialogue: 0,0:07:32.43,0:07:35.20,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vào đây phải không? Em biết ngay mà.
Dialogue: 0,0:07:35.20,0:07:37.81,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Làm "Đồ chơi" quanh năm nó phải khác chứ.
Dialogue: 0,0:07:37.81,0:07:39.32,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không!
Dialogue: 0,0:07:39.32,0:07:41.89,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không!
Dialogue: 0,0:07:44.50,0:07:48.67,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Lúc này tôi vẫn chưa xác định mình có thực lòng yêu cậu hay không,
Dialogue: 0,0:07:48.67,0:07:53.49,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tôi chỉ biết rằng mình không thể làm tình với ai khác ngoài cậu.
Dialogue: 0,0:07:53.49,0:07:59.20,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vậy nên... làm ơn cho tôi nghỉ chút...
Dialogue: 0,0:08:03.04,0:08:06.02,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hạnh phúc quá, Senpai!
Dialogue: 0,0:08:06.02,0:08:09.12,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Khoan đã, dừng lại ngay!
Dialogue: 0,0:08:09.61,0:08:14.44,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Xin lỗi, em sướng tới mức không thể ngừng lại được.
Dialogue: 0,0:08:16.13,0:08:17.54,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ngon tuyệt.
Dialogue: 0,0:08:17.54,0:08:19.14,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không, nếu cậu làm vậy
Dialogue: 0,0:08:20.27,0:08:27.14,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đừng chạm vào chúng! Đừng vừa chơi vừa bú 2 đầu ti tôi như vậy!
Dialogue: 0,0:08:29.73,0:08:35.63,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Buông tôi ra... dừng ở đây được rồi...
Dialogue: 0,0:08:38.97,0:08:41.98,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Bên trong, em sẽ bắn vô bên trong.
Dialogue: 0,0:08:41.98,0:08:43.25,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không!
Dialogue: 0,0:08:43.25,0:08:47.97,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em sẽ bắn hết vào trong Senpai.
Dialogue: 0,0:08:47.97,0:08:51.37,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không... Không... Tôi sắp ra rồi!
Dialogue: 0,0:08:51.37,0:08:53.63,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Sướng quá đi mất...
Dialogue: 0,0:08:53.63,0:08:54.39,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không.
Dialogue: 0,0:08:56.56,0:09:00.30,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không.
Dialogue: 0,0:09:02.20,0:09:08.08,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu bắn ra nhiều quá, tử cung tôi đầy ắp rồi...
Dialogue: 0,0:09:08.08,0:09:12.08,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Mới chơi chút mà đã phun ra như suối thế kia...
Dialogue: 0,0:09:12.08,0:09:14.07,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu đúng là đồ biến thái!?
Dialogue: 0,0:09:14.07,0:09:15.51,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Senpai...
Dialogue: 0,0:09:15.76,0:09:17.68,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Chà, mình yêu cậu ta mất rồi.{\i0}
Dialogue: 0,0:09:22.85,0:09:26.48,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hai đứa nó đẹp đôi đấy chứ, mà Shigure cũng khoái cậu ta.
Dialogue: 0,0:09:26.48,0:09:28.80,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không hẳn là vậy...
Dialogue: 0,0:09:28.80,0:09:30.72,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Anh không thấy mình giống cậu ta ư?
Dialogue: 0,0:09:30.72,0:09:37.19,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hồi cầu hôn em, anh chả khóc như mưa còn gì, bố vợ-con rể hợp cạ quá đi chứ.
Dialogue: 0,0:09:37.39,0:09:38.42,HHH Triple Ecchi Sign,,0000,0000,0000,,{\bord0\p1\pos(693,436)\fscx646.25\fscy510}m 5 -12 l -16 -12 l -16 3 l 5 3
Dialogue: 0,0:09:37.39,0:09:38.42,HHH Triple Ecchi Sign,,0000,0000,0000,,{\fad(50,30)\b1\4c&HFFFFFF&\3c&HEFF4F4&\c&H000000&\fs22\fnUVN Nguyen Du\pos(591,355)}Đã chọn xong \Nthằng con rể
Dialogue: 0,0:09:39.15,0:09:43.97,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Sau khi chứng kiến hai đứa nó làm chuyện kia, đột nhiên em lại có ham muốn...
Dialogue: 0,0:09:46.50,0:09:48.85,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ngày mai anh còn có một cuộc họp rất quan trọng...
Dialogue: 0,0:09:48.85,0:09:53.04,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không được, lâu rồi anh chưa làm "Đồ chơi" cho em.
Dialogue: 0,0:09:53.78,0:09:55.17,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ấy... Ấy đừng...
Dialogue: 0,0:10:03.98,0:10:06.32,HHH Triple Ecchi Sign,,0000,0000,0000,,{\fs40\bord0\fad(400,0)\i1\c&H000000&\4c&HFFFFFF&\3c&HFFFFFF&\b1\fnUVN Chim Bien Nang\pos(351,66)}Thời của mình đã tới
Dialogue: 0,0:10:09.27,0:10:11.25,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nè, thấy anh giai đằng kia không?
Dialogue: 0,0:10:11.36,0:10:14.30,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Dễ thương đấy chứ, y như một chú sóc con.
Dialogue: 0,0:10:14.52,0:10:18.03,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hừm... Ngầu một chút thì tốt, dễ thương thôi thì chưa đủ.
Dialogue: 0,0:10:19.02,0:10:21.47,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Rất hân hạnh được gặp mặt các quý cô...
Dialogue: 0,0:10:21.47,0:10:23.02,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chào anh giai!
Dialogue: 0,0:10:24.45,0:10:26.09,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tới giờ rồi nhỉ?
Dialogue: 0,0:10:26.09,0:10:27.84,HHH Triple Ecchi Dialogue,,0000,0000,0000,,S-Senpai?
Dialogue: 0,0:10:28.14,0:10:30.67,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không nhanh lên là muộn giờ xem phim đó.
Dialogue: 0,0:10:30.67,0:10:33.17,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hóa ra nãy giờ chị đứng đó à?!
Dialogue: 0,0:10:33.62,0:10:34.88,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thôi nào, mau đi thôi!
Dialogue: 0,0:10:35.16,0:10:37.40,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em xin lỗi, Senpai!
Dialogue: 0,0:10:37.40,0:10:38.87,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu xin lỗi làm chi?
Dialogue: 0,0:10:40.09,0:10:42.89,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tôi đâu có cấm cậu "hân hạnh làm quen" mấy em đó.
Dialogue: 0,0:10:43.65,0:10:48.36,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không có cậu thì vẫn còn khối kẻ theo đuôi tôi, nhỉ?
Dialogue: 0,0:10:48.60,0:10:50.16,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không... Không phải như chị... nghĩ...
Dialogue: 0,0:10:51.94,0:10:55.45,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Muốn làm gì thì làm, gã đào hoa!
Dialogue: 0,0:10:55.45,0:10:58.08,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tôi cóc cần phải hẹn hò với cậu nữa.
Dialogue: 0,0:10:58.67,0:11:01.59,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Cơ mà, sao mình lại cảm thấy tức tối như vậy...{\i0}
Dialogue: 0,0:11:03.59,0:11:04.56,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\i1}Ghen tị!?{\i0}
Dialogue: 0,0:11:04.85,0:11:06.72,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không thể nào!
Dialogue: 0,0:11:06.72,0:11:09.02,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Điều đó không thể xảy ra với mình!
Dialogue: 0,0:11:10.22,0:11:13.00,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hừm... những lúc như thế này.
Dialogue: 0,0:11:26.60,0:11:28.75,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hừ... Mếu máo cái gì.
Dialogue: 0,0:11:29.50,0:11:30.93,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cậu là đồ dê cụ.
Dialogue: 0,0:11:37.43,0:11:40.73,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Âm hộ của mình ướt đẫm rồi.
Dialogue: 0,0:11:41.10,0:11:43.75,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Lần trước cậu đã nói yêu tôi...
Dialogue: 0,0:11:44.43,0:11:47.29,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Giờ thì đừng hòng nuốt lời nhé.
Dialogue: 0,0:11:47.29,0:11:51.25,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tôi cho cậu tha hồ tự sướng luôn, đồ sóc dê cụ.
Dialogue: 0,0:11:52.14,0:11:54.73,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Shigure, Kita Komatsu-kun tới tìm con nè.
Dialogue: 0,0:11:57.55,0:11:58.65,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Senpai!
Dialogue: 0,0:11:58.65,0:12:02.48,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em biết chắc là chị sẽ thủ dâm và nghĩ tới em mà.
Dialogue: 0,0:12:02.48,0:12:04.28,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Khoan đã...
Dialogue: 0,0:12:04.28,0:12:06.28,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hai đứa cứ tự nhiên nhé, mẹ đi trước.
Dialogue: 0,0:12:06.28,0:12:07.53,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Mẹ ơi?!
Dialogue: 0,0:12:07.86,0:12:11.76,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không ngờ Senpai lại nhớ tới em...
Dialogue: 0,0:12:11.76,0:12:13.87,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Khoan đã, nếu cậu đâm vào lúc này
Dialogue: 0,0:12:13.87,0:12:17.46,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em sẽ cày cuốc cho tới khi tử cung chị nghiện mới thôi.
Dialogue: 0,0:12:17.46,0:12:18.37,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đừng mà.
Dialogue: 0,0:12:20.45,0:12:22.26,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không!
Dialogue: 0,0:12:22.26,0:12:27.25,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chị đỏ mặt như vậy trông dễ thương lắm, Senpai.
Dialogue: 0,0:12:30.57,0:12:31.79,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không!
Dialogue: 0,0:12:40.12,0:12:42.47,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Sao chị không nói yêu em như trước kia?
Dialogue: 0,0:12:43.09,0:12:44.85,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nè, mau nói yêu em đi chứ.
Dialogue: 0,0:12:46.19,0:12:48.82,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nói yêu em như chị vẫn thường làm!
Dialogue: 0,0:12:49.51,0:12:50.97,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nào... Nào...
Dialogue: 0,0:12:50.00,0:12:53.58,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}{\i1}Hừ... bố trẻ thánh tướng ghê nhỉ.{\i0}
Dialogue: 0,0:12:54.17,0:12:55.54,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thằng nào con nào...
Dialogue: 0,0:12:56.18,0:12:58.34,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thằng nào con nào nói yêu cậu!?
Dialogue: 0,0:12:58.34,0:13:02.75,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Lần này đừng trách chị độc ác, Kita Komatsu-kun!
Dialogue: 0,0:13:03.66,0:13:05.70,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Senpai, chị đáng sợ quá!
Dialogue: 0,0:13:06.96,0:13:09.47,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không quỳ xuống van xin thì đừng hòng chị tha.
Dialogue: 0,0:13:09.80,0:13:12.59,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Coi nào... Coi nào... Coi nào!
Dialogue: 0,0:13:14.03,0:13:16.22,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thằng nhỏ của cậu đang co giật bên trong chị,
Dialogue: 0,0:13:16.22,0:13:18.32,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không nhịn nổi nữa hả!?
Dialogue: 0,0:13:18.87,0:13:21.37,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chị sẽ bắt cậu trả giá thích đáng!
Dialogue: 0,0:13:22.21,0:13:25.40,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không, em sắp ra rồi!
Dialogue: 0,0:13:25.40,0:13:27.70,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không sao, cứ bắn ra đi.
Dialogue: 0,0:13:27.70,0:13:30.95,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nhưng hôm nay là tới chu kì của Senpai...
Dialogue: 0,0:13:31.69,0:13:34.18,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Sao cậu biết điều này?!
Dialogue: 0,0:13:34.18,0:13:38.80,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em luôn kiểm tra cẩn thận chu kì ghi trong sổ.
Dialogue: 0,0:13:38.80,0:13:43.83,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hả? Cái gì cơ? Cậu mắc chứng bại não à!
Dialogue: 0,0:13:45.93,0:13:48.38,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Không được, mau rút nó ra!
Dialogue: 0,0:13:48.78,0:13:51.88,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Trong chu kì thì sao chứ, còn lâu tôi mới rút ra nhé.
Dialogue: 0,0:13:52.15,0:13:53.84,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Sướng quá đi mất thôi...
Dialogue: 0,0:14:02.32,0:14:03.88,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:14:05.63,0:14:07.07,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:14:07.07,0:14:10.44,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nó đang tràn vào bên trong tử cung!
Dialogue: 0,0:14:18.84,0:14:20.38,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\b1}Xông lên anh em!{\b0}
Dialogue: 0,0:14:20.95,0:14:23.73,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\b1}Yay... yay, xông lên!{\b0}
Dialogue: 0,0:14:23.73,0:14:26.67,HHH Triple Ecchi Dialogue Alt.,,0000,0000,0000,,{\b1}Được thôi, tao sẽ dẫn đầu!{\b0}
Dialogue: 0,0:16:33.52,0:16:34.97,HHH Triple Ecchi Sign,,0000,0000,0000,,{\fad(450,25)\4c&HFFFFFF&\3c&HFFFFFF&\shad0\b1\c&H19151B&\fs22\pos(343,226)}Một năm sau
Dialogue: 0,0:16:35.20,0:16:37.63,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Thế Shigure chạy đi đâu rồi?
Dialogue: 0,0:16:37.63,0:16:40.65,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Con bé đang nhốt chồng bên trong phòng.
Dialogue: 0,0:16:40.65,0:16:45.10,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chắc là thèm quá hóa rồ đây mà, cũng lâu rồi hai đưa nó chưa quan hệ.
Dialogue: 0,0:16:45.10,0:16:48.29,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cứ tình hình này, chúng ta sẽ tha hồ bế cháu ngoại.
Dialogue: 0,0:16:48.29,0:16:51.63,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Nè... sữa của em đang trào ra kìa.
Dialogue: 0,0:16:51.63,0:16:53.47,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Giúp em bú bớt đi chứ?
Dialogue: 0,0:16:53.47,0:16:54.64,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Vâng!
Dialogue: 0,0:16:57.59,0:16:59.62,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Lâu rồi không gặp chú mày, dương vật.
Dialogue: 0,0:16:59.62,0:17:01.39,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Chiều chị đi nhá!
Dialogue: 0,0:17:02.43,0:17:04.06,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Đây là...!
Dialogue: 0,0:17:04.06,0:17:07.51,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cảm giác dương vật chà xát bên trong thật tuyệt!
Dialogue: 0,0:17:10.27,0:17:12.38,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Cơ thể em không kiềm chế nổi nữa.
Dialogue: 0,0:17:12.38,0:17:15.35,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Em sẽ khiến anh bắn hết vô bên trong.
Dialogue: 0,0:17:15.85,0:17:19.47,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Hi vọng hôm nay sẽ có kỉ lục mới, gắng lên nha.
Dialogue: 0,0:17:19.47,0:17:21.11,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ông xã.
Dialogue: 0,0:17:21.11,0:17:22.85,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Tuân lệnh...
Dialogue: 0,0:17:23.34,0:17:25.77,HHH Triple Ecchi Dialogue,,0000,0000,0000,,Ấy không!
Dialogue: 0,0:17:24.81,0:17:26.49,HHH Triple Ecchi Dialogue Alt.2,,0000,0000,0000,,{\an8}{\i0}Không cái gì mà không!{\i1}
