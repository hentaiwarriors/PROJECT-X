Dialogue: 0,0:00:01.92,0:00:06.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:07.77,0:00:10.63,Default,,0000,0000,0000,,Ko, làm ơn hãy dừng lại.
Dialogue: 0,0:00:12.37,0:00:17.71,Default,,0000,0000,0000,,Muộn rồi. Cái đó của cô hẳn đang thầm khóc lóc van xin điều đó.
Dialogue: 0,0:00:18.28,0:00:20.18,Default,,0000,0000,0000,,Ko phải như thế.
Dialogue: 0,0:00:20.31,0:00:24.48,Default,,0000,0000,0000,,Ta đã nói sai sao?\NCơ thể cô đang dần thành thực hơn rồi đó.
Dialogue: 0,0:00:24.85,0:00:26.82,Default,,0000,0000,0000,,Ko, làm ơn đừng.
Dialogue: 0,0:00:32.72,0:00:34.09,Default,,0000,0000,0000,,<i>Con quái vật này.</i>
Dialogue: 0,0:00:36.46,0:00:37.98,Default,,0000,0000,0000,,Gặp lại hai người sau nhé.
Dialogue: 0,0:00:42.87,0:00:44.96,Default,,0000,0000,0000,,Mẹ, đừng khóc nữa.
Dialogue: 0,0:00:45.74,0:00:48.47,Default,,0000,0000,0000,,Rumi, chỉ vì mẹ mà con đã bị cuốn vào chuyện này.
Dialogue: 0,0:00:49.54,0:00:52.30,Default,,0000,0000,0000,,Ko sao đâu mẹ. Đừng quá bi quan như thế.
Dialogue: 0,0:00:53.11,0:00:56.60,Default,,0000,0000,0000,,Mẹ ko biết mình phải làm gì bây giờ nữa.
Dialogue: 0,0:01:01.15,0:01:02.55,Default,,0000,0000,0000,,Mẹ.
Dialogue: 0,0:01:08.03,0:01:11.55,Default,,0000,0000,0000,,Thấy chưa? Cô ko đủ sức để kháng cự lại ta đâu.
Dialogue: 0,0:01:13.00,0:01:16.06,Default,,0000,0000,0000,,Đã đến lúc cô hiến dâng trinh tiết của mình cho ta rồi đó!
Dialogue: 0,0:01:17.00,0:01:20.99,Default,,0000,0000,0000,,Ko, làm ơn đừng nhét thứ cứng như vậy vào bên trong tôi!
Dialogue: 0,0:01:22.77,0:01:28.21,Default,,0000,0000,0000,,Nhìn xem này! "Động đào" của cô như muốn nuốt chửng cái của ta vào bên trong vậy!
Dialogue: 0,0:01:28.91,0:01:30.40,Default,,0000,0000,0000,,Ko, ko.
Dialogue: 0,0:01:32.15,0:01:33.74,Default,,0000,0000,0000,,Ngoan ngoãn đi nào.
Dialogue: 0,0:01:34.25,0:01:36.45,Default,,0000,0000,0000,,Đây chính là cơ hội để cô trở thành phụ nữ thực thụ đó.
Dialogue: 0,0:01:36.52,0:01:38.11,Default,,0000,0000,0000,,Yumi nói đúng đó.
Dialogue: 0,0:01:38.32,0:01:42.62,Default,,0000,0000,0000,,Ta sẽ giúp cô trở thành đàn bà thực thụ! Ta vào đây!
Dialogue: 0,0:01:45.93,0:01:49.96,Default,,0000,0000,0000,,Được rồi. Ta sẽ nhét hết nó vào bên trong.
Dialogue: 0,0:01:59.91,0:02:03.68,Default,,0000,0000,0000,,Đau quá! Đau quá! Dừng lại đi, làm ơn!
Dialogue: 0,0:02:08.82,0:02:11.45,Default,,0000,0000,0000,,Cảm giác mất đi sự trinh tiết thế nào?
Dialogue: 0,0:02:11.52,0:02:15.15,Default,,0000,0000,0000,,Đau lắm hả? Đau đến phát khóc đúng ko?
Dialogue: 0,0:02:17.40,0:02:19.83,Default,,0000,0000,0000,,<i>Chị ơi. Mẹ ơi.</i>
Dialogue: 0,0:02:34.98,0:02:37.57,Default,,0000,0000,0000,,Immoral Sisters\N~Đôi quả ngọt chín sớm~
Dialogue: 0,0:02:38.85,0:02:41.91,Default,,0000,0000,0000,,Đêm thứ ba\N"Đắm chìm trong tình yêu và khoái cảm"
Dialogue: 0,0:02:43.49,0:02:45.72,Default,,0000,0000,0000,,Phần A:\NTình cảm của Rumi và Tomoko
Dialogue: 0,0:02:52.70,0:02:54.60,Default,,0000,0000,0000,,Anh vẫn còn ở đây hả, Taketo?
Dialogue: 0,0:03:00.91,0:03:02.77,Default,,0000,0000,0000,,Sao ông lại ở đây?
Dialogue: 0,0:03:06.04,0:03:11.24,Default,,0000,0000,0000,,Một ngôi nhà toàn phụ nữ có vẻ thật an toàn, nhưng lại tồn tai những lỗ hổng ở khắp mọi nơi.
Dialogue: 0,0:03:15.89,0:03:18.91,Default,,0000,0000,0000,,Ko lẽ nào. Tomoko!
Dialogue: 0,0:03:21.96,0:03:23.32,Default,,0000,0000,0000,,Tomoko!
Dialogue: 0,0:03:35.24,0:03:37.64,Default,,0000,0000,0000,,Tomoko. Tại sao?
Dialogue: 0,0:03:38.41,0:03:43.35,Default,,0000,0000,0000,,Cô ấy đã yêu cầu điều đó. Cô ấy nói thật bất công nếu cô ấy phải đứng ngoài chuyện này.
Dialogue: 0,0:03:46.15,0:03:47.67,Default,,0000,0000,0000,,Tomoko...
Dialogue: 0,0:03:54.39,0:03:57.73,Default,,0000,0000,0000,,<i>Mình là một người mẹ vô trách nhiệm.</i>
Dialogue: 0,0:04:03.17,0:04:07.19,Default,,0000,0000,0000,,Đau lắm phải ko? Chị xin lỗi vì đã ko bảo vệ được em.
Dialogue: 0,0:04:08.14,0:04:11.34,Default,,0000,0000,0000,,Chị, có phải em đã trở nên xinh đẹp hơn?
Dialogue: 0,0:04:11.71,0:04:12.97,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:04:13.04,0:04:16.60,Default,,0000,0000,0000,,Làm chuyện đó với đàn ông cũng ko tệ lắm, phải ko?
Dialogue: 0,0:04:17.15,0:04:20.31,Default,,0000,0000,0000,,Chị có vẻ rất thích thú khi làm chuyện đó.
Dialogue: 0,0:04:20.58,0:04:22.92,Default,,0000,0000,0000,,Em đang nói gì vậy?
Dialogue: 0,0:04:23.15,0:04:26.09,Default,,0000,0000,0000,,Chuyện đó rất đau, đến mức khiến em gần như ngạt thở, nhưng...
Dialogue: 0,0:04:26.22,0:04:28.82,Default,,0000,0000,0000,,Nhưng chuyện đó thực sự rất tuyệt, phải ko chị?
Dialogue: 0,0:04:30.83,0:04:32.23,Default,,0000,0000,0000,,Tomoko.
Dialogue: 0,0:04:32.36,0:04:35.86,Default,,0000,0000,0000,,Chị Rumi, ngủ cùng em đêm nay nhé!
Dialogue: 0,0:04:45.71,0:04:47.90,Default,,0000,0000,0000,,Tomoko, đừng sợ.
Dialogue: 0,0:04:48.31,0:04:50.68,Default,,0000,0000,0000,,Chị sẽ luôn ở bên cạnh em đêm nay.
Dialogue: 0,0:04:50.75,0:04:51.51,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:04:51.58,0:04:55.11,Default,,0000,0000,0000,,Hãy quên chuyện hôm nay đi. Xem nó chỉ như một cơn ác mộng.
Dialogue: 0,0:04:57.42,0:04:59.51,Default,,0000,0000,0000,,- Chị Rumi?\N- Gì thế?
Dialogue: 0,0:05:00.22,0:05:03.13,Default,,0000,0000,0000,,Em cô đơn quá.
Dialogue: 0,0:05:03.63,0:05:07.29,Default,,0000,0000,0000,,Chị và mẹ đã phải chịu đựng chuyện đó rất lâu.
Dialogue: 0,0:05:07.36,0:05:11.06,Default,,0000,0000,0000,,Vậy mà em lại ko hề hay biết gì cả. Em thật là vô tâm.
Dialogue: 0,0:05:11.20,0:05:14.23,Default,,0000,0000,0000,,Ko, tất cả là lỗi của chị.
Dialogue: 0,0:05:14.54,0:05:18.77,Default,,0000,0000,0000,,Chị ko muốn em lo lắng, vì thế chị đã ko nói gì với em cả.
Dialogue: 0,0:05:19.44,0:05:22.61,Default,,0000,0000,0000,,Cũng chính vì thế, chị đã ko bảo vệ được em...
Dialogue: 0,0:05:22.68,0:05:24.81,Default,,0000,0000,0000,,Đó ko phải lỗi của chị mà!
Dialogue: 0,0:05:25.08,0:05:27.61,Default,,0000,0000,0000,,Chị ko làm gì sai hết.
Dialogue: 0,0:05:29.19,0:05:31.09,Default,,0000,0000,0000,,Cám ơn em, Tomoko.
Dialogue: 0,0:05:31.69,0:05:34.82,Default,,0000,0000,0000,,Em thấy cơ thể mình đang nóng ran lên.
Dialogue: 0,0:05:35.19,0:05:36.59,Default,,0000,0000,0000,,Tomoko?
Dialogue: 0,0:05:36.79,0:05:42.49,Default,,0000,0000,0000,,Cơ thể em đang rạo rực, giống như thứ đó vẫn còn ở bên trong em.
Dialogue: 0,0:05:42.57,0:05:44.56,Default,,0000,0000,0000,,Em nói gì vậy, Tomoko?
Dialogue: 0,0:05:44.64,0:05:49.47,Default,,0000,0000,0000,,Khi chị làm điều đó với đàn ông, chị cũng có cảm giác như vậy, phải ko?
Dialogue: 0,0:05:50.34,0:05:53.04,Default,,0000,0000,0000,,Đừng nói về những điều đó nữa, ngủ đi em!
Dialogue: 0,0:05:53.64,0:05:55.41,Default,,0000,0000,0000,,Kể cho em đi, chị Rumi!
Dialogue: 0,0:05:55.48,0:05:56.88,Default,,0000,0000,0000,,Tomoko!
Dialogue: 0,0:05:58.12,0:06:01.92,Default,,0000,0000,0000,,Ngực của em đã bị chạm vào như thế này hôm nay.
Dialogue: 0,0:06:02.05,0:06:05.68,Default,,0000,0000,0000,,Dừng lại đi, Tomoko! Em làm gì vậy?
Dialogue: 0,0:06:06.99,0:06:10.26,Default,,0000,0000,0000,,Hay đầu ti của chị đang cứng lên nè.
Dialogue: 0,0:06:17.74,0:06:18.86,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:06:18.94,0:06:22.67,Default,,0000,0000,0000,,Chị cũng rất nhạy cảm, giống như em.
Dialogue: 0,0:06:23.17,0:06:25.04,Default,,0000,0000,0000,,Ko phải như vậy.
Dialogue: 0,0:06:26.64,0:06:28.67,Default,,0000,0000,0000,,Em luồn tay vào đâu vậy?
Dialogue: 0,0:06:28.75,0:06:31.34,Default,,0000,0000,0000,,A, y như Yumi đã nói.
Dialogue: 0,0:06:31.48,0:06:33.88,Default,,0000,0000,0000,,Chỗ này của chị ướt thật đấy.
Dialogue: 0,0:06:34.89,0:06:38.51,Default,,0000,0000,0000,,Tomoko, đừng có luồn ngón tay em vào!
Dialogue: 0,0:06:42.23,0:06:45.96,Default,,0000,0000,0000,,<i>Mình chỉ là mẹ kế,</i>\N<i>nhưng mẹ kế cũng là mẹ.</i>
Dialogue: 0,0:06:47.40,0:06:50.89,Default,,0000,0000,0000,,<i>Hai chị em nó</i>\N<i>chắc hẳn còn đau đớn hơn mình.</i>
Dialogue: 0,0:06:51.54,0:06:53.73,Default,,0000,0000,0000,,<i>Mình phải tới an ủi chúng.</i>
Dialogue: 0,0:07:13.22,0:07:16.39,Default,,0000,0000,0000,,Tuyệt quá. Chị càng lúc càng ướt nè.
Dialogue: 0,0:07:17.66,0:07:19.39,Default,,0000,0000,0000,,Đừng, Tomoko.
Dialogue: 0,0:07:19.53,0:07:22.59,Default,,0000,0000,0000,,Nếu em cứ nói như vậy, chị sẽ phát điên lên mất.
Dialogue: 0,0:07:22.67,0:07:27.13,Default,,0000,0000,0000,,Em đã nhiều lần nhìn trộm chị làm chuyện này, vì thế em biết rất rõ.
Dialogue: 0,0:07:27.20,0:07:30.61,Default,,0000,0000,0000,,Chị rất nhạy cảm ở chỗ này, phải ko?
Dialogue: 0,0:07:31.51,0:07:32.91,Default,,0000,0000,0000,,Tomoko.
Dialogue: 0,0:07:41.55,0:07:42.95,Default,,0000,0000,0000,,Hai chị em nó...
Dialogue: 0,0:07:43.02,0:07:45.45,Default,,0000,0000,0000,,Này, sao cô ko vào trong với họ đi?
Dialogue: 0,0:07:45.96,0:07:47.29,Default,,0000,0000,0000,,Là cô ư!
Dialogue: 0,0:07:47.49,0:07:50.39,Default,,0000,0000,0000,,Vào trong và ôm lấy họ đi.
Dialogue: 0,0:07:52.00,0:07:53.26,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:07:53.36,0:07:55.96,Default,,0000,0000,0000,,Cô là mẹ bọn họ mà, phải ko nào?
Dialogue: 0,0:08:30.20,0:08:31.57,Default,,0000,0000,0000,,Mẹ?
Dialogue: 0,0:08:32.74,0:08:36.90,Default,,0000,0000,0000,,Rumi, Tomoko, mẹ là một người mẹ tồi.
Dialogue: 0,0:08:41.95,0:08:43.41,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:08:43.48,0:08:46.42,Default,,0000,0000,0000,,Làm ơn đừng khóc nữa. Đó ko phải lỗi của mẹ.
Dialogue: 0,0:08:46.62,0:08:50.42,Default,,0000,0000,0000,,Sao chúng ta ko ngủ chung giường với nhau đêm nay nhỉ?
Dialogue: 0,0:08:50.69,0:08:51.95,Default,,0000,0000,0000,,Tomoko.
Dialogue: 0,0:08:52.02,0:08:56.55,Default,,0000,0000,0000,,Đúng thế, mẹ à. Hãy quên hết những chuyện tồi tệ đó đi.
Dialogue: 0,0:08:57.39,0:08:59.26,Default,,0000,0000,0000,,Cám ơn con, Rumi.
Dialogue: 0,0:09:01.53,0:09:03.73,Default,,0000,0000,0000,,Một gia đình hạnh phúc.
Dialogue: 0,0:09:05.07,0:09:07.87,Default,,0000,0000,0000,,Là cô à! Cô còn dám đến đây nữa sao?
Dialogue: 0,0:09:08.21,0:09:13.37,Default,,0000,0000,0000,,Liệu tôi có thể chung vui với mọi người được ko?
Dialogue: 0,0:09:19.92,0:09:22.58,Default,,0000,0000,0000,,Tôi cũng giống như cô.
Dialogue: 0,0:09:23.72,0:09:26.45,Default,,0000,0000,0000,,Trinh tiết của tôi đã bị ông ta chiếm đoạt.
Dialogue: 0,0:09:28.29,0:09:32.09,Default,,0000,0000,0000,,Khi chứng kiến cô như vậy đã gợi lại cho tôi kỉ niệm tồi tệ đó.
Dialogue: 0,0:09:32.86,0:09:35.33,Default,,0000,0000,0000,,Hiện giờ tôi đang rất thành thực với ba người đó.
Dialogue: 0,0:09:36.10,0:09:39.23,Default,,0000,0000,0000,,Làm ơn. Tôi đã chán phải làm chuyện đó một mình lắm rồi.
Dialogue: 0,0:09:39.30,0:09:42.00,Default,,0000,0000,0000,,Tomoko, đừng dễ dàng tin!cô ta như vậy
Dialogue: 0,0:09:42.44,0:09:44.81,Default,,0000,0000,0000,,Chính cô ta đã đe dọa chúng ta...
Dialogue: 0,0:09:44.88,0:09:46.74,Default,,0000,0000,0000,,Ko sao đâu mà.
Dialogue: 0,0:09:47.24,0:09:48.37,Default,,0000,0000,0000,,Tomoko?
Dialogue: 0,0:09:48.45,0:09:51.04,Default,,0000,0000,0000,,Mọi việc vẫn ổn mà, phải ko, chị?
Dialogue: 0,0:10:16.54,0:10:20.91,Default,,0000,0000,0000,,Biết làm sao được, hai con đã trở thành phụ nữ thực thụ rồi.
Dialogue: 0,0:10:22.31,0:10:27.27,Default,,0000,0000,0000,,Con rất vui khi nghe mẹ nói thế.
Dialogue: 0,0:10:27.89,0:10:32.22,Default,,0000,0000,0000,,Chúng ta sẽ tốt nghiệp cùng nhau vào năm tới, phải ko chị.
Dialogue: 0,0:10:32.89,0:10:34.41,Default,,0000,0000,0000,,Tomoko...
Dialogue: 0,0:10:35.33,0:10:40.56,Default,,0000,0000,0000,,Sau khi chúng ta tốt nghiệp, chúng ta sẽ vui vẻ cùng nhau nhiều hơn, phải ko chị.
Dialogue: 0,0:10:41.97,0:10:46.27,Default,,0000,0000,0000,,Hai người quý nhau thật.\NY như cả hai là chị em ruột vậy.
Dialogue: 0,0:10:49.94,0:10:51.93,Default,,0000,0000,0000,,Cô bị kích thích quá nhỉ.
Dialogue: 0,0:10:52.41,0:10:56.14,Default,,0000,0000,0000,,Chồng cô ko thể khiến cô thỏa mãn , phải ko?
Dialogue: 0,0:11:00.65,0:11:03.92,Default,,0000,0000,0000,,Tomoko, em làm gì đó?
Dialogue: 0,0:11:05.12,0:11:07.22,Default,,0000,0000,0000,,Chị có thích như vậy ko?
Dialogue: 0,0:11:09.86,0:11:11.99,Default,,0000,0000,0000,,Chưa thấy nó bao giờ hả?
Dialogue: 0,0:11:13.36,0:11:15.26,Default,,0000,0000,0000,,Thưởng thức nó đi, từ từ thôi.
Dialogue: 0,0:11:18.20,0:11:19.83,Default,,0000,0000,0000,,Thật ko thể tin được.
Dialogue: 0,0:11:26.58,0:11:28.77,Default,,0000,0000,0000,,Mùi vị rất tuyệt, phải ko?
Dialogue: 0,0:11:36.29,0:11:40.19,Default,,0000,0000,0000,,Em sẽ dùng hai ngón tay của mình!\NSướng ko chị?
Dialogue: 0,0:11:40.46,0:11:42.36,Default,,0000,0000,0000,,Chị ướt đẫm rồi này.
Dialogue: 0,0:11:42.63,0:11:44.49,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:11:46.06,0:11:48.96,Default,,0000,0000,0000,,Nó vào trong rồi! Thứ đó đang vào sâu bên trong tôi!
Dialogue: 0,0:11:49.03,0:11:51.76,Default,,0000,0000,0000,,Thế này liệu có quá bất công với chồng cô.
Dialogue: 0,0:11:52.54,0:11:54.13,Default,,0000,0000,0000,,Đâm nó vào trong đi!
Dialogue: 0,0:11:57.34,0:12:00.80,Default,,0000,0000,0000,,Vậy là cô thích nó hơn hẳn cậu bé của chồng cô?
Dialogue: 0,0:12:02.21,0:12:04.34,Default,,0000,0000,0000,,Cô thích nó, phải ko?
Dialogue: 0,0:12:05.12,0:12:06.71,Default,,0000,0000,0000,,Tôi thích lắm!
Dialogue: 0,0:12:08.72,0:12:10.78,Default,,0000,0000,0000,,Chị, nữa đi!
Dialogue: 0,0:13:46.58,0:13:47.98,Default,,0000,0000,0000,,Yumi.
Dialogue: 0,0:13:48.05,0:13:49.85,Default,,0000,0000,0000,,Xin lỗi. Tôi làm cô thức giấc hả?
Dialogue: 0,0:13:50.15,0:13:52.25,Default,,0000,0000,0000,,Cô đi đâu thế?
Dialogue: 0,0:13:52.96,0:13:54.42,Default,,0000,0000,0000,,Tôi có việc phải đi bây giờ.
Dialogue: 0,0:13:54.59,0:13:56.06,Default,,0000,0000,0000,,Vào giờ này ư?
Dialogue: 0,0:13:56.43,0:14:01.06,Default,,0000,0000,0000,,Đúng vậy. Tôi cần phải chuẩn bị chu đáo kế hoach trả thù của mình.
Dialogue: 0,0:14:21.55,0:14:26.96,Default,,0000,0000,0000,,Đau quá, đau quá! Dừng lại đi, làm ơn.
Dialogue: 0,0:14:28.83,0:14:32.19,Default,,0000,0000,0000,,Cảm giác mất đi sự trinh tiết thế nào?
Dialogue: 0,0:14:32.26,0:14:33.96,Default,,0000,0000,0000,,Đau lắm hả? Đau đến phát khóc đúng ko?
Dialogue: 0,0:14:34.03,0:14:35.40,Default,,0000,0000,0000,,Ông già.
Dialogue: 0,0:14:39.87,0:14:41.27,Default,,0000,0000,0000,,Taketo.
Dialogue: 0,0:14:48.21,0:14:51.51,Default,,0000,0000,0000,,Mình cũng phải chịu đựng một số phận tương tự.
Dialogue: 0,0:14:53.05,0:14:55.38,Default,,0000,0000,0000,,Phần B:\NYumi, nữ thần của sự báo thù
Dialogue: 0,0:15:11.20,0:15:13.23,Default,,0000,0000,0000,,<i>Lại sai mật khẩu nữa rồi.</i>
Dialogue: 0,0:15:14.74,0:15:16.40,Default,,0000,0000,0000,,Taketo? Anh đến đây làm gì thế.
Dialogue: 0,0:15:16.47,0:15:18.87,Default,,0000,0000,0000,,Em làm gì ở đây vào lúc này thế, Yumi?
Dialogue: 0,0:15:19.48,0:15:23.24,Default,,0000,0000,0000,,Ông chủ có một chuyến công tác vào ngày mai, đó là một cuộc gặp rất quan trọng.
Dialogue: 0,0:15:23.31,0:15:26.21,Default,,0000,0000,0000,,Vì thế tôi phải ở lại đây để chuẩn bị tài liệu cho buổi gặp đó .
Dialogue: 0,0:15:27.38,0:15:30.68,Default,,0000,0000,0000,,Em thích làm việc cho ông già anh đến thế hay sao?
Dialogue: 0,0:15:31.39,0:15:35.83,Default,,0000,0000,0000,,Đó ko hề giống như tôi làm việc cho cha anh.
Dialogue: 0,0:15:36.19,0:15:37.52,Default,,0000,0000,0000,,Tôi làm việc vì chính bản thân tôi.
Dialogue: 0,0:15:37.59,0:15:40.56,Default,,0000,0000,0000,,Vậy, em ko hề yêu ông ta, phải ko?
Dialogue: 0,0:15:40.63,0:15:42.06,Default,,0000,0000,0000,,Taketo...
Dialogue: 0,0:15:42.13,0:15:44.23,Default,,0000,0000,0000,,Yumi, thật ko công bằng!
Dialogue: 0,0:15:44.77,0:15:47.83,Default,,0000,0000,0000,,Em luôn so sánh anh với ông ta,\Nvà cười nhạo anh đằng sau lưng!
Dialogue: 0,0:15:47.90,0:15:49.30,Default,,0000,0000,0000,,Ko, ko.
Dialogue: 0,0:15:50.01,0:15:51.91,Default,,0000,0000,0000,,Taketo, anh làm gì thế?
Dialogue: 0,0:15:51.98,0:15:53.50,Default,,0000,0000,0000,,Em im đi!
Dialogue: 0,0:15:53.64,0:15:56.94,Default,,0000,0000,0000,,Anh sẽ chứng minh cho em thấy là anh hơn ông ta nhiều!
Dialogue: 0,0:15:59.88,0:16:02.88,Default,,0000,0000,0000,,Đau lắm đó! Taketo-kun!
Dialogue: 0,0:16:04.99,0:16:06.35,Default,,0000,0000,0000,,Yumi...
Dialogue: 0,0:16:06.42,0:16:10.12,Default,,0000,0000,0000,,Nhẹ nhàng thôi anh.
Dialogue: 0,0:16:12.56,0:16:17.02,Default,,0000,0000,0000,,- Như thế!\N- Phải rồi. Đừng bắt em phải đợi.
Dialogue: 0,0:16:22.77,0:16:24.17,Default,,0000,0000,0000,,Taketo.
Dialogue: 0,0:16:26.38,0:16:28.94,Default,,0000,0000,0000,,Anh yêu em thật chứ?
Dialogue: 0,0:16:30.75,0:16:32.15,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:16:34.92,0:16:37.08,Default,,0000,0000,0000,,Chúng ta đều sắp trưởng thành rồi.
Dialogue: 0,0:16:37.15,0:16:41.28,Default,,0000,0000,0000,,Hãy cố chờ đến lúc cậu trở thành một người đàn ông trưởng thành và đáng tin cậy.
Dialogue: 0,0:16:43.56,0:16:46.83,Default,,0000,0000,0000,,Vậy, sao anh ko giúp em thoát khỏi tay ông ta?
Dialogue: 0,0:16:47.00,0:16:48.66,Default,,0000,0000,0000,,Tại vì...
Dialogue: 0,0:16:50.27,0:16:52.96,Default,,0000,0000,0000,,Đến khi nào thì anh mới có được dũng khí của một người đàn ông đây?
Dialogue: 0,0:16:56.17,0:16:59.34,Default,,0000,0000,0000,,Anh vẫn còn sợ cha anh, phải ko?
Dialogue: 0,0:17:03.75,0:17:05.27,Default,,0000,0000,0000,,Yumi...
Dialogue: 0,0:17:11.86,0:17:13.38,Default,,0000,0000,0000,,Tới lượt anh.
Dialogue: 0,0:17:22.57,0:17:23.97,Default,,0000,0000,0000,,Taketo.
Dialogue: 0,0:17:26.50,0:17:28.60,Default,,0000,0000,0000,,Sướng quá. Em muốn nữa.
Dialogue: 0,0:17:29.81,0:17:32.30,Default,,0000,0000,0000,,Yumi, anh ko thể chờ lâu hơn nữa!
Dialogue: 0,0:17:32.91,0:17:34.24,Default,,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:17:35.81,0:17:39.71,Default,,0000,0000,0000,,Nếu em làm chuyện này với anh, cha anh sẽ rất tức giận.
Dialogue: 0,0:17:39.82,0:17:40.78,Default,,0000,0000,0000,,Yumi.
Dialogue: 0,0:17:40.85,0:17:44.95,Default,,0000,0000,0000,,Nếu có chuyện xảy ra, anh sẽ bảo vệ em chứ?
Dialogue: 0,0:17:45.72,0:17:49.78,Default,,0000,0000,0000,,Tất nhiên. Anh sẽ chống lại ông ta đến cùng để bảo vệ em!
Dialogue: 0,0:17:50.19,0:17:51.99,Default,,0000,0000,0000,,Em có thể tin anh được ko?
Dialogue: 0,0:17:55.77,0:17:58.86,Default,,0000,0000,0000,,Anh vào bên trong em rồi, Taketo.
Dialogue: 0,0:17:59.84,0:18:03.20,Default,,0000,0000,0000,,Từ bây giờ, em đã thuộc về anh.
Dialogue: 0,0:18:03.57,0:18:05.37,Default,,0000,0000,0000,,Em sẽ trợ giúp cho anh.
Dialogue: 0,0:18:05.51,0:18:08.44,Default,,0000,0000,0000,,Yumi, cảm ơn em.
Dialogue: 0,0:18:12.12,0:18:14.81,Default,,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:18:19.22,0:18:22.21,Default,,0000,0000,0000,,Yumi, cho anh thấy đi nào.
Dialogue: 0,0:18:22.89,0:18:24.12,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:18:25.96,0:18:27.19,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:18:28.33,0:18:30.70,Default,,0000,0000,0000,,Anh có thể thấy hai chúng ta đang hòa làm một.
Dialogue: 0,0:18:30.77,0:18:33.07,Default,,0000,0000,0000,,Thấy hai chúng ta đang quấn lấy nhau, Yumi.
Dialogue: 0,0:18:34.30,0:18:36.17,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:18:59.63,0:19:01.79,Default,,0000,0000,0000,,Em ra đây!
Dialogue: 0,0:19:10.07,0:19:14.74,Default,,0000,0000,0000,,Cha anh sẽ rất ngạc nhiên khi ông ta đi công tác về.
Dialogue: 0,0:19:14.88,0:19:18.78,Default,,0000,0000,0000,,Khi ông ta biết chúng ta bắt tay với gia đình Kitazawa.
Dialogue: 0,0:19:19.25,0:19:21.34,Default,,0000,0000,0000,,Chiếu tướng, như ông ta thường nói.
Dialogue: 0,0:19:21.79,0:19:25.31,Default,,0000,0000,0000,,Anh đã cất bản giao kèo vào nơi nào đó thật an toàn, phải ko?
Dialogue: 0,0:19:25.39,0:19:28.69,Default,,0000,0000,0000,,Đúng vậy. Nó đang nằm trong két an toàn của ông già.
Dialogue: 0,0:19:29.16,0:19:33.12,Default,,0000,0000,0000,,Em sẽ giúp anh. Vậy thì, sao anh ko thử chống lại ông ta một lần xem sao?
Dialogue: 0,0:19:33.20,0:19:34.25,Default,,0000,0000,0000,,Em cần nó bây giờ hả?
Dialogue: 0,0:19:34.33,0:19:37.77,Default,,0000,0000,0000,,Đây là cơ hội để chứng minh cho cha anh thấy anh khó chơi đến mức nào.
Dialogue: 0,0:19:53.82,0:19:55.94,Default,,0000,0000,0000,,Các người ko có tiền hả?
Dialogue: 0,0:19:56.02,0:20:00.01,Default,,0000,0000,0000,,Vậy thì bán căn nhà này đi. Căn nhà này rất đáng giá đó.
Dialogue: 0,0:20:00.09,0:20:04.39,Default,,0000,0000,0000,,Tôi cần phải hỏi ý kiến chồng tôi. Tôi ko thể tự mình rao bán nó.
Dialogue: 0,0:20:04.46,0:20:07.19,Default,,0000,0000,0000,,Hỏi ý kiến chồng cô? Tại sao cô nhất quyết phải làm điều đó?
Dialogue: 0,0:20:07.26,0:20:10.99,Default,,0000,0000,0000,,Cô mới là người phải chịu trách nhiệm cho vụi tai nạn, chứ ko phải ông ta.
Dialogue: 0,0:20:11.07,0:20:13.26,Default,,0000,0000,0000,,Đừng thúc ép mẹ tôi nữa.
Dialogue: 0,0:20:13.64,0:20:15.43,Default,,0000,0000,0000,,Nếu anh muốn cơ thể tôi...
Dialogue: 0,0:20:15.51,0:20:19.00,Default,,0000,0000,0000,,Đồ điên! Đây ko phải lúc nói về chuyện đó!
Dialogue: 0,0:20:19.08,0:20:23.34,Default,,0000,0000,0000,,Các người phải trả toàn bộ chi phí trong vòng hai ngày, trước khi ông già tôi quay về.
Dialogue: 0,0:20:23.55,0:20:26.45,Default,,0000,0000,0000,,Điều đó là ko thể.
Dialogue: 0,0:20:27.35,0:20:33.19,Default,,0000,0000,0000,,Tôi ko còn hứng thú với hai con nhóc dâm đãng đã bị ông già tôi cưỡng đoạt.
Dialogue: 0,0:20:33.26,0:20:34.48,Default,,0000,0000,0000,,Anh thật xấu xa!
Dialogue: 0,0:20:34.59,0:20:37.69,Default,,0000,0000,0000,,Và anh cũng ko thể thỏa mãn chị Rumi.
Dialogue: 0,0:20:37.76,0:20:39.16,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:20:39.23,0:20:41.63,Default,,0000,0000,0000,,Anh vẫn chỉ là một kẻ yếu đuối.
Dialogue: 0,0:20:41.70,0:20:44.79,Default,,0000,0000,0000,,Cái gì? Cô dám nói tôi như thế hả?
Dialogue: 0,0:20:44.87,0:20:47.36,Default,,0000,0000,0000,,Chứ sao nữa! Anh đâu thể so với cha anh!
Dialogue: 0,0:20:47.44,0:20:48.80,Default,,0000,0000,0000,,Cô dám!
Dialogue: 0,0:20:49.31,0:20:51.21,Default,,0000,0000,0000,,Taketo, bình tĩnh nào.
Dialogue: 0,0:20:51.61,0:20:55.81,Default,,0000,0000,0000,,Chúng ta đang bàn công việc ở đây. Anh phải hiểu chứ?
Dialogue: 0,0:20:55.88,0:20:57.04,Default,,0000,0000,0000,,Ok.
Dialogue: 0,0:20:57.11,0:20:59.88,Default,,0000,0000,0000,,Taketo, anh lừa dối em phải ko.
Dialogue: 0,0:20:59.95,0:21:00.94,Default,,0000,0000,0000,,Em bảo sao?
Dialogue: 0,0:21:01.02,0:21:04.28,Default,,0000,0000,0000,,Những người này chưa hề bị chinh phục hoàn toàn.
Dialogue: 0,0:21:04.35,0:21:06.25,Default,,0000,0000,0000,,Đâu có.
Dialogue: 0,0:21:06.66,0:21:11.49,Default,,0000,0000,0000,,Nếu anh ko thể làm cho họ thỏa mãn, thì ko có lý nào anh có thể nắm được yếu điểm của họ.
Dialogue: 0,0:21:11.56,0:21:13.09,Default,,0000,0000,0000,,Yumi...
Dialogue: 0,0:21:14.03,0:21:17.30,Default,,0000,0000,0000,,Có thể chuyện này sẽ khiến cha anh...
Dialogue: 0,0:21:17.43,0:21:19.87,Default,,0000,0000,0000,,Em nói gì vậy, Yumi?
Dialogue: 0,0:21:20.04,0:21:22.10,Default,,0000,0000,0000,,Ý ba người thế nào?
Dialogue: 0,0:21:22.17,0:21:26.77,Default,,0000,0000,0000,,Nếu Taketo có thể thỏa mãn cả ba người, chúng tôi thắng.
Dialogue: 0,0:21:27.14,0:21:31.84,Default,,0000,0000,0000,,Nếu Taketo thua, bản giao kèo này sẽ được hủy đi.
Dialogue: 0,0:21:32.32,0:21:33.61,Default,,0000,0000,0000,,Yumi!
Dialogue: 0,0:21:33.68,0:21:35.17,Default,,0000,0000,0000,,Tôi đồng ý!
Dialogue: 0,0:21:35.25,0:21:36.78,Default,,0000,0000,0000,,Tôi cũng thế!
Dialogue: 0,0:21:41.39,0:21:44.45,Default,,0000,0000,0000,,Cho em thấy anh hơn hẳn cha anh đi nào.
Dialogue: 0,0:21:47.96,0:21:49.45,Default,,0000,0000,0000,,Mạnh nữa lên!
Dialogue: 0,0:21:49.70,0:21:53.64,Default,,0000,0000,0000,,Cha anh làm chuyện này tốt hơn anh nhiều đấy.
Dialogue: 0,0:21:53.70,0:21:55.33,Default,,0000,0000,0000,,Thế này được chưa?
Dialogue: 0,0:21:56.81,0:22:01.33,Default,,0000,0000,0000,,Mạnh nữa lên. Phải mạnh hơn cả cha anh nữa.
Dialogue: 0,0:22:05.88,0:22:08.01,Default,,0000,0000,0000,,Tuyệt lắm. Nó lớn lên rồi này.
Dialogue: 0,0:22:09.69,0:22:12.28,Default,,0000,0000,0000,,Tôi sẽ thưởng cho anh, bằng cách bú nó.
Dialogue: 0,0:22:14.29,0:22:17.32,Default,,0000,0000,0000,,Tôi rất thích ngón chân của các thanh niên trẻ.
Dialogue: 0,0:22:21.20,0:22:24.00,Default,,0000,0000,0000,,Đừng có liếm chân tôi thế.
Dialogue: 0,0:22:36.41,0:22:37.90,Default,,0000,0000,0000,,Anh đã ra rồi ư?
Dialogue: 0,0:22:38.18,0:22:40.81,Default,,0000,0000,0000,,Anh có muốn tôi bú mạnh hơn nữa ko?
Dialogue: 0,0:22:45.56,0:22:48.65,Default,,0000,0000,0000,,Chuyện này ko chỉ như thế này thôi đâu!
Dialogue: 0,0:22:48.73,0:22:54.22,Default,,0000,0000,0000,,Taketo, bây giờ đến lượt mẹ tôi,\Ncó lẽ anh sẽ ra hết luôn đấy.
Dialogue: 0,0:22:56.33,0:22:58.30,Default,,0000,0000,0000,,Còn khuya nhé, còn khuya.
Dialogue: 0,0:22:58.40,0:23:01.50,Default,,0000,0000,0000,,Trận đấu của chúng ta mới chỉ bắt đầu.\NHãy cứng lên đi nào.
Dialogue: 0,0:23:07.24,0:23:08.90,Default,,0000,0000,0000,,Sướng quá, sướng quá đi mất.
Dialogue: 0,0:23:08.98,0:23:12.38,Default,,0000,0000,0000,,Đã sáng rồi sao.
Dialogue: 0,0:23:12.68,0:23:15.48,Default,,0000,0000,0000,,Hôm nay cả hai chị em nó đều ko phải tới trường.
Dialogue: 0,0:23:15.55,0:23:20.25,Default,,0000,0000,0000,,Đây là cơ hội tốt để cậu chiều lòng cả hai đứa nó.
Dialogue: 0,0:23:21.12,0:23:22.85,Default,,0000,0000,0000,,Tại sao chúng ta phải bỏ lỡ chứ?
Dialogue: 0,0:23:24.39,0:23:27.80,Default,,0000,0000,0000,,Tôi luôn mong muốn một thứ to lớn như của cậu.
Dialogue: 0,0:23:29.40,0:23:31.92,Default,,0000,0000,0000,,Dù gì thì cũng phải cho tôi ăn sáng đã chứ.
Dialogue: 0,0:23:40.41,0:23:42.28,Default,,0000,0000,0000,,Tôi sắp ra nữa rồi.
Dialogue: 0,0:23:44.25,0:23:46.91,Default,,0000,0000,0000,,Ko, chị ơi, hãy cố kiềm chế.
Dialogue: 0,0:23:51.59,0:23:53.49,Default,,0000,0000,0000,,Cho tôi nghỉ chút nào.
Dialogue: 0,0:23:56.23,0:23:58.72,Default,,0000,0000,0000,,Mình ko thể ra lúc này được!\NMình ko được ra!
Dialogue: 0,0:23:59.00,0:24:01.12,Default,,0000,0000,0000,,Tôi sẽ ko thua anh đâu, Taketo!
Dialogue: 0,0:24:01.97,0:24:04.06,Default,,0000,0000,0000,,Ra hết tất cả và chịu thua đi!
Dialogue: 0,0:24:06.64,0:24:09.37,Default,,0000,0000,0000,,Tôi sẽ kì cọ cho anh ở chỗ này.
Dialogue: 0,0:24:09.47,0:24:12.10,Default,,0000,0000,0000,,Nhớ rằng sau chị ấy là tới lượt tôi đó, Taketo.
Dialogue: 0,0:24:12.78,0:24:15.57,Default,,0000,0000,0000,,Đừng có kì cọ ở chỗ đó!
Dialogue: 0,0:24:16.18,0:24:17.77,Default,,0000,0000,0000,,Tôi sắp ra rồi!
Dialogue: 0,0:24:19.58,0:24:21.24,Default,,0000,0000,0000,,Sướng quá đi mất!
Dialogue: 0,0:24:41.84,0:24:44.03,Default,,0000,0000,0000,,Nữa đi, nữa!
Dialogue: 0,0:25:01.42,0:25:03.05,Default,,0000,0000,0000,,Cậu thật phi thường!
Dialogue: 0,0:25:04.83,0:25:08.89,Default,,0000,0000,0000,,Làm ơn, Taketo, mạnh nữa lên!
Dialogue: 0,0:25:20.41,0:25:23.24,Default,,0000,0000,0000,,Đừng ngừng di chuyển ngón tay anh như vậy chứ.
Dialogue: 0,0:25:23.38,0:25:26.84,Default,,0000,0000,0000,,Hãy làm mạnh nữa lên nào!
Dialogue: 0,0:25:27.68,0:25:31.09,Default,,0000,0000,0000,,Ngừng lại thôi. Ngừng lại, mọi người.
Dialogue: 0,0:25:31.52,0:25:34.58,Default,,0000,0000,0000,,Vậy em xé nó đi nhé?\NEm sẽ xé vụn nó mà chấm dứt mọi chuyện.
Dialogue: 0,0:25:34.66,0:25:36.96,Default,,0000,0000,0000,,Ko. Ko thể như thế được.
Dialogue: 0,0:25:37.36,0:25:40.33,Default,,0000,0000,0000,,Vậy thì họ sẽ tiếp tục.
Dialogue: 0,0:25:40.60,0:25:44.53,Default,,0000,0000,0000,,Ko, ko. Anh kiệt sức rồi!
Dialogue: 0,0:25:45.20,0:25:47.60,Default,,0000,0000,0000,,Hãy tỏ ra đàn ông chút đi. Cố lên nào anh.
Dialogue: 0,0:25:47.94,0:25:49.60,Default,,0000,0000,0000,,Anh có muốn tiếp tục nữa hay ko nào?
Dialogue: 0,0:25:49.67,0:25:50.83,Default,,0000,0000,0000,,Nữa đi!
Dialogue: 0,0:25:51.31,0:25:52.67,Default,,0000,0000,0000,,Mạnh nữa lên!
Dialogue: 0,0:25:52.74,0:25:54.37,Default,,0000,0000,0000,,Đừng ngừng lại chứ!
Dialogue: 0,0:26:06.59,0:26:08.11,Default,,0000,0000,0000,,Như tôi đã nói...
Dialogue: 0,0:26:08.19,0:26:11.99,Default,,0000,0000,0000,,...Tôi chỉ giả vờ tuân lệnh ông ta.
Dialogue: 0,0:26:12.13,0:26:16.57,Default,,0000,0000,0000,,Hiện giờ ông ta đang tin chắc rằng mình đã thắng, đây là thời điểm tốt nhất để lật đổ ông ta.
Dialogue: 0,0:26:17.37,0:26:19.27,Default,,0000,0000,0000,,Tôi hiểu rồi. Cứ để việc này cho tôi.
Dialogue: 0,0:26:19.57,0:26:21.09,Default,,0000,0000,0000,,Tôi nữa.
Dialogue: 0,0:26:28.01,0:26:31.64,Default,,0000,0000,0000,,Hay lắm! Hai chị em cô cũng phục vụ ta một lúc!
Dialogue: 0,0:26:31.71,0:26:35.17,Default,,0000,0000,0000,,Tuyệt lắm. Bú nó đi!
Dialogue: 0,0:26:35.52,0:26:38.21,Default,,0000,0000,0000,,Kết thúc việc này cho khéo vào, cả hai cô!
Dialogue: 0,0:26:40.02,0:26:42.82,Default,,0000,0000,0000,,Làm ơn, hiệu trưởng, hãy tha cho tôi.
Dialogue: 0,0:26:43.73,0:26:47.66,Default,,0000,0000,0000,,Đừng có kháng cự lại ta, các cô phải trả món nợ do mẹ các cô đã mắc phải!
Dialogue: 0,0:26:47.73,0:26:50.76,Default,,0000,0000,0000,,Ko! Hiệu trưởng, làm ơn hãy dừng lại!
Dialogue: 0,0:26:51.07,0:26:55.06,Default,,0000,0000,0000,,Tuyệt lắm. Bú cái của ta mạnh nữa lên nào!
Dialogue: 0,0:26:55.14,0:26:57.97,Default,,0000,0000,0000,,Hiệu trưởng, ko. Tôi ko thể tiếp tục được nữa!
Dialogue: 0,0:26:59.08,0:27:02.84,Default,,0000,0000,0000,,Hai cô sẽ làm nô lệ cho ta trọn đời!
Dialogue: 0,0:27:02.91,0:27:05.54,Default,,0000,0000,0000,,Nhanh lên, phục vụ ta tiếp đi nào, cả hai cô!
Dialogue: 0,0:27:05.78,0:27:09.41,Default,,0000,0000,0000,,- Tha cho chúng tôi, làm ơn!\N- Ai đó giúp tôi với!
Dialogue: 0,0:27:09.49,0:27:11.21,Default,,0000,0000,0000,,Xin chào, văm phòng cảnh sát phải ko?
Dialogue: 0,0:27:11.82,0:27:14.15,Default,,0000,0000,0000,,Tôi có một tập tài liệu cần các ngài xem xét...
Dialogue: 0,0:27:14.22,0:27:17.35,Default,,0000,0000,0000,,...có liên quan tới một vụ bê bối ở học viện Nogawa.
Dialogue: 0,0:27:17.43,0:27:20.42,Default,,0000,0000,0000,,Thôi ngay đi nào! Cô muốn khóc muốn la gì tùy ý...
Dialogue: 0,0:27:20.50,0:27:25.37,Default,,0000,0000,0000,,...Chỉ có điều ta chính là Hiệu trưởng của ngôi trường này! Ko kẻ nào dám chống đối ta đâu!
Dialogue: 0,0:27:30.91,0:27:33.03,Default,,0000,0000,0000,,Một tháng sau
Dialogue: 0,0:27:33.11,0:27:36.88,Default,,0000,0000,0000,,Cha anh đang ngồi tù. Anh đã được tự do rồi.
Dialogue: 0,0:27:36.95,0:27:39.28,Default,,0000,0000,0000,,Nhưng anh vẫn ko bằng được ông ta.
Dialogue: 0,0:27:39.35,0:27:42.84,Default,,0000,0000,0000,,Thôi nào anh yêu, chúng ta mới thực sự là những người thắng cuộc ở đây.
Dialogue: 0,0:27:43.42,0:27:45.61,Default,,0000,0000,0000,,Coi nào, để tôi làm cho nó lớn lên.
Dialogue: 0,0:27:45.69,0:27:49.15,Default,,0000,0000,0000,,Anh tới sống tại đây vì muốn chuộc lại những tội lỗi của cha anh, phải ko?
Dialogue: 0,0:27:49.23,0:27:51.42,Default,,0000,0000,0000,,Đến lúc tôi ăn sáng rồi đây.
Dialogue: 0,0:27:54.16,0:27:57.56,Default,,0000,0000,0000,,Này chị, đừng tham lam như vậy chứ.\NEm cũng muốn nữa.
Dialogue: 0,0:27:57.63,0:28:00.16,Default,,0000,0000,0000,,Mới sáng sớm mà mọi người đã vui vẻ như vậy sao.
Dialogue: 0,0:28:00.27,0:28:01.29,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:28:01.37,0:28:03.34,Default,,0000,0000,0000,,Mẹ có muốn tham gia với bọn con ko?
Dialogue: 0,0:28:03.41,0:28:06.34,Default,,0000,0000,0000,,Mẹ rất thích điều đó, nhưng mẹ còn phải đến công ty.
Dialogue: 0,0:28:06.44,0:28:09.50,Default,,0000,0000,0000,,Mẹ sẽ dành thời gian để vui vẻ với cậu ấy khi mẹ trở về.
Dialogue: 0,0:28:09.58,0:28:11.34,Default,,0000,0000,0000,,Phải ko, Taketo?
Dialogue: 0,0:28:13.42,0:28:14.88,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:28:18.39,0:28:19.88,Default,,0000,0000,0000,,Lái xe an toàn
