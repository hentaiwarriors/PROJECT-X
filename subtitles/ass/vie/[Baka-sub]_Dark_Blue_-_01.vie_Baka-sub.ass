Dialogue: 0,0:00:03.05,0:00:05.03,Dark Blue Dialogue,,0000,0000,0000,,Đủ rồi đó!
Dialogue: 0,0:00:05.41,0:00:07.27,Dark Blue Dialogue,,0000,0000,0000,,Hãy để tôi yên!
Dialogue: 0,0:00:07.79,0:00:09.19,Dark Blue Dialogue,,0000,0000,0000,,Trong hoàn cảnh này mà ông...
Dialogue: 0,0:00:09.71,0:00:11.29,Dark Blue Dialogue,,0000,0000,0000,,Bởi vì trong hoàn cảnh này...
Dialogue: 0,0:00:11.72,0:00:13.07,Dark Blue Dialogue,,0000,0000,0000,,Nên tôi...
Dialogue: 0,0:00:13.07,0:00:15.43,Dark Blue Dialogue,,0000,0000,0000,,Tôi nói rồi, ông đang làm phiền tôi đó!
Dialogue: 0,0:00:16.55,0:00:17.53,Dark Blue Dialogue,,0000,0000,0000,,Làm phiền cô?
Dialogue: 0,0:00:17.95,0:00:20.35,Dark Blue Dialogue,,0000,0000,0000,,Tôi đến đây là để an ủi cô vậy mà...
Dialogue: 0,0:00:20.35,0:00:21.10,Dark Blue Dialogue,,0000,0000,0000,,Đồ vô ơn...
Dialogue: 0,0:00:21.64,0:00:22.45,Dark Blue Dialogue,,0000,0000,0000,,Nee-san...
Dialogue: 0,0:00:23.24,0:00:24.09,Dark Blue Dialogue,,0000,0000,0000,,Yukito.
Dialogue: 0,0:00:28.76,0:00:29.45,Dark Blue Dialogue,,0000,0000,0000,,Ta đi thôi.
Dialogue: 0,0:00:30.20,0:00:31.19,Dark Blue Dialogue,,0000,0000,0000,,Này!
Dialogue: 0,0:00:39.14,0:00:40.08,Dark Blue Dialogue,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:00:40.72,0:00:41.52,Dark Blue Dialogue,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:00:42.28,0:00:43.41,Dark Blue Dialogue,,0000,0000,0000,,Tại sao chuyện này lại xảy ra?
Dialogue: 0,0:00:45.17,0:00:47.26,Dark Blue Dialogue,,0000,0000,0000,,Tại sao lại là Kotomi...
Dialogue: 0,0:00:49.10,0:00:52.11,Dark Blue Dialogue,,0000,0000,0000,,Nee-san, chị hãy nằm xuống nghỉ một lát đi.
Dialogue: 0,0:00:57.07,0:00:58.96,Dark Blue Dialogue,,0000,0000,0000,,Xin lỗi nhé, Yukito.
Dialogue: 0,0:01:06.73,0:01:09.46,Dark Blue Dialogue,,0000,0000,0000,,Chị ấy không sao chứ? Tớ nghe thấy tiếng chị ấy khóc.
Dialogue: 0,0:01:10.76,0:01:13.29,Dark Blue Dialogue,,0000,0000,0000,,Ờ, xin lỗi đã khiến cậu lo lắng.
Dialogue: 0,0:01:13.91,0:01:15.96,Dark Blue Dialogue,,0000,0000,0000,,Ngốc quá, có gì phải xin lỗi chứ.
Dialogue: 0,0:01:21.91,0:01:23.05,Dark Blue Dialogue,,0000,0000,0000,,Yukito...
Dialogue: 0,0:01:24.41,0:01:27.32,Dark Blue Sign1,,0000,0000,0000,,{\fad(400,400)\pos(356,386)}- Một ngày trước -
Dialogue: 0,0:01:35.48,0:01:37.89,Dark Blue Dialogue,,0000,0000,0000,,Yukito-kun, cậu giận à?
Dialogue: 0,0:01:39.22,0:01:41.16,Dark Blue Dialogue,,0000,0000,0000,,Không, làm gì có.
Dialogue: 0,0:01:41.68,0:01:44.08,Dark Blue Dialogue,,0000,0000,0000,,Tớ đã có thể đưa cả hai chị em gái theo cùng.
Dialogue: 0,0:01:44.74,0:01:45.90,Dark Blue Dialogue,,0000,0000,0000,,Thệt là tốt quá.
Dialogue: 0,0:01:45.98,0:01:47.84,Dark Blue Dialogue,,0000,0000,0000,,Cậu cứ nhìn đăm đăm ngoài cửa kính,
Dialogue: 0,0:01:48.01,0:01:50.17,Dark Blue Dialogue,,0000,0000,0000,,khiến mình lo có phải do mình bắt cậu đi cùng hay không.
Dialogue: 0,0:01:50.72,0:01:52.69,Dark Blue Dialogue,,0000,0000,0000,,Những rặng núi kia thật hùng vĩ.
Dialogue: 0,0:01:53.14,0:01:54.89,Dark Blue Dialogue,,0000,0000,0000,,Cộng thêm những cây cầu rất đẹp nữa.
Dialogue: 0,0:01:55.79,0:01:57.10,Dark Blue Dialogue,,0000,0000,0000,,Đúng là vậy.
Dialogue: 0,0:01:57.40,0:01:58.59,Dark Blue Dialogue,,0000,0000,0000,,Chỉ là nó hơi đáng sợ một chút.
Dialogue: 0,0:01:59.15,0:02:02.29,Dark Blue Dialogue,,0000,0000,0000,,Trước đây cậu từng rất sợ độ cao.
Dialogue: 0,0:02:02.63,0:02:03.53,Dark Blue Dialogue,,0000,0000,0000,,Cậu trẻ con ghê.
Dialogue: 0,0:02:04.24,0:02:06.54,Dark Blue Dialogue,,0000,0000,0000,,Làm gì có!
Dialogue: 0,0:02:06.77,0:02:09.93,Dark Blue Dialogue,,0000,0000,0000,,Hừm. Cậu chỉ nhớ toàn những thứ đáng xấu hổ thôi.
Dialogue: 0,0:02:14.10,0:02:15.17,Dark Blue Dialogue,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:02:15.26,0:02:17.94,Dark Blue Dialogue,,0000,0000,0000,,Otoha, gia đình cậu có một căn nhà nghỉ mát phải không?
Dialogue: 0,0:02:18.32,0:02:21.68,Dark Blue Dialogue,,0000,0000,0000,,Đúng vậy nhưng nó không lớn như vầy.
Dialogue: 0,0:02:33.32,0:02:37.71,Dark Blue Dialogue,,0000,0000,0000,,Yukito-kun, cậu nhớ lời hứa hồi chúng mình còn nhỏ chứ?
Dialogue: 0,0:02:39.94,0:02:41.77,Dark Blue Dialogue,,0000,0000,0000,,Xin lỗi nhé. Thôi quên đi.
Dialogue: 0,0:02:42.16,0:02:43.08,Dark Blue Dialogue,,0000,0000,0000,,Không có gì đâu.
Dialogue: 0,0:02:43.85,0:02:44.82,Dark Blue Dialogue,,0000,0000,0000,,Chúng ta đi thôi.
Dialogue: 0,0:02:45.88,0:02:48.99,Dark Blue Dialogue,,0000,0000,0000,,Otoha... Những chuyện xảy ra trước đây...
Dialogue: 0,0:02:49.99,0:02:52.08,Dark Blue Dialogue,,0000,0000,0000,,Làm sao tớ quên được?
Dialogue: 0,0:03:08.69,0:03:10.97,Dark Blue Dialogue,,0000,0000,0000,,Tại sao? Kotomi!
Dialogue: 0,0:03:11.77,0:03:13.30,Dark Blue Dialogue,,0000,0000,0000,,Chúng tôi có một bản tin đặc biệt.
Dialogue: 0,0:03:13.90,0:03:16.00,Dark Blue Dialogue,,0000,0000,0000,,Hôm nay, vào lúc 18:45,
Dialogue: 0,0:03:16.22,0:03:22.11,Dark Blue Dialogue,,0000,0000,0000,,tại nhà ga Buryou-cho đã tìm thấy một nạn nhân bị đâm trọng thương.
Dialogue: 0,0:03:23.21,0:03:27.07,Dark Blue Dialogue,,0000,0000,0000,,Các nghi phạm đã bị phát hiện tại khu rừng thuộc Morino-cho.
Dialogue: 0,0:03:27.07,0:03:32.77,Dark Blue Dialogue,,0000,0000,0000,,Suzuki-san, Matsui-san, Kuroki-san - cả 3 tên đã bị kết án tội cố ý gây thương tích.
Dialogue: 0,0:03:33.04,0:03:34.83,Dark Blue Dialogue,,0000,0000,0000,,Chúng hiện đang trên đường trốn chạy.
Dialogue: 0,0:03:35.38,0:03:36.26,Dark Blue Dialogue,,0000,0000,0000,,Cảnh sát đang...
Dialogue: 0,0:03:36.26,0:03:38.88,Dark Blue Dialogue,,0000,0000,0000,,Bọn sát nhân... chúng đang bỏ trốn.
Dialogue: 0,0:03:39.67,0:03:41.52,Dark Blue Dialogue,,0000,0000,0000,,Có lẽ chính bọn chúng đã đập vỡ cửa kính.
Dialogue: 0,0:03:42.82,0:03:45.13,Dark Blue Dialogue,,0000,0000,0000,,Tôi không nhìn thấy ai ở xung quanh khu này...
Dialogue: 0,0:03:45.36,0:03:48.45,Dark Blue Dialogue,,0000,0000,0000,,Chỉ là, tôi phát hiện cây cầu đến đây đã bị phá hủy.
Dialogue: 0,0:03:48.79,0:03:50.52,Dark Blue Dialogue,,0000,0000,0000,,Nghĩa là chúng ta bị mắc kẹt ở đây ư?
Dialogue: 0,0:03:50.71,0:03:52.99,Dark Blue Dialogue,,0000,0000,0000,,Chúng ta không thể làm gì sao?
Dialogue: 0,0:03:53.56,0:03:55.80,Dark Blue Dialogue,,0000,0000,0000,,Cảnh sát đang làm cái quái gì thế?
Dialogue: 0,0:03:56.63,0:03:58.96,Dark Blue Dialogue,,0000,0000,0000,,Hãy để việc canh gác bên ngoài cho Nigel và người của ông ấy.
Dialogue: 0,0:03:59.95,0:04:02.74,Dark Blue Dialogue,,0000,0000,0000,,Tất cả mọi người hãy trở về phòng của mình.
