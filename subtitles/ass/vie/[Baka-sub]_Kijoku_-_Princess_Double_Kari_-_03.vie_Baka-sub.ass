Dialogue: 0,0:02:02.81,0:02:07.34,Default,,0000,0000,0000,,{\pos(317,463)\c&H080808&\3c&HFFFFFF&\4c&HFFFFFF&}Nhật kí {\c&H3B30FF&}công {\c&H1B1B1B&}chúa
Dialogue: 0,0:02:08.19,0:02:13.72,Default,,0000,0000,0000,,Phần 3: {\c&H0D0CF3&}Sa ngã
Dialogue: 0,0:02:20.73,0:02:22.79,Default,,0000,0000,0000,,Distol, con vật đó là sao?
Dialogue: 0,0:02:23.30,0:02:26.33,Default,,0000,0000,0000,,Đó là quà tặng của ta. Có vẻ như nàng rất cô đơn...
Dialogue: 0,0:02:26.41,0:02:29.00,Default,,0000,0000,0000,,mỗi khi không có ta bên cạnh.
Dialogue: 0,0:02:29.07,0:02:32.94,Default,,0000,0000,0000,,Nhớ nhung ngươi ư? Đừng có nằm mơ?
Dialogue: 0,0:02:33.01,0:02:36.34,Default,,0000,0000,0000,,Vả lại, ta không thích nuôi động vật.
Dialogue: 0,0:02:36.72,0:02:38.55,Default,,0000,0000,0000,,Nàng ghét động vật như vậy sao, Elena?
Dialogue: 0,0:02:39.25,0:02:43.31,Default,,0000,0000,0000,,Không phải vì lẽ đó. Ngươi không thấy làm vậy quá tàn nhẫn ư?
Dialogue: 0,0:02:43.92,0:02:46.41,Default,,0000,0000,0000,,Giam cầm một con thú đáng yêu như vậy trong chiếc lồng chật hẹp đó...
Dialogue: 0,0:02:47.36,0:02:50.19,Default,,0000,0000,0000,,Chà chà, ta biết là nàng thích mà.
Dialogue: 0,0:02:50.80,0:02:53.29,Default,,0000,0000,0000,,Câm miệng, ta đã nói rồi...
Dialogue: 0,0:03:01.87,0:03:04.84,Default,,0000,0000,0000,,Thế này là sao, Distol? Nó...
Dialogue: 0,0:03:06.91,0:03:09.18,Default,,0000,0000,0000,,Dừng lại, nhột quá.
Dialogue: 0,0:03:09.25,0:03:11.11,Default,,0000,0000,0000,,Có vẻ như nó rất khoái nàng.
Dialogue: 0,0:03:11.72,0:03:17.49,Default,,0000,0000,0000,,Ngươi nghĩ vậy sao? Nhưng ta không hề muốn nuôi...
Dialogue: 0,0:03:18.49,0:03:20.22,Default,,0000,0000,0000,,Ta bảo mi dừng lại.
Dialogue: 0,0:03:20.89,0:03:21.62,Default,,0000,0000,0000,,Cái gì vậy...
Dialogue: 0,0:03:26.67,0:03:27.76,Default,,0000,0000,0000,,Cái gì thế này!?
Dialogue: 0,0:03:33.07,0:03:33.94,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:03:35.14,0:03:36.20,Default,,0000,0000,0000,,Không!!!
Dialogue: 0,0:03:36.51,0:03:38.27,Default,,0000,0000,0000,,Dừng lại, thứ này thật ghê tởm!
Dialogue: 0,0:03:39.08,0:03:40.34,Default,,0000,0000,0000,,Đừng liếm chỗ đó!
Dialogue: 0,0:03:40.95,0:03:43.61,Default,,0000,0000,0000,,Distol, làm ơn bảo nó dừng lại đi!
Dialogue: 0,0:03:44.68,0:03:48.24,Default,,0000,0000,0000,,Thứ lỗi cho ta, những lúc thế này thì không ai sai khiến nổi nó đâu.
Dialogue: 0,0:03:49.55,0:03:53.75,Default,,0000,0000,0000,,Với lại, tình cảnh này rất hợp với nàng. Ta không nỡ bắt nó dừng tay.
Dialogue: 0,0:03:54.23,0:03:55.56,Default,,0000,0000,0000,,Ngươi nói cái gì?
Dialogue: 0,0:03:56.63,0:03:59.22,Default,,0000,0000,0000,,Không!!!!!
Dialogue: 0,0:04:02.20,0:04:06.86,Default,,0000,0000,0000,,Dừng lại. Thật ghê tởm. Không... không!!!!
Dialogue: 0,0:04:08.44,0:04:11.50,Default,,0000,0000,0000,,Nó đang dày vò cơ thể ta, đau quá!
Dialogue: 0,0:04:16.92,0:04:19.25,Default,,0000,0000,0000,,Không. Những vòi xúc tu đó đang...
Dialogue: 0,0:04:22.25,0:04:24.72,Default,,0000,0000,0000,,Thật tàn nhẫn, nhưng...
Dialogue: 0,0:04:28.13,0:04:30.56,Default,,0000,0000,0000,,Nhưng... lại sung sướng vô cùng.
Dialogue: 0,0:04:35.20,0:04:38.26,Default,,0000,0000,0000,,Distol, ngươi làm ơn... làm ơn...
Dialogue: 0,0:04:42.44,0:04:45.07,Default,,0000,0000,0000,,Quái vật này đã khiến nàng lên đỉnh rồi sao.
Dialogue: 0,0:04:52.18,0:04:54.05,Default,,0000,0000,0000,,Ta phát điên mất.
Dialogue: 0,0:04:56.02,0:04:58.32,Default,,0000,0000,0000,,Làm ơn, cho ta ra đi!
Dialogue: 0,0:05:09.43,0:05:11.87,Default,,0000,0000,0000,,Thôi được, làm cô ta ra đi.
Dialogue: 0,0:05:35.29,0:05:37.76,Default,,0000,0000,0000,,Ngừng tay được rồi đó, ngươi làm khá lắm.
Dialogue: 0,0:05:39.56,0:05:41.90,Default,,0000,0000,0000,,Sao hả? Nàng thích chứ?
Dialogue: 0,0:05:43.50,0:05:46.27,Default,,0000,0000,0000,,Chuyện gì vậy? Không mở miệng nổi nữa à?
Dialogue: 0,0:05:52.81,0:05:56.47,Default,,0000,0000,0000,,Ta sắp ra trận, nhưng trước đó, ta muốn vui vẻ với nàng một chút.
Dialogue: 0,0:05:57.38,0:05:59.21,Default,,0000,0000,0000,,Ý ngươi là gì?
Dialogue: 0,0:05:59.65,0:06:02.38,Default,,0000,0000,0000,,Hai người ân ái với nhau mãi cũng chán.
Dialogue: 0,0:06:03.26,0:06:04.19,Default,,0000,0000,0000,,Eleanor!
Dialogue: 0,0:06:04.26,0:06:05.72,Default,,0000,0000,0000,,Ngài vừa gọi tôi, Chủ nhân?
Dialogue: 0,0:06:08.39,0:06:12.69,Default,,0000,0000,0000,,Phải, ta có việc muốn nhờ cô đây.
Dialogue: 0,0:06:13.33,0:06:14.66,Default,,0000,0000,0000,,Cô ta là ai?
Dialogue: 0,0:06:15.37,0:06:18.77,Default,,0000,0000,0000,,Xin Công chúa thứ lỗi, thuộc hạ tên Eleanor.
Dialogue: 0,0:06:18.87,0:06:20.27,Default,,0000,0000,0000,,Eleanor?
Dialogue: 0,0:06:21.31,0:06:25.77,Default,,0000,0000,0000,,Vui lên nào. Hai chúng ta sẽ khiến nàng thích thú vô cùng.
Dialogue: 0,0:06:27.11,0:06:29.74,Default,,0000,0000,0000,,Gì cơ? Ngươi đang âm mưu điều gì, Di?
Dialogue: 0,0:06:30.38,0:06:32.61,Default,,0000,0000,0000,,Thứ lỗi cho thuộc hạ, Elena-sama.
Dialogue: 0,0:06:35.95,0:06:37.79,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:06:40.16,0:06:42.18,Default,,0000,0000,0000,,Ngươi, ngươi vừa làm gì ta?
Dialogue: 0,0:06:43.60,0:06:49.26,Default,,0000,0000,0000,,Để giúp Công chúa được thoải mái hơn một chút, thuộc hạ vừa sử dụng ma thuật.
Dialogue: 0,0:06:49.97,0:06:51.23,Default,,0000,0000,0000,,Ma thuật ư?
Dialogue: 0,0:06:51.87,0:06:54.40,Default,,0000,0000,0000,,Vâng, ví dụ như...
Dialogue: 0,0:06:58.64,0:06:59.67,Default,,0000,0000,0000,,C-Cái gì!?
Dialogue: 0,0:07:01.71,0:07:04.24,Default,,0000,0000,0000,,Xin lỗi đã bắt Người phải chờ đợi, Elena-sama.
Dialogue: 0,0:07:06.15,0:07:07.91,Default,,0000,0000,0000,,N-Ngươi định làm gì ta?
Dialogue: 0,0:07:08.39,0:07:10.38,Default,,0000,0000,0000,,Elena-sama, thứ lỗi cho thuộc hạ.
Dialogue: 0,0:07:12.22,0:07:13.35,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:07:19.03,0:07:21.36,Default,,0000,0000,0000,,Hãy để thuộc hạ giúp Công chúa thỏa mãn.
Dialogue: 0,0:07:22.77,0:07:24.06,Default,,0000,0000,0000,,Dừng lại...
Dialogue: 0,0:07:25.80,0:07:29.26,Default,,0000,0000,0000,,Elena, nàng có thích thằng nhỏ của Eleanor không?
Dialogue: 0,0:07:29.88,0:07:32.34,Default,,0000,0000,0000,,Ngươi là đồ điên!
Dialogue: 0,0:07:34.38,0:07:36.68,Default,,0000,0000,0000,,Công chúa không thích ư?
Dialogue: 0,0:07:37.32,0:07:39.28,Default,,0000,0000,0000,,Tất nhiên là không rồi...
Dialogue: 0,0:07:41.52,0:07:46.01,Default,,0000,0000,0000,,Thứ lỗi cho thuộc hạ, Elena-sama, thuộc hạ sẽ mạnh tay hơn chút nữa.
Dialogue: 0,0:07:46.49,0:07:50.93,Default,,0000,0000,0000,,Eleanor, cô phải gắng hết sức làm vừa lòng Elena đó nhé.
Dialogue: 0,0:07:51.00,0:07:52.33,Default,,0000,0000,0000,,Vâng, Chủ nhân.
Dialogue: 0,0:08:03.38,0:08:07.90,Default,,0000,0000,0000,,Elena-sama, hình như Công chúa bắt đầu thích thằng nhỏ của thuộc hạ rồi.
Dialogue: 0,0:08:10.32,0:08:13.91,Default,,0000,0000,0000,,Tuyệt quá đi mất, ta thích nó!
Dialogue: 0,0:08:15.55,0:08:20.08,Default,,0000,0000,0000,,Elena, nàng có muốn ta tham gia cùng không?
Dialogue: 0,0:08:20.16,0:08:23.09,Default,,0000,0000,0000,,Ta muốn. Hãy cho ta nhiều dương vật hơn nữa
Dialogue: 0,0:08:23.96,0:08:28.06,Default,,0000,0000,0000,,Khi nàng muốn cầu xin điều gì, phải cầu xin thật lễ phép... Elena.
Dialogue: 0,0:08:28.90,0:08:33.34,Default,,0000,0000,0000,,Làm ơn... Di, nhét thằng nhỏ của chàng vào bên trong em đi!!
Dialogue: 0,0:08:35.54,0:08:42.17,Default,,0000,0000,0000,,Chủ nhân, đừng phũ phàng với Elena-sama như vậy. Xin ngài hãy đâm thằng nhỏ của mình vào hậu môn Công chúa đi.
Dialogue: 0,0:08:43.92,0:08:48.35,Default,,0000,0000,0000,,Nếu nàng đã nhất quyết muốn vậy, tốt thôi. Ta vào đây, Elena.
Dialogue: 0,0:08:52.92,0:08:57.62,Default,,0000,0000,0000,,Công chúa ra rồi ư? Chuyện này chỉ vừa mới bắt đầu thôi mà.
Dialogue: 0,0:08:58.70,0:09:01.83,Default,,0000,0000,0000,,Hai dương vật, đang co giật...
Dialogue: 0,0:09:01.90,0:09:05.84,Default,,0000,0000,0000,,Đúng vậy Elena, nhưng lúc này ta chưa thỏa mãn đâu.
Dialogue: 0,0:09:08.31,0:09:09.77,Default,,0000,0000,0000,,Bự quá đi mất!
Dialogue: 0,0:09:10.44,0:09:14.88,Default,,0000,0000,0000,,Em ra nữa rồi!!!
Dialogue: 0,0:09:14.95,0:09:17.54,Default,,0000,0000,0000,,Elena, chúng ta sẽ lấp đầy hai lỗ của nàng.
Dialogue: 0,0:09:17.62,0:09:19.34,Default,,0000,0000,0000,,Phải không, Eleanor?
Dialogue: 0,0:09:20.85,0:09:22.65,Default,,0000,0000,0000,,Vâng, Chủ nhân.
Dialogue: 0,0:09:24.29,0:09:28.73,Default,,0000,0000,0000,,Hãy bắn lên người em... nhiều hơn nữa!
Dialogue: 0,0:09:33.97,0:09:37.77,Default,,0000,0000,0000,,Người đã chịu khuất phục rồi sao, Elena-sama?
Dialogue: 0,0:09:39.60,0:09:42.31,Default,,0000,0000,0000,,Âm hộ và hậu môn ta đang tràn đầy tinh dịch!
Dialogue: 0,0:09:44.64,0:09:49.44,Default,,0000,0000,0000,,Elena đã hoàn toàn sa ngã, vậy nên mình cần tới Tháp Tin lành ngay lập tức.
Dialogue: 0,0:09:50.28,0:09:54.34,Default,,0000,0000,0000,,Mình muốn gặp mặt Qoona trước khi tổng tiến công vào kinh đô Bastarauge.
Dialogue: 0,0:09:55.39,0:09:57.98,Default,,0000,0000,0000,,Dù đã gần như cầm chắc thằng lợi trong tay, nhưng biết đâu...
Dialogue: 0,0:09:58.06,0:10:02.86,Default,,0000,0000,0000,,Nếu mình thất bại, Qoona sẽ phải sống trọn đời trong tòa tháp kia.
Dialogue: 0,0:10:02.93,0:10:05.76,Default,,0000,0000,0000,,Hồi hộp quá...
Dialogue: 0,0:10:08.90,0:10:11.73,Default,,0000,0000,0000,,Nii-sama, anh tới rồi ư!
Dialogue: 0,0:10:12.80,0:10:15.86,Default,,0000,0000,0000,,Qoona, anh sắp đánh thẳng vào kinh đô Bastarauge rồi.
Dialogue: 0,0:10:16.51,0:10:20.60,Default,,0000,0000,0000,,Anh sẽ đoạt lấy báu vật và giải thoát em khỏi ngọn tháp đáng nguyền rủa này...
Dialogue: 0,0:10:21.28,0:10:26.48,Default,,0000,0000,0000,,Vậy thì tuyệt quá, nhưng ngộ nhỡ anh xảy ra chuyện gì, em...
Dialogue: 0,0:10:26.55,0:10:31.15,Default,,0000,0000,0000,,Anh sẽ không sao đâu, nhưng... anh cần thêm thời gian để giành thắng lợi hoàn toàn.
Dialogue: 0,0:10:31.22,0:10:35.52,Default,,0000,0000,0000,,Vậy nên anh sẽ phải vắng mặt khá lâu, cũng là lí do khiến anh tới đây hôm nay.
Dialogue: 0,0:10:35.59,0:10:38.19,Default,,0000,0000,0000,,Nii-sama, em vui lắm...
Dialogue: 0,0:10:38.76,0:10:40.96,Default,,0000,0000,0000,,Em có tâm nguyện gì không? Cứ nói anh nghe.
Dialogue: 0,0:10:41.03,0:10:43.59,Default,,0000,0000,0000,,Anh sẽ cố gắng hoàn thành giúp em.
Dialogue: 0,0:10:44.04,0:10:46.16,Default,,0000,0000,0000,,Nói ra thì xấu hổ...
Dialogue: 0,0:10:46.74,0:10:48.90,Default,,0000,0000,0000,,Đừng ngại, nói anh nghe đi.
Dialogue: 0,0:10:48.97,0:10:50.27,Default,,0000,0000,0000,,Nào.
Dialogue: 0,0:10:50.98,0:10:57.54,Default,,0000,0000,0000,,Em không muốn nói ra những lời dâm dật... nhưng em... vẫn muốn... thằng nhỏ của anh...
Dialogue: 0,0:10:58.15,0:11:01.85,Default,,0000,0000,0000,,Em không thể quên vòng tay ấm áp của anh.
Dialogue: 0,0:11:01.92,0:11:05.52,Default,,0000,0000,0000,,Em vừa nói gì? Anh nghe không rõ.
Dialogue: 0,0:11:06.46,0:11:08.95,Default,,0000,0000,0000,,Xấu hổ chết đi được, là... thằng nhỏ...
Dialogue: 0,0:11:09.33,0:11:12.46,Default,,0000,0000,0000,,Ra vậy, em đừng ngại.
Dialogue: 0,0:11:12.53,0:11:15.59,Default,,0000,0000,0000,,Chính điều đó đã mang lại động lực chiến thằng cho anh.
Dialogue: 0,0:11:16.37,0:11:17.39,Default,,0000,0000,0000,,Ý anh là...
Dialogue: 0,0:11:17.47,0:11:19.44,Default,,0000,0000,0000,,Uhm, nào em,
Dialogue: 0,0:11:30.75,0:11:31.74,Default,,0000,0000,0000,,Nii-sama?
Dialogue: 0,0:11:31.82,0:11:34.11,Default,,0000,0000,0000,,Không có gì, em làn tuyệt lắm.
Dialogue: 0,0:11:34.19,0:11:38.32,Default,,0000,0000,0000,,Niềm vui của anh chính là hạnh phúc của em, Nii-sama.
Dialogue: 0,0:11:38.39,0:11:41.32,Default,,0000,0000,0000,,Qoona, em dùng lưỡi của mình được không?
Dialogue: 0,0:11:41.39,0:11:43.69,Default,,0000,0000,0000,,Anh sẽ ra sớm thôi.
Dialogue: 0,0:11:43.76,0:11:46.20,Default,,0000,0000,0000,,Như vậy xấu hổ lắm.
Dialogue: 0,0:11:46.63,0:11:49.76,Default,,0000,0000,0000,,Qoona, chính em là người yêu cầu chuyện này trước tiên mà. Nào.
Dialogue: 0,0:11:50.10,0:11:51.63,Default,,0000,0000,0000,,Phải rồi...
Dialogue: 0,0:11:53.87,0:11:57.33,Default,,0000,0000,0000,,Tuyệt lắm. Cứ nhẹ nhàng như vậy.
Dialogue: 0,0:12:03.21,0:12:06.74,Default,,0000,0000,0000,,Qoona, anh sẽ bắn lên mặt em mất!
Dialogue: 0,0:12:07.05,0:12:08.92,Default,,0000,0000,0000,,Vâng, Nii-sama...
Dialogue: 0,0:12:10.09,0:12:11.52,Default,,0000,0000,0000,,Anh ra đây!
Dialogue: 0,0:12:19.60,0:12:21.79,Default,,0000,0000,0000,,Tuyệt quá, Qoona.
Dialogue: 0,0:12:23.37,0:12:25.84,Default,,0000,0000,0000,,Nếu em không thích thì cứ việc nhổ nó ra nhé.
Dialogue: 0,0:12:36.15,0:12:38.21,Default,,0000,0000,0000,,Mình phải cứu Qoona bằng mọi giá.
Dialogue: 0,0:12:39.02,0:12:44.11,Default,,0000,0000,0000,,Mỗi khi nghĩ tới Qoona, mình lại tự hứa với bản thân, bằng mọi giá phải chiến thắng.
Dialogue: 0,0:12:45.52,0:12:46.68,Default,,0000,0000,0000,,Và cuối cùng...
Dialogue: 0,0:12:48.36,0:12:50.29,Default,,0000,0000,0000,,Mình đã thật sự chiến thắng.
Dialogue: 0,0:12:52.33,0:12:55.89,Default,,0000,0000,0000,,Nhà ngục này đang giam giữ ba mĩ nhân của vương quốc bại trận.
Dialogue: 0,0:12:57.14,0:13:01.54,Default,,0000,0000,0000,,Căn nhà ngục ẩm thấp và nặng mùi.
Dialogue: 0,0:13:02.84,0:13:08.24,Default,,0000,0000,0000,,Trải qua hàng trăm năm, biết bao người vô tội đã chết dần chết mòn tại đây.
Dialogue: 0,0:13:09.18,0:13:14.71,Default,,0000,0000,0000,,Oán khí bao trùm khắp nơi.
Dialogue: 0,0:13:15.65,0:13:21.22,Default,,0000,0000,0000,,Quả là nơi giam giữ lí tưởng dành cho ba mĩ nhân Bastarauge.
Dialogue: 0,0:13:22.13,0:13:24.19,Default,,0000,0000,0000,,Là Distol phải không!?
Dialogue: 0,0:13:24.60,0:13:27.86,Default,,0000,0000,0000,,Khá lắm, Cecil. Nghe tiếng chân mà đoán ngay ra ta.
Dialogue: 0,0:13:28.97,0:13:30.94,Default,,0000,0000,0000,,Ngươi muốn gì!?
Dialogue: 0,0:13:31.00,0:13:33.66,Default,,0000,0000,0000,,Hoàng hậu Eliza, bà thấy thế nào?
Dialogue: 0,0:13:33.74,0:13:37.73,Default,,0000,0000,0000,,Distol phải không? Tại sao cậu cầm tù chúng ta...
Dialogue: 0,0:13:38.64,0:13:42.55,Default,,0000,0000,0000,,Ngươi! Ngươi định làm gì chúng ta!?
Dialogue: 0,0:13:42.81,0:13:45.37,Default,,0000,0000,0000,,Yuno, tính khí cô vẫn chẳng thay đổi chút nào.
Dialogue: 0,0:13:45.48,0:13:48.35,Default,,0000,0000,0000,,Lại âm mưu vượt ngục thêm lần nữa...
Dialogue: 0,0:13:48.49,0:13:50.75,Default,,0000,0000,0000,,bằng ma thuật của mình.
Dialogue: 0,0:13:50.82,0:13:54.35,Default,,0000,0000,0000,,Nếu không phải do ả phù thủy kia, ta đã sớm thoát ra rồi...
Dialogue: 0,0:13:57.06,0:14:01.69,Default,,0000,0000,0000,,Distol, ngươi muốn làm gì Yuno và ta cũng được, chỉ xin ngươi hãy buông tha cho Eliza-sama.
Dialogue: 0,0:14:01.77,0:14:04.36,Default,,0000,0000,0000,,Cầu xin ngươi mở lượng nhân từ?
Dialogue: 0,0:14:05.50,0:14:07.37,Default,,0000,0000,0000,,Nhân từ ư?
Dialogue: 0,0:14:08.04,0:14:11.84,Default,,0000,0000,0000,,Đổi lại, ta sẽ trao thân thể mình cho ngươi.
Dialogue: 0,0:14:11.91,0:14:12.97,Default,,0000,0000,0000,,Cecil, đừng!
Dialogue: 0,0:14:13.04,0:14:16.57,Default,,0000,0000,0000,,Cecil, đừng làm chuyện ngu ngốc đó vì ta.
Dialogue: 0,0:14:17.05,0:14:19.24,Default,,0000,0000,0000,,Nhưng thưa Hoàng hậu, nếu chúng ta không...
Dialogue: 0,0:14:19.88,0:14:24.51,Default,,0000,0000,0000,,Tôi sẽ không để cô chịu khổ một mình đâu... Tôi cũng...
Dialogue: 0,0:14:27.83,0:14:29.09,Default,,0000,0000,0000,,Hai người...
Dialogue: 0,0:14:29.19,0:14:37.83,Default,,0000,0000,0000,,Lòng trung thành của hai cô với Hoàng hậu quả là đáng ngưỡng mộ. Tuy nhiên, giá trị của Hoàng hậu đâu thấp như vậy chứ?
Dialogue: 0,0:14:38.97,0:14:39.83,Default,,0000,0000,0000,,Cái gì!
Dialogue: 0,0:14:41.14,0:14:45.30,Default,,0000,0000,0000,,Distol, điều gì đã khiến cậu thay đổi nhanh đến vậy...
Dialogue: 0,0:14:45.84,0:14:48.94,Default,,0000,0000,0000,,Đủ rồi, giải ba người này ra ngoài.
Dialogue: 0,0:14:49.01,0:14:52.28,Default,,0000,0000,0000,,Ta sẽ ban cho các người một ân huệ.
Dialogue: 0,0:14:57.36,0:15:01.02,Default,,0000,0000,0000,,Hẳn các người sẽ rất vui mừng vì điều đó.
Dialogue: 0,0:15:07.37,0:15:09.09,Default,,0000,0000,0000,,Sao lại như vậy...
Dialogue: 0,0:15:13.14,0:15:17.34,Default,,0000,0000,0000,,Thứ lỗi cho sự vô dụng của thuộc hạ, Hoàng hậu Eliza.
Dialogue: 0,0:15:18.18,0:15:21.98,Default,,0000,0000,0000,,Con người khốn kiếp, ta sẽ không tha cho bất kì kẻ nào!
Dialogue: 0,0:15:22.51,0:15:29.08,Default,,0000,0000,0000,,Ba kẻ này là những người đứng đầu Bastarauge, là nguyên nhân khiến các người nhà tan cửa nát.
Dialogue: 0,0:15:29.15,0:15:32.28,Default,,0000,0000,0000,,Hãy trút hết nỗi căm hận lên đầu bọn chúng!
Dialogue: 0,0:15:33.22,0:15:35.78,Default,,0000,0000,0000,,Ngẩng mặt lên, lũ đàn bà khốn kiếp.
Dialogue: 0,0:15:36.53,0:15:37.69,Default,,0000,0000,0000,,Tên chó săn, mau dừng tay!
Dialogue: 0,0:15:37.76,0:15:39.46,Default,,0000,0000,0000,,Giết chúng đi!
Dialogue: 0,0:15:40.50,0:15:45.46,Default,,0000,0000,0000,,Con gái ta đã bị lũ khốn Bastarauge cưỡng bức và giết hại.\NBọn chúng sẽ phải trả giá vì điều đó!
Dialogue: 0,0:15:46.44,0:15:49.37,Default,,0000,0000,0000,,Làm ơn hãy tha cho tôi...
Dialogue: 0,0:15:49.87,0:15:53.97,Default,,0000,0000,0000,,Con điếm này, ngươi không có tư cách lên tiếng ở đây.\NTa sẽ đâm nát âm hộ của ngươi.
Dialogue: 0,0:15:55.21,0:15:56.74,Default,,0000,0000,0000,,Làm đi!
Dialogue: 0,0:15:58.28,0:16:01.81,Default,,0000,0000,0000,,Eliza-sama... dừng tay!
Dialogue: 0,0:16:02.19,0:16:06.02,Default,,0000,0000,0000,,Kẻ như ngươi mà cũng được xưng là Tướng quân ư. Điếm thì muôn đời vẫn là điếm thôi!
Dialogue: 0,0:16:07.02,0:16:08.55,Default,,0000,0000,0000,,Lũ khốn kiếp!
Dialogue: 0,0:16:08.96,0:16:11.05,Default,,0000,0000,0000,,Con khốn Elf này còn dám cãi lại ư!
Dialogue: 0,0:16:15.93,0:16:20.30,Default,,0000,0000,0000,,Khốn kiếp, tuyệt quá đi mất. Âm hộ của loài Elf thật tuyệt làm sao.
Dialogue: 0,0:16:35.02,0:16:40.46,Default,,0000,0000,0000,,Sau khi "an bài" cho ba mĩ nhân kia, phải lập tức trở về cung điện. Chuyện của họ không còn đáng để bận tâm nữa.
Dialogue: 0,0:16:41.63,0:16:47.00,Default,,0000,0000,0000,,Mình cần phải đoạt được bảo vật. Một khi đã nắm tính mạng tên vua chó má đó trong tay, mọi chuyện sẽ rất dễ dàng.
Dialogue: 0,0:16:47.37,0:16:50.36,Default,,0000,0000,0000,,Tất nhiên, mình không việc gì phải giữ lời với tên khốn đó.
Dialogue: 0,0:17:05.18,0:17:06.58,Default,,0000,0000,0000,,Thành công rồi!
Dialogue: 0,0:17:07.52,0:17:12.58,Default,,0000,0000,0000,,Bảo vật cuối cùng đã thuộc về mình, phải mau tới chỗ Elena.
Dialogue: 0,0:17:13.39,0:17:16.72,Default,,0000,0000,0000,,Mình sẽ giải thoát Qoona và thiêu hủy mọi gánh nặng trên vai cô ấy!
Dialogue: 0,0:17:17.76,0:17:21.20,Default,,0000,0000,0000,,Elena, cuối cùng đã tới lúc ta dùng đến nàng.
Dialogue: 0,0:17:23.70,0:17:27.90,Default,,0000,0000,0000,,Đó là... không thể nào!?
Dialogue: 0,0:17:28.91,0:17:34.11,Default,,0000,0000,0000,,Bảo vật nằm trong tay ta đồng nghĩa với sự sụp đổ của Bastarauge.
Dialogue: 0,0:17:35.18,0:17:37.84,Default,,0000,0000,0000,,Hãy phá vỡ phong ấn bên trong báu vật hoàng gia!
Dialogue: 0,0:17:49.16,0:17:50.72,Default,,0000,0000,0000,,15 phút sau.
Dialogue: 0,0:17:52.60,0:17:54.43,Default,,0000,0000,0000,,Kết thúc rồi...
Dialogue: 0,0:17:58.24,0:17:59.33,Default,,0000,0000,0000,,Phải không?
Dialogue: 0,0:17:59.40,0:18:00.10,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:18:00.17,0:18:01.64,Default,,0000,0000,0000,,Eleanor, cô ở đâu?
Dialogue: 0,0:18:01.71,0:18:03.33,Default,,0000,0000,0000,,Phong ấn đã bị phá vỡ.
Dialogue: 0,0:18:03.91,0:18:06.00,Default,,0000,0000,0000,,Eleanor, hiện thân đi!
Dialogue: 0,0:18:06.08,0:18:11.95,Default,,0000,0000,0000,,Qoona đã được tự do, cùng với Chủ nhân đích thực của ta...
Dialogue: 0,0:18:12.42,0:18:15.82,Default,,0000,0000,0000,,Chủ nhân đích thực? Cô vừa nói gì, Eleanor!?
Dialogue: 0,0:18:18.69,0:18:21.66,Default,,0000,0000,0000,,Thôi bỏ đi, mục đích của ta đã đạt được.
Dialogue: 0,0:18:21.73,0:18:26.16,Default,,0000,0000,0000,,Dù phải tới tận chân trời góc bể... Ta cũng quyết tìm ra em, Qoona!
Dialogue: 0,0:18:31.50,0:18:32.70,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:18:32.77,0:18:35.07,Default,,0000,0000,0000,,Khoan đã, chuyện gì vậy?
Dialogue: 0,0:18:35.17,0:18:37.97,Default,,0000,0000,0000,,Ta không rõ. Ta sẽ đi kiếm tra ngay lập tức.
Dialogue: 0,0:18:38.48,0:18:43.74,Default,,0000,0000,0000,,Ngu ngốc, chàng phá bỏ phong ấn mà không hề nghĩ tới hậu quả sao?
Dialogue: 0,0:18:43.85,0:18:46.82,Default,,0000,0000,0000,,Hãy để em theo chàng tới đó.
Dialogue: 0,0:18:46.92,0:18:47.98,Default,,0000,0000,0000,,Elena?
Dialogue: 0,0:18:48.05,0:18:53.55,Default,,0000,0000,0000,,Là một phần của phong ấn, em cần phải biết.
Dialogue: 0,0:18:58.03,0:18:59.43,Default,,0000,0000,0000,,Đi nào, Elena.
Dialogue: 0,0:19:00.70,0:19:02.93,Default,,0000,0000,0000,,Bám chắc vào!
Dialogue: 0,0:19:03.23,0:19:04.17,Default,,0000,0000,0000,,Em biết rồi!
Dialogue: 0,0:19:04.87,0:19:06.70,Default,,0000,0000,0000,,Qoona, hi vọng em vẫn bình an.
Dialogue: 0,0:19:07.77,0:19:10.71,Default,,0000,0000,0000,,Lạ quá, ngọn tháp biến đâu mất rồi?
Dialogue: 0,0:19:10.78,0:19:12.83,Default,,0000,0000,0000,,Đáng nhẽ ta phải nhìn thấy nó rồi chứ...
Dialogue: 0,0:19:13.68,0:19:15.48,Default,,0000,0000,0000,,Di, chúng ta phải nhanh lên.
Dialogue: 0,0:19:15.55,0:19:16.27,Default,,0000,0000,0000,,Anh biết!
Dialogue: 0,0:19:26.06,0:19:27.32,Default,,0000,0000,0000,,Tòa tháp?
Dialogue: 0,0:19:27.43,0:19:29.09,Default,,0000,0000,0000,,Chuyện gì vậy!?
Dialogue: 0,0:19:31.10,0:19:33.00,Default,,0000,0000,0000,,Thế này là sao!?
Dialogue: 0,0:19:33.80,0:19:35.96,Default,,0000,0000,0000,,Dường như có bàn tay của Ma quỷ trong chuyện này.
Dialogue: 0,0:19:36.43,0:19:37.96,Default,,0000,0000,0000,,Elena, ở yên đó.
Dialogue: 0,0:19:38.64,0:19:40.13,Default,,0000,0000,0000,,N-Nhưng mà...
Dialogue: 0,0:19:40.20,0:19:42.07,Default,,0000,0000,0000,,Đừng đi đâu cả!
Dialogue: 0,0:19:42.44,0:19:43.74,Default,,0000,0000,0000,,Di!!!!
Dialogue: 0,0:19:44.48,0:19:49.88,Default,,0000,0000,0000,,Nếu là tên yêu quái đó... Vậy Qoona đã đi đâu?
Dialogue: 0,0:19:49.95,0:19:54.08,Default,,0000,0000,0000,,Qoona!
Dialogue: 0,0:19:54.62,0:19:57.02,Default,,0000,0000,0000,,Ngài tới đây nhanh hơn ta tưởng đó.
Dialogue: 0,0:19:57.79,0:19:58.91,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:19:58.99,0:20:01.98,Default,,0000,0000,0000,,Đừng lo, Qoona vẫn ổn.
Dialogue: 0,0:20:03.29,0:20:05.79,Default,,0000,0000,0000,,Eleanor, vậy nghĩa là sao!?
Dialogue: 0,0:20:06.26,0:20:14.23,Default,,0000,0000,0000,,Ngài đã hoàn thành công việc của mình, Qoona đã được giải thoát,\Ntất cả chỉ có vậy.
Dialogue: 0,0:20:14.67,0:20:16.83,Default,,0000,0000,0000,,Vậy sự hỗn loạn này là sao?
Dialogue: 0,0:20:16.91,0:20:18.84,Default,,0000,0000,0000,,Ngươi không hề cho ta biết chuyện này!
Dialogue: 0,0:20:20.51,0:20:25.95,Default,,0000,0000,0000,,Bản giao ước chỉ nhắc tới việc cứu Qoona.
Dialogue: 0,0:20:26.02,0:20:29.82,Default,,0000,0000,0000,,Những chuyện khác không hề được nói đến.
Dialogue: 0,0:20:30.52,0:20:32.96,Default,,0000,0000,0000,,Qoona của ngài đây.
Dialogue: 0,0:20:34.56,0:20:35.46,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:20:36.36,0:20:38.02,Default,,0000,0000,0000,,Nii-sama...
Dialogue: 0,0:20:38.10,0:20:40.00,Default,,0000,0000,0000,,Em không sao chứ, có bị thương chỗ nào không?
Dialogue: 0,0:20:40.97,0:20:45.60,Default,,0000,0000,0000,,Ánh sáng ở khắp nơi... căn phòng dường như đảo lộn, và em...
Dialogue: 0,0:20:45.94,0:20:48.13,Default,,0000,0000,0000,,Đừng lo, em an toàn rồi.
Dialogue: 0,0:20:48.21,0:20:52.40,Default,,0000,0000,0000,,Con người, ngươi làm tốt lắm.
Dialogue: 0,0:20:53.08,0:20:55.17,Default,,0000,0000,0000,,Hóa ra là ngươi!
Dialogue: 0,0:20:55.25,0:21:00.95,Default,,0000,0000,0000,,Lẽ ra ta có thể giết chết ngươi ngay lúc này.
Dialogue: 0,0:21:01.02,0:21:07.72,Default,,0000,0000,0000,,Nhưng sự hồi sinh của ta phần lớn là nhờ vào ngươi, ta tặng cho ngươi vương quốc nhỏ bé kia.
Dialogue: 0,0:21:10.56,0:21:16.50,Default,,0000,0000,0000,,Hoang đường! Ngươi là kẻ thuộc về quá khứ, ta sẽ giúp ngươi an nghỉ vĩnh viễn!
Dialogue: 0,0:21:18.74,0:21:20.03,Default,,0000,0000,0000,,Ngu ngốc!
Dialogue: 0,0:21:25.04,0:21:30.81,Default,,0000,0000,0000,,Ra vậy, năng lượng do chính ta ban tặng vừa cứu mạng ngươi, thú vị thật.
Dialogue: 0,0:21:31.22,0:21:32.08,Default,,0000,0000,0000,,Nii-sama!
Dialogue: 0,0:21:32.15,0:21:33.05,Default,,0000,0000,0000,,Ở nguyên đó.
Dialogue: 0,0:21:35.79,0:21:36.75,Default,,0000,0000,0000,,Qoona!
Dialogue: 0,0:21:36.82,0:21:41.22,Default,,0000,0000,0000,,Ngươi thật ngu ngốc khi dám chống lại chủ nhân của ta. Đây là hình phạt dành cho ngươi.
Dialogue: 0,0:21:41.73,0:21:45.32,Default,,0000,0000,0000,,Eleanor, theo ta nào. Chúng ta sẽ hồi sinh đế chế cổ đại của ta.
Dialogue: 0,0:21:45.40,0:21:48.39,Default,,0000,0000,0000,,Ma quỷ sẽ nhấn chìm thế giới này trong hỗn loạn.
Dialogue: 0,0:21:48.67,0:21:50.10,Default,,0000,0000,0000,,Tuân lệnh.
Dialogue: 0,0:21:51.24,0:21:52.90,Default,,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:21:53.94,0:21:55.87,Default,,0000,0000,0000,,Di, chàng không sao chứ?
Dialogue: 0,0:21:55.94,0:21:58.03,Default,,0000,0000,0000,,Di, đừng cố chấp nữa.
Dialogue: 0,0:21:58.88,0:22:01.04,Default,,0000,0000,0000,,Qoona!
Dialogue: 0,0:22:01.95,0:22:03.00,Default,,0000,0000,0000,,Một lời nguyền ư?
Dialogue: 0,0:22:03.08,0:22:04.31,Default,,0000,0000,0000,,Qoona!
Dialogue: 0,0:22:05.35,0:22:06.51,Default,,0000,0000,0000,,Qoona!
Dialogue: 0,0:22:06.58,0:22:08.92,Default,,0000,0000,0000,,Tỉnh lại đi, Qoona!
Dialogue: 0,0:22:11.12,0:22:13.75,Default,,0000,0000,0000,,Qoona!
Dialogue: 0,0:22:14.76,0:22:18.42,Default,,0000,0000,0000,,Vài ngày sau, thế giới chìm trong hỗn loạn và sợ hãi.
Dialogue: 0,0:22:18.50,0:22:23.49,Default,,0000,0000,0000,,Sự hồi sinh của Ma Vương cũng lũ thuộc hạ.
Dialogue: 0,0:22:23.57,0:22:29.60,Default,,0000,0000,0000,,Không một Vươn quốc nào đủ sức cầm cự với Ma Vương lâu dài.
Dialogue: 0,0:22:29.67,0:22:33.91,Default,,0000,0000,0000,,Giáo hội nhanh chóng sụp đổ.
Dialogue: 0,0:22:35.45,0:22:41.39,Default,,0000,0000,0000,,Có tin đồn Ma Vương đang âm mưu thôn tính Viễn Đông.
Dialogue: 0,0:22:42.19,0:22:46.09,Default,,0000,0000,0000,,Loài người đang đứng trước nguy cơ diệt vong.
Dialogue: 0,0:22:46.16,0:22:49.75,Default,,0000,0000,0000,,Tất cả đều do...
Dialogue: 0,0:22:51.63,0:22:52.36,Default,,0000,0000,0000,,Di.
Dialogue: 0,0:22:52.80,0:22:58.90,Default,,0000,0000,0000,,Elena, hôm nay trời lạnh. Nàng sẽ cảm lạnh mất.
Dialogue: 0,0:22:59.77,0:23:01.10,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:23:01.84,0:23:05.54,Default,,0000,0000,0000,,Asteria đang tìm cách để ngăn chặn thảm họa này.
Dialogue: 0,0:23:05.61,0:23:08.24,Default,,0000,0000,0000,,Tuy nhiên, những người sống sót lũ lượt kéo tới thành phố.
Dialogue: 0,0:23:08.81,0:23:14.01,Default,,0000,0000,0000,,Mọi chuyện vẫn được êm đẹp cho tới giờ là nhờ trí thông minh và tài lãnh đạo Elena.
Dialogue: 0,0:23:14.55,0:23:15.81,Default,,0000,0000,0000,,Chàng vẫn muốn đi ư?
Dialogue: 0,0:23:15.89,0:23:21.09,Default,,0000,0000,0000,,Uh, Chỉ khi tiêu diệt tất cả bọn chúng, lời nguyền trên người Qoona mới bị phá giải.
Dialogue: 0,0:23:21.66,0:23:23.79,Default,,0000,0000,0000,,Chàng nói đúng...
Dialogue: 0,0:23:24.93,0:23:29.80,Default,,0000,0000,0000,,Em sẽ không ngăn cản chàng. Em sẽ thay chàng trông coi vương quốc và em gái, cho tới khi chàng quay về.
Dialogue: 0,0:23:29.87,0:23:32.03,Default,,0000,0000,0000,,Xin lỗi nàng vì tất cả.
Dialogue: 0,0:23:32.90,0:23:33.84,Default,,0000,0000,0000,,Đừng khóc.
Dialogue: 0,0:23:34.27,0:23:36.21,Default,,0000,0000,0000,,Chàng cầm lấy đi.
Dialogue: 0,0:23:37.47,0:23:38.87,Default,,0000,0000,0000,,Lá bùa may mắn.
Dialogue: 0,0:23:38.94,0:23:39.91,Default,,0000,0000,0000,,Nó sẽ có ích.
Dialogue: 0,0:23:39.98,0:23:41.74,Default,,0000,0000,0000,,Ờ, ta biết.
Dialogue: 0,0:23:42.21,0:23:44.04,Default,,0000,0000,0000,,Làm ơn hãy sống sót trở về.
Dialogue: 0,0:23:44.11,0:23:47.64,Default,,0000,0000,0000,,Vì em gái chàng, và vì em...
Dialogue: 0,0:23:48.85,0:23:50.34,Default,,0000,0000,0000,,Ta hứa.
Dialogue: 0,0:23:50.42,0:23:51.48,Default,,0000,0000,0000,,Distol...
Dialogue: 0,0:23:55.03,0:23:58.83,Default,,0000,0000,0000,,Distol, làm ơn hãy ái ân cùng em trước khi chàng đi.
Dialogue: 0,0:23:58.90,0:24:01.06,Default,,0000,0000,0000,,Tất nhiên rồi. Cho tới khi nàng thỏa mãn.
Dialogue: 0,0:24:01.13,0:24:03.46,Default,,0000,0000,0000,,Em hạnh phúc lắm, Distol.
Dialogue: 0,0:24:34.67,0:24:36.60,Default,,0000,0000,0000,,Di, đừng chết...
Dialogue: 0,0:24:37.47,0:24:40.96,Default,,0000,0000,0000,,Năm 359, Thiên Lang Yểm Nguyệt.
Dialogue: 0,0:24:41.04,0:24:44.24,Default,,0000,0000,0000,,Trận chiến mới chỉ bắt đầu.
Dialogue: 0,0:24:45.84,0:24:49.64,Default,,0000,0000,0000,,Hết
