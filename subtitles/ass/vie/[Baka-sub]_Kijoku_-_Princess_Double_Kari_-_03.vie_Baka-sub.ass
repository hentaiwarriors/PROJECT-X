Dialogue: 0,0:00:29.09,0:00:32.42,Default,,0000,0000,0000,,Một vùng đất khác của Bastarauge vừa bị chinh phục...
Dialogue: 0,0:00:32.96,0:00:35.02,Default,,0000,0000,0000,,Ta sẽ lui quân khi thời cơ chín muồi.
Dialogue: 0,0:00:35.09,0:00:36.15,Default,,0000,0000,0000,,Tâu Hoàng tử...
Dialogue: 0,0:00:37.66,0:00:40.60,Default,,0000,0000,0000,,Trưởng làng có một báu vật.
Dialogue: 0,0:00:41.13,0:00:42.69,Default,,0000,0000,0000,,Ra vậy, đưa ta xem.
Dialogue: 0,0:00:42.77,0:00:43.83,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:00:44.70,0:00:45.86,Default,,0000,0000,0000,,Đây là?
Dialogue: 0,0:00:45.94,0:00:51.97,Default,,0000,0000,0000,,Theo lời ông ta, nếu ai đủ sức nắm giữ vật này, người đó sẽ sai khiến được quái vật theo ý muốn của mình.
Dialogue: 0,0:00:52.64,0:00:53.54,Default,,0000,0000,0000,,Quái vật ư?
Dialogue: 0,0:00:53.61,0:00:54.44,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:00:54.51,0:00:57.48,Default,,0000,0000,0000,,Phát hiện này đáng giá đây. Ta sẽ ban thưởng cho ngươi sau.
Dialogue: 0,0:00:57.55,0:00:58.64,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:00:59.65,0:01:01.59,Default,,0000,0000,0000,,Mình sẽ cần tới thứ này.
Dialogue: 0,0:01:03.36,0:01:07.35,Default,,0000,0000,0000,,Để cứu được Qoona, mình buộc phải làm cho Elena sa ngã.
Dialogue: 0,0:01:07.86,0:01:11.59,Default,,0000,0000,0000,,Dù đoạt được báu vật của Bastarauge, mình vẫn không thể cứu được Qoona
Dialogue: 0,0:01:11.66,0:01:17.10,Default,,0000,0000,0000,,nếu chưa phá vỡ được phong ấn trong cơ thể Elena.
Dialogue: 0,0:01:18.90,0:01:22.43,Default,,0000,0000,0000,,Khi nắm chặt thứ này và nghĩ về Elena.
Dialogue: 0,0:01:24.14,0:01:27.60,Default,,0000,0000,0000,,Không biết mình sẽ được chiêm ngưỡng cảnh tượng thú vị nào đây...
Dialogue: 0,0:01:32.25,0:01:34.38,Default,,0000,0000,0000,,Sau mỗi trận chiến, ngươi càng lúc càng mạnh hơn.
Dialogue: 0,0:01:34.49,0:01:40.43,Default,,0000,0000,0000,,Chấp nhận ta, ngươi sẽ có mọi thứ mình muốn.
Dialogue: 0,0:01:41.16,0:01:42.79,Default,,0000,0000,0000,,Hãy chấp nhận ta.
Dialogue: 0,0:01:44.60,0:01:46.79,Default,,0000,0000,0000,,Tin rằng mình có thể làm được điều này.
Dialogue: 0,0:01:46.87,0:01:48.80,Default,,0000,0000,0000,,Triệu hồi quái vật.
Dialogue: 0,0:01:54.24,0:01:55.57,Default,,0000,0000,0000,,Hãy phục tùng ta.
Dialogue: 0,0:02:01.43,0:02:14.15,Type,,0000,0000,0000,,{\p1\c&H000000&\fscx1031.25\fscy618.75\pos(543,219)}m 1 -14 l -21 -14 l -21 3 l 1 3
