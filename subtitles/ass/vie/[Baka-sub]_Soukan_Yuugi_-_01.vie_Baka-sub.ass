Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:29.50,0:00:32.47,Default,,0000,0000,0000,,Mẹ ơi, Mẹ, Mom, âm hộ của mẹ.
Dialogue: 0,0:00:32.97,0:00:36.10,Default,,0000,0000,0000,,Tuyệt quá đi mất! Thằng nhỏ của con bị nó nuốt trọn, và...
Dialogue: 0,0:00:36.28,0:00:37.74,Default,,0000,0000,0000,,...Con phát điên mất!
Dialogue: 0,0:00:38.44,0:00:41.07,Default,,0000,0000,0000,,Mình đang định làm gì nhỉ?
Dialogue: 0,0:00:41.21,0:00:46.21,Default,,0000,0000,0000,,À phải rồi, mình phải nấu xong bữa tối trước lúc Yuri đi học về.
Dialogue: 0,0:00:53.26,0:00:56.85,Default,,0000,0000,0000,,Ngực...ngực của mẹ...Mẹ ơi, ngực của mẹ tuyệt quá.
Dialogue: 0,0:01:00.50,0:01:03.13,Default,,0000,0000,0000,,Nấu gì cho bữa tối bây giờ nhỉ?
Dialogue: 0,0:01:03.50,0:01:06.70,Default,,0000,0000,0000,,Gần đây Yuri gặp vấn đề với những thức ăn nhiều đạm...
Dialogue: 0,0:01:06.77,0:01:09.24,Default,,0000,0000,0000,,...vì vậy con bé cần nhiều chất sơ để tránh bị khó tiêu sau khi ăn...
Dialogue: 0,0:01:09.31,0:01:13.40,Default,,0000,0000,0000,,Con bé đang trong tuổi phát triển, vì vậy nó rất mau đói và hay ăn vặt...
Dialogue: 0,0:01:13.48,0:01:16.14,Default,,0000,0000,0000,,...tăng cân là chuyện ko thể tránh khỏi...
Dialogue: 0,0:01:16.32,0:01:20.88,Default,,0000,0000,0000,,Uhm, khi mình bằng tuổi con bé, mình cũng gặp phải những vấn đề như vậy.
Dialogue: 0,0:01:20.95,0:01:22.15,Default,,0000,0000,0000,,Mẹ ơi!
Dialogue: 0,0:01:22.79,0:01:26.55,Default,,0000,0000,0000,,Mẹ, con ko nhịn nổi nữa rồi. Con sẽ tè ra mất.
Dialogue: 0,0:01:26.66,0:01:29.22,Default,,0000,0000,0000,,Ko! Ko được, nếu con làm vậy...
Dialogue: 0,0:01:29.83,0:01:32.85,Default,,0000,0000,0000,,Nhưng mà...Con đang cố để rút nó ra, mà nó cứ...
Dialogue: 0,0:01:33.97,0:01:35.83,Default,,0000,0000,0000,,Mông con cứ tự động di chuyển...
Dialogue: 0,0:01:35.90,0:01:38.06,Default,,0000,0000,0000,,...cảm giác tuyệt diệu đó khiến con ko tài nào rút nó ra được!
Dialogue: 0,0:01:38.27,0:01:40.33,Default,,0000,0000,0000,,Chuyện gì đang xảy ra thế này...
Dialogue: 0,0:01:40.41,0:01:42.64,Default,,0000,0000,0000,,Tại sao mình lại làm chuyện này cùng Yu-chan chứ...
Dialogue: 0,0:02:09.97,0:02:11.80,Default,,0000,0000,0000,,Con tè ra mất... Khó chịu quá...
Dialogue: 0,0:02:12.44,0:02:14.23,Default,,0000,0000,0000,,Thằng nhỏ của con sắp nổ tung rồi...
Dialogue: 0,0:02:15.37,0:02:16.34,Default,,0000,0000,0000,,Con ko thể nhịn nổi nữa rồi!
Dialogue: 0,0:02:16.84,0:02:19.64,Default,,0000,0000,0000,,Yu-chan, ko được...ko được làm thế...
Dialogue: 0,0:02:20.28,0:02:22.30,Default,,0000,0000,0000,,Đừng tè trong người mẹ!
Dialogue: 0,0:02:29.86,0:02:30.82,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:02:43.10,0:02:46.00,Default,,0000,0000,0000,,Thằng nhỏ của Yu-chan...tuyệt quá đi mất...
Dialogue: 0,0:02:46.07,0:02:47.27,Default,,0000,0000,0000,,Nó vẫn tiếp tục bự dần lên...
Dialogue: 0,0:02:47.34,0:02:49.33,Default,,0000,0000,0000,,Ko...ko...
Dialogue: 0,0:02:57.05,0:02:58.07,Default,,0000,0000,0000,,Con ra đây!
Dialogue: 0,0:02:59.42,0:03:03.05,Default,,0000,0000,0000,,K-Ko!
Dialogue: 0,0:03:08.03,0:03:09.15,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:03:09.56,0:03:13.76,Default,,0000,0000,0000,,Mông của con...con ko cách nào ngừng nó lại được...
Dialogue: 0,0:03:13.83,0:03:17.27,Default,,0000,0000,0000,,Và rồi...cảm giác tuyệt diệu đó cứ tăng dần lên, thế nên...
Dialogue: 0,0:03:18.37,0:03:20.36,Default,,0000,0000,0000,,Con xin lỗi...Mẹ...
Dialogue: 0,0:03:20.44,0:03:22.91,Default,,0000,0000,0000,,Con đã tè ra người mẹ mất rồi...
Dialogue: 0,0:03:27.31,0:03:30.75,Default,,0000,0000,0000,,Tại sao...chuyện này lại xảy ra...tại sao chứ...
Dialogue: 0,0:03:34.59,0:03:37.45,Default,,0000,0000,0000,,Coi nào Yuji, nhanh tay lên nào.
Dialogue: 0,0:03:37.52,0:03:40.05,Default,,0000,0000,0000,,Em làm gì cũng chậm như rùa ấy.
Dialogue: 0,0:03:40.33,0:03:40.72,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:03:41.26,0:03:42.42,Default,,0000,0000,0000,,Chị...
Dialogue: 0,0:03:42.53,0:03:46.46,Default,,0000,0000,0000,,...chị bỏ tật vừa nói vừa dùng tay đánh vào đầu em hộ cái.
Dialogue: 0,0:03:46.57,0:03:49.80,Default,,0000,0000,0000,,Chị khỏe như voi ấy, em chịu sao nổi.
Dialogue: 0,0:03:49.87,0:03:52.43,Default,,0000,0000,0000,,"Đánh" là sao hả.
Dialogue: 0,0:03:52.50,0:03:54.60,Default,,0000,0000,0000,,Chị mới chạm nhẹ lên đầu em, điêu vừa thôi chứ?
Dialogue: 0,0:03:54.87,0:03:56.84,Default,,0000,0000,0000,,Phải thế này...
Dialogue: 0,0:03:57.24,0:03:58.04,Default,,0000,0000,0000,,... mới gọi là đánh nè!
Dialogue: 0,0:03:58.94,0:04:03.04,Default,,0000,0000,0000,,Nhát gan như em thì có bạn gái sao nổi, huh?
Dialogue: 0,0:04:03.62,0:04:07.35,Default,,0000,0000,0000,,Còn chị, chị cứ dữ dằn như vậy thì kiếm đâu ra bạn trai...
Dialogue: 0,0:04:07.52,0:04:11.79,Default,,0000,0000,0000,,Em nói thật chứ, tay chị to thế kia...
Dialogue: 0,0:04:11.86,0:04:15.05,Default,,0000,0000,0000,,...chạm nhẹ vào ai là người đó vô nhà thương liền.
Dialogue: 0,0:04:15.29,0:04:17.06,Default,,0000,0000,0000,,Khi đó cảnh sát sẽ viếng thăm chị, và rồi...
Dialogue: 0,0:04:17.13,0:04:20.90,Default,,0000,0000,0000,,...tuổi xuân của chị sẽ trôi qua sau song sắt nhà tù...
Dialogue: 0,0:04:21.00,0:04:23.59,Default,,0000,0000,0000,,Chị nhớ nhé, phải thật chú ý, kẻo ko lại làm ai đó phải vô viện thì khổ...
Dialogue: 0,0:04:23.67,0:04:25.26,Default,,0000,0000,0000,,...phải thật cẩn thận.
Dialogue: 0,0:04:25.50,0:04:26.47,Default,,0000,0000,0000,,Yuji...
Dialogue: 0,0:04:26.74,0:04:31.18,Default,,0000,0000,0000,,Mày có muốn chị cho mày vô nhà thương mà tiếp tục nói nhảm ko hả!?
Dialogue: 0,0:04:31.24,0:04:32.27,Default,,0000,0000,0000,,Ấy-ấy ko...
Dialogue: 0,0:04:32.34,0:04:36.18,Default,,0000,0000,0000,,...người chị hiền lành dễ thương tốt bụng nhất trên đời ko bao giờ dùng bạo lực với em trai mình cả...
Dialogue: 0,0:04:36.38,0:04:37.35,Default,,0000,0000,0000,,Phải ko chị?
Dialogue: 0,0:04:37.42,0:04:39.18,Default,,0000,0000,0000,,Um... chị mày đâu có dùng bạo lực...
Dialogue: 0,0:04:39.25,0:04:42.08,Default,,0000,0000,0000,,Mày chả bảo chị chỉ đùa thôi...
Dialogue: 0,0:04:42.15,0:04:47.32,Default,,0000,0000,0000,,...cũng đủ khiến ai đó vô nhà thương, vậy nên...
Dialogue: 0,0:04:47.53,0:04:48.72,Default,,0000,0000,0000,,Thôi nào, thôi nào...
Dialogue: 0,0:04:49.16,0:04:51.86,Default,,0000,0000,0000,,Cả hai con đều ở cùng một lứa tuổi...
Dialogue: 0,0:04:51.93,0:04:55.99,Default,,0000,0000,0000,,Khoa học đã chứng minh chị em ruột cùng lứa tuổi thì luôn luôn xung đột với nhau...
Dialogue: 0,0:04:56.10,0:04:58.93,Default,,0000,0000,0000,,Nhân tiện, hai con đã nhớ ra mình đang làm gì rồi chứ?
Dialogue: 0,0:04:59.37,0:05:01.70,Default,,0000,0000,0000,,Sắp đến giờ vào học rồi, phải ko bà chị?
Dialogue: 0,0:05:02.01,0:05:02.91,Default,,0000,0000,0000,,Phải rồi...
Dialogue: 0,0:05:02.97,0:05:05.84,Default,,0000,0000,0000,,Chào mẹ, bọn con đi học đây!
Dialogue: 0,0:05:06.68,0:05:08.74,Default,,0000,0000,0000,,Uhm, chào các con.
Dialogue: 0,0:05:09.05,0:05:11.04,Default,,0000,0000,0000,,Đi đường cẩn thận nhé.
Dialogue: 0,0:05:30.04,0:05:31.47,Default,,0000,0000,0000,,Ai đắp mền cho mình vậy?
Dialogue: 0,0:05:40.05,0:05:41.38,Default,,0000,0000,0000,,Yu-chan, con có ở đó ko?
Dialogue: 0,0:05:41.51,0:05:42.98,Default,,0000,0000,0000,,Hmm...
Dialogue: 0,0:05:43.58,0:05:45.74,Default,,0000,0000,0000,,Hướng dẫn kiểu gì vậy nhỉ?
Dialogue: 0,0:05:46.02,0:05:47.38,Default,,0000,0000,0000,,Mình chả hiểu gì cả...
Dialogue: 0,0:05:48.89,0:05:50.08,Default,,0000,0000,0000,,Chuyện gì vậy, mẹ?
Dialogue: 0,0:05:50.89,0:05:52.05,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:05:52.19,0:05:53.16,Default,,0000,0000,0000,,Um...
Dialogue: 0,0:05:53.76,0:05:56.75,Default,,0000,0000,0000,,Chiếc mền này...là con...
Dialogue: 0,0:05:57.06,0:05:58.03,Default,,0000,0000,0000,,Um...
Dialogue: 0,0:05:58.10,0:05:59.32,Default,,0000,0000,0000,,Yu-chan, là con hả?
Dialogue: 0,0:05:59.40,0:06:00.16,Default,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:06:00.23,0:06:01.13,Default,,0000,0000,0000,,Cám ơn con.
Dialogue: 0,0:06:01.43,0:06:03.33,Default,,0000,0000,0000,,Mẹ quên ko đóng cửa sổ...
Dialogue: 0,0:06:03.64,0:06:06.83,Default,,0000,0000,0000,,...khi trời tối, gió lạnh sẽ lùa vào.
Dialogue: 0,0:06:07.04,0:06:08.27,Default,,0000,0000,0000,,Mẹ sẽ bị cảm lạnh mất.
Dialogue: 0,0:06:10.38,0:06:11.50,Default,,0000,0000,0000,,T-Thôi nào.
Dialogue: 0,0:06:11.58,0:06:13.44,Default,,0000,0000,0000,,Mẹ mau nước mắt quá đấy.
Dialogue: 0,0:06:13.61,0:06:15.51,Default,,0000,0000,0000,,Mẹ đang mệt phải ko?
Dialogue: 0,0:06:15.95,0:06:18.75,Default,,0000,0000,0000,,Uh, có lẽ vậy. Mẹ già mất rồi.
Dialogue: 0,0:06:19.15,0:06:21.88,Default,,0000,0000,0000,,C-Con thì lại ko nghĩ như vậy.
Dialogue: 0,0:06:22.02,0:06:25.25,Default,,0000,0000,0000,,Bởi vì...sau nhiều năm mà mẹ vẫn trẻ và đẹp...
Dialogue: 0,0:06:25.32,0:06:26.59,Default,,0000,0000,0000,,...như thời con gái...
Dialogue: 0,0:06:26.89,0:06:27.88,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:06:34.80,0:06:38.29,Default,,0000,0000,0000,,U-Uhm, mẹ phải đi làm bữa tối bây giờ...
Dialogue: 0,0:06:38.37,0:06:41.40,Default,,0000,0000,0000,,...Yuri sẽ rất đói sau khi đi học về...
Dialogue: 0,0:06:41.61,0:06:43.01,Default,,0000,0000,0000,,V-Vâng...
Dialogue: 0,0:06:43.07,0:06:46.70,Default,,0000,0000,0000,,Mặc dù chị ấy sợ tăng cân, chị ấy vẫn ăn rất nhiều...
Dialogue: 0,0:06:47.98,0:06:49.04,Default,,0000,0000,0000,,Đ-Đúng vậy...
Dialogue: 0,0:06:49.11,0:06:49.55,Default,,0000,0000,0000,,À phải rồi!
Dialogue: 0,0:06:49.55,0:06:50.34,Default,,0000,0000,0000,,Hả! Chuyện gì vậy?
Dialogue: 0,0:06:50.42,0:06:54.68,Default,,0000,0000,0000,,À. Con muốn nhờ mẹ một chuyện.
Dialogue: 0,0:06:56.39,0:06:57.95,Default,,0000,0000,0000,,Oh, là chuyện gì vậy?
Dialogue: 0,0:06:58.39,0:07:01.33,Default,,0000,0000,0000,,Lâu rồi mới thấy con nhờ mẹ giúp đây.
Dialogue: 0,0:07:02.43,0:07:04.33,Default,,0000,0000,0000,,Là về thứ này...
Dialogue: 0,0:07:07.00,0:07:08.43,Default,,0000,0000,0000,,Thứ gì vậy?
Dialogue: 0,0:07:08.60,0:07:10.66,Default,,0000,0000,0000,,Um, biết nói thế nào bây giờ nhỉ...
Dialogue: 0,0:07:10.90,0:07:11.89,Default,,0000,0000,0000,,Đó là...
Dialogue: 0,0:07:11.97,0:07:12.96,Default,,0000,0000,0000,,Hmm?
Dialogue: 0,0:07:14.44,0:07:16.17,Default,,0000,0000,0000,,T-Thứ này...
Dialogue: 0,0:07:16.27,0:07:19.84,Default,,0000,0000,0000,,...được một nhân viên tiếp thị tặng cho con trên đường về nhà.
Dialogue: 0,0:07:20.48,0:07:24.24,Default,,0000,0000,0000,,Anh ta nói bộ trò chơi này sẽ giúp những người chơi gần gũi nhau hơn.
Dialogue: 0,0:07:24.75,0:07:27.55,Default,,0000,0000,0000,,Anh ta vô cùng quả quyết về chuyện đó...
Dialogue: 0,0:07:27.89,0:07:30.48,Default,,0000,0000,0000,,...vì thế con đã nhận nó...
Dialogue: 0,0:07:30.96,0:07:32.82,Default,,0000,0000,0000,,Trò chơi này cần ít nhất hai người, vì vậy...
Dialogue: 0,0:07:33.59,0:07:34.75,Default,,0000,0000,0000,,Hmm...
Dialogue: 0,0:07:34.96,0:07:38.92,Default,,0000,0000,0000,,Vì vậy, con muốn mẹ chơi thử trò này cùng con...nếu mẹ ko phiền...
Dialogue: 0,0:07:39.60,0:07:42.19,Default,,0000,0000,0000,,Um, thôi bỏ đi...cứ coi như con chưa nói gì cả.
Dialogue: 0,0:07:42.40,0:07:44.80,Default,,0000,0000,0000,,...Trò gieo xúc xắc này thật nhảm nhí, phải ko ạ?
Dialogue: 0,0:07:45.67,0:07:47.00,Default,,0000,0000,0000,,Được mà...
Dialogue: 0,0:07:47.07,0:07:49.77,Default,,0000,0000,0000,,Lâu rồi hai mẹ con ta chưa chơi xúc xắc, phải ko nhỉ?
Dialogue: 0,0:07:50.18,0:07:51.14,Default,,0000,0000,0000,,Thế ạ?
Dialogue: 0,0:07:51.28,0:07:52.81,Default,,0000,0000,0000,,Ngay cả trong dịp Tết cũng ko ạ?
Dialogue: 0,0:07:52.81,0:07:54.64,Default,,0000,0000,0000,,Phải, trò này cũng đâu tốn nhiều thời gian.
Dialogue: 0,0:07:54.71,0:07:55.65,Default,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:07:56.78,0:07:58.51,Default,,0000,0000,0000,,Umm...
Dialogue: 0,0:08:02.32,0:08:03.62,Default,,0000,0000,0000,,Để mẹ xem nào.
Dialogue: 0,0:08:03.82,0:08:05.55,Default,,0000,0000,0000,,M-Mẹ...n-này...
Dialogue: 0,0:08:05.62,0:08:06.49,Default,,0000,0000,0000,,Hm?
Dialogue: 0,0:08:06.56,0:08:07.72,Default,,0000,0000,0000,,Mẹ áp vào gần con quá đấy.
Dialogue: 0,0:08:11.20,0:08:14.43,Default,,0000,0000,0000,,Oh, Yu-chan...con ko thích gần gũi mẹ sao.
Dialogue: 0,0:08:14.50,0:08:16.26,Default,,0000,0000,0000,,Con đâu có ý đó!
Dialogue: 0,0:08:16.33,0:08:18.86,Default,,0000,0000,0000,,Vậy hả? Hi vọng là con ko nói dối mẹ.
Dialogue: 0,0:08:19.00,0:08:20.77,Default,,0000,0000,0000,,Thôi bỏ đi, mẹ coi này.
Dialogue: 0,0:08:22.64,0:08:23.66,Default,,0000,0000,0000,,Trên đó viết gì thế?
Dialogue: 0,0:08:24.04,0:08:27.64,Default,,0000,0000,0000,,''Gieo xúc xắc, nếu bạn tiến vào ô đỏ, hãy rút một tấm thẻ''...
Dialogue: 0,0:08:27.71,0:08:29.37,Default,,0000,0000,0000,,...là điều duy nhất viết trên đó.
Dialogue: 0,0:08:29.45,0:08:31.85,Default,,0000,0000,0000,,Và sau đó là...''nhắm tới phần thưởng''...
Dialogue: 0,0:08:31.92,0:08:33.71,Default,,0000,0000,0000,,Trò này có vẻ lỗi thời.
Dialogue: 0,0:08:37.86,0:08:40.09,Default,,0000,0000,0000,,U-um...Mẹ?
Dialogue: 0,0:08:40.49,0:08:44.79,Default,,0000,0000,0000,,Này, này, Yu-chan...Mẹ đang nóng lòng muốn xem những tấm thẻ đó viết gì.
Dialogue: 0,0:08:44.86,0:08:47.59,Default,,0000,0000,0000,,Trong những trò chơi thế này, phần thú vị nhất chính là những tấm thẻ.
Dialogue: 0,0:08:47.67,0:08:49.72,Default,,0000,0000,0000,,Mẹ, mẹ cho rằng nó thú vị sao?
Dialogue: 0,0:08:49.80,0:08:52.53,Default,,0000,0000,0000,,Ehh? Ko hẳn là như vậy.
Dialogue: 0,0:08:52.64,0:08:53.66,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:08:53.87,0:08:58.71,Default,,0000,0000,0000,,Uhm, Mẹ ko biết gì về mấy trò chơi điện tử con hay chơi...
Dialogue: 0,0:08:58.78,0:09:02.11,Default,,0000,0000,0000,,...Mẹ thấy Yu-chan và Yuri rất thích chơi điện tử cùng nhau...
Dialogue: 0,0:09:02.21,0:09:06.27,Default,,0000,0000,0000,,...''Mẹ cảm thấy ghen tị, nếu các con có thể chơi thì sao mẹ lại ko chơi được chứ,''...
Dialogue: 0,0:09:06.35,0:09:09.48,Default,,0000,0000,0000,,...Vậy nên khi con nói...
Dialogue: 0,0:09:09.55,0:09:13.75,Default,,0000,0000,0000,,...''Hãy chơi thử cùng con,''... Nhưng ko phải mẹ cũng mê những trò chơi đó đâu nhé.
Dialogue: 0,0:09:13.89,0:09:16.09,Default,,0000,0000,0000,,Uhm, ko phải vậy đâu nhé...
Dialogue: 0,0:09:17.33,0:09:18.45,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:09:18.96,0:09:22.19,Default,,0000,0000,0000,,Xem nào, ai sẽ bắt đầu trước đây...
Dialogue: 0,0:09:22.60,0:09:24.09,Default,,0000,0000,0000,,Vậy để con gieo trước nhé.
Dialogue: 0,0:09:27.47,0:09:29.96,Default,,0000,0000,0000,,Một, hai, ba, bốn...
Dialogue: 0,0:09:30.04,0:09:31.37,Default,,0000,0000,0000,,À ha.
Dialogue: 0,0:09:35.85,0:09:39.18,Default,,0000,0000,0000,,Xem ra trò chơi này ko được như quảng cáo rồi.
Dialogue: 0,0:09:39.52,0:09:40.85,Default,,0000,0000,0000,,Có lẽ là vậy.
Dialogue: 0,0:09:41.25,0:09:43.24,Default,,0000,0000,0000,,Rồi, giờ tới lượt mẹ.
Dialogue: 0,0:09:46.52,0:09:50.36,Default,,0000,0000,0000,,Oh...nhỏ hơn Yu-chan một nước...
Dialogue: 0,0:09:50.93,0:09:51.90,Default,,0000,0000,0000,,Heh...
Dialogue: 0,0:09:52.00,0:09:54.59,Default,,0000,0000,0000,,Được lắm, coi chừng bị mẹ đá đó nhé.
Dialogue: 0,0:09:55.27,0:09:56.93,Default,,0000,0000,0000,,Một, hai, ba...
Dialogue: 0,0:09:57.04,0:09:58.70,Default,,0000,0000,0000,,Oh, vào ô đỏ rồi.
Dialogue: 0,0:09:58.77,0:09:59.53,Default,,0000,0000,0000,,Vâng, đúng vậy ạ.
Dialogue: 0,0:09:59.60,0:10:03.77,Default,,0000,0000,0000,,Có vẻ như người được thần may mắn ủng hộ là Mẹ chứ ko phải con.
Dialogue: 0,0:10:04.04,0:10:06.10,Default,,0000,0000,0000,,Coi nào, mẹ mau rút tấm thẻ đó ra đi.
Dialogue: 0,0:10:08.31,0:10:09.61,Default,,0000,0000,0000,,Để xem nào...
Dialogue: 0,0:10:10.12,0:10:11.88,Default,,0000,0000,0000,,Trên đó viết gì thế, hả mẹ?
Dialogue: 0,0:10:12.12,0:10:13.88,Default,,0000,0000,0000,,Huh? Trên đó viết gì ạ?
Dialogue: 0,0:10:13.99,0:10:15.25,Default,,0000,0000,0000,,Là từ này, nhưng mà...
Dialogue: 0,0:10:13.99,0:10:15.64,Note,,0000,0000,0000,,{\i1}Ôm{\i0}
Dialogue: 0,0:10:16.25,0:10:17.65,Default,,0000,0000,0000,,Viết gì thế mẹ?
Dialogue: 0,0:10:17.86,0:10:18.98,Default,,0000,0000,0000,,Viết gì ạ?
Dialogue: 0,0:10:20.16,0:10:21.82,Default,,0000,0000,0000,,Ế? Um...M-Mẹ?
Dialogue: 0,0:10:22.29,0:10:24.63,Default,,0000,0000,0000,,Trên đó viết ''Houyou'' (Ôm)...
Dialogue: 0,0:10:26.03,0:10:29.80,Default,,0000,0000,0000,,Um! Ahh...umm...M-Mẹ!
Dialogue: 0,0:10:30.03,0:10:32.83,Default,,0000,0000,0000,,Ahh, Yu-chan, má con mềm thật đấy...
Dialogue: 0,0:10:32.94,0:10:36.13,Default,,0000,0000,0000,,Oh, dừng lại đi mẹ. Con đâu còn là trẻ con nữa...
Dialogue: 0,0:10:36.67,0:10:38.04,Default,,0000,0000,0000,,Nhột quá đi.
Dialogue: 0,0:10:39.21,0:10:41.91,Default,,0000,0000,0000,,Mẹ đã làm theo yêu cầu của tấm thẻ...
Dialogue: 0,0:10:41.98,0:10:44.07,Default,,0000,0000,0000,,Đến lượt Yu-chan đó...
Dialogue: 0,0:10:44.45,0:10:45.38,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:10:46.52,0:10:47.68,Default,,0000,0000,0000,,Yu-chan?
Dialogue: 0,0:10:48.05,0:10:49.14,Default,,0000,0000,0000,,Ah, vâng...
Dialogue: 0,0:10:49.25,0:10:50.28,Default,,0000,0000,0000,,Đến lượt con.
Dialogue: 0,0:10:56.06,0:10:59.03,Default,,0000,0000,0000,,Coi kìa, con cũng vào một ô đỏ rồi.
Dialogue: 0,0:10:59.23,0:11:00.16,Default,,0000,0000,0000,,V-vâng...
Dialogue: 0,0:11:03.00,0:11:04.20,Default,,0000,0000,0000,,Sao thế?
Dialogue: 0,0:11:04.37,0:11:06.74,Default,,0000,0000,0000,,Từ này khó đọc quá.
Dialogue: 0,0:11:06.80,0:11:10.17,Default,,0000,0000,0000,,Được rồi, được rồi...Để mẹ đọc cho.
Dialogue: 0,0:11:13.31,0:11:16.25,Default,,0000,0000,0000,,Đó là ''Seppun'' (Hôn).
Dialogue: 0,0:11:16.31,0:11:19.51,Note,,0000,0000,0000,,{\i1}Hôn{\i0}
Dialogue: 0,0:11:17.05,0:11:19.71,Default,,0000,0000,0000,,Vậy con phải làm gì bây giờ?
Dialogue: 0,0:11:22.92,0:11:23.76,Default,,0000,0000,0000,,Ở đây.
Dialogue: 0,0:11:23.76,0:11:24.85,Default,,0000,0000,0000,,Huh? Gì cơ?
Dialogue: 0,0:11:25.39,0:11:28.83,Default,,0000,0000,0000,,Thôi nào! ''Seppun'' có nghĩa là Hôn...
Dialogue: 0,0:11:29.13,0:11:32.79,Default,,0000,0000,0000,,Vậy nên, con phải hôn lên má của mẹ.
Dialogue: 0,0:11:32.86,0:11:34.26,Default,,0000,0000,0000,,Sao cơ!
Dialogue: 0,0:11:34.37,0:11:35.46,Default,,0000,0000,0000,,Heh...
Dialogue: 0,0:11:35.87,0:11:39.43,Default,,0000,0000,0000,,Đó là luật của trò chơi, con phải tuân theo chứ.
Dialogue: 0,0:11:39.50,0:11:41.23,Default,,0000,0000,0000,,Oh, vâng. Vậy thì...
Dialogue: 0,0:11:43.88,0:11:46.17,Default,,0000,0000,0000,,Được rồi, đừng ngại mà...
Dialogue: 0,0:11:56.96,0:11:59.29,Default,,0000,0000,0000,,Này! Yu-Yu-chan?
Dialogue: 0,0:11:59.66,0:12:03.12,Default,,0000,0000,0000,,Yu-chan...k-ko, ko phải môi...ah!
Dialogue: 0,0:12:06.36,0:12:08.30,Default,,0000,0000,0000,,Yu-chan, dừng lại...
Dialogue: 0,0:12:49.57,0:12:53.31,Default,,0000,0000,0000,,Yu-chan...Mẹ xin lỗi... vào giữa chừng mẹ đã...
Dialogue: 0,0:12:53.95,0:12:55.81,Default,,0000,0000,0000,,...ko, ko có gì đâu.
Dialogue: 0,0:12:56.01,0:12:57.68,Default,,0000,0000,0000,,Phải, ko có gì hết.
Dialogue: 0,0:12:58.15,0:12:59.08,Default,,0000,0000,0000,,v-vâng.
Dialogue: 0,0:12:59.52,0:13:00.54,Default,,0000,0000,0000,,Uh, um...
Dialogue: 0,0:13:00.69,0:13:06.65,Default,,0000,0000,0000,,...nhưng mà, đôi môi của mẹ thật tuyệt... vì thế...con đã ko thể kiềm chế được...
Dialogue: 0,0:13:07.59,0:13:09.08,Default,,0000,0000,0000,,Huh? Gì thế?
Dialogue: 0,0:13:09.16,0:13:13.10,Default,,0000,0000,0000,,À...có gì đó đã xảy ra trong đầu con...
Dialogue: 0,0:13:13.70,0:13:16.86,Default,,0000,0000,0000,,Dường như...nó xuất phát từ bên trong cơ thể con...
Dialogue: 0,0:13:17.00,0:13:17.99,Default,,0000,0000,0000,,...um...
Dialogue: 0,0:13:18.14,0:13:19.33,Default,,0000,0000,0000,,...cơ thể con đột ngột nóng ran lên...
Dialogue: 0,0:13:19.57,0:13:20.54,Default,,0000,0000,0000,,...và...
Dialogue: 0,0:13:20.84,0:13:21.81,Default,,0000,0000,0000,,...con đã ko kiềm chế được bản thân...
Dialogue: 0,0:13:21.97,0:13:24.37,Default,,0000,0000,0000,,O-oh vậy à...
Dialogue: 0,0:13:25.01,0:13:26.57,Default,,0000,0000,0000,,Mẹ hiểu mà.
Dialogue: 0,0:13:26.68,0:13:29.08,Default,,0000,0000,0000,,Hmm, thật ra mẹ cũng ko hiểu tại sao...
Dialogue: 0,0:13:30.28,0:13:33.72,Default,,0000,0000,0000,,Uhm, mẹ phải đi chuẩn bị bữa tối bây giờ đây...
Dialogue: 0,0:13:34.32,0:13:35.51,Default,,0000,0000,0000,,Oh vâng...
Dialogue: 0,0:13:35.59,0:13:38.61,Default,,0000,0000,0000,,...chúng ta vừa mới bắt đầu chơi, thật đáng tiếc.
Dialogue: 0,0:13:39.32,0:13:41.12,Default,,0000,0000,0000,,Mẹ rất tiếc...
Dialogue: 0,0:13:45.63,0:13:47.33,Default,,0000,0000,0000,,Dù mẹ nghĩ vậy, nhưng...
Dialogue: 0,0:13:47.60,0:13:49.26,Default,,0000,0000,0000,,...con muốn chơi thêm một lúc nữa ko?
Dialogue: 0,0:13:49.33,0:13:50.30,Default,,0000,0000,0000,,Eh...
Dialogue: 0,0:13:51.24,0:13:53.43,Default,,0000,0000,0000,,Mẹ lạ thật đấy.
Dialogue: 0,0:13:53.64,0:13:55.23,Default,,0000,0000,0000,,Vâng, chúng ta tiếp tục nhé!
Dialogue: 0,0:13:57.24,0:13:59.71,Default,,0000,0000,0000,,Tuyệt, Yu-chan đã bị mẹ vượt qua!
Dialogue: 0,0:13:59.88,0:14:00.90,Default,,0000,0000,0000,,Arg...
Dialogue: 0,0:14:01.61,0:14:04.88,Default,,0000,0000,0000,,Một, hai, ba, bốn, năm...ah.
Dialogue: 0,0:14:06.72,0:14:08.24,Default,,0000,0000,0000,,Mẹ...chuyện gì vậy?
Dialogue: 0,0:14:08.95,0:14:10.44,Default,,0000,0000,0000,,Oh, lại vào ô đỏ sao.
Dialogue: 0,0:14:10.56,0:14:11.65,Default,,0000,0000,0000,,Con rút thẻ hộ mẹ nhé?
Dialogue: 0,0:14:12.66,0:14:14.65,Default,,0000,0000,0000,,Tấm thẻ viết gì thế nhỉ?
Dialogue: 0,0:14:15.19,0:14:18.39,Default,,0000,0000,0000,,Chữ trên thẻ khó đọc thật...
Dialogue: 0,0:14:18.73,0:14:21.16,Note,,0000,0000,0000,,{\i1}Cởi hết đồ trên người{\i0}
Dialogue: 0,0:14:26.94,0:14:28.34,Default,,0000,0000,0000,,M-Mẹ!?
Dialogue: 0,0:14:49.43,0:14:51.36,Default,,0000,0000,0000,,Ko, Yu-chan...
Dialogue: 0,0:14:51.86,0:14:53.13,Default,,0000,0000,0000,,Đừng nhìn...làm ơn...
Dialogue: 0,0:14:56.87,0:14:58.20,Default,,0000,0000,0000,,M-Mẹ...
Dialogue: 0,0:15:08.11,0:15:09.84,Default,,0000,0000,0000,,Y-Yu-chan...
Dialogue: 0,0:15:09.95,0:15:12.88,Default,,0000,0000,0000,,D-Dường như có thứ gì đó đang điều khiển mẹ.
Dialogue: 0,0:15:13.95,0:15:16.15,Default,,0000,0000,0000,,Con nhắm mắt lại đi...
Dialogue: 0,0:15:16.35,0:15:17.12,Default,,0000,0000,0000,,Đừng nhìn.
Dialogue: 0,0:15:17.39,0:15:19.72,Default,,0000,0000,0000,,M-Mẹ đang khỏa thân sao...
Dialogue: 0,0:15:25.30,0:15:27.92,Default,,0000,0000,0000,,Yu-chan...con cũng phải cởi đồ đó.
Dialogue: 0,0:15:28.23,0:15:30.53,Default,,0000,0000,0000,,Sao cơ!? Cả con ư? Sao lại như vậy?
Dialogue: 0,0:15:31.94,0:15:33.13,Default,,0000,0000,0000,,G-Gì thế?
Dialogue: 0,0:15:33.24,0:15:34.53,Default,,0000,0000,0000,,Huh? Cơ thể mình...
Dialogue: 0,0:15:34.61,0:15:36.40,Default,,0000,0000,0000,,Sao mình lại tự cởi đồ ra thế này?
Dialogue: 0,0:15:36.71,0:15:39.01,Default,,0000,0000,0000,,Mẹ, cơ thể con đang tự hành động.
Dialogue: 0,0:15:40.18,0:15:41.87,Default,,0000,0000,0000,,A! Xấu hổ quá đi mất!
Dialogue: 0,0:15:43.18,0:15:45.78,Default,,0000,0000,0000,,C-Cả quần lót nữa sao? Ôi ko...
Dialogue: 0,0:15:47.42,0:15:50.11,Default,,0000,0000,0000,,M-Mẹ...Con xấu hổ lắm.
Dialogue: 0,0:15:50.46,0:15:51.65,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:15:52.72,0:15:56.16,Default,,0000,0000,0000,,Này, Yu-chan...chúng ta ko chơi trò này nữa nhé.
Dialogue: 0,0:15:56.39,0:15:59.30,Default,,0000,0000,0000,,V-vâng...chắc là có gì đó ko ổn...
Dialogue: 0,0:16:01.33,0:16:03.99,Default,,0000,0000,0000,,Ko! Con ko muốn làm vậy, nhưng... Tay của con!
Dialogue: 0,0:16:04.10,0:16:05.13,Default,,0000,0000,0000,,Yu-chan!?
Dialogue: 0,0:16:10.48,0:16:15.57,Note,,0000,0000,0000,,{\i1}Làm bằng miệng{\i0}
Dialogue: 0,0:16:11.71,0:16:14.34,Default,,0000,0000,0000,,S-Sao thế này!? Con ko thể cử động được!?
Dialogue: 0,0:16:14.48,0:16:15.57,Default,,0000,0000,0000,,Yu-chan!?
Dialogue: 0,0:16:16.48,0:16:18.21,Default,,0000,0000,0000,,Mẹ! Mẹ làm gì vậy?
Dialogue: 0,0:16:19.72,0:16:21.41,Default,,0000,0000,0000,,K-ko...Mẹ ko thể!
Dialogue: 0,0:16:25.59,0:16:27.75,Default,,0000,0000,0000,,Mẹ...chỗ đó bẩn lắm!
Dialogue: 0,0:16:51.42,0:16:54.82,Default,,0000,0000,0000,,Ko, ko, no...Yu-chan, dừng lại đi!
Dialogue: 0,0:17:29.69,0:17:32.55,Default,,0000,0000,0000,,Đừng liếm chỗ đó...
Dialogue: 0,0:17:35.56,0:17:37.46,Default,,0000,0000,0000,,Yu-chan...dừng lại!
Dialogue: 0,0:17:38.60,0:17:41.76,Default,,0000,0000,0000,,Âm hộ của mẹ ướt đẫm rồi!
Dialogue: 0,0:17:44.87,0:17:47.80,Default,,0000,0000,0000,,Lưỡi của Yu-chan...lưỡi của con!
Dialogue: 0,0:17:48.57,0:17:49.94,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:17:57.72,0:18:01.95,Default,,0000,0000,0000,,Nữa đi, nữa đi... Yu-chan...Yu-chan...
Dialogue: 0,0:18:04.32,0:18:06.02,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:18:34.25,0:18:35.28,Note,,0000,0000,0000,,{\i1}Hiệp đầu tiên{\i0}
Dialogue: 0,0:19:00.71,0:19:06.24,Default,,0000,0000,0000,,Gì thế này...tại sao...tại sao nó... lại tự hoạt động như vậy!?
Dialogue: 0,0:19:07.02,0:19:09.99,Default,,0000,0000,0000,,Ko, ko!
Dialogue: 0,0:19:14.59,0:19:15.62,Note,,0000,0000,0000,,{\i1}Chơi bằng ngực{\i0}
Dialogue: 0,0:19:16.16,0:19:17.63,Default,,0000,0000,0000,,Y-Yu-chan...
Dialogue: 0,0:19:18.06,0:19:19.32,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:19:43.25,0:19:46.66,Default,,0000,0000,0000,,M-Mẹ...làm ơn hãy dừng lại... con sắp ra rồi...
Dialogue: 0,0:19:46.76,0:19:48.95,Default,,0000,0000,0000,,Thằng nhỏ của con, có gì đó sắp sửa bắn ra...
Dialogue: 0,0:20:18.86,0:20:19.82,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:20:19.99,0:20:22.65,Default,,0000,0000,0000,,Con sắp ra rồi...Con sắp ra rồi...Mẹ ơi!
Dialogue: 0,0:20:38.98,0:20:39.74,Note,,0000,0000,0000,,{\i1}Tiếp tục ân ái{\i0}
Dialogue: 0,0:20:45.82,0:20:47.01,Default,,0000,0000,0000,,Y-Yu-chan...
Dialogue: 0,0:20:48.75,0:20:51.72,Default,,0000,0000,0000,,Mẹ ko thể tiếp tục được nữa...ngừng lại đi...
Dialogue: 0,0:20:53.49,0:20:55.86,Default,,0000,0000,0000,,M-Mẹ...con...
Dialogue: 0,0:20:55.93,0:20:57.62,Default,,0000,0000,0000,,...Con đâu muốn làm thế này...
Dialogue: 0,0:20:57.80,0:20:58.92,Default,,0000,0000,0000,,Con nói thật đó.
Dialogue: 0,0:20:59.06,0:21:02.03,Default,,0000,0000,0000,,Con ko hề muốn làm mẹ phiền lòng...
Dialogue: 0,0:21:02.93,0:21:04.26,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:21:11.01,0:21:13.03,Default,,0000,0000,0000,,thằng nhỏ của Yu-chan...vào bên trong mẹ rồi...
Dialogue: 0,0:21:14.61,0:21:15.64,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:21:18.22,0:21:21.34,Default,,0000,0000,0000,,Âm hộ của mình...cùng với thằng nhỏ của Yu-chan...
Dialogue: 0,0:21:22.75,0:21:24.28,Default,,0000,0000,0000,,...chúng phát ra nhiều tiếng động quá.
Dialogue: 0,0:21:27.89,0:21:30.42,Default,,0000,0000,0000,,C-Con xin lỗi, Mẹ! Mông của con cứ tự di chuyển...
Dialogue: 0,0:21:30.49,0:21:32.99,Default,,0000,0000,0000,,Nhưng mà, tuyệt quá đi mất...con ko thể ngừng lại được.
Dialogue: 0,0:21:33.20,0:21:34.56,Default,,0000,0000,0000,,Nó đang vào sâu hơn...
Dialogue: 0,0:21:36.87,0:21:40.33,Default,,0000,0000,0000,,Mẹ, con đã cố...kiềm chế tình cảm của mình...
Dialogue: 0,0:21:40.40,0:21:42.40,Default,,0000,0000,0000,,...con biết mẹ ko thích điều đó, nhưng mà...
Dialogue: 0,0:21:42.47,0:21:44.57,Default,,0000,0000,0000,,...con ko thể...con sắp kiệt sức rồi.
Dialogue: 0,0:21:45.11,0:21:46.27,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:21:46.34,0:21:49.18,Default,,0000,0000,0000,,Con ko cần phải chịu đựng chuyện đó vì mẹ đâu.
Dialogue: 0,0:21:50.05,0:21:53.31,Default,,0000,0000,0000,,Nhưng...tất cả là do con đã mang hộp trò chơi đó về đây...
Dialogue: 0,0:21:53.38,0:21:56.32,Default,,0000,0000,0000,,...con đã ngu ngốc tin rằng nó có thể khiến hai mẹ con ta gần gũi nhau hơn...
Dialogue: 0,0:21:57.15,0:21:58.68,Default,,0000,0000,0000,,Ko sao đâu, Yu-chan.
Dialogue: 0,0:21:59.49,0:22:02.22,Default,,0000,0000,0000,,Con và mẹ đã gần gũi nhau hơn rồi đó...
Dialogue: 0,0:22:02.56,0:22:03.79,Default,,0000,0000,0000,,...giống như một cặp tình nhân.
Dialogue: 0,0:22:04.26,0:22:05.36,Default,,0000,0000,0000,,Tình nhân ư?
Dialogue: 0,0:22:05.36,0:22:06.62,Default,,0000,0000,0000,,Tình nhân? Phải, là tình nhân.
Dialogue: 0,0:22:07.03,0:22:09.93,Default,,0000,0000,0000,,Chỉ có các cặp tình nhân mới gần gũi nhau theo cách này...
Dialogue: 0,0:22:10.30,0:22:11.27,Default,,0000,0000,0000,,Mẹ nói thật chứ?
Dialogue: 0,0:22:11.44,0:22:13.40,Default,,0000,0000,0000,,Vì vậy...vì vậy con làm ơn...
Dialogue: 0,0:22:13.47,0:22:16.80,Default,,0000,0000,0000,,Mẹ muốn con ân ái cùng mẹ lâu hơn nữa...
Dialogue: 0,0:22:17.78,0:22:19.47,Default,,0000,0000,0000,,Yu-chan, con đồng ý chứ?
Dialogue: 0,0:22:20.81,0:22:22.68,Default,,0000,0000,0000,,Ân ái cùng mẹ...
Dialogue: 0,0:22:27.22,0:22:31.15,Default,,0000,0000,0000,,Yu-chan, con thấy ko, nó đang vào bên trong mẹ?
Dialogue: 0,0:22:35.16,0:22:36.22,Default,,0000,0000,0000,,Nó vào bên trong rồi.
Dialogue: 0,0:22:36.73,0:22:37.85,Default,,0000,0000,0000,,Mẹ...
Dialogue: 0,0:22:38.13,0:22:39.39,Default,,0000,0000,0000,,Con làm nhé?
Dialogue: 0,0:22:39.66,0:22:40.65,Default,,0000,0000,0000,,Ko, con ko cần làm gì hết...
Dialogue: 0,0:22:40.73,0:22:42.67,Default,,0000,0000,0000,,...Mẹ sẽ làm tất cả chuyện này cho con.
Dialogue: 0,0:22:43.17,0:22:44.40,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:22:48.31,0:22:51.33,Default,,0000,0000,0000,,M-Mẹ...phải chăng con là...một gã háo sắc?
Dialogue: 0,0:22:52.04,0:22:54.84,Default,,0000,0000,0000,,Phải, đúng như vậy. Yu-chan, con là một cậu bé háo sắc...
Dialogue: 0,0:22:56.58,0:22:59.71,Default,,0000,0000,0000,,...bởi vì, thằng nhỏ của con đang co giật liên hồi bên trong mẹ.
Dialogue: 0,0:22:59.88,0:23:01.11,Default,,0000,0000,0000,,Đó là...
Dialogue: 0,0:23:01.29,0:23:04.35,Default,,0000,0000,0000,,Nhưng mà, mẹ yêu gã háo sắc Yu-chan vô cùng...
Dialogue: 0,0:23:06.36,0:23:08.92,Default,,0000,0000,0000,,Thằng nhỏ của Yu-chan đang vào sâu bên trong...
Dialogue: 0,0:23:09.76,0:23:11.69,Default,,0000,0000,0000,,Sướng quá! Sướng quá đi mất!
Dialogue: 0,0:23:12.40,0:23:14.96,Default,,0000,0000,0000,,Đừng dừng lại, hãy chơi mẹ nữa đi...
Dialogue: 0,0:23:17.74,0:23:20.26,Default,,0000,0000,0000,,Mẹ...tuyệt quá!
Dialogue: 0,0:23:20.37,0:23:21.46,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:23:21.54,0:23:23.67,Default,,0000,0000,0000,,Mẹ, mẹ thỏa mãn chứ?
Dialogue: 0,0:23:23.74,0:23:25.04,Default,,0000,0000,0000,,Mẹ thỏa mãn chứ?
Dialogue: 0,0:23:26.58,0:23:29.41,Default,,0000,0000,0000,,Thằng nhỏ của Yu-chan... bởi vì đó là thằng nhỏ của Yu-chan...
Dialogue: 0,0:23:30.51,0:23:32.54,Default,,0000,0000,0000,,Hãy ra đi, hãy ra thật nhiều...
Dialogue: 0,0:23:32.68,0:23:35.02,Default,,0000,0000,0000,,...thật nhiều sữa...lấp đầy cơ thể mẹ bằng sữa của con!
Dialogue: 0,0:23:36.29,0:23:37.91,Default,,0000,0000,0000,,Vâng, vì con là con của mẹ mà!
Dialogue: 0,0:23:38.46,0:23:39.48,Default,,0000,0000,0000,,Mẹ!
Dialogue: 0,0:23:40.66,0:23:41.72,Default,,0000,0000,0000,,Yu-chan!
Dialogue: 0,0:23:43.06,0:23:44.03,Default,,0000,0000,0000,,Yu-chan...
Dialogue: 0,0:23:44.50,0:23:45.99,Default,,0000,0000,0000,,...nếu chúng ta đã là tình nhân...
Dialogue: 0,0:23:46.96,0:23:48.59,Default,,0000,0000,0000,,...mẹ muốn con hôn mẹ!
Dialogue: 0,0:24:05.02,0:24:07.01,Default,,0000,0000,0000,,Mẹ sắp ra rồi...Mẹ sắp ra rồi!
Dialogue: 0,0:24:07.08,0:24:08.48,Default,,0000,0000,0000,,Yu-chan!
Dialogue: 0,0:24:09.12,0:24:10.81,Default,,0000,0000,0000,,Con cũng vậy...Mẹ!
Dialogue: 0,0:24:40.32,0:24:42.48,Default,,0000,0000,0000,,Yuji...Em bị sao thế?
Dialogue: 0,0:24:42.55,0:24:44.49,Default,,0000,0000,0000,,Sao tự nhiên lại nắm tay Mẹ làm gì vậy?
Dialogue: 0,0:24:44.59,0:24:48.72,Default,,0000,0000,0000,,Oh, hiểu rồi...em vẫn luôn là đứa chỉ biết bám váy mẹ, phải ko nhỉ?
Dialogue: 0,0:24:48.96,0:24:49.95,Default,,0000,0000,0000,,Có gì xấu chứ?
Dialogue: 0,0:24:50.79,0:24:52.85,Default,,0000,0000,0000,,Bởi vì Mẹ và em đã...
Dialogue: 0,0:24:53.80,0:24:55.77,Default,,0000,0000,0000,,Mẹ con ta đã gần gũi nhau hơn!
Dialogue: 0,0:24:58.30,0:24:59.36,Default,,0000,0000,0000,,Huh?
