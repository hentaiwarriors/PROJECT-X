Dialogue: 0,0:00:02.33,0:00:12.82,Default,,0000,0000,0000,,Project thuộc về website : Baka-sub.net
Dialogue: 0,0:00:30.33,0:00:32.35,Default,,0000,0000,0000,,Chà, xem chừng đây là cách giải quyết hay nhất!
Dialogue: 0,0:00:34.03,0:00:35.96,Default,,0000,0000,0000,,Hoa hậu ngực!?
Dialogue: 0,0:00:36.60,0:00:39.59,Default,,0000,0000,0000,,Phải, giải thưởng dành cho người thắng cuộc là $10,000.
Dialogue: 0,0:00:39.67,0:00:42.16,Default,,0000,0000,0000,,Vậy tức là khoảng...1 ,000,000 yen.
Dialogue: 0,0:00:43.07,0:00:45.04,Default,,0000,0000,0000,,Ko chỉ có vậy.
Dialogue: 0,0:00:45.41,0:00:51.00,Default,,0000,0000,0000,,Ai trong số chúng ta giành chiến thắng, Wataru-chan sẽ thuộc về người đó, thế nào hả?
Dialogue: 0,0:00:55.38,0:00:57.75,Default,,0000,0000,0000,,Nghe được đấy, tôi chấp nhận.
Dialogue: 0,0:00:59.82,0:01:01.19,Default,,0000,0000,0000,,Chấp nhận thách đấu.
Dialogue: 0,0:01:01.96,0:01:03.82,Default,,0000,0000,0000,,Wataru sẽ thuộc về tôi.
Dialogue: 0,0:01:05.06,0:01:07.29,Default,,0000,0000,0000,,Có vẻ như ko ai ở đây muốn trở thành kẻ thua cuộc cả.
Dialogue: 0,0:01:14.14,0:01:19.70,Default,,0000,0000,0000,,Được lắm, người thắng cuộc sẽ có được trái tim của Wataru-chan.
Dialogue: 0,0:01:35.89,0:01:38.66,Default,,0000,0000,0000,,Quay lại đây sau một năm!
Dialogue: 0,0:01:38.86,0:01:42.52,Default,,0000,0000,0000,,Cuộc thi được chờ đợi nhất, ''Hoa hậu ngực!''
Dialogue: 0,0:01:42.60,0:01:46.53,Default,,0000,0000,0000,,Xuất sắc vượt qua các vòng thi sơ khảo...
Dialogue: 0,0:01:46.64,0:01:52.13,Default,,0000,0000,0000,,Xin trân trọng giới thiệu 13 thí sinh lọt vào vòng chung khảo đêm nay!
Dialogue: 0,0:02:00.15,0:02:02.48,Default,,0000,0000,0000,,Huh, lúc này thì ko có chị em gì đâu nhé.
Dialogue: 0,0:02:02.55,0:02:04.11,Default,,0000,0000,0000,,Bây giờ chị là đối thủ của em.
Dialogue: 0,0:02:05.42,0:02:07.06,Default,,0000,0000,0000,,Ko vấn đề gì.
Dialogue: 0,0:02:07.06,0:02:09.68,Default,,0000,0000,0000,,Tôi sẽ ko chịu thua đâu!
Dialogue: 0,0:02:11.43,0:02:16.33,Default,,0000,0000,0000,,Thưa quý vị, chúng tôi xin được phép bắt đầu phần thi ''Tài năng''.
Dialogue: 0,0:02:16.40,0:02:20.66,Default,,0000,0000,0000,,Mời quý vị chiêm ngưỡng các thí sinh của chúng ta trổ tài.
Dialogue: 0,0:02:22.90,0:02:27.57,Default,,0000,0000,0000,,Vâng, quý vị thấy sao về màn tung hứng bóng bằng ngực của Ms. Chan ạ?
Dialogue: 0,0:02:34.58,0:02:36.52,Default,,0000,0000,0000,,Chà, tài thật!
Dialogue: 0,0:02:38.39,0:02:40.68,Default,,0000,0000,0000,,Thí sinh tiếp theo của chúng ta...
Dialogue: 0,0:02:41.09,0:02:43.89,Default,,0000,0000,0000,,Người đẹp đến từ Thụy Điển...
Dialogue: 0,0:02:43.89,0:02:45.73,Default,,0000,0000,0000,,Sắp tới lượt Fumie rồi nhỉ.
Dialogue: 0,0:02:45.73,0:02:47.30,Default,,0000,0000,0000,,...xin trân trọng giới thiệu.
Dialogue: 0,0:02:47.30,0:02:49.56,Default,,0000,0000,0000,,Ms. Shane Annison!
Dialogue: 0,0:02:51.13,0:02:56.13,Default,,0000,0000,0000,,Cô ấy sẽ gửi đến chúng ta một vũ điệu mê hồn, mô phỏng theo các kĩ thuật của môn quần vợt.
Dialogue: 0,0:02:56.84,0:02:59.07,Default,,0000,0000,0000,,Wow, thật tuyệt vời, thưa quý vị...
Dialogue: 0,0:02:59.07,0:02:59.80,Default,,0000,0000,0000,,Fumie-san.
Dialogue: 0,0:03:01.88,0:03:03.43,Default,,0000,0000,0000,,Wataru-kun?
Dialogue: 0,0:03:04.68,0:03:06.34,Default,,0000,0000,0000,,Tôi tới để chúc chị may mắn.
Dialogue: 0,0:03:07.98,0:03:09.42,Default,,0000,0000,0000,,C, cám ơn cậu...
Dialogue: 0,0:03:09.68,0:03:11.08,Default,,0000,0000,0000,,Chị căng thẳng đấy à?
Dialogue: 0,0:03:11.52,0:03:13.18,Default,,0000,0000,0000,,Uhm, một chút thôi.
Dialogue: 0,0:03:14.59,0:03:15.72,Default,,0000,0000,0000,,À phải rồi.
Dialogue: 0,0:03:16.19,0:03:19.52,Default,,0000,0000,0000,,Lúc này tôi cần được thư giãn, cậu giúp tôi nhé?!
Dialogue: 0,0:03:23.43,0:03:25.30,Default,,0000,0000,0000,,Cậu hiểu ý tôi mà.
Dialogue: 0,0:03:25.67,0:03:27.23,Default,,0000,0000,0000,,F, Fumie-san, nhưng mà...
Dialogue: 0,0:03:27.30,0:03:29.20,Default,,0000,0000,0000,,Đừng lo, đi nào.
Dialogue: 0,0:03:31.01,0:03:31.44,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:03:31.51,0:03:33.06,Default,,0000,0000,0000,,Fumie-san, chúng ta ko thể!
Dialogue: 0,0:03:33.14,0:03:34.80,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:03:36.04,0:03:38.44,Default,,0000,0000,0000,,Này cô, sắp đến phần thi của cô rồi đó!
Dialogue: 0,0:03:38.51,0:03:39.64,Default,,0000,0000,0000,,Cô định đi đâu vậy!?
Dialogue: 0,0:03:39.75,0:03:41.61,Default,,0000,0000,0000,,Tôi sẽ quay lại ngay.
Dialogue: 0,0:03:44.15,0:03:45.85,Default,,0000,0000,0000,,Chuyện gì vậy ta...
Dialogue: 0,0:03:47.39,0:03:51.69,Default,,0000,0000,0000,,Đến với chúng ta từ đất nước Tây Ban Nha xinh đẹp, Ms. Zoe Sabastian....
Dialogue: 0,0:03:54.20,0:03:57.56,Default,,0000,0000,0000,,Lâu lắm rồi...chúng ta mới được ở cùng nhau thế này.
Dialogue: 0,0:03:58.43,0:03:59.37,Default,,0000,0000,0000,,...đúng vậy.
Dialogue: 0,0:04:00.74,0:04:04.00,Default,,0000,0000,0000,,Chiếc Bikini khiến chúng khít nhau quá, ráng chịu nhé.
Dialogue: 0,0:04:08.58,0:04:12.27,Default,,0000,0000,0000,,Fumie-san, ngực chị...thật mềm mại.
Dialogue: 0,0:04:13.52,0:04:17.21,Default,,0000,0000,0000,,Hãy làm bất cứ điều gì cậu thích, chúng là của cậu mà, Wataru-kun.
Dialogue: 0,0:04:19.02,0:04:21.25,Default,,0000,0000,0000,,Phải rồi, như thế.
Dialogue: 0,0:04:22.66,0:04:25.09,Default,,0000,0000,0000,,T....Tôi ko thể!
Dialogue: 0,0:04:26.19,0:04:27.72,Default,,0000,0000,0000,,Cậu sao vậy, Wataru-kun?
Dialogue: 0,0:04:27.80,0:04:29.56,Default,,0000,0000,0000,,Hiếm khi thấy cậu bị kích thích như vậy.
Dialogue: 0,0:04:29.63,0:04:31.46,Default,,0000,0000,0000,,Có lẽ là do thời tiết bên Mĩ ?
Dialogue: 0,0:04:39.81,0:04:43.01,Default,,0000,0000,0000,,Thôi nào, đừng kích thích tôi nữa, nhét nó vào bên trong tôi đi!
Dialogue: 0,0:04:43.38,0:04:44.11,Default,,0000,0000,0000,,V, vâng...
Dialogue: 0,0:04:47.02,0:04:48.88,Default,,0000,0000,0000,,Bự..quá!
Dialogue: 0,0:04:49.18,0:04:52.28,Default,,0000,0000,0000,,Fumie-san, chị đừng nhấp nhô người mạnh như vậy, nếu ko...
Dialogue: 0,0:04:52.35,0:04:55.35,Default,,0000,0000,0000,,Nhưng mà....Wataru-kun, ngực của tôi!
Dialogue: 0,0:04:59.26,0:05:00.28,Default,,0000,0000,0000,,Dĩ nhiên rồi.
Dialogue: 0,0:05:05.07,0:05:06.09,Default,,0000,0000,0000,,Wataru-kun.
Dialogue: 0,0:05:08.80,0:05:11.93,Default,,0000,0000,0000,,Wataru-kun, tuyệt...quá!
Dialogue: 0,0:05:12.41,0:05:15.50,Default,,0000,0000,0000,,Cơ thể tôi đang nóng ran lên!
Dialogue: 0,0:05:17.71,0:05:21.94,Default,,0000,0000,0000,,Dường như tôi...sắp bị thiêu cháy bởi khoái cảm!
Dialogue: 0,0:05:27.02,0:05:29.75,Default,,0000,0000,0000,,F, Fumie-san, tôi ko chịu nổi nữa rồi...!
Dialogue: 0,0:05:30.79,0:05:34.23,Default,,0000,0000,0000,,Được thôi, ra đi, ra đi nào!
Dialogue: 0,0:05:35.96,0:05:41.77,Default,,0000,0000,0000,,Wataru-kun...tôi muốn...cậu ra...vào...bên trong tôi!!!
Dialogue: 0,0:05:42.87,0:05:44.77,Default,,0000,0000,0000,,Ôi, nước đâu, mang nước đến đây ngay!
Dialogue: 0,0:05:54.65,0:05:56.84,Default,,0000,0000,0000,,Thật đáng tiếc, dìu cô ấy vào trong nào.
Dialogue: 0,0:05:56.92,0:05:58.75,Default,,0000,0000,0000,,Ai đó hãy dìu cô ấy vào sau cánh gà!
Dialogue: 0,0:05:58.85,0:06:00.65,Default,,0000,0000,0000,,Gọi người tiếp theo đi!
Dialogue: 0,0:06:03.63,0:06:05.32,Default,,0000,0000,0000,,Đến phần thi của tôi rồi...
Dialogue: 0,0:06:07.36,0:06:08.56,Default,,0000,0000,0000,,Tôi phải đi đây.
Dialogue: 0,0:06:10.93,0:06:12.19,Default,,0000,0000,0000,,Fumie-san...
Dialogue: 0,0:06:15.17,0:06:18.04,Default,,0000,0000,0000,,Xin lỗi cô, một tai nạn đáng tiếc vừa xảy ra ngay trên sân khấu.
Dialogue: 0,0:06:18.11,0:06:21.17,Default,,0000,0000,0000,,....để quên đi sai sót đáng tiếc này, xin được gửi tới quý vị...
Dialogue: 0,0:06:21.28,0:06:23.64,Default,,0000,0000,0000,,Ms. Fumie Fusano!
Dialogue: 0,0:06:28.55,0:06:30.81,Default,,0000,0000,0000,,Fumie-san thật tuyệt.
Dialogue: 0,0:06:32.15,0:06:34.64,Default,,0000,0000,0000,,Bắt quả tang.
Dialogue: 0,0:06:37.73,0:06:39.69,Default,,0000,0000,0000,,Lần này thì đừng hòng chối cãi nữa nhé.
Dialogue: 0,0:06:39.99,0:06:42.36,Default,,0000,0000,0000,,Thiên vị như thế là ko được đâu.
Dialogue: 0,0:06:42.63,0:06:43.60,Default,,0000,0000,0000,,Mình nói thế đúng ko nhỉ?
Dialogue: 0,0:06:43.67,0:06:48.03,Default,,0000,0000,0000,,....với tiết mục biểu diễn thư pháp Nhật Bản.
Dialogue: 0,0:06:48.60,0:06:52.16,Default,,0000,0000,0000,,Này các quý ông, đừng hiểu sai ý tôi.
Dialogue: 0,0:07:00.55,0:07:03.45,Default,,0000,0000,0000,,Tuyệt quá, cậu làm tuyệt quá, Tomoka-san...
Dialogue: 0,0:07:05.09,0:07:07.15,Default,,0000,0000,0000,,Mình sẽ làm cậu sung sướng hơn nữa.
Dialogue: 0,0:07:10.53,0:07:14.79,Default,,0000,0000,0000,,Tomoka-san, tuyệt, tuyệt...quá đi mất!
Dialogue: 0,0:07:16.83,0:07:18.32,Default,,0000,0000,0000,,Tuyệt lắm hả?
Dialogue: 0,0:07:18.80,0:07:21.00,Default,,0000,0000,0000,,Vâng, ko thể nói gì hơn ngoài hai chữ: Tuyệt vời!
Dialogue: 0,0:07:21.07,0:07:25.10,Default,,0000,0000,0000,,Có lẽ đây là một môn nghệ thuật bí truyền của người Nhật Bản.
Dialogue: 0,0:07:26.07,0:07:30.01,Default,,0000,0000,0000,,Bên cạnh đó, tư thế viết thư pháp của cô ấy thật "kinh điển".
Dialogue: 0,0:07:38.79,0:07:40.81,Default,,0000,0000,0000,,Coi nào Wataru-chan.
Dialogue: 0,0:07:42.62,0:07:46.56,Default,,0000,0000,0000,,Nếu cậu ko làm mình thỏa mãn, mình sẽ la to lên cho người khác nghe thấy đó.
Dialogue: 0,0:07:47.13,0:07:49.26,Default,,0000,0000,0000,,Đ, được rồi, mình làm đây...
Dialogue: 0,0:07:51.83,0:07:55.03,Default,,0000,0000,0000,,Wataru-chan, cậu thật mạnh mẽ, quá mạnh mẽ!
Dialogue: 0,0:07:56.30,0:07:58.83,Default,,0000,0000,0000,,T, Tomoka-san, bên trong cậu tuyệt quá!
Dialogue: 0,0:07:58.91,0:08:01.90,Default,,0000,0000,0000,,Wataru-chan, ngực mình, ngực mình nữa!
Dialogue: 0,0:08:02.01,0:08:02.77,Default,,0000,0000,0000,,Uhm!
Dialogue: 0,0:08:08.25,0:08:10.55,Default,,0000,0000,0000,,Wataru-chan, cậu làm tuyệt quá!
Dialogue: 0,0:08:11.72,0:08:13.55,Default,,0000,0000,0000,,M, Mình...!!
Dialogue: 0,0:08:15.39,0:08:17.45,Default,,0000,0000,0000,,Tomoka-san, mình, mình cũng vậy!
Dialogue: 0,0:08:17.86,0:08:21.26,Default,,0000,0000,0000,,Ko sao cả...ra đi, ra vào bên trong mình đi!
Dialogue: 0,0:08:22.53,0:08:24.62,Default,,0000,0000,0000,,Uống sữa của mình đi!
Dialogue: 0,0:08:29.40,0:08:31.40,Default,,0000,0000,0000,,Tomoka-san, sữa của cậu ngon quá.
Dialogue: 0,0:08:32.37,0:08:34.34,Default,,0000,0000,0000,,Của cậu cũng vậy...
Dialogue: 0,0:08:34.41,0:08:36.17,Default,,0000,0000,0000,,Ngon tuyệt...
Dialogue: 0,0:08:38.75,0:08:42.44,Default,,0000,0000,0000,,Tiếp sau đây, là một kiều nữ Nhật Bản khác!
Dialogue: 0,0:08:42.52,0:08:44.95,Default,,0000,0000,0000,,Ms. Tomoka Fusano!
Dialogue: 0,0:08:49.16,0:08:52.46,Default,,0000,0000,0000,,Cô ấy sẽ trình diễn tiết mục ảo thuật: dùng ngực bói bài.
Dialogue: 0,0:08:52.56,0:08:57.19,Default,,0000,0000,0000,,Ngực cô ấy sẽ rung lên mỗi khi có ai đó đoán chính xác tấm bài, wow!
Dialogue: 0,0:09:01.80,0:09:03.83,Default,,0000,0000,0000,,Mình thắng chắc rồi.
Dialogue: 0,0:09:04.84,0:09:07.90,Default,,0000,0000,0000,,Thí sinh tiếp theo, và cũng là người cuối cùng.
Dialogue: 0,0:09:08.21,0:09:12.27,Default,,0000,0000,0000,,Một nữ cao bồi đến từ Arizona, Ms. Lena Ashford!
Dialogue: 0,0:09:15.38,0:09:17.01,Default,,0000,0000,0000,,Đến tiết mục của Lena rồi hả.
Dialogue: 0,0:09:25.43,0:09:26.86,Default,,0000,0000,0000,,Này Wataru!
Dialogue: 0,0:09:28.86,0:09:30.66,Default,,0000,0000,0000,,Đừng yên tại đó nhé.
Dialogue: 0,0:09:33.77,0:09:35.17,Default,,0000,0000,0000,,Tốt rồi, hãy xem đây!
Dialogue: 0,0:09:44.25,0:09:46.30,Default,,0000,0000,0000,,Tuyệt vời, tuyệt vời.
Dialogue: 0,0:09:46.41,0:09:49.85,Default,,0000,0000,0000,,Cú ném tuyệt lắm, Ms. Ashford.
Dialogue: 0,0:09:50.32,0:09:52.79,Default,,0000,0000,0000,,Được rồi, để tôi cầm nó giúp cô.
Dialogue: 0,0:09:53.69,0:09:55.21,Default,,0000,0000,0000,,Vâng, vâng.
Dialogue: 0,0:09:56.19,0:09:58.78,Default,,0000,0000,0000,,Cô đã hoàn thành bài biểu diễn của mình, mời vào sau cánh gà nghỉ ngơi.
Dialogue: 0,0:09:59.23,0:10:00.19,Default,,0000,0000,0000,,Uh, vâng.
Dialogue: 0,0:10:06.70,0:10:09.80,Default,,0000,0000,0000,,Vâng thưa quý vị, phần thi "Tài năng" xin được kết thúc ở đây.
Dialogue: 0,0:10:09.90,0:10:11.90,Default,,0000,0000,0000,,Cảm ơn các thí sinh rất nhiều.
Dialogue: 0,0:10:13.37,0:10:18.57,Default,,0000,0000,0000,,Gặp mình ở bể bơi sau 15 phút nữa, Lena.
Dialogue: 0,0:10:19.21,0:10:21.65,Default,,0000,0000,0000,,...mời quý vị và các bản nghỉ giải lao trong giây lát.
Dialogue: 0,0:10:33.86,0:10:35.19,Default,,0000,0000,0000,,Wataru.
Dialogue: 0,0:10:50.14,0:10:51.77,Default,,0000,0000,0000,,Trông mình...buồn cười lắm à?
Dialogue: 0,0:10:52.25,0:10:53.84,Default,,0000,0000,0000,,Oh, ko hề!
Dialogue: 0,0:10:54.38,0:10:55.44,Default,,0000,0000,0000,,Cậu...
Dialogue: 0,0:10:57.22,0:10:59.24,Default,,0000,0000,0000,,...đẹp lắm.
Dialogue: 0,0:11:00.92,0:11:02.12,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:11:22.51,0:11:23.67,Default,,0000,0000,0000,,Mình ngồi xuống kia nhé.
Dialogue: 0,0:11:23.78,0:11:24.94,Default,,0000,0000,0000,,Uhm...
Dialogue: 0,0:11:26.68,0:11:30.67,Default,,0000,0000,0000,,Từ lần đầu gặp cậu...mình thực sự rất vui.
Dialogue: 0,0:11:32.25,0:11:39.42,Default,,0000,0000,0000,,Trước giờ cha luôn coi mình như con trai, vì vậy đây là lần đầu tiên mình mặc váy dạ hội.
Dialogue: 0,0:11:40.03,0:11:43.93,Default,,0000,0000,0000,,Cậu là người đầu tiên được thấy mình trong bộ dạng này.
Dialogue: 0,0:11:45.73,0:11:47.93,Default,,0000,0000,0000,,Cảm ơn cậu.
Dialogue: 0,0:11:49.27,0:11:50.07,Default,,0000,0000,0000,,Wataru.
Dialogue: 0,0:11:51.34,0:11:55.44,Default,,0000,0000,0000,,Làm ơn...hãy biến mình thành người đàn bà của cậu.
Dialogue: 0,0:11:56.71,0:11:57.74,Default,,0000,0000,0000,,Lena...
Dialogue: 0,0:12:16.96,0:12:20.06,Default,,0000,0000,0000,,Wataru...hãy làm bất cứ điều gì cậu muốn.
Dialogue: 0,0:12:21.84,0:12:22.56,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:12:32.21,0:12:35.24,Default,,0000,0000,0000,,Wataru, cậu tuyệt lắm!
Dialogue: 0,0:12:35.92,0:12:39.15,Default,,0000,0000,0000,,M, mình sẽ làm cậu sung sướng như vậy.
Dialogue: 0,0:12:45.06,0:12:46.15,Default,,0000,0000,0000,,L, Lena...
Dialogue: 0,0:12:47.03,0:12:48.15,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:12:48.60,0:12:51.00,Default,,0000,0000,0000,,Wataru, thằng nhỏ của cậu bự quá.
Dialogue: 0,0:12:51.37,0:12:55.06,Default,,0000,0000,0000,,Đó là vì ngực cậu quá mềm mại, Lena.
Dialogue: 0,0:12:56.00,0:12:57.13,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:13:21.50,0:13:23.19,Default,,0000,0000,0000,,Tuyệt chứ hả?
Dialogue: 0,0:13:23.26,0:13:25.43,Default,,0000,0000,0000,,Uhm, rất tuyệt...
Dialogue: 0,0:13:45.49,0:13:49.01,Default,,0000,0000,0000,,Nó đang bự lên trong miệng mình, ah.
Dialogue: 0,0:13:49.59,0:13:51.15,Default,,0000,0000,0000,,Lena, mình sắp ra rồi!
Dialogue: 0,0:14:08.18,0:14:10.84,Default,,0000,0000,0000,,Wataru, ngon quá.
Dialogue: 0,0:14:15.35,0:14:18.11,Default,,0000,0000,0000,,Mình.
Dialogue: 0,0:14:22.26,0:14:24.99,Default,,0000,0000,0000,,Amazing, thằng nhỏ của cậu cứng thật đấy.
Dialogue: 0,0:14:27.09,0:14:29.89,Default,,0000,0000,0000,,Wataru, mình nhét nó vào trong đây.
Dialogue: 0,0:14:31.50,0:14:32.23,Default,,0000,0000,0000,,Vào...
Dialogue: 0,0:14:32.93,0:14:34.09,Default,,0000,0000,0000,,Bên trong!
Dialogue: 0,0:14:35.54,0:14:37.97,Default,,0000,0000,0000,,Wataru, cậu phải chơi mình thật mạnh đấy nhé.
Dialogue: 0,0:14:38.31,0:14:40.27,Default,,0000,0000,0000,,Rồi, rồi!
Dialogue: 0,0:14:44.21,0:14:46.51,Default,,0000,0000,0000,,Wataru...thật mạnh mẽ!
Dialogue: 0,0:14:50.89,0:14:51.65,Default,,0000,0000,0000,,Lena...
Dialogue: 0,0:14:51.85,0:14:55.25,Default,,0000,0000,0000,,Lena, bên trong cậu khít quá.
Dialogue: 0,0:14:55.39,0:14:59.12,Default,,0000,0000,0000,,Đó là do thằng nhỏ của cậu đang bự lên!
Dialogue: 0,0:15:00.26,0:15:01.42,Default,,0000,0000,0000,,Lena!
Dialogue: 0,0:15:05.20,0:15:08.33,Default,,0000,0000,0000,,K, ko...mình sắp ra rồi.
Dialogue: 0,0:15:08.47,0:15:09.77,Default,,0000,0000,0000,,Phải..chúng ta sẽ ra cùng nhau!
Dialogue: 0,0:15:13.91,0:15:18.54,Default,,0000,0000,0000,,Ra rồi, ra rồi, ra rồi, ra rồi!
Dialogue: 0,0:15:42.77,0:15:45.74,Default,,0000,0000,0000,,Wataru, nó thật là ấm...
Dialogue: 0,0:15:45.81,0:15:46.53,Default,,0000,0000,0000,,Lena...
Dialogue: 0,0:15:59.55,0:16:03.96,Default,,0000,0000,0000,,Kình thưa quý vị, xin lỗi vì đã bắt quý vị phải chờ lâu.
Dialogue: 0,0:16:04.02,0:16:09.26,Default,,0000,0000,0000,,Ban giám khảo vừa đưa ra quyết định cuối cùng về tân hoa hậu !
Dialogue: 0,0:16:09.90,0:16:13.86,Default,,0000,0000,0000,,Đã có vài rắc rối nhỏ xảy ra, nhưng điều đó ko hề ảnh hướng tới sự thành công của cuộc thi.
Dialogue: 0,0:16:14.17,0:16:19.87,Default,,0000,0000,0000,,Trước khi công bố tên người thắng cuộc, xin gửi tới quý vị vũ điệu do chính các thi sinh biểu diễn!
Dialogue: 0,0:16:19.94,0:16:21.27,Default,,0000,0000,0000,,Nổi nhạc lên nào!
Dialogue: 0,0:16:29.52,0:16:31.95,Default,,0000,0000,0000,,Một tiết mục ko thể bỏ qua!
Dialogue: 0,0:16:32.02,0:16:33.71,Default,,0000,0000,0000,,Mọi người, hãy cùng thưởng thức nào!
Dialogue: 0,0:16:41.03,0:16:42.09,Default,,0000,0000,0000,,Ôi trời...
Dialogue: 0,0:16:42.46,0:16:45.76,Default,,0000,0000,0000,,Mệt chết mất, mắt mình díu lại rồi.
Dialogue: 0,0:16:49.44,0:16:53.40,Default,,0000,0000,0000,,À này, người thay thế cho cô ả bị cháy tóc là ai vậy?
Dialogue: 0,0:16:53.51,0:16:55.53,Default,,0000,0000,0000,,Vừa nãy chả nghe thấy họ thông báo gì cả.
Dialogue: 0,0:16:55.74,0:16:57.07,Default,,0000,0000,0000,,Tôi ko rõ.
Dialogue: 0,0:16:57.18,0:17:00.94,Default,,0000,0000,0000,,Khi tôi quay về, phần biểu diễn đã kết thúc.
Dialogue: 0,0:17:01.18,0:17:02.44,Default,,0000,0000,0000,,Thế hả.
Dialogue: 0,0:17:02.55,0:17:06.45,Default,,0000,0000,0000,,Hình như tôi đã gặp cô ta ở đâu rồi thì phải.
Dialogue: 0,0:17:06.72,0:17:08.71,Default,,0000,0000,0000,,Trông giống thật.
Dialogue: 0,0:17:09.06,0:17:11.32,Default,,0000,0000,0000,,Cô nói tôi mới để ý, đúng là giống thật.
Dialogue: 0,0:17:13.46,0:17:15.33,Default,,0000,0000,0000,,Bây giờ sẽ là thời khắc quan trọng nhất của đêm nay!
Dialogue: 0,0:17:15.43,0:17:17.33,Default,,0000,0000,0000,,Đã đến lúc công bố tên người chiến thắng!
Dialogue: 0,0:17:19.67,0:17:22.66,Default,,0000,0000,0000,,Trong số các mĩ nhân đến từ khắp nơi trên thế giới...
Dialogue: 0,0:17:22.74,0:17:25.50,Default,,0000,0000,0000,,Ai sẽ là Tân hoa hậu!?
Dialogue: 0,0:17:27.78,0:17:29.40,Default,,0000,0000,0000,,L, là ai vậy!?
Dialogue: 0,0:17:30.21,0:17:33.70,Default,,0000,0000,0000,,Thật ko thể tưởng tượng được, người thắng cuộc chính là người được xét thay thế thí sinh số 5...
Dialogue: 0,0:17:38.59,0:17:40.58,Default,,0000,0000,0000,,Mrs. Sakurai!
Dialogue: 0,0:17:43.42,0:17:45.15,Default,,0000,0000,0000,,Mờ cô bước lên phía trước, Mrs. Sakurai.
Dialogue: 0,0:17:45.26,0:17:47.52,Default,,0000,0000,0000,,Xin mời cô.
Dialogue: 0,0:17:51.20,0:17:53.93,Default,,0000,0000,0000,,M, Mrs. Sakurai, đó chẳng phải là...
Dialogue: 0,0:17:57.44,0:17:58.63,Default,,0000,0000,0000,,D, Dì!?
Dialogue: 0,0:18:00.14,0:18:02.90,Default,,0000,0000,0000,,Thấy ko, mẹ con lúc nào cũng là số một.
Dialogue: 0,0:18:04.75,0:18:05.94,Default,,0000,0000,0000,,C, Cha!?
Dialogue: 0,0:18:07.72,0:18:11.05,Default,,0000,0000,0000,,Đây là $10,000 tiền thưởng và vương miện Tân hoa hậu.
Dialogue: 0,0:18:11.15,0:18:13.35,Default,,0000,0000,0000,,Các quý ông, xin hãy bình tĩnh.
Dialogue: 0,0:18:13.42,0:18:14.55,Default,,0000,0000,0000,,Vậy ra, dì ấy là....
Dialogue: 0,0:18:15.39,0:18:21.26,Default,,0000,0000,0000,,Một tràng pháo tay nhiệt liệt cho Tân hoa hậu ngực của chúng ta, Mrs. Sakurai !
Dialogue: 0,0:18:34.21,0:18:37.84,Default,,0000,0000,0000,,Ko ngờ mẹ kế của Wataru-chan cũng tham gia cuộc thi này.
Dialogue: 0,0:18:37.91,0:18:39.94,Default,,0000,0000,0000,,Ko lẽ chúng ta phải chấm dứt chuyện này ở đây.
Dialogue: 0,0:18:40.31,0:18:45.22,Default,,0000,0000,0000,,Ko vấn đề gì, sự thật là cả ba chúng ta đều đã làm chuyện này với cậu ấy.
Dialogue: 0,0:18:45.29,0:18:46.62,Default,,0000,0000,0000,,Em nói đúng.
Dialogue: 0,0:18:46.69,0:18:51.09,Default,,0000,0000,0000,,Thế nên chúng ta sẽ tiếp tục cuộc chơi này, nhưng theo luật mới.
Dialogue: 0,0:18:52.69,0:18:56.32,Default,,0000,0000,0000,,Mọi người bắt tôi cởi hết đồ ra thế này, như vậy là....
Dialogue: 0,0:18:57.93,0:19:01.20,Default,,0000,0000,0000,,Phải, cả bốn chúng ta sẽ làm chuyện đó cùng nhau.
Dialogue: 0,0:19:01.67,0:19:03.76,Default,,0000,0000,0000,,Luật chơi rất công bằng.
Dialogue: 0,0:19:04.24,0:19:08.17,Default,,0000,0000,0000,,Ai làm cậu thỏa mãn nhất sẽ thắng.
Dialogue: 0,0:19:08.88,0:19:12.74,Default,,0000,0000,0000,,Nhưng hôm nay cũng muộn rồi, vả lại tôi đang rất mệt.
Dialogue: 0,0:19:13.78,0:19:15.01,Default,,0000,0000,0000,,Đó chính là lí do đấy!
Dialogue: 0,0:19:15.08,0:19:17.61,Default,,0000,0000,0000,,Mệt mỏi thì cần phải thư giãn chứ!
Dialogue: 0,0:19:18.02,0:19:21.18,Default,,0000,0000,0000,,Bọn mình sẽ giúp cậu thư giãn!
Dialogue: 0,0:19:22.12,0:19:24.61,Default,,0000,0000,0000,,Đúng vậy, cô ấy nói phải đó, Wataru-kun.
Dialogue: 0,0:19:25.53,0:19:28.83,Default,,0000,0000,0000,,Cùng nhau tận hưởng nào.
Dialogue: 0,0:19:32.27,0:19:35.93,Default,,0000,0000,0000,,Thằng nhỏ của cậu mà ko bự lên thì bọn mình "quê" lắm đó.
Dialogue: 0,0:19:38.61,0:19:42.01,Default,,0000,0000,0000,,Hừ, có tôi ở đây thì chuyện đó xảy ra sao được.
Dialogue: 0,0:19:44.11,0:19:45.58,Default,,0000,0000,0000,,Cô coi thường tôi đó hả.
Dialogue: 0,0:19:45.65,0:19:48.58,Default,,0000,0000,0000,,Hai người bị sao vậy?
Dialogue: 0,0:19:49.05,0:19:51.98,Default,,0000,0000,0000,,Ý cô là chúng tôi có vấn đề sao?
Dialogue: 0,0:19:52.09,0:19:54.32,Default,,0000,0000,0000,,Đúng là thế rồi, chị ấy nhiều "kinh nghiệm" hơn chúng ta mà.
Dialogue: 0,0:19:54.42,0:19:56.95,Default,,0000,0000,0000,,Em chê chị già chứ gì?
Dialogue: 0,0:20:00.03,0:20:02.89,Default,,0000,0000,0000,,C, chúng ta đâu cần phải làm vậy
Dialogue: 0,0:20:03.10,0:20:05.83,Default,,0000,0000,0000,,Chị nói đúng, người có quyền quyết định...
Dialogue: 0,0:20:05.97,0:20:08.43,Default,,0000,0000,0000,,Là Wataru-chan.
Dialogue: 0,0:20:17.71,0:20:21.81,Default,,0000,0000,0000,,Wataru-kun, thằng nhỏ của cậu cứng như vậy là do ngực tôi phải ko.
Dialogue: 0,0:20:22.82,0:20:26.81,Default,,0000,0000,0000,,Đúng vậy, ngực của em khiến cậu ấy bự lên như vậy.
Dialogue: 0,0:20:30.06,0:20:32.58,Default,,0000,0000,0000,,Lena, đừng bảo tôi là cô sắp....
Dialogue: 0,0:20:34.26,0:20:36.09,Default,,0000,0000,0000,,N, nhưng mà...
Dialogue: 0,0:20:36.73,0:20:40.10,Default,,0000,0000,0000,,Tôi ko thể kiềm chế nổi.
Dialogue: 0,0:20:40.30,0:20:42.56,Default,,0000,0000,0000,,Ai bảo cô thích tranh ngồi trên đó cơ.
Dialogue: 0,0:20:42.67,0:20:43.83,Default,,0000,0000,0000,,Đổi chỗ cho tôi nào!
Dialogue: 0,0:20:44.91,0:20:46.63,Default,,0000,0000,0000,,Hừ, cô thật là...
Dialogue: 0,0:20:49.58,0:20:53.07,Default,,0000,0000,0000,,Wataru-chan, thưởng thức mình đi nào.
Dialogue: 0,0:20:57.42,0:21:00.25,Default,,0000,0000,0000,,Wataru-chan, tuyệt, tuyệt quá đi mất!
Dialogue: 0,0:21:01.42,0:21:03.58,Default,,0000,0000,0000,,Tomoka, em chơi xấu vậy.
Dialogue: 0,0:21:03.66,0:21:06.75,Default,,0000,0000,0000,,Làm vậy thì Wataru-kun sao có thể chọn lựa công bằng chứ.
Dialogue: 0,0:21:07.83,0:21:10.02,Default,,0000,0000,0000,,Ai thèm quan tâm chứ.
Dialogue: 0,0:21:11.23,0:21:14.13,Default,,0000,0000,0000,,Quá khó để kiềm chế ham muốn của mình mỗi khi ở bên Wataru-chan.
Dialogue: 0,0:21:15.17,0:21:17.64,Default,,0000,0000,0000,,Liếm mình mạnh nữa lên nào!
Dialogue: 0,0:21:19.77,0:21:20.90,Default,,0000,0000,0000,,Cô ấy nói đúng!
Dialogue: 0,0:21:24.58,0:21:26.74,Default,,0000,0000,0000,,Wataru, ngon quá...
Dialogue: 0,0:21:33.69,0:21:37.68,Default,,0000,0000,0000,,Wataru-chan, tuyệt quá..nữa đi, nữa!
Dialogue: 0,0:21:40.63,0:21:43.06,Default,,0000,0000,0000,,Hai người chơi xấu quá.
Dialogue: 0,0:21:43.13,0:21:45.00,Default,,0000,0000,0000,,Thật bất công...
Dialogue: 0,0:21:46.40,0:21:47.66,Default,,0000,0000,0000,,Tôi cũng muốn điều đó!
Dialogue: 0,0:21:49.84,0:21:51.57,Default,,0000,0000,0000,,W, Wataru-kun!
Dialogue: 0,0:21:52.44,0:21:57.57,Default,,0000,0000,0000,,Nữa đi, nữa đi...vuốt ve nó và làm cho nó ướt đẫm đi!
Dialogue: 0,0:22:00.01,0:22:04.21,Default,,0000,0000,0000,,Wataru-chan, mình...ra...rồi!
Dialogue: 0,0:22:17.50,0:22:19.06,Default,,0000,0000,0000,,Xin lỗi, Wataru-chan.
Dialogue: 0,0:22:26.91,0:22:28.93,Default,,0000,0000,0000,,Là sữa "tình yêu" của Wataru-kun...
Dialogue: 0,0:22:39.05,0:22:40.14,Default,,0000,0000,0000,,Fumie!
Dialogue: 0,0:22:41.86,0:22:42.79,Default,,0000,0000,0000,,Ngon quá...
Dialogue: 0,0:22:47.56,0:22:49.65,Default,,0000,0000,0000,,N, nhột quá.
Dialogue: 0,0:23:02.74,0:23:04.23,Default,,0000,0000,0000,,Wataru!
Dialogue: 0,0:23:05.78,0:23:08.81,Default,,0000,0000,0000,,Chúng tôi làm cậu kích thích đến mức này sao?
Dialogue: 0,0:23:08.92,0:23:10.54,Default,,0000,0000,0000,,P, phải.
Dialogue: 0,0:23:11.22,0:23:13.35,Default,,0000,0000,0000,,Vậy thì mình vui vẻ tiếp nào.
Dialogue: 0,0:23:13.92,0:23:16.32,Default,,0000,0000,0000,,Phải đó, với cả hai chúng tôi.
Dialogue: 0,0:23:16.49,0:23:17.62,Default,,0000,0000,0000,,Được.
Dialogue: 0,0:23:19.26,0:23:20.23,Default,,0000,0000,0000,,T, tôi vào đây!
Dialogue: 0,0:23:22.60,0:23:24.12,Default,,0000,0000,0000,,N, như vậy là sao?
Dialogue: 0,0:23:34.51,0:23:35.53,Default,,0000,0000,0000,,Khít quá!
Dialogue: 0,0:23:37.61,0:23:38.74,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:23:43.08,0:23:47.88,Default,,0000,0000,0000,,Hừ, đáng lẽ ra mình phải được làm chuyện đó với chị ấy.
Dialogue: 0,0:23:48.49,0:23:53.39,Default,,0000,0000,0000,,Đừng phàn nàn nữa, chính em là người khơi mào cho chuyện này mà.
Dialogue: 0,0:23:53.89,0:23:59.16,Default,,0000,0000,0000,,Phải đó, chúng tôi chỉ làm như cô nói thôi mà...
Dialogue: 0,0:23:59.93,0:24:02.37,Default,,0000,0000,0000,,Biết rồi, đừng nói nữa.
Dialogue: 0,0:24:05.44,0:24:06.91,Default,,0000,0000,0000,,Wataru-chan...
Dialogue: 0,0:24:07.74,0:24:08.47,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:24:09.08,0:24:12.41,Default,,0000,0000,0000,,Kết thúc hiệp này mau lên, mình đang chờ hiệp sau của cậu đó, vậy nhé?
Dialogue: 0,0:24:23.52,0:24:26.32,Default,,0000,0000,0000,,Wataru-kun, ko việc gì phải vội hết.
Dialogue: 0,0:24:26.39,0:24:28.09,Default,,0000,0000,0000,,Hãy từ từ tận hưởng nó.
Dialogue: 0,0:24:28.53,0:24:30.26,Default,,0000,0000,0000,,Nhưng mà, tôi sắp, tôi sắp!!
Dialogue: 0,0:24:30.86,0:24:34.46,Default,,0000,0000,0000,,L, Lena, nếu cô siết tôi chặt như vậy, cô sẽ....
Dialogue: 0,0:24:47.61,0:24:49.78,Default,,0000,0000,0000,,Wataru-chan, cậu thật tuyệt vời!
Dialogue: 0,0:24:50.78,0:24:55.41,Default,,0000,0000,0000,,Vừa ra xong mà thằng nhỏ của cậu vẫn...cứng như vậy!
Dialogue: 0,0:24:55.96,0:24:57.69,Default,,0000,0000,0000,,Bởi vì chuyện này thật tuyệt!
Dialogue: 0,0:25:00.33,0:25:02.96,Default,,0000,0000,0000,,N, Nhưng có lẽ đây sẽ là hiệp cuối cùng của mình hôm nay.
Dialogue: 0,0:25:05.00,0:25:06.16,Default,,0000,0000,0000,,Ko được!
Dialogue: 0,0:25:09.04,0:25:13.44,Default,,0000,0000,0000,,Wataru, kết thúc nhanh hiệp này đi nào.
Dialogue: 0,0:25:13.44,0:25:16.88,Default,,0000,0000,0000,,Đêm vẫn còn dài, cậu phải làm cả hai chúng tôi thỏa mãn đã.
Dialogue: 0,0:25:16.88,0:25:19.11,Default,,0000,0000,0000,,Đừng có làm phiền bọn em chứ!
Dialogue: 0,0:25:19.21,0:25:22.11,Default,,0000,0000,0000,,Vẫn đang là hiệp của em mà.
Dialogue: 0,0:25:23.58,0:25:25.64,Default,,0000,0000,0000,,Uh, vậy thì tiếp tục đi!
Dialogue: 0,0:25:25.72,0:25:27.19,Default,,0000,0000,0000,,Ra nhanh lên nào cưng.
Dialogue: 0,0:25:27.25,0:25:29.88,Default,,0000,0000,0000,,Ko, ko phải lúc này, ko phải lúc này!
Dialogue: 0,0:25:30.29,0:25:34.73,Default,,0000,0000,0000,,Ko phải lúc này, ya, ya...ya!
Dialogue: 0,0:25:36.93,0:25:38.36,Default,,0000,0000,0000,,Tốt lắm, Wataru!
Dialogue: 0,0:25:38.43,0:25:39.87,Default,,0000,0000,0000,,Lại đây nào.
Dialogue: 0,0:25:46.21,0:25:47.30,Default,,0000,0000,0000,,Khoan đã.
Dialogue: 0,0:25:49.44,0:25:52.31,Default,,0000,0000,0000,,Wataru-kun, mau bự lên nào.
Dialogue: 0,0:25:52.95,0:25:54.54,Default,,0000,0000,0000,,Nó xìu đi rồi.
Dialogue: 0,0:25:55.32,0:25:58.11,Default,,0000,0000,0000,,Cậu "đo giường" rồi sao, Wataru-kun?
Dialogue: 0,0:25:59.45,0:26:02.32,Default,,0000,0000,0000,,Ko được, ai cho phép cậu dừng lúc này hả.
Dialogue: 0,0:26:03.29,0:26:06.09,Default,,0000,0000,0000,,Wataru-kun, coi nào...cậu làm được mà.
Dialogue: 0,0:26:06.49,0:26:07.96,Default,,0000,0000,0000,,Cố lên nào, Wataru.
Dialogue: 0,0:26:08.60,0:26:10.59,Default,,0000,0000,0000,,Wataru-kun, sướng ko hả?
Dialogue: 0,0:26:10.70,0:26:12.13,Default,,0000,0000,0000,,Wataru...
Dialogue: 0,0:26:13.53,0:26:15.87,Default,,0000,0000,0000,,Có vẻ như mình là người may mắn nhất.
Dialogue: 0,0:26:34.76,0:26:39.72,Default,,0000,0000,0000,,Fumie, Tomoka, hai cô dậy cho bò và ngựa ăn đi nào.
Dialogue: 0,0:26:41.13,0:26:46.03,Default,,0000,0000,0000,,Tại sao chúng tôi phải làm điều đó?
Dialogue: 0,0:26:47.83,0:26:50.27,Default,,0000,0000,0000,,Kệ cô ta đi.
Dialogue: 0,0:26:50.34,0:26:53.36,Default,,0000,0000,0000,,Mệt chết đi được, dậy sao nổi.
Dialogue: 0,0:26:54.94,0:26:57.43,Default,,0000,0000,0000,,Đêm qua đúng là kinh hoàng.
Dialogue: 0,0:26:57.51,0:27:01.21,Default,,0000,0000,0000,,Phải, đến mức mà cậu ấy lại va đầu vào tường lần nữa.
Dialogue: 0,0:27:01.78,0:27:04.18,Default,,0000,0000,0000,,Hai cô là khách ko mời ở đây.
Dialogue: 0,0:27:04.25,0:27:06.55,Default,,0000,0000,0000,,Ít ra hai cô cũng phải làm gì đó chứ.
Dialogue: 0,0:27:06.79,0:27:10.81,Default,,0000,0000,0000,,Đó là do cô cứ giữ rịt Wataru-kun ở đây.
Dialogue: 0,0:27:10.89,0:27:13.12,Default,,0000,0000,0000,,Dù sao, cậu ấy cũng đã phục hồi trí nhớ.
Dialogue: 0,0:27:13.53,0:27:15.93,Default,,0000,0000,0000,,Này hai người, quên chuyện đó đi.
Dialogue: 0,0:27:16.30,0:27:19.09,Default,,0000,0000,0000,,Đánh thức Wataru-chan nào.
Dialogue: 0,0:27:21.07,0:27:23.16,Default,,0000,0000,0000,,Theo cách của chúng ta...
Dialogue: 0,0:27:26.51,0:27:27.80,Default,,0000,0000,0000,,Nhất trí.
Dialogue: 0,0:27:29.78,0:27:32.04,Default,,0000,0000,0000,,Wataru, dậy đi nào.
Dialogue: 0,0:27:32.15,0:27:35.67,Default,,0000,0000,0000,,Cậu định ngủ nướng bao lâu nữa hả.
Dialogue: 0,0:27:35.78,0:27:39.18,Default,,0000,0000,0000,,Còn nằm lì ở đấy là ko có "quà" đâu nhé.
Dialogue: 0,0:27:44.06,0:27:46.42,Default,,0000,0000,0000,,Các cô...là ai?
Dialogue: 0,0:27:49.13,0:27:50.86,Default,,0000,0000,0000,,Sao các cô lại khỏa thân hết thế này?
Dialogue: 0,0:27:51.67,0:27:54.10,Default,,0000,0000,0000,,Đây...là đâu?
Dialogue: 0,0:27:54.90,0:27:56.84,Default,,0000,0000,0000,,Huh, quần áo tôi đâu rồi!?
Dialogue: 0,0:28:01.11,0:28:04.57,Default,,0000,0000,0000,,Wataru, đừng vậy chứ!?
