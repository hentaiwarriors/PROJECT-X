Dialogue: 0,0:00:05.48,0:00:10.28,Default,,0000,0000,0000,,Hè rồi mà mình vẫn phải đến trường, chỉ vì bỏ quên quyển vở...
Dialogue: 0,0:00:13.52,0:00:16.34,Default,,0000,0000,0000,,Nếu đang là tiết học bơi thì hay biết mấy.
Dialogue: 0,0:00:17.87,0:00:24.79,Alt,,0000,0000,0000,,{\an8\fad(600,600)\pos(496,15.667)\bord3\xshad5\4c&H78006A&\c&H7F00E0&\3c&H2BC5F6&}Baka-sub trân trọng giới thiệu
Dialogue: 0,0:00:28.96,0:00:31.28,Default,,0000,0000,0000,,Hy vọng sẽ không có ai thấy mình.
Dialogue: 0,0:00:31.28,0:00:34.53,Default,,0000,0000,0000,,Nghỉ hè rồi mà lại mò vào đây, trông ngu hết chỗ nói.
Dialogue: 0,0:00:36.37,0:00:37.99,Default,,0000,0000,0000,,Tiếng gì vậy?
Dialogue: 0,0:00:37.99,0:00:40.97,Default,,0000,0000,0000,,Nó phát ra từ phòng thay đồ.
Dialogue: 0,0:00:44.88,0:00:45.89,Default,,0000,0000,0000,,Cô gái ấy...
Dialogue: 0,0:00:45.89,0:00:48.42,Default,,0000,0000,0000,,Không phải bạn cùng lớp mình... Natsumize Sukumi sao?
Dialogue: 0,0:00:52.34,0:00:54.40,Default,,0000,0000,0000,,Sướng quá đi...
Dialogue: 0,0:00:57.06,0:01:00.05,Default,,0000,0000,0000,,Nếu mình sờ mó nhiều như vậy, nó sẽ thấm xuyên qua mất.
Dialogue: 0,0:01:00.05,0:01:01.07,Default,,0000,0000,0000,,Chỗ đó...
Dialogue: 0,0:01:03.25,0:01:04.93,Default,,0000,0000,0000,,Sướng quá... Sướng quá...
Dialogue: 0,0:01:04.93,0:01:06.12,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:01:07.21,0:01:08.58,Default,,0000,0000,0000,,"Tuyệt quá"?
Dialogue: 0,0:01:09.44,0:01:11.45,Default,,0000,0000,0000,,C-C-Cậu đang làm cái quái gì vậy hả?
Dialogue: 0,0:01:12.31,0:01:13.09,Default,,0000,0000,0000,,Tớ xin lỗi!
Dialogue: 0,0:01:13.47,0:01:17.55,Default,,0000,0000,0000,,Cậu... Cậu là... Hình như cái gì đó-yui!
Dialogue: 0,0:01:17.55,0:01:19.95,Default,,0000,0000,0000,,"Cái gì đó"?
Dialogue: 0,0:01:19.95,0:01:23.67,Default,,0000,0000,0000,,Còn muốn đứng ngoài đó nhìn đến bao giờ hả? Mau vào đây!
Dialogue: 0,0:01:32.12,0:01:34.14,Default,,0000,0000,0000,,Sao của cậu lại to lên vậy hả?
Dialogue: 0,0:01:34.14,0:01:36.48,Default,,0000,0000,0000,,Vải lại, sao nó lại chui ra ngoài thế kia?
Dialogue: 0,0:01:36.48,0:01:37.69,Default,,0000,0000,0000,,Hết cách rồi!
Dialogue: 0,0:01:37.69,0:01:39.32,Default,,0000,0000,0000,,Bởi vì cậu... Ơ...
Dialogue: 0,0:01:40.44,0:01:41.92,Default,,0000,0000,0000,,Quả nhiên là cậu đã nhìn thấy đúng không?
Dialogue: 0,0:01:41.92,0:01:44.55,Default,,0000,0000,0000,,Cậu đã nhìn thấy tôi thủ đâm phải không?
Dialogue: 0,0:01:44.55,0:01:45.83,Default,,0000,0000,0000,,Đồ biến thái!
Dialogue: 0,0:01:45.83,0:01:47.45,Default,,0000,0000,0000,,N-Nhưng mà...
Dialogue: 0,0:01:47.45,0:01:49.74,Default,,0000,0000,0000,,{\i1}Ai{\i0} cho cậu giấu hả!?
Dialogue: 0,0:01:50.79,0:01:53.59,Default,,0000,0000,0000,,Giấu thế đâu có công bằng!
Dialogue: 0,0:01:54.06,0:01:55.93,Default,,0000,0000,0000,,Thế tớ phải làm sao đây?
Dialogue: 0,0:01:55.93,0:01:57.64,Default,,0000,0000,0000,,Đ-Để xem nào.
Dialogue: 0,0:01:57.64,0:01:59.60,Default,,0000,0000,0000,,Chạm vào nó đi!
Dialogue: 0,0:01:59.60,0:02:01.05,Default,,0000,0000,0000,,Thủ dâm ấy.
Dialogue: 0,0:02:01.55,0:02:03.05,Default,,0000,0000,0000,,Không thể nào...
Dialogue: 0,0:02:03.05,0:02:04.40,Default,,0000,0000,0000,,Không làm được à??
Dialogue: 0,0:02:04.40,0:02:06.60,Default,,0000,0000,0000,,Ừm. Cái đó...
Dialogue: 0,0:02:06.60,0:02:08.94,Default,,0000,0000,0000,,Vậy à? Tôi biết rồi.
Dialogue: 0,0:02:08.94,0:02:10.11,Default,,0000,0000,0000,,Thế thì...
Dialogue: 0,0:02:10.11,0:02:13.20,Default,,0000,0000,0000,,Ơ, Natsumize-san, không lẽ cậu định...
Dialogue: 0,0:02:13.20,0:02:17.45,Default,,0000,0000,0000,,Tôi sẽ hét to lên. "Cứu tôi với! Có người định hiếp tôi!"
Dialogue: 0,0:02:17.45,0:02:18.60,Default,,0000,0000,0000,,K-Khoan đã!
Dialogue: 0,0:02:18.60,0:02:21.15,Default,,0000,0000,0000,,Thế thì cậu hãy biết vâng lời.
Dialogue: 0,0:02:23.14,0:02:27.12,Default,,0000,0000,0000,,Lúc nãy không nhìn rõ lắm, nhưng sao cái thứ {\i1}chết tiệt{\i0} này to vậy hả?
Dialogue: 0,0:02:27.12,0:02:30.20,Default,,0000,0000,0000,,Thật là, vẻ mặt cứ như con gái, mà dưới này thì...
Dialogue: 0,0:02:30.20,0:02:32.33,Default,,0000,0000,0000,,Nào, làm cho tôi xem coi.
Dialogue: 0,0:02:35.68,0:02:36.84,Default,,0000,0000,0000,,Làm thật kìa.
Dialogue: 0,0:02:36.84,0:02:39.09,Default,,0000,0000,0000,,Cậu bảo tớ làm mà!
Dialogue: 0,0:02:39.09,0:02:42.57,Default,,0000,0000,0000,,Hể, với con trai là vậy ư.
Dialogue: 0,0:02:42.57,0:02:44.06,Default,,0000,0000,0000,,Trông ngu ngu sao ấy.
Dialogue: 0,0:02:44.06,0:02:46.39,Default,,0000,0000,0000,,Giờ nó còn to hơn trước.
Dialogue: 0,0:02:46.39,0:02:49.56,Default,,0000,0000,0000,,Cái gì làm cậu hứng tình thế hả?
Dialogue: 0,0:02:49.56,0:02:53.82,Default,,0000,0000,0000,,Đò là vì Natsumize-san... Khi nhìn cậu thủ dâm tớ...
Dialogue: 0,0:02:53.82,0:02:55.61,Default,,0000,0000,0000,,Cậu là cô gái dễ thương nhất lớp.
Dialogue: 0,0:02:55.61,0:03:00.28,Default,,0000,0000,0000,,Đừng có nghĩ là tôi sẽ vui vì lời khen đó đâu nhé!
Dialogue: 0,0:03:01.57,0:03:04.10,Default,,0000,0000,0000,,Vậy thì... Thế này thì sao?
Dialogue: 0,0:03:04.62,0:03:06.02,Default,,0000,0000,0000,,Thế nào?
Dialogue: 0,0:03:06.02,0:03:07.12,Default,,0000,0000,0000,,"Thế nào"?
Dialogue: 0,0:03:07.12,0:03:09.63,Default,,0000,0000,0000,,Không hứng tình à?
Dialogue: 0,0:03:09.63,0:03:11.07,Default,,0000,0000,0000,,Cái đó...
Dialogue: 0,0:03:11.07,0:03:12.45,Default,,0000,0000,0000,,Thế này thì sao?
Dialogue: 0,0:03:15.35,0:03:17.13,Default,,0000,0000,0000,,Hứng tình chưa?
Dialogue: 0,0:03:18.31,0:03:19.01,Default,,0000,0000,0000,,Ừm.
Dialogue: 0,0:03:19.40,0:03:21.40,Default,,0000,0000,0000,,Đã vậy thì...
Dialogue: 0,0:03:24.10,0:03:27.06,Default,,0000,0000,0000,,Trực tiếp chạm vào nó sướng quá đi.
Dialogue: 0,0:03:27.06,0:03:29.19,Default,,0000,0000,0000,,C-Cậu thì sao hả?
Dialogue: 0,0:03:29.95,0:03:32.44,Default,,0000,0000,0000,,Ừm, hứng cực kì luôn.
Dialogue: 0,0:03:32.80,0:03:35.72,Default,,0000,0000,0000,,Gì chứ? Cậu hứng thật ư?
Dialogue: 0,0:03:35.72,0:03:36.94,Default,,0000,0000,0000,,Đồ biến thái.
Dialogue: 0,0:03:40.76,0:03:43.22,Default,,0000,0000,0000,,Bởi vì cậu tạo ra những âm thanh dâm đãng,
Dialogue: 0,0:03:44.30,0:03:45.91,Default,,0000,0000,0000,,và của cậu vừa hồng lại vừa đẹp...
Dialogue: 0,0:03:45.91,0:03:49.42,Default,,0000,0000,0000,,Này! Đừng nói những lời đó!
Dialogue: 0,0:03:56.52,0:03:57.41,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:04:00.85,0:04:01.74,Default,,0000,0000,0000,,Tớ cũng...
Dialogue: 0,0:04:06.25,0:04:11.28,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:04:18.74,0:04:20.15,Default,,0000,0000,0000,,Thật là!
Dialogue: 0,0:04:20.15,0:04:22.75,Default,,0000,0000,0000,,Dám thủ dâm trước mặt con gái.
Dialogue: 0,0:04:22.75,0:04:24.81,Default,,0000,0000,0000,,Cậu đúng là biến thái.
Dialogue: 0,0:04:24.81,0:04:26.16,Default,,0000,0000,0000,,Bởi vì...
Dialogue: 0,0:04:26.16,0:04:27.24,Default,,0000,0000,0000,,Đúng rồi!
Dialogue: 0,0:04:27.24,0:04:30.58,Default,,0000,0000,0000,,Cậu phải đến đây mỗi ngày vào giờ này nghe chưa.
Dialogue: 0,0:04:31.15,0:04:32.33,Default,,0000,0000,0000,,Mỗi ngày?!
Dialogue: 0,0:04:32.75,0:04:34.81,Default,,0000,0000,0000,,Thế thì tôi la lên đó...
Dialogue: 0,0:04:34.81,0:04:37.44,Default,,0000,0000,0000,,Tớ biết rồi! Tớ sẽ đến mỗi ngày.
Dialogue: 0,0:04:37.44,0:04:38.34,Default,,0000,0000,0000,,Tốt.
Dialogue: 0,0:04:45.71,0:04:46.81,Default,,0000,0000,0000,,Màu trắng ư?
Dialogue: 0,0:04:46.81,0:04:48.35,Default,,0000,0000,0000,,Sao nó lại ở đây?
Dialogue: 0,0:04:49.70,0:04:51.26,Default,,0000,0000,0000,,Cậu muốn tôi mặc nó sao?
Dialogue: 0,0:04:51.26,0:04:53.21,Default,,0000,0000,0000,,Ừm. Tớ nghĩ nó hợp với cậu lắm.
Dialogue: 0,0:04:53.21,0:04:56.79,Default,,0000,0000,0000,,Cậu nói cái quái gì vậy. Tôi đã mặc đồ bơi rồi còn gì.
Dialogue: 0,0:04:56.79,0:04:59.49,Default,,0000,0000,0000,,Sao còn phải thay cái thứ đó cơ chứ?
Dialogue: 0,0:04:59.49,0:05:03.72,Default,,0000,0000,0000,,Ơ, tớ nghĩ đồ bơi màu trắng sẽ đặc biệt hơn một chút...
Dialogue: 0,0:05:05.49,0:05:06.87,Default,,0000,0000,0000,,Cậu cuồng đồ bơi à?
Dialogue: 0,0:05:08.70,0:05:11.08,Default,,0000,0000,0000,,Cái đó... Vâng.
Dialogue: 0,0:05:11.08,0:05:12.81,Default,,0000,0000,0000,,Thật đáng tiếc.
Dialogue: 0,0:05:12.81,0:05:15.65,Default,,0000,0000,0000,,Bởi vì tôi ghét đồ bơi.
Dialogue: 0,0:05:16.38,0:05:17.45,Default,,0000,0000,0000,,Phải, phải.
Dialogue: 0,0:05:17.45,0:05:22.19,Default,,0000,0000,0000,,Bởi vậy nên {\i1}lúc đó{\i0} thằng nhỏ của cậu mới lớn như vậy.
Dialogue: 0,0:05:22.19,0:05:23.89,Default,,0000,0000,0000,,Ơ, cái đó...
Dialogue: 0,0:05:23.89,0:05:24.81,Default,,0000,0000,0000,,Nhìn này?
Dialogue: 0,0:05:27.66,0:05:29.42,Default,,0000,0000,0000,,Tôi biết ngay mà.
Dialogue: 0,0:05:30.36,0:05:32.23,Default,,0000,0000,0000,,Cậu đã lớn như vầy.
Dialogue: 0,0:05:32.23,0:05:34.45,Default,,0000,0000,0000,,Cơ thể cậu không biết nói dối.
Dialogue: 0,0:05:36.91,0:05:38.61,Default,,0000,0000,0000,,Nó đang rộn lên.
Dialogue: 0,0:05:40.47,0:05:43.25,Default,,0000,0000,0000,,Hình như là phải mơn trớn như vầy.
Dialogue: 0,0:05:44.42,0:05:46.50,Default,,0000,0000,0000,,Cái mặt đó là sao vậy?
Dialogue: 0,0:05:46.50,0:05:48.07,Default,,0000,0000,0000,,Sướng lắm ư?
Dialogue: 0,0:05:50.49,0:05:53.31,Default,,0000,0000,0000,,Như vầy sướng lắm phải không?
Dialogue: 0,0:05:57.91,0:06:00.44,Default,,0000,0000,0000,,Với cậu thế này đã đủ rồi sao?
Dialogue: 0,0:06:02.23,0:06:06.55,Default,,0000,0000,0000,,Nghe không hả? Tôi hỏi là {\i1}chỉ{\i0} làm thế này là đủ rồi sao?
Dialogue: 0,0:06:07.83,0:06:09.30,Default,,0000,0000,0000,,Tớ muốn cậu...
Dialogue: 0,0:06:09.30,0:06:10.41,Default,,0000,0000,0000,,Muốn gì tôi?
Dialogue: 0,0:06:12.93,0:06:13.86,Default,,0000,0000,0000,,Âm hộ của cậu!
Dialogue: 0,0:06:13.86,0:06:15.10,Default,,0000,0000,0000,,Tớ muốn chạm vào nó.
Dialogue: 0,0:06:15.10,0:06:16.46,Default,,0000,0000,0000,,Đừng hòng.
Dialogue: 0,0:06:16.46,0:06:17.78,Default,,0000,0000,0000,,Không thể nào!
Dialogue: 0,0:06:18.54,0:06:20.55,Default,,0000,0000,0000,,Thái độ đó là sao hả?
Dialogue: 0,0:06:20.55,0:06:21.80,Default,,0000,0000,0000,,Bởi vì...
Dialogue: 0,0:06:21.80,0:06:23.90,Default,,0000,0000,0000,,Thật là hết cách.
Dialogue: 0,0:06:36.81,0:06:38.83,Default,,0000,0000,0000,,Ai nói là cho cậu sờ trực tiếp hả?
Dialogue: 0,0:06:40.67,0:06:41.84,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:06:41.84,0:06:46.34,Default,,0000,0000,0000,,Không được, tôi sẽ không dừng lại được mất!
Dialogue: 0,0:06:50.66,0:06:52.91,Default,,0000,0000,0000,,Sukumi... Tớ...
Dialogue: 0,0:06:54.81,0:06:56.81,Default,,0000,0000,0000,,Tôi sẽ không để cậu ra đâu.
Dialogue: 0,0:06:56.81,0:06:58.96,Default,,0000,0000,0000,,Tớ... Tớ...
Dialogue: 0,0:06:58.96,0:07:00.52,Default,,0000,0000,0000,,Gì hả?
Dialogue: 0,0:07:02.21,0:07:03.64,Default,,0000,0000,0000,,Cậu làm gì vậy hả?
Dialogue: 0,0:07:03.64,0:07:07.66,Default,,0000,0000,0000,,Tớ muốn đút thằng nhỏ vào trong... âm hộ của cậu!
Dialogue: 0,0:07:08.50,0:07:09.91,Default,,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:07:12.62,0:07:14.86,Default,,0000,0000,0000,,Hừm! Tự nhiên đè người ta xuống như vậy.
Dialogue: 0,0:07:14.86,0:07:17.44,Default,,0000,0000,0000,,Cậu bị đần hả? Đồ biến thái!
Dialogue: 0,0:07:18.06,0:07:19.34,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:07:19.34,0:07:20.88,Default,,0000,0000,0000,,Xuống dưới một chút.
Dialogue: 0,0:07:23.84,0:07:26.36,Default,,0000,0000,0000,,Chỗ đó...
Dialogue: 0,0:07:30.09,0:07:32.05,Default,,0000,0000,0000,,Nó vào bên trong rồi.
Dialogue: 0,0:07:32.05,0:07:33.51,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:07:33.51,0:07:36.06,Default,,0000,0000,0000,,Thứ to lớn đó đang vào bên trong tôi!
Dialogue: 0,0:07:36.55,0:07:39.86,Default,,0000,0000,0000,,Tớ xin lỗi, Sukumi, nhưng tớ không thể dừng lại được.
Dialogue: 0,0:07:39.86,0:07:41.38,Default,,0000,0000,0000,,Nó lấp đầy tôi rồi!
Dialogue: 0,0:07:41.38,0:07:45.07,Default,,0000,0000,0000,,Âm hộ của tôi bị thằng nhỏ của cậu lấp đầy rồi!
Dialogue: 0,0:07:45.07,0:07:47.64,Default,,0000,0000,0000,,Tớ di chuyển đây, Sukumi.
Dialogue: 0,0:07:47.64,0:07:48.82,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:07:52.11,0:07:53.05,Default,,0000,0000,0000,,Nó đang bị banh ra!
Dialogue: 0,0:07:53.05,0:07:55.03,Default,,0000,0000,0000,,Nó đang bị banh rộng ra!
Dialogue: 0,0:07:55.97,0:07:59.51,Default,,0000,0000,0000,,Tôi có thể cảm thấy nó đang cọ xát bên trong!
Dialogue: 0,0:07:59.51,0:08:00.67,Default,,0000,0000,0000,,Nó đang di chuyển.
Dialogue: 0,0:08:00.67,0:08:04.30,Default,,0000,0000,0000,,Tôi có thể cảm thấy thằng nhỏ mập mạp của cậu di chuyển bên trong tôi!
Dialogue: 0,0:08:04.30,0:08:06.42,Default,,0000,0000,0000,,Tuy đau nhưng mà sướng!
Dialogue: 0,0:08:06.42,0:08:08.26,Default,,0000,0000,0000,,Tớ sẽ làm nhanh hơn nữa.
Dialogue: 0,0:08:09.51,0:08:11.76,Default,,0000,0000,0000,,Không! Tôi đến giới hạn rồi!
Dialogue: 0,0:08:13.14,0:08:16.36,Default,,0000,0000,0000,,Nóng quá! Bên trong tôi nóng quá!
Dialogue: 0,0:08:16.36,0:08:19.95,Default,,0000,0000,0000,,Thằng nhỏ lại to lên nữa rồi!
Dialogue: 0,0:08:19.95,0:08:23.94,Default,,0000,0000,0000,,Cái này là làm tình... Mình đang làm tình với Sukumi!
Dialogue: 0,0:08:23.94,0:08:25.31,Default,,0000,0000,0000,,Im đê, im đê!
Dialogue: 0,0:08:25.31,0:08:29.89,Default,,0000,0000,0000,,Không đời nào có chuyện đó!
Dialogue: 0,0:08:30.49,0:08:33.49,Default,,0000,0000,0000,,Sukumi, tiếng rên của cậu thật tuyệt.
Dialogue: 0,0:08:33.49,0:08:36.21,Default,,0000,0000,0000,,Tôi không có!
Dialogue: 0,0:08:39.84,0:08:41.52,Default,,0000,0000,0000,,Cậu sướng lắm phải không?
Dialogue: 0,0:08:41.52,0:08:44.70,Default,,0000,0000,0000,,Sướng cái con khỉ mốc?!
Dialogue: 0,0:08:46.80,0:08:50.96,Default,,0000,0000,0000,,Cứ đút ra đút vào như thế thì làm sao sướng được cơ chứ...
Dialogue: 0,0:08:50.96,0:08:52.61,Default,,0000,0000,0000,,Nó chạm đến tận bên trong.
Dialogue: 0,0:08:58.27,0:09:00.64,Default,,0000,0000,0000,,Nó chạm đến rồi!
Dialogue: 0,0:09:01.46,0:09:05.15,Default,,0000,0000,0000,,Thằng nhỏ... Thằng nhỏ đã chạm đến mọi ngõ ngách bên trong tôi!
Dialogue: 0,0:09:05.15,0:09:08.44,Default,,0000,0000,0000,,Nó muốn vào trong tử cung của tôi!
Dialogue: 0,0:09:08.44,0:09:09.79,Default,,0000,0000,0000,,Không được!
Dialogue: 0,0:09:09.79,0:09:11.58,Default,,0000,0000,0000,,Tôi...!
Dialogue: 0,0:09:13.15,0:09:14.56,Default,,0000,0000,0000,,Tớ cũng đến giới hạn rồi!
Dialogue: 0,0:09:23.70,0:09:26.41,Default,,0000,0000,0000,,Lần sao tớ sẽ làm tốt hơn.
Dialogue: 0,0:09:26.41,0:09:28.58,Default,,0000,0000,0000,,Ngày mai làm nữa nhé, Sukumi.
Dialogue: 0,0:09:36.68,0:09:39.68,Default,,0000,0000,0000,,Ơ, bỗng nhiên tớ tìm được cái này.
Dialogue: 0,0:09:39.68,0:09:43.58,Default,,0000,0000,0000,,Thôi lảm nhảm đi! Mau lôi thằng nhỏ đó ra đây!
Dialogue: 0,0:09:43.58,0:09:45.40,Default,,0000,0000,0000,,Ư-Ừm...
Dialogue: 0,0:09:49.36,0:09:52.88,Default,,0000,0000,0000,,Bị kích thích chỉ vì đồ bơi, cậu biến thái ghê nhỉ?
Dialogue: 0,0:09:52.88,0:09:55.51,Default,,0000,0000,0000,,Thế tại sao tôi còn phải mặc đồng phục hả?
Dialogue: 0,0:09:55.51,0:09:57.17,Default,,0000,0000,0000,,Tớ xin lỗi.
Dialogue: 0,0:09:57.17,0:10:01.30,Default,,0000,0000,0000,,Thấy không? Với cái thứ ngu ngốc và biếng thái như cậu thì dùng chân đã là quá đủ.
Dialogue: 0,0:10:01.30,0:10:03.06,Default,,0000,0000,0000,,Nào! Nào! Nào!
Dialogue: 0,0:10:06.38,0:10:09.25,Default,,0000,0000,0000,,Chỉ mình cậu được sướng, chả công bằng tí nào!
Dialogue: 0,0:10:09.25,0:10:11.53,Default,,0000,0000,0000,,Cho dù cậu nói vậy.
Dialogue: 0,0:10:18.05,0:10:19.15,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:10:21.00,0:10:23.41,Default,,0000,0000,0000,,Phải, chỗ đó.
Dialogue: 0,0:10:23.41,0:10:25.07,Default,,0000,0000,0000,,Liếm nữa đi.
Dialogue: 0,0:10:25.07,0:10:26.80,Default,,0000,0000,0000,,Liếm hết tất cả.
Dialogue: 0,0:10:26.80,0:10:28.12,Default,,0000,0000,0000,,Phải.
Dialogue: 0,0:10:28.12,0:10:29.37,Default,,0000,0000,0000,,Phải.
Dialogue: 0,0:10:30.44,0:10:32.55,Default,,0000,0000,0000,,Tuyệt quá đi mất.
Dialogue: 0,0:10:44.55,0:10:45.54,Default,,0000,0000,0000,,Sukumi.
Dialogue: 0,0:10:45.54,0:10:46.44,Default,,0000,0000,0000,,Cậu ra rồi ư?
Dialogue: 0,0:10:46.95,0:10:47.97,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:10:48.57,0:10:50.09,Default,,0000,0000,0000,,Đừng có hỏi mấy chuyện đó.
Dialogue: 0,0:10:50.87,0:10:52.30,Default,,0000,0000,0000,,Ừm.
Dialogue: 0,0:10:52.76,0:10:56.30,Default,,0000,0000,0000,,Cậu mau ngồi xuống ghế vào quay người lại.
Dialogue: 0,0:10:57.06,0:10:58.07,Default,,0000,0000,0000,,Cậu đang làm gì vậy?
Dialogue: 0,0:10:58.07,0:11:00.70,Default,,0000,0000,0000,,Im đi! Không được nhìn!
Dialogue: 0,0:11:00.70,0:11:01.81,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:11:02.96,0:11:06.03,Default,,0000,0000,0000,,Đây không phải là vì cậu đâu nhé.
Dialogue: 0,0:11:06.03,0:11:08.82,Default,,0000,0000,0000,,Tôi thích nên tôi mặc thôi.
Dialogue: 0,0:11:09.52,0:11:10.66,Default,,0000,0000,0000,,Cảm ơn cậu.
Dialogue: 0,0:11:12.70,0:11:13.53,Default,,0000,0000,0000,,Không được.
Dialogue: 0,0:11:13.53,0:11:15.91,Default,,0000,0000,0000,,Tôi sẽ không để cậu muốn làm gì thì làm đâu.
Dialogue: 0,0:11:16.47,0:11:20.81,Default,,0000,0000,0000,,Tôi không phải là loại dễ dãi đâu nhé.
Dialogue: 0,0:11:20.81,0:11:23.66,Default,,0000,0000,0000,,Hôm nay tôi sẽ làm cho cậu điên lên mới thôi.
Dialogue: 0,0:11:24.75,0:11:26.23,Default,,0000,0000,0000,,Chuẩn bị tinh thần đi.
Dialogue: 0,0:11:30.70,0:11:33.50,Default,,0000,0000,0000,,Thấy không? Nó đang vào đấy.
Dialogue: 0,0:11:34.01,0:11:36.03,Default,,0000,0000,0000,,Có nhìn thấy không hả?
Dialogue: 0,0:11:36.03,0:11:37.18,Default,,0000,0000,0000,,Có.
Dialogue: 0,0:11:38.47,0:11:43.72,Default,,0000,0000,0000,,Bên trong tôi... thằng nhỏ mập mạp này chạm đến mọi ngõ ngách bên trong tôi.
Dialogue: 0,0:11:45.56,0:11:46.57,Default,,0000,0000,0000,,Thế nào?
Dialogue: 0,0:11:47.61,0:11:49.18,Default,,0000,0000,0000,,Sướng cực kì.
Dialogue: 0,0:11:50.24,0:11:53.73,Default,,0000,0000,0000,,Vẫn chưa xong đâu... vẫn chưa xong đâu đấy...
Dialogue: 0,0:12:03.01,0:12:05.59,Default,,0000,0000,0000,,Ai cho phép tự tiện thế hả?
Dialogue: 0,0:12:07.63,0:12:08.50,Default,,0000,0000,0000,,Tớ xin lỗi.
Dialogue: 0,0:12:08.92,0:12:12.26,Default,,0000,0000,0000,,Đồ ngốc! Cậu đang làm cái quái gì vậy hả?
Dialogue: 0,0:12:18.48,0:12:19.47,Default,,0000,0000,0000,,Sướng chứ?
Dialogue: 0,0:12:20.23,0:12:26.27,Default,,0000,0000,0000,,Tôi... chả thấy sướng... gì cả!
Dialogue: 0,0:12:28.64,0:12:30.77,Default,,0000,0000,0000,,Khích quá. Tớ ra đây.
Dialogue: 0,0:12:30.77,0:12:34.07,Default,,0000,0000,0000,,Ra cái đầu cậu chứ ra.
Dialogue: 0,0:12:34.07,0:12:37.74,Default,,0000,0000,0000,,Tôi phải ra trước cậu!
Dialogue: 0,0:12:50.27,0:12:51.67,Default,,0000,0000,0000,,Chúng ta đã ra cùng nhau.
Dialogue: 0,0:12:52.89,0:12:56.99,Default,,0000,0000,0000,,Tôi... đã bảo là... không hề mà...
Dialogue: 0,0:12:56.99,0:13:01.91,Default,,0000,0000,0000,,Tôi... không có ra...
Dialogue: 0,0:13:05.81,0:13:08.81,Default,,0000,0000,0000,,Sao mấy thứ này lại ở đây?
Dialogue: 0,0:13:10.42,0:13:12.74,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:13:15.39,0:13:18.82,Default,,0000,0000,0000,,Đó là hậu môn của tôi!
Dialogue: 0,0:13:21.37,0:13:25.37,Default,,0000,0000,0000,,Đó là giấc mơ của cậu à?
Dialogue: 0,0:13:25.37,0:13:28.57,Default,,0000,0000,0000,,Sao tôi lại bị con bạch tuộc ấy chơi lỗ hậu cơ chứ?
Dialogue: 0,0:13:28.57,0:13:31.29,Default,,0000,0000,0000,,Su-Sukumi, cậu có hứng thú với lỗ hậu không?
Dialogue: 0,0:13:31.29,0:13:33.79,Default,,0000,0000,0000,,Không hề!
Dialogue: 0,0:13:33.79,0:13:35.17,Default,,0000,0000,0000,,Cái gì đây?
Dialogue: 0,0:13:38.33,0:13:40.18,Default,,0000,0000,0000,,Cư xử cho đàng hoàng đấy.
Dialogue: 0,0:13:40.58,0:13:43.18,Default,,0000,0000,0000,,Hể? Đâu mất tiêu rồi?
Dialogue: 0,0:13:43.59,0:13:47.41,Default,,0000,0000,0000,,Cái tai...
Dialogue: 0,0:13:49.21,0:13:50.43,Default,,0000,0000,0000,,Đây rồi.
Dialogue: 0,0:13:50.96,0:13:51.65,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:13:51.65,0:13:54.27,Default,,0000,0000,0000,,Ai cho cậu chạm vào đó hả?
Dialogue: 0,0:13:56.89,0:13:59.57,Default,,0000,0000,0000,,Nè... Có thôi ngay không thì bảo?
Dialogue: 0,0:14:13.93,0:14:16.46,Default,,0000,0000,0000,,Sukumi. Mang tai mèo vào đi.
Dialogue: 0,0:14:17.00,0:14:18.05,Default,,0000,0000,0000,,Tại sao?
Dialogue: 0,0:14:18.67,0:14:20.12,Default,,0000,0000,0000,,Đừng hòng...
Dialogue: 0,0:14:21.33,0:14:24.93,Default,,0000,0000,0000,,Bởi vì cậu sẽ trở thành em mèo con của tớ.
Dialogue: 0,0:14:24.93,0:14:27.89,Default,,0000,0000,0000,,Đồ ngốc! Đừng hòng làm thế...
Dialogue: 0,0:14:31.23,0:14:33.37,Default,,0000,0000,0000,,Thôi nào, đôi tai...
Dialogue: 0,0:14:33.37,0:14:38.16,Default,,0000,0000,0000,,Tôi biết rồi. Nên đừng thô bạo như vậy!
Dialogue: 0,0:14:39.28,0:14:41.12,Default,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:14:41.12,0:14:42.13,Default,,0000,0000,0000,,Sukumi.
Dialogue: 0,0:14:42.13,0:14:46.43,Default,,0000,0000,0000,,Nếu là mèo thì cậu phải cư xử giống như mèo chứ.
Dialogue: 0,0:14:46.43,0:14:49.29,Default,,0000,0000,0000,,Không đời nào, nyan~!
Dialogue: 0,0:14:49.29,0:14:53.58,Default,,0000,0000,0000,,Thằng nhỏ của cậu chủ sướng quá, nyan~!
Dialogue: 0,0:14:56.02,0:14:59.59,Default,,0000,0000,0000,,Âm hộ của em... Âm hộ của em... nyan~!
Dialogue: 0,0:14:59.59,0:15:01.42,Default,,0000,0000,0000,,Âm hộ em sướng lắm phải không?
Dialogue: 0,0:15:02.60,0:15:05.09,Default,,0000,0000,0000,,Hổng có sướng gì cả, nyan~!
Dialogue: 0,0:15:05.09,0:15:06.50,Default,,0000,0000,0000,,Thật chứ?
Dialogue: 0,0:15:06.50,0:15:08.17,Default,,0000,0000,0000,,Sướng lắm, nyan~!
Dialogue: 0,0:15:08.17,0:15:14.49,Default,,0000,0000,0000,,Thằng nhỏ của cậu chủ đâm vào bên trong em, còn đuôi thì ở bên trong hậu môn em - Sướng quá đi mất, nyan~!
Dialogue: 0,0:15:14.49,0:15:15.69,Default,,0000,0000,0000,,Ngoan lắm.
Dialogue: 0,0:15:15.69,0:15:17.06,Default,,0000,0000,0000,,Anh sẽ thưởng cho em.
Dialogue: 0,0:15:17.06,0:15:18.52,Default,,0000,0000,0000,,Nyan?
Dialogue: 0,0:15:19.17,0:15:20.73,Default,,0000,0000,0000,,Hãy đâm sâu nữa đi!
Dialogue: 0,0:15:20.73,0:15:23.19,Default,,0000,0000,0000,,Vào hết bên trong em, cậu chủ!
Dialogue: 0,0:15:23.19,0:15:26.54,Default,,0000,0000,0000,,Hãy đâm thẳng nhỏ mập mạp của cậu vào bên trong em!
Dialogue: 0,0:15:26.54,0:15:28.28,Default,,0000,0000,0000,,Sướng quá, nyan~!
Dialogue: 0,0:15:28.28,0:15:32.20,Default,,0000,0000,0000,,Mỗi khi di chuyển, cái đuôi lại xoan tròn, sướng quá, nyan~!
Dialogue: 0,0:15:32.20,0:15:34.17,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:15:34.17,0:15:36.94,Default,,0000,0000,0000,,Thằng nhỏ của cậu đang khuấy động bên trong em, nyan~!
Dialogue: 0,0:15:37.87,0:15:40.16,Default,,0000,0000,0000,,Sướng quá đi mất, nyan~!
Dialogue: 0,0:15:40.16,0:15:41.29,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:15:41.29,0:15:42.51,Default,,0000,0000,0000,,Em ra mất, nyan~!
Dialogue: 0,0:15:42.51,0:15:44.47,Default,,0000,0000,0000,,Em ra đây, nyan~!
Dialogue: 0,0:15:44.95,0:15:48.22,Default,,0000,0000,0000,,Nyan~!
Dialogue: 0,0:15:50.16,0:15:52.41,Default,,0000,0000,0000,,Trông có vẻ cậu sướng lắm phải không?
Dialogue: 0,0:15:53.32,0:15:55.46,Default,,0000,0000,0000,,Hổng dám đâu, nyan~!
Dialogue: 0,0:15:55.46,0:15:57.10,Default,,0000,0000,0000,,Đau.
Dialogue: 0,0:15:57.10,0:15:59.10,Default,,0000,0000,0000,,
