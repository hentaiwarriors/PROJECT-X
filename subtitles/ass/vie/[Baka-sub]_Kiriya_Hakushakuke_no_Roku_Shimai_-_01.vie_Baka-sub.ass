Dialogue: 0,0:00:31.55,0:00:32.84,Kiriya Dialogue,,0000,0000,0000,,Kurenai.
Dialogue: 0,0:00:33.85,0:00:36.73,Kiriya Dialogue,,0000,0000,0000,,Vậy em sẽ lập khế ước với anh chứ?
Dialogue: 0,0:00:37.15,0:00:46.36,Kiriya Title,,0000,0000,0000,,{\fad(1000,0)\fs45\3c&H001030&\move(350,102,350,73,220,9196)}{\i1}Baka-sub trân trọng giới thiệu
Dialogue: 0,0:00:56.35,0:00:57.67,Kiriya Dialogue,,0000,0000,0000,,Đến nơi rồi.
Dialogue: 0,0:00:57.67,0:00:59.95,Kiriya Dialogue,,0000,0000,0000,,Này, Onii-san, bên này!
Dialogue: 0,0:01:01.48,0:01:04.30,Kiriya Dialogue,,0000,0000,0000,,Cậu là Tokitsu Daisuke-kun.
Dialogue: 0,0:01:04.97,0:01:07.22,Kiriya Dialogue,,0000,0000,0000,,Vâng... Tôi đây...
Dialogue: 0,0:01:07.22,0:01:09.23,Kiriya Dialogue,,0000,0000,0000,,Tên tôi là Kiriya Botan
Dialogue: 0,0:01:09.23,0:01:12.82,Kiriya Dialogue,,0000,0000,0000,,Có lẽ tôi sẽ là chị hay em gái của cậu đấy.
Dialogue: 0,0:01:12.82,0:01:13.87,Kiriya Dialogue,,0000,0000,0000,,Hả~?
Dialogue: 0,0:01:13.87,0:01:15.28,Kiriya Dialogue,,0000,0000,0000,,Thôi, đi nào.
Dialogue: 0,0:01:16.03,0:01:19.67,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Vậy thì lá thư này không phải là một trò chơi khăm rồi.{\i0}
Dialogue: 0,0:01:23.71,0:01:26.03,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Một lá thư lạ tôi nhận được ở Tokyo,{\i0}
Dialogue: 0,0:01:26.03,0:01:28.93,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}đã dẫn tôi đến dây.{\i0}
Dialogue: 0,0:01:30.69,0:01:34.15,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Người gửi là một nhà văn khét tiếng,{\i0}
Dialogue: 0,0:01:34.15,0:01:35.41,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Kiriya Fujino.{\i0}
Dialogue: 0,0:01:36.45,0:01:39.45,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Nhưng cô ấy đã chết cách đây 10 năm.{\i0}
Dialogue: 0,0:01:46.58,0:01:47.80,Kiriya Dialogue,,0000,0000,0000,,Đến nơi rồi.
Dialogue: 0,0:02:12.72,0:02:15.70,Kiriya Dialogue,,0000,0000,0000,,Xin lỗi vì bắt cậu chờ lâu.
Dialogue: 0,0:02:15.70,0:02:18.44,Kiriya Dialogue,,0000,0000,0000,,Chào mừng cậu, Tokitsu Daisuke-san.
Dialogue: 0,0:02:18.44,0:02:24.16,Kiriya Dialogue,,0000,0000,0000,,Tôi là vợ của Kiriya Takaomi, tên tôi là Sumi.
Dialogue: 0,0:02:24.16,0:02:25.73,Kiriya Dialogue,,0000,0000,0000,,Rất vui được gặp bà.
Dialogue: 0,0:02:26.75,0:02:28.59,Kiriya Dialogue,,0000,0000,0000,,Cậu đã cao lớn thế nào rồi ư?
Dialogue: 0,0:02:28.59,0:02:29.33,Kiriya Dialogue,,0000,0000,0000,,Ể?
Dialogue: 0,0:02:29.54,0:02:31.72,Kiriya Dialogue,,0000,0000,0000,,Dĩ nhiên là cậu không nhớ gì rồi.
Dialogue: 0,0:02:31.72,0:02:33.13,Kiriya Dialogue,,0000,0000,0000,,Không thể trách cậu được.
Dialogue: 0,0:02:33.13,0:02:36.92,Kiriya Dialogue,,0000,0000,0000,,Đã có rất nhiều chuyện xảy ra khi cậu còn nhỏ.
Dialogue: 0,0:02:36.92,0:02:37.79,Kiriya Dialogue,,0000,0000,0000,,Ơ...
Dialogue: 0,0:02:37.79,0:02:41.10,Kiriya Dialogue,,0000,0000,0000,,Người bên này là trụ cột của gia đình, Kiriya Takaomi.
Dialogue: 0,0:02:41.10,0:02:43.01,Kiriya Dialogue,,0000,0000,0000,,Xin lỗi về bộ dạng của ông ấy,
Dialogue: 0,0:02:43.01,0:02:46.71,Kiriya Dialogue,,0000,0000,0000,,gần đây chồng tôi đã phải chịu nhiều áp lực nên không được khỏe cho lắm.
Dialogue: 0,0:02:47.67,0:02:48.89,Kiriya Dialogue,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:02:48.89,0:02:52.12,Kiriya Dialogue,,0000,0000,0000,,Hân hạnh được gặp ngài, tôi là Tokitsu Daisuke.
Dialogue: 0,0:02:52.12,0:02:57.11,Kiriya Dialogue,,0000,0000,0000,,Phía sau Takaomi là người giúp việc ở đây, Sagane Aya.
Dialogue: 0,0:02:57.11,0:02:58.06,Kiriya Dialogue,,0000,0000,0000,,Xin chào.
Dialogue: 0,0:03:00.51,0:03:02.73,Kiriya Dialogue,,0000,0000,0000,,Để tôi giới thiệu các cô con gái của mình.
Dialogue: 0,0:03:03.99,0:03:05.87,Kiriya Dialogue,,0000,0000,0000,,Đứa con cả, Nadeshiko.
Dialogue: 0,0:03:06.79,0:03:09.25,Kiriya Dialogue,,0000,0000,0000,,Rất vui được gặp cậu, Daisuke-sama.
Dialogue: 0,0:03:11.34,0:03:13.60,Kiriya Dialogue,,0000,0000,0000,,Người đứng bên cạnh nó là-
Dialogue: 0,0:03:13.60,0:03:16.42,Kiriya Dialogue,,0000,0000,0000,,Ồ, tôi không cần phải nói nữa, đúng không?
Dialogue: 0,0:03:16.42,0:03:19.09,Kiriya Dialogue,,0000,0000,0000,,Gì vậy!? Giới thiệu con đàng hoàng đi!
Dialogue: 0,0:03:19.84,0:03:22.55,Kiriya Dialogue,,0000,0000,0000,,Cạnh Botan là đứa thứ ba, Ai.
Dialogue: 0,0:03:26.07,0:03:29.07,Kiriya Dialogue,,0000,0000,0000,,Cạnh Ai là đứa thứ năm, Kurenai.
Dialogue: 0,0:03:32.49,0:03:35.21,Kiriya Dialogue,,0000,0000,0000,,Người cuối cùng là đứa út, Sumire.
Dialogue: 0,0:03:36.59,0:03:39.22,Kiriya Dialogue,,0000,0000,0000,,Rất vui được biết anh, Daisuke-sama.
Dialogue: 0,0:03:41.63,0:03:43.97,Kiriya Dialogue,,0000,0000,0000,,Có chuyện này tôi không hiểu cho lắm.
Dialogue: 0,0:03:44.65,0:03:48.96,Kiriya Dialogue,,0000,0000,0000,,Lá thư đó bảo tôi phải là một việc gì đó ở đây...
Dialogue: 0,0:03:49.71,0:03:52.05,Kiriya Dialogue,,0000,0000,0000,,Không hẳn là vậy, Daisuke-san.
Dialogue: 0,0:03:53.52,0:03:57.49,Kiriya Dialogue,,0000,0000,0000,,Việc cậu phải làm ở đây chỉ là một cái cớ thôi.
Dialogue: 0,0:03:57.49,0:04:00.29,Kiriya Dialogue,,0000,0000,0000,,Tôi cần cậu chăm sóc cho các con của tôi.
Dialogue: 0,0:04:01.08,0:04:05.95,Kiriya Dialogue,,0000,0000,0000,,Tôi muốn cậu có một đứa con với một trong các đứa con gái của tôi.
Dialogue: 0,0:04:05.95,0:04:08.66,Kiriya Dialogue,,0000,0000,0000,,Hả... Sao? Chuyện này-
Dialogue: 0,0:04:08.66,0:04:13.84,Kiriya Dialogue,,0000,0000,0000,,Daisuke-san, cậu đã được chọn để ở bên một trong nhũng dứa con gái của dòng họ Kiriya.
Dialogue: 0,0:04:13.84,0:04:18.66,Kiriya Dialogue,,0000,0000,0000,,Rồi cậu sẽ là người nối dỗi dòng họ Kiriya, và có mọi thứ ở đây.
Dialogue: 0,0:04:38.11,0:04:39.48,Kiriya Dialogue,,0000,0000,0000,,Daisuke!
Dialogue: 0,0:04:39.48,0:04:43.60,Kiriya Dialogue,,0000,0000,0000,,Sao không để Botan-neesan dẫn cậu đi tham quan dinh thự?
Dialogue: 0,0:04:43.60,0:04:45.92,Kiriya Dialogue,,0000,0000,0000,,Đợi đã, để em làm cho!
Dialogue: 0,0:04:45.92,0:04:48.00,Kiriya Dialogue,,0000,0000,0000,,Da... Daisuke-san.
Dialogue: 0,0:04:48.00,0:04:49.76,Kiriya Dialogue,,0000,0000,0000,,Cho tôi xin đi...
Dialogue: 0,0:04:53.11,0:04:54.62,Kiriya Dialogue,,0000,0000,0000,,Hân hạnh được gặp cậu.
Dialogue: 0,0:04:55.20,0:04:56.24,Kiriya Dialogue,,0000,0000,0000,,Chị là...?
Dialogue: 0,0:04:57.23,0:05:00.20,Kiriya Dialogue,,0000,0000,0000,,Tôi là Rudomiller Michaelovener Graschaft.
Dialogue: 0,0:05:01.29,0:05:03.74,Kiriya Dialogue,,0000,0000,0000,,Cậu có thể gọi tôi là Ruda, Daisuke.
Dialogue: 0,0:05:04.94,0:05:07.80,Kiriya Dialogue,,0000,0000,0000,,Vâng... Sao chị lại biết tên tôi vậy?
Dialogue: 0,0:05:09.35,0:05:12.40,Kiriya Dialogue,,0000,0000,0000,,Những chuyện này chỉ nên nói khi chúng ta đang ở riêng với nhau thôi.
Dialogue: 0,0:05:14.68,0:05:16.54,Kiriya Dialogue,,0000,0000,0000,,Này, cô kia!
Dialogue: 0,0:05:18.43,0:05:22.40,Kiriya Dialogue,,0000,0000,0000,,Ku-re-na-i, anh quên rồi sao?
Dialogue: 0,0:05:23.35,0:05:24.85,Kiriya Dialogue,,0000,0000,0000,,Daisuke-san.
Dialogue: 0,0:05:24.85,0:05:26.13,Kiriya Dialogue,,0000,0000,0000,,Mình sẽ không thua đâu.
Dialogue: 0,0:05:34.29,0:05:35.43,Kiriya Dialogue,,0000,0000,0000,,Chờ đã.
Dialogue: 0,0:05:36.24,0:05:37.71,Kiriya Dialogue,,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:05:37.71,0:05:39.28,Kiriya Dialogue,,0000,0000,0000,,Kurenai.
Dialogue: 0,0:05:39.28,0:05:41.19,Kiriya Dialogue,,0000,0000,0000,,Em sẽ lập khế ước với anh chứ?
Dialogue: 0,0:05:51.94,0:05:53.33,Kiriya Dialogue,,0000,0000,0000,,Cậu ngủ có ngon không?
Dialogue: 0,0:05:53.33,0:05:54.20,Kiriya Dialogue,,0000,0000,0000,,Vâng...
Dialogue: 0,0:05:56.04,0:05:57.26,Kiriya Dialogue,,0000,0000,0000,,Có chuyện gì sao?
Dialogue: 0,0:05:57.26,0:05:58.24,Kiriya Dialogue,,0000,0000,0000,,Tại sao-
Dialogue: 0,0:05:58.24,0:06:00.79,Kiriya Dialogue,,0000,0000,0000,,Cậu là người duy nhất ngủ ở đây.
Dialogue: 0,0:06:00.79,0:06:03.02,Kiriya Dialogue,,0000,0000,0000,,Tôi chỉ muốn chắc rằng cậu được ngon giấc.
Dialogue: 0,0:06:05.03,0:06:07.12,Kiriya Dialogue,,0000,0000,0000,,Cậu đã quen với dinh thự này chưa?
Dialogue: 0,0:06:07.12,0:06:10.62,Kiriya Dialogue,,0000,0000,0000,,Vâng, Kurenai-san đã dẫn tôi đi rất nhiều nơi.
Dialogue: 0,0:06:10.62,0:06:12.95,Kiriya Dialogue,,0000,0000,0000,,Ngày mai, em ấy sẽ chỉ cho tôi khu vườn.
Dialogue: 0,0:06:13.71,0:06:14.50,Kiriya Dialogue,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:06:15.91,0:06:17.32,Kiriya Dialogue,,0000,0000,0000,,Có gì không ổn sao?
Dialogue: 0,0:06:18.17,0:06:20.86,Kiriya Dialogue,,0000,0000,0000,,Thực tình, tôi không rõ chuyện gì đang diễn ra nữa.
Dialogue: 0,0:06:22.31,0:06:27.86,Kiriya Dialogue,,0000,0000,0000,,Lần cuối cùng tôi đến đây là 10 năm trước, trong đám tang của em gái chị, Kiriya Fujino-san.
Dialogue: 0,0:06:27.86,0:06:30.55,Kiriya Dialogue,,0000,0000,0000,,Đó là lần đầu tiên tôi nhận được thư mời đến đây.
Dialogue: 0,0:06:31.36,0:06:36.79,Kiriya Dialogue,,0000,0000,0000,,Vậy à, sao cậu không xem cho kỹ hơn lá thư lần đó?
Dialogue: 0,0:06:36.79,0:06:39.34,Kiriya Dialogue,,0000,0000,0000,,Và tại sao bây giờ cậu lại ở đây?
Dialogue: 0,0:06:40.51,0:06:43.31,Kiriya Dialogue,,0000,0000,0000,,Có lẽ vì bây giờ tôi là một phóng viên chăng?
Dialogue: 0,0:06:43.31,0:06:46.61,Kiriya Dialogue,,0000,0000,0000,,Tôi nghĩ ở đây sẽ có thứ gì đó thú vị cho bài viết của mình.
Dialogue: 0,0:06:46.61,0:06:52.12,Kiriya Dialogue,,0000,0000,0000,,Xin thứ lỗi, nhưng trong một thời gian tôi đã nghe nhiều tin đồn kì lạ về bá tước Kiriya.
Dialogue: 0,0:06:52.12,0:06:54.85,Kiriya Dialogue,,0000,0000,0000,,A, cậu nghe được những tin đồn gì?
Dialogue: 0,0:06:55.64,0:06:57.75,Kiriya Dialogue,,0000,0000,0000,,Chúng chả tốt lành gì.
Dialogue: 0,0:06:57.75,0:07:02.04,Kiriya Dialogue,,0000,0000,0000,,Nhưng tôi vẫn phải nghe vì tôi cần chúng cho công việc của mình.
Dialogue: 0,0:07:02.04,0:07:03.72,Kiriya Dialogue,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:07:03.72,0:07:09.10,Kiriya Dialogue,,0000,0000,0000,,Tôi cứ nghĩ việc lúc trước chỉ là một trò đùa.
Dialogue: 0,0:07:10.61,0:07:13.62,Kiriya Dialogue,,0000,0000,0000,,Ồ, vậy cậu đang nghi ngờ điều đó ư?
Dialogue: 0,0:07:13.62,0:07:15.50,Kiriya Dialogue,,0000,0000,0000,,Tôi vốn là một phóng viên mà...
Dialogue: 0,0:07:15.50,0:07:21.14,Kiriya Dialogue,,0000,0000,0000,,Vậy để chị xóa bỏ hết mọi nghi ngờ trong đầu cậu.
Dialogue: 0,0:07:21.14,0:07:23.46,Kiriya Dialogue,,0000,0000,0000,,A... Na... Nadeshiko-san.
Dialogue: 0,0:07:23.46,0:07:26.05,Kiriya Dialogue,,0000,0000,0000,,Cậu đã cứng thế này rồi.
Dialogue: 0,0:07:27.36,0:07:31.00,Kiriya Dialogue,,0000,0000,0000,,Không sao đâu, chị sẽ chăm sóc cho cậu.
Dialogue: 0,0:07:36.67,0:07:40.98,Kiriya Dialogue,,0000,0000,0000,,Nadeshiko-san, hôm nay không phải là đêm-
Dialogue: 0,0:07:43.76,0:07:47.10,Kiriya Dialogue,,0000,0000,0000,,Kiểm tra xem cậu có phù hợp hay không
Dialogue: 0,0:07:47.83,0:07:50.45,Kiriya Dialogue,,0000,0000,0000,,là trách nhiệm của người chị cả như chị.
Dialogue: 0,0:07:51.29,0:07:53.36,Kiriya Dialogue,,0000,0000,0000,,Nhưng, ngày mai mới là đêm "yotogi"...
Dialogue: 0,0:07:51.29,0:07:54.12,note,,0000,0000,0000,,Yotogi: Viếng thăm ai đó cả đêm.
Dialogue: 0,0:07:54.97,0:07:57.11,Kiriya Dialogue,,0000,0000,0000,,Độ cứng, kích cỡ,
Dialogue: 0,0:07:57.11,0:08:00.75,Kiriya Dialogue,,0000,0000,0000,,cả độ dài đều rất tuyệt vời.
Dialogue: 0,0:08:00.75,0:08:03.74,Kiriya Dialogue,,0000,0000,0000,,Chị rất hài lòng, nhưng...
Dialogue: 0,0:08:10.75,0:08:15.01,Kiriya Dialogue,,0000,0000,0000,,Lần này, chị sẽ để Daisuke-kun làm chị thỏa mãn.
Dialogue: 0,0:08:16.69,0:08:19.87,Kiriya Dialogue,,0000,0000,0000,,Không cần phải vội, đôi gò bồng này không chạy đi đâu mà sợ.
Dialogue: 0,0:08:22.08,0:08:26.00,Kiriya Dialogue,,0000,0000,0000,,Tôi đã thích thú với chúng từ lần đầu gặp nhau.
Dialogue: 0,0:08:43.21,0:08:45.34,Kiriya Dialogue,,0000,0000,0000,,Chị hài lòng chứ?
Dialogue: 0,0:08:45.34,0:08:46.74,Kiriya Dialogue,,0000,0000,0000,,Onee-san.
Dialogue: 0,0:08:46.74,0:08:47.84,Kiriya Dialogue,,0000,0000,0000,,Ôi...
Dialogue: 0,0:08:48.54,0:08:50.77,Kiriya Dialogue,,0000,0000,0000,,Daisuke-san, đến đây.
Dialogue: 0,0:08:53.42,0:08:54.14,Kiriya Dialogue,,0000,0000,0000,,Vâng.
Dialogue: 0,0:09:01.44,0:09:03.39,Kiriya Dialogue,,0000,0000,0000,,Chị... còn trinh sao?
Dialogue: 0,0:09:05.80,0:09:08.99,Kiriya Dialogue,,0000,0000,0000,,Mọi cô con gái của dòng họ Kiriya...
Dialogue: 0,0:09:08.99,0:09:12.41,Kiriya Dialogue,,0000,0000,0000,,đều đợi cậu cho nghi thức "yotogi",
Dialogue: 0,0:09:12.41,0:09:15.50,Kiriya Dialogue,,0000,0000,0000,,chúng tôi bảo vệ sự trong trắng của mình cho ngày này.
Dialogue: 0,0:09:15.50,0:09:18.85,Kiriya Dialogue,,0000,0000,0000,,Dù vậy, việc này khó hơn chị tưởng.
Dialogue: 0,0:09:18.85,0:09:21.49,Kiriya Dialogue,,0000,0000,0000,,Chị không thể kìm chế được nữa.
Dialogue: 0,0:09:22.67,0:09:26.37,Kiriya Dialogue,,0000,0000,0000,,Nào, đừng ngại, cậu hãy di chuyển đi.
Dialogue: 0,0:09:26.37,0:09:27.24,Kiriya Dialogue,,0000,0000,0000,,Vậy thì...
Dialogue: 0,0:09:31.40,0:09:34.73,Kiriya Dialogue,,0000,0000,0000,,Thằng nhỏ cứng cáp của Daisuke-san...
Dialogue: 0,0:09:35.65,0:09:37.16,Kiriya Dialogue,,0000,0000,0000,,ở bên trong chị.
Dialogue: 0,0:09:42.47,0:09:45.16,Kiriya Dialogue,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:09:46.99,0:09:49.90,Kiriya Dialogue,,0000,0000,0000,,Nadeshiko-san, ngực chị phun ra sữa kìa.
Dialogue: 0,0:09:51.85,0:09:56.94,Kiriya Dialogue,,0000,0000,0000,,Mỗi khi chị bị kích thích như vầy, dù không mang thai,
Dialogue: 0,0:09:56.94,0:10:01.25,Kiriya Dialogue,,0000,0000,0000,,ngực chị cũng phun ra sữa.
Dialogue: 0,0:10:02.59,0:10:08.29,Kiriya Dialogue,,0000,0000,0000,,Nữa đi... Nữa đi... Chị muốn cậu làm mạnh hơn nữa, Daisuke-san!
Dialogue: 0,0:10:09.54,0:10:10.66,Kiriya Dialogue,,0000,0000,0000,,Vâng.
Dialogue: 0,0:10:13.99,0:10:17.03,Kiriya Dialogue,,0000,0000,0000,,Sướng quá, sữa lại phun ra nữa rồi!
Dialogue: 0,0:10:19.47,0:10:23.38,Kiriya Dialogue,,0000,0000,0000,,Sữa của Nadeshiko-san ngon quá.
Dialogue: 0,0:10:23.38,0:10:26.17,Kiriya Dialogue,,0000,0000,0000,,Cậu đang nếm nó ư?
Dialogue: 0,0:10:27.22,0:10:29.36,Kiriya Dialogue,,0000,0000,0000,,Sữa cứ tuôn ra không ngừng.
Dialogue: 0,0:10:30.37,0:10:32.10,Kiriya Dialogue,,0000,0000,0000,,Đừng mút nó như vậy~!
Dialogue: 0,0:10:32.10,0:10:33.88,Kiriya Dialogue,,0000,0000,0000,,Chị chắc là tôi không được làm vậy ư?
Dialogue: 0,0:10:33.88,0:10:37.34,Kiriya Dialogue,,0000,0000,0000,,Cậu không thể làm thế với ngực chị-
Dialogue: 0,0:10:41.74,0:10:45.61,Kiriya Dialogue,,0000,0000,0000,,Âm hộ của Nadeshiko-san đang trở nên khít hơn,
Dialogue: 0,0:10:45.61,0:10:50.06,Kiriya Dialogue,,0000,0000,0000,,và thằng nhỏ của tôi dễ dàng bị hút vào.
Dialogue: 0,0:10:50.06,0:10:56.34,Kiriya Dialogue,,0000,0000,0000,,Phải, âm hộ chị đang hút thằng nhỏ của Daisuke-san.
Dialogue: 0,0:10:56.34,0:10:59.77,Kiriya Dialogue,,0000,0000,0000,,Nó đang hút thằng nhỏ của cậu-
Dialogue: 0,0:11:01.93,0:11:06.23,Kiriya Dialogue,,0000,0000,0000,,Thằng nhỏ của cậu đang khuấy động âm hộ của chị...
Dialogue: 0,0:11:06.23,0:11:11.06,Kiriya Dialogue,,0000,0000,0000,,Chị chưa từng nghĩ nó lại sướng như vầy~!
Dialogue: 0,0:11:12.13,0:11:15.46,Kiriya Dialogue,,0000,0000,0000,,Chị... Chị sắp ra rồi!
Dialogue: 0,0:11:15.46,0:11:20.15,Kiriya Dialogue,,0000,0000,0000,,Dù đây là lần đầu tiên nhưng... âm hộ... âm hộ chị...
Dialogue: 0,0:11:20.15,0:11:24.34,Kiriya Dialogue,,0000,0000,0000,,Chị cảm thấy... cảm thấy như muốn nổ tung vậy~!
Dialogue: 0,0:11:24.81,0:11:29.89,Kiriya Dialogue,,0000,0000,0000,,Được rồi, Nadeshiko-san, tôi... tôi cũng đến giới hạn rồi-
Dialogue: 0,0:11:29.89,0:11:31.02,Kiriya Dialogue,,0000,0000,0000,,Cùng ra nào!
Dialogue: 0,0:11:31.02,0:11:34.23,Kiriya Dialogue,,0000,0000,0000,,Nếu vậy, hãy ra cùng chị.
Dialogue: 0,0:11:34.23,0:11:36.04,Kiriya Dialogue,,0000,0000,0000,,Âm hộ chị cũng sẽ ra.
Dialogue: 0,0:11:36.04,0:11:39.55,Kiriya Dialogue,,0000,0000,0000,,Chị sẽ ra với âm hộ~!!
Dialogue: 0,0:11:40.89,0:11:42.96,Kiriya Dialogue,,0000,0000,0000,,Tôi... ra đây!
Dialogue: 0,0:11:48.52,0:11:51.54,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Âm hộ chị nóng quá...{\i0}
Dialogue: 0,0:11:51.54,0:11:55.12,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Có gì đó nóng hổi đang ra bên trong!{\i0}
Dialogue: 0,0:11:55.12,0:11:59.45,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Nóng quá... Nóng quá đi mất~!!!{\i0}
Dialogue: 0,0:12:10.89,0:12:11.95,Kiriya Dialogue,,0000,0000,0000,,Đây là...
Dialogue: 0,0:12:11.95,0:12:14.62,Kiriya Dialogue,,0000,0000,0000,,Nghĩa vụ của con gái của dòng họ Kiriya sao?
Dialogue: 0,0:12:16.32,0:12:18.63,Kiriya Dialogue,,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:12:25.74,0:12:27.28,Kiriya Dialogue,,0000,0000,0000,,Anh sẽ không sao đâu.
Dialogue: 0,0:12:27.28,0:12:28.80,Kiriya Dialogue,,0000,0000,0000,,Vì vậy, đừng ngại.
Dialogue: 0,0:12:29.42,0:12:31.59,Kiriya Dialogue,,0000,0000,0000,,Không, em không muốn vậy!
Dialogue: 0,0:12:31.59,0:12:35.19,Kiriya Dialogue,,0000,0000,0000,,Nếu em làm vậy Daisuke... Daisuke sẽ không như trước đây nữa.
Dialogue: 0,0:12:35.19,0:12:37.55,Kiriya Dialogue,,0000,0000,0000,,Em không muốn chuyện đó xảy ra.
Dialogue: 0,0:12:39.81,0:12:42.59,Kiriya Dialogue,,0000,0000,0000,,Đây quả là một khu vườn rộng lớn nhỉ~?
Dialogue: 0,0:12:42.59,0:12:48.99,Kiriya Dialogue,,0000,0000,0000,,Từ lâu, dòng họ chúng em bị dị ứng với ánh sáng mặt trời.
Dialogue: 0,0:12:48.99,0:12:50.96,Kiriya Dialogue,,0000,0000,0000,,À, anh hiểu rồi.
Dialogue: 0,0:12:50.96,0:12:54.25,Kiriya Dialogue,,0000,0000,0000,,Thảo nào luôn có rèm che ở mọi nơi dù đang là buổi trưa.
Dialogue: 0,0:12:54.25,0:12:55.62,Kiriya Dialogue,,0000,0000,0000,,Là thế đấy.
Dialogue: 0,0:12:55.62,0:12:58.65,Kiriya Dialogue,,0000,0000,0000,,Dù vậy, em thích kiểu ánh sáng thế này hơn.
Dialogue: 0,0:13:04.41,0:13:07.93,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Anh sướng không? Em cảm thấy sướng quá.{\i0}
Dialogue: 0,0:13:07.93,0:13:09.34,Kiriya Dialogue,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:13:10.12,0:13:13.53,Kiriya Dialogue,,0000,0000,0000,,Nhà kho này, bây giờ dùng làm gì vậy?
Dialogue: 0,0:13:13.53,0:13:16.77,Kiriya Dialogue,,0000,0000,0000,,Nó được dùng để chứa đồ của Fujino-neesama.
Dialogue: 0,0:13:16.77,0:13:17.93,Kiriya Dialogue,,0000,0000,0000,,Vậy à?
Dialogue: 0,0:13:18.76,0:13:22.75,Kiriya Dialogue,,0000,0000,0000,,Daisuke, anh quên cái nhà kho này rồi ư?
Dialogue: 0,0:13:22.75,0:13:27.15,Kiriya Dialogue,,0000,0000,0000,,Anh từng chơi với Fujino-neesama và Sumire rất nhiều lần ở đây.
Dialogue: 0,0:13:28.34,0:13:33.21,Kiriya Dialogue,,0000,0000,0000,,Thật ra, anh chỉ nhớ là mình có đến dinh thự này trước đây thôi.
Dialogue: 0,0:13:34.25,0:13:36.41,Kiriya Dialogue,,0000,0000,0000,,Vậy là anh đã quên rồi.
Dialogue: 0,0:13:37.51,0:13:38.40,Kiriya Dialogue,,0000,0000,0000,,Anh xin lỗi.
Dialogue: 0,0:13:38.40,0:13:42.20,Kiriya Dialogue,,0000,0000,0000,,Không sao đâu, từ bây giờ sẽ có rất nhiều cơ hội để tạo dựng lại những kí ức mới.
Dialogue: 0,0:13:43.90,0:13:44.67,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Anh ta to thật!{\i0}
Dialogue: 0,0:13:45.28,0:13:48.11,Kiriya Dialogue,,0000,0000,0000,,Gon, đây là khách của nhà Kiriya.
Dialogue: 0,0:13:49.50,0:13:50.90,Kiriya Dialogue,,0000,0000,0000,,Anh hiểu rồi chứ, để chúng tôi ở riêng với nhau.
Dialogue: 0,0:13:52.58,0:13:53.95,Kiriya Dialogue,,0000,0000,0000,,Ai vậy?
Dialogue: 0,0:13:53.95,0:13:55.80,Kiriya Dialogue,,0000,0000,0000,,Là Gon, người giúp việc của bọn em.
Dialogue: 0,0:13:55.80,0:13:58.78,Kiriya Dialogue,,0000,0000,0000,,Nhìn có vẻ đáng sợ, nhưng anh ta không phải người xấu.
Dialogue: 0,0:13:59.91,0:14:01.45,Kiriya Dialogue,,0000,0000,0000,,Này, Daisuke.
Dialogue: 0,0:14:02.15,0:14:05.17,Kiriya Dialogue,,0000,0000,0000,,Đêm "yotogi" bắt đầu từ tối nay phải không?
Dialogue: 0,0:14:05.90,0:14:09.02,Kiriya Dialogue,,0000,0000,0000,,Anh đã quyết định đến chỗ của ai chưa?
Dialogue: 0,0:14:09.75,0:14:12.60,Kiriya Dialogue,,0000,0000,0000,,Không, anh vẫn không chắc mình có nên đi hay không nữa...
Dialogue: 0,0:14:12.60,0:14:13.69,Kiriya Dialogue,,0000,0000,0000,,Đừng nói vậy!
Dialogue: 0,0:14:13.69,0:14:16.60,Kiriya Dialogue,,0000,0000,0000,,Nó rất quan trọng với bọn em đấy!
Dialogue: 0,0:14:16.60,0:14:18.56,Kiriya Dialogue,,0000,0000,0000,,Anh phải nghiêm túc chứ?
Dialogue: 0,0:14:18.56,0:14:19.60,Kiriya Dialogue,,0000,0000,0000,,Anh xin lỗi.
Dialogue: 0,0:14:19.60,0:14:20.99,Kiriya Dialogue,,0000,0000,0000,,Anh thật thiếu tôn trọng!
Dialogue: 0,0:14:20.99,0:14:22.21,Kiriya Dialogue,,0000,0000,0000,,Nghe này.
Dialogue: 0,0:14:22.21,0:14:25.75,Kiriya Dialogue,,0000,0000,0000,,Lần đầu tiên luôn đem lại cho anh nhiều cảm xúc nhất, vì vậy...
Dialogue: 0,0:14:25.82,0:14:30.05,Kiriya Dialogue,,0000,0000,0000,,tối nay, anh nên đến với người mà anh có quan hệ gần gũi nhất,
Dialogue: 0,0:14:30.05,0:14:32.12,Kiriya Dialogue,,0000,0000,0000,,Em nghĩ vậy đó.
Dialogue: 0,0:14:32.12,0:14:35.24,Kiriya Dialogue,,0000,0000,0000,,Nghĩa là... Anh nên đến chỗ em à?
Dialogue: 0,0:14:36.35,0:14:38.75,Kiriya Dialogue,,0000,0000,0000,,Im đi, đêm "yotogi" là...
Dialogue: 0,0:14:38.75,0:14:42.58,Kiriya Dialogue,,0000,0000,0000,,nghi thức rất quan trọng với bọn em, đừng đem nó ra đùa giỡn!
Dialogue: 0,0:14:42.58,0:14:44.09,Kiriya Dialogue,,0000,0000,0000,,Anh biết rồi.
Dialogue: 0,0:14:44.09,0:14:46.57,Kiriya Dialogue,,0000,0000,0000,,Nhất định... Nhất định là vậy nhé?
Dialogue: 0,0:14:46.57,0:14:47.43,Kiriya Dialogue,,0000,0000,0000,,Ừ.
Dialogue: 0,0:14:56.21,0:15:00.15,Kiriya Dialogue,,0000,0000,0000,,A, Daisuke. Anh định cầu nguyện à?
Dialogue: 0,0:15:00.15,0:15:03.24,Kiriya Dialogue,,0000,0000,0000,,Không hẳn, tôi chỉ đi dạo quanh đây thôi.
Dialogue: 0,0:15:03.24,0:15:05.07,Kiriya Dialogue,,0000,0000,0000,,Xin lỗi đã là phiền chị cầu nguyện.
Dialogue: 0,0:15:05.07,0:15:06.63,Kiriya Dialogue,,0000,0000,0000,,Không sao.
Dialogue: 0,0:15:07.71,0:15:12.31,Kiriya Dialogue,,0000,0000,0000,,Này, Ruda... đêm "yotogi" là thứ rất quan trọng với gia đình này ư?
Dialogue: 0,0:15:12.31,0:15:14.85,Kiriya Dialogue,,0000,0000,0000,,Và... tại sao lại là tôi?
Dialogue: 0,0:15:17.81,0:15:21.58,Kiriya Dialogue,,0000,0000,0000,,Từ lâu tình dục đã là một nghi thức rất quan trọng.
Dialogue: 0,0:15:21.58,0:15:22.50,Kiriya Dialogue,,0000,0000,0000,,Và...
Dialogue: 0,0:15:23.39,0:15:26.15,Kiriya Dialogue,,0000,0000,0000,,Nếu cậu muốn biết vì sao mình được chọn,
Dialogue: 0,0:15:26.15,0:15:29.90,Kiriya Dialogue,,0000,0000,0000,,tối nay và ngày mai, cậu hãy đến đây lúc trời tối.
Dialogue: 0,0:15:31.46,0:15:32.21,Kiriya Dialogue,,0000,0000,0000,,Ru-
Dialogue: 0,0:15:37.76,0:15:39.92,Kiriya Dialogue,,0000,0000,0000,,Xin lỗi, Sumi-san.
Dialogue: 0,0:15:39.92,0:15:41.12,Kiriya Dialogue,,0000,0000,0000,,Tôi có vài chuyện cần bàn...
Dialogue: 0,0:15:41.12,0:15:43.14,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Đêm ''yotogi" sẽ diễn ra trong 4 ngày.{\i0}
Dialogue: 0,0:15:43.14,0:15:46.43,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Không biết ai sẽ là người được Daisuke-san lựa chọn?{\i0}
Dialogue: 0,0:15:46.43,0:15:47.33,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Vâng.{\i0}
Dialogue: 0,0:15:47.33,0:15:51.75,Kiriya Dialogue,,0000,0000,0000,,Và đây không phải là lúc thích hợp để Botan đến đây.
Dialogue: 0,0:15:51.75,0:15:55.04,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Không có gì phải lo lắng đâu ạ.{\i0}
Dialogue: 0,0:15:55.04,0:15:59.05,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Con chắc Botan đã biết tầm quan trọng của nghi thức "yotogi" mà.{\i0}
Dialogue: 0,0:15:59.05,0:16:00.91,Kiriya Dialogue,,0000,0000,0000,,Thế thì không sao.
Dialogue: 0,0:16:09.12,0:16:11.55,Kiriya Dialogue,,0000,0000,0000,,Vâng, mời vào.
Dialogue: 0,0:16:13.31,0:16:14.48,Kiriya Dialogue,,0000,0000,0000,,Chào anh.
Dialogue: 0,0:16:14.48,0:16:16.89,Kiriya Dialogue,,0000,0000,0000,,Anh thật sự đã đến.
Dialogue: 0,0:16:19.63,0:16:20.69,Kiriya Dialogue,,0000,0000,0000,,Kurenai.
Dialogue: 0,0:16:23.30,0:16:25.38,Kiriya Dialogue,,0000,0000,0000,,Ơ... Đợi đã.
Dialogue: 0,0:16:26.09,0:16:31.02,Kiriya Dialogue,,0000,0000,0000,,Tối nay... em muốn làm anh được sướng trước.
Dialogue: 0,0:16:31.82,0:16:33.60,Kiriya Dialogue,,0000,0000,0000,,Làm ơn, được không?
Dialogue: 0,0:16:35.01,0:16:35.69,Kiriya Dialogue,,0000,0000,0000,,Ừ, được...
Dialogue: 0,0:16:37.12,0:16:39.39,Kiriya Dialogue,,0000,0000,0000,,Em biết không... Kurenai.
Dialogue: 0,0:16:39.39,0:16:41.79,Kiriya Dialogue,,0000,0000,0000,,Hừ, gì vậy?
Dialogue: 0,0:16:41.79,0:16:45.18,Kiriya Dialogue,,0000,0000,0000,,Em nghĩ nó làm anh sướng chỗ nào hả?
Dialogue: 0,0:16:45.18,0:16:46.83,Kiriya Dialogue,,0000,0000,0000,,Em bảo đảm anh sẽ rất là sướng.
Dialogue: 0,0:16:46.83,0:16:48.11,Kiriya Dialogue,,0000,0000,0000,,Nhìn này.
Dialogue: 0,0:16:48.11,0:16:50.35,Kiriya Dialogue,,0000,0000,0000,,Đúng như em nghĩ.
Dialogue: 0,0:16:50.44,0:16:55.06,Kiriya Dialogue,,0000,0000,0000,,Con trai sẽ rất sướng nếu có cô nào giẫm lên thằng nhỏ của họ thế này.
Dialogue: 0,0:16:55.95,0:16:57.53,Kiriya Dialogue,,0000,0000,0000,,Em nghe ở đâu ra vậy?
Dialogue: 0,0:16:57.53,0:17:00.58,Kiriya Dialogue,,0000,0000,0000,,Nè... nè... nè... lạ quá đi?
Dialogue: 0,0:17:01.71,0:17:03.61,Kiriya Dialogue,,0000,0000,0000,,Nó cong lại dễ thương quá...
Dialogue: 0,0:17:03.61,0:17:06.07,Kiriya Dialogue,,0000,0000,0000,,Và rất là cứng nữa.
Dialogue: 0,0:17:06.07,0:17:10.95,Kiriya Dialogue,,0000,0000,0000,,Em không thể tin là thằng nhỏ sẽ bị kích thích khi được giẫm lên.
Dialogue: 0,0:17:11.91,0:17:15.36,Kiriya Dialogue,,0000,0000,0000,,Này... này... Anh không cảm thấy sướng sao?
Dialogue: 0,0:17:15.36,0:17:18.27,Kiriya Dialogue,,0000,0000,0000,,Có chất dịch trào ra từ đầu của nó kìa. {original had "tip of yours" but that sounded weird}
Dialogue: 0,0:17:18.27,0:17:20.89,Kiriya Dialogue,,0000,0000,0000,,Vừa nóng lại vừa dính nữa...
Dialogue: 0,0:17:20.89,0:17:23.03,Kiriya Dialogue,,0000,0000,0000,,Em biết là anh thích nó mà.
Dialogue: 0,0:17:23.03,0:17:26.49,Kiriya Dialogue,,0000,0000,0000,,Khi đỏ mặt anh trông thật đáng yêu.
Dialogue: 0,0:17:26.49,0:17:28.22,Kiriya Dialogue,,0000,0000,0000,,Nhưng em sẽ không để anh ra đâu.
Dialogue: 0,0:17:28.22,0:17:29.33,Kiriya Dialogue,,0000,0000,0000,,Đây này!
Dialogue: 0,0:17:30.22,0:17:31.90,Kiriya Dialogue,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:17:31.90,0:17:36.30,Kiriya Dialogue,,0000,0000,0000,,Muốn em dừng lại sao? Nhưng thằng nhỏ của anh đã cứng thế này.
Dialogue: 0,0:17:36.30,0:17:39.36,Kiriya Dialogue,,0000,0000,0000,,Nếu anh thật sự không muốn, thì anh đã tự dừng lại rồi.
Dialogue: 0,0:17:41.33,0:17:44.26,Kiriya Dialogue,,0000,0000,0000,,Thật tuyệt! Nó thật dính và đang co giật.
Dialogue: 0,0:17:44.26,0:17:45.89,Kiriya Dialogue,,0000,0000,0000,,Thằng nhỏ của anh dễ thương quá.
Dialogue: 0,0:17:45.89,0:17:49.25,Kiriya Dialogue,,0000,0000,0000,,Thấy không? Thằng nhỏ của anh đã sướng vậy rồi.
Dialogue: 0,0:17:49.25,0:17:51.73,Kiriya Dialogue,,0000,0000,0000,,Em nhớ chuyện này đấy, Kurenai.
Dialogue: 0,0:17:51.73,0:17:54.64,Kiriya Dialogue,,0000,0000,0000,,Daisuke cũng nên nhớ chuyện này đi.
Dialogue: 0,0:17:54.64,0:17:58.49,Kiriya Dialogue,,0000,0000,0000,,Rằng thằng nhỏ của anh đã ra trong khi bị em giẫm lên.
Dialogue: 0,0:17:59.37,0:18:01.78,Kiriya Dialogue,,0000,0000,0000,,Anh nên nhìn cho kĩ vào.
Dialogue: 0,0:18:01.78,0:18:05.34,Kiriya Dialogue,,0000,0000,0000,,Thằng nhỏ hư hỏng của anh sẽ ra khi bị em giẫm lên thế này.
Dialogue: 0,0:18:06.19,0:18:10.84,Kiriya Dialogue,,0000,0000,0000,,Tuyệt quá, nó ra thật nhiều chất dịch nhầy nhụa.
Dialogue: 0,0:18:11.47,0:18:13.87,Kiriya Dialogue,,0000,0000,0000,,Nó làm em thật kích thích.
Dialogue: 0,0:18:13.87,0:18:16.90,Kiriya Dialogue,,0000,0000,0000,,Sắp ra rồi ư? Tinh dịch của anh sắp ra rồi ư!?
Dialogue: 0,0:18:16.90,0:18:20.72,Kiriya Dialogue,,0000,0000,0000,,Vậy... Em sẽ tiếp tục cho đến khi anh đạt cực khoái nhé?
Dialogue: 0,0:18:25.86,0:18:27.13,Kiriya Dialogue,,0000,0000,0000,,Ra rồi.
Dialogue: 0,0:18:27.13,0:18:29.75,Kiriya Dialogue,,0000,0000,0000,,Thật... tuyệt~
Dialogue: 0,0:18:33.07,0:18:36.94,Kiriya Dialogue,,0000,0000,0000,,Nhiều quá và còn ấm nữa.
Dialogue: 0,0:18:36.94,0:18:40.07,Kiriya Dialogue,,0000,0000,0000,,Thứ này sẽ khiến em có thai ư?
Dialogue: 0,0:18:40.07,0:18:42.23,Kiriya Dialogue,,0000,0000,0000,,Lần này đến lược anh nhỉ?
Dialogue: 0,0:18:45.78,0:18:48.77,Kiriya Dialogue,,0000,0000,0000,,Daisuke, em nghĩ tối nay chúng ta chỉ làm đến đây thôi-
Dialogue: 0,0:18:50.45,0:18:53.50,Kiriya Dialogue,,0000,0000,0000,,Anh không quan tâm đêm "yotogi" là gì, và cũng không thích bị ép buộc như thế này.
Dialogue: 0,0:18:53.50,0:18:56.29,Kiriya Dialogue,,0000,0000,0000,,Anh muốn em dừng những việc như thế này, được chứ?
Dialogue: 0,0:18:57.72,0:18:59.94,Kiriya Dialogue,,0000,0000,0000,,V-Vâng, em xin lỗi.
Dialogue: 0,0:19:04.39,0:19:05.21,Kiriya Dialogue,,0000,0000,0000,,Nè...
Dialogue: 0,0:19:05.93,0:19:07.73,Kiriya Dialogue,,0000,0000,0000,,Mai anh có đến không?
Dialogue: 0,0:19:07.73,0:19:08.59,Kiriya Dialogue,,0000,0000,0000,,Có.
Dialogue: 0,0:19:08.59,0:19:09.82,Kiriya Dialogue,,0000,0000,0000,,Thật không?
Dialogue: 0,0:19:09.82,0:19:11.45,Kiriya Dialogue,,0000,0000,0000,,Anh hứa.
Dialogue: 0,0:19:15.92,0:19:18.42,Kiriya Dialogue,,0000,0000,0000,,Hừ... Kurenai thật là.
Dialogue: 0,0:19:20.92,0:19:22.39,Kiriya Dialogue,,0000,0000,0000,,A... Ai!
Dialogue: 0,0:19:23.02,0:19:27.54,Kiriya Dialogue,,0000,0000,0000,,Xin lỗi, anh không biết là em đang ở trong này.
Dialogue: 0,0:19:27.56,0:19:28.49,Kiriya Dialogue,,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:19:29.58,0:19:31.72,Kiriya Dialogue,,0000,0000,0000,,Em đang chờ anh ở đây.
Dialogue: 0,0:19:33.09,0:19:34.79,Kiriya Dialogue,,0000,0000,0000,,Em sẽ có con với Daisuke.
Dialogue: 0,0:19:35.54,0:19:37.66,Kiriya Dialogue,,0000,0000,0000,,Đó là nghĩ vụ của con gái dòng họ Kiriya.
Dialogue: 0,0:19:40.27,0:19:43.11,Kiriya Dialogue,,0000,0000,0000,,Nó vẫn còn sung lắm, Daisuke.
Dialogue: 0,0:19:43.11,0:19:45.59,Kiriya Dialogue,,0000,0000,0000,,Em muốn anh đút nó vào bên trong em.
Dialogue: 0,0:19:52.70,0:19:54.93,Kiriya Dialogue,,0000,0000,0000,,Em thấy thế nào, Ai?
Dialogue: 0,0:19:54.93,0:19:59.04,Kiriya Dialogue,,0000,0000,0000,,Em không biết, nhưng... em thấy lạ lắm.
Dialogue: 0,0:19:59.04,0:20:01.33,Kiriya Dialogue,,0000,0000,0000,,Còn ở đây?
Dialogue: 0,0:20:06.50,0:20:09.16,Kiriya Dialogue,,0000,0000,0000,,Ai, em đã ướt như vầy rồi.
Dialogue: 0,0:20:09.45,0:20:11.32,Kiriya Dialogue,,0000,0000,0000,,Lần này hãy làm đôi bên cùng sướng.
Dialogue: 0,0:20:12.05,0:20:12.66,Kiriya Dialogue,,0000,0000,0000,,Vâng.
Dialogue: 0,0:20:32.05,0:20:34.26,Kiriya Dialogue,,0000,0000,0000,,Ai, anh đút vào đây.
Dialogue: 0,0:20:35.39,0:20:39.13,Kiriya Dialogue,,0000,0000,0000,,Em muốn thứ cứng cáp của Daisuke đút mạnh vào bên trong em-
Dialogue: 0,0:20:43.87,0:20:45.60,Kiriya Dialogue,,0000,0000,0000,,Ai, em có sao không?
Dialogue: 0,0:20:47.33,0:20:48.46,Kiriya Dialogue,,0000,0000,0000,,Em không sao.
Dialogue: 0,0:20:48.46,0:20:49.79,Kiriya Dialogue,,0000,0000,0000,,Tiếp tục đi anh.
Dialogue: 0,0:20:49.79,0:20:50.58,Kiriya Dialogue,,0000,0000,0000,,Ừm.
Dialogue: 0,0:20:53.77,0:20:55.55,Kiriya Dialogue,,0000,0000,0000,,Nóng quá.
Dialogue: 0,0:20:55.55,0:20:58.73,Kiriya Dialogue,,0000,0000,0000,,Em có thể cảm nhận được bên trong đang rộng ra và nóng lên.
Dialogue: 0,0:20:59.50,0:21:01.83,Kiriya Dialogue,,0000,0000,0000,,Ai, em có sướng không?
Dialogue: 0,0:21:02.56,0:21:04.23,Kiriya Dialogue,,0000,0000,0000,,Em không biết.
Dialogue: 0,0:21:04.23,0:21:06.11,Kiriya Dialogue,,0000,0000,0000,,Em không biết, nhưng-
Dialogue: 0,0:21:07.12,0:21:11.77,Kiriya Dialogue,,0000,0000,0000,,Ai, sẽ sướng hơn nếu em tự nhịp hông đấy.
Dialogue: 0,0:21:12.57,0:21:13.39,Kiriya Dialogue,,0000,0000,0000,,Vâng.
Dialogue: 0,0:21:15.09,0:21:17.61,Kiriya Dialogue,,0000,0000,0000,,Nó đang cọ sát mãnh liệt bên trong em!
Dialogue: 0,0:21:17.61,0:21:19.39,Kiriya Dialogue,,0000,0000,0000,,Sâu bên trong...
Dialogue: 0,0:21:19.39,0:21:22.06,Kiriya Dialogue,,0000,0000,0000,,Nó khiến em rung lên mỗi khi nó chạm vào sâu bên trong.
Dialogue: 0,0:21:23.36,0:21:25.53,Kiriya Dialogue,,0000,0000,0000,,Ai, sướng lắm, đúng không?
Dialogue: 0,0:21:26.49,0:21:27.78,Kiriya Dialogue,,0000,0000,0000,,Em bắt đầu cảm thấy sướng rồi.
Dialogue: 0,0:21:30.91,0:21:36.72,Kiriya Dialogue,,0000,0000,0000,,Thằng nhỏ... cứng cáp... của Daisuke đang đâm vào bên trong em.
Dialogue: 0,0:21:39.05,0:21:41.75,Kiriya Dialogue,,0000,0000,0000,,Không... Không... Em sẽ vỡ mất!
Dialogue: 0,0:21:41.75,0:21:43.88,Kiriya Dialogue,,0000,0000,0000,,Ai, anh...
Dialogue: 0,0:21:43.88,0:21:46.65,Kiriya Dialogue,,0000,0000,0000,,Ra đi, hãy ra bên trong em.
Dialogue: 0,0:21:46.65,0:21:50.31,Kiriya Dialogue,,0000,0000,0000,,Em muốn anh bắn sâu vào trong âm hộ của em~!
Dialogue: 0,0:21:51.27,0:21:53.02,Kiriya Dialogue,,0000,0000,0000,,Ai!
Dialogue: 0,0:21:54.69,0:21:59.51,Kiriya Dialogue,,0000,0000,0000,,Nóng quá... Thứ gì đó nóng hổi đang ra bên trong~!
Dialogue: 0,0:22:12.20,0:22:15.64,Kiriya Dialogue,,0000,0000,0000,,Ai, sao chúng ta không tắm chung với nhau?
Dialogue: 0,0:22:16.46,0:22:18.19,Kiriya Dialogue,,0000,0000,0000,,Em còn có việc phải làm.
Dialogue: 0,0:22:18.64,0:22:19.39,Kiriya Dialogue,,0000,0000,0000,,Hử?
Dialogue: 0,0:22:24.14,0:22:25.22,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Là nghi thức à?{\i0}
Dialogue: 0,0:22:27.24,0:22:29.98,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Nếu cậu muốn biết tại sao mình được chọn,{\i0}
Dialogue: 0,0:22:29.98,0:22:33.76,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}tối nay và ngày mai, cậu hãy đến đây lúc trời tối.{\i0}
Dialogue: 0,0:22:40.71,0:22:42.31,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Daisuke.{\i0}
Dialogue: 0,0:22:45.10,0:22:47.07,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Khuya rồi sao đèn vẫn còn sáng nhỉ?{\i0}
Dialogue: 0,0:22:47.07,0:22:48.81,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Chuyện gì đang diễn ra vậy?{\i0}
Dialogue: 0,0:22:55.89,0:22:58.06,Kiriya Dialogue Alternate,,0000,0000,0000,,{\i1}Chuyện gì... thế này?{\i0}
