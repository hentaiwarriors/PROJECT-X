Dialogue: 0,0:00:01.01,0:00:04.50,Default,,0000,0000,0000,,Haru-kun... Đừng làm chuyện đó ở đây...
Dialogue: 0,0:00:04.50,0:00:05.65,Default,,0000,0000,0000,,Akina...
Dialogue: 0,0:00:05.65,0:00:09.27,Default,,0000,0000,0000,,Nhỡ hai người kia phát hiện ra thì sao...
Dialogue: 0,0:00:09.27,0:00:14.92,Default,,0000,0000,0000,,Haru-kun... Nếu cậu cứ sờ nắn chỗ đó, mình sẽ...
Dialogue: 0,0:00:28.51,0:00:30.05,Default,,0000,0000,0000,,Haru-kun...
Dialogue: 0,0:00:35.44,0:00:36.84,Default,,0000,0000,0000,,Như thế này hả?
Dialogue: 0,0:00:37.11,0:00:39.20,Default,,0000,0000,0000,,Uh, cậu tiếp tục làm vậy đi.
Dialogue: 0,0:00:40.86,0:00:44.36,Default,,0000,0000,0000,,Thằng nhỏ của Haru-kun bự lên ghê chưa kìa... Cậu sắp ra rồi phải không?
Dialogue: 0,0:00:44.36,0:00:48.86,Default,,0000,0000,0000,,Hai bầu vú của Akina cũng bự ghê ha... Tuyệt quá.
Dialogue: 0,0:01:02.75,0:01:04.25,Default,,0000,0000,0000,,Ra rồi! Mình ra rồi!
Dialogue: 0,0:01:06.41,0:01:08.66,Default,,0000,0000,0000,,Haru-kun, cậu thích chứ?
Dialogue: 0,0:01:08.86,0:01:11.70,Default,,0000,0000,0000,,Ah.. Akina, cậu cứ tiếp tục làm vậy đi.
Dialogue: 0,0:01:18.30,0:01:21.30,Default,,0000,0000,0000,,Tinh dịch của Haru-kun làm ngực mình ướt nhẹp cả rồi..
Dialogue: 0,0:01:21.73,0:01:25.32,Default,,0000,0000,0000,,Akina.. Mình vừa mới ra xong mà, nếu cậu cứ liếm nó như vậy, mình sẽ..
Dialogue: 0,0:01:33.13,0:01:33.75,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:01:53.68,0:01:58.09,Default,,0000,0000,0000,,Thằng bệnh này tên là Honnoji Tsukasa, kẻ thù số một của phụ nữ.
Dialogue: 0,0:01:58.12,0:02:00.47,Default,,0000,0000,0000,,Ở lớp học nó chỉ toàn gây phiền phức cho mọi người.
Dialogue: 0,0:02:00.73,0:02:04.91,Default,,0000,0000,0000,,Éo hiểu tại sao, thằng này luôn bám theo tôi như hình với bóng.
Dialogue: 0,0:02:05.21,0:02:08.05,Default,,0000,0000,0000,,Mà này, tình hình đang dần xấu đi đó, Haruomi.
Dialogue: 0,0:02:08.15,0:02:12.39,Default,,0000,0000,0000,,Nhãn hiệu kem One O One cuối cùng đã vươn vòi bạch tuộc sang thị trấn này!
Dialogue: 0,0:02:17.20,0:02:18.86,Default,,0000,0000,0000,,Hừm?
Dialogue: 0,0:02:18.86,0:02:20.86,Default,,0000,0000,0000,,Tao đang nói tới One O One đấy nhé!
Dialogue: 0,0:02:20.86,0:02:23.27,Default,,0000,0000,0000,,One O One? Chẳng phải là-
Dialogue: 0,0:02:24.28,0:02:26.47,Default,,0000,0000,0000,,Chào cậu, Honnoji-kun.
Dialogue: 0,0:02:26.47,0:02:28.76,Default,,0000,0000,0000,,Ah! Akina-Hime!
Dialogue: 0,0:02:31.47,0:02:34.25,Default,,0000,0000,0000,,Ba cậu đi đâu cùng nhau thế kia?
Dialogue: 0,0:02:34.25,0:02:36.57,Default,,0000,0000,0000,,Tụi mình nghe nói hiệu kem One O One sắp khai trương ở ngay gần đây.
Dialogue: 0,0:02:36.60,0:02:39.67,Default,,0000,0000,0000,,Vậy nên khi cần phải đi mua sắm vài thứ, tụi mình tranh thủ ghé qua chỗ đó luôn.
Dialogue: 0,0:02:39.67,0:02:42.09,Default,,0000,0000,0000,,Tụi mình định nán lại ở đây một lúc để xem cậu làm ăn ra sao.
Dialogue: 0,0:02:42.28,0:02:43.33,Default,,0000,0000,0000,,Hừm.
Dialogue: 0,0:02:43.66,0:02:45.68,Default,,0000,0000,0000,,Vậy, One O One là-
Dialogue: 0,0:02:46.11,0:02:47.25,Default,,0000,0000,0000,,Ác chiến lắm đấy!
Dialogue: 0,0:02:47.25,0:02:49.70,Default,,0000,0000,0000,,Nhãn hiệu kem này nổi tiếng khắp Nhật Bản.
Dialogue: 0,0:02:49.83,0:02:52.18,Default,,0000,0000,0000,,Cửa hiệu của chúng ta sẽ bị bọn họ làm cho điêu đứng!
Dialogue: 0,0:02:52.41,0:02:56.60,Default,,0000,0000,0000,,Oh, nếu mình nhớ không lầm, ba mẹ của Honnoji-kun cũng sở hữu một chuỗi các tiệm kem, phải không nhỉ?
Dialogue: 0,0:02:56.66,0:02:57.73,Default,,0000,0000,0000,,Hết rồi...
Dialogue: 0,0:02:57.73,0:02:59.99,Default,,0000,0000,0000,,Nhà tài trợ số một của Tanabata nay đã là quá khứ.
Dialogue: 0,0:02:57.73,0:02:59.99,Default,,0000,0000,0440,,{\bord0\shad0\p1\c&HAEA6FF&\fscx682.5\fscy237.5\pos(482,93)}m 24 -14 l -25 -14 l -25 9 l 24 9
Dialogue: 0,0:02:57.73,0:02:59.99,Default,,0000,0000,0440,,{\fs30\bord1\pos(310,46)}Tanabata là một lễ hội của Nhật Bản
Dialogue: 0,0:03:00.05,0:03:03.35,Default,,0000,0000,0000,,Với tư cách là con trai nhà tài trợ, mình cảm thấy như đã mất đi toàn bộ danh dự và lòng kiêu hãnh.
Dialogue: 0,0:03:03.58,0:03:05.80,Default,,0000,0000,0000,,Mày làm quái gì có danh dự với lòng kiêu hãnh chứ!?
Dialogue: 0,0:03:06.16,0:03:09.69,Default,,0000,0000,0000,,Cậu chỉ biết cười trên nỗi đau khổ của người khác mà thôi.
Dialogue: 0,0:03:09.98,0:03:13.61,Default,,0000,0000,0000,,Vậy, tiệm "One O One" định khai trương ở đâu?
Dialogue: 0,0:03:13.68,0:03:14.59,Default,,0000,0000,0000,,Ngay đằng kia kìa.
Dialogue: 0,0:03:14.69,0:03:15.77,Default,,0000,0000,0000,,Hả?!
Dialogue: 0,0:03:17.07,0:03:18.54,Default,,0000,0000,0000,,Đùa nhau đấy hả...
Dialogue: 0,0:03:19.52,0:03:23.15,Default,,0000,0000,0000,,Thôi nhé, gặp lại cậu sau, Haru-kun. Tụi mình qua đó ngắm nghía một chút đây.
Dialogue: 0,0:03:23.35,0:03:24.78,Default,,0000,0000,0000,,Đi thôi, Suzu.
Dialogue: 0,0:03:34.71,0:03:35.95,Default,,0000,0000,0000,,Mưa à?
Dialogue: 0,0:03:41.34,0:03:44.28,Default,,0000,0000,0000,,{\i1}kì thi sắp đến, lại chuẩn bị cắm đầu vào học;
Dialogue: 0,0:03:44.51,0:03:47.91,Default,,0000,0000,0000,,{\i1}mọi thứ càng lúc càng rối tung lên...
Dialogue: 0,0:04:00.81,0:04:02.51,Default,,0000,0000,0000,,S-Sắp tè ra quần rồi!
Dialogue: 0,0:04:02.64,0:04:05.22,Default,,0000,0000,0000,,Bỏ mẹ! Toa-lét! Toa-lét!
Dialogue: 0,0:04:13.62,0:04:15.45,Default,,0000,0000,0000,,X-Xin lỗi, Himenohara!
Dialogue: 0,0:04:15.61,0:04:17.77,Default,,0000,0000,0000,,Cậu không sao... chứ?!
Dialogue: 0,0:04:18.72,0:04:20.81,Default,,0000,0000,0000,,EEEEEEEEEEH?!
Dialogue: 0,0:04:22.83,0:04:24.76,Default,,0000,0000,0000,,Hi-Himenohara! Khoan-
Dialogue: 0,0:04:27.67,0:04:29.30,Default,,0000,0000,0000,,Này Honnoji!
Dialogue: 0,0:04:29.63,0:04:33.68,Default,,0000,0000,0000,,Cậu dám mang thứ này tới trường sao?
Dialogue: 0,0:04:34.30,0:04:38.32,Default,,0000,0000,0000,,Lớp trưởng cáu lên nhìn dễ thương ghê, hệt như Akina-Hime vậy.
Dialogue: 0,0:04:38.40,0:04:40.32,Default,,0000,0000,0000,,Thèm chết à...!
Dialogue: 0,0:04:41.74,0:04:43.33,Default,,0000,0000,0000,,Hi-Himenohara...
Dialogue: 0,0:04:43.88,0:04:46.03,Default,,0000,0000,0000,,D-Dừng lại!
Dialogue: 0,0:04:46.03,0:04:50.87,Default,,0000,0000,0000,,Cậu... Cậu đeo{\i1} thứ này {\i0} trong suốt giờ học sao?!
Dialogue: 0,0:04:50.87,0:04:53.85,Default,,0000,0000,0000,,Không lẽ nào.. trong cả những ngày khác nữa?
Dialogue: 0,0:04:54.47,0:04:58.08,Default,,0000,0000,0000,,Chuyện này không hay ho chút nào... nhưng dù vậy, mình sẽ không kể nó cho ai biết đâu.
Dialogue: 0,0:04:58.39,0:05:01.56,Default,,0000,0000,0000,,Cậu chỉ nên làm điều này với chàng trai mà cậu thích thôi chứ.
Dialogue: 0,0:05:07.79,0:05:09.50,Default,,0000,0000,0000,,Khác gì nhau đâu.
Dialogue: 0,0:05:10.19,0:05:12.53,Default,,0000,0000,0000,,Không khác... Cái gì?
Dialogue: 0,0:05:14.77,0:05:18.35,Default,,0000,0000,0000,,D-Dừng lại đi, Himenohara! Nếu tiến xa hơn nữa, chúng ta sẽ gặp rắc rối to!
Dialogue: 0,0:05:27.70,0:05:31.47,Default,,0000,0000,0000,,{\i1}Không còn cách nào khác... Mình phải nhanh chấm dứt chuyện này mới được.
Dialogue: 0,0:05:35.07,0:05:38.06,Default,,0000,0000,0000,,H-Himenohara... Cậu la lớn quá đó!
Dialogue: 0,0:05:54.49,0:05:57.83,Default,,0000,0000,0000,,Thôi chết rồi... Tệ quá! Mình sẽ ra bên trong mất-
Dialogue: 0,0:06:07.66,0:06:12.00,Default,,0000,0000,0000,,Haru-kun, chuyện gì vậy? Sao cứ ngẩn mặt ra thế.
Dialogue: 0,0:06:12.00,0:06:14.62,Default,,0000,0000,0000,,Hở? A-Ah, không có gì.
Dialogue: 0,0:06:14.84,0:06:17.18,Default,,0000,0000,0000,,Haru-chan, em mệt à?
Dialogue: 0,0:06:17.18,0:06:18.44,Default,,0000,0000,0000,,K-Không!
Dialogue: 0,0:06:18.85,0:06:23.05,Default,,0000,0000,0000,,Ah, nhân tiện, lần tới em sẽ ghé qua tiệm One O One xem sao.
Dialogue: 0,0:06:23.38,0:06:27.77,Default,,0000,0000,0000,,Ah! Kem ở đó ngon lắm đấy!
Dialogue: 0,0:06:27.79,0:06:30.70,Default,,0000,0000,0000,,K-Khoan đã nào, Onee-chan! Họ là đối thủ của chúng ta mà?
Dialogue: 0,0:06:30.89,0:06:33.11,Default,,0000,0000,0000,,Tiệm đó to lắm đấy.
Dialogue: 0,0:06:33.18,0:06:38.03,Default,,0000,0000,0000,,Vậy hả? Có lẽ chị sẽ suy nghĩ một chút về vấn đề này.
Dialogue: 0,0:06:38.17,0:06:40.43,Default,,0000,0000,0000,,Mafuyu, em cũng phải nghĩ cách dùm mọi người đi chứ.
Dialogue: 0,0:06:40.79,0:06:43.34,Default,,0000,0000,0000,,Chỉ biết trông cậy vào Natsumi-san và mọi người là không tốt...
Dialogue: 0,0:06:43.35,0:06:44.97,Default,,0000,0000,0000,,Haru-kun!
Dialogue: 0,0:06:45.59,0:06:47.76,Default,,0000,0000,0000,,Tên nhìn lén xấu xa, thích ý kiến hả?
Dialogue: 0,0:06:48.40,0:06:50.12,Default,,0000,0000,0000,,Nhìn lén...?
Dialogue: 0,0:06:50.22,0:06:52.12,Default,,0000,0000,0000,,Ah, không có gì đâu!
Dialogue: 0,0:06:52.12,0:06:55.68,Default,,0000,0000,0000,,Là Mafuyu quá đa cảm đó thôi! Ha... Ha! Ha!
Dialogue: 0,0:06:56.25,0:06:59.30,Default,,0000,0000,0000,,Khỉ thật... Nhỏ Mafuyu này!
Dialogue: 0,0:07:05.46,0:07:07.29,Default,,0000,0000,0000,,Đau quá...
Dialogue: 0,0:07:07.39,0:07:10.09,Default,,0000,0000,0000,,Cái quái gì vậy?
Dialogue: 0,0:07:12.67,0:07:15.31,Default,,0000,0000,0000,,Cuối cùng cũng xong.
Dialogue: 0,0:07:17.11,0:07:20.05,Default,,0000,0000,0000,,Mẹ, hôm nay đen vãi hà.
Dialogue: 0,0:07:27.66,0:07:28.57,Default,,0000,0000,0000,,Ái chà...
Dialogue: 0,0:07:35.79,0:07:38.54,Default,,0000,0000,0000,,Natsumi-san, để em đổ rác dùm cho nhé.
Dialogue: 0,0:07:38.70,0:07:42.72,Default,,0000,0000,0000,,Oh, Haru-chan, việc đó-
Dialogue: 0,0:07:43.31,0:07:46.67,Default,,0000,0000,0000,,Đây là việc chị Natsumi giao cho em.
Dialogue: 0,0:07:47.23,0:07:52.29,Default,,0000,0000,0000,,Vậy hả? Thì để đó anh đổ dùm cho, em không cần phải bận tâm đâu?
Dialogue: 0,0:07:53.92,0:07:55.07,Default,,0000,0000,0000,,Trông anh thật ngứa mắt.
Dialogue: 0,0:07:55.16,0:07:55.75,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:07:57.75,0:07:58.79,Default,,0000,0000,0000,,Em đi đây.
Dialogue: 0,0:08:07.02,0:08:07.87,Default,,0000,0000,0000,,Tuyệt!
Dialogue: 0,0:08:13.33,0:08:16.10,Default,,0000,0000,0000,,Akina-Hime vẫn nổi trội như mọi khi!
Dialogue: 0,0:08:16.99,0:08:18.55,Default,,0000,0000,0000,,Bố thằng bệnh...
Dialogue: 0,0:08:18.55,0:08:21.56,Default,,0000,0000,0000,,Nhờ cả vào thân hình to vật vã của mày đó, Haru-kyun, anh em với nhau mà!
Dialogue: 0,0:08:24.11,0:08:27.24,Default,,0000,0000,0000,,Này tên kia! Có dừng lại không thì bảo?
Dialogue: 0,0:08:27.44,0:08:31.46,Default,,0000,0000,0000,,À mà lớp trưởng này, sao hôm nay không thấy Himenoharanhỉ?!
Dialogue: 0,0:08:31.52,0:08:34.14,Default,,0000,0000,0000,,Vẫn còn cố quay phim hả!
Dialogue: 0,0:08:36.36,0:08:39.82,Default,,0000,0000,0000,,Phiền phức quá đi. Con trai đúng thật là-
Dialogue: 0,0:08:39.95,0:08:42.83,Default,,0000,0000,0000,,Ha-Haru-kyun, đau quá...
Dialogue: 0,0:08:42.99,0:08:44.62,Default,,0000,0000,0000,,Cú ra đòn tuyệt hảo.
Dialogue: 0,0:08:45.54,0:08:49.69,Default,,0000,0000,0000,,Thôi mình tới phòng y tế đây. Cậu muốn xử nó thế nào thì tùy!
Dialogue: 0,0:08:52.46,0:08:57.33,Default,,0000,0000,0000,,Xin lỗi, cô còn cao dán hay... Eh?
Dialogue: 0,0:08:59.68,0:09:01.74,Default,,0000,0000,0000,,Sensei không có ở đây ư...
Dialogue: 0,0:09:03.93,0:09:05.76,Default,,0000,0000,0000,,Ai ở trong đó vậy?
Dialogue: 0,0:09:06.70,0:09:09.45,Default,,0000,0000,0000,,Oh, là Himenohara.
Dialogue: 0,0:09:10.49,0:09:12.75,Default,,0000,0000,0000,,Oái, cô ấy cầm cái gì trên tay vậy?!
Dialogue: 0,0:09:12.81,0:09:15.59,Default,,0000,0000,0000,,Lỡ ai đó phát hiện ra thứ này thì nguy to!
Dialogue: 0,0:09:16.05,0:09:20.07,Default,,0000,0000,0000,,Đợi đã, Shiki-kun. Cậu làm gì ở đây vậy?
Dialogue: 0,0:09:20.36,0:09:21.44,Default,,0000,0000,0000,,Làm gì á?
Dialogue: 0,0:09:21.44,0:09:26.50,Default,,0000,0000,0000,,Ah! Uhm.. Cái này.. K-Khoan, đừng hiểu lầm! Để mình giải thích!
Dialogue: 0,0:09:26.50,0:09:28.10,Default,,0000,0000,0000,,Không cần đâu. Mình biết cả rồi.
Dialogue: 0,0:09:28.95,0:09:30.49,Default,,0000,0000,0000,,Mình biết tất cả mọi chuyện.
Dialogue: 0,0:09:31.76,0:09:35.09,Default,,0000,0000,0000,,Cậu và Suzu... Ở trường học...
Dialogue: 0,0:09:35.91,0:09:38.33,Default,,0000,0000,0000,,Nhưng cậu đã có Akina rồi... Phải không?
Dialogue: 0,0:09:38.65,0:09:40.78,Default,,0000,0000,0000,,M-Mình và Himenohara chỉ-
Dialogue: 0,0:09:40.84,0:09:42.83,Default,,0000,0000,0000,,Làm ơn hãy chia tay với Suzu.
Dialogue: 0,0:09:43.07,0:09:43.55,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:09:43.68,0:09:46.17,Default,,0000,0000,0000,,Tất nhiên, mình sẽ không bắt cậu phải làm chuyện đó không công đâu.
Dialogue: 0,0:09:46.39,0:09:49.86,Default,,0000,0000,0000,,Điều mà Shiki-kun muốn... Mình sẽ đáp ứng cho cậu.
Dialogue: 0,0:09:50.18,0:09:52.21,Default,,0000,0000,0000,,Đ-Điều mình muốn ư...?
Dialogue: 0,0:09:52.21,0:09:54.07,Default,,0000,0000,0000,,Mình đồng ý làm chuyện đó với cậu.
Dialogue: 0,0:09:54.20,0:09:57.08,Default,,0000,0000,0000,,C-Chuyện đó?! Byakudan, cậu...
Dialogue: 0,0:09:57.21,0:09:59.36,Default,,0000,0000,0000,,Cậu không trốn thoát khỏi tay mình được đâu, Shiki-kun.
Dialogue: 0,0:09:59.66,0:10:01.72,Default,,0000,0000,0000,,Mình sẽ lập tức chứng minh cho cậu thấy điều đó.
Dialogue: 0,0:10:01.79,0:10:02.56,Default,,0000,0000,0000,,K-Khoan-
Dialogue: 0,0:10:04.23,0:10:08.31,Default,,0000,0000,0000,,T-Thấy chưa? Thằng nhỏ của cậu đã bự lên như vậy rồi.
Dialogue: 0,0:10:08.31,0:10:10.63,Default,,0000,0000,0000,,K-Khoan đã, chuyện đó-
Dialogue: 0,0:10:10.76,0:10:14.52,Default,,0000,0000,0000,,Dù là mình hay Suzu, cảm giác phấn khích cũng hệt như nhau, phải không?
Dialogue: 0,0:10:15.34,0:10:18.87,Default,,0000,0000,0000,,Shiki-kun có thể làm chuyện đó với bất kì người con gái nào mà, đúng không?
Dialogue: 0,0:10:19.13,0:10:20.99,Default,,0000,0000,0000,,Không lẽ cậu...
Dialogue: 0,0:10:21.09,0:10:23.90,Default,,0000,0000,0000,,S-Sao hả? Mình sẽ cho cậu biết tay!
Dialogue: 0,0:10:32.65,0:10:36.08,Default,,0000,0000,0000,,{\i1}Đ-Đó là... dương vật của đàn ông...
Dialogue: 0,0:10:36.44,0:10:38.73,Default,,0000,0000,0000,,{\i1}Nó thật bự... và nóng.
Dialogue: 0,0:10:48.40,0:10:53.17,Default,,0000,0000,0000,,{\i1}Cơ thể mình đang nóng bừng lên... Tệ quá, đầu óc mình hoàn toàn trống rỗng!
Dialogue: 0,0:10:53.43,0:10:55.16,Default,,0000,0000,0000,,B-Byakudan...
Dialogue: 0,0:10:58.95,0:11:01.30,Default,,0000,0000,0000,,B-Byakudan, m-mình ra đây!
Dialogue: 0,0:11:04.96,0:11:10.09,Default,,0000,0000,0000,,C-Cậu ra nhiều thật đó... Nhưng cậu vẫn chưa hoàn toàn thỏa mãn, đúng không?
Dialogue: 0,0:11:12.93,0:11:14.53,Default,,0000,0000,0000,,Byakudan, dừng lại..
Dialogue: 0,0:11:14.82,0:11:19.92,Default,,0000,0000,0000,,Lại đây nào, Shiki-kun. Dứt khoát mình sẽ khiến cậu phải chia tay Suzu.
Dialogue: 0,0:11:20.02,0:11:23.19,Default,,0000,0000,0000,,Tất cả chuyện này là sao vậy... Hừ.
Dialogue: 0,0:11:24.04,0:11:27.99,Default,,0000,0000,0000,,Sh-Shiki-kun, cậu thật thô bạo... Chưa gì đã-
Dialogue: 0,0:11:31.84,0:11:33.08,Default,,0000,0000,0000,,C-Chỗ đó...!
Dialogue: 0,0:11:36.84,0:11:39.52,Default,,0000,0000,0000,,Chỗ đó của cậu ướt lắm rồi, phải không?
Dialogue: 0,0:11:39.98,0:11:40.99,Default,,0000,0000,0000,,Mình làm nhé.
Dialogue: 0,0:11:41.22,0:11:46.28,Default,,0000,0000,0000,,L-Làm đi. Mình sẽ khiến thằng nhỏ bự con của cậu thỏa mãn.
Dialogue: 0,0:11:50.56,0:11:52.52,Default,,0000,0000,0000,,Byakudan... Biết ngay mà, cậu vẫn còn...
Dialogue: 0,0:11:53.50,0:11:55.75,Default,,0000,0000,0000,,Đ-Đừng để ý tới chuyện đó.
Dialogue: 0,0:11:55.82,0:11:57.42,Default,,0000,0000,0000,,Thôi được...
Dialogue: 0,0:11:57.65,0:11:59.09,Default,,0000,0000,0000,,Mình bắt đầu đây.
Dialogue: 0,0:12:27.57,0:12:29.66,Default,,0000,0000,0000,,{\i1}Tuyệt quá đi mất... Mình không thể dừng lại được!
Dialogue: 0,0:12:29.89,0:12:33.19,Default,,0000,0000,0000,,{\i1}Vậy ra "làm tình với một chàng trai" là như thế này sao?
Dialogue: 0,0:12:33.35,0:12:35.71,Default,,0000,0000,0000,,{\i1}T-Tuyệt quá...
Dialogue: 0,0:12:35.93,0:12:36.78,Default,,0000,0000,0000,,Hả?!
Dialogue: 0,0:12:39.63,0:12:40.77,Default,,0000,0000,0000,,Su...!
Dialogue: 0,0:12:42.50,0:12:45.60,Default,,0000,0000,0000,,K-Không phải như cậu nghĩ đâu! Chỉ là-
Dialogue: 0,0:12:49.16,0:12:52.50,Default,,0000,0000,0000,,{\i1}Không lẽ nào... cậu ấy đã nghe thấy tất cả ư?
Dialogue: 0,0:12:55.60,0:12:57.63,Default,,0000,0000,0000,,K-Khoan đã! Shiki-kun!
Dialogue: 0,0:13:04.29,0:13:06.31,Default,,0000,0000,0000,,Đ-Đừng làm vậy, Suzu!
Dialogue: 0,0:13:12.62,0:13:15.23,Default,,0000,0000,0000,,Đừng mà! Đừng mà! Suzu!
Dialogue: 0,0:13:15.95,0:13:20.33,Default,,0000,0000,0000,,Himenohara, chỗ đó... Tệ quá! Mình ra rồi! Mình ra rồi!
Dialogue: 0,0:13:22.48,0:13:24.93,Default,,0000,0000,0000,,Suzu!
Dialogue: 0,0:13:36.07,0:13:37.84,Default,,0000,0000,0000,,Anh về rồi đây.
Dialogue: 0,0:13:42.22,0:13:43.29,Default,,0000,0000,0000,,Hở?
Dialogue: 0,0:13:55.87,0:13:57.60,Default,,0000,0000,0000,,Em bình thường trở lại rồi hả?!
Dialogue: 0,0:14:01.98,0:14:03.15,Default,,0000,0000,0000,,Muốn giỡn à?
Dialogue: 0,0:14:03.32,0:14:04.04,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:14:05.15,0:14:07.04,Default,,0000,0000,0000,,Nhỏ này sao vậy?
Dialogue: 0,0:14:08.94,0:14:09.72,Default,,0000,0000,0000,,Hừm?
Dialogue: 0,0:14:19.42,0:14:22.79,Default,,0000,0000,0000,,Oa... cái gì thế này...
Dialogue: 0,0:14:24.26,0:14:25.50,Default,,0000,0000,0000,,Thích không?
Dialogue: 0,0:14:25.76,0:14:27.30,Default,,0000,0000,0000,,Ma...fuyu?!
Dialogue: 0,0:14:27.30,0:14:28.41,Default,,0000,0000,0000,,Thấy hưng phấn không?
Dialogue: 0,0:14:28.57,0:14:30.41,Default,,0000,0000,0000,,K-Không hẳn vậy.
Dialogue: 0,0:14:31.93,0:14:32.85,Default,,0000,0000,0000,,Anh sẽ bị phạt.
Dialogue: 0,0:14:32.85,0:14:33.37,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:14:36.02,0:14:40.75,Default,,0000,0000,0000,,N-Này! Mafuyu! Thế này là sao hả?!
Dialogue: 0,0:14:41.18,0:14:42.22,Default,,0000,0000,0000,,Thêm bạn bớt thù nào.
Dialogue: 0,0:14:42.42,0:14:45.59,Default,,0000,0000,0000,,B-Bạn thù cái gì? Ý em là sao hả?
Dialogue: 0,0:14:46.47,0:14:49.64,Default,,0000,0000,0000,,Haruomi, anh thường hay nổi hứng với mấy "thứ này", phải không?
Dialogue: 0,0:14:50.39,0:14:51.96,Default,,0000,0000,0000,,Muốn xem hàng của em không?
Dialogue: 0,0:14:52.12,0:14:53.62,Default,,0000,0000,0000,,Nói vớ vẩn!
Dialogue: 0,0:14:53.72,0:14:54.83,Default,,0000,0000,0000,,Anh sợ à?
Dialogue: 0,0:14:54.90,0:14:57.58,Default,,0000,0000,0000,,K-Không, không hẳn là vậy.
Dialogue: 0,0:15:01.27,0:15:04.18,Default,,0000,0000,0000,,Vậy, Haruomi... Cái gì đây?
Dialogue: 0,0:15:04.34,0:15:06.27,Default,,0000,0000,0000,,Đ-Đây là... Từ lần trước...
Dialogue: 0,0:15:06.30,0:15:10.25,Default,,0000,0000,0000,,Nếu anh chịu nghe lời, em sẽ làm cho anh sung sướng hơn nữa.
Dialogue: 0,0:15:11.56,0:15:14.30,Default,,0000,0000,0000,,Em... Em học ở đâu ra mấy trò này...
Dialogue: 0,0:15:18.94,0:15:21.26,Default,,0000,0000,0000,,Kiềm chế nhiều không tốt cho sức khỏe đâu anh.
Dialogue: 0,0:15:23.32,0:15:26.16,Default,,0000,0000,0000,,Thằng nhỏ của Haruomi đang co giật này.
Dialogue: 0,0:15:28.74,0:15:30.54,Default,,0000,0000,0000,,Sướng lắm phải không?
Dialogue: 0,0:15:33.51,0:15:35.01,Default,,0000,0000,0000,,Còn thế này thì sao?
Dialogue: 0,0:15:35.96,0:15:38.41,Default,,0000,0000,0000,,{\i1}Cảm giác mềm mại này... Chết thật!
Dialogue: 0,0:15:38.64,0:15:43.08,Default,,0000,0000,0000,,Haruomi... Thằng nhỏ của anh đang run lên vì thích thú, phải không?
Dialogue: 0,0:15:43.74,0:15:47.33,Default,,0000,0000,0000,,Chính là khuôn mặt đáng yêu này... anh sắp ra rồi, phải không?
Dialogue: 0,0:15:47.33,0:15:49.13,Default,,0000,0000,0000,,Mafuyu, dừng-
Dialogue: 0,0:15:52.33,0:15:55.95,Default,,0000,0000,0000,,Vậy là anh ra rồi sao? Đáng yêu thật đó, Haruomi.
Dialogue: 0,0:15:56.77,0:16:00.92,Default,,0000,0000,0000,,Anh đúng là quái vật, Haruomi, vừa mới ra xong mà đã...
Dialogue: 0,0:16:05.39,0:16:06.86,Default,,0000,0000,0000,,Mùi vị thật khác lạ...
Dialogue: 0,0:16:06.86,0:16:09.71,Default,,0000,0000,0000,,Ma-Mafuyu... Em cởi trói cho anh được không?
Dialogue: 0,0:16:09.77,0:16:11.76,Default,,0000,0000,0000,,Anh im đi. Cấm động đậy nhé.
Dialogue: 0,0:16:23.75,0:16:29.34,Default,,0000,0000,0000,,{\i1}Tuyệt quá... Sao lại như vậy... nhỏ này bú nó thật điêu luyện.
Dialogue: 0,0:16:36.30,0:16:37.24,Default,,0000,0000,0000,,Nào...
Dialogue: 0,0:16:37.86,0:16:39.50,Default,,0000,0000,0000,,Ma-Mafuyu..
Dialogue: 0,0:16:39.89,0:16:43.71,Default,,0000,0000,0000,,Anh tưởng sẽ kiềm chế được mãi ư? Sắp tới sẽ còn sung sướng hơn nhiều đấy.
Dialogue: 0,0:16:43.71,0:16:46.50,Default,,0000,0000,0000,,Đây là lần đầu tiên của em phải không? Đừng ép mình quá.
Dialogue: 0,0:16:46.50,0:16:49.36,Default,,0000,0000,0000,,Em không sao. Anh bớt lời đi được không, Haruomi.
Dialogue: 0,0:16:49.69,0:16:53.58,Default,,0000,0000,0000,,Em muốn được ngắm gương mặt thỏa mãn của anh.
Dialogue: 0,0:16:57.59,0:16:58.71,Default,,0000,0000,0000,,Mafuyu!
Dialogue: 0,0:16:59.16,0:17:01.22,Default,,0000,0000,0000,,Không sao mà! Đừng cựa quậy!
Dialogue: 0,0:17:04.27,0:17:07.32,Default,,0000,0000,0000,,Hừ, em lúc nào cũng tự mãn như vậy.
Dialogue: 0,0:17:07.42,0:17:10.21,Default,,0000,0000,0000,,Cứ đợi đấy, anh sẽ làm em sung sướng ngay thôi.
Dialogue: 0,0:17:23.19,0:17:24.97,Default,,0000,0000,0000,,Ha-Haruomi...
Dialogue: 0,0:17:37.98,0:17:41.43,Default,,0000,0000,0000,,Em ướt đẫm rồi nè. Đã nhận ra chưa?
Dialogue: 0,0:17:41.77,0:17:43.03,Default,,0000,0000,0000,,Không hẳn vậy...
Dialogue: 0,0:17:45.04,0:17:46.97,Default,,0000,0000,0000,,Thật chẳng đáng yêu chút nào cả.
Dialogue: 0,0:17:47.73,0:17:50.45,Default,,0000,0000,0000,,Đều tại Haruomi hết... Đừng hòng bắt nạt được em.
Dialogue: 0,0:17:50.51,0:17:51.62,Default,,0000,0000,0000,,Vậy sao.
Dialogue: 0,0:17:52.00,0:17:52.76,Default,,0000,0000,0000,,S-Sao hả?
Dialogue: 0,0:17:52.96,0:17:56.96,Default,,0000,0000,0000,,Được thôi, anh sẽ hỏi trực tiếp thân thể của Mafuyu.
Dialogue: 0,0:18:08.58,0:18:12.49,Default,,0000,0000,0000,,Sao hả, Haruomi? Anh sắp ra rồi phải không?
Dialogue: 0,0:18:13.57,0:18:15.74,Default,,0000,0000,0000,,Đến giờ mà em vẫn còn nói ra được những câu như vậy sao?
Dialogue: 0,0:18:17.43,0:18:20.60,Default,,0000,0000,0000,,Trẻ hư sẽ phải chịu phạt.
Dialogue: 0,0:18:39.11,0:18:43.34,Default,,0000,0000,0000,,Haruomi... Cơ thể em đang nóng bừng lên.
Dialogue: 0,0:18:43.76,0:18:46.08,Default,,0000,0000,0000,,Cuối cùng thì em cũng chịu nói thật rồi hả?!
Dialogue: 0,0:18:46.32,0:18:47.65,Default,,0000,0000,0000,,Chỉ một chút nữa thôi!
Dialogue: 0,0:18:50.68,0:18:53.35,Default,,0000,0000,0000,,Mafuyu... A-Anh ra đây!
Dialogue: 0,0:18:53.62,0:18:56.53,Default,,0000,0000,0000,,Haruomi! Nữa đi! Nữa đi!!
Dialogue: 0,0:18:57.60,0:19:00.03,Default,,0000,0000,0000,,Chịu hết nổi rồi! Anh ra đây, Mafuyu!
Dialogue: 0,0:19:18.71,0:19:22.10,Default,,0000,0000,0000,,Hừ, đúng là tự làm khổ mình.
Dialogue: 0,0:19:23.00,0:19:26.49,Default,,0000,0000,0000,,Những lúc thế này, em thật đáng yêu biết bao.
Dialogue: 0,0:19:38.99,0:19:42.97,Default,,0000,0000,0000,,C-Chào buổi sáng! Hôm nay em dậy sớm thật đấy.
Dialogue: 0,0:19:43.02,0:19:44.71,Default,,0000,0000,0000,,Có chuyện gì vậy?
Dialogue: 0,0:19:44.81,0:19:45.71,Default,,0000,0000,0000,,Bento.
Dialogue: 0,0:19:45.86,0:19:46.76,Default,,0000,0000,0000,,Bento?
Dialogue: 0,0:19:46.84,0:19:49.01,Default,,0000,0000,0000,,Em sẽ làm bữa trưa cho Haruomi.
Dialogue: 0,0:19:50.08,0:19:50.84,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:19:51.27,0:19:52.84,Default,,0000,0000,0000,,EEEEEEEEEEEEH??
Dialogue: 0,0:19:56.93,0:20:02.60,Default,,0000,0000,0000,,Giờ mới là lúc cần phải suy tính đây, mình vẫn chưa nghĩ ra cách nào để ngăn chặn sự bành trướng của One O One.
Dialogue: 0,0:20:03.58,0:20:06.06,Default,,0000,0000,0000,,Haruomi. Thế này là sao hả?
Dialogue: 0,0:20:06.25,0:20:07.25,Default,,0000,0000,0000,,Sao cái gì...
Dialogue: 0,0:20:07.49,0:20:10.59,Default,,0000,0000,0000,,Mày đã âm thầm đi tắm biển cùng ba người bọn họ phải không?
Dialogue: 0,0:20:10.62,0:20:12.50,Default,,0000,0000,0000,,"Âm thầm" là thế nào...
Dialogue: 0,0:20:13.03,0:20:15.82,Default,,0000,0000,0000,,Sao mày không rủ tao đi cùng hả!
Dialogue: 0,0:20:15.82,0:20:19.98,Default,,0000,0000,0000,,Akina-Hime mặc áo tắm! Natsumi-san nữa! Mafuyu-tan cũng vậy!!
Dialogue: 0,0:20:20.05,0:20:24.10,Default,,0000,0000,0000,,Honnoji... Mày nên dừng lại trước khi mọi người tưởng tao với mày là gay!?
Dialogue: 0,0:20:24.21,0:20:24.79,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:20:27.38,0:20:29.57,Default,,0000,0000,0000,,Tóm lại, mày muốn gì?
Dialogue: 0,0:20:30.09,0:20:32.15,Default,,0000,0000,0000,,Đây. Mày thấy sao hả?
Dialogue: 0,0:20:32.54,0:20:35.94,Default,,0000,0000,0000,,Trông giống trang phục hầu gái, có điều... tao chưa nhìn thấy mấy mẫu này bao giờ.
Dialogue: 0,0:20:36.13,0:20:38.49,Default,,0000,0000,0000,,Và nữa, mày không thấy chúng hơi "đẫy đà" quá sao?
Dialogue: 0,0:20:38.72,0:20:41.03,Default,,0000,0000,0000,,Thực ra, chính tao là người thiết kế mấy mẫu này đó.
Dialogue: 0,0:20:44.43,0:20:45.61,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:20:46.56,0:20:48.84,Default,,0000,0000,0000,,Haruomi, đây là nguyện vọng duy nhất của tao.
Dialogue: 0,0:20:48.97,0:20:50.74,Default,,0000,0000,0000,,Tao sẽ cho mày mượn máy quay phim.
Dialogue: 0,0:20:50.74,0:20:56.00,Default,,0000,0000,0000,,Hãy gạ Natsumi-san và hai người kia mặc thứ này, và quay phim lại cho tao-
Dialogue: 0,0:20:58.97,0:21:01.09,Default,,0000,0000,0000,,Hừ, công nhận mày bệnh vãi hà..
Dialogue: 0,0:21:01.29,0:21:03.48,Default,,0000,0000,0000,,Oh, khoan đã...
Dialogue: 0,0:21:08.90,0:21:11.55,Default,,0000,0000,0000,,Thôi được, anh sẽ giúp chú.
Dialogue: 0,0:21:11.68,0:21:14.19,Default,,0000,0000,0000,,Ơ... Đây là...
Dialogue: 0,0:21:14.94,0:21:17.75,Default,,0000,0000,0000,,C-Chúng ta phải mặc thứ này để bán kem sao?
Dialogue: 0,0:21:17.88,0:21:20.33,Default,,0000,0000,0000,,Waah! Dễ thương quá!
Dialogue: 0,0:21:20.40,0:21:22.10,Default,,0000,0000,0000,,Hình như kích cỡ không đúng?
Dialogue: 0,0:21:22.82,0:21:26.60,Default,,0000,0000,0000,,Thứ mà tiệm chúng ta còn thiếu chính là... những cô phục vụ trẻ đẹp!
Dialogue: 0,0:21:27.09,0:21:31.70,Default,,0000,0000,0000,,Nhưng với sự {\i1}cải tổ{\i0} mạnh mẽ này, chúng ta chắc chắn sẽ thành công!
Dialogue: 0,0:21:32.13,0:21:35.42,Default,,0000,0000,0000,,Nhưng mà... trông cứ như là một tiệm kem đồi trụy ấy?
Dialogue: 0,0:21:35.42,0:21:38.95,Default,,0000,0000,0000,,K-Không phải vậy! Trông mọi người ai cũng đáng yêu mà!
Dialogue: 0,0:21:38.99,0:21:40.75,Default,,0000,0000,0000,,Haruomi sẽ mặc thứ này.
Dialogue: 0,0:21:40.75,0:21:41.47,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:21:41.73,0:21:45.36,Default,,0000,0000,0000,,Thôi được, chúng ta sẽ cùng nhau làm cho cửa hàng phát triển vượt bậc!
Dialogue: 0,0:21:45.39,0:21:48.00,Default,,0000,0000,0000,,Eh?! Onee-chan!
Dialogue: 0,0:21:48.92,0:21:53.85,Default,,0000,0000,0000,,Chào mừng quý khách! BB Ice hiện đang mở chiến dịch đặc biệt chăm sóc khách hàng!
Dialogue: 0,0:21:54.34,0:21:57.70,Default,,0000,0000,0000,,Dịch vụ đặc biệt của tiệm kem MiMi!
Dialogue: 0,0:21:58.16,0:21:59.92,Default,,0000,0000,0000,,Xin quý khách vui lòng xếp hàng.
Dialogue: 0,0:22:00.28,0:22:03.98,Default,,0000,0000,0000,,Hôm nay, bản tiệm đặc biệt giảm giá các loại kem Vani!
Dialogue: 0,0:22:04.17,0:22:06.78,Default,,0000,0000,0000,,Vậy à, bán cho tôi một kem Vani đúp nào.
Dialogue: 0,0:22:06.78,0:22:11.00,Default,,0000,0000,0000,,Vâng. Kem Vani phải không ạ? Của quý khách đây.
Dialogue: 0,0:22:11.16,0:22:13.68,Default,,0000,0000,0000,,Cám ơn quý khách đã ủng hộ.
Dialogue: 0,0:22:41.97,0:22:43.76,Default,,0000,0000,0000,,Chúng ta thành công rồi, Onee-chan!
Dialogue: 0,0:22:43.99,0:22:46.28,Default,,0000,0000,0000,,Uh, Aki-nee-chan!
Dialogue: 0,0:22:46.28,0:22:48.86,Default,,0000,0000,0000,,Haruomi... tiêu rồi.
Dialogue: 0,0:22:59.38,0:23:00.98,Default,,0000,0000,0000,,Akina, mình vào nhé.
Dialogue: 0,0:23:02.51,0:23:03.95,Default,,0000,0000,0000,,Cậu thích bộ đồ này đến vậy cơ à?
Dialogue: 0,0:23:03.95,0:23:07.01,Default,,0000,0000,0000,,Eh?! H-Haru-kun! Ít ra cậu cũng phải gõ cửa trước chứ!
Dialogue: 0,0:23:07.06,0:23:11.89,Default,,0000,0000,0000,,Có gì mà phải giấu giếm chứ? Bộ trang phục này hợp với cậu lắm đó, Akina.
Dialogue: 0,0:23:12.15,0:23:14.05,Default,,0000,0000,0000,,Haru-kun...
Dialogue: 0,0:23:14.76,0:23:21.30,Default,,0000,0000,0000,,Haru-kun, hôm nay cậu làm việc vất vả lắm phải không? Đêm nay mình sẽ chỉ làm hầu gái cho riêng mình Haru-kun thôi.
Dialogue: 0,0:23:23.85,0:23:27.60,Default,,0000,0000,0000,,Haru-kun... Thế này xấu hổ chết đi được.
Dialogue: 0,0:23:28.35,0:23:31.88,Default,,0000,0000,0000,,Hôm nay, để thưởng công, hầu gái Akina sẽ làm mọi điều mình muốn, phải không?
Dialogue: 0,0:23:31.91,0:23:33.32,Default,,0000,0000,0000,,P-Phải...
Dialogue: 0,0:23:33.84,0:23:36.88,Default,,0000,0000,0000,,Thằng nhỏ của Haru-kun nóng ran lên rồi nè...
Dialogue: 0,0:23:50.08,0:23:55.96,Default,,0000,0000,0000,,Akina bú nó điêu luyện quá. Kĩ thuật của cậu... thật tuyệt vời.
Dialogue: 0,0:24:02.62,0:24:05.10,Default,,0000,0000,0000,,Cậu giúp mình ra nhé, Akina?
Dialogue: 0,0:24:05.36,0:24:09.77,Default,,0000,0000,0000,,Tất nhiên rồi. Mình sẽ dùng miệng làm cho cậu sung sướng hơn nữa.
Dialogue: 0,0:24:19.71,0:24:21.24,Default,,0000,0000,0000,,Mình ra đây, Akina!
Dialogue: 0,0:24:32.48,0:24:33.56,Default,,0000,0000,0000,,Akina..
Dialogue: 0,0:24:37.12,0:24:39.70,Default,,0000,0000,0000,,Đ-Đừng! Chỗ đó...
Dialogue: 0,0:24:40.42,0:24:45.64,Default,,0000,0000,0000,,Cách Haru-kun sờ nắn nơi đó... thật dâm đãng làm sao.
Dialogue: 0,0:24:50.44,0:24:55.44,Default,,0000,0000,0000,,Đồng ý cho mình làm những điều này... Từ khi nào Akina lại trở nên dâm đãng đến vậy?
Dialogue: 0,0:24:56.06,0:24:57.57,Default,,0000,0000,0000,,Không phải thế...
Dialogue: 0,0:25:12.00,0:25:15.86,Default,,0000,0000,0000,,Akina... Tuyệt quá, cậu ướt đẫm rồi nè!
Dialogue: 0,0:25:17.66,0:25:19.65,Default,,0000,0000,0000,,Cậu có nghe thấy {\i1}tiếng gì{\i0} không?
Dialogue: 0,0:25:21.12,0:25:24.78,Default,,0000,0000,0000,,Haru-kun, đừng mà! Mình ra rồi.
Dialogue: 0,0:25:25.92,0:25:26.90,Default,,0000,0000,0000,,Mình vào đây.
Dialogue: 0,0:25:28.63,0:25:33.60,Default,,0000,0000,0000,,Bên trong Akina thật ấm áp... dường như nó đang tan chảy ra.
Dialogue: 0,0:25:45.55,0:25:47.22,Default,,0000,0000,0000,,Mình ra đây, Akina!
Dialogue: 0,0:25:58.82,0:26:03.52,Default,,0000,0000,0000,,Nó đang đâm sâu vào... Thằng nhỏ của Haru-kun đang đâm sâu vào bên trong mình!
Dialogue: 0,0:26:08.22,0:26:10.48,Default,,0000,0000,0000,,Akina, mình sắp ra nữa rồi!
Dialogue: 0,0:26:12.21,0:26:16.49,Default,,0000,0000,0000,,K-Không được! Cậu vừa bắn vào bên trong mình một phút trước mà!
Dialogue: 0,0:26:18.55,0:26:23.58,Default,,0000,0000,0000,,Ha-Haru-kun, cậu ra nhiều quá... Nhiều lắm đó!
Dialogue: 0,0:26:23.71,0:26:26.03,Default,,0000,0000,0000,,Akina, mình lại ra nữa rồi!
Dialogue: 0,0:26:39.65,0:26:45.86,Default,,0000,0000,0000,,Tinh dịch của Haru-kun... đang trào ra ngoài.
Dialogue: 0,0:26:46.28,0:26:50.10,Default,,0000,0000,0000,,Tuyệt quá, tinh dịch đang bôi trơn chỗ đó... Ôi! Mình sắp-
Dialogue: 0,0:26:52.06,0:26:56.11,Default,,0000,0000,0000,,Haru-kun, mình sắp ra nữa rồi!
Dialogue: 0,0:26:56.21,0:26:58.47,Default,,0000,0000,0000,,Akina! Akina!
Dialogue: 0,0:27:14.70,0:27:17.74,Default,,0000,0000,0000,,Tuyệt quá... Nó trào ra nhiều chưa kìa...
Dialogue: 0,0:27:17.84,0:27:21.89,Default,,0000,0000,0000,,Hừ... Haru-kun, làm gì mà cứ hùng hục như trâu vậy.
Dialogue: 0,0:27:24.66,0:27:26.75,Default,,0000,0000,0000,,Akina... Thôi mà...
Dialogue: 0,0:27:28.42,0:27:29.11,Default,,0000,0000,0000,,Hả?!
Dialogue: 0,0:27:29.50,0:27:33.88,Default,,0000,0000,0000,,Thằng nhỏ của Haru-chan... vẫn sung mãn như mọi khi.
Dialogue: 0,0:27:34.10,0:27:35.90,Default,,0000,0000,0000,,Sau Natsumi sẽ tới lượt em.
Dialogue: 0,0:27:35.97,0:27:36.49,Default,,0000,0000,0000,,Hể?!
Dialogue: 0,0:27:36.55,0:27:39.10,Default,,0000,0000,0000,,Na-Natsumi-san, chuyện đó... Chị nhớ không!
Dialogue: 0,0:27:39.43,0:27:42.83,Default,,0000,0000,0000,,Ôi ôi, Haru-chan... Đó là bí mật mà.
Dialogue: 0,0:27:42.89,0:27:44.88,Default,,0000,0000,0000,,Haruomi, lần sau tới lượt em.
Dialogue: 0,0:27:45.08,0:27:47.07,Default,,0000,0000,0000,,Haru-kun là bạn trai của em mà!
Dialogue: 0,0:27:47.17,0:27:49.88,Default,,0000,0000,0000,,Hai đứa thật to gan, dám giấu chị chuyện đó sao!
Dialogue: 0,0:27:49.92,0:27:52.07,Default,,0000,0000,0000,,Chị không thể độc chiếm anh ấy được đâu.
Dialogue: 0,0:27:52.40,0:27:53.52,Default,,0000,0000,0000,,Hừ.
Dialogue: 0,0:27:53.67,0:27:55.14,Default,,0000,0000,0000,,Vậy nhé, Haru-chan...
Dialogue: 0,0:27:55.21,0:27:56.94,Default,,0000,0000,0000,,Bắt đầu thưởng công nào.
