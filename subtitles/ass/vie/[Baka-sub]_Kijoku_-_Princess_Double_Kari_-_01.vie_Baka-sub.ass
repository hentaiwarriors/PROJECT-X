Dialogue: 0,0:00:15.61,0:00:27.70,Type,,0000,0000,0000,,{\fs30\c&H5A50FF&\pos(567,72)}Baka-sub\N{\fs25\c&H7D1562&}Baka-sub.net
Dialogue: 0,0:00:15.61,0:00:27.70,Type,,0000,0000,0000,,
Dialogue: 0,0:00:16.73,0:00:21.16,Default,,0000,0000,0000,,{\pos(317,463)\c&H080808&\3c&HFFFFFF&\4c&HFFFFFF&}Nhật kí {\c&H3B30FF&}công {\c&H1B1B1B&}chúa
Dialogue: 0,0:00:22.03,0:00:26.56,Default,,0000,0000,0000,,Phần 1: {\c&H1107CD&}Hương tàn ngọc nát
Dialogue: 0,0:00:44.32,0:00:46.34,Default,,0000,0000,0000,,Nàng cảm thấy sao, Công chúa Eleasteria?
Dialogue: 0,0:00:46.42,0:00:49.69,Default,,0000,0000,0000,,Hay ta nên gọi nàng là Elena?
Dialogue: 0,0:00:50.26,0:00:55.25,Default,,0000,0000,0000,,Distol, sao ngươi dám bắt cóc và giam cầm ta tai nơi tồi tệ này.
Dialogue: 0,0:00:55.33,0:00:58.26,Default,,0000,0000,0000,,Làm như vậy đâu xứng với tác phong của một quý ông?
Dialogue: 0,0:00:58.33,0:00:59.80,Default,,0000,0000,0000,,Nàng thật cứng đầu.
Dialogue: 0,0:00:59.87,0:01:03.70,Default,,0000,0000,0000,,Có điều, hi vọng nàng hiểu tình thế của mình lúc này.
Dialogue: 0,0:01:05.11,0:01:07.07,Default,,0000,0000,0000,,Ở nơi này chỉ có duy nhất mình ta.
Dialogue: 0,0:01:07.14,0:01:10.17,Default,,0000,0000,0000,,Dù nàng khóc lóc cầu xin, cũng chẳng ai đoái hoài tới nàng đâu.
Dialogue: 0,0:01:14.05,0:01:15.54,Default,,0000,0000,0000,,Không! Cấm ngươi lại gần ta...!
Dialogue: 0,0:01:16.95,0:01:18.71,Default,,0000,0000,0000,,Cái gì!? Đừng động vào ta!
Dialogue: 0,0:01:20.85,0:01:24.29,Default,,0000,0000,0000,,To gan, ngươi biết hậu quả của việc này rồi chứ!?
Dialogue: 0,0:01:24.36,0:01:28.23,Default,,0000,0000,0000,,Có cần ta nhắc lại rằng nàng không còn là Công chúa nữa không?
Dialogue: 0,0:01:29.36,0:01:30.66,Default,,0000,0000,0000,,Giờ với ta, nàng chỉ là... một con điếm!
Dialogue: 0,0:01:32.13,0:01:32.76,Default,,0000,0000,0000,,Không!!
Dialogue: 0,0:01:34.23,0:01:35.67,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:01:37.64,0:01:39.76,Default,,0000,0000,0000,,Dừng lại đi, đừng nhìn!
Dialogue: 0,0:01:40.17,0:01:42.37,Default,,0000,0000,0000,,Một thân hình hoàn hảo, kèm theo bộ ngực tuyệt đẹp.
Dialogue: 0,0:01:42.44,0:01:45.07,Default,,0000,0000,0000,,Đàn ông ai mà không thích những thứ đó chứ.
Dialogue: 0,0:01:47.58,0:01:51.98,Default,,0000,0000,0000,,Nếu ngươi còn tiếp tục, ta sẽ bắt ngươi phải trả giá cho chuyện đó!
Dialogue: 0,0:01:52.52,0:01:56.01,Default,,0000,0000,0000,,Tỏ ra biết điều một chút đi nào, ta sẽ dạy cho nàng biết: niềm hoan lạc thực sự là thế nào.
Dialogue: 0,0:01:56.59,0:01:59.72,Default,,0000,0000,0000,,Không đời nào! Thả ta ra, thả ta ra mau!
Dialogue: 0,0:02:01.16,0:02:03.56,Default,,0000,0000,0000,,Nàng sẽ sớm đổi ý thôi.
Dialogue: 0,0:02:05.37,0:02:06.76,Default,,0000,0000,0000,,Dừng lại, tên biến thái!
Dialogue: 0,0:02:07.63,0:02:08.29,Default,,0000,0000,0000,,Dừng lại ngay!
Dialogue: 0,0:02:11.57,0:02:13.23,Default,,0000,0000,0000,,Sao vậy? Nàng thấy thích rồi hả?
Dialogue: 0,0:02:14.07,0:02:15.84,Default,,0000,0000,0000,,Tất nhiên là... không...!!
Dialogue: 0,0:02:16.78,0:02:18.04,Default,,0000,0000,0000,,Không gì cơ?
Dialogue: 0,0:02:25.05,0:02:27.85,Default,,0000,0000,0000,,Có vẻ như công chúa của chúng ta rất nhạy cảm ở bộ phận này.
Dialogue: 0,0:02:28.49,0:02:29.79,Default,,0000,0000,0000,,Không phải như vậy.
Dialogue: 0,0:02:33.09,0:02:35.29,Default,,0000,0000,0000,,Không, không phải chỗ đó!
Dialogue: 0,0:02:37.93,0:02:41.83,Default,,0000,0000,0000,,Không, đó là, đó là...!
Dialogue: 0,0:02:43.00,0:02:43.49,Default,,0000,0000,0000,,Ngươi làm gì vậy!?
Dialogue: 0,0:02:43.57,0:02:46.63,Default,,0000,0000,0000,,Làm gì ư? Nàng nhìn lại mình đi!
Dialogue: 0,0:02:46.71,0:02:48.87,Default,,0000,0000,0000,,Không, chỗ đó là, chỗ đó là...!
Dialogue: 0,0:02:50.88,0:02:53.00,Default,,0000,0000,0000,,Không, làm ơn hãy dừng lại!
Dialogue: 0,0:02:53.08,0:02:56.05,Default,,0000,0000,0000,,Nàng ướt đẫm rồi... công chua kiểu gì thế này.
Dialogue: 0,0:02:57.62,0:03:01.58,Default,,0000,0000,0000,,Thế này khác gì tra tấn... ngươi đang săm soi chỗ đó của ta...
Dialogue: 0,0:03:01.66,0:03:05.85,Default,,0000,0000,0000,,Hóa ra, nàng thấy kích thích khi bị ta tra tấn. Thật dâm đãng!
Dialogue: 0,0:03:06.69,0:03:09.13,Default,,0000,0000,0000,,Không, không đúng!
Dialogue: 0,0:03:09.20,0:03:14.13,Default,,0000,0000,0000,,Nàng muốn kêu gào gì thì tùy, nhưng cơ thể nàng không hề nói dối.
Dialogue: 0,0:03:16.80,0:03:20.17,Default,,0000,0000,0000,,Công chúa Eleasteria, hẳn nàng biết ta sắp làm chuyện gì, phải không?
Dialogue: 0,0:03:20.31,0:03:21.80,Default,,0000,0000,0000,,Đừng...
Dialogue: 0,0:03:23.51,0:03:26.17,Default,,0000,0000,0000,,Đừng... Không...!
Dialogue: 0,0:03:26.25,0:03:29.58,Default,,0000,0000,0000,,Nhìn kĩ vào. Nó đang đâm vào bên trong nàng đó!
Dialogue: 0,0:03:30.48,0:03:36.25,Default,,0000,0000,0000,,Distol, nếu ngươi dừng lại, ta hứa sẽ không trách tội ngươi... làm ơn!
Dialogue: 0,0:03:36.69,0:03:38.18,Default,,0000,0000,0000,,Ta không cần sự tha thứ của nàng!
Dialogue: 0,0:03:39.73,0:03:40.99,Default,,0000,0000,0000,,Dừng lại!!
Dialogue: 0,0:03:41.76,0:03:43.82,Default,,0000,0000,0000,,Cảm giác bị dày vò thế nào hả?
Dialogue: 0,0:03:45.90,0:03:48.33,Default,,0000,0000,0000,,Sao vậy? Nàng thèm muốn đến mức này rồi sao?
Dialogue: 0,0:03:48.94,0:03:52.30,Default,,0000,0000,0000,,Dừng lại, dừng lại, làm ơn dừng lại đi. Ta cầu xin ngươi.
Dialogue: 0,0:03:52.94,0:03:54.70,Default,,0000,0000,0000,,Tại sao ngươi lại đối xử với ta như vậy!?
Dialogue: 0,0:03:54.77,0:03:56.61,Default,,0000,0000,0000,,Nàng không cần... biết!
Dialogue: 0,0:04:03.35,0:04:04.11,Default,,0000,0000,0000,,Khít quá đi.
Dialogue: 0,0:04:04.18,0:04:08.38,Default,,0000,0000,0000,,Rút nó ra, rút ra! Đau quá, đau chết mất!
Dialogue: 0,0:04:12.26,0:04:14.39,Default,,0000,0000,0000,,Vậy ra đây là lần đầu tiên của nàng.
Dialogue: 0,0:04:15.16,0:04:18.15,Default,,0000,0000,0000,,Ngươi thật nhẫn tâm... đời con gái của ta đã...
Dialogue: 0,0:04:19.87,0:04:21.06,Default,,0000,0000,0000,,Đừng đâm vào nữa!
Dialogue: 0,0:04:21.67,0:04:22.26,Default,,0000,0000,0000,,Đừng đâm vào nữa!
Dialogue: 0,0:04:22.34,0:04:27.77,Default,,0000,0000,0000,,Màng trinh của Elena rách toạc và quấn lấy thằng nhỏ của tôi.
Dialogue: 0,0:04:29.01,0:04:39.25,Default,,0000,0000,0000,,Hẳn các bạn đang rất thắc mắc vì sao tôi làm vậy với Elena.\NVậy nên tôi sẽ kể cho các bạn nghe đầu đuôi câu chuyện.
Dialogue: 0,0:04:41.76,0:04:45.21,Default,,0000,0000,0000,,Tôi vốn là một hoàng tử của Vương quốc Asteria.
Dialogue: 0,0:04:45.76,0:04:48.13,Default,,0000,0000,0000,,Là trẻ mồ côi, tôi không hề có một gia đình thực sự, nhưng...
Dialogue: 0,0:04:48.19,0:04:51.03,Default,,0000,0000,0000,,Có một người đã thương yêu và che chở tôi cho tới lúc chết... Hoàng hậu Rosaria...
Dialogue: 0,0:04:52.07,0:04:53.59,Default,,0000,0000,0000,,Nii-sama. Nii-sama?
Dialogue: 0,0:04:56.60,0:05:00.33,Default,,0000,0000,0000,,Di, chuyện gì vậy? Con không nghe thấy Qoona gọi sao?
Dialogue: 0,0:05:01.24,0:05:05.64,Default,,0000,0000,0000,,Không có gì đâu, mẫu thân... Con chỉ hơi mơ màng một chút.
Dialogue: 0,0:05:06.88,0:05:08.68,Default,,0000,0000,0000,,Anh vẫn khỏe đấy chứ?
Dialogue: 0,0:05:08.95,0:05:11.75,Default,,0000,0000,0000,,Anh không sao, Qoona, cám ơn em đã quan tâm.
Dialogue: 0,0:05:12.79,0:05:14.95,Default,,0000,0000,0000,,Nii-sama, làm vậy nhột lắm.
Dialogue: 0,0:05:15.02,0:05:18.68,Default,,0000,0000,0000,,Qoona, Mẫu thân và anh có việc phải đi xa kể từ ngày mai.
Dialogue: 0,0:05:18.76,0:05:20.73,Default,,0000,0000,0000,,Em không buồn chứ?
Dialogue: 0,0:05:23.60,0:05:25.00,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:05:25.07,0:05:27.97,Default,,0000,0000,0000,,Em không sao, em hứa sẽ không buồn đâu.
Dialogue: 0,0:05:28.03,0:05:31.47,Default,,0000,0000,0000,,Anh sẽ quay lại sớm thôi, vậy nên em đừng lo lắng gì cả.
Dialogue: 0,0:05:32.71,0:05:35.83,Default,,0000,0000,0000,,Vâng, em hứa, Nii-sama...
Dialogue: 0,0:05:37.34,0:05:39.47,Default,,0000,0000,0000,,Đừng khóc, Qoona.
Dialogue: 0,0:05:40.51,0:05:43.85,Default,,0000,0000,0000,,Làm vậy Mẫu thân sẽ không vui đâu.
Dialogue: 0,0:05:46.75,0:05:55.56,Default,,0000,0000,0000,,Quốc vương Bastarauge là một kẻ đam mê quyền lực, ông ta muốn thôn tính Asteria về tay mình.
Dialogue: 0,0:05:55.63,0:06:04.73,Default,,0000,0000,0000,,Bastarauge cho quân càn quét khắp các vùng đất, vây hãm quốc vương tại kinh đô, vậy nên chúng tôi chỉ còn một lựa chọn duy nhất.
Dialogue: 0,0:06:04.80,0:06:10.94,Default,,0000,0000,0000,,Mọi người đều nghĩ Bastarauge đã chủ động lui quân.
Dialogue: 0,0:06:11.01,0:06:16.68,Default,,0000,0000,0000,,Nhưng thật ra, là một quốc gia nhỏ bé, chúng tôi đã buộc phải tuân theo mọi yêu sách của ông ta.
Dialogue: 0,0:06:18.79,0:06:23.15,Default,,0000,0000,0000,,Đổi lại cho việc lui quân, Bastarauge đã chiếm lấy nhiều đất đai.
Dialogue: 0,0:06:23.22,0:06:28.59,Default,,0000,0000,0000,,và hạn chế quân đội của vương quốc tôi... nhắm tiến tới thôn tính toàn vương quốc.
Dialogue: 0,0:06:30.10,0:06:32.09,Default,,0000,0000,0000,,Chưa dừng lại ở đó...
Dialogue: 0,0:06:33.67,0:06:38.07,Default,,0000,0000,0000,,Basterauge yêu cầu hai con tin là thành viên của Hoàng gia
Dialogue: 0,0:06:38.14,0:06:43.04,Default,,0000,0000,0000,,Hai người đó là: Hoàng hậu Rosaria và tôi, Hoàng tử Distol.
Dialogue: 0,0:06:50.72,0:06:54.31,Default,,0000,0000,0000,,Mẫu thân, Nii-sama. Con đã chuẩn bị rất nhiều hoa, để chúc cho hai người thượng lộ bình an.
Dialogue: 0,0:06:54.95,0:06:56.35,Default,,0000,0000,0000,,Cám ơn em, Qoona.
Dialogue: 0,0:06:57.39,0:06:59.48,Default,,0000,0000,0000,,Tại nơi đó, con sẽ gắng hết sức mình bảo vệ cho mẹ.
Dialogue: 0,0:07:00.26,0:07:01.73,Default,,0000,0000,0000,,Di, con...
Dialogue: 0,0:07:01.80,0:07:04.99,Default,,0000,0000,0000,,Mẫu thân, mẹ đã luôn yêu thương con dù chúng ta không phải ruột thịt.
Dialogue: 0,0:07:05.63,0:07:08.29,Default,,0000,0000,0000,,Giờ là lúc để con báo hiếu cho mẹ.
Dialogue: 0,0:07:08.87,0:07:12.43,Default,,0000,0000,0000,,Dù không phải ruột thịt, ta vẫn luôn coi con như con đẻ của mình.
Dialogue: 0,0:07:13.47,0:07:14.53,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:07:15.34,0:07:17.28,Default,,0000,0000,0000,,Đừng quên Qoona đấy nhé.
Dialogue: 0,0:07:18.04,0:07:20.67,Default,,0000,0000,0000,,Con bé yêu thương con hơn bất kì ai.
Dialogue: 0,0:07:20.75,0:07:22.91,Default,,0000,0000,0000,,Chắc chắn rồi, Mẫu thân.
Dialogue: 0,0:07:22.98,0:07:24.21,Default,,0000,0000,0000,,Con hứa.
Dialogue: 0,0:07:24.62,0:07:26.74,Default,,0000,0000,0000,,Hứa thì phải giữ lời đó, Di.
Dialogue: 0,0:07:27.59,0:07:30.89,Default,,0000,0000,0000,,Vài ngày sau khi chúng tôi tới Bastarauge.
Dialogue: 0,0:07:32.26,0:07:33.99,Default,,0000,0000,0000,,Mẫu thân tôi đột ngột qua đời ư!?
Dialogue: 0,0:07:34.46,0:07:35.49,Default,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:07:35.56,0:07:37.12,Default,,0000,0000,0000,,Bà qua đời ra sao!?
Dialogue: 0,0:07:37.56,0:07:42.09,Default,,0000,0000,0000,,Ta thật lòng chia buồn về chuyện của mẫu thân của cậu. Chúng ta sẽ tổ chức tang lễ trọng thể cho bà.
Dialogue: 0,0:07:42.87,0:07:45.17,Default,,0000,0000,0000,,Tại sao, chuyện gì đã xảy ra!?
Dialogue: 0,0:07:45.24,0:07:46.33,Default,,0000,0000,0000,,Bà ấy bị cảm nặng.
Dialogue: 0,0:07:46.41,0:07:50.17,Default,,0000,0000,0000,,Không thể nào! Ngày hôm qua mẫu thân tôi còn rất khỏe mạnh mà!
Dialogue: 0,0:07:50.94,0:07:54.11,Default,,0000,0000,0000,,Ta vừa nói cho cậu biết nguyên do rồi, không còn gì khác nữa đâu!
Dialogue: 0,0:07:54.18,0:07:57.05,Default,,0000,0000,0000,,Ông đã làm gì mẫu thân tôi!?
Dialogue: 0,0:07:57.12,0:07:59.41,Default,,0000,0000,0000,,Distol, cậu biết mình đang nói chuyện với ai không hả!
Dialogue: 0,0:07:59.65,0:08:02.59,Default,,0000,0000,0000,,Các người còn đứng đó làm gì? Mau đưa hoàng tử về nghỉ ngơi!
Dialogue: 0,0:08:03.12,0:08:05.22,Default,,0000,0000,0000,,Khốn kiếp, buông ta ra!
Dialogue: 0,0:08:05.29,0:08:10.06,Default,,0000,0000,0000,,Distol, ta cấm cậu rời khỏi nơi ở trong vòng 1 tháng. Hãy tĩnh tâm lại.
Dialogue: 0,0:08:13.20,0:08:16.36,Default,,0000,0000,0000,,Kể từ đó, tôi ngày đêm luyện kiếm.
Dialogue: 0,0:08:16.84,0:08:20.00,Default,,0000,0000,0000,,Tôi luôn nguyền rủa sự yếu đuối của mình. Tôi phải mạnh mẽ hơn.
Dialogue: 0,0:08:21.07,0:08:24.81,Default,,0000,0000,0000,,Tôi không muốn mất đi bất kì người thân nào nữa.
Dialogue: 0,0:08:24.88,0:08:27.87,Default,,0000,0000,0000,,Phải mạnh hơn tất cả...
Dialogue: 0,0:08:32.12,0:08:33.81,Default,,0000,0000,0000,,Công chúa Eleasteria.
Dialogue: 0,0:08:34.52,0:08:37.12,Default,,0000,0000,0000,,Ta đã bảo anh bao lần rồi, cứ gọi ta là Elena thôi.
Dialogue: 0,0:08:37.19,0:08:39.32,Default,,0000,0000,0000,,Tôi xin lỗi, Công chúa Elena.
Dialogue: 0,0:08:39.39,0:08:40.72,Default,,0000,0000,0000,,Xin hãy tha thứ cho tôi.
Dialogue: 0,0:08:41.76,0:08:44.79,Default,,0000,0000,0000,,Biết vậy là được rồi, không cần xin lỗi ta đâu.
Dialogue: 0,0:08:44.86,0:08:48.16,Default,,0000,0000,0000,,Uhm, nàng tìm tôi có chuyện gì, công chúa Elena?
Dialogue: 0,0:08:49.00,0:08:53.10,Default,,0000,0000,0000,,Ta chỉ tình cờ đi dạo qua nơi này thôi, ta không tìm anh.
Dialogue: 0,0:08:53.67,0:08:55.07,Default,,0000,0000,0000,,Công chúa!
Dialogue: 0,0:08:55.38,0:08:57.90,Default,,0000,0000,0000,,Oh, là các cô sao. Đừng làm ta giật mình như vậy chứ.
Dialogue: 0,0:08:57.98,0:08:59.44,Default,,0000,0000,0000,,Thứ lỗi cho thuộc hạ, Công chúa Elena.
Dialogue: 0,0:08:59.51,0:09:01.34,Default,,0000,0000,0000,,Thuộc hạ thật quá vô lễ.
Dialogue: 0,0:09:02.08,0:09:06.42,Default,,0000,0000,0000,,Có điều, Công chúa. Xin người đừng tới gần anh ta.
Dialogue: 0,0:09:08.62,0:09:11.82,Default,,0000,0000,0000,,Cecil, ngươi lấy quyền gì cấm đoán ta nói chuyện với người khác hả?!
Dialogue: 0,0:09:11.89,0:09:12.82,Default,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:09:15.13,0:09:17.43,Default,,0000,0000,0000,,Distol, mong anh bỏ quá cho sự vô lễ của bọn họ.
Dialogue: 0,0:09:17.50,0:09:18.40,Default,,0000,0000,0000,,Tôi không để bụng đâu.
Dialogue: 0,0:09:19.53,0:09:21.26,Default,,0000,0000,0000,,Thôi nào. Bỏ qua đi, Công chúa
Dialogue: 0,0:09:21.33,0:09:22.53,Default,,0000,0000,0000,,Quốc vương triệu kiến Người.
Dialogue: 0,0:09:23.44,0:09:24.20,Default,,0000,0000,0000,,Cha ta ư?
Dialogue: 0,0:09:24.84,0:09:27.20,Default,,0000,0000,0000,,Vâng, quốc vương muốn công chúa nhanh tới phòng họp.
Dialogue: 0,0:09:27.61,0:09:28.51,Default,,0000,0000,0000,,Được thôi.
Dialogue: 0,0:09:30.61,0:09:32.41,Default,,0000,0000,0000,,Mau lên nào, Công chúa.
Dialogue: 0,0:09:33.18,0:09:34.61,Default,,0000,0000,0000,,Ờ, ngươi nói phải.
Dialogue: 0,0:09:35.28,0:09:38.58,Default,,0000,0000,0000,,Elena luôn mất tự chủ khi ở cạnh mình.
Dialogue: 0,0:09:39.25,0:09:41.58,Default,,0000,0000,0000,,Hẳn đó là lí do khiến Đại tướng Cecil nhìn mình với con mắt thiếu thiện cảm đến vậy?
Dialogue: 0,0:09:42.36,0:09:45.45,Default,,0000,0000,0000,,Cận vệ còn lại, Yuno, là một nàng Elf đến từ nơi khác.
Dialogue: 0,0:09:46.13,0:09:48.86,Default,,0000,0000,0000,,Chứng tỏ nàng ta là một pháp sư rất cao tay.
Dialogue: 0,0:09:49.26,0:09:51.25,Default,,0000,0000,0000,,Một kẻ khó đối phó.
Dialogue: 0,0:09:52.90,0:09:56.36,Default,,0000,0000,0000,,Tôi vẫn miệt mài luyện kiếm từ ngày này qua ngày khác.
Dialogue: 0,0:09:57.30,0:10:01.87,Default,,0000,0000,0000,,Điều khiến tôi lo lắng nhất bây giờ là Qoona, em gái tôi.
Dialogue: 0,0:10:02.44,0:10:05.04,Default,,0000,0000,0000,,Cho tới một ngày, tôi nhận được tin sét đánh.
Dialogue: 0,0:10:07.55,0:10:09.45,Default,,0000,0000,0000,,Kính chào nữ hoàng Elizabeth.
Dialogue: 0,0:10:09.52,0:10:12.08,Default,,0000,0000,0000,,Cậu luyện tập siêng năng thật.
Dialogue: 0,0:10:12.15,0:10:15.85,Default,,0000,0000,0000,,Tôi chỉ là một kiếm thủ xoàng xĩnh.
Dialogue: 0,0:10:16.79,0:10:23.70,Default,,0000,0000,0000,,Ta đã suy nghĩ rất nhiều về chuyện này, ta nghĩ mình nên cho cậu biết.
Dialogue: 0,0:10:23.76,0:10:24.66,Default,,0000,0000,0000,,Chuyện gì kia?
Dialogue: 0,0:10:24.73,0:10:28.39,Default,,0000,0000,0000,,Quốc vương sẽ cho triệu kiến cậu sớm thôi.
Dialogue: 0,0:10:28.47,0:10:32.23,Default,,0000,0000,0000,,Dù cậu làm gì, cũng đừng tỏ ra kích động.
Dialogue: 0,0:10:32.61,0:10:33.94,Default,,0000,0000,0000,,Chuyện liên quan đến tôi ư?
Dialogue: 0,0:10:34.01,0:10:37.67,Default,,0000,0000,0000,,Mẫu hậu, Người không nên tới những nơi như thế này.
Dialogue: 0,0:10:37.74,0:10:38.80,Default,,0000,0000,0000,,Xin Người hãy mau quay về đi.
Dialogue: 0,0:10:40.68,0:10:42.88,Default,,0000,0000,0000,,Di, cha ta muốn gặp mặt anh.
Dialogue: 0,0:10:42.95,0:10:44.78,Default,,0000,0000,0000,,Hãy tới cung điện của Người.
Dialogue: 0,0:10:44.85,0:10:45.61,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:10:46.09,0:10:49.71,Default,,0000,0000,0000,,Thái độ của Elena với Hoàng hậu lạ thật?
Dialogue: 0,0:10:49.79,0:10:53.78,Default,,0000,0000,0000,,Dù không phải mẹ con ruột thịt, cũng đâu cần phải phũ phàng như vậy?
Dialogue: 0,0:10:55.03,0:10:56.59,Default,,0000,0000,0000,,Distol, tới theo yêu cầu của ngài.
Dialogue: 0,0:11:00.17,0:11:05.93,Default,,0000,0000,0000,,Ta vừa nhận được tin: Phong ấn đang dần suy yếu.
Dialogue: 0,0:11:06.01,0:11:07.34,Default,,0000,0000,0000,,Phong ấn?
Dialogue: 0,0:11:07.41,0:11:13.14,Default,,0000,0000,0000,,Để phòng ngừa, Giáo hội đã yêu cầu một Thánh nữ
Dialogue: 0,0:11:13.21,0:11:18.41,Default,,0000,0000,0000,,Được lựa chọn từ vương quốc của cậu- Asteria.
Dialogue: 0,0:11:19.09,0:11:20.02,Default,,0000,0000,0000,,Đừng nói là...!?
Dialogue: 0,0:11:20.09,0:11:22.99,Default,,0000,0000,0000,,Đúng vậy, người được chọn chính là
Dialogue: 0,0:11:23.06,0:11:26.92,Default,,0000,0000,0000,,Công chúa Qoonashirea Elasteria.
Dialogue: 0,0:11:26.99,0:11:29.29,Default,,0000,0000,0000,,Qoona!? Qoonashirea được chọn làm Thánh nữ sao!?
Dialogue: 0,0:11:29.36,0:11:34.73,Default,,0000,0000,0000,,Quốc vương Asteria đã chấp thuận, lễ phong Thánh sẽ được tiến hành sau vài ngày tới.
Dialogue: 0,0:11:35.80,0:11:37.43,Default,,0000,0000,0000,,Không thể nào!
Dialogue: 0,0:11:37.50,0:11:40.03,Default,,0000,0000,0000,,Cậu nên tự hào về điều đó.
Dialogue: 0,0:11:41.31,0:11:43.07,Default,,0000,0000,0000,,Đó là tất cả những gì ta muốn nói, cậu về được rồi.
Dialogue: 0,0:11:44.08,0:11:49.21,Default,,0000,0000,0000,,Nhiều năm trước đây, thế giới này chìm trong cơn ác mộng do Ma Vương tạo nên.
Dialogue: 0,0:11:49.28,0:11:55.24,Default,,0000,0000,0000,,Quyền năng của hắn sánh ngang các vị thần, cùng với lũ thuộc hạ đông đảo.
Dialogue: 0,0:11:55.32,0:11:58.81,Default,,0000,0000,0000,,Đó là thời kì đen tối nhất trong lịch sử loài người.
Dialogue: 0,0:11:58.89,0:12:02.99,Default,,0000,0000,0000,,Tuy nhiên, 1000 năm trước đây, Ma Vương đã bị phong ấn.
Dialogue: 0,0:12:03.06,0:12:12.33,Default,,0000,0000,0000,,Để làm được điều này, cái giá phải trả vô cùng lớn.
Dialogue: 0,0:12:12.41,0:12:15.47,Default,,0000,0000,0000,,Đó là sự hiến dâng.
Dialogue: 0,0:12:15.54,0:12:20.77,Default,,0000,0000,0000,,Chiến tranh nổ ra liên miên, con người thù ghét lẫn nhau, phong ấn cũng vì thế mà suy yếu.
Dialogue: 0,0:12:20.85,0:12:29.62,Default,,0000,0000,0000,,Để khôi phục Phong ấn, Giáo hội yêu cầu hiến tế.
Dialogue: 0,0:12:29.69,0:12:37.19,Default,,0000,0000,0000,,Vật hiến tế là một trinh nữ, có trái tim thánh thiện.
Dialogue: 0,0:12:38.87,0:12:47.53,Default,,0000,0000,0000,,Người được chọn sẽ bị giam cầm ở Tháp Tin lành trong vòng một năm.
Dialogue: 0,0:12:49.18,0:12:53.87,Default,,0000,0000,0000,,Họ hiến dâng sự sống của mình để khôi phục phong ấn.
Dialogue: 0,0:12:53.95,0:12:59.78,Default,,0000,0000,0000,,Một khi phong ấn khôi phục hoàn toàn, cũng đồng nghĩa với cái chết của vật hiến tế.
Dialogue: 0,0:12:59.85,0:13:06.12,Default,,0000,0000,0000,,Người thiếu nữ đó sẽ được muôn dân cầu phúc và ngợi ca hành động hi sinh cao cả của mình.
Dialogue: 0,0:13:06.19,0:13:09.13,Default,,0000,0000,0000,,Vậy nên họ được người đời coi như các Thánh nữ.
Dialogue: 0,0:13:10.40,0:13:14.33,Default,,0000,0000,0000,,Một khi đã bước chân vào Tháp Tin lành, một phong ấn ma thuật sẽ được dựng lên xung quanh.
Dialogue: 0,0:13:14.40,0:13:18.36,Default,,0000,0000,0000,,Khi chuyện đó bắt đầu, nội bất xuất, ngoại bất nhập.
Dialogue: 0,0:13:19.37,0:13:24.83,Default,,0000,0000,0000,,Qoona được chọn làm Thánh nữ kế tiếp.\NMột lần nữa, mình lại không thể bảo vệ được người thân yêu nhất!
Dialogue: 0,0:13:24.91,0:13:26.38,Default,,0000,0000,0000,,Là do mình quá yếu hèn ư?
Dialogue: 0,0:13:26.45,0:13:29.41,Default,,0000,0000,0000,,Khốn kiếp! Sao lại là Qoona chứ!?
Dialogue: 0,0:13:29.48,0:13:31.01,Default,,0000,0000,0000,,Không đúng!
Dialogue: 0,0:13:32.05,0:13:34.58,Default,,0000,0000,0000,,Luyện kiếm ngày đêm thì có ích gì chứ!?
Dialogue: 0,0:13:34.65,0:13:36.95,Default,,0000,0000,0000,,Không lẽ mọi nỗ lực của ta đều trở nên vô nghĩa sao!?
Dialogue: 0,0:13:44.53,0:13:45.76,Default,,0000,0000,0000,,Chết này!
Dialogue: 0,0:13:46.73,0:13:51.47,Default,,0000,0000,0000,,Một con yêu tinh, tại sao lại... ? Không lẽ nào...!
Dialogue: 0,0:13:53.14,0:13:54.50,Default,,0000,0000,0000,,Ngài cừ lắm.
Dialogue: 0,0:14:02.72,0:14:05.28,Default,,0000,0000,0000,,Quả đúng như lời đồn đại, Hoang tử Distol.
Dialogue: 0,0:14:05.35,0:14:07.01,Default,,0000,0000,0000,,Rất hân hạnh được gặp ngài.
Dialogue: 0,0:14:07.59,0:14:08.85,Default,,0000,0000,0000,,Ngươi! Ngươi là ai?
Dialogue: 0,0:14:09.56,0:14:13.32,Default,,0000,0000,0000,,Xin ngài hãy cất kiếm đi. Ta không có ý định giao đấu với ngài.
Dialogue: 0,0:14:13.39,0:14:14.05,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:14:14.13,0:14:18.76,Default,,0000,0000,0000,,Ta là đồng minh của ngài. Ta tới để giúp đỡ ngài.
Dialogue: 0,0:14:19.80,0:14:21.79,Default,,0000,0000,0000,,Ngươi vừa nói gì, đồng minh ư?
Dialogue: 0,0:14:23.17,0:14:26.80,Default,,0000,0000,0000,,Xin lỗi, ta cần kiểm tra năng lực của ngài.
Dialogue: 0,0:14:26.87,0:14:29.81,Default,,0000,0000,0000,,Kiểm tra? Sao ngươi dám!
Dialogue: 0,0:14:30.54,0:14:32.57,Default,,0000,0000,0000,,Ta không thể... cử động?
Dialogue: 0,0:14:32.65,0:14:35.27,Default,,0000,0000,0000,,Như ta đã nói, xin ngài hãy bỏ gươm xuống.
Dialogue: 0,0:14:35.35,0:14:38.84,Default,,0000,0000,0000,,Ta chỉ muốn nói chuyện tay đôi cùng ngài.
Dialogue: 0,0:14:38.92,0:14:43.25,Default,,0000,0000,0000,,Đừng có nằm mơ, ta không có gì để nói với loại người như ngươi cả!
Dialogue: 0,0:14:43.86,0:14:47.85,Default,,0000,0000,0000,,Cho dù là chuyện về em gái của ngài, Công chúa Qoonashirea sao?
Dialogue: 0,0:14:48.49,0:14:51.09,Default,,0000,0000,0000,,Ngươi... vừa nói gì!?
Dialogue: 0,0:14:52.16,0:14:54.60,Default,,0000,0000,0000,,Ngài muốn cứu công chúa Qoonashirea chứ?
Dialogue: 0,0:14:55.17,0:14:56.40,Default,,0000,0000,0000,,...Được rồi.
Dialogue: 0,0:14:57.87,0:14:58.70,Default,,0000,0000,0000,,Nói đi...
Dialogue: 0,0:14:58.77,0:15:01.93,Default,,0000,0000,0000,,Có vẻ như ngài đã chịu lắng nghe ta nói.
Dialogue: 0,0:15:02.01,0:15:08.64,Default,,0000,0000,0000,,Như ngài đã biết, bất kì ai chấp nhận vào Tháp Tin lành thì đều một đi không trở lại.
Dialogue: 0,0:15:09.72,0:15:17.21,Default,,0000,0000,0000,,Ngay cả khi ngài cứu thoát em gái mình khỏi đó, cô ấy vẫn bị ma thuật đó khống chế, sinh mệnh bị rút ngắn là điều tất yếu.
Dialogue: 0,0:15:18.29,0:15:22.06,Default,,0000,0000,0000,,Đúng vậy! Dù là cách khác, ta vẫn không thể cứu nổi Qoona!
Dialogue: 0,0:15:23.03,0:15:26.76,Default,,0000,0000,0000,,Uhm, ta biết cách để vô hiệu hóa ma thuật đó.
Dialogue: 0,0:15:26.83,0:15:30.46,Default,,0000,0000,0000,,Để cứu mạng Qoona.
Dialogue: 0,0:15:31.57,0:15:34.10,Default,,0000,0000,0000,,Ta sẽ ban cho ngài sức mạnh.
Dialogue: 0,0:15:34.17,0:15:37.63,Default,,0000,0000,0000,,Sức mạnh và trí tuệ đủ để giúp ngài cứu Qoona.
Dialogue: 0,0:15:37.71,0:15:41.20,Default,,0000,0000,0000,,Sức mạnh và trí tuệ đủ để cứu Qoona... tốt hơn cả là ngươi không nên lừa ta.
Dialogue: 0,0:15:41.71,0:15:44.27,Default,,0000,0000,0000,,Tất nhiên, đổi lại ta cũng có vài điều kiện nho nhỏ.
Dialogue: 0,0:15:44.35,0:15:45.68,Default,,0000,0000,0000,,Điều kiện?
Dialogue: 0,0:15:47.45,0:15:50.68,Default,,0000,0000,0000,,Phải, ta muốn ngài thôn tính Bastarauge
Dialogue: 0,0:15:50.76,0:15:53.25,Default,,0000,0000,0000,,và lấy đi bảo vật hoàng gia.\NBảo vật?
Dialogue: 0,0:15:54.13,0:15:57.12,Default,,0000,0000,0000,,Hoang đường... nghe cứ như đùa vậy.
Dialogue: 0,0:15:58.53,0:16:04.09,Default,,0000,0000,0000,,Không ai trên đời này có thể làm được chuyện ngươi vừa yêu cầu.
Dialogue: 0,0:16:09.38,0:16:14.24,Default,,0000,0000,0000,,Chắc chắn ngài sẽ làm được... với sức mạnh và trí tuệ được trao.
Dialogue: 0,0:16:14.31,0:16:17.28,Default,,0000,0000,0000,,Vả lại, ngài có lí do để thôn tính Bastarauge.
Dialogue: 0,0:16:17.35,0:16:19.65,Default,,0000,0000,0000,,Ngươi vừa nói gì!? Nói lại ta nghe xem nào!
Dialogue: 0,0:16:20.85,0:16:24.48,Default,,0000,0000,0000,,Ta sẽ cho ngài biết sự thật về cái chết của Mẫu thân ngài.
Dialogue: 0,0:16:42.04,0:16:45.50,Default,,0000,0000,0000,,Bastarauge!!!!!
Dialogue: 0,0:16:47.81,0:16:49.28,Default,,0000,0000,0000,,Ngài nhận ra rồi chứ?
Dialogue: 0,0:16:49.85,0:16:52.98,Default,,0000,0000,0000,,Hắn ta làm mọi việc chỉ để chiếm được Hoàng hậu Rosaria.
Dialogue: 0,0:16:53.95,0:16:56.98,Default,,0000,0000,0000,,Được, ta sẽ làm bất kì điều gì ngươi muốn! Hãy cho ta sức mạnh!
Dialogue: 0,0:16:57.96,0:17:02.19,Default,,0000,0000,0000,,Kể cả khi điều đó khiến tương lai của ngài thay đổi hoàn toàn?
Dialogue: 0,0:17:02.86,0:17:08.60,Default,,0000,0000,0000,,Chỉ cần báo thù được cho Mẫu thân và cứu sống Qoona, ta sẵn sàng bán linh hồn mình cho quỷ dữ!
Dialogue: 0,0:17:10.67,0:17:11.64,Default,,0000,0000,0000,,Ra vậy...
Dialogue: 0,0:17:15.74,0:17:17.21,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:17:17.81,0:17:21.44,Default,,0000,0000,0000,,Không, không ngờ ngươi lại dễ thương hệt như giọng nói của mình vậy.
Dialogue: 0,0:17:21.91,0:17:26.54,Default,,0000,0000,0000,,Ai quan tâm chứ, thân thể và giọng nói này biết đâu chỉ là ảo ảnh.
Dialogue: 0,0:17:28.42,0:17:31.72,Default,,0000,0000,0000,,Chắc hẳn cô nói đúng... Tên cô là gì?
Dialogue: 0,0:17:32.16,0:17:34.82,Default,,0000,0000,0000,,Xin ngài thứ lỗi. Tên tôi là Eleanor.
Dialogue: 0,0:17:35.53,0:17:36.89,Default,,0000,0000,0000,,Rất vui được gặp ngài.
Dialogue: 0,0:17:37.73,0:17:39.03,Default,,0000,0000,0000,,Cô làm gì vậy?
Dialogue: 0,0:17:39.80,0:17:42.36,Default,,0000,0000,0000,,Một nghi lễ để kí giao ước.
Dialogue: 0,0:17:42.90,0:17:44.06,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:17:44.14,0:17:47.54,Default,,0000,0000,0000,,Vâng, làm ơn, Distol-sama.
Dialogue: 0,0:17:49.51,0:17:50.34,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:17:51.28,0:17:54.41,Default,,0000,0000,0000,,Distol-sama, xin phép ngài.
Dialogue: 0,0:17:57.22,0:18:02.12,Default,,0000,0000,0000,,Tuyệt quá... không thể diễn tả bằng lời. Cô đúng là một phù thủy.
Dialogue: 0,0:18:12.26,0:18:14.03,Default,,0000,0000,0000,,Làm ơn đừng dày vò tôi như vậy.
Dialogue: 0,0:18:20.51,0:18:25.94,Default,,0000,0000,0000,,Lần đầu tiên của cô sao... cô em phù thủy!
Dialogue: 0,0:18:28.05,0:18:31.78,Default,,0000,0000,0000,,Ngài là người đầu tiên... được phép...
Dialogue: 0,0:18:37.69,0:18:39.56,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:18:45.20,0:18:48.17,Default,,0000,0000,0000,,Nó đang vào sâu bên trong tôi... Distol-sama.
Dialogue: 0,0:18:50.47,0:18:53.46,Default,,0000,0000,0000,,Nữa đi, mạnh nữa lên...
Dialogue: 0,0:18:57.01,0:18:58.98,Default,,0000,0000,0000,,Distol-sama.
Dialogue: 0,0:19:01.55,0:19:03.91,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:19:04.95,0:19:07.82,Default,,0000,0000,0000,,Eleanor, ta sẽ khiến cô lên đỉnh!
Dialogue: 0,0:19:08.72,0:19:10.38,Default,,0000,0000,0000,,Như ngài mong muốn...
Dialogue: 0,0:19:17.10,0:19:21.43,Default,,0000,0000,0000,,Distol-sama... ngài ra hết lên người tôi rồi... nhiều quá.
Dialogue: 0,0:19:26.51,0:19:28.03,Default,,0000,0000,0000,,Hãy đón nhận sức mạnh mới của ngài...
Dialogue: 0,0:19:28.11,0:19:29.23,Default,,0000,0000,0000,,Qoona...
Dialogue: 0,0:19:31.71,0:19:33.54,Default,,0000,0000,0000,,Đây là sức mạnh mới... của ta sao?
Dialogue: 0,0:19:41.52,0:19:45.15,Default,,0000,0000,0000,,Với sức mạnh này, ta có thể phá tan cả khu rừng.
Dialogue: 0,0:19:45.22,0:19:51.22,Default,,0000,0000,0000,,Tuyệt thật... Bastarauge không sớm thì muộn cũng sẽ bị ta thôn tính thôi!
Dialogue: 0,0:19:51.80,0:19:54.10,Default,,0000,0000,0000,,Giao ước đã hoàn thành.
Dialogue: 0,0:19:54.17,0:19:56.07,Default,,0000,0000,0000,,Ngài không thể quay lại nữa.
Dialogue: 0,0:19:56.17,0:19:59.93,Default,,0000,0000,0000,,Phải, ta biết. Sức mạnh này là dành cho Qoona!
Dialogue: 0,0:20:00.01,0:20:03.87,Default,,0000,0000,0000,,Chúng ta bắt đầu luôn chứ, Chủ nhân?
Dialogue: 0,0:20:04.61,0:20:06.34,Default,,0000,0000,0000,,Mọi thứ đã thay đổi kể từ ngày đó.
Dialogue: 0,0:20:10.15,0:20:12.01,Default,,0000,0000,0000,,Chuyện gì!? Chuyện gì vừa xảy ra vậy?
Dialogue: 0,0:20:12.08,0:20:14.11,Default,,0000,0000,0000,,Hoàng tử Distol đang tấn công vào đây!
Dialogue: 0,0:20:14.19,0:20:16.85,Default,,0000,0000,0000,,Tiến vào đây? Hắn ta điên rồi ư!?
Dialogue: 0,0:20:16.92,0:20:21.62,Default,,0000,0000,0000,,Quân lính bảo vệ phía trước đã bị giết sạch, hoàng tử đang tiến vào trong.
Dialogue: 0,0:20:21.69,0:20:24.36,Default,,0000,0000,0000,,Hắn thật to gan.
Dialogue: 0,0:20:24.43,0:20:26.16,Default,,0000,0000,0000,,Đại tướng Cecil lại không có ở đây...
Dialogue: 0,0:20:26.27,0:20:26.96,Default,,0000,0000,0000,,Bao nhiêu người?
Dialogue: 0,0:20:27.03,0:20:27.46,Default,,0000,0000,0000,,Chỉ có một thôi.
Dialogue: 0,0:20:27.53,0:20:28.63,Default,,0000,0000,0000,,C, Cái gì!?
Dialogue: 0,0:20:30.67,0:20:32.86,Default,,0000,0000,0000,,Muốn sống thì mau tránh đường.
Dialogue: 0,0:20:33.84,0:20:36.10,Default,,0000,0000,0000,,Ngăn hắn lại!
Dialogue: 0,0:20:36.18,0:20:37.16,Default,,0000,0000,0000,,Lũ ngu xuẩn...
Dialogue: 0,0:20:38.48,0:20:39.31,Default,,0000,0000,0000,,Distol!?
Dialogue: 0,0:20:39.71,0:20:41.20,Default,,0000,0000,0000,,Nàng đang làm gì vậy, Công chúa Eleasteria?
Dialogue: 0,0:20:41.28,0:20:43.27,Default,,0000,0000,0000,,Xin lỗi vì ta đã vào mà không gõ cưả.
Dialogue: 0,0:20:44.72,0:20:48.55,Default,,0000,0000,0000,,Bên ngoài ầm ĩ vậy mà nàng vẫn ở nguyên tại đây, khiến ta vô cùng bất ngờ đó.
Dialogue: 0,0:20:49.32,0:20:51.92,Default,,0000,0000,0000,,Tại sao ngươi lại làm vậy, Distol?
Dialogue: 0,0:20:51.99,0:20:53.69,Default,,0000,0000,0000,,Ta không có nghĩa vụ phải trả lời nàng.
Dialogue: 0,0:21:01.17,0:21:05.00,Default,,0000,0000,0000,,Đây là cái giá... của sức mạnh sao?
Dialogue: 0,0:21:05.07,0:21:05.87,Default,,0000,0000,0000,,Chủ nhân?
Dialogue: 0,0:21:05.94,0:21:07.43,Default,,0000,0000,0000,,Eleanor, chuyện gì vậy?
Dialogue: 0,0:21:07.87,0:21:11.83,Default,,0000,0000,0000,,Quân đội đang kéo tới đấy, ngài phải nhanh lên mới được.
Dialogue: 0,0:21:11.91,0:21:13.90,Default,,0000,0000,0000,,Thôi được, ngựa đã sẵn sàng.
Dialogue: 0,0:21:19.35,0:21:22.79,Default,,0000,0000,0000,,Eleanor, chăm sóc Elena giúp ta. Hãy tới Asteria.
Dialogue: 0,0:21:22.86,0:21:24.12,Default,,0000,0000,0000,,Còn ngài?
Dialogue: 0,0:21:24.19,0:21:25.78,Default,,0000,0000,0000,,Cô phải biết chứ.
Dialogue: 0,0:21:33.60,0:21:35.29,Default,,0000,0000,0000,,Vâng, ai vậy?
Dialogue: 0,0:21:35.40,0:21:36.37,Default,,0000,0000,0000,,Là anh đây, Distol.
Dialogue: 0,0:21:40.14,0:21:44.10,Default,,0000,0000,0000,,Hoàng tử Distol tới để giải cứu Công chúa Qoonashirea Eleasteria xinh đẹp đây.
Dialogue: 0,0:21:44.18,0:21:46.14,Default,,0000,0000,0000,,Nii-sama! Nii-sama!!
Dialogue: 0,0:21:46.21,0:21:49.15,Default,,0000,0000,0000,,Anh tới rồi đây, Qoona, xin lỗi vì đã khiến em phải đợi lâu.
Dialogue: 0,0:21:49.58,0:21:52.02,Default,,0000,0000,0000,,Em nhớ anh lắm.
Dialogue: 0,0:21:52.08,0:21:53.28,Default,,0000,0000,0000,,Anh cũng vậy.
Dialogue: 0,0:21:54.09,0:21:59.82,Default,,0000,0000,0000,,Nhưng em đã được chọn làm Thánh nữ. Em phải bảo vệ mọi người.
Dialogue: 0,0:21:59.89,0:22:03.16,Default,,0000,0000,0000,,Em sẽ không còn được gặp lại anh nữa.
Dialogue: 0,0:22:03.23,0:22:05.46,Default,,0000,0000,0000,,Về chuyện đó, Qoona, nghe kĩ những gì anh nói đây.
Dialogue: 0,0:22:05.53,0:22:07.52,Default,,0000,0000,0000,,Anh sẽ giải thoát cho em, Qoona.
Dialogue: 0,0:22:08.27,0:22:12.03,Default,,0000,0000,0000,,Nhưng việc đó sẽ khiến thế giới này chìm trong hỗn loạn.
Dialogue: 0,0:22:12.10,0:22:15.13,Default,,0000,0000,0000,,Hãy lắng nghe thật kĩ những gì anh nói, không còn nhiều thời gian đâu.
Dialogue: 0,0:22:15.21,0:22:17.94,Default,,0000,0000,0000,,Để cứu em, anh cần phải làm một số chuyện.
Dialogue: 0,0:22:18.01,0:22:19.34,Default,,0000,0000,0000,,Nii-sama...
Dialogue: 0,0:22:19.98,0:22:26.98,Default,,0000,0000,0000,,Qoona, hứa với anh em sẽ sống thật tốt đi. Chắc chắn anh sẽ giải thoát cho em.
Dialogue: 0,0:22:27.05,0:22:29.28,Default,,0000,0000,0000,,Nii-sama... Nii-sama...
Dialogue: 0,0:22:29.36,0:22:31.29,Default,,0000,0000,0000,,Vậy nhé? Hãy đợi anh.
Dialogue: 0,0:22:33.69,0:22:34.99,Default,,0000,0000,0000,,Nii-sama!!!
Dialogue: 0,0:22:35.06,0:22:38.12,Default,,0000,0000,0000,,Để cứu được Qoona, ngài cần hoàn thành hai việc.
Dialogue: 0,0:22:39.63,0:22:43.73,Default,,0000,0000,0000,,Việc thứ nhất: đánh cắp báu vật từ lâu đài của Bastarauge.
Dialogue: 0,0:22:44.67,0:22:50.34,Default,,0000,0000,0000,,Việc còn lại là phá hoại sự trong trắng của Elaine để làm suy yếu phong ấn.
Dialogue: 0,0:22:51.11,0:22:55.41,Default,,0000,0000,0000,,Hiểu rồi, ta phải làm suy yếu phong ấn bên trong Elena!
Dialogue: 0,0:23:05.12,0:23:07.15,Default,,0000,0000,0000,,Chuyện gì vậy, nàng thấy thích thú rồi ư?
Dialogue: 0,0:23:07.23,0:23:08.92,Default,,0000,0000,0000,,Không đời nào!
Dialogue: 0,0:23:08.99,0:23:12.76,Default,,0000,0000,0000,,Thật mất hứng. Ta sẽ đâm vào sâu hơn nữa.
Dialogue: 0,0:23:13.60,0:23:14.76,Default,,0000,0000,0000,,Ngươi định làm gì!?
Dialogue: 0,0:23:29.11,0:23:32.74,Default,,0000,0000,0000,,Có vẻ như nàng thích điều này, Công chúa Elena.
Dialogue: 0,0:23:35.75,0:23:37.98,Default,,0000,0000,0000,,Câm miệng, không đời nào...
Dialogue: 0,0:23:38.42,0:23:42.33,Default,,0000,0000,0000,,Nàng vẫn còn coi mình là Công chúa ư? Đồ dâm đãng!
Dialogue: 0,0:23:45.03,0:23:46.52,Default,,0000,0000,0000,,Thế này thì sao hả!
Dialogue: 0,0:23:54.07,0:23:57.13,Default,,0000,0000,0000,,Khít quá... không ngờ lại tuyệt đến vậy?
Dialogue: 0,0:23:58.08,0:24:01.20,Default,,0000,0000,0000,,Tại sao... cơ thể ta...!?
Dialogue: 0,0:24:05.95,0:24:07.04,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:24:13.06,0:24:15.25,Default,,0000,0000,0000,,Hi vọng là nàng thích.
Dialogue: 0,0:24:32.08,0:24:36.98,Default,,0000,0000,0000,,Cảm giác bị ra vào trong ngay lần đầu mất trinh thế nào hả?
Dialogue: 0,0:24:38.12,0:24:42.31,Default,,0000,0000,0000,,Trinh tiết của ta... bị cướp đi... như vậy sao...?
Dialogue: 0,0:24:45.32,0:24:48.78,Default,,0000,0000,0000,,Dù bị dày vò dã man, cô ta vẫn chưa hề suy sụp.
Dialogue: 0,0:24:50.16,0:24:58.63,Default,,0000,0000,0000,,Mình phải tiếp tục dày vò cô ta đến khi phẩm giá và đức hạnh trong cô ta hoàn toàn biến mất. Mình phải tha hóa cô ta bằng mọi giá.
Dialogue: 0,0:24:59.97,0:25:05.67,Default,,0000,0000,0000,,Mình sẽ làm tất cả để cứu Qoona, dù phải tạo ra bao nhiêu kẻ thù.
Dialogue: 0,0:25:05.81,0:25:09.18,Default,,0000,0000,0000,,Giống như... Mình mặc kệ!
Dialogue: 0,0:25:09.25,0:25:13.41,Default,,0000,0000,0000,,Dù cho chuyện này phải trả giá bằng sự diệt vong của toàn nhân loại!
Dialogue: 0,0:25:15.99,0:25:19.39,Default,,0000,0000,0000,,Còn tiếp...
