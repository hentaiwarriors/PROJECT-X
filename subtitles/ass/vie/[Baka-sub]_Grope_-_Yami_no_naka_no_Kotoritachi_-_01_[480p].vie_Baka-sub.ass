Dialogue: 0,0:00:13.34,0:00:15.72,Default1,,0000,0000,0000,,Hân hạnh được gặp anh. Em là Mirano.
Dialogue: 0,0:00:16.32,0:00:19.12,Default1,,0000,0000,0000,,Cuối cúng chúng ta có thể công khai quan hệ.
Dialogue: 0,0:00:19.53,0:00:24.19,Default1,,0000,0000,0000,,Từ bây giờ, mỗi ngày sẽ là một niềm vui.
Dialogue: 0,0:00:24.95,0:00:26.95,Default1,,0000,0000,0000,,Mong anh hãy chiếu cố đến em.
Dialogue: 0,0:00:28.37,0:00:33.59,Default1,,0000,0000,0000,,Mới đây, Nana-san bạn em, cô ấy rất xin đẹp.
Dialogue: 0,0:00:34.46,0:00:40.12,Default1,,0000,0000,0000,,Cô ấy và chồng đang rất gần gũi và hạnh phúc.
Dialogue: 0,0:00:41.07,0:00:44.19,Default1,,0000,0000,0000,,Hay tối thiểu, đó là điều mà em đã nghĩ.
Dialogue: 0,0:00:45.56,0:00:52.79,Default1,,0000,0000,0000,,Nhưng cô ấy đã ngoại tình với một người đàn ông khác...
Dialogue: 0,0:00:52.99,0:00:57.29,Default1,,0000,0000,0000,,Trên hết, người đàn ông đó là bạn của chồng cô ấy...
Dialogue: 0,0:00:57.49,0:01:00.36,Default1,,0000,0000,0000,,Ơ... Họ đã làm những chuyện đáng lí ra không nên làm...
Dialogue: 0,0:01:00.53,0:01:02.86,Default1,,0000,0000,0000,,N-Nhưng cô ấy nói rằng cô ấy không ngu suẩn...
Dialogue: 0,0:01:03.43,0:01:06.03,Default1,,0000,0000,0000,,Cô ấy được người đàn ông ấy ôm ấp một cách mạnh mẽ...
Dialogue: 0,0:01:06.37,0:01:12.05,Default1,,0000,0000,0000,,Và rồi... C-Cô ấy không thể cưỡng lại được dương vật mạnh mẽ của người đàn ông đó...
Dialogue: 0,0:01:12.05,0:01:12.09,Default1,,0000,0000,0000,,Và rồi... C-Cô ấy không thể cưỡng lại được dương vật mạnh mẽ của người đàn ông đó...
Dialogue: 0,0:01:12.90,0:01:17.53,Default1,,0000,0000,0000,,Em hỏi cô ấy đã cảm thấy như thế nào...
Dialogue: 0,0:01:17.98,0:01:24.82,Default1,,0000,0000,0000,,Và cô ấy đã nói, "A, nó thật tuyệt vời..."
Dialogue: 0,0:01:25.96,0:01:27.02,Default1,,0000,0000,0000,,Em... {\alpha&HFF&}đang phải chịu đựng sự cô đơn...
Dialogue: 0,0:01:27.02,0:01:29.82,Default1,,0000,0000,0000,,Em... đang phải chịu đựng sự cô đơn...
Dialogue: 0,0:01:30.99,0:01:32.97,Default1,,0000,0000,0000,,Chồng em luôn đi làm về trễ...
Dialogue: 0,0:01:33.89,0:01:37.87,Default1,,0000,0000,0000,,Thế nên, để lấp đầy sự cô đơn đó, em...
Dialogue: 0,0:01:40.99,0:01:42.52,Default1,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:01:42.52,0:01:44.36,Default1,,0000,0000,0000,,Thật là ngượng nếu tiếp tục thế này.
Dialogue: 0,0:01:45.03,0:01:46.10,Default1,,0000,0000,0000,,N-Nhưng...
Dialogue: 0,0:01:47.23,0:01:47.81,Default1,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:01:55.71,0:01:58.74,Default1,,0000,0000,0000,,Hãy làm cho Mirano ra đi!
Dialogue: 0,0:02:06.27,0:02:06.96,Default1,,0000,0000,0000,,Chào buổi sáng!
Dialogue: 0,0:02:09.56,0:02:10.75,Default1,,0000,0000,0000,,Hừm...
Dialogue: 0,0:02:11.47,0:02:14.04,Default1,,0000,0000,0000,,Khi một ai đó nói "Chào buổi sáng!", thì cậu phải đáp lại "Chào buổi sáng!".
Dialogue: 0,0:02:15.30,0:02:16.97,Default1,,0000,0000,0000,,Cậu đang làm gì đó, Fubuki?
Dialogue: 0,0:02:17.31,0:02:18.54,Default1,,0000,0000,0000,,Xin lỗi, Shinichi!
Dialogue: 0,0:02:19.47,0:02:23.08,Default1,,0000,0000,0000,,Tớ không thể từ chối yêu cầu của Momose-san được.
Dialogue: 0,0:02:23.92,0:02:28.42,Default1,,0000,0000,0000,,Dù tớ không hỏi, thì tớ cũng thừa biết cậu đang ở đâu.
Dialogue: 0,0:02:29.95,0:02:32.20,Default1,,0000,0000,0000,,Cậu nghĩ chúng ta đã quen biết nhau bao lâu rồi hả?
Dialogue: 0,0:02:34.90,0:02:37.72,Default1,,0000,0000,0000,,Dù hôm nay là ngày lễ, nhưng đừng có uể oải như vậy.
Dialogue: 0,0:02:37.93,0:02:39.74,Default1,,0000,0000,0000,,Í nhất, thì cũng hãy tập thể dục đi chứ.
Dialogue: 0,0:02:54.97,0:02:56.66,Default1,,0000,0000,0000,,Thật là sảng khoái khi đứng dưới ánh mặt trời.
Dialogue: 0,0:02:55.44,0:02:58.45,Sign/Note,,0000,0000,0000,,{\an8\pos(362,16)}{\bord7\blur7\shad0\c&HFFFFFF&\3c&H3C3C3C&}Một buổi sáng như mọi ngày
Dialogue: 0,0:03:01.36,0:03:02.20,Default1,,0000,0000,0000,,Hừm...
Dialogue: 0,0:03:03.94,0:03:05.36,Default1,,0000,0000,0000,,Vẻ mặt dửng dưng đó là sao...
Dialogue: 0,0:03:11.65,0:03:13.63,Default1,,0000,0000,0000,,Tên đó thật là biến thái!
Dialogue: 0,0:03:18.70,0:03:19.73,Default1,,0000,0000,0000,,Mà này...
Dialogue: 0,0:03:20.22,0:03:23.34,Default1,,0000,0000,0000,,Mình để lộ quần chíp như vậy là chuyện bình thường sao?!
Dialogue: 0,0:03:24.73,0:03:26.47,FlashBack,,0000,0000,0000,,Lúc nảy tớ lỡ nhìn thấy quần chíp của cậu.
Dialogue: 0,0:03:30.35,0:03:31.52,FlashBack,,0000,0000,0000,,Xin lỗi...
Dialogue: 0,0:03:31.88,0:03:33.80,FlashBack,,0000,0000,0000,,Nhưng tớ nghĩ mình rất may mắn...
Dialogue: 0,0:03:40.00,0:03:42.02,Default1,,0000,0000,0000,,Mình biết là cậu ta có để ý mà... {\alpha&HFF&}chắc chắn...
Dialogue: 0,0:03:42.02,0:03:42.74,Default1,,0000,0000,0000,,Mình biết là cậu ta có để ý mà... chắc chắn...
Dialogue: 0,0:03:43.16,0:03:44.03,Default1,,0000,0000,0000,,Đồ ngốc! {\alpha&HFF&}Khờ khạo!
Dialogue: 0,0:03:44.03,0:03:44.74,Default1,,0000,0000,0000,,Đồ ngốc! Khờ khạo!
Dialogue: 0,0:03:49.39,0:03:50.96,Default1,,0000,0000,0000,,Tối thiểu... {\alpha&HFF&}cũng phải thấy mình đã có chút thay đổi chứ...
Dialogue: 0,0:03:50.96,0:03:52.48,Default1,,0000,0000,0000,,Tối thiểu... cũng phải thấy mình đã có chút thay đổi chứ...
Dialogue: 0,0:03:53.37,0:03:56.38,Sign/Note,,0000,0000,0000,,{\an8\pos(368,57)}{\bord6\blur6\shad0\c&HFFFFFF&\3c&H434343&}Một khung cảnh bình thường
Dialogue: 0,0:03:59.02,0:04:00.61,Default1,,0000,0000,0000,,Cậu thật là đáng ghen tị...
Dialogue: 0,0:04:01.04,0:04:04.28,Default1,,0000,0000,0000,,Cậu đã có được "Madonna" của học viện Sakuragaoka.
Dialogue: 0,0:04:05.93,0:04:07.70,Default1,,0000,0000,0000,,Cô ấy đơn thuần chỉ giống như chị gái tớ thôi.
Dialogue: 0,0:04:08.90,0:04:10.14,Default1,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:04:10.77,0:04:14.16,Default1,,0000,0000,0000,,Cô ấy hạ gục bấy cứ ai tỏ tình với mình như một khẩu súng cao su ấy.
Dialogue: 0,0:04:14.86,0:04:17.01,Default1,,0000,0000,0000,,Nếu cậu muốn, thì cứ tự nhiên.
Dialogue: 0,0:04:20.62,0:04:24.53,Default1,,0000,0000,0000,,Không ai được phép nhìm trộm, hai tên biến thái!
Dialogue: 0,0:04:24.53,0:04:27.53,Sign/Note,,0000,0000,0000,,{\an8\pos(366,5)}{\bord5\blur5\shad0\c&HFFFFFF&\3c&H3C3C3C&}Tất cả chỉ còn là quá khứ xa vời
Dialogue: 0,0:04:25.96,0:04:26.99,Default1,,0000,0000,0000,,Shinichi...
Dialogue: 0,0:04:27.77,0:04:30.07,Default1,,0000,0000,0000,,Cậu vẫn chưa hiểu tại sao tớ lại được nhiều bạn nữ hâm mộ, đúng không?
Dialogue: 0,0:04:32.41,0:04:35.29,Default1,,0000,0000,0000,,Tớ thừa biết cách nhanh nhất là đưa cô ấy lên giường, phải không.
Dialogue: 0,0:04:36.27,0:04:38.65,Default1,,0000,0000,0000,,Chỉ cần để cô ấy cho tớ thế là.
Dialogue: 0,0:04:40.00,0:04:40.73,Default1,,0000,0000,0000,,Ừ, ừ...
Dialogue: 0,0:04:59.15,0:05:01.40,Credit,,0000,0000,0000,,{\fad(1000,0)\fnUVN Ai Cap Nang\pos(441,453)}Baka-sub trân trọng giới thiệu
Dialogue: 0,0:05:20.96,0:05:22.21,Default1,,0000,0000,0000,,Nữa... {\alpha&HFF&}sờ mó tớ nhiều hơn nữa...
Dialogue: 0,0:05:22.21,0:05:23.71,Default1,,0000,0000,0000,,Nữa... sờ mó tớ nhiều hơn nữa...
Dialogue: 0,0:05:24.11,0:05:27.83,Default1,,0000,0000,0000,,Tớ là... của cậu... vì vậy...
Dialogue: 0,0:05:28.68,0:05:31.03,Default1,,0000,0000,0000,,Vì vậy, sờ mó tớ nhiều hơn nữa...
Dialogue: 0,0:05:41.27,0:05:44.48,Default1,,0000,0000,0000,,Tớ cũng muốn làm cho cậu được sung sướng...
Dialogue: 0,0:05:50.84,0:05:51.97,Default1,,0000,0000,0000,,Không được! {\alpha&HFF&}Chỗ đó xấu hổ lắm...
Dialogue: 0,0:05:51.97,0:05:52.55,Default1,,0000,0000,0000,,Không được! {\alpha&HFF&}Chỗ đó xấu hổ lắm...
Dialogue: 0,0:05:52.55,0:05:53.86,Default1,,0000,0000,0000,,Không được! Chỗ đó xấu hổ lắm...
Dialogue: 0,0:05:56.75,0:05:57.69,Default1,,0000,0000,0000,,Đồ dâm đãng!
Dialogue: 0,0:05:59.14,0:06:01.92,Default1,,0000,0000,0000,,Fubuki, không phải cậu mới là người dâm đãng sao?
Dialogue: 0,0:06:27.73,0:06:29.78,Default1,,0000,0000,0000,,Nó đã như vầy...
Dialogue: 0,0:06:30.71,0:06:32.51,Default1,,0000,0000,0000,,Chỉ có cậu làm tớ...
Dialogue: 0,0:06:35.21,0:06:38.30,Default1,,0000,0000,0000,,Chỉ nhìn nó thôi mà tớ đã trở nên dâm đãng...
Dialogue: 0,0:06:39.62,0:06:40.85,Default1,,0000,0000,0000,,Ngon lắm...
Dialogue: 0,0:06:44.05,0:06:46.30,Default1,,0000,0000,0000,,Nhanh lên, tớ muốn hòa làm một với cậu...
Dialogue: 0,0:06:59.51,0:07:02.33,Default1,,0000,0000,0000,,Nếu đau, chúng ta có thể dừng lại, Fubuki.
Dialogue: 0,0:07:04.14,0:07:05.45,Default1,,0000,0000,0000,,Tớ không muốn thế!
Dialogue: 0,0:07:06.16,0:07:08.74,Default1,,0000,0000,0000,,Bởi vì tớ vui lắm.
Dialogue: 0,0:07:09.52,0:07:13.39,Default1,,0000,0000,0000,,Dù nó đau, nhưng nó đau vì hòa làm một với cậu.
Dialogue: 0,0:07:13.93,0:07:16.74,Default1,,0000,0000,0000,,Tớ muốn cảm nhận cậu nhiều hơn nữa!
Dialogue: 0,0:07:29.14,0:07:31.70,Default1,,0000,0000,0000,,Thằng nhỏ của cậu đang ở bên trong tớ!
Dialogue: 0,0:07:33.17,0:07:35.93,Default1,,0000,0000,0000,,Nó đâm thật sâu...{\alpha&HFF&} bên trong tớ...
Dialogue: 0,0:07:35.93,0:07:37.04,Default1,,0000,0000,0000,,Nó đâm thật sâu... bên trong tớ...
Dialogue: 0,0:07:37.38,0:07:39.24,Default1,,0000,0000,0000,,Sướng quá!
Dialogue: 0,0:07:42.78,0:07:43.23,Default1,,0000,0000,0000,,Nữa?!
Dialogue: 0,0:07:48.30,0:07:49.07,Default1,,0000,0000,0000,,Sướng quá! {\alpha&HFF&}Sướng quá!!
Dialogue: 0,0:07:49.07,0:07:49.72,Default1,,0000,0000,0000,,Sướng quá! Sướng quá!!
Dialogue: 0,0:08:03.54,0:08:04.58,Default1,,0000,0000,0000,,Được rồi! {\alpha&HFF&}Cậu có thể làm bất cứ những gì cậu thích với cơ thể của tớ!
Dialogue: 0,0:08:04.58,0:08:07.15,Default1,,0000,0000,0000,,Được rồi! Cậu có thể làm bất cứ những gì cậu thích với cơ thể của tớ!
Dialogue: 0,0:08:09.46,0:08:10.29,Default1,,0000,0000,0000,,Nữa đi!{\alpha&HFF&} Nữa đi! Hãy ra bên trong tớ!!
Dialogue: 0,0:08:10.29,0:08:11.64,Default1,,0000,0000,0000,,Nữa đi! Nữa đi!{\alpha&HFF&} Hãy ra bên trong tớ!!
Dialogue: 0,0:08:11.64,0:08:12.79,Default1,,0000,0000,0000,,Nữa đi! Nữa đi! Hãy ra bên trong tớ!!
Dialogue: 0,0:08:12.99,0:08:14.01,Default1,,0000,0000,0000,,Tớ biết...{\alpha&HFF&} Tớ biết cậu sắp!!
Dialogue: 0,0:08:14.01,0:08:14.74,Default1,,0000,0000,0000,,Tớ biết... Tớ biết cậu sắp ra!!
Dialogue: 0,0:08:15.12,0:08:17.16,Default1,,0000,0000,0000,,Nóng như vậy! {\alpha&HFF&}Lớn như vậy! Nó sắp...
Dialogue: 0,0:08:17.16,0:08:19.39,Default1,,0000,0000,0000,,Nóng như vậy! Lớn như vậy! {\alpha&HFF&}Nó sắp...
Dialogue: 0,0:08:19.39,0:08:20.50,Default1,,0000,0000,0000,,Nóng như vậy! Lớn như vậy! Nó sắp...
Dialogue: 0,0:08:22.01,0:08:23.10,Default1,,0000,0000,0000,,Sắp ra rồi, đúng không?!
Dialogue: 0,0:08:24.61,0:08:25.36,Default1,,0000,0000,0000,,Được rồi! {\alpha&HFF&}Cứ ra trong tớ bất cứ lúc nào cậu thích!!
Dialogue: 0,0:08:25.36,0:08:28.00,Default1,,0000,0000,0000,,Được rồi! Cứ ra trong tớ bất cứ lúc nào cậu thích!!
Dialogue: 0,0:08:29.41,0:08:33.56,Default1,,0000,0000,0000,,Tớ muốn cậu lấp đầy bên trong tớ!!
Dialogue: 0,0:08:53.06,0:08:53.69,Default1,,0000,0000,0000,,Ơ...
Dialogue: 0,0:08:58.26,0:08:59.69,Default1,,0000,0000,0000,,Anh không sao chứ?
Dialogue: 0,0:09:01.14,0:09:02.35,Default1,,0000,0000,0000,,Ừ-Ừ...
Dialogue: 0,0:09:04.06,0:09:05.46,Default1,,0000,0000,0000,,Đó chỉ là giấc mơ ư?
Dialogue: 0,0:09:05.92,0:09:08.29,Default1,,0000,0000,0000,,Ơ... Em là Senju.
Dialogue: 0,0:09:08.90,0:09:10.34,Default1,,0000,0000,0000,,Là Senju Mizuha.
Dialogue: 0,0:09:11.31,0:09:15.02,Default1,,0000,0000,0000,,Em rất biết ơn vì nhờ anh mà em không bị thương...
Dialogue: 0,0:09:15.36,0:09:16.05,Default1,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:09:17.80,0:09:19.52,Default1,,0000,0000,0000,,Ôi... X-Xin lỗi...
Dialogue: 0,0:09:21.85,0:09:25.96,Default1,,0000,0000,0000,,Kawahara-san, nếu không có anh ở đây,  có lẽ em đã ngã xuống cầu thang.
Dialogue: 0,0:09:30.60,0:09:31.52,Default1,,0000,0000,0000,,Có lẽ bây giờ...
Dialogue: 0,0:09:36.96,0:09:39.22,Default1,,0000,0000,0000,,Anh lại cứu em lần nữa...
Dialogue: 0,0:09:58.26,0:10:01.09,Default1,,0000,0000,0000,,Hừm, anh đã quên rồi, đúng không?
Dialogue: 0,0:10:03.48,0:10:05.86,Default1,,0000,0000,0000,,Em hơi bị sốc đấy.
Dialogue: 0,0:10:09.02,0:10:14.32,Default1,,0000,0000,0000,,Cậu định để một tiểu thư như Senju-san mặc bộ đồ bẩn thỉu đó sau?
Dialogue: 0,0:10:14.32,0:10:16.25,Default1,,0000,0000,0000,,Để con gái duy nhất của nhà Senju Zaibatsu
Dialogue: 0,0:10:16.25,0:10:19.61,Default1,,0000,0000,0000,,trở nên ủ rủ vì mặc váy để lộ cả quần chíp, cô nghĩ tôi sẽ để yên à?!
Dialogue: 0,0:10:19.94,0:10:23.39,Default1,,0000,0000,0000,,Ơ... Ơ, em...
Dialogue: 0,0:10:30.40,0:10:31.46,Default1,,0000,0000,0000,,N-Này!
Dialogue: 0,0:10:31.83,0:10:32.77,Default1,,0000,0000,0000,,Chờ đã!
Dialogue: 0,0:10:36.16,0:10:39.69,Default1,,0000,0000,0000,,Nếu em có suy nghĩ của riêng mình, thì đừng do dự nói ra.
Dialogue: 0,0:10:40.59,0:10:41.36,Default1,,0000,0000,0000,,V-Vâng...
Dialogue: 0,0:10:42.52,0:10:46.76,Default1,,0000,0000,0000,,Nếu em muốn trở nên tự tin hơn, tại sao không tham gia vào một CLB thể thao nào đó?
Dialogue: 0,0:10:48.73,0:10:49.83,Default1,,0000,0000,0000,,Nhưng đừng chọn bóng chuyền!
Dialogue: 0,0:10:50.34,0:10:51.31,Default1,,0000,0000,0000,,Bởi vì có một con quỷ ở đó.
Dialogue: 0,0:10:51.97,0:10:54.27,Default1,,0000,0000,0000,,Em vẫn nhớ như in cái này hôm đó.
Dialogue: 0,0:10:55.26,0:10:58.69,Default1,,0000,0000,0000,,Chính những lời nói đó của Senpai đã cho em dũng khí.
Dialogue: 0,0:10:59.25,0:11:03.95,Default1,,0000,0000,0000,,Đó là lí do bây giờ em đang mặc bộ đồng phục này đấy, Senpai.
Dialogue: 0,0:11:07.26,0:11:08.98,Default1,,0000,0000,0000,,Em có chút kích động.
Dialogue: 0,0:11:08.98,0:11:10.29,Default1,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:11:11.09,0:11:14.66,Default1,,0000,0000,0000,,Thật là vô vọng khi gợi lại khoảng thời gian đó, đúng không?
Dialogue: 0,0:11:15.04,0:11:15.81,Default1,,0000,0000,0000,,Shinichi?
Dialogue: 0,0:11:17.18,0:11:18.19,Default1,,0000,0000,0000,,Shinichi, là cậu phải không?
Dialogue: 0,0:11:18.75,0:11:19.62,Default1,,0000,0000,0000,,Takaaki!
Dialogue: 0,0:11:20.39,0:11:21.22,Default1,,0000,0000,0000,,Shinichi!
Dialogue: 0,0:11:24.63,0:11:25.51,Default1,,0000,0000,0000,,Fubuki...
Dialogue: 0,0:11:26.67,0:11:27.72,Default1,,0000,0000,0000,,Shinichi...
Dialogue: 0,0:11:30.76,0:11:32.59,Default1,,0000,0000,0000,,Hãy lấy chỗ này làm căn cứ.
Dialogue: 0,0:11:34.87,0:11:38.26,Default1,,0000,0000,0000,,Trông giống như nó bị chấn động, không gian bị bóp méo và dễ dàng sụp đổ.
Dialogue: 0,0:11:39.04,0:11:42.26,Default1,,0000,0000,0000,,Nghe đây. Luôn phải đi một nhóm hai người, được chứ?
Dialogue: 0,0:11:46.32,0:11:50.32,Default1,,0000,0000,0000,,Trước khi gặp hai người, chúng tớ đã đi một vòng, và...
Dialogue: 0,0:11:50.32,0:11:51.96,Default1,,0000,0000,0000,,Đây là lối duy nhất thông ra ngoài...
Dialogue: 0,0:12:00.07,0:12:03.06,Default1,,0000,0000,0000,,Tớ vẫn luôn yên cậu...
Dialogue: 0,0:12:05.62,0:12:08.58,Sign/Note,,0000,0000,0000,,{\bord7\blur7\3c&H3C3C3C&\pos(363,53)}Khoảnh khắc bình yên
Dialogue: 0,0:12:05.80,0:12:07.51,Default1,,0000,0000,0000,,Fu-Fubuki...
Dialogue: 0,0:12:18.79,0:12:20.90,Default1,,0000,0000,0000,,Đừng nói như thể chúng ta sắp chết vậy.
Dialogue: 0,0:12:21.79,0:12:23.28,Default1,,0000,0000,0000,,Tớ không còn gì hối tiếc...
Dialogue: 0,0:12:24.57,0:12:28.62,Default1,,0000,0000,0000,,Nếu phải chết, tớ muốn được chết trong vòng tay của cậu.
Dialogue: 0,0:12:38.60,0:12:42.89,Default1,,0000,0000,0000,,Tuy ướt đẫm mồ hôi và còn có mùi... Nhưng nếu không có nó, em sẽ không kiềm chế được, đúng không?
Dialogue: 0,0:12:45.39,0:12:46.72,Default1,,0000,0000,0000,,Anh sẽ làm em quên nó đi...
Dialogue: 0,0:12:47.19,0:12:49.70,Default1,,0000,0000,0000,,Thứ duy nhất em phải nghĩ đến là dương vật của anh.
Dialogue: 0,0:12:53.29,0:12:54.21,Thinking,,0000,0000,0000,,Nó đang di chuyển...
Dialogue: 0,0:12:54.70,0:12:58.93,Thinking,,0000,0000,0000,,Nó... đang di chuyển và đâm thật sâu bên trong mình.
Dialogue: 0,0:13:00.28,0:13:02.74,Thinking,,0000,0000,0000,,Đau đớn lúc nãy cứ như sự dối trá vậy.
Dialogue: 0,0:13:24.96,0:13:26.45,Default1,,0000,0000,0000,,Senpai!{\alpha&HFF&} Senpai! Senpai!
Dialogue: 0,0:13:26.45,0:13:28.08,Default1,,0000,0000,0000,,Senpai! Senpai! {\alpha&HFF&}Senpai!
Dialogue: 0,0:13:28.08,0:13:29.37,Default1,,0000,0000,0000,,Senpai! Senpai! Senpai!
Dialogue: 0,0:13:37.63,0:13:39.00,Default1,,0000,0000,0000,,Em muốn quên!
Dialogue: 0,0:13:40.67,0:13:42.28,Default1,,0000,0000,0000,,Hãy làm em quên nó đi!
Dialogue: 0,0:13:50.61,0:13:51.79,Default1,,0000,0000,0000,,Đâm thật sâu bên trong em!
Dialogue: 0,0:13:54.39,0:13:55.90,Default1,,0000,0000,0000,,Nếu cứ như vầy! {\alpha&HFF&}Em sắp ra! Em sắp ra!!
Dialogue: 0,0:13:55.90,0:13:57.09,Default1,,0000,0000,0000,,Nếu cứ như vầy! Em sắp ra!{\alpha&HFF&} Em sắp ra!!
Dialogue: 0,0:13:57.09,0:13:58.00,Default1,,0000,0000,0000,,Nếu cứ như vầy! Em sắp ra! Em sắp ra!!
Dialogue: 0,0:13:58.00,0:14:00.34,Default1,,0000,0000,0000,,Tử cung em có thể cảm thấy thằng nhỏ của anh...!
Dialogue: 0,0:14:01.69,0:14:02.90,Default1,,0000,0000,0000,,Em cảm thấy cái gì?
Dialogue: 0,0:14:03.58,0:14:04.87,Default1,,0000,0000,0000,,Cái gì sắp ra?
Dialogue: 0,0:14:05.99,0:14:07.53,Default1,,0000,0000,0000,,Thằng nhỏ... {\alpha&HFF&}Thằng nhỏ của anh...!
Dialogue: 0,0:14:07.53,0:14:08.97,Default1,,0000,0000,0000,,Thằng nhỏ... Thằng nhỏ của anh...!
Dialogue: 0,0:14:10.57,0:14:13.06,Default1,,0000,0000,0000,,Nếu em muốn quên nó đi, thì phải làm theo những gì anh bảo.
Dialogue: 0,0:14:13.43,0:14:14.84,Default1,,0000,0000,0000,,Chỉ có làm như vậy, em mới có thể quên được nó.
Dialogue: 0,0:14:16.33,0:14:17.30,Default1,,0000,0000,0000,,Nó đang vang lên...{\alpha&HFF&} giọng nói của một người đàn ông mạnh mẽ...
Dialogue: 0,0:14:17.30,0:14:19.35,Default1,,0000,0000,0000,,Nó đang vang lên... giọng nói của môt người đàn ông mạnh mẽ...
Dialogue: 0,0:14:21.63,0:14:22.60,Default1,,0000,0000,0000,,Tử cung của em...{\alpha&HFF&} có thể cảm thấy thằng nhỏ của anh...!
Dialogue: 0,0:14:22.60,0:14:23.99,Default1,,0000,0000,0000,,Tử cung của em... có thể cảm thấy thằng nhỏ của anh...!
Dialogue: 0,0:14:29.15,0:14:31.51,Default1,,0000,0000,0000,,Dương vật của anh! {\alpha&HFF&}Dương vật của anh đang chạm vào tử cung của em!
Dialogue: 0,0:14:31.51,0:14:34.79,Default1,,0000,0000,0000,,Dương vật của anh! Dương vật của anh đang chạm vào tử cung của em!
Dialogue: 0,0:14:42.37,0:14:43.80,Default1,,0000,0000,0000,,Em sẽ phát điên mất! {\alpha&HFF&}Em sẽ phát điên mất!!
Dialogue: 0,0:14:43.80,0:14:45.59,Default1,,0000,0000,0000,,Em sẽ phát điên mất! Em sẽ phát điên mất!!
Dialogue: 0,0:15:06.71,0:15:07.71,Default1,,0000,0000,0000,,Senpai...
Dialogue: 0,0:15:12.97,0:15:15.09,Default1,,0000,0000,0000,,Sen…{\alpha&HFF&}pai…
Dialogue: 0,0:15:15.09,0:15:15.76,Default1,,0000,0000,0000,,Sen…pai…
Dialogue: 0,0:15:24.34,0:15:25.34,Default1,,0000,0000,0000,,Ơ, cái này?
Dialogue: 0,0:15:26.88,0:15:28.62,Default1,,0000,0000,0000,,Đó là vật tớ để ở phòng CLB.
Dialogue: 0,0:15:28.75,0:15:29.47,Default1,,0000,0000,0000,,Nó là gì vậy?
Dialogue: 0,0:15:30.16,0:15:31.93,Default1,,0000,0000,0000,,Cái gì cũng được!
Dialogue: 0,0:15:32.43,0:15:34.57,Default1,,0000,0000,0000,,Nói tóm lại, nó là thứ rất cần thiết đối với các cô gái...
Dialogue: 0,0:15:35.92,0:15:37.89,Default1,,0000,0000,0000,,Hử? Hai người bọn họ không có ở đây à.
Dialogue: 0,0:15:38.34,0:15:39.73,Default1,,0000,0000,0000,,Takaaki! {\alpha&HFF&}Senju-san!
Dialogue: 0,0:15:39.73,0:15:40.72,Default1,,0000,0000,0000,,Takaaki! Senju-san!
Dialogue: 0,0:15:41.44,0:15:42.75,Default1,,0000,0000,0000,,Xin lỗi, tớ ngủ quên...
Dialogue: 0,0:15:43.33,0:15:44.85,Default1,,0000,0000,0000,,tiểu thư vẫn còn đang ngủ.
Dialogue: 0,0:15:45.22,0:15:46.72,Default1,,0000,0000,0000,,Trông em ấy không được khỏe.
Dialogue: 0,0:15:51.53,0:15:52.45,Default1,,0000,0000,0000,,Em có sao không?
Dialogue: 0,0:15:52.48,0:15:55.85,Default1,,0000,0000,0000,,E-Em không sao... Xin lỗi đã làm chị lo lắng.
Dialogue: 0,0:15:56.09,0:16:00.25,Default1,,0000,0000,0000,,Em chỉ bị thiếu máu chút thôi, nhưng bây giờ thì ổn rồi.
Dialogue: 0,0:16:02.66,0:16:03.72,Default1,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:16:05.28,0:16:07.32,Default1,,0000,0000,0000,,Nó thật đáng yêu và ấm áp.
Dialogue: 0,0:16:07.63,0:16:09.43,Default1,,0000,0000,0000,,Thấy không? Không phải nó rất có ích sao?
Dialogue: 0,0:16:12.15,0:16:13.96,Default1,,0000,0000,0000,,Thế, giao mọi chuyện còn lại cho cậu nhé, Takaaki-kun.
Dialogue: 0,0:16:14.42,0:16:15.85,Default1,,0000,0000,0000,,Không có gì phải lo cả...
Dialogue: 0,0:16:16.11,0:16:20.39,Default1,,0000,0000,0000,,Tớ, Tsumura Takaaki, hứa sẽ phục vụ hết sức mình như là một hiệp sĩ.
Dialogue: 0,0:16:20.67,0:16:22.91,Default1,,0000,0000,0000,,Tớ hi vọng là hiệp sĩ sẽ không hóa thành sói.
Dialogue: 0,0:16:23.67,0:16:25.89,Default1,,0000,0000,0000,,Cứ yên tâm. Cậu ấy là bạn tốt nhất của tớ mà.
Dialogue: 0,0:16:26.49,0:16:27.10,Default1,,0000,0000,0000,,Ừ.
Dialogue: 0,0:16:30.06,0:16:31.41,Default1,,0000,0000,0000,,Xin lỗi, Takaaki...
Dialogue: 0,0:16:32.45,0:16:33.00,Default1,,0000,0000,0000,,Tớ đi nghỉ trước đây.
Dialogue: 0,0:16:33.97,0:16:34.53,Default1,,0000,0000,0000,,Ừ.
Dialogue: 0,0:16:41.46,0:16:42.07,Default1,,0000,0000,0000,,Shinichi!
Dialogue: 0,0:16:47.04,0:16:47.71,Default1,,0000,0000,0000,,Xin lỗi...
Dialogue: 0,0:16:48.94,0:16:50.77,Default1,,0000,0000,0000,,Tớ lỡ ngủ quên...
Dialogue: 0,0:16:52.04,0:16:53.15,Default1,,0000,0000,0000,,Hãy chia nhau tìm em ấy.
Dialogue: 0,0:16:54.03,0:16:55.46,Default1,,0000,0000,0000,,Senju-san!
Dialogue: 0,0:16:56.79,0:16:58.56,Default1,,0000,0000,0000,,Nếu em có ở đó, thì hãy trả lời đi!
Dialogue: 0,0:16:59.88,0:17:02.35,Default1,,0000,0000,0000,,Cả hai biến mất? Ý cậu là sao?
Dialogue: 0,0:17:02.56,0:17:06.00,Default1,,0000,0000,0000,,Xin lỗi... Có lẽ "biến mất" không đúng cho lắm.
Dialogue: 0,0:17:07.27,0:17:10.26,Default1,,0000,0000,0000,,Khi cậu ta và Momose-san ra ngoài, cậu ta đã tỏ tình với em ấy.
Dialogue: 0,0:17:13.90,0:17:16.68,Default1,,0000,0000,0000,,Sau khi họ hẹn gặp nhau, Shinichi đã...
Dialogue: 0,0:17:16.84,0:17:19.26,Default1,,0000,0000,0000,,Không thể nào! Chuyện đó là không thể!
Dialogue: 0,0:17:23.99,0:17:25.10,Default1,,0000,0000,0000,,Không thể nào...
Dialogue: 0,0:17:27.44,0:17:29.37,Default1,,0000,0000,0000,,Dù cả tớ cũng không muốn tin.
Dialogue: 0,0:17:29.51,0:17:32.76,Default1,,0000,0000,0000,,Mới nãy, cậu ta đã đuổi theo khi em ấy đi vệ sinh.
Dialogue: 0,0:18:00.46,0:18:02.60,Default1,,0000,0000,0000,,Mặc dù mình đã tìm ra Senju-san...
Dialogue: 0,0:18:03.12,0:18:04.69,Default1,,0000,0000,0000,,Vậy mà cậu ấy và Fubuki lại biến mất.
Dialogue: 0,0:18:21.82,0:18:24.11,Default1,,0000,0000,0000,,Đừng mà, Takaaki-kun...
Dialogue: 0,0:18:24.56,0:18:26.69,Default1,,0000,0000,0000,,Dừng lại... Lúc này không phải dành cho chuyện đó...
Dialogue: 0,0:18:28.91,0:18:29.76,Default1,,0000,0000,0000,,Dừng lại...{\alpha&HFF&} Tớ...
Dialogue: 0,0:18:29.76,0:18:30.48,Default1,,0000,0000,0000,,Dừng lại... Tớ...
Dialogue: 0,0:18:32.28,0:18:34.15,Default1,,0000,0000,0000,,Tớ sẽ không cho phép cậu nói vậy...
Dialogue: 0,0:18:41.70,0:18:42.53,Default1,,0000,0000,0000,,Tớ vui lắm...
Dialogue: 0,0:18:45.90,0:18:47.81,Default1,,0000,0000,0000,,cậu đã đáp lại nụ hôn của tớ.
Dialogue: 0,0:18:53.31,0:18:54.22,Default1,,0000,0000,0000,,Không phải vậy...
Dialogue: 0,0:18:54.22,0:18:57.69,Default1,,0000,0000,0000,,Chính cậu đã dùng vũ lực đưa lưỡi cậu vào trong miệng tớ...
Dialogue: 0,0:19:08.80,0:19:10.37,Default1,,0000,0000,0000,,Nhưng cậu chấp nhận nó...
Dialogue: 0,0:19:11.10,0:19:12.93,Default1,,0000,0000,0000,,bằng cách quấn lấy lưỡi của tớ...
Dialogue: 0,0:19:18.43,0:19:20.11,Default1,,0000,0000,0000,,Xin lỗi vì đã làm điều không phải với cậu.
Dialogue: 0,0:19:20.45,0:19:21.25,Default1,,0000,0000,0000,,Cậu có ghét tớ không?
Dialogue: 0,0:19:21.82,0:19:23.13,Default1,,0000,0000,0000,,Còn phải hỏi...
Dialogue: 0,0:19:24.04,0:19:25.75,Default1,,0000,0000,0000,,Cậu thật là xấu xa.
Dialogue: 0,0:19:27.54,0:19:28.06,Default1,,0000,0000,0000,,Không!
Dialogue: 0,0:19:28.74,0:19:29.56,Default1,,0000,0000,0000,,Đừng...
Dialogue: 0,0:19:29.94,0:19:31.47,Default1,,0000,0000,0000,,chạm vào chỗ đó!
Dialogue: 0,0:19:33.81,0:19:34.61,Default1,,0000,0000,0000,,Không...
Dialogue: 0,0:19:35.34,0:19:37.53,Default1,,0000,0000,0000,,Nó bắt đầu cứng lên này, Momose-san.
Dialogue: 0,0:19:42.77,0:19:43.60,Default1,,0000,0000,0000,,Đừng... {\alpha&HFF&}Chúng ta không nên làm ở đây... Thả tớ ra...
Dialogue: 0,0:19:43.60,0:19:44.93,Default1,,0000,0000,0000,,Đừng... Chúng ta không nên làm ở đây... {\alpha&HFF&}Thả tớ ra...
Dialogue: 0,0:19:44.93,0:19:45.80,Default1,,0000,0000,0000,,Đừng... Chúng ta không nên làm ở đây... Thả tớ ra...
Dialogue: 0,0:19:47.23,0:19:48.41,Default1,,0000,0000,0000,,Takaaki-kun...
Dialogue: 0,0:19:51.86,0:19:52.93,Thinking,,0000,0000,0000,,Không, {\alpha&HFF&}không... Nụ hôn của cậu ấy đang hút nước bọt của mình...
Dialogue: 0,0:19:52.93,0:19:53.73,Thinking,,0000,0000,0000,,Không, không...{\alpha&HFF&} Nụ hôn của cậu ấy đang hút nước bọt của mình...
Dialogue: 0,0:19:53.73,0:19:55.12,Thinking,,0000,0000,0000,,Không, không... Nụ hôn của cậu ấy đang hút nước bọt của mình...
Dialogue: 0,0:19:56.45,0:19:58.41,Thinking,,0000,0000,0000,,Nước bọt... {\alpha&HFF&}đang bị hút...
Dialogue: 0,0:19:58.41,0:19:59.52,Thinking,,0000,0000,0000,,Nước bọt... đang bị hút...
Dialogue: 0,0:20:01.01,0:20:02.01,Thinking,,0000,0000,0000,,Dù mình không muốn thế...
Dialogue: 0,0:20:02.86,0:20:04.53,Thinking,,0000,0000,0000,,Lưỡi mình đang bị quấn lấy...
Dialogue: 0,0:20:04.86,0:20:07.29,Thinking,,0000,0000,0000,,Nước bọt tràn ra khỏi miệng mình...
Dialogue: 0,0:20:08.80,0:20:10.56,Thinking,,0000,0000,0000,,Cái gì đây? {\alpha&HFF&}Tuyệt quá...
Dialogue: 0,0:20:10.56,0:20:11.62,Thinking,,0000,0000,0000,,Cái gì đây? Tuyệt quá...
Dialogue: 0,0:20:14.10,0:20:16.39,Thinking,,0000,0000,0000,,Mình cảm thấy... tê dại...
Dialogue: 0,0:20:25.02,0:20:27.26,Thinking,,0000,0000,0000,,Lần này, nước bọt lại tràn vào miệng mình...
Dialogue: 0,0:20:28.77,0:20:33.49,Thinking,,0000,0000,0000,,Nó tràn vào miệng mình một cách mạnh mẽ...
Dialogue: 0,0:20:34.33,0:20:36.47,Thinking,,0000,0000,0000,,Nụ hôn của Shinichi...
Dialogue: 0,0:20:37.05,0:20:38.44,Thinking,,0000,0000,0000,,Nó hoàn toàn khác...
Dialogue: 0,0:20:42.61,0:20:43.64,Thinking,,0000,0000,0000,,Không...{\alpha&HFF&} Không!
Dialogue: 0,0:20:43.64,0:20:44.24,Thinking,,0000,0000,0000,,Không... Không!
Dialogue: 0,0:20:51.36,0:20:54.50,Default1,,0000,0000,0000,,Vì nụ hôn... mà cậu đã hứng lên rồi, đúng không?
Dialogue: 0,0:20:58.91,0:21:01.34,Default1,,0000,0000,0000,,Vậy với Shinichi thì sao?
Dialogue: 0,0:21:04.23,0:21:05.76,Default1,,0000,0000,0000,,Tớ không có cảm giác đó!
Dialogue: 0,0:21:06.59,0:21:09.05,Default1,,0000,0000,0000,,Vậy đây là lần đầu tiên, cậu sướng như vậy.
Dialogue: 0,0:21:09.77,0:21:10.94,Default1,,0000,0000,0000,,Lần đầu tiên?
Dialogue: 0,0:21:11.61,0:21:13.62,Default1,,0000,0000,0000,,Fubuki và Takaaki?
Dialogue: 0,0:21:14.39,0:21:17.09,Default1,,0000,0000,0000,,Tớ xin cậu... Đừng nói nữa...
Dialogue: 0,0:21:21.82,0:21:23.23,Default1,,0000,0000,0000,,Nhưng đừng quên...
Dialogue: 0,0:21:24.20,0:21:26.96,Default1,,0000,0000,0000,,Người chỉ cho Momose-san khoái cảm của nụ hôn thật sự... {\alpha&HFF&}là tớ...
Dialogue: 0,0:21:26.96,0:21:27.86,Default1,,0000,0000,0000,,Người chỉ cho Momose-san khoái cảm của nụ hôn thật sự... là tớ...
Dialogue: 0,0:21:30.42,0:21:32.16,Default1,,0000,0000,0000,,Nữa! Nhiều hơn nữa!
Dialogue: 0,0:21:32.70,0:21:35.06,Default1,,0000,0000,0000,,Momose-san, nhấc thẳng mông cậu lên!
Dialogue: 0,0:21:36.91,0:21:37.70,Default1,,0000,0000,0000,,Không!
Dialogue: 0,0:21:38.97,0:21:43.46,Default1,,0000,0000,0000,,Mình có thể cảm thấy... thằng nhỏ của Takaaki-kun... nó đang cứng lên...
Dialogue: 0,0:21:47.01,0:21:50.15,Default1,,0000,0000,0000,,Ở dưới này của tớ đã cứng lên. Cậu cảm thấy nó không?
Dialogue: 0,0:21:50.15,0:21:51.56,Default1,,0000,0000,0000,,K-Không...
Dialogue: 0,0:21:53.23,0:21:54.90,Default1,,0000,0000,0000,,Cảm giác này...
Dialogue: 0,0:21:57.16,0:22:00.99,Default1,,0000,0000,0000,,L-Làm ơn... đừng làm như vậy với tớ...
Dialogue: 0,0:22:03.51,0:22:05.26,Default1,,0000,0000,0000,,Đừng sờ mó ngực tớ!
Dialogue: 0,0:22:05.66,0:22:06.69,Default1,,0000,0000,0000,,Tớ biết mà...
Dialogue: 0,0:22:07.84,0:22:10.26,Default1,,0000,0000,0000,,Cậu rên rỉ giống y như lúc nãy.
Dialogue: 0,0:22:10.54,0:22:11.99,Default1,,0000,0000,0000,,Lúc nãy là ý gì chứ?
Dialogue: 0,0:22:12.51,0:22:15.82,Default1,,0000,0000,0000,,Không lẽ Fubuki và Takaaki lúc nào cũng làm chuyện này sao?
Dialogue: 0,0:22:16.16,0:22:16.86,Default1,,0000,0000,0000,,Tớ vui lắm...
Dialogue: 0,0:22:18.19,0:22:21.02,Default1,,0000,0000,0000,,Lần đó. Ở trên sân thượng...
Dialogue: 0,0:22:21.70,0:22:22.67,Default1,,0000,0000,0000,,Đừng nhìn!
Dialogue: 0,0:22:24.89,0:22:27.12,Default1,,0000,0000,0000,,Cậu sợ nó bị tớ làm vấy bẩn à?
Dialogue: 0,0:22:27.97,0:22:29.60,Default1,,0000,0000,0000,,Chuẩn bị tốt đấy...
Dialogue: 0,0:22:32.72,0:22:35.04,Default1,,0000,0000,0000,,Nhưng tớ sẽ vấy bẩn nó thêm lần nữa.
Dialogue: 0,0:22:35.60,0:22:38.19,Default1,,0000,0000,0000,,Vì vậy, tớ đã là bảo cậu đừng có sờ mó ngực tớ nữa!
Dialogue: 0,0:22:38.77,0:22:40.04,Default1,,0000,0000,0000,,Không! Cậu mạnh bạo quá!!
Dialogue: 0,0:22:41.79,0:22:43.00,Default1,,0000,0000,0000,,Tớ có thể cảm thấy nó!
Dialogue: 0,0:22:44.06,0:22:45.84,Default1,,0000,0000,0000,,Nóng quá! Không!
Dialogue: 0,0:22:46.52,0:22:48.09,Default1,,0000,0000,0000,,Hãy để nó vào giữa cặp đùi của cậu.
Dialogue: 0,0:22:50.41,0:22:52.91,Default1,,0000,0000,0000,,Momose-san, tớ sẽ để cậu cảm nhận nhiều hơn nữa...
Dialogue: 0,0:22:53.42,0:22:54.56,Default1,,0000,0000,0000,,K-Không...
Dialogue: 0,0:22:55.35,0:22:59.49,Default1,,0000,0000,0000,,Tớ không thể làm chuyện này được.
Dialogue: 0,0:23:00.59,0:23:01.51,Default1,,0000,0000,0000,,Không...
Dialogue: 0,0:23:02.14,0:23:03.73,Default1,,0000,0000,0000,,Đừng siết chặt như vậy!
Dialogue: 0,0:23:12.98,0:23:16.64,Default1,,0000,0000,0000,,Của Takaaki-kun lớn quá... Nó giống như muốn nổ tung...
Dialogue: 0,0:23:16.80,0:23:20.00,Default1,,0000,0000,0000,,Nếu cậu ta thấy Momose-san như bây giờ...
Dialogue: 0,0:23:20.19,0:23:20.89,Default1,,0000,0000,0000,,Không... {\alpha&HFF&}K-Không...
Dialogue: 0,0:23:20.89,0:23:22.02,Default1,,0000,0000,0000,,Không... K-Không...
Dialogue: 0,0:23:22.04,0:23:24.80,Default1,,0000,0000,0000,,Đừng nói vậy...
Dialogue: 0,0:23:25.05,0:23:27.69,Default1,,0000,0000,0000,,Ồ... Lúc này đây, cậu thuộc về tớ...
Dialogue: 0,0:23:30.40,0:23:32.46,Default1,,0000,0000,0000,,Nó đã lớn như thế này...{\alpha&HFF&} Nó nóng quá!
Dialogue: 0,0:23:32.46,0:23:33.14,Default1,,0000,0000,0000,,Nó đã lớn như thế này... Nó nóng quá!
Dialogue: 0,0:23:34.56,0:23:37.98,Default1,,0000,0000,0000,,Của Takaaki-kun... nóng quá!
Dialogue: 0,0:23:40.10,0:23:41.54,Default1,,0000,0000,0000,,Cậu muốn tớ đút vào không?
Dialogue: 0,0:23:45.98,0:23:46.68,Default1,,0000,0000,0000,,Ừm...
Dialogue: 0,0:23:50.53,0:23:52.32,Default1,,0000,0000,0000,,Được rồi, cậu làm đi...
Dialogue: 0,0:23:57.93,0:23:58.60,Default1,,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:24:01.78,0:24:02.90,Default1,,0000,0000,0000,,Cô ấy đã lừa dối mình?
Dialogue: 0,0:24:03.91,0:24:05.65,Default1,,0000,0000,0000,,Lời tỏ tình lúc đó...
Dialogue: 0,0:24:13.74,0:24:16.66,Default1,,0000,0000,0000,,Fubuki, chết tiệt... dám cùng với Takaaki làm chuyện đó...
Dialogue: 0,0:24:19.55,0:24:21.02,Default1,,0000,0000,0000,,Em muốn anh đút nó vào không?
Dialogue: 0,0:24:18.39,0:24:21.35,Sign/Note,,0000,0000,0000,,{\an8\pos(363,4)}{\bord6\blur6\shad0\3c&H3C3C3C&}Cơn phẫn nộ của kẻ xa rời thực tế
Dialogue: 0,0:24:22.57,0:24:25.31,Default1,,0000,0000,0000,,Đừng... Cậu tuyệt đối không thể...
Dialogue: 0,0:24:25.93,0:24:26.46,Default1,,0000,0000,0000,,Tớ...{\alpha&HFF&} Tớ... vẫn còn trinh...
Dialogue: 0,0:24:26.46,0:24:27.24,Default1,,0000,0000,0000,,Tớ... Tớ... {\alpha&HFF&}vẫn còn trinh...
Dialogue: 0,0:24:27.24,0:24:28.49,Default1,,0000,0000,0000,,Tớ... Tớ... vẫn còn trinh...
Dialogue: 0,0:24:29.64,0:24:31.65,Default1,,0000,0000,0000,,Cậu vẫn nghĩ về cậu ta đúng không?
Dialogue: 0,0:24:33.55,0:24:36.41,Default1,,0000,0000,0000,,Làm ơn... Đừng quấy rầy tớ nữa...
Dialogue: 0,0:24:41.35,0:24:43.64,Default1,,0000,0000,0000,,Xin lỗi! Tớ không định làm cho cậu khóc.
Dialogue: 0,0:24:44.29,0:24:46.12,Default1,,0000,0000,0000,,Nhưng ít nhất cậu có thể dùng miệng được chứ?
Dialogue: 0,0:24:47.45,0:24:49.27,Default1,,0000,0000,0000,,Được chứ, Momose-san?
Dialogue: 0,0:24:51.62,0:24:52.88,Default1,,0000,0000,0000,,Ừm...
Dialogue: 0,0:24:54.62,0:24:56.91,Default1,,0000,0000,0000,,Được rồi...
Dialogue: 0,0:24:57.69,0:24:59.64,Default1,,0000,0000,0000,,Chỉ cần cậu buông tha cho tớ...
Dialogue: 0,0:25:01.21,0:25:04.15,Default1,,0000,0000,0000,,Takaaki-kun, tại sao lại trong tư thế này?
Dialogue: 0,0:25:05.56,0:25:07.58,Default1,,0000,0000,0000,,Xấu hổ lắm...
Dialogue: 0,0:25:08.79,0:25:11.92,Default1,,0000,0000,0000,,Cậu có thể cho tớ xem tư thế xấu hổ của cậu khi thủ dâm, được không?
Dialogue: 0,0:25:13.97,0:25:15.63,Default1,,0000,0000,0000,,Nó là để đổi lấy sự trong trắng của cậu.
Dialogue: 0,0:25:18.78,0:25:19.99,Thinking,,0000,0000,0000,,Mùi của nó thật tuyệt!
Dialogue: 0,0:25:21.80,0:25:24.19,Thinking,,0000,0000,0000,,Đó là mùi của đàn ông...
Dialogue: 0,0:25:26.09,0:25:28.85,Thinking,,0000,0000,0000,,Nó sẽ xé toạt màng trinh của mình?
Dialogue: 0,0:25:31.17,0:25:35.02,Thinking,,0000,0000,0000,,Nếu mình không nhanh lên, sự trong trắng của mình.
Dialogue: 0,0:25:45.28,0:25:47.46,Default1,,0000,0000,0000,,Cậu thích cậu ta đến vậy ư?
Dialogue: 0,0:25:49.06,0:25:51.86,Default1,,0000,0000,0000,,cậu ta luôn tử tế với tất cả các cô gái khác.
Dialogue: 0,0:25:53.60,0:25:54.72,Default1,,0000,0000,0000,,K-Không phải!
Dialogue: 0,0:25:59.26,0:26:03.77,Default1,,0000,0000,0000,,Khi ở bên cậu, cậu ta lúc nào cũng nghĩ về tiểu thư.
Dialogue: 0,0:26:07.74,0:26:12.54,Default1,,0000,0000,0000,,Lúc nãy, ở trong toilet, em ấy chắc hẳn đã bị cậu ta cưỡng bức.
Dialogue: 0,0:26:14.71,0:26:16.00,Thinking,,0000,0000,0000,,Không... {\alpha&HFF&}Không thể nào...
Dialogue: 0,0:26:16.00,0:26:16.91,Thinking,,0000,0000,0000,,Không... Không thể nào...
Dialogue: 0,0:26:51.74,0:26:52.91,Thinking,,0000,0000,0000,,Không... {\alpha&HFF&}Mình muốn nó...
Dialogue: 0,0:26:52.91,0:26:55.03,Thinking,,0000,0000,0000,,Không... Mình muốn nó...
Dialogue: 0,0:26:56.88,0:27:01.91,Thinking,,0000,0000,0000,,Không! Nếu cứ ngửi cái mùi kinh khủng này, mình sẽ không kiềm chế được mất!
Dialogue: 0,0:27:10.10,0:27:11.07,Thinking,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:27:11.77,0:27:13.85,Thinking,,0000,0000,0000,,Nó lớn lên trong miệng mình...
Dialogue: 0,0:27:14.59,0:27:16.93,Thinking,,0000,0000,0000,,Mình trở nên hứng hơn nữa...
Dialogue: 0,0:27:27.63,0:27:29.99,Thinking,,0000,0000,0000,,Không được... Mình phải mút nó...
Dialogue: 0,0:27:30.56,0:27:32.15,Thinking,,0000,0000,0000,,Nếu không mình sẽ mất sự trong trắng...
Dialogue: 0,0:27:47.98,0:27:49.35,Thinking,,0000,0000,0000,,Không,{\alpha&HFF&} mình...
Dialogue: 0,0:27:49.35,0:27:50.16,Thinking,,0000,0000,0000,,Không, mình...
Dialogue: 0,0:27:52.16,0:27:52.88,Thinking,,0000,0000,0000,,Không thể...
Dialogue: 0,0:27:52.88,0:27:54.88,Thinking,,0000,0000,0000,,
