Dialogue: 0,0:00:03.23,0:00:07.19,Default,,0000,0000,0000,,<i>Anh trai cùng cha khác mẹ với tôi đã về để dự đáng tang bố mẹ tôi.</i>
Dialogue: 0,0:00:08.34,0:00:10.24,Default,,0000,0000,0000,,<i>Đã sáu năm không gặp...</i>
Dialogue: 0,0:00:11.44,0:00:14.34,Default,,0000,0000,0000,,<i>..và trong một đêm ở nhà tang lễ...</i>
Dialogue: 0,0:00:19.21,0:00:24.31,Default,,0000,0000,0000,,Không! Onii-chan... Anh tàn nhẫn lắm...
Dialogue: 0,0:00:27.19,0:00:28.52,Default,,0000,0000,0000,,<i>Nhưng mặc khác...</i>
Dialogue: 0,0:00:28.69,0:00:32.82,Default,,0000,0000,0000,,<i>...Có lẽ tôi có tình cảm với Onii-chan như là với một người đàn ông...</i>
Dialogue: 0,0:00:34.13,0:00:40.76,Default,,0000,0000,0000,,<i>Trong lúc bị cưỡng hiếp, tôi nhận ra rằng tôi vô cùng vui sướng...</i>
Dialogue: 0,0:00:48.51,0:00:50.77,Default,,0000,0000,0000,,<i>Theo lời của Onii-chan...</i>
Dialogue: 0,0:00:51.35,0:00:53.25,Default,,0000,0000,0000,,<i>...tôi chuyển đến trường nội trú...</i>
Dialogue: 0,0:00:53.62,0:00:56.31,Default,,0000,0000,0000,,<i>...nơi Onii-chan làm giáo viên.</i>
Dialogue: 0,0:01:02.82,0:01:05.66,Default,,0000,0000,0000,,<i>Onii-chan không chịu nói gì với tôi cả...</i>
Dialogue: 0,0:01:06.03,0:01:09.66,Default,,0000,0000,0000,,<i>...nhưng chắc chắn anh ấy có lý do để đến đây.</i>
Dialogue: 0,0:01:10.60,0:01:12.59,Default,,0000,0000,0000,,<i>Có lẽ đó là một bí mật đen tối...</i>
Dialogue: 0,0:02:05.95,0:02:07.58,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:02:08.69,0:02:12.99,Default,,0000,0000,0000,,Nếu em rên lớn như vậy, thành viên của đội hợp xướng bên cạnh sẽ nghe thấy mất.
Dialogue: 0,0:02:13.76,0:02:16.42,Default,,0000,0000,0000,,Không sao đây. Họ toàn là những tiểu thư con nhà giàu.
Dialogue: 0,0:02:17.00,0:02:19.26,Default,,0000,0000,0000,,Dù có nghe họ cũng không biết gì đâu.
Dialogue: 0,0:02:19.90,0:02:21.16,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:02:47.66,0:02:53.60,Default,,0000,0000,0000,,Em muốn nó... Em muốn thằng nhỏ cứng cáp của thầy. Em muốn nó đút vào bên trong em.
Dialogue: 0,0:02:54.60,0:02:57.50,Default,,0000,0000,0000,,Vẫn chưa đâu. Em phải ra với thứ này trước cái đã.
Dialogue: 0,0:02:58.14,0:03:01.37,Default,,0000,0000,0000,,Vậy thì cứ để nó ở đấy. Em muốn thầy làm tình với hậu môn của em.
Dialogue: 0,0:03:02.08,0:03:02.87,Default,,0000,0000,0000,,Vậy ư.
Dialogue: 0,0:03:05.78,0:03:08.65,Default,,0000,0000,0000,,Làm ơn... Hãy nhanh trao nó cho em.
Dialogue: 0,0:03:09.18,0:03:12.05,Default,,0000,0000,0000,,Rồi, tôi đút nó vào ngay đây.
Dialogue: 0,0:03:47.99,0:03:54.49,Default,,0000,0000,0000,,Nữa đi... Có đau em cũng mặc kệ. Làm ơn! Mạnh nữa! Hãy làm mạnh hơn nữa!!
Dialogue: 0,0:03:54.60,0:03:59.06,Default,,0000,0000,0000,,Không ngờ tên khốn đó lại huấn luyện ra một con điếm như vầy.
Dialogue: 0,0:04:00.10,0:04:02.63,Default,,0000,0000,0000,,Đúng như em mong muốn nhé...
Dialogue: 0,0:04:30.33,0:04:34.29,Default,,0000,0000,0000,,Thật không thể tin là em lại bú nó sau khi nó đã ở trong hậu môn của em.
Dialogue: 0,0:04:35.74,0:04:38.50,Default,,0000,0000,0000,,Hắn ta đã dạy em như vậy ư?
Dialogue: 0,0:04:41.11,0:04:43.81,Default,,0000,0000,0000,,Em đã có quyết định về việc trả thù chưa?
Dialogue: 0,0:04:45.61,0:04:46.58,Default,,0000,0000,0000,,Thế nào hả?
Dialogue: 0,0:04:58.03,0:04:58.75,Default,,0000,0000,0000,,Thế nào hả?!
Dialogue: 0,0:05:06.87,0:05:08.50,Default,,0000,0000,0000,,Em bằng lòng với nó ư?
Dialogue: 0,0:05:09.04,0:05:12.40,Default,,0000,0000,0000,,Nổi đau bị em bị hắn ta chơi đâu rồi?
Dialogue: 0,0:05:16.54,0:05:18.01,Default,,0000,0000,0000,,Nếu thầy lấy được...
Dialogue: 0,0:05:18.61,0:05:19.10,Default,,0000,0000,0000,,Lấy cái gì?
Dialogue: 0,0:05:20.38,0:05:23.15,Default,,0000,0000,0000,,Nếu thầy lấy được nó, ông ấy coi như xong...
Dialogue: 0,0:05:23.72,0:05:27.28,Default,,0000,0000,0000,,Nó là gì? Nói cho tôi biết. Làm sao tôi mới lấy được nó?
Dialogue: 0,0:05:28.92,0:05:34.79,Default,,0000,0000,0000,,Khi nhìn vào lịch trình của ông ấy. \NAnh sẽ thấy một khoảng thời gian trống...
Dialogue: 0,0:05:35.23,0:05:39.67,Default,,0000,0000,0000,,...do cuộc bầu cử quá căng thẳng nên có thể ông ta sẽ ngủ lại văn phòng.
Dialogue: 0,0:05:41.27,0:05:42.93,Default,,0000,0000,0000,,Đó là cơ hội của chúng ta.
Dialogue: 0,0:06:02.86,0:06:05.83,Default,,0000,0000,0000,,Tại sao? Các cô lại làm thế với tôi?
Dialogue: 0,0:06:07.23,0:06:09.75,Default,,0000,0000,0000,,Bởi vì cô chướng mắt quá!
Dialogue: 0,0:06:10.40,0:06:14.99,Default,,0000,0000,0000,,Cô nghĩ mình là em gái của Sensei là ngon lắm sao?
Dialogue: 0,0:06:16.40,0:06:20.20,Default,,0000,0000,0000,,Mùi của cô hôi quá! Đặc biệt là ở đây!
Dialogue: 0,0:06:22.38,0:06:25.04,Default,,0000,0000,0000,,Vừa nãy là cô rên lên vì sung sướng ư?
Dialogue: 0,0:06:25.75,0:06:28.11,Default,,0000,0000,0000,,Có lẽ vậy. Thử lại xem.
Dialogue: 0,0:06:28.38,0:06:29.37,Default,,0000,0000,0000,,Để coi nào.
Dialogue: 0,0:06:33.09,0:06:34.25,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:06:34.82,0:06:36.76,Default,,0000,0000,0000,,Đau ư? Đừng có xạo.
Dialogue: 0,0:06:37.19,0:06:38.92,Default,,0000,0000,0000,,Đầu nó vào rồi này.
Dialogue: 0,0:06:39.43,0:06:42.23,Default,,0000,0000,0000,,Đút vào nữa đi. Hãy tiếp đãi cô ta nào.
Dialogue: 0,0:06:42.46,0:06:43.45,Default,,0000,0000,0000,,Phải.
Dialogue: 0,0:06:49.27,0:06:53.30,Default,,0000,0000,0000,,Hãy tha cho tôi. Tôi đã làm gì các cô cơ chứ?
Dialogue: 0,0:06:57.55,0:06:58.88,Default,,0000,0000,0000,,Nhìn này!
Dialogue: 0,0:06:59.05,0:06:59.71,Default,,0000,0000,0000,,Để xem!
Dialogue: 0,0:07:00.08,0:07:02.28,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:07:02.45,0:07:03.97,Default,,0000,0000,0000,,Cho chị xem nữa!
Dialogue: 0,0:07:04.65,0:07:07.59,Default,,0000,0000,0000,,Nè, gửi bức ảnh này cho ai đó nhé.
Dialogue: 0,0:07:08.39,0:07:10.72,Default,,0000,0000,0000,,Ý kiến hay đấy. Mọi người sẽ vui lắm.
Dialogue: 0,0:07:11.06,0:07:14.83,Default,,0000,0000,0000,,Không! Đừng làm chuyện kinh khủng như vậy! Tôi xin các cô!
Dialogue: 0,0:07:15.10,0:07:16.69,Default,,0000,0000,0000,,Chúng ta nên làm gì đây?
Dialogue: 0,0:07:22.17,0:07:23.23,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:07:25.61,0:07:26.90,Default,,0000,0000,0000,,Lại là các cô à...
Dialogue: 0,0:07:27.08,0:07:29.20,Default,,0000,0000,0000,,Thầy định mang chúng tôi ra trong buổi hợp ư?
Dialogue: 0,0:07:29.54,0:07:33.11,Default,,0000,0000,0000,,Nếu thế thì thầy hãy nghĩ lại đi.
Dialogue: 0,0:07:33.48,0:07:36.54,Default,,0000,0000,0000,,Tôi sẽ gửi bức ảnh đáng xấu hổ này cho tất cả mọi người.
Dialogue: 0,0:07:36.85,0:07:39.18,Default,,0000,0000,0000,,Nó sẽ có ở khắc mọi nơi. Thầy có muốn thế không hả?
Dialogue: 0,0:07:39.82,0:07:42.29,Default,,0000,0000,0000,,Onii-chan! Ngăn họ lại đi!
Dialogue: 0,0:07:42.46,0:07:44.52,Default,,0000,0000,0000,,Thầy muốn thế nào hả, Sensei?
Dialogue: 0,0:07:44.79,0:07:45.55,Default,,0000,0000,0000,,Cứ tự nhiên.
Dialogue: 0,0:07:46.19,0:07:47.22,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:07:51.13,0:07:56.80,Default,,0000,0000,0000,,Dù đó em ấy là em gái tôi, nhưng nó chả liên quan gì đến tôi cả.
Dialogue: 0,0:07:57.41,0:07:58.84,Default,,0000,0000,0000,,Nói thẳng ra là tôi mặc kệ.
Dialogue: 0,0:07:59.57,0:08:00.84,Default,,0000,0000,0000,,Không thể nào...
Dialogue: 0,0:08:03.61,0:08:07.95,Default,,0000,0000,0000,,Không cần phải ngại. Tôi sẽ tống tiền bố mẹ các cô để tôi giữ im lặng.
Dialogue: 0,0:08:08.45,0:08:10.44,Default,,0000,0000,0000,,Đó là điều kẻ thông minh nên làm.
Dialogue: 0,0:08:11.49,0:08:12.95,Default,,0000,0000,0000,,Không được làm thế!
Dialogue: 0,0:08:13.19,0:08:15.59,Default,,0000,0000,0000,,Nếu bố mẹ chúng tôi phát hiện thì nguy mất.
Dialogue: 0,0:08:16.02,0:08:20.36,Default,,0000,0000,0000,,Làm ơn! Chúng tôi sẽ không tái phạm nữa, nên hãy tha cho chúng tôi lần này!
Dialogue: 0,0:08:21.83,0:08:24.16,Default,,0000,0000,0000,,Để xem... tôi phải làm sao đây?
Dialogue: 0,0:08:31.14,0:08:33.07,Default,,0000,0000,0000,,Thôi nào, làm đi chứ.
Dialogue: 0,0:08:42.02,0:08:44.78,Default,,0000,0000,0000,,Đây là cuộc thi ai là người giỏi nhất.
Dialogue: 0,0:09:09.81,0:09:11.71,Default,,0000,0000,0000,,Thật là một cảnh tượng tuyệt vời.
Dialogue: 0,0:09:17.15,0:09:19.95,Default,,0000,0000,0000,,Đây là lần đầu tiên tôi cân ba đấy.
Dialogue: 0,0:09:20.39,0:09:22.55,Default,,0000,0000,0000,,Tôi sẽ tha hồ hưởng thụ.
Dialogue: 0,0:10:07.97,0:10:10.40,Default,,0000,0000,0000,,Đau quá... Đau quá...
Dialogue: 0,0:10:12.41,0:10:16.14,Default,,0000,0000,0000,,Làm ơn... Dừng lại đi...
Dialogue: 0,0:10:22.05,0:10:25.75,Default,,0000,0000,0000,,Không được... Tôi sẽ chết mất...
Dialogue: 0,0:11:04.29,0:11:06.89,Default,,0000,0000,0000,,Sao thế? Chúng ta chỉ mới bắt đầu thôi mà.
Dialogue: 0,0:11:08.30,0:11:09.92,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:11:10.16,0:11:11.00,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:11:11.00,0:11:11.66,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:11:12.83,0:11:14.99,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:11:22.78,0:11:26.27,Default,,0000,0000,0000,,Em sẽ không cần phải sợ bọn họ nữa. Biết rồi chứ?
Dialogue: 0,0:11:28.55,0:11:34.65,Default,,0000,0000,0000,,Cảm ơn anh đã cứu em. Nhưng anh cũng đâu cần phải làm như vậy...
Dialogue: 0,0:11:34.79,0:11:40.32,Default,,0000,0000,0000,,Đừng có nhầm lẫn. Anh làm không phải vì em mà là vì anh thích.
Dialogue: 0,0:11:41.10,0:11:44.59,Default,,0000,0000,0000,,Đâu phải lúc nào cũng được cân ba như thế.
Dialogue: 0,0:11:45.87,0:11:48.53,Default,,0000,0000,0000,,Cảm ơn em đã tạo cơ hội cho anh.
Dialogue: 0,0:11:51.24,0:11:54.44,Default,,0000,0000,0000,,Có việc này anh cần nhờ em.
Dialogue: 0,0:11:58.28,0:11:59.58,Default,,0000,0000,0000,,<i>Hỡi công dân của khu phố.</i>
Dialogue: 0,0:11:59.71,0:12:01.41,Default,,0000,0000,0000,,<i>Chúng tôi xin lỗi vì bản thông báo này.</i>
Dialogue: 0,0:12:01.82,0:12:06.25,Default,,0000,0000,0000,,<i>Hãy bỏ phiếu cho Takamine Daizaburo! Hãy bỏ phiếu cho Takamine Daizaburo!</i>
Dialogue: 0,0:12:06.99,0:12:12.95,Default,,0000,0000,0000,,<i>Cuộc bầu cử sắp đến gấn. Hãy bỏ phiếu cho Takamine Daizaburo!</i>
Dialogue: 0,0:12:13.69,0:12:16.36,Default,,0000,0000,0000,,<i>Takamine Daizaburo sẽ làm hết mình vì bạn.</i>
Dialogue: 0,0:12:17.20,0:12:26.44,Default,,0000,0000,0000,,<i>Hãy bỏ phiếu cho Takamine Daizaburo! Hãy bỏ phiếu cho Takamine Daizaburo!</i>
Dialogue: 0,0:12:37.35,0:12:38.01,Default,,0000,0000,0000,,Onii-...
Dialogue: 0,0:13:10.02,0:13:10.71,Default,,0000,0000,0000,,Onii-chan...
Dialogue: 0,0:13:12.12,0:13:16.65,Default,,0000,0000,0000,,Anh thích những cô gái xinh đẹp à?
Dialogue: 0,0:13:17.66,0:13:19.68,Default,,0000,0000,0000,,Đẹp xấu không quan trọng.
Dialogue: 0,0:13:19.99,0:13:20.93,Default,,0000,0000,0000,,Vậy ư...
Dialogue: 0,0:13:21.86,0:13:23.96,Default,,0000,0000,0000,,Hôm nay em làm tốt lắm.
Dialogue: 0,0:13:24.63,0:13:25.39,Default,,0000,0000,0000,,Thật không?
Dialogue: 0,0:13:25.57,0:13:29.70,Default,,0000,0000,0000,,Ừm. Em xứng đáng được thưởng.
Dialogue: 0,0:14:05.87,0:14:07.43,Default,,0000,0000,0000,,Onii-chan. Làm em đi!
Dialogue: 0,0:14:50.62,0:14:52.21,Default,,0000,0000,0000,,Onii-chan!
Dialogue: 0,0:15:32.53,0:15:33.55,Default,,0000,0000,0000,,<i>Em gái ư?</i>
Dialogue: 0,0:16:25.48,0:16:31.98,Default,,0000,0000,0000,,Banzai! Banzai! Banzai! Banzai!
Dialogue: 0,0:16:32.09,0:16:33.89,Default,,0000,0000,0000,,{\pos(309,40)\c&H123834&\3c&H83BEB7&}Học viện Takamine
Dialogue: 0,0:16:33.89,0:16:36.58,Default,,0000,0000,0000,,Hiệu trưởng, chúc mừng ngài đắc cử.
Dialogue: 0,0:16:37.59,0:16:40.39,Default,,0000,0000,0000,,Mong ngài quan tâm đến sự nghiệp giáo dục!
Dialogue: 0,0:16:41.13,0:16:45.96,Default,,0000,0000,0000,,Cảm ơn mọi người. Nhờ mọi người chăm lo cho ngôi trường này trong khi ta đi vắng...
Dialogue: 0,0:16:48.24,0:16:49.29,Default,,0000,0000,0000,,Elena đâu rồi?
Dialogue: 0,0:16:49.74,0:16:53.17,Default,,0000,0000,0000,,Lúc sáng tôi còn thấy em ấy, nhưng bây giờ thì...
Dialogue: 0,0:16:55.16,0:16:59.00,Default,,0000,0000,0000,,{\pos(329,40)\c&H563540&\3c&HB2867B&\4c&HE1D4D3&}Phòng hiệu trưởng
Dialogue: 0,0:16:59.95,0:17:01.64,Default,,0000,0000,0000,,Nó đã đi đâu vậy kìa?
Dialogue: 0,0:17:08.76,0:17:09.81,Default,,0000,0000,0000,,Kano-sensei?
Dialogue: 0,0:17:11.43,0:17:14.72,Default,,0000,0000,0000,,Tôi cần nói chuyện riêng với thầy.
Dialogue: 0,0:17:19.20,0:17:21.63,Default,,0000,0000,0000,,Cậu muốn nói gì với ta?
Dialogue: 0,0:17:30.04,0:17:31.53,Default,,0000,0000,0000,,...Elena!
Dialogue: 0,0:17:35.42,0:17:37.48,Default,,0000,0000,0000,,Như vầy là có ý gì đây?
Dialogue: 0,0:17:38.59,0:17:41.99,Default,,0000,0000,0000,,Mười năm trước, có một người đàn ông dính vào một món nợ khổng lồ...
Dialogue: 0,0:17:42.39,0:17:45.12,Default,,0000,0000,0000,,...vì đã vai tiền bọn cho vay nặng lãi.
Dialogue: 0,0:17:45.99,0:17:49.72,Default,,0000,0000,0000,,Cuối cùng, tên cho vay đó đã có được ngôi trường...
Dialogue: 0,0:17:50.20,0:17:53.00,Default,,0000,0000,0000,,...gia đình của người đàn ông đó trở nên điêu đứng.
Dialogue: 0,0:17:57.50,0:18:00.67,Default,,0000,0000,0000,,Vợ của người đàn ông đó cũng khá là đẹp.
Dialogue: 0,0:18:01.41,0:18:03.74,Default,,0000,0000,0000,,Tên cho vay đó đồng ý xóa nợ nếu ông ta...
Dialogue: 0,0:18:04.08,0:18:06.38,Default,,0000,0000,0000,,...để hắn chơi vợ của mình...
Dialogue: 0,0:18:07.15,0:18:09.98,Default,,0000,0000,0000,,...hết lần này đến lần khác không chút thương xót.
Dialogue: 0,0:18:11.85,0:18:16.15,Default,,0000,0000,0000,,Không! Dừng lại đi! Làm ơn!
Dialogue: 0,0:18:18.06,0:18:20.82,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:18:30.90,0:18:34.03,Default,,0000,0000,0000,,Em không hề biết là bố lại làm như vậy...
Dialogue: 0,0:18:34.88,0:18:35.84,Default,,0000,0000,0000,,Không chỉ có vậy đâu...
Dialogue: 0,0:18:38.68,0:18:39.74,Default,,0000,0000,0000,,Tha lỗi cho anh...
Dialogue: 0,0:18:41.25,0:18:45.05,Default,,0000,0000,0000,,Chỉ cần hắn ta còn hứng thú với em thì chúng ta sẽ không sao.
Dialogue: 0,0:18:45.72,0:18:47.05,Default,,0000,0000,0000,,Nếu em không xinh đẹp...
Dialogue: 0,0:18:47.39,0:18:49.32,Default,,0000,0000,0000,,...thì có lẽ chúng ta đã chết rồi.
Dialogue: 0,0:18:49.96,0:18:53.95,Default,,0000,0000,0000,,Nhưng em hãy cố chịu đựng, vì tương lai chúng ta!
Dialogue: 0,0:18:55.63,0:18:59.36,Default,,0000,0000,0000,,Ngày hôm sau, người phụ nữ đó đã nhảy lầu tự sát.
Dialogue: 0,0:19:04.37,0:19:09.31,Default,,0000,0000,0000,,Ông ta bị con trai mình căm ghét... Và rồi do lái xe bất cẩn...
Dialogue: 0,0:19:09.44,0:19:11.91,Default,,0000,0000,0000,,...ông ta đã chết cùng với người vợ sau của mình.
Dialogue: 0,0:19:14.98,0:19:20.01,Default,,0000,0000,0000,,Đứa con không hề đổ lỗi cái chết của mẹ mình lên người cha...
Dialogue: 0,0:19:20.92,0:19:24.82,Default,,0000,0000,0000,,Sự tồn tại của ông ta chả có nghĩ lý gì với tôi.
Dialogue: 0,0:19:27.03,0:19:29.72,Default,,0000,0000,0000,,Người duy nhất tôi muốn báo thù chính là...
Dialogue: 0,0:19:30.33,0:19:31.52,Default,,0000,0000,0000,,...tên cho vay nặng lãi đó!
Dialogue: 0,0:19:33.03,0:19:35.70,Default,,0000,0000,0000,,Nhưng sau khi hắn có được ngôi trường...
Dialogue: 0,0:19:36.00,0:19:39.23,Default,,0000,0000,0000,,...hắn bỏ nghề cũ và trở thành một nhà giáo.
Dialogue: 0,0:19:40.54,0:19:44.38,Default,,0000,0000,0000,,Bây giờ hắn ta lại là một nhà chính trị... Phải...
Dialogue: 0,0:19:44.98,0:19:47.28,Default,,0000,0000,0000,,Tôi đang nói ông đó, Takamine Daizaburo!
Dialogue: 0,0:19:54.12,0:19:58.49,Default,,0000,0000,0000,,Trả thù ư? Đó là lý do cậu làm thế với Elena ư?
Dialogue: 0,0:20:04.53,0:20:05.26,Default,,0000,0000,0000,,Đó là gì vậy?
Dialogue: 0,0:20:05.93,0:20:08.93,Default,,0000,0000,0000,,Ông làm việc cũng có tổ chức ghê, \Nđưa ra những lời khuyên đúng đắng.
Dialogue: 0,0:20:09.37,0:20:13.20,Default,,0000,0000,0000,,Gây dựng một hình tượng tốt. Nhưng không thể tin ông lại...
Dialogue: 0,0:20:13.34,0:20:17.40,Default,,0000,0000,0000,,...làm thế với cháu gái của mình, thật là...
Dialogue: 0,0:20:18.91,0:20:22.58,Default,,0000,0000,0000,,Đây là những cảnh quan hệ điên cuồng do ông tự tay ghi lại đấy.
Dialogue: 0,0:20:25.05,0:20:29.35,Default,,0000,0000,0000,,Đây là hiểu lầm thôi! Ta không hề làm gì Shirone cả!
Dialogue: 0,0:20:31.63,0:20:33.46,Default,,0000,0000,0000,,Đây chỉ là một phần bộ sưu tập của ông thôi...
Dialogue: 0,0:20:34.09,0:20:38.26,Default,,0000,0000,0000,,...Nhưng nhiêu đây cũng đủ tố cáo ông rồi.
Dialogue: 0,0:20:40.90,0:20:44.84,Default,,0000,0000,0000,,Ta... Ta phải làm gì cậu mới chịu tha cho ta?
Dialogue: 0,0:20:45.07,0:20:50.14,Default,,0000,0000,0000,,Trước tiên hãy trả ngồi trường quý báo của mẹ tôi lại cho tôi.
Dialogue: 0,0:20:51.01,0:20:52.17,Default,,0000,0000,0000,,Cái đó...
Dialogue: 0,0:20:53.01,0:20:54.61,Default,,0000,0000,0000,,Ông chả còn lựa chọn nào khác đâu.
Dialogue: 0,0:20:56.88,0:20:57.71,Default,,0000,0000,0000,,Được rồi.
Dialogue: 0,0:20:59.25,0:21:05.25,Default,,0000,0000,0000,,Ta đồng ý, nên hãy cởi trói cho Elena. Con bé không làm gì sai cả.
Dialogue: 0,0:21:05.83,0:21:06.62,Default,,0000,0000,0000,,Được thôi.
Dialogue: 0,0:21:07.16,0:21:10.65,Default,,0000,0000,0000,,Giờ ông đã không thể trở thành một chính trị gia.
Dialogue: 0,0:21:15.60,0:21:16.90,Default,,0000,0000,0000,,Để xem nào...
Dialogue: 0,0:21:18.51,0:21:22.24,Default,,0000,0000,0000,,Cậu muốn đấm ta ư? Được rồi... Cứ đấm ta đi.
Dialogue: 0,0:21:31.45,0:21:31.94,Default,,0000,0000,0000,,Bú nó!
Dialogue: 0,0:21:33.72,0:21:35.15,Default,,0000,0000,0000,,Tôi bảo cô bú nó!
Dialogue: 0,0:21:35.99,0:21:38.58,Default,,0000,0000,0000,,Cậu đã hứa sẽ để con gái ta đi mà!
Dialogue: 0,0:21:39.59,0:21:42.03,Default,,0000,0000,0000,,Tôi chỉ hứa cởi trói cho cô ta thôi.
Dialogue: 0,0:21:42.83,0:21:44.85,Default,,0000,0000,0000,,Thế nào? Cô không muốn ư?
Dialogue: 0,0:21:45.67,0:21:49.30,Default,,0000,0000,0000,,Nếu cô không làm thì sẽ có chuyện còn tồi tệ hơn xảy ra đó.
Dialogue: 0,0:21:50.07,0:21:54.06,Default,,0000,0000,0000,,Nếu bí mật của bố cô bị phanh phui, \Ncuộc sống của cô sẽ hoàn toàn bị đảo lộn.
Dialogue: 0,0:21:55.61,0:21:57.70,Default,,0000,0000,0000,,Dừng lại ngay! Đừng có giỡn mặt ta!
Dialogue: 0,0:21:58.48,0:21:59.10,Default,,0000,0000,0000,,Thế nào?
Dialogue: 0,0:22:08.49,0:22:12.29,Default,,0000,0000,0000,,Được. Cứ như vậy... Tôi còn rất nhiều trò vui.
Dialogue: 0,0:22:16.60,0:22:20.76,Default,,0000,0000,0000,,Đây không phải là cưỡng hiếp. Mà là do cô ấy tự nguyện.
Dialogue: 0,0:22:21.70,0:22:24.47,Default,,0000,0000,0000,,Không phải giống như những gì ông đã làm với mẹ tôi sao?
Dialogue: 0,0:22:34.18,0:22:38.14,Default,,0000,0000,0000,,Dùng môi vào lưỡi của cô để liếm nó.
Dialogue: 0,0:22:39.89,0:22:41.88,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:22:49.93,0:22:51.92,Default,,0000,0000,0000,,Đúng rồi, tôi có băng quan hệ của ông mà...
Dialogue: 0,0:22:52.23,0:22:58.50,Default,,0000,0000,0000,,Tôi có thể dùng nó mỗi ngày, \Nđể luyện tập... cùng với con gái của ông.
Dialogue: 0,0:23:02.74,0:23:03.71,Default,,0000,0000,0000,,Phải...
Dialogue: 0,0:23:08.85,0:23:10.32,Default,,0000,0000,0000,,Không được nhả ra. Nuốt hết đi.
Dialogue: 0,0:23:16.69,0:23:20.56,Default,,0000,0000,0000,,Sướng quá! Lần đầu tiên tôi được sướng như vầy đó.
Dialogue: 0,0:23:26.27,0:23:30.40,Default,,0000,0000,0000,,Hiệu trưởng, con gái của ông đẹp thật đấy.
Dialogue: 0,0:23:31.54,0:23:33.67,Default,,0000,0000,0000,,Chỉ nhìn thôi mà tôi cương lên vậy rồi.
Dialogue: 0,0:23:34.14,0:23:36.01,Default,,0000,0000,0000,,Tôi có thể ra hoài không hết...
Dialogue: 0,0:23:48.76,0:23:52.92,Default,,0000,0000,0000,,Bố ơi... Cứu con... Bố ơi!
Dialogue: 0,0:23:54.66,0:23:58.50,Default,,0000,0000,0000,,Cứu ư? Đó cũng chính là những gì mẹ tôi và Shirone đã van xin đó.
Dialogue: 0,0:24:14.95,0:24:18.58,Default,,0000,0000,0000,,Thế nào hả hiệu trưởng? Ông có thấy hậu môn của con gái mình không?
Dialogue: 0,0:24:21.96,0:24:24.82,Default,,0000,0000,0000,,Bố ơi! Không được nhìn!
Dialogue: 0,0:25:02.16,0:25:03.93,Default,,0000,0000,0000,,Sướng quá...
Dialogue: 0,0:25:27.75,0:25:32.62,Default,,0000,0000,0000,,Bố ơi... Con xin lỗi... Con... Con...
Dialogue: 0,0:26:06.09,0:26:10.86,Default,,0000,0000,0000,,<i>Onii-chan đã có người đẹp như Elena rồi nên anh ấy sẽ không cần mình nữa.</i>
Dialogue: 0,0:26:11.63,0:26:17.76,Default,,0000,0000,0000,,<i>Anh ấy nhất định sẽ không làm tình với mình nữa... Nhất định là vậy...</i>
Dialogue: 0,0:26:31.35,0:26:36.22,Default,,0000,0000,0000,,<i>Thật là lạ. Mình đang sung sướng như vầy nhưng nó thật trống rỗng.</i>
Dialogue: 0,0:26:37.82,0:26:40.19,Default,,0000,0000,0000,,<i>Mình đang chơi một người đẹp như vầy nhưng...</i>
Dialogue: 0,0:27:12.43,0:27:15.59,Default,,0000,0000,0000,,Vĩnh biệt anh, Onii-chan...
Dialogue: 0,0:27:26.17,0:27:27.76,Default,,0000,0000,0000,,<i>Mình đã trả thù xong.</i>
Dialogue: 0,0:27:28.74,0:27:35.88,Default,,0000,0000,0000,,<i>Bây giờ mình cần phải chăm sóc cho Noelle và trở lại cuộc sống vốn có của mình.</i>
Dialogue: 0,0:27:39.39,0:27:41.68,Default,,0000,0000,0000,,<i>Bây giờ chúng ta có thể sống như anh em...</i>
Dialogue: 0,0:27:42.82,0:27:47.95,Default,,0000,0000,0000,,<i>Không, chúng ta sẽ bắt đầu một mối quan hệ nam nữ.</i>
Dialogue: 0,0:27:52.30,0:27:52.89,Default,,0000,0000,0000,,<i>Noelle...</i>
Dialogue: 0,0:27:52.89,0:27:54.89,Default,,0000,0000,0000,,
