Dialogue: 0,0:00:11.40,0:00:18.50,Default,,0000,0000,0000,,Một ác quỉ xấu xa như mi lại dám phá hoại luật lệ của\Nvũ trụ và làm rối loạn trật tự của thế giới.
Dialogue: 0,0:00:20.11,0:00:23.87,Default,,0000,0000,0000,,Mặc dù tội ác của ngươi là ko thể tha thứ...
Dialogue: 0,0:00:25.11,0:00:31.18,Default,,0000,0000,0000,,...Nhưng Thượng đế nhân từ vẫn sẽ tha thứ cho ngươi.
Dialogue: 0,0:00:33.75,0:00:41.29,Default,,0000,0000,0000,,Chúng ta, những người con của Thượng đế sẽ\Nsẽ thanh tẩy linh hồn tội lỗi của ngươi.
Dialogue: 0,0:00:43.56,0:00:49.66,Default,,0000,0000,0000,,Tất cả mọi người, hãy thanh tẩy con ác quỉ này.
Dialogue: 0,0:00:58.78,0:01:01.27,Default,,0000,0000,0000,,Với sức mạnh thần thánh của chúng ta!
Dialogue: 0,0:01:06.19,0:01:08.15,Default,,0000,0000,0000,,C-Cái gì?
Dialogue: 0,0:01:11.46,0:01:15.79,Default,,0000,0000,0000,,VIPER - GTS -\NÁc quỉ chinh phục Thiên đường.
Dialogue: 0,0:01:22.37,0:01:25.17,Default,,0000,0000,0000,,Ôi chúa ơi!
Dialogue: 0,0:01:26.37,0:01:28.60,Default,,0000,0000,0000,,Carrera đã bị bắt đi!
Dialogue: 0,0:01:29.08,0:01:31.01,Default,,0000,0000,0000,,Mình phải làm gì bây giờ?
Dialogue: 0,0:01:31.58,0:01:32.64,Default,,0000,0000,0000,,Mình phải làm gì bây giờ?
Dialogue: 0,0:01:34.18,0:01:36.05,Default,,0000,0000,0000,,A, mình có ý này!
Dialogue: 0,0:01:51.93,0:01:53.76,Default,,0000,0000,0000,,Cám ơn anh vì đã triệu hồi.
Dialogue: 0,0:01:54.10,0:01:57.83,Default,,0000,0000,0000,,Tôi là Rati đến từ Địa ngục.
Dialogue: 0,0:01:58.31,0:02:00.67,Default,,0000,0000,0000,,Cô bé cũng là ác quỉ à?
Dialogue: 0,0:02:00.77,0:02:01.83,Default,,0000,0000,0000,,Vâng ạ!
Dialogue: 0,0:02:02.41,0:02:04.90,Default,,0000,0000,0000,,Chắc hẳn phải có rất nhiều nữ ác quỉ ở địa ngục.
Dialogue: 0,0:02:05.65,0:02:07.84,Default,,0000,0000,0000,,Cả cô bé này và Carrera đều là nữ.
Dialogue: 0,0:02:07.95,0:02:11.25,Default,,0000,0000,0000,,Anh biết chị Carrera à?
Dialogue: 0,0:02:11.59,0:02:13.95,Default,,0000,0000,0000,,Thế thì, tôi sẽ phục vụ anh tốt hơn những người khác.
Dialogue: 0,0:02:14.19,0:02:15.85,Default,,0000,0000,0000,,Hãy cho tôi biết mong ước của anh.
Dialogue: 0,0:02:16.16,0:02:17.65,Default,,0000,0000,0000,,Mong ước của tôi á?
Dialogue: 0,0:02:17.99,0:02:19.72,Default,,0000,0000,0000,,Để xem nào...
Dialogue: 0,0:02:21.06,0:02:22.69,Default,,0000,0000,0000,,Ấy ko, ko phải về mong ước của tôi!
Dialogue: 0,0:02:24.73,0:02:25.82,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:02:25.93,0:02:29.60,Default,,0000,0000,0000,,Chị Carrera đã bị bắt đi bởi các thiên thần?
Dialogue: 0,0:02:29.87,0:02:32.10,Default,,0000,0000,0000,,Ko, ko thể nào.
Dialogue: 0,0:02:32.24,0:02:34.04,Default,,0000,0000,0000,,Ko thể như vậy được!
Dialogue: 0,0:02:34.17,0:02:34.88,Default,,0000,0000,0000,,Ko thể như vậy được!
Dialogue: 0,0:02:34.88,0:02:37.50,Default,,0000,0000,0000,,Nghe này, tôi nghĩ chúng ta cần giải cứu cô ấy...\NKo thể như vậy được!
Dialogue: 0,0:02:37.61,0:02:38.87,Default,,0000,0000,0000,,Được rồi!
Dialogue: 0,0:02:39.48,0:02:41.68,Default,,0000,0000,0000,,Tôi sẽ đi cứu chị Carrera.
Dialogue: 0,0:02:42.15,0:02:43.62,Default,,0000,0000,0000,,Ko nên lãng phí thời gian nữa!
Dialogue: 0,0:02:52.73,0:02:54.28,Default,,0000,0000,0000,,Cô ta đi rồi.
Dialogue: 0,0:02:54.80,0:02:56.56,Default,,0000,0000,0000,,Nhưng cô bé này trông có vẻ ko đáng tin cậy lắm...
Dialogue: 0,0:02:57.83,0:02:58.59,Default,,0000,0000,0000,,Thử lần nữa nào.
Dialogue: 0,0:03:03.14,0:03:06.13,Default,,0000,0000,0000,,Tôi là Alphina, một trong các sứ giả của Địa ngục.
Dialogue: 0,0:03:06.67,0:03:09.84,Default,,0000,0000,0000,,Vì sao cậu lại triệu hồi tôi tới đây?
Dialogue: 0,0:03:10.11,0:03:11.41,Default,,0000,0000,0000,,A, một ác quỉ nam...
Dialogue: 0,0:03:11.65,0:03:14.05,Default,,0000,0000,0000,,Nam ác quỉ thì có vấn đề gì à?
Dialogue: 0,0:03:14.21,0:03:14.81,Default,,0000,0000,0000,,Sao?
Dialogue: 0,0:03:14.95,0:03:16.85,Default,,0000,0000,0000,,À ko ý tôi là...
Dialogue: 0,0:03:17.35,0:03:19.84,Default,,0000,0000,0000,,Tôi muốn anh giải cứu Carrera.
Dialogue: 0,0:03:21.19,0:03:23.66,Default,,0000,0000,0000,,Giải cứu Carrera?
Dialogue: 0,0:03:32.47,0:03:36.53,Default,,0000,0000,0000,,Chị Carrera, chị ở đâu?
Dialogue: 0,0:03:47.75,0:03:49.58,Default,,0000,0000,0000,,Các ngươi dám chặn đường ta ư?
Dialogue: 0,0:03:50.12,0:03:52.85,Default,,0000,0000,0000,,Thế thì, ta sẽ quăng tất cả bọn ngươi xuống Địa ngục.
Dialogue: 0,0:03:53.09,0:03:55.08,Default,,0000,0000,0000,,Tới đây nào!
Dialogue: 0,0:04:01.13,0:04:02.29,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:04:03.36,0:04:08.32,Default,,0000,0000,0000,,Các thiên thần đã bắt\NCarrera, và Rati đã đuổi theo bọn họ.
Dialogue: 0,0:04:08.84,0:04:09.67,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:04:11.17,0:04:13.00,Default,,0000,0000,0000,,Vậy thì, tôi sẽ buộc phải ra tay thôi.
Dialogue: 0,0:04:13.74,0:04:17.84,Default,,0000,0000,0000,,Cám ơn vì đã báo cho tôi biết. Bây giờ tôi sẽ lo việc này.
Dialogue: 0,0:04:18.35,0:04:21.78,Default,,0000,0000,0000,,Đợi đã. Hãy để tôi giúp anh một tay.
Dialogue: 0,0:04:21.92,0:04:22.75,Default,,0000,0000,0000,,Ko được.
Dialogue: 0,0:04:22.85,0:04:23.65,Default,,0000,0000,0000,,Vì sao chứ?
Dialogue: 0,0:04:23.78,0:04:26.55,Default,,0000,0000,0000,,Việc này chỉ liên quan tới ác quỉ và thiên thần.
Dialogue: 0,0:04:26.82,0:04:28.88,Default,,0000,0000,0000,,Con người như cậu nên tránh xa việc này.
Dialogue: 0,0:04:28.99,0:04:30.85,Default,,0000,0000,0000,,Nhưng còn về bản hợp đồng thì sao?
Dialogue: 0,0:04:31.43,0:04:34.79,Default,,0000,0000,0000,,Carrera sẽ được trợ giúp bởi các ác quỉ khác.
Dialogue: 0,0:04:35.20,0:04:37.86,Default,,0000,0000,0000,,Điều này ko hề liên quan tới những ước muốn của anh.
Dialogue: 0,0:04:38.30,0:04:40.93,Default,,0000,0000,0000,,Điều này cũng có nghĩa là bản hợp đồng đã vô giá trị.
Dialogue: 0,0:04:41.34,0:04:43.10,Default,,0000,0000,0000,,Ko thể như vậy được.
Dialogue: 0,0:04:43.94,0:04:46.60,Default,,0000,0000,0000,,Vậy thì tôi sẽ yêu cầu một bản hợp đồng khác!
Dialogue: 0,0:04:47.64,0:04:49.13,Default,,0000,0000,0000,,Ước muốn của tôi lúc này là...
Dialogue: 0,0:04:50.24,0:04:54.37,Default,,0000,0000,0000,,...Tôi muốn được tự tay giải cứu cho Carrera!
Dialogue: 0,0:04:58.09,0:04:58.88,Default,,0000,0000,0000,,Cậu nghiêm túc đấy chứ?
Dialogue: 0,0:04:58.99,0:04:59.75,Default,,0000,0000,0000,,Chắc chắn rồi!
Dialogue: 0,0:05:01.02,0:05:04.42,Default,,0000,0000,0000,,Cậu muốn tự tay giải cứu cô ấy...
Dialogue: 0,0:05:07.93,0:05:10.12,Default,,0000,0000,0000,,Được thôi, chúng ta sẽ kí hợp đồng.
Dialogue: 0,0:05:32.75,0:05:34.35,Default,,0000,0000,0000,,Gì thế này?
Dialogue: 0,0:05:34.62,0:05:35.71,Default,,0000,0000,0000,,Ko thể như vậy được.
Dialogue: 0,0:05:38.19,0:05:39.25,Default,,0000,0000,0000,,Chị?
Dialogue: 0,0:05:41.53,0:05:44.40,Default,,0000,0000,0000,,Chị Carrera!
Dialogue: 0,0:05:54.84,0:05:56.83,Default,,0000,0000,0000,,Các người đang làm gì chị Carrera vậy!
Dialogue: 0,0:05:56.94,0:05:58.14,Default,,0000,0000,0000,,Dừng lại ngay!
Dialogue: 0,0:05:58.25,0:06:00.58,Default,,0000,0000,0000,,Hãy câm miệng, ác quỉ kia.
Dialogue: 0,0:06:01.62,0:06:04.45,Default,,0000,0000,0000,,Chúng ta đang tiến hành nghi lễ thanh tẩy.
Dialogue: 0,0:06:04.92,0:06:06.55,Default,,0000,0000,0000,,Thanh tẩy?
Dialogue: 0,0:06:06.79,0:06:09.05,Default,,0000,0000,0000,,Đúng thế, tên ác quỉ xấu xa.
Dialogue: 0,0:06:09.22,0:06:13.35,Default,,0000,0000,0000,,Ngươi sẽ bị thanh tẩy bởi sức mạnh thần thánh của chúng ta.
Dialogue: 0,0:06:14.03,0:06:18.46,Default,,0000,0000,0000,,Với thân thể này, hãy đón nhận tình thương của Thượng đế nhân từ.
Dialogue: 0,0:06:21.20,0:06:23.47,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:06:29.28,0:06:31.44,Default,,0000,0000,0000,,Tôi đang ở đâu thế này?
Dialogue: 0,0:06:31.58,0:06:32.64,Default,,0000,0000,0000,,Ở Địa ngục.
Dialogue: 0,0:06:32.75,0:06:33.64,Default,,0000,0000,0000,,Địa ngục á?
Dialogue: 0,0:06:33.91,0:06:37.28,Default,,0000,0000,0000,,Nhưng Carrera đang ở Thiên đường...
Dialogue: 0,0:06:38.15,0:06:40.35,Default,,0000,0000,0000,,Tôi muốn mời thêm một ác quỉ nữa làm đồng mình của chúng ta.
Dialogue: 0,0:06:40.72,0:06:41.65,Default,,0000,0000,0000,,Đồng minh hả?
Dialogue: 0,0:06:43.06,0:06:44.65,Default,,0000,0000,0000,,Cô trong đó phải ko, Mercedes?
Dialogue: 0,0:06:44.96,0:06:45.93,Default,,0000,0000,0000,,Tôi vào đây.
Dialogue: 0,0:06:46.99,0:06:47.69,Default,,0000,0000,0000,,Mercedes?
Dialogue: 0,0:06:52.80,0:06:55.10,Default,,0000,0000,0000,,Anh muốn gì, Alphina?
Dialogue: 0,0:06:56.27,0:06:58.97,Default,,0000,0000,0000,,Đây là một chuyện khẩn cấp.\NCô có thể giúp tôi chứ?
Dialogue: 0,0:06:59.11,0:07:01.20,Default,,0000,0000,0000,,Tôi đã nói với anh là tôi ko muốn làm bất cứ việc gì cả!
Dialogue: 0,0:07:01.81,0:07:07.11,Default,,0000,0000,0000,,Carrera đã bị các thiên thần bắt đi, và\NRati đã một mình đuổi theo họ.
Dialogue: 0,0:07:07.25,0:07:08.71,Default,,0000,0000,0000,,Anh vừa nói gì?
Dialogue: 0,0:07:08.98,0:07:10.18,Default,,0000,0000,0000,,Là thật à?
Dialogue: 0,0:07:10.32,0:07:10.98,Default,,0000,0000,0000,,Uhm.
Dialogue: 0,0:07:11.42,0:07:13.39,Default,,0000,0000,0000,,Cậu ta đã chứng kiến mọi việc.
Dialogue: 0,0:07:14.92,0:07:15.91,Default,,0000,0000,0000,,Ngươi là...
Dialogue: 0,0:07:17.99,0:07:19.58,Default,,0000,0000,0000,,Ngươi là gã khốn đó!
Dialogue: 0,0:07:20.73,0:07:22.89,Default,,0000,0000,0000,,Oh, cô đúng là Mercedes rồi.
Dialogue: 0,0:07:23.60,0:07:26.36,Default,,0000,0000,0000,,Sao ngươi lại ở đây?
Dialogue: 0,0:07:27.20,0:07:29.86,Default,,0000,0000,0000,,Hai người biết nhau à?
Dialogue: 0,0:07:30.30,0:07:31.93,Default,,0000,0000,0000,,Hmm, một chút...
Dialogue: 0,0:07:32.07,0:07:35.67,Default,,0000,0000,0000,,Với lại, tại sao con người\Nlại được phép tới Địa ngục?
Dialogue: 0,0:07:36.48,0:07:40.47,Default,,0000,0000,0000,,Tất cả chúng ta sẽ tới\Nthiên đường để giải cứu Carrera.
Dialogue: 0,0:07:40.81,0:07:42.58,Default,,0000,0000,0000,,Chúng ta tới thiên đường cùng nhau á?
Dialogue: 0,0:07:43.82,0:07:46.91,Default,,0000,0000,0000,,Đó là mong ước của anh ta, một phần trong bản hợp đồng.
Dialogue: 0,0:07:47.69,0:07:49.25,Default,,0000,0000,0000,,Hợp đồng?
Dialogue: 0,0:07:49.39,0:07:52.62,Default,,0000,0000,0000,,Thế nên, tôi muốn cô đi với chúng tôi.
Dialogue: 0,0:07:52.83,0:07:54.92,Default,,0000,0000,0000,,Chắc chắn rồi, tôi sẽ đi!
Dialogue: 0,0:08:34.87,0:08:36.20,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:08:38.74,0:08:39.80,Default,,0000,0000,0000,,Hãy để tôi bắt đầu.
Dialogue: 0,0:09:00.13,0:09:02.43,Default,,0000,0000,0000,,Ngực của ngươi tuy nhỏ nhưng khá là dễ thương đấy.
Dialogue: 0,0:09:02.90,0:09:04.02,Default,,0000,0000,0000,,Cảm thấy dễ chịu chứ?
Dialogue: 0,0:09:04.73,0:09:08.50,Default,,0000,0000,0000,,Ta sẽ khiến ngươi thấy sướng ngay bây giờ.
Dialogue: 0,0:09:26.05,0:09:32.01,Default,,0000,0000,0000,,Ác quỉ xấu xa, ta sẽ tự tay mình thanh tẩy ngươi.
Dialogue: 0,0:09:43.87,0:09:45.96,Default,,0000,0000,0000,,Ta ko tin được là ngươi lại ướt thế này.
Dialogue: 0,0:09:47.31,0:09:49.54,Default,,0000,0000,0000,,Tôi đã sẵn sàng.
Dialogue: 0,0:09:53.75,0:09:56.55,Default,,0000,0000,0000,,Tôi cũng vậy.
Dialogue: 0,0:10:08.16,0:10:10.89,Default,,0000,0000,0000,,Bắt đầu thôi!
Dialogue: 0,0:10:17.67,0:10:19.10,Default,,0000,0000,0000,,Cái quái gì vậy?
Dialogue: 0,0:10:22.11,0:10:23.34,Default,,0000,0000,0000,,Thế này là sao?
Dialogue: 0,0:10:25.05,0:10:27.17,Default,,0000,0000,0000,,Các người đinh làm gì?
Dialogue: 0,0:10:30.25,0:10:33.58,Default,,0000,0000,0000,,Ác quỉ, đừng lo.
Dialogue: 0,0:10:34.69,0:10:37.59,Default,,0000,0000,0000,,Ngươi sẽ bị thanh tẩy ngay bây giờ.
Dialogue: 0,0:10:39.23,0:10:43.32,Default,,0000,0000,0000,,Hãy để cơ thế ngươi đón nhận tình thương của Thượng đế.
Dialogue: 0,0:10:55.94,0:10:58.17,Default,,0000,0000,0000,,Ko...
Dialogue: 0,0:11:00.88,0:11:03.61,Default,,0000,0000,0000,,Nó to quá!
Dialogue: 0,0:11:06.62,0:11:09.95,Default,,0000,0000,0000,,Đừng kháng cự nữa, mi sẽ cảm thấy khá hơn sớm thôi.
Dialogue: 0,0:11:12.69,0:11:16.82,Default,,0000,0000,0000,,Mi sẽ phải tiếp tục cầu xin để được đón nhận tình thương của Thượng đế.
Dialogue: 0,0:11:18.33,0:11:22.67,Default,,0000,0000,0000,,Chuyện đó sẽ ko bao giờ xảy ra!
Dialogue: 0,0:11:24.17,0:11:25.87,Default,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:11:27.77,0:11:29.74,Default,,0000,0000,0000,,Mở to mắt ra mà nhìn.
Dialogue: 0,0:11:30.31,0:11:33.25,Default,,0000,0000,0000,,Nhìn xem thứ gì đang vào bên trong ngươi.
Dialogue: 0,0:11:36.28,0:11:39.55,Default,,0000,0000,0000,,Thứ to lớn đó đang ấn vào sâu bên trong mình...
Dialogue: 0,0:11:40.09,0:11:43.45,Default,,0000,0000,0000,,Mình ko thể chịu được nữa,\Nthế này xấu hổ quá.
Dialogue: 0,0:11:43.79,0:11:51.09,Default,,0000,0000,0000,,Đúng rồi. Ta sẽ thanh tẩy từng phần trên cơ thể ngươi.
Dialogue: 0,0:12:02.31,0:12:03.30,Default,,0000,0000,0000,,Rati.
Dialogue: 0,0:12:18.83,0:12:19.76,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:12:26.20,0:12:30.14,Default,,0000,0000,0000,,Thật tuyệt vời...
Dialogue: 0,0:12:35.81,0:12:38.78,Default,,0000,0000,0000,,Dù chỉ xem nhưng tôi vẫn thấy thật phấn khích.
Dialogue: 0,0:12:40.95,0:12:42.88,Default,,0000,0000,0000,,Tôi ko thể nhịn lậu hơn nữa...
Dialogue: 0,0:12:43.78,0:12:47.78,Default,,0000,0000,0000,,Trái ngược hẳn với những gì họ đã nói, Thiên thần chả có gì ngoài sự to mồm.
Dialogue: 0,0:12:48.36,0:12:51.09,Default,,0000,0000,0000,,Hãy cố chịu đựng chút nữa, Rati...
Dialogue: 0,0:12:51.66,0:12:54.06,Default,,0000,0000,0000,,Đến lúc chị phản công rồi đây.
Dialogue: 0,0:12:56.30,0:12:59.13,Default,,0000,0000,0000,,Hãy chờ chị, Rati.
Dialogue: 0,0:13:01.50,0:13:05.03,Default,,0000,0000,0000,,Ngươi đã cảm nhận được tình thương của Thượng đế chưa?
Dialogue: 0,0:13:05.97,0:13:09.37,Default,,0000,0000,0000,,Tôi ko cảm thấy gì hết!
Dialogue: 0,0:13:09.61,0:13:10.51,Default,,0000,0000,0000,,Ngươi chắc chứ?
Dialogue: 0,0:13:12.05,0:13:13.41,Default,,0000,0000,0000,,Đừng có nói dối.
Dialogue: 0,0:13:14.31,0:13:19.78,Default,,0000,0000,0000,,Âm hộ của ngươi đang khít lại xung quanh cái đó của ta.
Dialogue: 0,0:13:20.82,0:13:22.81,Default,,0000,0000,0000,,Ngươi chắc hẳn đang mong chờ nhận được sức mạnh thần thánh của ta.
Dialogue: 0,0:13:23.56,0:13:28.79,Default,,0000,0000,0000,,Ta sẽ trao nó cho ngươi ngay bây giờ,\Nngươi sẽ cảm nhận được lòng nhân từ của Thượng đế!
Dialogue: 0,0:13:46.45,0:13:49.90,Default,,0000,0000,0000,,Oh thật đáng thương, ngươi ko thích à?
Dialogue: 0,0:13:53.35,0:13:54.95,Default,,0000,0000,0000,,Dừng lại đi.
Dialogue: 0,0:13:58.96,0:14:00.95,Default,,0000,0000,0000,,Chúng ta mới chỉ bắt đầu nghi lễ .
Dialogue: 0,0:14:01.90,0:14:04.19,Default,,0000,0000,0000,,Chúng ta sẽ ban cho ngươi thêm sức mạnh.
Dialogue: 0,0:14:12.24,0:14:15.07,Default,,0000,0000,0000,,Ta sẽ thanh tẩy miệng của ngươi.
Dialogue: 0,0:14:15.74,0:14:17.30,Default,,0000,0000,0000,,Vậy nên hãy bú nó đi.
Dialogue: 0,0:14:17.98,0:14:19.17,Default,,0000,0000,0000,,Đồ ác quỉ xấu xa!
Dialogue: 0,0:14:20.28,0:14:21.80,Default,,0000,0000,0000,,V-Vâng.
Dialogue: 0,0:14:27.05,0:14:28.32,Default,,0000,0000,0000,,Đúng rồi...
Dialogue: 0,0:14:29.16,0:14:30.35,Default,,0000,0000,0000,,Đúng rồi, là như vậy đó.
Dialogue: 0,0:14:31.83,0:14:37.99,Default,,0000,0000,0000,,Ngươi sẽ sớm cảm nhận được tình yêu thương vĩ đại của Thượng đế.
Dialogue: 0,0:14:49.51,0:14:51.34,Default,,0000,0000,0000,,Cuối cùng cũng tới rồi.
Dialogue: 0,0:14:53.28,0:14:55.77,Default,,0000,0000,0000,,Các bậc thang dẫn lên cổng ngầm này dài thật.
Dialogue: 0,0:14:56.32,0:14:59.41,Default,,0000,0000,0000,,Này, Alphina, nơi này chính là...
Dialogue: 0,0:15:00.15,0:15:01.31,Default,,0000,0000,0000,,Thiên đường.
Dialogue: 0,0:15:01.45,0:15:02.35,Default,,0000,0000,0000,,Là nơi này ư?
Dialogue: 0,0:15:03.12,0:15:06.15,Default,,0000,0000,0000,,Chính xác hơn, nơi này là tận cùng của thiên đường...
Dialogue: 0,0:15:06.76,0:15:09.75,Default,,0000,0000,0000,,...và cũng là nơi mà các thiên thần gọi là "hòn đảo Trụy lạc."
Dialogue: 0,0:15:10.30,0:15:11.89,Default,,0000,0000,0000,,Đảo Trụy lạc...
Dialogue: 0,0:15:12.70,0:15:13.79,Default,,0000,0000,0000,,Này, Alphina.
Dialogue: 0,0:15:14.07,0:15:14.90,Default,,0000,0000,0000,,Chuyện gì thế?
Dialogue: 0,0:15:15.27,0:15:19.60,Default,,0000,0000,0000,,Làm sao chúng ta có thể tìm được Carrera\Nở một nơi rộng lớn thế này?
Dialogue: 0,0:15:20.57,0:15:27.04,Default,,0000,0000,0000,,Dễ thôi, chúng ta sẽ hỏi các thiên thần đã bắt họ đi.
Dialogue: 0,0:15:36.26,0:15:40.32,Default,,0000,0000,0000,,Tất cả mọi người, hãy kết thúc đi.
Dialogue: 0,0:15:42.93,0:15:48.42,Default,,0000,0000,0000,,Chúng ta sẽ kết thúc việc thanh tẩy ác quỉ với thần vật của Thượng đế.
Dialogue: 0,0:15:48.54,0:15:51.37,Default,,0000,0000,0000,,Cô thật sự chẳng tinh ý chút nào cả.
Dialogue: 0,0:15:51.84,0:15:52.74,Default,,0000,0000,0000,,Ngươi vừa nói gì?
Dialogue: 0,0:15:54.24,0:15:55.30,Default,,0000,0000,0000,,Gì thế này?
Dialogue: 0,0:15:55.51,0:15:58.81,Default,,0000,0000,0000,,Những thiên thần kia có vẻ sắp chết vì kiệt sức rồi.
Dialogue: 0,0:15:59.68,0:16:02.01,Default,,0000,0000,0000,,Hình như họ hết sức mạnh "thần thánh" rồi thì phải.
Dialogue: 0,0:16:02.65,0:16:04.41,Default,,0000,0000,0000,,Sao có thể như vậy được?
Dialogue: 0,0:16:04.72,0:16:07.91,Default,,0000,0000,0000,,Tôi sẽ ko thể thỏa mãn chỉ với mức độ này.
Dialogue: 0,0:16:08.15,0:16:09.75,Default,,0000,0000,0000,,Các người làm tôi thực sự thấ vọng đấy .
Dialogue: 0,0:16:11.79,0:16:14.39,Default,,0000,0000,0000,,Đừng bao giờ đánh giá thấp sức mạnh của ác quỉ, hãy nhớ kĩ điều đó!
Dialogue: 0,0:16:15.76,0:16:17.46,Default,,0000,0000,0000,,Thú vị đấy.
Dialogue: 0,0:16:18.63,0:16:21.86,Default,,0000,0000,0000,,Nếu ngươi muốn khiêu khích chúng ta...
Dialogue: 0,0:16:22.24,0:16:25.10,Default,,0000,0000,0000,,...Ngươi sẽ được toại nguyện.
Dialogue: 0,0:16:25.67,0:16:26.76,Default,,0000,0000,0000,,Chuẩn bị đi!
Dialogue: 0,0:16:32.35,0:16:33.71,Default,,0000,0000,0000,,Tín hiệu này...
Dialogue: 0,0:16:34.21,0:16:35.15,Default,,0000,0000,0000,,Ko thể nào.
Dialogue: 0,0:16:35.85,0:16:38.18,Default,,0000,0000,0000,,Ko nhầm được đâu, chính là hắn ta.
Dialogue: 0,0:16:42.59,0:16:46.42,Default,,0000,0000,0000,,Sao hắn có thể tới được Thiên đường?
Dialogue: 0,0:16:47.49,0:16:49.89,Default,,0000,0000,0000,,Ác quỉ trắng, Alphina!
Dialogue: 0,0:17:04.14,0:17:07.26,Default,,0000,0000,0000,,Ác quỉ trắng, ngươi lại xuất hiện rồi!
Dialogue: 0,0:17:07.11,0:17:08.48,Default,,0000,0000,0000,,Đừng hòng trốn thoát vào hôm nay!
Dialogue: 0,0:17:08.85,0:17:09.87,Default,,0000,0000,0000,,Ta sẽ tóm được ngươi!
Dialogue: 0,0:17:22.13,0:17:22.89,Default,,0000,0000,0000,,Họ đang tới!
Dialogue: 0,0:17:23.36,0:17:25.02,Default,,0000,0000,0000,,Họ đang tới, Ogawa!
Dialogue: 0,0:17:25.47,0:17:29.37,Default,,0000,0000,0000,,Ẩn sâu trong bóng đêm, ngươi\Ntồn tai giữa ánh sáng và bóng tối.
Dialogue: 0,0:17:29.54,0:17:32.90,Default,,0000,0000,0000,,Được tạo thành bởi loại thép cứng rắn nhất\Nvà tôi luyện trong ngọn lửa lạnh giá nhất của sự hỗn mang.
Dialogue: 0,0:17:42.48,0:17:42.91,Default,,0000,0000,0000,,Hắn ta biến mất rồi ư?
Dialogue: 0,0:17:43.12,0:17:46.31,Default,,0000,0000,0000,,Xé toang bầu trời,\NThanh gươm của bóng tối!
Dialogue: 0,0:17:47.29,0:17:48.18,Default,,0000,0000,0000,,Đó là một cái bẫy!
Dialogue: 0,0:17:56.00,0:17:57.83,Default,,0000,0000,0000,,Thanh gươm phán quyết...
Dialogue: 0,0:17:58.73,0:18:00.93,Default,,0000,0000,0000,,Cậu ta có thể thực hiện một câu thần chú mạnh như vậy...
Dialogue: 0,0:18:01.77,0:18:04.86,Default,,0000,0000,0000,,Ogawa, cậu đúng là một đồng đội tuyệt vời.
Dialogue: 0,0:18:10.34,0:18:12.24,Default,,0000,0000,0000,,Tôi ko thể cử động được.
Dialogue: 0,0:18:13.71,0:18:16.20,Default,,0000,0000,0000,,Tia sáng đỏ đó là gì?
Dialogue: 0,0:18:17.28,0:18:21.41,Default,,0000,0000,0000,,Sao chúng có thể phá vỡ sức mạnh bảo vệ xung quanh mình?
Dialogue: 0,0:18:21.59,0:18:23.42,Default,,0000,0000,0000,,Cô tỉnh rồi hả, thiên thần?
Dialogue: 0,0:18:24.19,0:18:25.02,Default,,0000,0000,0000,,Con người ư?
Dialogue: 0,0:18:25.83,0:18:27.32,Default,,0000,0000,0000,,Tôi có điều muốn hỏi cô đây.
Dialogue: 0,0:18:27.59,0:18:29.86,Default,,0000,0000,0000,,Ác quỉ đã bị cô bắt đi hiện giờ ở đâu?
Dialogue: 0,0:18:31.30,0:18:33.46,Default,,0000,0000,0000,,Các người giam cô ấy ở đâu?
Dialogue: 0,0:18:48.98,0:18:51.61,Default,,0000,0000,0000,,Ngươi thích kiểu chơi này chứ?
Dialogue: 0,0:18:53.69,0:18:56.75,Default,,0000,0000,0000,,Vật đó đang quấy đảo cả âm hộ và lỗ đít của ngươi. Chắc hẳn phải sướng lắm nhỉ.
Dialogue: 0,0:18:58.02,0:19:00.75,Default,,0000,0000,0000,,Chắc chắn là rất sướng.\NĐến mức mi ko thể thốt nên lời.
Dialogue: 0,0:19:01.43,0:19:07.80,Default,,0000,0000,0000,,Lỗ kia của ngươi cũng đang khít lại và xoắn lấy cái đó của ta.
Dialogue: 0,0:19:11.54,0:19:14.73,Default,,0000,0000,0000,,Một ác quỉ kiêu ngạo như ngươi cũng có lúc ngoan ngoãn thế này sao?
Dialogue: 0,0:19:15.68,0:19:19.30,Default,,0000,0000,0000,,Ngươi sẽ sớm cảm nhận được sức mạnh tối thượng từ lòng nhân từ của Thượng đế.
Dialogue: 0,0:19:20.15,0:19:21.88,Default,,0000,0000,0000,,Hãy cố đợi đến lúc đó.
Dialogue: 0,0:19:33.66,0:19:36.13,Default,,0000,0000,0000,,Mi muốn biết những ác quỉ kia ở đâu sao?
Dialogue: 0,0:19:36.93,0:19:40.73,Default,,0000,0000,0000,,Chẳng phải đi hỏi chính ngưới bắt là cách hiệu quả nhất hay sao.
Dialogue: 0,0:19:42.14,0:19:45.87,Default,,0000,0000,0000,,Vì thế mà mi giăng bẫy bọn ta.
Dialogue: 0,0:19:46.97,0:19:50.27,Default,,0000,0000,0000,,Ta sẽ ko để ngươi toại nguyện đâu... ác quỉ trắng.
Dialogue: 0,0:19:52.15,0:19:54.11,Default,,0000,0000,0000,,Em giận đấy à, Ivy?
Dialogue: 0,0:19:54.65,0:19:57.14,Default,,0000,0000,0000,,Là do em đuổi theo trước mà, phải ko?
Dialogue: 0,0:19:59.19,0:20:00.88,Default,,0000,0000,0000,,Nhưng làm thế này cũng vô ích thôi...
Dialogue: 0,0:20:03.36,0:20:04.91,Default,,0000,0000,0000,,...vì ta sẽ ko bao giờ nói cho mi!
Dialogue: 0,0:20:07.26,0:20:09.59,Default,,0000,0000,0000,,Chúng ta đã lâu ko gặp nhau.
Dialogue: 0,0:20:11.83,0:20:13.66,Default,,0000,0000,0000,,Em ko nhớ anh à?
Dialogue: 0,0:20:14.27,0:20:16.29,Default,,0000,0000,0000,,Em còn ko cả gọi anh bằng tên thật là sao?
Dialogue: 0,0:20:16.90,0:20:18.46,Default,,0000,0000,0000,,Cái gì mà 'lâu ko gặp nhau'?
Dialogue: 0,0:20:19.31,0:20:21.21,Default,,0000,0000,0000,,Anh là đồ ác quỉ dối trá.
Dialogue: 0,0:20:21.44,0:20:24.24,Default,,0000,0000,0000,,'AIphina' chẳng phải là tên của anh sao?
Dialogue: 0,0:20:25.04,0:20:28.50,Default,,0000,0000,0000,,Tất cả mọi lời anh nói với tôi đều là dối trá...
Dialogue: 0,0:20:30.18,0:20:31.81,Default,,0000,0000,0000,,Vậy em tức giận như vậy...
Dialogue: 0,0:20:32.59,0:20:35.05,Default,,0000,0000,0000,,...là vì anh ko tới thăm em sao?
Dialogue: 0,0:20:35.35,0:20:36.15,Default,,0000,0000,0000,,Anh là đồ ngốc!
Dialogue: 0,0:20:38.72,0:20:40.21,Default,,0000,0000,0000,,Sao có thể như vậy được?
Dialogue: 0,0:20:41.03,0:20:42.86,Default,,0000,0000,0000,,Cô ta đã ra ko biết bao nhiêu lần...
Dialogue: 0,0:20:44.16,0:20:47.86,Default,,0000,0000,0000,,Ko lẽ chúng ta bó tay sao...
Dialogue: 0,0:20:56.38,0:20:59.34,Default,,0000,0000,0000,,Các người chỉ lãng phí thời gian thôi.
Dialogue: 0,0:21:00.08,0:21:05.45,Default,,0000,0000,0000,,Chúng ta, những thiên thần, sẽ ko bao giờ thỏa hiệp với ác quỉ.
Dialogue: 0,0:21:06.22,0:21:09.25,Default,,0000,0000,0000,,Ko phải như thế này!
Dialogue: 0,0:21:10.06,0:21:12.39,Default,,0000,0000,0000,,Chuyện này ko hề liên quan tới thiên thần hay ác quỉ gì hết.
Dialogue: 0,0:21:12.96,0:21:13.98,Default,,0000,0000,0000,,Tôi chỉ...
Dialogue: 0,0:21:14.93,0:21:17.90,Default,,0000,0000,0000,,Tôi chỉ muốn giải cứu Carrera!
Dialogue: 0,0:21:18.40,0:21:22.63,Default,,0000,0000,0000,,Làm ơn hãy cho tôi biết cô ấy ở đâu!
Dialogue: 0,0:21:22.74,0:21:26.60,Default,,0000,0000,0000,,Cậu thật sự muốn giải cứu cô ta đến mức này ư?
Dialogue: 0,0:21:26.87,0:21:27.57,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:21:28.61,0:21:30.10,Default,,0000,0000,0000,,Tôi rất muốn!
Dialogue: 0,0:21:31.81,0:21:33.44,Default,,0000,0000,0000,,Tốt thôi.
Dialogue: 0,0:21:34.58,0:21:36.14,Default,,0000,0000,0000,,Tôi sẽ nói cho cậu nơi giam giữ cô ta.
Dialogue: 0,0:21:36.68,0:21:37.98,Default,,0000,0000,0000,,Cám ơn!
Dialogue: 0,0:21:38.68,0:21:41.62,Default,,0000,0000,0000,,Cám ơn cô! Cám ơn cô rất nhiều!
Dialogue: 0,0:21:54.10,0:21:56.50,Default,,0000,0000,0000,,Oh tôi hiểu rồi. Cô rất muốn điều đó, đúng ko?
Dialogue: 0,0:21:57.37,0:21:58.23,Default,,0000,0000,0000,,Vậy thì...
Dialogue: 0,0:21:58.44,0:21:59.63,Default,,0000,0000,0000,,Để tôi giúp cô.
Dialogue: 0,0:22:08.81,0:22:09.71,Default,,0000,0000,0000,,Sao lại thế?
Dialogue: 0,0:22:10.75,0:22:13.48,Default,,0000,0000,0000,,Tôi đã hứa sẽ nói cho cậu biết chỗ đó rồi mà.
Dialogue: 0,0:22:13.99,0:22:16.42,Default,,0000,0000,0000,,Nhưng cô ko cảm thấy dễ chịu sao?
Dialogue: 0,0:22:17.42,0:22:18.72,Default,,0000,0000,0000,,Chúng ta tiếp tục nhé.
Dialogue: 0,0:22:24.30,0:22:25.29,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:22:26.20,0:22:31.36,Default,,0000,0000,0000,,Cái đó của cậu đang vào sâu trong tôi.
Dialogue: 0,0:22:31.74,0:22:35.17,Default,,0000,0000,0000,,Chỗ đó của tôi đang nóng ran lên và ướt nữa.
Dialogue: 0,0:22:37.31,0:22:41.18,Default,,0000,0000,0000,,Tôi ướt lắm rồi.
Dialogue: 0,0:22:46.29,0:22:48.45,Default,,0000,0000,0000,,Làm ơn, hãy ra cùng với tôi.
Dialogue: 0,0:22:49.46,0:22:51.12,Default,,0000,0000,0000,,Chúng ta sẽ ra cùng nhau!
Dialogue: 0,0:23:13.75,0:23:15.21,Default,,0000,0000,0000,,Ko thể như thế được!
Dialogue: 0,0:23:15.38,0:23:18.37,Default,,0000,0000,0000,,Có vẻ như cô là người cuối cùng sót lại.
Dialogue: 0,0:23:19.42,0:23:21.05,Default,,0000,0000,0000,,Còn muốn thanh tẩy tôi nữa ko?
Dialogue: 0,0:23:21.92,0:23:22.98,Default,,0000,0000,0000,,Hay là...
Dialogue: 0,0:23:28.76,0:23:29.52,Default,,0000,0000,0000,,Chắc đau lắm nhỉ.
Dialogue: 0,0:23:29.63,0:23:31.46,Default,,0000,0000,0000,,Carrera yêu dấu của anh!
Dialogue: 0,0:23:32.00,0:23:34.02,Default,,0000,0000,0000,,Carrera, em ở đâu?
Dialogue: 0,0:23:34.50,0:23:35.47,Default,,0000,0000,0000,,Carrera yêu dấu của anh!
Dialogue: 0,0:23:37.60,0:23:38.47,Default,,0000,0000,0000,,Oh, Carrera!
Dialogue: 0,0:23:38.87,0:23:40.20,Default,,0000,0000,0000,,Anh làm gì ở đây vậy?
Dialogue: 0,0:23:41.57,0:23:43.04,Default,,0000,0000,0000,,Em ko sao chứ?
Dialogue: 0,0:23:43.28,0:23:44.21,Default,,0000,0000,0000,,Em có bị thương ko?
Dialogue: 0,0:23:44.31,0:23:46.01,Default,,0000,0000,0000,,E-Em ko sao.
Dialogue: 0,0:23:47.55,0:23:48.98,Default,,0000,0000,0000,,Tạ ơn trời...
Dialogue: 0,0:23:50.18,0:23:51.88,Default,,0000,0000,0000,,Anh đến để cứu em ư?
Dialogue: 0,0:23:52.15,0:23:54.85,Default,,0000,0000,0000,,Carrera...
Dialogue: 0,0:23:55.82,0:23:57.98,Default,,0000,0000,0000,,Gì thế?
Dialogue: 0,0:23:58.66,0:23:59.65,Default,,0000,0000,0000,,Hey!
Dialogue: 0,0:23:59.93,0:24:02.92,Default,,0000,0000,0000,,Tỉnh lại đi, Ogawa!
Dialogue: 0,0:24:03.76,0:24:06.20,Default,,0000,0000,0000,,Ko! Anh ko được chết!
Dialogue: 0,0:24:08.03,0:24:09.93,Default,,0000,0000,0000,,Đó là việc mình cần phải làm.
Dialogue: 0,0:24:10.14,0:24:12.63,Default,,0000,0000,0000,,Mình cảm thấy thật hạnh phúc.
Dialogue: 0,0:24:13.04,0:24:18.20,Default,,0000,0000,0000,,Mình ko muốn sống cả đời chỉ với lòng căm thù và nỗi phiền muộn.
Dialogue: 0,0:24:18.81,0:24:21.54,Default,,0000,0000,0000,,Nhưng bây giờ mình đã trải qua những khoảnh khắc tuyệt vời nhất của đời mình.
Dialogue: 0,0:24:22.18,0:24:26.31,Default,,0000,0000,0000,,Mình đã biết đến niềm hạnh phúc tột cùng của tình yêu.
Dialogue: 0,0:24:26.65,0:24:29.02,Default,,0000,0000,0000,,Và mình đã làm được điều tốt đẹp nhất dành cho cô ấy.
Dialogue: 0,0:24:31.19,0:24:32.66,Default,,0000,0000,0000,,Mình đã thấy ánh sáng rồi.
Dialogue: 0,0:24:33.26,0:24:35.66,Default,,0000,0000,0000,,Mình sắp được tái sinh ư?
Dialogue: 0,0:24:36.36,0:24:39.30,Default,,0000,0000,0000,,Kiếp sau của mình chắc sẽ rất tuyệt.
Dialogue: 0,0:24:40.17,0:24:42.03,Default,,0000,0000,0000,,Mình có thể cảm nhận được điều đó...
Dialogue: 0,0:24:48.11,0:24:49.84,Default,,0000,0000,0000,,Anh tỉnh rồi à?
Dialogue: 0,0:24:53.41,0:24:57.04,Default,,0000,0000,0000,,Chào buổi sáng. Bữa sáng của anh sẽ có ngay bây giờ.
Dialogue: 0,0:24:57.82,0:25:00.22,Default,,0000,0000,0000,,Sao em lại ở trong phòng anh?
Dialogue: 0,0:25:00.79,0:25:03.62,Default,,0000,0000,0000,,Chẳng phải chúng ta đã kí hợp đồng lúc ở công viên sao.
Dialogue: 0,0:25:03.76,0:25:05.05,Default,,0000,0000,0000,,Đúng thế...
Dialogue: 0,0:25:05.42,0:25:09.95,Default,,0000,0000,0000,,Nhưng anh đã kí một hợp đồng khác với Alphina.
Dialogue: 0,0:25:10.06,0:25:10.93,Default,,0000,0000,0000,,Ko sao hết.
Dialogue: 0,0:25:11.03,0:25:15.66,Default,,0000,0000,0000,,Em kí hợp đồng với anh trước anh ta nên bản hợp đồng của anh ta sẽ ko có hiệu lực.
Dialogue: 0,0:25:16.34,0:25:18.77,Default,,0000,0000,0000,,Vậy em đến để thu lấy linh hồn anh à?
Dialogue: 0,0:25:18.87,0:25:20.17,Default,,0000,0000,0000,,Ồ, ko đâu.
Dialogue: 0,0:25:20.77,0:25:21.50,Default,,0000,0000,0000,,Sao thế?
Dialogue: 0,0:25:21.77,0:25:27.80,Default,,0000,0000,0000,,Vì nếu lấy đi linh hồn anh, em sẽ ko thể làm chuyện đó với anh.
Dialogue: 0,0:25:28.88,0:25:30.54,Default,,0000,0000,0000,,Carrera, tình yêu của anh!
Dialogue: 0,0:25:30.65,0:25:32.34,Default,,0000,0000,0000,,Trông anh ko khác gì một tên đần.
Dialogue: 0,0:25:32.92,0:25:34.22,Default,,0000,0000,0000,,Âm thanh này là...
Dialogue: 0,0:25:34.52,0:25:35.54,Default,,0000,0000,0000,,Mercedes!
Dialogue: 0,0:25:35.65,0:25:37.55,Default,,0000,0000,0000,,Cô làm gì trên ngăn chứa đồ của tôi vậy?
Dialogue: 0,0:25:38.12,0:25:40.56,Default,,0000,0000,0000,,Bởi vì phòng anh quá bé.
Dialogue: 0,0:25:40.76,0:25:41.42,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:25:41.66,0:25:44.69,Default,,0000,0000,0000,,Tôi ko thể tin là bốn người có thể sống trong cái xó này.
Dialogue: 0,0:25:44.80,0:25:45.89,Default,,0000,0000,0000,,Bốn người?
Dialogue: 0,0:25:46.03,0:25:46.90,Default,,0000,0000,0000,,Oh ko.
Dialogue: 0,0:25:51.20,0:25:52.57,Default,,0000,0000,0000,,Em đang làm gì thế?
Dialogue: 0,0:25:53.04,0:25:54.56,Default,,0000,0000,0000,,Chào buổi sáng!
Dialogue: 0,0:25:54.97,0:25:56.70,Default,,0000,0000,0000,,Chào buổi sáng?
Dialogue: 0,0:25:56.98,0:26:01.07,Default,,0000,0000,0000,,Hãy để em bày tỏ lòng biết ơn của mình về việc giải cứu chị Carrera và em.
Dialogue: 0,0:26:01.91,0:26:03.38,Default,,0000,0000,0000,,Anh ko thích à?
Dialogue: 0,0:26:03.48,0:26:05.18,Default,,0000,0000,0000,,Anh ko có ý đó.
Dialogue: 0,0:26:05.35,0:26:06.32,Default,,0000,0000,0000,,Em làm việc này rất tuyệt đấy.
Dialogue: 0,0:26:06.42,0:26:08.85,Default,,0000,0000,0000,,Thật hả? Tuyệt quá.
Dialogue: 0,0:26:09.36,0:26:12.62,Default,,0000,0000,0000,,Em chưa hề bú một thứ to như thế bao giờ.
Dialogue: 0,0:26:13.19,0:26:15.09,Default,,0000,0000,0000,,Nhưng em sẽ làm hết sức có thể.
Dialogue: 0,0:26:15.53,0:26:17.33,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:26:17.86,0:26:20.73,Default,,0000,0000,0000,,Tôi ko nghĩ là Rati giỏi trong việc này.
Dialogue: 0,0:26:20.83,0:26:25.53,Default,,0000,0000,0000,,Em ko biết là em làm việc đó giỏi hay ko,\Nnhưng mọi người đều thích thú khi em làm việc đó.
Dialogue: 0,0:26:26.57,0:26:27.90,Default,,0000,0000,0000,,Để chị bú nói với, nào.
Dialogue: 0,0:26:28.17,0:26:29.11,Default,,0000,0000,0000,,Đợi đã nào...
Dialogue: 0,0:26:29.78,0:26:30.33,Default,,0000,0000,0000,,Ko phải thế này chứ!
Dialogue: 0,0:26:30.44,0:26:32.21,Default,,0000,0000,0000,,Có vẻ như mọi người đang rất vui.
Dialogue: 0,0:26:32.48,0:26:34.11,Default,,0000,0000,0000,,C-Carrera.
Dialogue: 0,0:26:35.38,0:26:37.78,Default,,0000,0000,0000,,Hai người làm ko đến nỗi nào...
Dialogue: 0,0:26:37.98,0:26:39.61,Default,,0000,0000,0000,,...nhưng hãy nhìn tôi mà học tập này.
Dialogue: 0,0:26:40.49,0:26:41.35,Default,,0000,0000,0000,,Cùng vui vẻ nào.
Dialogue: 0,0:26:42.39,0:26:44.69,Default,,0000,0000,0000,,Ko việc gì phải căng thẳng như thế.
Dialogue: 0,0:26:47.39,0:26:49.59,Default,,0000,0000,0000,,Cho tới khi anh thực sự mãn nguyện.
Dialogue: 0,0:26:50.73,0:26:53.03,Default,,0000,0000,0000,,Em sẽ ở bên anh mãi mãi.
Dialogue: 0,0:27:00.24,0:27:01.80,Default,,0000,0000,0000,,Oops, thế này là tiêu con rồi.
Dialogue: 0,0:27:29.24,0:27:38.87,Default,,0000,0000,0000,,Mọi người đều có một giấc mơ\Nđược đắm chìm trong ánh hào quang rực rỡ.
Dialogue: 0,0:27:39.21,0:27:51.85,Default,,0000,0000,0000,,Với nụ hôn của ác quỉ.\NBạn có mơ ước về một cuộc sống vĩnh hằng?
Dialogue: 0,0:27:54.03,0:27:56.52,Default,,0000,0000,0000,,Hãy giữ lại linh hồn và bản thân bạn.
Dialogue: 0,0:27:56.70,0:28:04.50,Default,,0000,0000,0000,,Những từ ngữ của bóng đêm\Nsẽ nói lên những ước muốn đã bị quên lãng của bạn.
Dialogue: 0,0:28:04.74,0:28:07.21,Default,,0000,0000,0000,,Hãy giữ lại linh hồn và trái tim của bạn.
Dialogue: 0,0:28:07.37,0:28:18.18,Default,,0000,0000,0000,,Tôi ko muốn những cảm xúc giả dối.\NĐiều tôi thực sự muốn chỉ là sự thật mà thôi.
