Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:19.66,0:00:20.57,Default,,0000,0000,0000,,Rồi sao?
Dialogue: 0,0:00:25.97,0:00:31.14,Default,,0000,0000,0000,,Bạn tôi nói rằng đây là máu của một vị thánh.
Dialogue: 0,0:00:31.83,0:00:36.92,Default,,0000,0000,0000,,Chính là nó. Thứ này chính là máu của mẫu thí nghiệm có bí danh "N".
Dialogue: 0,0:00:38.31,0:00:40.30,Default,,0000,0000,0000,,Vị thánh mà cô nói tới đó hiện giờ ở đâu?
Dialogue: 0,0:00:40.62,0:00:42.12,Default,,0000,0000,0000,,Tôi ko biết rõ chi tiết việc này.
Dialogue: 0,0:00:51.56,0:00:59.28,Default,,0000,0000,0000,,Nghe đây, ta sẽ dành cho cô một bài học đặc biệt hơn nếu cô tìm ra chủ nhân của những giọt máu này.
Dialogue: 0,0:00:59.79,0:01:02.42,Default,,0000,0000,0000,,Vâng, thưa giáo sư Ihabella.
Dialogue: 0,0:01:17.20,0:01:18.45,Default,,0000,0000,0000,,Yếu mà láo hả!
Dialogue: 0,0:01:23.13,0:01:24.83,Default,,0000,0000,0000,,Ở đây này! Ở đây này!
Dialogue: 0,0:01:25.62,0:01:28.64,Default,,0000,0000,0000,,Mọi người hãy nhìn tôi này!
Dialogue: 0,0:01:39.17,0:01:42.44,Default,,0000,0000,0000,,Các người sẽ bị thương hết nếu cứ nhìn chằm chằm vào cô ta như vậy.
Dialogue: 0,0:02:04.13,0:02:09.66,Default,,0000,0000,0000,,DAIAKUJI 4
Dialogue: 0,0:04:38.69,0:04:40.60,Default,,0000,0000,0000,,Chết tiệt, cô ta ghê gớm thật.
Dialogue: 0,0:04:43.41,0:04:45.26,Default,,0000,0000,0000,,Nhận lấy này
Dialogue: 0,0:04:48.55,0:04:49.84,Default,,0000,0000,0000,,Tóm được rồi! Huh?
Dialogue: 0,0:04:50.14,0:04:51.46,Default,,0000,0000,0000,,Chết đi Akuji.
Dialogue: 0,0:04:52.74,0:04:54.20,Default,,0000,0000,0000,,Ngươi sẽ bị tiêu diệt bởi giáo phái Nako.
Dialogue: 0,0:05:01.49,0:05:02.13,Default,,0000,0000,0000,,Đây rồi!
Dialogue: 0,0:05:08.10,0:05:10.62,Default,,0000,0000,0000,,Tsukigase-sama đã bị đánh bại?
Dialogue: 0,0:05:10.69,0:05:12.14,Default,,0000,0000,0000,,Ôi lạy Chúa...
Dialogue: 0,0:05:15.61,0:05:18.30,Default,,0000,0000,0000,,Chạy thôi! Rút lui! Rút lui thôi!
Dialogue: 0,0:05:18.50,0:05:20.29,Default,,0000,0000,0000,,Anh nợ tôi lần này đấy.
Dialogue: 0,0:05:20.38,0:05:23.42,Default,,0000,0000,0000,,Ko hổ danh là Toko, hỗ trợ tuyệt lắm!
Dialogue: 0,0:05:39.87,0:05:41.10,Default,,0000,0000,0000,,Thấy thế nào?
Dialogue: 0,0:05:41.65,0:05:45.65,Default,,0000,0000,0000,,Cô rất khoái chuyện này, phải ko?
Dialogue: 0,0:05:47.53,0:05:52.40,Default,,0000,0000,0000,,Cái này sẽ là trải nghiệm tuyệt vời nhất dành cho cô!
Dialogue: 0,0:05:52.40,0:05:53.56,Default,,0000,0000,0000,,Vâng...
Dialogue: 0,0:05:53.56,0:06:00.83,Default,,0000,0000,0000,,Hắn ta rất thích nhìn chằm chằm vào chỗ đó, và hắn có thể nhìn vào đó hàng tháng trời.
Dialogue: 0,0:06:00.90,0:06:02.43,Default,,0000,0000,0000,,Oh ko...
Dialogue: 0,0:06:02.83,0:06:05.29,Default,,0000,0000,0000,,Oh, nó đang co giật kìa.
Dialogue: 0,0:06:14.50,0:06:18.11,Default,,0000,0000,0000,,Coi kìa, cô ta bắt đầu ướt rồi.
Dialogue: 0,0:06:18.37,0:06:21.77,Default,,0000,0000,0000,,Nhưng nhìn chăm chăm theo cái kiểu này thì tệ quá!
Dialogue: 0,0:06:22.18,0:06:27.36,Default,,0000,0000,0000,,Trông thật lố bịch, nhơ bẩn, và mùi thì kinh quá!
Dialogue: 0,0:06:36.03,0:06:37.60,Default,,0000,0000,0000,,Muốn ra rồi hả?
Dialogue: 0,0:06:37.79,0:06:38.61,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:06:39.45,0:06:41.71,Default,,0000,0000,0000,,Vậy hãy nghĩ tới chuyện đó và ra đi!
Dialogue: 0,0:06:41.71,0:06:43.71,Default,,0000,0000,0000,,Vâng, Tôi ra rồi!
Dialogue: 0,0:06:47.40,0:06:48.61,Default,,0000,0000,0000,,Tôi ra rồi...
Dialogue: 0,0:06:49.33,0:06:50.38,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:06:55.07,0:06:56.85,Default,,0000,0000,0000,,Tôi ra rồi. Tôi ra rồi.
Dialogue: 0,0:07:00.90,0:07:02.08,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:07:14.13,0:07:18.38,Default,,0000,0000,0000,,Ko, đau quá!
Dialogue: 0,0:07:19.67,0:07:23.08,Default,,0000,0000,0000,,Tốt, cô đã có hứng thú với tôi rồi!
Dialogue: 0,0:07:26.91,0:07:28.58,Default,,0000,0000,0000,,Làm ơn dừng lại!
Dialogue: 0,0:07:29.54,0:07:30.42,Default,,0000,0000,0000,,Ko, cô ko được!
Dialogue: 0,0:07:33.04,0:07:35.82,Default,,0000,0000,0000,,Những cặp tình nhân đau khổ còn phải chịu đựng hơn thế nhiều cô bé ạ.
Dialogue: 0,0:07:56.05,0:07:57.45,Default,,0000,0000,0000,,Cô ta thật kiên cường.
Dialogue: 0,0:07:57.84,0:07:59.23,Default,,0000,0000,0000,,Cô ta đã chịu khai gì chưa?
Dialogue: 0,0:07:59.45,0:08:00.24,Default,,0000,0000,0000,,Chưa hề
Dialogue: 0,0:08:01.11,0:08:05.93,Default,,0000,0000,0000,,Qua kiểm tra tôi đoán chắc rằng bọn họ còn kiếm tiền bằng cách khác nữa.
Dialogue: 0,0:08:06.74,0:08:08.36,Default,,0000,0000,0000,,Nếu chúng ta ko giải quyết dứt điểm việc này...
Dialogue: 0,0:08:09.30,0:08:11.12,Default,,0000,0000,0000,,Chúng ta sẽ đời nào...
Dialogue: 0,0:08:11.93,0:08:15.87,Default,,0000,0000,0000,,Có vẻ như cô vẫn chưa học được cách xưng hô ở đây.
Dialogue: 0,0:08:23.62,0:08:30.59,Default,,0000,0000,0000,,Cô có biết nguyên lý của chất lỏng khi ngập ứ trong dạ dày thì nó sẽ phải tuôn ra theo đường âm hộ ko?
Dialogue: 0,0:08:54.00,0:08:58.71,Default,,0000,0000,0000,,Thấy cỗ máy " khoái lạc vĩnh cửu " này của tôi hoạt động ra sao?
Dialogue: 0,0:09:10.34,0:09:13.03,Default,,0000,0000,0000,,Lâu lắm rồi tôi chưa làm chuyện này với cô nhỉ, Yuko.
Dialogue: 0,0:09:16.26,0:09:18.72,Default,,0000,0000,0000,,Vậy nên hôm nay chắc hẳn tôi và anh sẽ có nhiều trò vui đây.
Dialogue: 0,0:09:23.09,0:09:24.90,Default,,0000,0000,0000,,Cô chẳng thay đổi gì cả.
Dialogue: 0,0:09:26.18,0:09:28.05,Default,,0000,0000,0000,,Trò vui mới chỉ bắt đầu thôi mà.
Dialogue: 0,0:09:40.06,0:09:41.20,Default,,0000,0000,0000,,Chủ nhân Akuji...
Dialogue: 0,0:09:45.01,0:09:45.75,Default,,0000,0000,0000,,Kikuko
Dialogue: 0,0:09:47.28,0:09:50.19,Default,,0000,0000,0000,,Tôi mang cô ấy đi cùng vì cô ấy cũng muốn làm chuyện này.
Dialogue: 0,0:09:50.28,0:09:51.09,Default,,0000,0000,0000,,Tới đây nào.
Dialogue: 0,0:09:51.16,0:09:51.67,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:09:51.72,0:09:53.43,Default,,0000,0000,0000,,Cô chăm sóc chỗ này nhé.
Dialogue: 0,0:09:53.80,0:09:56.02,Default,,0000,0000,0000,,Chủ nhân Akuji, đây là niềm vinh hạnh của tôi.
Dialogue: 0,0:09:58.67,0:10:00.67,Default,,0000,0000,0000,,Cô khá hơn trước nhiều đấy Kikuko.
Dialogue: 0,0:10:02.57,0:10:04.71,Default,,0000,0000,0000,,Cám ơn chủ nhân.
Dialogue: 0,0:10:07.28,0:10:08.32,Default,,0000,0000,0000,,Thế này được chứ hả?
Dialogue: 0,0:10:08.32,0:10:10.27,Default,,0000,0000,0000,,Được, rất được...
Dialogue: 0,0:10:11.79,0:10:16.93,Default,,0000,0000,0000,,Akuji, đừng cử động. Hôm nay hai chúng tôi sẽ phục vụ anh.
Dialogue: 0,0:10:17.08,0:10:18.63,Default,,0000,0000,0000,,Thế hả?
Dialogue: 0,0:10:20.63,0:10:22.41,Default,,0000,0000,0000,,Hôm nay Akuji sẽ là đồ chơi của mình...
Dialogue: 0,0:10:29.95,0:10:32.32,Default,,0000,0000,0000,,Đã lâu lắm rồi kể từ lần đó.
Dialogue: 0,0:10:33.04,0:10:35.41,Default,,0000,0000,0000,,Cô vẫn khít như thế.
Dialogue: 0,0:10:36.61,0:10:38.79,Default,,0000,0000,0000,,Cám ơn. Chúng ta làm thôi nào!
Dialogue: 0,0:10:43.18,0:10:44.03,Default,,0000,0000,0000,,Thấy thế nào?
Dialogue: 0,0:10:44.52,0:10:46.08,Default,,0000,0000,0000,,Tuyệt lắm.
Dialogue: 0,0:10:48.00,0:10:53.17,Default,,0000,0000,0000,,Kikuko, hãy quan sát thật cẩn thận. Đây chính là điểm cực khoái của chủ nhân cô.
Dialogue: 0,0:10:55.49,0:10:56.61,Default,,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:10:58.92,0:11:02.17,Default,,0000,0000,0000,,Cậu bé của anh lại lớn hơn nữa rồi, Akuji.
Dialogue: 0,0:11:02.51,0:11:03.60,Default,,0000,0000,0000,,Yuko-san...
Dialogue: 0,0:11:04.96,0:11:08.45,Default,,0000,0000,0000,,Bên trong, bên trong, tuyệt quá.
Dialogue: 0,0:11:10.30,0:11:13.12,Default,,0000,0000,0000,,Kikuko-chan, lại đây nào.
Dialogue: 0,0:11:14.63,0:11:15.22,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:11:16.08,0:11:18.15,Default,,0000,0000,0000,,Thứ lỗi cho tôi, Chủ nhân.
Dialogue: 0,0:11:19.25,0:11:22.02,Default,,0000,0000,0000,,Cô ướt đẫm rồi kìa.
Dialogue: 0,0:11:24.39,0:11:26.64,Default,,0000,0000,0000,,Tôi, tôi cũng thế...
Dialogue: 0,0:11:27.82,0:11:32.83,Default,,0000,0000,0000,,Lưỡi của chủ nhân đang luồn sâu vào bên trông, sâu bên trong...
Dialogue: 0,0:11:33.17,0:11:35.25,Default,,0000,0000,0000,,Cô lúc nào cũng dễ thương như vậy.
Dialogue: 0,0:11:39.44,0:11:42.61,Default,,0000,0000,0000,,Kìa, hãy quan sát vũ khí đầy uy lực của chủ nhân cô kìa.
Dialogue: 0,0:11:43.44,0:11:47.64,Default,,0000,0000,0000,,Vật đó chui hẳn vào trong và bị bao bọc hoàn toàn trong đó. Thật tuyệt vời!
Dialogue: 0,0:11:48.21,0:11:50.33,Default,,0000,0000,0000,,Cô còn phải cố gắng hơn nữa Kikuko.
Dialogue: 0,0:11:50.54,0:11:51.52,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:12:02.70,0:12:05.09,Default,,0000,0000,0000,,Cậu bé của anh đâm vào sâu quá.
Dialogue: 0,0:12:05.72,0:12:08.92,Default,,0000,0000,0000,,Đúng rồi, cứng nữa lên! Sâu, sâu nữa...
Dialogue: 0,0:12:22.71,0:12:24.74,Default,,0000,0000,0000,,Tôi chưa bao giờ cảm thấy sinh lực tràn trề như lúc này!
Dialogue: 0,0:12:25.00,0:12:28.25,Default,,0000,0000,0000,,Sau tất cả mọi chuyện, cô vẫn là người tuyệt nhất Yuko. Tôi đánh giá rất cao điều đó.
Dialogue: 0,0:12:29.14,0:12:32.34,Default,,0000,0000,0000,,Kikuko, Tôi sẽ chơi cô ngay bây giờ. Dang chân ra nào.
Dialogue: 0,0:12:32.76,0:12:34.14,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:12:38.73,0:12:40.64,Default,,0000,0000,0000,,Thêm một chút nữa nhỉ.
Dialogue: 0,0:12:40.64,0:12:41.12,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:12:41.62,0:12:43.03,Default,,0000,0000,0000,,Cây roi của tôi biến mất rồi.
Dialogue: 0,0:12:43.40,0:12:45.81,Default,,0000,0000,0000,,Akuji lấy nó đi rồi.
Dialogue: 0,0:12:46.19,0:12:47.31,Default,,0000,0000,0000,,Anh ta trong căn phòng kia kìa.
Dialogue: 0,0:12:48.84,0:12:49.67,Default,,0000,0000,0000,,Cái...?
Dialogue: 0,0:12:50.80,0:12:54.34,Default,,0000,0000,0000,,Ooh, Motoko hả! Cô có muốn tham gia luôn ko?
Dialogue: 0,0:12:57.03,0:12:58.07,Default,,0000,0000,0000,,Ngốc thật
Dialogue: 0,0:13:00.37,0:13:01.51,Default,,0000,0000,0000,,Thủ lĩnh, Chủ nhân.
Dialogue: 0,0:13:01.79,0:13:03.27,Default,,0000,0000,0000,,Đều đang bận cả rồi.
Dialogue: 0,0:13:03.55,0:13:05.61,Default,,0000,0000,0000,,Cô ả của giáo phái Nako cuối cùng đã chịu cung khai rồi.
Dialogue: 0,0:13:05.97,0:13:08.74,Default,,0000,0000,0000,,Chúng đã mở các cửa hiệu kinh doanh để làm gia tăng ngân quỹ của mình.
Dialogue: 0,0:13:09.06,0:13:09.68,Default,,0000,0000,0000,,Cửa hiệu kinh doanh?
Dialogue: 0,0:13:11.48,0:13:12.59,Default,,0000,0000,0000,,Cửa hiệu ư?
Dialogue: 0,0:13:12.75,0:13:14.43,Default,,0000,0000,0000,,Nene-sama sẽ ko bao giờ làm như vậy cả?!
Dialogue: 0,0:13:14.50,0:13:17.68,Default,,0000,0000,0000,,Cô ấy nói đó là lệnh của Nako-sama.
Dialogue: 0,0:13:17.68,0:13:18.66,Default,,0000,0000,0000,,Ko thể nào...
Dialogue: 0,0:13:18.66,0:13:22.26,Default,,0000,0000,0000,,Họ nói dối. Dù sao thì, chúng ta cũng phải giải cứu Nene-sama...
Dialogue: 0,0:13:22.26,0:13:26.99,Default,,0000,0000,0000,,Thế là quá đủ rồi. Chúng tôi đã đóng góp rất nhiều tiền của, trong khi chúng tôi phải ăn uống thiếu thốn.
Dialogue: 0,0:13:27.08,0:13:32.34,Default,,0000,0000,0000,,Phụ nữ đã phải bán mình ở các cửa hiệu đó và tiền thu được đều phải góp vào ngân quỹ.
Dialogue: 0,0:13:32.44,0:13:35.99,Default,,0000,0000,0000,,Chúng tôi làm tất cả những việc đó chỉ để được ban cho máu của Nako-sama.
Dialogue: 0,0:13:36.30,0:13:40.36,Default,,0000,0000,0000,,Làm ơn! Chỉ cần một chút để cứu con trai tôi...
Dialogue: 0,0:13:40.36,0:13:41.53,Default,,0000,0000,0000,,Ko, tôi sẽ nhận được trước!
Dialogue: 0,0:13:41.53,0:13:44.34,Default,,0000,0000,0000,,Tôi đã được hứa từ sáu tháng trước!
Dialogue: 0,0:13:44.34,0:13:46.28,Default,,0000,0000,0000,,Nako-sama! Nako-sama!
Dialogue: 0,0:13:46.41,0:13:47.63,Default,,0000,0000,0000,,Mọi người...
Dialogue: 0,0:13:47.70,0:13:51.26,Default,,0000,0000,0000,,Dừng lại ngay! Chúng tôi ko thể ban máu cho tất cả được...
Dialogue: 0,0:13:51.50,0:13:58.25,Default,,0000,0000,0000,,Máu! Máu! Ban cho chúng tôi máu!
Dialogue: 0,0:14:03.51,0:14:07.66,Default,,0000,0000,0000,,Tóm lại, chúng gọi thứ này là máu của thánh Nako?
Dialogue: 0,0:14:08.48,0:14:09.49,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:14:09.79,0:14:12.76,Default,,0000,0000,0000,,Những giọt máu này quả thật có khả năng chữa lành bệnh tật.
Dialogue: 0,0:14:13.20,0:14:17.94,Default,,0000,0000,0000,,Vị thánh đó đang ở tổng đàn của giáo phái Nako trong vùng Shikina.
Dialogue: 0,0:14:18.83,0:14:20.02,Default,,0000,0000,0000,,Giáo sư Ihabella
Dialogue: 0,0:14:22.20,0:14:25.65,Default,,0000,0000,0000,,Làm ơn hãy để mắt đến tôi nhiều hơn nữa.
Dialogue: 0,0:14:33.04,0:14:35.79,Default,,0000,0000,0000,,Ko, bàn tay của bà...
Dialogue: 0,0:14:37.28,0:14:39.58,Default,,0000,0000,0000,,Cho tôi thấy cô có những gì nào.
Dialogue: 0,0:14:40.86,0:14:41.58,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:14:47.17,0:14:48.08,Default,,0000,0000,0000,,Làm ơn...
Dialogue: 0,0:14:57.08,0:14:59.66,Default,,0000,0000,0000,,Máu, ban cho chúng tôi máu!
Dialogue: 0,0:14:59.94,0:15:03.10,Default,,0000,0000,0000,,Nếu tôi có được một chút thôi...
Dialogue: 0,0:15:13.18,0:15:14.38,Default,,0000,0000,0000,,Yoko-san
Dialogue: 0,0:15:14.38,0:15:16.55,Default,,0000,0000,0000,,Đừng lo. Tôi sẽ luôn ở bên cạnh cô.
Dialogue: 0,0:15:16.98,0:15:18.84,Default,,0000,0000,0000,,Làm ơn hãy trốn đi, Yoko-san.
Dialogue: 0,0:15:19.01,0:15:19.78,Default,,0000,0000,0000,,Yume...
Dialogue: 0,0:15:20.19,0:15:27.02,Default,,0000,0000,0000,,Đây hoàn toàn là lỗi của tôi. Nếu tôi ko ở đây, sẽ ko ai phải chịu đựng đau khổ như vậy.
Dialogue: 0,0:15:27.41,0:15:28.98,Default,,0000,0000,0000,,Ko, ko phải như vậy.
Dialogue: 0,0:15:29.27,0:15:32.40,Default,,0000,0000,0000,,Yume. Cô chẳng phải đã chứng kiến bọn họ được chữa khỏi bệnh tật, đúng ko.
Dialogue: 0,0:15:32.76,0:15:35.73,Default,,0000,0000,0000,,Cô đã cứu mạng đứa bé.
Dialogue: 0,0:15:36.02,0:15:38.95,Default,,0000,0000,0000,,Và còn rất nhiều người già yếu bệnh nằm liệt giường nữa.
Dialogue: 0,0:15:39.07,0:15:43.03,Default,,0000,0000,0000,,Nhưng cuộc sống của mọi người cũng vì thế mà trở nên tồi tệ.
Dialogue: 0,0:15:43.52,0:15:45.64,Default,,0000,0000,0000,,Đây ko phải lúc diễn kịch nữa.
Dialogue: 0,0:15:45.87,0:15:50.09,Default,,0000,0000,0000,,Tôi ko đáng được sống. Tạm biệt, Yoko-san.
Dialogue: 0,0:15:50.09,0:15:51.30,Default,,0000,0000,0000,,Yume, đợi đã!
Dialogue: 0,0:15:53.60,0:15:54.46,Default,,0000,0000,0000,,Oh ko...
Dialogue: 0,0:15:54.46,0:15:55.40,Default,,0000,0000,0000,,Chuyện gì thế này?
Dialogue: 0,0:15:56.09,0:15:58.17,Default,,0000,0000,0000,,Làm ơn hãy trốn ngay đi Yoko-san!
Dialogue: 0,0:15:58.62,0:15:59.48,Default,,0000,0000,0000,,Yume...
Dialogue: 0,0:15:59.74,0:16:03.23,Default,,0000,0000,0000,,Ngay lúc này, ko có ai ở đây, nhanh lên!
Dialogue: 0,0:16:05.75,0:16:06.73,Default,,0000,0000,0000,,Yoko-san...
Dialogue: 0,0:16:06.87,0:16:10.34,Default,,0000,0000,0000,,Yume, tôi đã luôn luôn yêu cô...
Dialogue: 0,0:16:12.30,0:16:14.60,Default,,0000,0000,0000,,Chúng ta sẽ ra đi cùng nhau.
Dialogue: 0,0:16:23.71,0:16:26.29,Default,,0000,0000,0000,,Vậy ra đây là việc kinh doanh các cửa hiệu của giáo phái Nako.
Dialogue: 0,0:16:26.75,0:16:27.92,Default,,0000,0000,0000,,Thật đáng kinh tởm.
Dialogue: 0,0:16:28.04,0:16:29.98,Default,,0000,0000,0000,,Chủ nhân, chúng ta làm gì bây giờ?
Dialogue: 0,0:16:30.49,0:16:33.08,Default,,0000,0000,0000,,Tới Shikina. Thu thập tất cả bọn chúng!!
Dialogue: 0,0:16:33.13,0:16:33.81,Default,,0000,0000,0000,,Anh nghiêm túc đấy chứ?
Dialogue: 0,0:16:34.07,0:16:37.60,Default,,0000,0000,0000,,Đúng thế, chúng ta phải xóa sổ những kẻ muốn cạnh tranh với chúng ta.
Dialogue: 0,0:16:46.37,0:16:48.27,Default,,0000,0000,0000,,Cô đẹp lắm, Yume.
Dialogue: 0,0:16:48.87,0:16:57.20,Default,,0000,0000,0000,,Ko phải thế. Tôi ko có bộ ngực lớn như cô, vả lại...
Dialogue: 0,0:16:58.03,0:17:03.23,Default,,0000,0000,0000,,Điều đó ko đúng. Cô xinh đẹp hơn bất cứ người phụ nữ nào khác, Yume.
Dialogue: 0,0:17:46.87,0:17:48.71,Default,,0000,0000,0000,,Ko, đừng...!
Dialogue: 0,0:18:09.07,0:18:09.82,Default,,0000,0000,0000,,Yume...
Dialogue: 0,0:18:10.90,0:18:11.92,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:18:12.68,0:18:15.70,Default,,0000,0000,0000,,Ko sao đâu. Từ bây giờ cô sẽ ko phải đau khổ nữa.
Dialogue: 0,0:18:16.08,0:18:17.70,Default,,0000,0000,0000,,Ko, đừng làm thế...
Dialogue: 0,0:18:17.90,0:18:21.37,Default,,0000,0000,0000,,Hãy thư giãn, và để tôi làm cho cô sung sướng.
Dialogue: 0,0:18:21.96,0:18:24.70,Default,,0000,0000,0000,,Đừng bú nó... Ko... dơ lắm...
Dialogue: 0,0:19:00.28,0:19:01.02,Default,,0000,0000,0000,,Yume
Dialogue: 0,0:19:01.94,0:19:02.93,Default,,0000,0000,0000,,Yoko-san
Dialogue: 0,0:19:06.14,0:19:07.60,Default,,0000,0000,0000,,Vui chứ?
Dialogue: 0,0:19:11.41,0:19:12.45,Default,,0000,0000,0000,,Các người là ai?
Dialogue: 0,0:19:12.71,0:19:15.95,Default,,0000,0000,0000,,Đúng là mẫu thí nghiệm này. Cuối cùng ta đã tìm được cô.
Dialogue: 0,0:19:18.03,0:19:19.48,Default,,0000,0000,0000,,Ý bà là sao?
Dialogue: 0,0:19:20.33,0:19:22.49,Default,,0000,0000,0000,,Cái gì? Cái gì...
Dialogue: 0,0:19:22.49,0:19:23.15,Default,,0000,0000,0000,,Lại đây!
Dialogue: 0,0:19:23.16,0:19:24.02,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:19:24.02,0:19:25.55,Default,,0000,0000,0000,,Bà làm gì Yume vậy?
Dialogue: 0,0:19:25.55,0:19:28.55,Default,,0000,0000,0000,,Yume? Cô ta bây giờ có cái tên đẹp đấy.
Dialogue: 0,0:19:28.92,0:19:30.20,Default,,0000,0000,0000,,Cho dù cô ta chỉ là một con chuột thí nghiệm.
Dialogue: 0,0:19:30.58,0:19:31.70,Default,,0000,0000,0000,,Chuột thí nghiệm?
Dialogue: 0,0:19:31.76,0:19:35.60,Default,,0000,0000,0000,,Đúng thế, cô thực ra là một công trình nghiên cứu của ta.
Dialogue: 0,0:19:36.03,0:19:37.12,Default,,0000,0000,0000,,Cô phải đi với ta.
Dialogue: 0,0:19:37.12,0:19:39.19,Default,,0000,0000,0000,,Ko! Tôi sẽ ko đi đâu hết.
Dialogue: 0,0:19:41.39,0:19:44.22,Default,,0000,0000,0000,,Cô ko thể kháng lệnh của ta được đâu.
Dialogue: 0,0:19:45.42,0:19:46.22,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:19:46.22,0:19:47.13,Default,,0000,0000,0000,,Đừng có kháng cự nữa.
Dialogue: 0,0:19:48.05,0:19:52.94,Default,,0000,0000,0000,,Ta đã đặt cô dưới sự kiểm soát của ta từ khi cô được sinh ra.
Dialogue: 0,0:19:53.06,0:19:54.21,Default,,0000,0000,0000,,Ko thể nào...
Dialogue: 0,0:19:54.21,0:19:57.05,Default,,0000,0000,0000,,Vậy nên cô ko thể ko tuân theo những gì ta nói được đâu.
Dialogue: 0,0:19:57.20,0:20:02.66,Default,,0000,0000,0000,,Cô đang rất muốn nhét thứ đó của cô vào âm hộ cô ta đúng ko.
Dialogue: 0,0:20:04.24,0:20:05.26,Default,,0000,0000,0000,,Ko phải thế...
Dialogue: 0,0:20:05.45,0:20:09.49,Default,,0000,0000,0000,,Cô rất muốn làm chuyện đó với cô ta mà.
Dialogue: 0,0:20:09.87,0:20:11.58,Default,,0000,0000,0000,,Yume, đừng nghe lời bà ta!
Dialogue: 0,0:20:11.86,0:20:16.41,Default,,0000,0000,0000,,Thấy thế nào? Có phải cái đó của cô đang nóng ran lên phải ko?
Dialogue: 0,0:20:16.41,0:20:19.45,Default,,0000,0000,0000,,Ko... Tôi... ko hề...
Dialogue: 0,0:20:19.72,0:20:23.07,Default,,0000,0000,0000,,Nhìn xem kìa, cô ta có thân hình thật quyến rũ.
Dialogue: 0,0:20:24.51,0:20:26.67,Default,,0000,0000,0000,,Và cái âm hộ tuyệt đẹp kia nữa.
Dialogue: 0,0:20:28.45,0:20:29.40,Default,,0000,0000,0000,,Chuyện gì thế?
Dialogue: 0,0:20:32.11,0:20:32.91,Default,,0000,0000,0000,,Yume!
Dialogue: 0,0:20:32.95,0:20:36.17,Default,,0000,0000,0000,,Ko sao đâu! Tôi sẽ ra hết tất cả ngay bây giờ...
Dialogue: 0,0:20:50.33,0:20:53.70,Default,,0000,0000,0000,,Cô ko thể thực hiện nổi việc đó đâu. Bởi vì ta ko cho phép điều đó.
Dialogue: 0,0:20:53.70,0:20:58.36,Default,,0000,0000,0000,,Ko, làm ơn! Hãy để nó ra! Nếu ko, tôi sẽ phát điên mất!
Dialogue: 0,0:21:00.72,0:21:03.16,Default,,0000,0000,0000,,Cái quái gì thế này?
Dialogue: 0,0:21:04.18,0:21:07.51,Default,,0000,0000,0000,,Ê này, thánh Nako của các người đâu rồi?
Dialogue: 0,0:21:07.98,0:21:09.44,Default,,0000,0000,0000,,Ở trong điện thờ...
Dialogue: 0,0:21:10.25,0:21:12.80,Default,,0000,0000,0000,,Cả thần thánh cũng ko thể giúp chúng tôi ngày hôm nay...
Dialogue: 0,0:21:13.44,0:21:18.54,Default,,0000,0000,0000,,Chết tiệt, chúng bỏ mặc cho người dân bị tấn công và chết dần chết mòn khi có chuyện.
Dialogue: 0,0:21:23.84,0:21:27.05,Default,,0000,0000,0000,,Đừng lo. Cô sẽ ko thể ra ngay cả khi ta làm thế này.
Dialogue: 0,0:21:27.38,0:21:29.05,Default,,0000,0000,0000,,Chỉ khi cô nhét nó vào chỗ đó.
Dialogue: 0,0:21:32.44,0:21:34.58,Default,,0000,0000,0000,,Cô sẽ cảm thấy khuây khỏa sau khi cô ra hết thôi.
Dialogue: 0,0:21:34.75,0:21:35.80,Default,,0000,0000,0000,,Khuây khỏa...
Dialogue: 0,0:21:36.15,0:21:39.27,Default,,0000,0000,0000,,Làm đi nào, chẳng phải cô muốn ra lắm sao?
Dialogue: 0,0:21:39.61,0:21:40.47,Default,,0000,0000,0000,,Yume...
Dialogue: 0,0:21:42.03,0:21:43.03,Default,,0000,0000,0000,,Cô có muốn được ra ko?
Dialogue: 0,0:21:43.60,0:21:44.21,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:21:45.99,0:21:49.50,Default,,0000,0000,0000,,Nói to lên nào. Cô định làm gì với thứ này?
Dialogue: 0,0:21:50.53,0:21:54.21,Default,,0000,0000,0000,,Tôi sẽ nhét nó vào trong Yoko-san...
Dialogue: 0,0:21:54.27,0:21:55.99,Default,,0000,0000,0000,,Cô muốn nhét nó vào đâu?
Dialogue: 0,0:21:56.29,0:21:59.24,Default,,0000,0000,0000,,Tôi sẽ nhét nó vào trong...
Dialogue: 0,0:21:59.24,0:22:04.91,Default,,0000,0000,0000,,Cô sẽ làm điều đó nhanh và mạnh nhất có thể và sẽ ra hết mọi thứ chứ?
Dialogue: 0,0:22:05.05,0:22:13.30,Default,,0000,0000,0000,,Vâng. Tôi sẽ làm điều đó nhanh và mạnh nhất có thể và sẽ ra hết những gì tôi có.
Dialogue: 0,0:22:13.44,0:22:16.10,Default,,0000,0000,0000,,Huh, được lắm! Tới đi nào!
Dialogue: 0,0:22:19.26,0:22:22.17,Default,,0000,0000,0000,,Yume, dừng lại! Tỉnh lại đi!
Dialogue: 0,0:22:22.39,0:22:25.97,Default,,0000,0000,0000,,Vô dụng thôi. Lúc này cô ta chỉ nghĩ tới chuyện đó.
Dialogue: 0,0:22:27.27,0:22:29.83,Default,,0000,0000,0000,,Tôi muốn được ra... ngay bây giờ...
Dialogue: 0,0:22:43.51,0:22:44.55,Default,,0000,0000,0000,,Có sướng ko?
Dialogue: 0,0:22:44.63,0:22:46.42,Default,,0000,0000,0000,,Vâng, sướng lắm.
Dialogue: 0,0:22:46.93,0:22:48.35,Default,,0000,0000,0000,,Hãy ra tất cả những gì cô có thể.
Dialogue: 0,0:22:48.88,0:22:49.58,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:23:02.93,0:23:05.45,Default,,0000,0000,0000,,Ko, làm ơn dừng lại!
Dialogue: 0,0:23:08.95,0:23:10.46,Default,,0000,0000,0000,,Yume, làm hơn hãy dừng lại!
Dialogue: 0,0:23:15.01,0:23:18.29,Default,,0000,0000,0000,,Wow, nó khít hơn rồi, khít hơn rồi...
Dialogue: 0,0:23:24.91,0:23:26.77,Default,,0000,0000,0000,,Yume, làm ơn dừng lại.
Dialogue: 0,0:23:29.73,0:23:31.36,Default,,0000,0000,0000,,Dừng lại đi, làm ơn!
Dialogue: 0,0:23:36.00,0:23:40.16,Default,,0000,0000,0000,,Làm ơn, nếu cô cứ tiếp tục như vậy...
Dialogue: 0,0:23:44.04,0:23:44.96,Default,,0000,0000,0000,,Yume!
Dialogue: 0,0:23:45.22,0:23:48.11,Default,,0000,0000,0000,,Yume, Yume, cái của cô...
Dialogue: 0,0:23:49.37,0:23:50.57,Default,,0000,0000,0000,,đang vào trong tôi...
Dialogue: 0,0:23:52.60,0:23:54.44,Default,,0000,0000,0000,,Yume, cô...!
Dialogue: 0,0:24:11.96,0:24:13.72,Default,,0000,0000,0000,,Yume! Yume!
Dialogue: 0,0:24:15.61,0:24:18.96,Default,,0000,0000,0000,,Tôi ra rồi, ra rồi, ra rồi!
Dialogue: 0,0:24:55.75,0:24:56.56,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:24:56.86,0:24:59.22,Default,,0000,0000,0000,,Cái này coi như để trả nợ cô việc lần trước.
Dialogue: 0,0:25:04.91,0:25:05.62,Default,,0000,0000,0000,,Kia là ?
Dialogue: 0,0:25:06.83,0:25:08.33,Default,,0000,0000,0000,,Này, tỉnh lại đi!
Dialogue: 0,0:25:09.08,0:25:10.48,Default,,0000,0000,0000,,Anh là...
Dialogue: 0,0:25:10.81,0:25:13.11,Default,,0000,0000,0000,,Làm ơn hãy cứu... Yume.
Dialogue: 0,0:25:13.98,0:25:16.32,Default,,0000,0000,0000,,Yume! Làm ơn hãy cứu cô ấy...
Dialogue: 0,0:25:16.52,0:25:17.56,Default,,0000,0000,0000,,Này!
