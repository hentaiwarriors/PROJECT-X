Dialogue: 0,0:00:43.91,0:00:44.80,Default,,0000,0000,0000,,Shinobu.
Dialogue: 0,0:00:44.91,0:00:46.13,Default,,0000,0000,0000,,Em lại nấp ở đây hả?
Dialogue: 0,0:00:51.38,0:00:55.82,Default,,0000,0000,0000,,Tại sao Shinobu luôn như vậy nhỉ?
Dialogue: 0,0:01:23.21,0:01:24.84,Default,,0000,0000,0000,,Em ko sao chứ, Shinobu?
Dialogue: 0,0:01:25.61,0:01:27.55,Default,,0000,0000,0000,,Vâng, Em ko sao.
Dialogue: 0,0:01:27.65,0:01:31.85,Default,,0000,0000,0000,,Anh ko cần phải giúp gì đâu, em ổn mà...
Dialogue: 0,0:01:33.46,0:01:34.85,Default,,0000,0000,0000,,Này, đưa tay cho anh.
Dialogue: 0,0:01:35.19,0:01:36.49,Default,,0000,0000,0000,,Dùng nó đi.
Dialogue: 0,0:01:37.53,0:01:38.36,Default,,0000,0000,0000,,Cám ơn anh.
Dialogue: 0,0:01:38.83,0:01:41.39,Default,,0000,0000,0000,,Hiếm có thằng nào tốt bụng được như anh đâu, em biết mà.
Dialogue: 0,0:01:41.50,0:01:44.93,Default,,0000,0000,0000,,Nào bây giờ chúng ta chơi xích đu nhé, hoặc là trượt ván?
Dialogue: 0,0:01:45.33,0:01:47.46,Default,,0000,0000,0000,,Hay là em muốn chơi đu quay?
Dialogue: 0,0:01:47.57,0:01:49.09,Default,,0000,0000,0000,,Trò leo dốc cũng vui lắm đấy.
Dialogue: 0,0:01:50.31,0:01:51.64,Default,,0000,0000,0000,,Anh là đồ keo kiệt.
Dialogue: 0,0:01:51.77,0:01:54.11,Default,,0000,0000,0000,,Trêu chọc em gái mình khi nó đang bị bùn dây khắp người thế này.
Dialogue: 0,0:01:55.54,0:01:58.10,Default,,0000,0000,0000,,Anh sẽ kéo em lên. Đứng dậy nào.
Dialogue: 0,0:01:59.65,0:02:02.12,Default,,0000,0000,0000,,Thôi được rồi, em sẽ tha lỗi cho anh.
Dialogue: 0,0:02:03.15,0:02:05.12,Default,,0000,0000,0000,,Nhưng này, em ko nợ nần gì anh đâu nhé.
Dialogue: 0,0:02:06.15,0:02:07.12,Default,,0000,0000,0000,,Shinobu...
Dialogue: 0,0:02:07.46,0:02:11.12,Default,,0000,0000,0000,,Chúng ta sắp về tới nhà rồi, cho em nắm tay anh nhé?
Dialogue: 0,0:02:12.83,0:02:15.89,Default,,0000,0000,0000,,Lời nói của cô bé lúc đó đã làm tôi vô cùng sửng sốt.
Dialogue: 0,0:02:21.44,0:02:28.60,sign,,0000,0000,0000,,{\move(373,287,373,337)\fad(0,1000)}Oshino
Dialogue: 0,0:02:21.87,0:02:23.54,Default,,0000,0000,0000,,Ai da, khó chịu quá !
Dialogue: 0,0:02:23.74,0:02:25.81,Default,,0000,0000,0000,,Take-chan, khăn tắm đâu rồi?
Dialogue: 0,0:02:25.94,0:02:27.34,Default,,0000,0000,0000,,Cứ đi tắm đi.
Dialogue: 0,0:02:27.51,0:02:28.60,Default,,0000,0000,0000,,Anh sẽ lấy khăn cho.
Dialogue: 0,0:02:28.74,0:02:30.07,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:02:31.08,0:02:36.38,Default,,0000,0000,0000,,Sao Take-chan ko hiểu ra nhỉ?
Dialogue: 0,0:02:42.29,0:02:45.42,Default,,0000,0000,0000,,Shinobu, vào giường em mà ngủ đi.
Dialogue: 0,0:02:45.89,0:02:47.12,Default,,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:02:47.33,0:02:50.39,Default,,0000,0000,0000,,Em sẽ ở đây đến khi mẹ đi làm về...
Dialogue: 0,0:02:52.83,0:02:54.43,Default,,0000,0000,0000,,Chào cô, Fujino-san.
Dialogue: 0,0:02:54.67,0:02:56.93,Default,,0000,0000,0000,,Đến khi nào cháu mới chịu gọi cô là mẹ?
Dialogue: 0,0:02:57.64,0:02:59.27,Default,,0000,0000,0000,,Cháu có thể gọi cô là mẹ nếu muốn.
Dialogue: 0,0:03:00.91,0:03:01.84,Default,,0000,0000,0000,,Cô đùa đấy.
Dialogue: 0,0:03:01.98,0:03:03.20,Default,,0000,0000,0000,,À, Shinobu đâu rồi?
Dialogue: 0,0:03:03.34,0:03:05.11,Default,,0000,0000,0000,,Em ấy đi ngủ rồi.
Dialogue: 0,0:03:05.25,0:03:08.18,Default,,0000,0000,0000,,Em ấy nói sẽ thức đến khi cô đi làm về nhưng...
Dialogue: 0,0:03:08.45,0:03:09.75,Default,,0000,0000,0000,,Cô hiểu rồi.
Dialogue: 0,0:03:27.04,0:03:30.03,Default,,0000,0000,0000,,Bây giờ, cởi đồ ra nhanh lên nào.
Dialogue: 0,0:03:30.17,0:03:33.70,Default,,0000,0000,0000,,Fujino-san, chúng ta ko nên làm điều này nữa.
Dialogue: 0,0:03:38.38,0:03:39.54,Default,,0000,0000,0000,,Đừng lo việc đó.
Dialogue: 0,0:03:39.68,0:03:42.21,Default,,0000,0000,0000,,Chúng ta chỉ làm thế này khi Shinobu đã ngủ say.
Dialogue: 0,0:03:59.83,0:04:02.53,Default,,0000,0000,0000,,Đừng có cứng lên. Đừng có to lên.
Dialogue: 0,0:04:02.87,0:04:05.10,Default,,0000,0000,0000,,À đúng rồi, mình sẽ nghĩ đến thứ gì đó thật kinh khủng.
Dialogue: 0,0:04:05.27,0:04:08.30,Default,,0000,0000,0000,,Xác ếch, thân thể một con voi bị phân hủy...
Dialogue: 0,0:04:08.51,0:04:09.24,Default,,0000,0000,0000,,Ko ăn thua rồi.
Dialogue: 0,0:04:10.04,0:04:12.17,Default,,0000,0000,0000,,Nó đang to lên.
Dialogue: 0,0:04:15.58,0:04:17.98,Default,,0000,0000,0000,,Mình ko muốn làm chuyện này với cô ấy.
Dialogue: 0,0:04:18.09,0:04:20.55,Default,,0000,0000,0000,,Chết tiệt, nó tự cứng lên.
Dialogue: 0,0:04:20.72,0:04:22.99,Default,,0000,0000,0000,,Mình ko thể làm gì được.
Dialogue: 0,0:04:23.26,0:04:25.25,Default,,0000,0000,0000,,Fujino-san, làm ơn dừng lại.
Dialogue: 0,0:04:25.89,0:04:27.19,Default,,0000,0000,0000,,Dừng cái gì?
Dialogue: 0,0:04:27.50,0:04:29.73,Default,,0000,0000,0000,,Đừng đùa giỡn với thân thể cháu.
Dialogue: 0,0:04:29.93,0:04:32.87,Default,,0000,0000,0000,,Ko sao đâu, dù gì chúng ta cũng đang sống cùng nhau.
Dialogue: 0,0:05:23.85,0:05:27.15,Default,,0000,0000,0000,,Thôi nào, hãy ra đi.
Dialogue: 0,0:05:27.29,0:05:28.41,Default,,0000,0000,0000,,Cháu ra đây.
Dialogue: 0,0:05:29.89,0:05:31.22,Default,,0000,0000,0000,,Ngon thật.
Dialogue: 0,0:05:31.33,0:05:33.19,Default,,0000,0000,0000,,Rất tươi ngon.
Dialogue: 0,0:05:34.93,0:05:38.92,Default,,0000,0000,0000,,Nó vẫn còn cứng? Cháu đúng là một con quái vật dữ dằn.
Dialogue: 0,0:05:39.10,0:05:40.86,Default,,0000,0000,0000,,Nhưng ra thế thì nhiều thật.
Dialogue: 0,0:05:41.77,0:05:46.60,Default,,0000,0000,0000,,Cái đó của mình ko thể coi là biểu trưng của sự gan lì, nó chỉ tương tự như thế, nhưng là theo nghĩa khác!
Dialogue: 0,0:06:24.95,0:06:26.41,Default,,0000,0000,0000,,Ngon quá.
Dialogue: 0,0:06:38.33,0:06:39.92,Default,,0000,0000,0000,,Cảm giác này là...
Dialogue: 0,0:06:42.00,0:06:43.09,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:06:47.47,0:06:51.46,Default,,0000,0000,0000,,Cô muốn trông thấy cháu thích thú với nó.
Dialogue: 0,0:06:59.68,0:07:01.77,Default,,0000,0000,0000,,Sướng quá, sướng quá.
Dialogue: 0,0:07:02.82,0:07:06.15,Default,,0000,0000,0000,,Cái đó của chau vẫn tiếp tục to lên này.
Dialogue: 0,0:07:20.80,0:07:21.89,Default,,0000,0000,0000,,Cháu sắp ra chưa?
Dialogue: 0,0:07:22.54,0:07:23.73,Default,,0000,0000,0000,,Cháu sắp ra chưa?
Dialogue: 0,0:07:24.47,0:07:26.70,Default,,0000,0000,0000,,Hãy ra vào bên trong cô.
Dialogue: 0,0:07:30.04,0:07:32.24,Default,,0000,0000,0000,,Cô cũng ra đây.
Dialogue: 0,0:07:32.85,0:07:33.97,Default,,0000,0000,0000,,Cô ra đây.
Dialogue: 0,0:07:35.38,0:07:39.84,Default,,0000,0000,0000,,Cô ra đây!
Dialogue: 0,0:07:43.09,0:07:46.39,Default,,0000,0000,0000,,Mình làm điều này bao lâu rồi nhỉ?
Dialogue: 0,0:07:46.89,0:07:50.02,Default,,0000,0000,0000,,Ko lẽ mình phải tự xử mới có thể thoát được cảnh này?
Dialogue: 0,0:07:50.33,0:07:51.96,Default,,0000,0000,0000,,Tâm trạng của mẹ.
Dialogue: 0,0:07:52.83,0:07:56.74,Default,,0000,0000,0000,,Anh có nghĩ mẹ cần làm gì đó để giải tỏa căng thẳng khi chúng ta đang ở trường ko?
Dialogue: 0,0:07:56.87,0:07:59.64,Default,,0000,0000,0000,,Anh ko biết. Anh đoán cô ấy rất mệt mỏi.
Dialogue: 0,0:07:59.91,0:08:02.17,Default,,0000,0000,0000,,Vả lại, suy nghĩ của em lạc hậu quá rồi.
Dialogue: 0,0:08:02.31,0:08:07.84,Default,,0000,0000,0000,,Hãy cố thông cảm cho mẹ em vì cô ấy ko thể luôn ở bên cạnh em.
Dialogue: 0,0:08:08.08,0:08:09.17,Default,,0000,0000,0000,,Vâng, nhưng...
Dialogue: 0,0:08:10.72,0:08:13.35,Default,,0000,0000,0000,,Shinobu vẫn chưa biết gì cả.
Dialogue: 0,0:08:13.75,0:08:18.19,Default,,0000,0000,0000,,Mẹ cô ấy và tôi đang dan díu với nhau. Chúng tôi là nhân tình của nhau.
Dialogue: 0,0:08:18.99,0:08:22.93,Default,,0000,0000,0000,,Tốt nhất là Shinobu ko nên biết gì cả.
Dialogue: 0,0:08:23.23,0:08:27.17,Default,,0000,0000,0000,,Nhưng em ko thực sự quan tâm tới điều đó vào hôm nay.
Dialogue: 0,0:08:28.30,0:08:29.93,Default,,0000,0000,0000,,Anh biết tại sao ko?
Dialogue: 0,0:08:30.20,0:08:31.33,Default,,0000,0000,0000,,Anh ko biết.
Dialogue: 0,0:08:31.61,0:08:35.51,Default,,0000,0000,0000,,Anh hãy thử đoán xem nào?
Dialogue: 0,0:08:35.71,0:08:37.47,Default,,0000,0000,0000,,Em đang yêu hả?
Dialogue: 0,0:08:37.91,0:08:42.44,Default,,0000,0000,0000,,Vì như người ta nói: khi yêu, em sẽ thấy thế giới khác hoàn toàn.
Dialogue: 0,0:08:42.82,0:08:45.12,Default,,0000,0000,0000,,Anh nói gì vậy?
Dialogue: 0,0:08:45.29,0:08:47.31,Default,,0000,0000,0000,,Ko thể tin được là anh lại nói như vậy.
Dialogue: 0,0:08:47.69,0:08:51.56,Default,,0000,0000,0000,,Em sẽ kể cho anh khi anh trở nên thật ngốc nghếch.
Dialogue: 0,0:08:52.56,0:08:56.00,Default,,0000,0000,0000,,Này, đừng nhìn vào di động của em chứ!
Dialogue: 0,0:08:53.46,0:08:57.43,sign,,0000,0000,0000,,{\pos(278,172)}Chị đã trở về! - Fumino
Dialogue: 0,0:08:58.23,0:09:00.93,Default,,0000,0000,0000,,Chại ấy rất ghét Fujino-san.
Dialogue: 0,0:09:01.17,0:09:04.40,Default,,0000,0000,0000,,Tại sao Fumi-nee lại căm ghét mẹ ruột của mình?
Dialogue: 0,0:09:07.21,0:09:08.68,Default,,0000,0000,0000,,Oh, Mình nghĩ ra rồi.
Dialogue: 0,0:09:38.04,0:09:39.27,Default,,0000,0000,0000,,Anh muốn ra rồi à?
Dialogue: 0,0:09:39.37,0:09:41.00,Default,,0000,0000,0000,,Anh có thể tiếp tục.
Dialogue: 0,0:09:42.41,0:09:44.47,Default,,0000,0000,0000,,Vậy hãy cố giữ đi.
Dialogue: 0,0:10:11.64,0:10:13.54,Default,,0000,0000,0000,,Anh làm việc này tốt đấy.
Dialogue: 0,0:10:17.04,0:10:18.74,Default,,0000,0000,0000,,Anh làm hăng thật.
Dialogue: 0,0:10:24.32,0:10:26.95,Default,,0000,0000,0000,,Vậy chúng ta ra cùng nhau được chưa?
Dialogue: 0,0:10:27.29,0:10:28.72,Default,,0000,0000,0000,,Ko phải bây giờ.
Dialogue: 0,0:10:39.13,0:10:41.93,Default,,0000,0000,0000,,Anh ko thể chơi em khi cái đó của anh ỉu xìu thế được.
Dialogue: 0,0:10:42.07,0:10:44.77,Default,,0000,0000,0000,,Em sẽ làm cho nó cứng lên, đến mức anh có thể dùng nó để đóng đinh.
Dialogue: 0,0:11:16.24,0:11:17.80,Default,,0000,0000,0000,,Nó cứng lên rồi.
Dialogue: 0,0:11:18.34,0:11:20.50,Default,,0000,0000,0000,,Cái đó của anh phồng ra khỏi nhực em rồi.
Dialogue: 0,0:11:20.64,0:11:23.54,Default,,0000,0000,0000,,Bây giờ em sẽ làm hai viên bi của anh.
Dialogue: 0,0:12:01.38,0:12:03.75,Default,,0000,0000,0000,,Ra đi nào.
Dialogue: 0,0:12:03.92,0:12:06.44,Default,,0000,0000,0000,,Hãy ra thật nhiều vào.
Dialogue: 0,0:12:11.59,0:12:15.86,Default,,0000,0000,0000,,Dòng sữa của anh có mùi đàn ông thật đặc trưng. Nó làm em cảm thấy thật bay bổng.
Dialogue: 0,0:12:20.50,0:12:24.60,Default,,0000,0000,0000,,Đúng rồi, như thế. Làm mạnh nữa vào.
Dialogue: 0,0:12:24.71,0:12:25.87,Default,,0000,0000,0000,,Đúng rồi, như vậy đó.
Dialogue: 0,0:12:27.07,0:12:28.80,Default,,0000,0000,0000,,Bang, bang, bang.
Dialogue: 0,0:12:29.61,0:12:31.17,Default,,0000,0000,0000,,Chơi em đi.
Dialogue: 0,0:12:35.08,0:12:36.21,Default,,0000,0000,0000,,Đúng rồi.
Dialogue: 0,0:12:44.09,0:12:46.29,Default,,0000,0000,0000,,Đúng rồi, nữa.
Dialogue: 0,0:12:48.63,0:12:50.86,Default,,0000,0000,0000,,Nữa đi, nữa.
Dialogue: 0,0:12:57.77,0:12:59.47,Default,,0000,0000,0000,,Chơi em đi.
Dialogue: 0,0:13:03.74,0:13:11.62,Default,,0000,0000,0000,,Hãy chơi em đến mức "tóc" ở chỗ đó của em rụng ra ngoài.
Dialogue: 0,0:13:17.63,0:13:21.15,Default,,0000,0000,0000,,Đúng rồi. Như thế.
Dialogue: 0,0:13:24.73,0:13:26.26,Default,,0000,0000,0000,,Như thế...
Dialogue: 0,0:13:26.40,0:13:27.87,Default,,0000,0000,0000,,Anh ra rồi.
Dialogue: 0,0:13:27.97,0:13:29.37,Default,,0000,0000,0000,,Vâng, hãy ra đi.
Dialogue: 0,0:13:29.47,0:13:30.56,Default,,0000,0000,0000,,Đưa hết chúng cho em.
Dialogue: 0,0:13:31.61,0:13:34.63,Default,,0000,0000,0000,,Hãy ra thật nhiều sữa từ cậu bé của anh.
Dialogue: 0,0:13:59.10,0:14:02.76,Default,,0000,0000,0000,,Anh có thấy cui khi Fumi-chan trở về ko?
Dialogue: 0,0:14:02.87,0:14:04.27,Default,,0000,0000,0000,,Dĩ nhiên là anh vui.
Dialogue: 0,0:14:04.47,0:14:07.46,Default,,0000,0000,0000,,Anh có vẻ rất ngạc nhiên, vậy...
Dialogue: 0,0:14:07.61,0:14:10.24,Default,,0000,0000,0000,,Xin lỗi, em về nhà một mình được ko.
Dialogue: 0,0:14:10.68,0:14:12.58,Default,,0000,0000,0000,,Anh để quên vài thứ.
Dialogue: 0,0:14:14.48,0:14:17.68,Default,,0000,0000,0000,,Anh định làm gì vào giờ ăn tối vậy?
Dialogue: 0,0:14:26.49,0:14:30.47,sign,,0000,0000,0000,,{\pos(278,172)}Chị đã trở về! - Fumino
Dialogue: 0,0:14:31.97,0:14:33.40,Default,,0000,0000,0000,,Điều này tệ thật.
Dialogue: 0,0:14:34.00,0:14:36.03,Default,,0000,0000,0000,,Thực sự rất tệ.
Dialogue: 0,0:14:36.67,0:14:41.11,Default,,0000,0000,0000,,Lí do Fumi-nee ra đi là vì chị ấy căm ghét Fujino-san.
Dialogue: 0,0:14:41.64,0:14:46.01,Default,,0000,0000,0000,,Và điều khiến cô ấy bỏ đi chính là những việc đã xảy ra vào ngày đó.
Dialogue: 0,0:14:47.31,0:14:51.55,Default,,0000,0000,0000,,Kể cả khi chị ấy quay về, sẽ chẳng có gì thay đổi cả.
Dialogue: 0,0:14:52.12,0:14:59.39,Default,,0000,0000,0000,,Hơn nữa, Fujino-san còn làm chuyện đó nhiều hơn kể từ khi chị ấy bỏ đi.
Dialogue: 0,0:15:01.76,0:15:05.20,Default,,0000,0000,0000,,Nhưng vấn đề tai hại nhất là...
Dialogue: 0,0:15:06.70,0:15:10.50,Default,,0000,0000,0000,,Việc Fujino-san và tôi đang dan díu với nhau.
Dialogue: 0,0:15:11.94,0:15:15.17,Default,,0000,0000,0000,,Nếu chị ấy biết được chuyện này, mọi chuyện sẽ vô cùng tồi tệ.
Dialogue: 0,0:15:16.38,0:15:20.58,Default,,0000,0000,0000,,Vì sao cô ấy lại dan díu với mình nhỉ?
Dialogue: 0,0:15:20.98,0:15:21.71,Default,,0000,0000,0000,,Có thể là...
Dialogue: 0,0:15:24.08,0:15:27.52,Default,,0000,0000,0000,,Có vẻ như Fujino-san đã ko thỏa mãn trong nhiều hôm...
Dialogue: 0,0:15:27.69,0:15:29.82,Default,,0000,0000,0000,,Shinobu đã được nhận vào học tại một ngôi trường mới...
Dialogue: 0,0:15:30.12,0:15:32.32,Default,,0000,0000,0000,,Mình ước gì Fumi-nee có thể trở về.
Dialogue: 0,0:15:32.86,0:15:33.72,Default,,0000,0000,0000,,Ai đó?
Dialogue: 0,0:15:33.86,0:15:35.12,Default,,0000,0000,0000,,Là cô đây.
Dialogue: 0,0:15:48.81,0:15:52.37,Default,,0000,0000,0000,,Fujino-san, làm ơn dừng lại.
Dialogue: 0,0:15:52.48,0:15:56.21,Default,,0000,0000,0000,,Oh thôi nào, cô đang rất vui nên sẽ dạy cháu một vài thứ.
Dialogue: 0,0:15:56.32,0:15:58.25,Default,,0000,0000,0000,,Chuyện này vui lắm đấy.
Dialogue: 0,0:16:27.85,0:16:30.18,Default,,0000,0000,0000,,Cô hiểu vì sao cháu lại ra nhanh như vậy, lần đầu phải ko?
Dialogue: 0,0:16:30.32,0:16:32.62,Default,,0000,0000,0000,,Và nó đặc sệt như thạch.
Dialogue: 0,0:16:34.72,0:16:36.62,Default,,0000,0000,0000,,Cháu vẫn còn cứng này.
Dialogue: 0,0:16:36.89,0:16:38.95,Default,,0000,0000,0000,,Cô nghĩ mình còn có thể thưởng thức thêm chút nữa.
Dialogue: 0,0:17:03.88,0:17:06.32,Default,,0000,0000,0000,,Mình đáng ra phải từ chối ngay từ lúc đó.
Dialogue: 0,0:17:06.52,0:17:08.35,Default,,0000,0000,0000,,Yeah, Mình cần phải làm như vậy.
Dialogue: 0,0:17:09.22,0:17:13.13,Default,,0000,0000,0000,,CẢ mình và Shinobu sẽ rời khỏi ngôi nhà này vào một lúc nào đó.
Dialogue: 0,0:17:13.39,0:17:15.83,Default,,0000,0000,0000,,Dù cho Fujino-san là một phụ nữ dâm đãng...
Dialogue: 0,0:17:15.93,0:17:17.73,Default,,0000,0000,0000,,...Mình chỉ cần dứt khoát từ chối việc đó với cô ta...
Dialogue: 0,0:17:18.37,0:17:21.96,Default,,0000,0000,0000,,Nhưng hiện giờ nếu cứ thế này, mọi việc sẽ trở nên tồi tệ.
Dialogue: 0,0:17:22.14,0:17:23.33,Default,,0000,0000,0000,,Mình quyết tâm rồi.
Dialogue: 0,0:17:27.57,0:17:29.57,Default,,0000,0000,0000,,Đây là chỗ cháu nhét nó vào.
Dialogue: 0,0:17:29.68,0:17:31.20,Default,,0000,0000,0000,,Làm điều đó nhanh lên nào.
Dialogue: 0,0:17:31.31,0:17:33.34,Default,,0000,0000,0000,,Nhét nó vào thật sâu nhé.
Dialogue: 0,0:17:35.88,0:17:39.34,Default,,0000,0000,0000,,Chỗ này này. Đầu tiên ấn nó vào đây.
Dialogue: 0,0:17:40.99,0:17:42.62,Default,,0000,0000,0000,,Đừng sợ.
Dialogue: 0,0:17:42.82,0:17:45.35,Default,,0000,0000,0000,,Nó ko cắn cháu đâu.
Dialogue: 0,0:17:54.03,0:17:56.63,Default,,0000,0000,0000,,Cái đó vào trong cô rồi! Nó vẫn đang vào trong!
Dialogue: 0,0:17:57.04,0:17:59.94,Default,,0000,0000,0000,,Cái đó đang ở bên trong.
Dialogue: 0,0:18:01.48,0:18:04.60,Default,,0000,0000,0000,,Cô ko cần dạy cháu phải làm gì tiếp theo, đúng ko?
Dialogue: 0,0:18:17.19,0:18:19.22,Default,,0000,0000,0000,,Đúng rồi, như thế.
Dialogue: 0,0:18:19.43,0:18:20.62,Default,,0000,0000,0000,,Ra rồi vào.
Dialogue: 0,0:18:22.93,0:18:24.69,Default,,0000,0000,0000,,Tiếp tục như thế.
Dialogue: 0,0:18:25.13,0:18:26.36,Default,,0000,0000,0000,,Ra rồi vào.
Dialogue: 0,0:18:29.54,0:18:30.70,Default,,0000,0000,0000,,Nữa đi.
Dialogue: 0,0:18:33.34,0:18:34.77,Default,,0000,0000,0000,,Chơi cô mạnh hơn nữa.
Dialogue: 0,0:18:35.18,0:18:36.90,Default,,0000,0000,0000,,Cô muốn cháu làm thật mạnh .
Dialogue: 0,0:18:38.95,0:18:41.41,Default,,0000,0000,0000,,Như thế. Đúng rồi, đúng rồi.
Dialogue: 0,0:18:41.52,0:18:43.28,Default,,0000,0000,0000,,Chơi đúng chỗ đó.
Dialogue: 0,0:18:43.65,0:18:44.64,Default,,0000,0000,0000,,Cháu khá lắm.
Dialogue: 0,0:18:45.09,0:18:46.81,Default,,0000,0000,0000,,Đừng rút nó ra.
Dialogue: 0,0:18:46.95,0:18:48.22,Default,,0000,0000,0000,,Cái đó của cháu tuyệt lắm.
Dialogue: 0,0:18:48.76,0:18:50.48,Default,,0000,0000,0000,,Đừng rút cái của cháu ra.
Dialogue: 0,0:18:59.43,0:19:02.66,Default,,0000,0000,0000,,Cô muốn cháu làm thật chậm và nhẹ nhàng nhưng hãy nhét tất cả cái đó vào trong khi cháu làm.
Dialogue: 0,0:19:05.81,0:19:08.37,Default,,0000,0000,0000,,Đúng rồi. Cái đó của cháu tuyệt lắm.
Dialogue: 0,0:19:18.09,0:19:19.11,Default,,0000,0000,0000,,Đúng rồi.
Dialogue: 0,0:19:21.59,0:19:24.42,Default,,0000,0000,0000,,Sướng quá. Thật tuyệt vời.
Dialogue: 0,0:19:37.70,0:19:38.76,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:19:40.57,0:19:42.30,Default,,0000,0000,0000,,Đúng rồi.
Dialogue: 0,0:19:55.89,0:19:57.48,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:19:58.66,0:20:00.15,Default,,0000,0000,0000,,Take-san.
Dialogue: 0,0:20:10.87,0:20:12.93,Default,,0000,0000,0000,,Take-chan, anh tắm lâu quá đấy.
Dialogue: 0,0:20:13.04,0:20:14.34,Default,,0000,0000,0000,,Nhanh ra ngoài đi nào.
Dialogue: 0,0:20:15.68,0:20:18.34,Default,,0000,0000,0000,,Cứ trả lời nó như bình thường.
Dialogue: 0,0:20:18.95,0:20:20.71,Default,,0000,0000,0000,,Anh sẽ ra ngay khi tắm xong.
Dialogue: 0,0:20:20.81,0:20:23.65,Default,,0000,0000,0000,,Được thôi. Nhưng phải nhanh lên đấy.
Dialogue: 0,0:20:25.05,0:20:28.08,Default,,0000,0000,0000,,Cháu biết là chúng ta đang ở trong tình thế khó xử nhưng chúng ta hãy ngừng việc này lại.
Dialogue: 0,0:20:28.22,0:20:30.12,Default,,0000,0000,0000,,Đây chỉ là do chúng ta mất tự chủ, đúng ko?
Dialogue: 0,0:20:30.36,0:20:31.12,Default,,0000,0000,0000,,Cháu nói đúng.
Dialogue: 0,0:20:31.29,0:20:35.09,Default,,0000,0000,0000,,Có vẻ như cái đó của cháu đang mềm đi bên trong cô.
Dialogue: 0,0:20:40.17,0:20:41.33,Default,,0000,0000,0000,,Fujino-san!
Dialogue: 0,0:20:44.57,0:20:46.47,Default,,0000,0000,0000,,Cô đùa đấy.
Dialogue: 0,0:20:47.11,0:20:48.90,Default,,0000,0000,0000,,Chúng ta tiếp tục nào.
Dialogue: 0,0:20:50.78,0:20:55.31,Default,,0000,0000,0000,,Thấy chưa? Cái đó của cháu lại cứng lên rồi.
Dialogue: 0,0:21:16.67,0:21:17.14,Default,,0000,0000,0000,,Đúng thế.
Dialogue: 0,0:21:50.44,0:21:53.67,Default,,0000,0000,0000,,Nó đang vào sâu trong tử cung cô.
Dialogue: 0,0:21:54.94,0:21:57.14,Default,,0000,0000,0000,,Cái đó của cháu vẫn tiếp tục cứng lên.
Dialogue: 0,0:21:57.94,0:22:00.50,Default,,0000,0000,0000,,Cháu hồi phục nhanh như vậy vì cháu hãy còn đang thanh niên.
Dialogue: 0,0:22:00.65,0:22:01.81,Default,,0000,0000,0000,,Tuyệt vời.
Dialogue: 0,0:22:02.68,0:22:04.78,Default,,0000,0000,0000,,Sao? Cháu sắp ra rồi à?
Dialogue: 0,0:22:05.15,0:22:06.18,Default,,0000,0000,0000,,Vậy chúng ta ra cùng nhau nhé.
Dialogue: 0,0:22:07.45,0:22:09.62,Default,,0000,0000,0000,,Hãy ngắm nhìn gương mặt cô khi cô thỏa mãn.
Dialogue: 0,0:22:24.67,0:22:31.13,Default,,0000,0000,0000,,Cô ra rồi!
Dialogue: 0,0:22:35.62,0:22:39.88,Default,,0000,0000,0000,,Ko thể tin được là tôi lại yêu một người như anh.
Dialogue: 0,0:22:40.22,0:22:41.08,Default,,0000,0000,0000,,Sao em lại nói vậy?
Dialogue: 0,0:22:41.19,0:22:42.42,Default,,0000,0000,0000,,Cuộc cãi vã của một đôi tình nhân, huh?
Dialogue: 0,0:22:42.69,0:22:44.09,Default,,0000,0000,0000,,Hay thật.
Dialogue: 0,0:22:44.39,0:22:46.62,Default,,0000,0000,0000,,So với họ, mình...
Dialogue: 0,0:22:46.76,0:22:48.32,Default,,0000,0000,0000,,Hãy để anh làm điều đó lần cuối.
Dialogue: 0,0:22:48.46,0:22:49.45,Default,,0000,0000,0000,,Dừng lại ngay.
Dialogue: 0,0:22:50.86,0:22:53.06,Default,,0000,0000,0000,,Hey, Chờ đã nào.
Dialogue: 0,0:23:10.18,0:23:12.65,Default,,0000,0000,0000,,Cậu nhìn tôi kiểu gì đấy?
Dialogue: 0,0:23:14.76,0:23:15.69,Default,,0000,0000,0000,,Takeshi!
Dialogue: 0,0:23:16.92,0:23:18.22,Default,,0000,0000,0000,,Fumi-nee?
