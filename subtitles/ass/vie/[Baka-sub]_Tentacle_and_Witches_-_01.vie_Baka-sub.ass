Dialogue: 0,0:00:29.53,0:00:32.46,Default,Ino,0000,0000,0000,,Thủy linh, trường sinh bất lão.
Dialogue: 0,0:00:33.07,0:00:36.44,Default,Ino,0000,0000,0000,,Các bậc hiền giả của nhân loại, xin hãy quy tụ về đây...
Dialogue: 0,0:00:36.44,0:00:38.84,Default,Ino,0000,0000,0000,,Truyền hơi thở sinh mệnh cho tôi!
Dialogue: 0,0:00:43.70,0:00:48.29,Default,Tac,0000,0000,0000,,Ở trong tòa lâu đài này, có một phù thủy đang định cư.
Dialogue: 0,0:00:48.29,0:00:50.39,Default,Tac,0000,0000,0000,,Tên cô ấy là Yuko Morino.
Dialogue: 0,0:00:50.39,0:00:54.39,Default,Tac,0000,0000,0000,,Vào ban ngày, cô ấy chính là giáo viên chủ nhiệm của tôi.
Dialogue: 0,0:00:55.11,0:00:58.00,Default,Ino,0000,0000,0000,,Trên bàn tay phù thủy, xen giữa các ngón tay...
Dialogue: 0,0:00:58.00,0:01:00.29,Default,Ino,0000,0000,0000,,Tôi triệu hồi người, nữ thủy thần Qreet !
Dialogue: 0,0:01:00.29,0:01:03.31,Default,Mor,0000,0000,0000,,Tắm mình trong ánh dương rực rỡ, và ban hơi ấm cho tôi!
Dialogue: 0,0:01:16.62,0:01:22.63,Default,sign{1.17},0000,0000,0000,,{\bord0\c&H9FAE28&\fnUVN Chim Bien Nhe\pos(359,470)}Phần 1          Biến thành xúc tu
Dialogue: 0,0:01:16.62,0:01:22.63,Default,sign{1.17},0000,0000,0000,,{\bord5\fnUVN Nguyen Du\3c&H040305&\c&HA16BD2&\pos(357,38)}Baka-sub {\c&HA6666C&}trân trọng giới thiệu
Dialogue: 0,0:01:16.62,0:01:22.63,Default,sign{1.17},0000,0000,0000,,{\bord2\fnUVN Nguyen Du\c&HA16BD2&\pos(357,38)\3c&HFFFFFF&}Baka-sub {\c&HA6666C&}trân trọng giới thiệu
Dialogue: 0,0:01:25.40,0:01:26.97,Default,Tac,0000,0000,0000,,Tên tôi là Tachibana Ichiro.
Dialogue: 0,0:01:27.51,0:01:30.34,Default,Tac,0000,0000,0000,,Căng mắt ngắm nhìn Yuko-sensei xinh đẹp quyến rũ...
Dialogue: 0,0:01:30.34,0:01:34.20,Default,Tac,0000,0000,0000,,là thú vui độc nhất của tôi hiện giờ.
Dialogue: 0,0:01:35.06,0:01:37.71,Default,Tac,0000,0000,0000,,Ái chà, hai đầu vú cô ấy phơi hết cả ra ngoài rồi!
Dialogue: 0,0:01:37.71,0:01:39.44,Default,Tac,0000,0000,0000,,Tròn trịa và ướt át!
Dialogue: 0,0:01:40.31,0:01:42.55,Default,Tac,0000,0000,0000,,Thiên đường là đây!
Dialogue: 0,0:01:53.19,0:01:55.23,Default,Gir,0000,0000,0000,,Ngươi đang làm trò gì thế, tên người phàm?
Dialogue: 0,0:01:57.74,0:02:00.61,Default,Tac,0000,0000,0000,,Cô là...
Dialogue: 0,0:02:10.44,0:02:11.68,Default,Ino,0000,0000,0000,,Á!
Dialogue: 0,0:02:13.08,0:02:16.05,Default,Tac,0000,0000,0000,,Yuko-sensei, tuyệt quá!
Dialogue: 0,0:02:16.05,0:02:18.98,Default,Tac,0000,0000,0000,,Bộ ngực cô lúc nào cũng sung mãn và rực rỡ như vậy!
Dialogue: 0,0:02:21.99,0:02:25.01,Default,Tac,0000,0000,0000,,Mềm mại và co giãn như bắp đùi!
Dialogue: 0,0:02:28.40,0:02:30.93,Default,Tac,0000,0000,0000,,Oaaa!
Dialogue: 0,0:02:33.28,0:02:34.65,Default,Ino,0000,0000,0000,,Ichiro-kun!
Dialogue: 0,0:02:36.95,0:02:38.06,Default,Ino,0000,0000,0000,,Em không sao chứ?
Dialogue: 0,0:02:39.55,0:02:43.50,Default,Gir,0000,0000,0000,,Cứ mặc kệ tên biến thái này nằm đây và tiếp tục trận đấu thôi, Yuko-sensei!
Dialogue: 0,0:02:44.62,0:02:47.65,Default,Tac,0000,0000,0000,,Mẹ trẻ này tên là Futaba Lily Ramses.
Dialogue: 0,0:02:47.65,0:02:49.65,Default,Tac,0000,0000,0000,,Cô ta có 1/4 dòng máu Ai Cập.
Dialogue: 0,0:02:49.65,0:02:51.84,Default,Tac,0000,0000,0000,,Người đẹp dáng chuẩn, nói chung là ngon lành.
Dialogue: 0,0:02:52.48,0:02:54.49,Default,Tac,0000,0000,0000,,Bỏ cái chân cô ra nào!
Dialogue: 0,0:02:55.62,0:02:57.39,Default,Lil,0000,0000,0000,,Mi vẫn chưa ngỏm à, người phàm?
Dialogue: 0,0:02:57.39,0:02:59.03,Default,Lil,0000,0000,0000,,Đúng là sống dai hơn cả loài gián!
Dialogue: 0,0:03:00.40,0:03:02.33,Default,Tac,0000,0000,0000,,Nhưng tính tình y như con dở hơi.
Dialogue: 0,0:03:02.33,0:03:05.04,Default,Tac,0000,0000,0000,,Ếu hiểu sao, cô ta chỉ lạnh nhạt với riêng mình.
Dialogue: 0,0:03:05.04,0:03:09.39,Default,Tac,0000,0000,0000,,Chả biết ông bà già cô ả dạy dỗ kiểu gì mà tôi lại trở thành thằng hứng tạ thế này?
Dialogue: 0,0:03:09.39,0:03:11.76,Default,Tac,0000,0000,0000,,Nếu có dịp mình phải xem mặt ông bà già cô ả mới được.
Dialogue: 0,0:03:11.76,0:03:13.04,Default,Tac,0000,0000,0000,,Và rồi... thì là mà...
Dialogue: 0,0:03:13.86,0:03:15.17,Default,Lil,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:03:15.17,0:03:17.96,Default,Lil,0000,0000,0000,,Tên người phàm này bất lực rồi.
Dialogue: 0,0:03:19.60,0:03:21.80,Default,Ino,0000,0000,0000,,Ichiro-kun thật là...
Dialogue: 0,0:03:21.80,0:03:23.28,Default,Ino,0000,0000,0000,,Giờ cô phải xử lí em thế nào đây?
Dialogue: 0,0:03:24.54,0:03:26.40,Default,Tac,0000,0000,0000,,Cám ơn đã giúp đỡ!
Dialogue: 0,0:03:26.97,0:03:29.25,Default,Lil,0000,0000,0000,,Ngươi nhìn cái gì thế hả, tên dê cụ!?
Dialogue: 0,0:03:29.25,0:03:31.21,Default,Tac,0000,0000,0000,,Tôi có cử động nổi nữa đâu mà dê với chả không!
Dialogue: 0,0:03:31.21,0:03:33.79,Default,Tac,0000,0000,0000,,Cứ thế này thì cô sẽ chẳng lấy chồng nổi đâu!
Dialogue: 0,0:03:33.79,0:03:35.65,Default,Lil,0000,0000,0000,,Không cần ngươi lo.
Dialogue: 0,0:03:35.65,0:03:40.40,Default,Lil,0000,0000,0000,,Với ta lúc này, từ sáng sớm tới tối mịt, không một phút ngơi nghỉ,
Dialogue: 0,0:03:40.40,0:03:43.38,Default,Lil,0000,0000,0000,,24 giờ trong ngày, 365 ngày trong năm, ta luôn giữ gìn phẩm giá thanh cao!
Dialogue: 0,0:03:47.44,0:03:50.55,Default,Tac,0000,0000,0000,,Cái quái gì vậy!? Thích đóng vai công chúa kiêu kì à!
Dialogue: 0,0:03:50.55,0:03:54.16,Default,Kid,0000,0000,0000,,Này Tachibana! Sao mày dám gọi Lily-sama như vậy!
Dialogue: 0,0:03:54.16,0:03:56.89,Default,Kid,0000,0000,0000,,Cố ấy không cùng đẳng cấp với mày!
Dialogue: 0,0:03:56.89,0:04:00.25,Default,Kid,0000,0000,0000,,Lỡ cô ấy nổi điên lên và chuyển trường thì sao?
Dialogue: 0,0:04:02.32,0:04:05.32,Default2,sign{4.05},0000,0000,0000,,{\pos(566,69)\fad(600,0)}Futaba Lily Ramses
Dialogue: 0,0:04:02.99,0:04:05.32,Default,Tac,0000,0000,0000,,Cô ả chuyển tới đây từ vài tháng trước.
Dialogue: 0,0:04:05.78,0:04:08.33,Default,Tac,0000,0000,0000,,Và nhanh chóng trở thành thần tượng trong trường học.
Dialogue: 0,0:04:08.94,0:04:11.97,Default,Tac,0000,0000,0000,,Dáng vẻ ngây thơ đã khiến cô ả nổi tiếng không thua gì Yuko-sensei.
Dialogue: 0,0:04:12.71,0:04:13.57,Default,Lil,0000,0000,0000,,Đỡ này!
Dialogue: 0,0:04:17.91,0:04:19.06,Default,Tac,0000,0000,0000,,Mắt mũi để đâu thế hả, cái đồ...
Dialogue: 0,0:04:20.07,0:04:22.01,Default,Tac,0000,0000,0000,,Con ả chết tiệt!
Dialogue: 0,0:04:22.01,0:04:27.32,Default,Tac,0000,0000,0000,,Đêm nay ông sẽ "xử đẹp" mày trong mơ, sau khi vui vẻ với Yuko-sensei!
Dialogue: 0,0:04:28.26,0:04:29.09,Default,Tac,0000,0000,0000,,Cơ mà...
Dialogue: 0,0:04:29.75,0:04:32.60,Default,Lil,0000,0000,0000,,Lượn đi cho nước nó trong, người phàm!
Dialogue: 0,0:04:34.55,0:04:38.14,Default,Tac,0000,0000,0000,,Đang chuẩn bị "báo thù trong mơ" thì cô bỗng dưng xồ ra!?
Dialogue: 0,0:04:38.14,0:04:40.92,Default,Lil,0000,0000,0000,,Hả? Định giả ngu với ta hả?
Dialogue: 0,0:04:40.92,0:04:43.09,Default,Lil,0000,0000,0000,,Chúng ta tới đây với cùng một mục đích.
Dialogue: 0,0:04:43.74,0:04:44.51,Default,Tac,0000,0000,0000,,Nhìn lén?
Dialogue: 0,0:04:44.51,0:04:46.05,Default,Lil,0000,0000,0000,,Không phải!
Dialogue: 0,0:04:48.30,0:04:52.13,Default,Tac,0000,0000,0000,,Giờ mới để ý, cô ả ăn mặc kiểu gì thế kia.
Dialogue: 0,0:04:52.13,0:04:54.85,Default,Tac,0000,0000,0000,,Nhìn cũng na ná Yuko-sensei.
Dialogue: 0,0:04:55.40,0:04:57.59,Default,Tac,0000,0000,0000,,Trông hệt như trang phục phù thủy biến thái.
Dialogue: 0,0:04:57.59,0:04:59.65,Default,Lil,0000,0000,0000,,Cái gì mà "phù thủy biến thái"!?
Dialogue: 0,0:04:59.65,0:05:04.46,Default,Lil,0000,0000,0000,,Đây là bộ trang phục truyền thống do tổ tiên ta để lại!
Dialogue: 0,0:05:05.27,0:05:07.31,Default,Tac,0000,0000,0000,,Xem ra cô có vẻ khoái "phù thủy" nhề?
Dialogue: 0,0:05:07.84,0:05:08.76,Default,Lil,0000,0000,0000,,Đồ đần độn!
Dialogue: 0,0:05:08.76,0:05:09.69,Default,,0000,0000,0000,,Ái da...
Dialogue: 0,0:05:10.30,0:05:13.63,Default,Lil,0000,0000,0000,,Ngươi có chối cãi nữa thì cũng chẳng ăn thua đâu.
Dialogue: 0,0:05:13.63,0:05:16.29,Default,Lil,0000,0000,0000,,Việc ngươi mò tới đây chính là bằng chứng xác thực nhất.
Dialogue: 0,0:05:16.29,0:05:17.96,Default,Tac,0000,0000,0000,,Bằng chứng?
Dialogue: 0,0:05:18.83,0:05:22.96,Default,Lil,0000,0000,0000,,Chỉ mình ta mới đủ tư cách trở thành đồ đệ của Phù thủy mặt trời Yuko Inoue!{check}
Dialogue: 0,0:05:22.96,0:05:25.43,Default,Tac,0000,0000,0000,,Đồ đệ của phù thủy mặt trời?
Dialogue: 0,0:05:27.13,0:05:28.84,Default,Lil,0000,0000,0000,,Vẫn còn cố cãi hử?
Dialogue: 0,0:05:28.84,0:05:31.17,Default,Lil,0000,0000,0000,,Mánh khóe tinh vi thật đấy.
Dialogue: 0,0:05:31.71,0:05:34.49,Default,Lil,0000,0000,0000,,Xem ra mi còn thông minh hơn vẻ bề ngoài.
Dialogue: 0,0:05:34.49,0:05:38.92,Default,Lil,0000,0000,0000,,Nhưng việc ngươi phá vỡ kết giới và mò vào đây đã chứng minh tất cả!
Dialogue: 0,0:05:39.68,0:05:40.92,Default,Lil,0000,0000,0000,,Sẵn sàng chịu chết đi!
Dialogue: 0,0:05:41.53,0:05:45.30,Default,Lil,0000,0000,0000,,Với lá bùa tiên tri mua từ tay ông lão bán rong ven đường, ta sẽ xóa đi trí nhớ của ngươi!
Dialogue: 0,0:05:45.30,0:05:48.16,Default,Tac,0000,0000,0000,,Tôi éo biết cô đang lảm nhảm cái gì, nhưng tự nhiên bụng dạ óc ách khó chịu quá.
Dialogue: 0,0:05:49.11,0:05:55.89,Default,Lil,0000,0000,0000,,Trên bàn tay phù thủy, xen giữa các ngón tay,  tôi triệu hồi người, nữ thủy thần Qreet!
Dialogue: 0,0:05:51.92,0:05:53.65,Default,Tac,0000,0000,0000,,Bùa chú?
Dialogue: 0,0:05:53.65,0:05:55.89,Default,Tac,0000,0000,0000,,Cô là Ma pháp thiếu nữ à?
Dialogue: 0,0:05:55.89,0:05:58.33,Default,Lil,0000,0000,0000,,Ta là {\i1}Phù thủy{\i0}!
Dialogue: 0,0:05:58.33,0:06:00.85,Default,Tac,0000,0000,0000,,Tachibana Ichiro, ngươi sẽ phải hối hận!
Dialogue: 0,0:06:12.05,0:06:15.50,Default,Ino,0000,0000,0000,,Ichiro-kun... Ichiro-kun!
Dialogue: 0,0:06:20.22,0:06:22.44,Default,Ino,0000,0000,0000,,Em có nghe thấy cô nói gì không?
Dialogue: 0,0:06:22.44,0:06:26.00,Default,Tac,0000,0000,0000,,Em vẫn ổn. Em nghe thấy rồi.
Dialogue: 0,0:06:27.79,0:06:30.40,Default,Tac,0000,0000,0000,,Á! Quái vật!
Dialogue: 0,0:06:30.40,0:06:32.47,Default,Tac,0000,0000,0000,,Khoan đã, tay em bị sao thế này?
Dialogue: 0,0:06:34.40,0:06:37.20,Default,Tac,0000,0000,0000,,Chuyện quái gì vậy!?
Dialogue: 0,0:06:37.20,0:06:39.81,Default,Ino,0000,0000,0000,,Bình tĩnh nào, Ichiro-kun.
Dialogue: 0,0:06:39.81,0:06:42.75,Default,Ino,0000,0000,0000,,Em đã bị biến thành quái vật!
Dialogue: 0,0:06:42.75,0:06:44.75,Default,Tac,0000,0000,0000,,Biến thành quái vật?
Dialogue: 0,0:06:44.75,0:06:48.74,Default,Ino,0000,0000,0000,,Em cứ bình tĩnh nghe cô nói hết đã nào!?
Dialogue: 0,0:06:51.70,0:06:55.91,Default,Tac,0000,0000,0000,,Tên nhà quê dở hơi, toàn đi ve vãn Yuko-sensei!
Dialogue: 0,0:06:55.91,0:06:57.78,Default,Man,0000,0000,0000,,Xin lỗi, Tiểu thư.
Dialogue: 0,0:06:57.78,0:07:00.50,Default,Man,0000,0000,0000,,Cô đang gặp phiền phức phải không?
Dialogue: 0,0:07:01.73,0:07:05.63,Default,Man,0000,0000,0000,,Tôi có một thứ...
Dialogue: 0,0:07:06.14,0:07:10.07,Default,Man,0000,0000,0000,,Nếu cô muốn, nó sẽ biến bất kì ai thành người hầu của cô.
Dialogue: 0,0:07:25.30,0:07:27.65,Default,Lil,0000,0000,0000,,Nhưng tôi không ngờ chuyện này lại xảy ra.
Dialogue: 0,0:07:29.35,0:07:33.15,Default,Tac,0000,0000,0000,,Biến tôi thành người hầu của cô!?
Dialogue: 0,0:07:33.15,0:07:35.00,Default,Lil,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:07:35.00,0:07:39.42,Default,Tac,0000,0000,0000,,Ha! Đéo ai muốn làm người hầu cho con dở hơi như cô?
Dialogue: 0,0:07:39.42,0:07:41.25,Default,Lil,0000,0000,0000,,Anh nói cái gì!?
Dialogue: 0,0:07:41.25,0:07:43.36,Default,Ino,0000,0000,0000,,Hai người thôi đi nào! {reverse order?}
Dialogue: 0,0:07:44.11,0:07:47.99,Default,Ino,0000,0000,0000,,Futaba-san thật sự hối hận về chuyện vừa rồi.
Dialogue: 0,0:07:47.99,0:07:50.20,Default,Ino,0000,0000,0000,,Hãy tha lỗi cho cô ấy, Ichiro-kun.
Dialogue: 0,0:07:50.65,0:07:53.25,Default,Tac,0000,0000,0000,,Đừng có ra vẻ với tôi nữa!
Dialogue: 0,0:07:55.41,0:07:57.26,Default,Ino,0000,0000,0000,,Lá bùa này là đồ giả.
Dialogue: 0,0:07:58.35,0:08:02.06,Default,Ino,0000,0000,0000,,Nhìn qua thì nó rất giống loại bùa chú biến kẻ khác thành người hầu,
Dialogue: 0,0:08:02.06,0:08:03.85,Default,Ino,0000,0000,0000,,Nhưng lại là đồ giả mạo.
Dialogue: 0,0:08:03.85,0:08:05.59,Default,Ino,0000,0000,0000,,Cô ngửi thấy mùi ma quỷ từ nó.
Dialogue: 0,0:08:07.03,0:08:09.35,Default,Tac,0000,0000,0000,,Làm ơn đi, Yuko-sensei!
Dialogue: 0,0:08:09.35,0:08:12.73,Default,Tac,0000,0000,0000,,Hãy biến em trở lại bình thường!
Dialogue: 0,0:08:12.73,0:08:15.05,Default,Ino,0000,0000,0000,,Đối phó với quái vật là chuyện vô cùng khó khăn.
Dialogue: 0,0:08:15.65,0:08:17.74,Default,Ino,0000,0000,0000,,Cô e rằng sẽ mất một khoảng thời gian dài.
Dialogue: 0,0:08:17.74,0:08:19.43,Default,Tac,0000,0000,0000,,Ôi trời!
Dialogue: 0,0:08:23.11,0:08:24.51,Default,Ino,0000,0000,0000,,Chính là nó...
Dialogue: 0,0:08:29.82,0:08:30.99,Default,Ino,0000,0000,0000,,Futaba-san!
Dialogue: 0,0:08:34.23,0:08:35.69,Default,Lil,0000,0000,0000,,Không!
Dialogue: 0,0:08:37.55,0:08:40.00,Default,Tac,0000,0000,0000,,Mình vừa làm gì thế này?
Dialogue: 0,0:08:40.00,0:08:44.24,Default,Tac,0000,0000,0000,,Ichiro-kun, làm tình với cô đi.
Dialogue: 0,0:08:44.24,0:08:45.66,Default,Tac,0000,0000,0000,,Ế!?
Dialogue: 0,0:08:46.24,0:08:47.98,Default,Lil,0000,0000,0000,,Yuko-sensei!?
Dialogue: 0,0:08:47.98,0:08:53.35,Default,Ino,0000,0000,0000,,Để quái vật hoàn toàn hiện hình, cần có năng lượng của kẻ triệu hồi.
Dialogue: 0,0:08:53.35,0:08:55.16,Default,Ino,0000,0000,0000,,Chúng cũng cần năng lượng để sinh tồn!
Dialogue: 0,0:08:55.80,0:09:00.11,Default,Ino,0000,0000,0000,,Nhưng em lại không bị triệu hồi bởi bất kì ai, Ichiro-kun.
Dialogue: 0,0:09:00.11,0:09:05.68,Default,Ino,0000,0000,0000,,Vậy nên em buộc phải hấp thu năng lượng trực tiếp từ phù thủy! {wording?}
Dialogue: 0,0:09:06.21,0:09:08.07,Default,Tac,0000,0000,0000,,Trực tiếp?
Dialogue: 0,0:09:08.07,0:09:11.70,Default,Ino,0000,0000,0000,,Nói cách khác, em buộc phải làm tình với phù thủy!
Dialogue: 0,0:09:11.70,0:09:14.98,Default,Tac,0000,0000,0000,,Đúng rồi. Mình đã hiểu ra toàn bộ vấn đề.
Dialogue: 0,0:09:15.66,0:09:20.04,Default,Tac,0000,0000,0000,,Khi cơ thể mình cần năng lượng, sự đói khát sẽ chuyển hóa thành dục vọng mãnh liệt...
Dialogue: 0,0:09:20.04,0:09:22.56,Default,Tac,0000,0000,0000,,cùng ham muốn giải tỏa cơn khát bằng năng lượng của Yuko-sensei.
Dialogue: 0,0:09:22.56,0:09:24.84,Default,Tac,0000,0000,0000,,Trực tiếp thông qua Yuko-sensei,
Dialogue: 0,0:09:24.84,0:09:28.22,Default,Tac,0000,0000,0000,,hấp thu chất dinh dưỡng từ chính dâm thủy của cô ấy. {lit mucous  \Nmembrane}
Dialogue: 0,0:09:28.76,0:09:32.05,Default,Tac,0000,0000,0000,,Ăn đến khi no bụng mới thôi!
Dialogue: 0,0:09:32.05,0:09:35.72,Default,Tac,0000,0000,0000,,Làm tình với phù thủy để đổi lấy chất dinh dưỡng!
Dialogue: 0,0:09:36.72,0:09:38.43,Default,Tac,0000,0000,0000,,Gieo hạt giống của mình vào bên trong cơ thể phù thủy!
Dialogue: 0,0:09:38.43,0:09:39.77,Default,Tac,0000,0000,0000,,Và sau đó chơi họ không ngừng nghỉ!
Dialogue: 0,0:09:39.77,0:09:41.47,Default,Tac,0000,0000,0000,,Không ngừng nghỉ!
Dialogue: 0,0:09:42.76,0:09:45.07,Default,Tac,0000,0000,0000,,Yuko-sensei, em...
Dialogue: 0,0:09:46.60,0:09:47.48,Default,Tac,0000,0000,0000,,Mau chạy đi.
Dialogue: 0,0:09:47.48,0:09:53.98,Default,Ino,0000,0000,0000,,Mau chơi cô... và hấp thu năng lượng đi!
Dialogue: 0,0:09:53.98,0:09:55.81,Default,Ino,0000,0000,0000,,Đừng cố chống lại nó.
Dialogue: 0,0:09:55.81,0:09:58.91,Default,Ino,0000,0000,0000,,Nếu em từ chối, bản năng ác quỷ sẽ chiếm hữu cơ thể,
Dialogue: 0,0:09:58.91,0:10:02.13,Default,Ino,0000,0000,0000,,và khiến em phát điên!
Dialogue: 0,0:10:02.68,0:10:07.50,Default,Ino,0000,0000,0000,,Khi đó không chỉ cô và Futaba-san...
Dialogue: 0,0:10:07.93,0:10:10.69,Default,Ino,0000,0000,0000,,Hàng trăm người phụ nữ khác sẽ trở thành nạn nhân của em!
Dialogue: 0,0:10:10.69,0:10:12.06,Default,Tac,0000,0000,0000,,Yuko-sensei...
Dialogue: 0,0:10:13.25,0:10:19.20,Default,Ino,0000,0000,0000,,Cô vẫn còn là trinh nữ, vậy nên hãy bắt đầu thật nhẹ nhàng thôi nhé.
Dialogue: 0,0:10:19.50,0:10:23.44,Default,Tac,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:10:38.43,0:10:39.42,Default,Ino,0000,0000,0000,,Ichiro-kun...
Dialogue: 0,0:10:41.92,0:10:43.15,Default,Lil,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:10:43.76,0:10:46.17,Default,Ino,0000,0000,0000,,Đừng lại gần đây, Futaba-san!
Dialogue: 0,0:10:46.83,0:10:49.61,Default,Ino,0000,0000,0000,,Đừng hít vào mùi hương đó!
Dialogue: 0,0:10:49.61,0:10:52.39,Default,Ino,0000,0000,0000,,Nó có khả năng kích dục rất mạnh!
Dialogue: 0,0:10:52.39,0:10:53.03,Default,Lil,0000,0000,0000,,Kích dục ư?
Dialogue: 0,0:10:53.03,0:10:56.83,Default,Ino,0000,0000,0000,,Quái vật dùng tinh dịch của chúng để khống chế phù thủy, và biến họ...
Dialogue: 0,0:10:56.83,0:10:58.40,Default,Ino,0000,0000,0000,,thành nô lệ! {syn for magic? this sounds kinda silly}
Dialogue: 0,0:10:58.40,0:11:01.44,Default,Tac,0000,0000,0000,,Yuko-sensei nói đúng.
Dialogue: 0,0:11:01.44,0:11:05.63,Default,Tac,0000,0000,0000,,Bản năng ác quỷ đang mách bảo mình làm điều đó!
Dialogue: 0,0:11:05.63,0:11:09.29,Default,Tac,0000,0000,0000,,Chỉ cần hít phải mùi hương đó, cô ấy sẽ mãi mãi...
Dialogue: 0,0:11:09.29,0:11:11.03,Default,Tac,0000,0000,0000,,trở thành nô lệ của mình!
Dialogue: 0,0:11:11.03,0:11:17.46,Default,Tac,0000,0000,0000,,Luôn luôn thèm khát, và không thể sống thiếu "chuyện ấy" cùng mình được!
Dialogue: 0,0:11:17.46,0:11:19.69,Default,Tac,0000,0000,0000,,Mầm mống ác quỷ và thiên đường tội lỗi!
Dialogue: 0,0:11:21.68,0:11:28.85,Default,Tac,0000,0000,0000,,Nào, Yuko-sensei! Hãy biến thành nô lệ vĩnh cửu, và thỏa mãn dục vọng của em đi!
Dialogue: 0,0:11:31.40,0:11:35.34,Default,Ino,0000,0000,0000,,Đừng lo. Cô sẽ nuốt hết tinh dịch của em.
Dialogue: 0,0:11:35.34,0:11:38.09,Default,Ino,0000,0000,0000,,Vậy nên, làm ơn chỉ nhắm tới một mình cô!
Dialogue: 0,0:11:38.71,0:11:40.11,Default,Ino,0000,0000,0000,,Đừng tấn công Futaba-san!
Dialogue: 0,0:11:52.12,0:11:53.12,Default,Lil,0000,0000,0000,,Yuko-sensei!
Dialogue: 0,0:12:12.36,0:12:17.81,Default,Ino,0000,0000,0000,,Xấu hổ chết đi được, nhưng cô đã chấp nhận hiến thân mình để nghiên cứu ma thuật,
Dialogue: 0,0:12:17.81,0:12:18.88,Default,Ino,0000,0000,0000,,Cô vẫn là trinh nữ.
Dialogue: 0,0:12:19.87,0:12:24.39,Default,Ino,0000,0000,0000,,Xin lỗi vì đã lấy đi lần đầu tiên của em trong hoàn cảnh này.
Dialogue: 0,0:12:50.93,0:12:51.48,Default,Ino,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:12:53.31,0:12:55.60,Default,Ino,0000,0000,0000,,Đừng đùa nghịch hai đầu vú cô như vậy!
Dialogue: 0,0:12:55.60,0:12:57.66,Default,Ino,0000,0000,0000,,Dường như cô sắp tan chảy rồi!
Dialogue: 0,0:12:58.70,0:13:02.59,Default,Ino,0000,0000,0000,,Hai bầu ngực cô như đang tan chảy!
Dialogue: 0,0:13:07.55,0:13:10.99,Default,Ino,0000,0000,0000,,Hai đầu ti... chúng đang...
Dialogue: 0,0:13:18.54,0:13:19.74,Default,Ino,0000,0000,0000,,Hai đầu ti, đừng mà!
Dialogue: 0,0:13:22.18,0:13:23.88,Default,Ino,0000,0000,0000,,Ichiro-kun, đừng mà!
Dialogue: 0,0:14:30.33,0:14:33.78,Default,Ino{14.30},0000,0000,0000,,Em định dùng nó để "xử lí" cô sao?
Dialogue: 0,0:14:34.46,0:14:36.26,Default,Ino,0000,0000,0000,,Cô sẽ không quên chuyện này đâu!
Dialogue: 0,0:14:36.67,0:14:39.15,Default,Ino,0000,0000,0000,,Rồi em sẽ phải trả giá!
Dialogue: 0,0:14:58.30,0:15:01.04,Default,Ino,0000,0000,0000,,Chúa ơi! Không thể nào!
Dialogue: 0,0:15:01.95,0:15:04.69,Default,Ino,0000,0000,0000,,Sướng quá đi mất!?
Dialogue: 0,0:15:12.05,0:15:13.31,Default,Ino,0000,0000,0000,,Ôi không!
Dialogue: 0,0:15:31.71,0:15:32.94,Default,Ino,0000,0000,0000,,Hậu môn cô!
Dialogue: 0,0:15:32.94,0:15:35.22,Default,Ino,0000,0000,0000,,Cả hậu môn và âm hộ cô... Sướng quá đi mất!
Dialogue: 0,0:15:35.22,0:15:40.77,Default,Ino,0000,0000,0000,,Em định chơi rách hậu môn cô sao!
Dialogue: 0,0:15:54.71,0:15:57.16,Default,Ino,0000,0000,0000,,Phải rồi! Cô sắp mất tự chủ rồi!
Dialogue: 0,0:15:57.16,0:15:58.44,Default,Ino,0000,0000,0000,,Em đang hủy hoại cô!
Dialogue: 0,0:16:27.95,0:16:28.67,Default,Ino,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:16:28.67,0:16:29.64,Default,Ino,0000,0000,0000,,Cô ra rồi!
Dialogue: 0,0:16:29.64,0:16:31.29,Default,Ino,0000,0000,0000,,Cô ra rồi, cô ra rồi, cô ra rồi!
Dialogue: 0,0:16:56.51,0:16:58.10,Default,Tac,0000,0000,0000,,Để em giúp cô một tay.
Dialogue: 0,0:16:59.32,0:17:00.61,Default,Tac,0000,0000,0000,,Nào!
Dialogue: 0,0:17:43.90,0:17:47.25,Default,Tac,0000,0000,0000,,Chào buổi sáng, Yuko-sensei.
Dialogue: 0,0:17:47.57,0:17:48.74,Default,Ino,0000,0000,0000,,Chào buổi sáng.
Dialogue: 0,0:17:51.05,0:17:52.51,Default,Gir{2; simul},0000,0000,0000,,Sensei! Sensei!
Dialogue: 0,0:17:52.51,0:17:54.05,Default,Gir,0000,0000,0000,,Chào buổi sáng!
Dialogue: 0,0:17:54.05,0:17:55.25,Default,Gir,0000,0000,0000,,Chào buổi sáng.
Dialogue: 0,0:17:55.25,0:17:56.49,Default,Ino,0000,0000,0000,,Chào buổi sáng.
Dialogue: 0,0:17:57.00,0:18:03.73,Default,Tac,0000,0000,0000,,Bất chấp chuyện xảy ra hôm qua, Yuko-sensei vẫn mỉm cười với tôi như chưa từng có gì xảy ra. {wording?}
Dialogue: 0,0:18:04.32,0:18:07.37,Default,Lil,0000,0000,0000,,Chào buổi sáng, Yuko-sensei.
Dialogue: 0,0:18:07.37,0:18:08.96,Default,Ino,0000,0000,0000,,Chào buổi sáng, Futaba-san.
Dialogue: 0,0:18:09.78,0:18:13.23,Default,Lil,0000,0000,0000,,Chẳng phải hôm qua Yuko-sensei vừa giúp cậu trừ tà sao?
Dialogue: 0,0:18:13.23,0:18:16.97,Default,Lil,0000,0000,0000,,Tên người hầu này kể cũng to gan nhỉ!
Dialogue: 0,0:18:16.97,0:18:18.23,Default,Tac,0000,0000,0000,,Tôi không phải tên người hầu!
Dialogue: 0,0:18:18.23,0:18:20.43,Default,Tac,0000,0000,0000,,Vả lại, chính cô là đầu trò vụ này chứ ai!
Dialogue: 0,0:18:21.71,0:18:23.95,Default,Lil,0000,0000,0000,,Ê! Cái gì thế kia!?
Dialogue: 0,0:18:30.14,0:18:31.72,Default,Lil,0000,0000,0000,,Sensei, nhìn nè!
Dialogue: 0,0:18:32.30,0:18:35.03,Default,Ino,0000,0000,0000,,Trông giống một chiếc sừng.
Dialogue: 0,0:18:36.78,0:18:40.23,Default,Ino,0000,0000,0000,,Có thể đó là di chứng sau khi em biến hình.
Dialogue: 0,0:18:40.60,0:18:41.94,Default,Tac,0000,0000,0000,,Vậy là em mọc sừng à?
Dialogue: 0,0:18:41.94,0:18:46.24,Default,Tac,0000,0000,0000,,Cơ mà, có gì đâu mà hai người sán lại ghê thế...
Dialogue: 0,0:18:46.84,0:18:49.45,Default,Lil,0000,0000,0000,,Cậu nghĩ mình là ai hả, đồ mọc sừng?
Dialogue: 0,0:18:49.45,0:18:51.86,Default,Lil,0000,0000,0000,,Thế này là trái với nội quy trường học nhé! {dress code?}
Dialogue: 0,0:18:51.86,0:18:54.25,Default,Lil,0000,0000,0000,,Một là cưa đi, hai là giao nó cho tôi!
Dialogue: 0,0:18:54.25,0:18:56.19,Default,Tac,0000,0000,0000,,Đừng có kéo đầu tôi, mẹ trẻ.
Dialogue: 0,0:18:56.19,0:18:57.91,Default,Tac,0000,0000,0000,,Thế mẹ trẻ muốn tôi giao cái của nợ kia kiểu gì?
Dialogue: 0,0:18:57.91,0:18:59.74,Default,Lil,0000,0000,0000,,Để tôi xem đã nào!
Dialogue: 0,0:19:00.51,0:19:02.79,Default,Lil,0000,0000,0000,,Đây là một báu vật ma pháp!
Dialogue: 0,0:19:02.79,0:19:04.49,Default,Tac,0000,0000,0000,,Này, nhẹ tay hộ cái!
Dialogue: 0,0:19:05.06,0:19:08.37,Default,Lil,0000,0000,0000,,Đưa đây! Đưa nó cho tôi! Đưa nó cho tôi!
Dialogue: 0,0:19:14.41,0:19:16.28,Default,Ino,0000,0000,0000,,Các em hiểu cả rồi chứ.
Dialogue: 0,0:19:16.28,0:19:21.49,Default,Ino,0000,0000,0000,,Đây là trường hợp rất hiếm gặp, vậy nên hãy đối xử hòa đồng với Tachibana-kun nhé?
Dialogue: 0,0:19:21.49,0:19:23.51,Default,Class,0000,0000,0000,,Vâng!
Dialogue: 0,0:19:23.51,0:19:25.21,Default,Ino,0000,0000,0000,,Em về chỗ đi, Tachibana-kun.
Dialogue: 0,0:19:25.76,0:19:27.16,Default,Tac,0000,0000,0000,,Làm sao để gỡ bỏ nó?
Dialogue: 0,0:19:27.16,0:19:33.72,Default,Tac,0000,0000,0000,,Việc này tương đối khó khăn, vậy nên các cậu cứ xem như không có là được!
Dialogue: 0,0:19:33.72,0:19:36.10,Default,Kid,0000,0000,0000,,Quái tật ư? Chà, cũng không trách được cậu.
Dialogue: 0,0:19:36.10,0:19:39.39,Default,Kid,0000,0000,0000,,Nếu Yuko-sensei đã nói vậy, bọn mình sẽ không ngại đâu,
Dialogue: 0,0:19:39.39,0:19:40.60,Default,Kid,0000,0000,0000,,Tachibana.
Dialogue: 0,0:19:40.60,0:19:42.05,Default,Kid,0000,0000,0000,,Trông hợp với mày đấy chứ!
Dialogue: 0,0:19:42.60,0:19:45.76,Default,Kid,0000,0000,0000,,Cuối cùng cậu cũng đã cởi mở hơn rồi đó, Tachibana!
Dialogue: 0,0:00:00.00,0:00:00.00,Default,Kid,0000,0000,0000,,{confidence/speaking-wise}
Dialogue: 0,0:19:45.76,0:19:48.05,Default,Tac,0000,0000,0000,,Bọn nó còn chả thèm chớp mắt.
Dialogue: 0,0:19:53.74,0:19:57.23,Default,Tac,0000,0000,0000,,Tuyệt vời! Đi ăn thôi!
Dialogue: 0,0:20:00.79,0:20:01.72,Default,Tac,0000,0000,0000,,Này, chuyện gì thế... Ái
Dialogue: 0,0:20:01.72,0:20:04.67,Default,Kid,0000,0000,0000,,Nếu mọc sừng thì Lily-chan có thích mình không nhỉ?
Dialogue: 0,0:20:04.67,0:20:06.72,Default,Kid,0000,0000,0000,,Mày hên thiệt đó, Tachibana...
Dialogue: 0,0:20:06.72,0:20:11.72,Default,sign{20.07},0000,0000,0000,,{\p1\c&H0A090B&\3c&H30455F&\fscx446.25\fscy150\pos(476,85)}m 30 -13 l -24 -13 l -24 15 l 30 15 
Dialogue: 0,0:20:06.72,0:20:11.72,Default,sign{20.07},0000,0000,0000,,{\bord0\an8\c&HFFFFFF&\fnUVN Chim Bien Nhe\pos(364,28)}Câu lạc bộ huyền bí
Dialogue: 0,0:20:20.19,0:20:22.04,Default,Tac,0000,0000,0000,,Chuyện quái gì vậy!?
Dialogue: 0,0:20:22.04,0:20:23.40,Default,Tac,0000,0000,0000,,Nếu lại là chuyện cái sừng thì quên đi!
Dialogue: 0,0:20:32.90,0:20:36.48,Default,Tac,0000,0000,0000,,À này, Lily-san, có chuyện gì vậy?
Dialogue: 0,0:20:36.70,0:20:39.32,Default,Lil,0000,0000,0000,,Bắt đầu đi, người phàm.
Dialogue: 0,0:20:39.32,0:20:40.35,Default,Tac,0000,0000,0000,,Hả?
Dialogue: 0,0:20:40.35,0:20:43.89,Default,Lil,0000,0000,0000,,Mau lại đây... Hừ!
Dialogue: 0,0:20:44.28,0:20:46.64,Default,Lil,0000,0000,0000,,Làm tình với tôi lẹ lên nào!
Dialogue: 0,0:20:46.64,0:20:47.91,Default,Tac,0000,0000,0000,,Làm tình!?
Dialogue: 0,0:20:47.91,0:20:49.23,Default,Tac,0000,0000,0000,,Cô đùa tôi đấy hả?
Dialogue: 0,0:20:49.77,0:20:54.47,Default,Lil,0000,0000,0000,,Vì tôi mà anh bị biến thành quái vật xúc tu.
Dialogue: 0,0:20:54.47,0:20:56.75,Default,Tac,0000,0000,0000,,Lily, cô...
Dialogue: 0,0:20:57.36,0:20:59.90,Default,Lil,0000,0000,0000,,Mau lên nào...
Dialogue: 0,0:20:59.90,0:21:01.22,Default,Lil,0000,0000,0000,,Tôi không ngại đâu.
Dialogue: 0,0:21:02.61,0:21:05.73,Default,Tac,0000,0000,0000,,Tôi không thể làm vậy được, Lily.
Dialogue: 0,0:21:05.73,0:21:08.03,Default,Tac,0000,0000,0000,,Nếu tiếp tục hít phải mùi hương của tôi, cô sẽ...
Dialogue: 0,0:21:12.69,0:21:14.93,Default,Lil,0000,0000,0000,,Đừng bận tâm về chuyện đó.
Dialogue: 0,0:21:17.24,0:21:21.54,Default,Lil,0000,0000,0000,,Nhìn cho kĩ này, Ichiro!
Dialogue: 0,0:21:22.35,0:21:24.49,Default,Lil,0000,0000,0000,,Nhìn xem chúng cương cứng tới mức nào rồi.
Dialogue: 0,0:21:30.15,0:21:33.04,Default,Tac,0000,0000,0000,,Khoan đã, đừng bảo là...
Dialogue: 0,0:21:33.04,0:21:36.69,Default,Lil,0000,0000,0000,,Tôi có thể chịu đựng sự thô bạo của cậu.
Dialogue: 0,0:21:37.13,0:21:38.96,Default,Tac,0000,0000,0000,,Cô hứng lên rồi ư?
Dialogue: 0,0:21:38.96,0:21:44.04,Default,Lil,0000,0000,0000,,Đúng vậy. Hôm qua tôi đã hít phải mùi hương đó.
Dialogue: 0,0:21:44.04,0:21:45.75,Default,Tac,0000,0000,0000,,Tôi xin lỗi!
Dialogue: 0,0:21:45.75,0:21:48.54,Default,Lil,0000,0000,0000,,Đồ ngốc, đó là lỗi của tôi! {this?}
Dialogue: 0,0:21:48.54,0:21:51.69,Default,Lil,0000,0000,0000,,Tôi đã hít đủ số lượng cần thiết!
Dialogue: 0,0:21:52.03,0:21:54.19,Default,Tac,0000,0000,0000,,Cô nói cái gì, Lily?
Dialogue: 0,0:21:54.64,0:22:01.03,Default,Lil,0000,0000,0000,,Vậy nên trước khi lại biến hình và làm hại các cô gái khác, cậu phải hút lấy năng lượng của tôi!
Dialogue: 0,0:22:01.03,0:22:08.44,Default,Lil,0000,0000,0000,,Thích nhé, cậu vốn chẳng xứng đôi vừa lứa với một mĩ nhân như tôi!
Dialogue: 0,0:22:08.82,0:22:10.24,Default,Tac,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:22:10.24,0:22:12.15,Default,Lil,0000,0000,0000,,Đây là lần đầu của tôi!
Dialogue: 0,0:22:14.99,0:22:16.77,Default,Tac,0000,0000,0000,,Không lẽ cô...
Dialogue: 0,0:22:17.43,0:22:21.32,Default,Lil,0000,0000,0000,,Hay là cậu không thích tôi?
Dialogue: 0,0:22:22.18,0:22:26.53,Default,Lil,0000,0000,0000,,Tôi không quyến rũ bằng Yuko-sensei sao?
Dialogue: 0,0:22:26.53,0:22:29.00,Default,Tac,0000,0000,0000,,Không đúng, không đúng một chút nào!
Dialogue: 0,0:22:29.93,0:22:31.75,Default,Tac,0000,0000,0000,,Ngược lại mới đúng!
Dialogue: 0,0:22:31.75,0:22:34.81,Default,Tac,0000,0000,0000,,Bây giờ tôi chỉ muốn chơi cô ngay lập tức!
Dialogue: 0,0:22:36.16,0:22:38.89,Default,Lil,0000,0000,0000,,Được thôi. Nếu cậu đã nói vậy.
Dialogue: 0,0:22:38.89,0:22:41.45,Default,Lil,0000,0000,0000,,Tôi sẽ cho cậu được toại nguyện một lần.
Dialogue: 0,0:22:42.03,0:22:46.06,Default,Lil,0000,0000,0000,,Dù tôi không muốn, nhưng cứ xem như lần này là ngoại lệ đi.
Dialogue: 0,0:22:55.24,0:22:56.89,Default,Tac,0000,0000,0000,,Xin lỗi, Lily.
Dialogue: 0,0:22:56.89,0:22:59.42,Default,Tac,0000,0000,0000,,Tôi không thể kìm nén dục vọng của mình được nữa.
Dialogue: 0,0:22:59.77,0:23:02.03,Default,Tac,0000,0000,0000,,Tôi muốn chơi cô ngay lập tức.
Dialogue: 0,0:23:02.70,0:23:04.85,Default,Lil,0000,0000,0000,,Vậy thì làm đi, Ichiro!
Dialogue: 0,0:23:04.85,0:23:07.72,Default,Lil,0000,0000,0000,,Mau chơi tôi đi....
Dialogue: 0,0:23:15.44,0:23:18.76,Default,Lil,0000,0000,0000,,Khoan đã... Này!
Dialogue: 0,0:23:27.72,0:23:28.65,Default,Lil,0000,0000,0000,,Ichiro!
Dialogue: 0,0:23:32.75,0:23:34.29,Default,Tac,0000,0000,0000,,Lily...
Dialogue: 0,0:23:34.89,0:23:37.23,Default,Tac,0000,0000,0000,,Tôi biến hình đây.
Dialogue: 0,0:23:40.40,0:23:42.32,Default,Lil,0000,0000,0000,,Được, tôi sẵn sàng rồi.
Dialogue: 0,0:23:58.90,0:24:00.00,Default,Lil,0000,0000,0000,,Đùa giỡn tôi đi!
Dialogue: 0,0:24:00.94,0:24:03.99,Default,Lil,0000,0000,0000,,Toàn thân tôi bị trói chặt!
Dialogue: 0,0:24:04.55,0:24:07.42,Default,Lil,0000,0000,0000,,Cậu coi tôi như một thứ đồ chơi sao!
Dialogue: 0,0:24:18.00,0:24:21.36,Default,Lil,0000,0000,0000,,Ichiro, cậu đang làm gì vậy?
Dialogue: 0,0:24:22.90,0:24:25.30,Default,Lil,0000,0000,0000,,Ichiro, cậu ra rồi ư?
Dialogue: 0,0:24:25.30,0:24:26.34,Default,Lil,0000,0000,0000,,Nó đang trào ra!?
Dialogue: 0,0:24:27.31,0:24:30.00,Default,Lil,0000,0000,0000,,Chúa ơi, mùi hương quái quỉ...
Dialogue: 0,0:24:45.60,0:24:49.77,Default,Lil,0000,0000,0000,,Nóng quá! Tôi hứng lên rồi!
Dialogue: 0,0:24:50.74,0:24:52.66,Default,Lil,0000,0000,0000,,Cơ thể tôi tê liệt rồi!
Dialogue: 0,0:24:52.66,0:24:55.89,Default,Lil,0000,0000,0000,,Mùi hương của cậu đang xâm chiếm cơ thế tôi.
Dialogue: 0,0:24:55.89,0:24:57.01,Default,Lil,0000,0000,0000,,Tuyệt quá!
Dialogue: 0,0:24:58.47,0:25:00.74,Default,Lil,0000,0000,0000,,Không lẽ cậu định...
Dialogue: 0,0:25:14.63,0:25:15.94,Default,Lil,0000,0000,0000,,Tại sao!?
Dialogue: 0,0:25:16.78,0:25:18.96,Default,Lil,0000,0000,0000,,Tại sao lại sướng như vậy!?
Dialogue: 0,0:25:26.05,0:25:29.50,Default,Lil,0000,0000,0000,,Cậu coi tử cung tôi như món đồ chơi!
Dialogue: 0,0:25:30.43,0:25:32.49,Default,Lil,0000,0000,0000,,Tôi sẽ không tha cho cậu đâu!
Dialogue: 0,0:25:46.69,0:25:47.91,Default,Lil,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:25:47.91,0:25:50.61,Default,Lil,0000,0000,0000,,Cậu sắp làm tôi ra rồi!
Dialogue: 0,0:25:54.45,0:25:55.70,Default,Lil,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:25:58.71,0:26:00.45,Default,Lil,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:26:00.45,0:26:01.77,Default,Lil,0000,0000,0000,,Nó đang tràn vào bên trong tôi!
Dialogue: 0,0:26:20.49,0:26:22.05,Default,Lil,0000,0000,0000,,Ôi nhiều quá...
Dialogue: 0,0:26:23.98,0:26:25.13,Default,Lil,0000,0000,0000,,Cậu ra nhiều quá!
Dialogue: 0,0:26:32.05,0:26:33.60,Default,Lil,0000,0000,0000,,Biết ngay mà!
Dialogue: 0,0:26:33.60,0:26:35.68,Default,Lil,0000,0000,0000,,Đồ dê cụ.
Dialogue: 0,0:26:36.22,0:26:38.61,Default,Lil,0000,0000,0000,,Cậu là đồ quái vật xúc tu biến thái!
Dialogue: 0,0:26:51.98,0:26:54.10,Default,Lil,0000,0000,0000,,Cậu vẫn chưa thỏa mãn, phải không?
Dialogue: 0,0:26:55.20,0:26:58.46,Default,Lil,0000,0000,0000,,Thôi được. Chúng ta sẽ cùng nhau vui vẻ tiếp.
Dialogue: 0,0:28:37.43,0:28:41.40,Default,sign{28.38},0000,0000,0000,,{\fad(450,400)\fnUVN Nguyen Du\pos(111,384)\3c&H060E17&}Tập sau ...
Dialogue: 0,0:28:40.23,0:28:42.25,Default,Lil,0000,0000,0000,,Phải rồi! Ngay trong giờ sinh hoạt!
Dialogue: 0,0:28:43.21,0:28:44.65,Default,Lil,0000,0000,0000,,Đó là lần đầu tiên!
Dialogue: 0,0:28:48.47,0:28:52.85,Default,Lil,0000,0000,0000,,Nè! Cậu soi ngực Yuko-sensei đến mức chảy máu cam đấy hả!?
Dialogue: 0,0:28:52.85,0:28:53.81,Default,Lil,0000,0000,0000,,Đồ dê cụ!
Dialogue: 0,0:28:54.13,0:28:55.64,Default,Tac,0000,0000,0000,,Cái quái gì thế này?
Dialogue: 0,0:28:57.55,0:29:01.06,Default,Tac,0000,0000,0000,,Đừng tưởng mọc hai cái sừng đã là oai nhé!
Dialogue: 0,0:29:01.89,0:29:04.29,Default,Tac,0000,0000,0000,,Yuko-sensei, sao bụng cô lại...
Dialogue: 0,0:29:06.50,0:29:08.55,Default,Ino,0000,0000,0000,,Lại còn sao nữa hả?
Dialogue: 0,0:29:08.55,0:29:11.10,Default,,0000,0000,0000,,Em phải chịu trách nhiệm chuyện này đó.
