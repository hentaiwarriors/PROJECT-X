Dialogue: 0,0:00:01.00,0:00:02.45,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:02.84,0:00:04.15,Default,,0000,0000,0000,,Này anh?
Dialogue: 0,0:00:04.34,0:00:05.02,Default,,0000,0000,0000,,Gì thế?
Dialogue: 0,0:00:05.57,0:00:09.74,Default,,0000,0000,0000,,Nơi này là nhà của anh phải ko, Akuji-san?
Dialogue: 0,0:00:10.19,0:00:10.88,Default,,0000,0000,0000,,Đúng thế.
Dialogue: 0,0:00:11.48,0:00:13.94,Default,,0000,0000,0000,,Vậy là anh đang về thăm nhà, nhưng tại sao?
Dialogue: 0,0:00:25.78,0:00:27.33,Default,,0000,0000,0000,,Ta là chủ nhân đời thứ ba của Wakame-gumi,
Dialogue: 0,0:00:27.45,0:00:28.57,Default,,0000,0000,0000,,Yamamoto Akuji.
Dialogue: 0,0:00:29.40,0:00:31.59,Default,,0000,0000,0000,,Ta tới thăm các người đây.
Dialogue: 0,0:00:37.00,0:00:42.40,Default,,0000,0000,0000,,DAIAKUJI
Dialogue: 0,0:03:07.89,0:03:10.46,Default,,0000,0000,0000,,Akuji, cuối cùng thì hắn ta cũng quay lại.
Dialogue: 0,0:03:10.75,0:03:12.39,Default,,0000,0000,0000,,Anh ta đã trở về nhà.
Dialogue: 0,0:03:12.76,0:03:13.52,Default,,0000,0000,0000,,Thì sao chứ?
Dialogue: 0,0:03:13.83,0:03:17.07,Default,,0000,0000,0000,,Bà đang cố ngăn cản anh ấy trở lại đây ư?
Dialogue: 0,0:03:18.35,0:03:19.80,Default,,0000,0000,0000,,Luật lệ của nơi này đã thay đổi.
Dialogue: 0,0:03:20.21,0:03:22.26,Default,,0000,0000,0000,,Nơi này vì thế cũng thay đổi luôn.
Dialogue: 0,0:03:23.22,0:03:24.52,Default,,0000,0000,0000,,Bà chỉ là một tình nhân của...
Dialogue: 0,0:03:25.75,0:03:28.53,Default,,0000,0000,0000,,Ông vẫn chưa hiểu vị thế của mình vào lúc này, phải ko?
Dialogue: 0,0:03:32.35,0:03:33.71,Default,,0000,0000,0000,,Motoko!
Dialogue: 0,0:03:34.48,0:03:37.05,Default,,0000,0000,0000,,Con gái ông thì thức thời hơn ông nhiều.
Dialogue: 0,0:03:37.27,0:03:39.34,Default,,0000,0000,0000,,Vì thế tôi đã bổ nhiệm cô ấy làm một thành viên ban chấp pháp ở đây.
Dialogue: 0,0:03:39.85,0:03:41.55,Default,,0000,0000,0000,,Motoko, con...!
Dialogue: 0,0:03:41.59,0:03:43.11,Default,,0000,0000,0000,,Ra khỏi đây ngay.
Dialogue: 0,0:03:43.54,0:03:46.44,Default,,0000,0000,0000,,Tôi ko quan tâm trước đây ông là một ủy viên như thế nào.
Dialogue: 0,0:03:46.44,0:03:48.44,Default,,0000,0000,0000,,Nơi này ko còn chỗ dành cho ông nữa.
Dialogue: 0,0:03:48.86,0:03:50.55,Default,,0000,0000,0000,,Về nhà đi.
Dialogue: 0,0:03:50.55,0:03:51.52,Default,,0000,0000,0000,,Motoko
Dialogue: 0,0:03:51.80,0:03:54.14,Default,,0000,0000,0000,,Tiễn ông ấy về nhà.
Dialogue: 0,0:03:54.36,0:03:55.32,Default,,0000,0000,0000,,Các người làm gì thế hả?
Dialogue: 0,0:03:55.32,0:03:57.59,Default,,0000,0000,0000,,Buông ta ra! Buông ra!
Dialogue: 0,0:03:58.82,0:04:00.68,Default,,0000,0000,0000,,Làm ơn hãy làm theo những gì bà ấy nói.
Dialogue: 0,0:04:00.68,0:04:02.95,Default,,0000,0000,0000,,Đấy là lựa chọn tốt nhất dành cho cha, thưa cha.
Dialogue: 0,0:04:03.22,0:04:05.39,Default,,0000,0000,0000,,Motoko, con...
Dialogue: 0,0:04:05.88,0:04:08.81,Default,,0000,0000,0000,,Bà sẽ phải hối hận đó, Ran!
Dialogue: 0,0:04:09.02,0:04:09.95,Default,,0000,0000,0000,,Motoko
Dialogue: 0,0:04:10.17,0:04:10.87,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:04:11.55,0:04:17.08,Default,,0000,0000,0000,,Cô có biết lí do tại sao ta lại bổ nhiệm cô, con gái của Kaga, vào cương vị này ko?
Dialogue: 0,0:04:17.08,0:04:20.09,Default,,0000,0000,0000,,Đó là vì tôi đánh giá cao năng lực của cô.
Dialogue: 0,0:04:20.09,0:04:22.78,Default,,0000,0000,0000,,Đừng làm tôi thất vọng.
Dialogue: 0,0:04:29.53,0:04:30.57,Default,,0000,0000,0000,,Cười lên nào.
Dialogue: 0,0:04:30.85,0:04:32.30,Default,,0000,0000,0000,,Yeah!
Dialogue: 0,0:04:32.30,0:04:34.76,Default,,0000,0000,0000,,Đây là trụ sở của maphia sao.
Dialogue: 0,0:04:35.02,0:04:35.95,Default,,0000,0000,0000,,Mình phải làm gì đây?
Dialogue: 0,0:04:36.08,0:04:38.54,Default,,0000,0000,0000,,Đến những nơi thế này làm mình thấy mắc tiểu quá.
Dialogue: 0,0:04:39.17,0:04:39.93,Default,,0000,0000,0000,,Thưa ngài
Dialogue: 0,0:04:40.24,0:04:41.83,Default,,0000,0000,0000,,Chúng tôi ko thể làm gì khác.
Dialogue: 0,0:04:42.27,0:04:44.57,Default,,0000,0000,0000,,Chính ngài đã khiêu chiến trước.
Dialogue: 0,0:04:45.68,0:04:46.83,Default,,0000,0000,0000,,Được thôi!
Dialogue: 0,0:04:46.83,0:04:49.63,Default,,0000,0000,0000,,Cho chúng thấy sức mạnh của tổ chức tình nguyện nào!
Dialogue: 0,0:04:49.63,0:04:51.45,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:04:51.45,0:04:53.31,Default,,0000,0000,0000,,Chiến nào.
Dialogue: 0,0:04:53.57,0:04:55.11,Default,,0000,0000,0000,,Chúng ta bắt đầu thôi.
Dialogue: 0,0:04:55.11,0:04:56.92,Default,,0000,0000,0000,,Nhưng phòng vệ sinh ở đâu vậy...
Dialogue: 0,0:04:59.85,0:05:00.89,Default,,0000,0000,0000,,Thế này...
Dialogue: 0,0:05:01.76,0:05:04.79,Default,,0000,0000,0000,,Chúng ta có tham gia luôn ko nhỉ?
Dialogue: 0,0:05:04.79,0:05:06.76,Default,,0000,0000,0000,,Cô xông lên làm gì thế?
Dialogue: 0,0:05:06.93,0:05:09.39,Default,,0000,0000,0000,,Cô chán sống rồi sao?
Dialogue: 0,0:05:09.61,0:05:11.60,Default,,0000,0000,0000,,Cô đang là thủ lĩnh đấy.
Dialogue: 0,0:05:11.94,0:05:13.86,Default,,0000,0000,0000,,Hả?Cái này là sao đây?!
Dialogue: 0,0:05:14.78,0:05:17.07,Default,,0000,0000,0000,,Ta sẽ lấy mạng cô!
Dialogue: 0,0:05:24.04,0:05:25.55,Default,,0000,0000,0000,,Tôi xin lỗi!
Dialogue: 0,0:05:28.19,0:05:30.46,Default,,0000,0000,0000,,Ah, ko.
Dialogue: 0,0:05:35.86,0:05:37.38,Default,,0000,0000,0000,,Ở đâu nhỉ?
Dialogue: 0,0:05:39.37,0:05:42.69,Default,,0000,0000,0000,,Đây là trụ sở maphia sao.
Dialogue: 0,0:05:43.03,0:05:44.26,Default,,0000,0000,0000,,Cho phép tôi được giới thiệu.
Dialogue: 0,0:05:44.27,0:05:44.76,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:05:45.00,0:05:47.38,Default,,0000,0000,0000,,Tên tôi là Shogoku Hashima.
Dialogue: 0,0:05:47.64,0:05:48.38,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:05:48.38,0:05:49.19,Default,,0000,0000,0000,,Xong phim!
Dialogue: 0,0:05:53.74,0:05:55.31,Default,,0000,0000,0000,,Hết tên rồi.
Dialogue: 0,0:05:55.44,0:05:56.95,Default,,0000,0000,0000,,Có ai còn tên ko?
Dialogue: 0,0:06:04.90,0:06:07.01,Default,,0000,0000,0000,,Ko! Dừng lại!
Dialogue: 0,0:06:07.26,0:06:10.68,Default,,0000,0000,0000,,Nếu cô còn giãy giụa, cô sẽ làm bị thương chính mình đấy.
Dialogue: 0,0:06:14.83,0:06:16.06,Default,,0000,0000,0000,,Giải trí chút nào.
Dialogue: 0,0:06:23.81,0:06:24.70,Default,,0000,0000,0000,,Vào này!
Dialogue: 0,0:06:26.42,0:06:27.60,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:06:33.04,0:06:36.98,Default,,0000,0000,0000,,Làm chuyện này vào giữa trận chiến quả thật là thú vị.
Dialogue: 0,0:06:37.54,0:06:38.13,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:06:38.66,0:06:40.71,Default,,0000,0000,0000,,Ko! Đừng ra vào trong!
Dialogue: 0,0:06:44.42,0:06:46.58,Default,,0000,0000,0000,,Sướng thật đấy.
Dialogue: 0,0:06:59.44,0:07:01.69,Default,,0000,0000,0000,,Tôi đã bảo anh đừng ra vào trong mà.
Dialogue: 0,0:07:12.33,0:07:15.19,Default,,0000,0000,0000,,Mình đã nghĩ sẽ có nhà tắm nào đó gần đây.
Dialogue: 0,0:07:16.08,0:07:18.26,Default,,0000,0000,0000,,Hi vọng ko ai nhìn thấy.
Dialogue: 0,0:07:19.58,0:07:20.77,Default,,0000,0000,0000,,Cái gì thế này?
Dialogue: 0,0:07:21.13,0:07:24.55,Default,,0000,0000,0000,,Ko, đừng lại gần đây.
Dialogue: 0,0:07:25.91,0:07:27.54,Default,,0000,0000,0000,,Tôi ko thể ngừng ngay được
Dialogue: 0,0:07:33.88,0:07:35.69,Default,,0000,0000,0000,,Mùi của...
Dialogue: 0,0:07:37.24,0:07:38.77,Default,,0000,0000,0000,,Phụ nữ!
Dialogue: 0,0:08:02.86,0:08:04.04,Default,,0000,0000,0000,,Này, Tsujiya.
Dialogue: 0,0:08:04.40,0:08:05.74,Default,,0000,0000,0000,,Chồng cô thế nào rồi?
Dialogue: 0,0:08:05.86,0:08:08.21,Default,,0000,0000,0000,,Bọn tôi đã chia tay nhau, tất cả là lỗi của anh.
Dialogue: 0,0:08:08.31,0:08:09.10,Default,,0000,0000,0000,,Lỗi của tôi hả?
Dialogue: 0,0:08:15.38,0:08:17.19,Default,,0000,0000,0000,,Làm ơn đừng.
Dialogue: 0,0:08:17.19,0:08:19.00,Default,,0000,0000,0000,,Đừng làm thế với tôi.
Dialogue: 0,0:08:19.00,0:08:21.32,Default,,0000,0000,0000,,Một cô vợ mới cưới thật khêu gợi.
Dialogue: 0,0:08:22.37,0:08:23.40,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:08:38.46,0:08:39.61,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:08:39.61,0:08:42.17,Default,,0000,0000,0000,,Chồng tôi đang trên đường về nhà.
Dialogue: 0,0:08:43.90,0:08:44.89,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:08:44.89,0:08:47.13,Default,,0000,0000,0000,,Nếu tôi ra... Nếu tôi ra...
Dialogue: 0,0:08:47.13,0:08:48.21,Default,,0000,0000,0000,,Chồng tôi sẽ...
Dialogue: 0,0:08:49.16,0:08:51.71,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:08:55.16,0:08:56.21,Default,,0000,0000,0000,,Ông xã...
Dialogue: 0,0:08:57.50,0:08:59.62,Default,,0000,0000,0000,,Anh cưỡng bức vợ của thuộc hạ mình.
Dialogue: 0,0:09:00.11,0:09:01.34,Default,,0000,0000,0000,,Anh thật bỉ ổi!
Dialogue: 0,0:09:01.76,0:09:03.80,Default,,0000,0000,0000,,Chồng cô về muộn quá.
Dialogue: 0,0:09:03.90,0:09:06.74,Default,,0000,0000,0000,,Thế nên tôi "dùng" cô trước hắn ta thôi mà.
Dialogue: 0,0:09:06.97,0:09:08.76,Default,,0000,0000,0000,,Đừng coi tôi là thức ăn chứ.
Dialogue: 0,0:09:08.80,0:09:09.89,Default,,0000,0000,0000,,Chết đi, tên khốn!
Dialogue: 0,0:09:14.62,0:09:17.99,Default,,0000,0000,0000,,Cô chưa đủ tuổi chĩa súng vào tôi đâu.
Dialogue: 0,0:09:17.99,0:09:19.21,Default,,0000,0000,0000,,Buông tôi ra.
Dialogue: 0,0:10:25.33,0:10:27.53,Default,,0000,0000,0000,,Cô cũng ướt như lần đó.
Dialogue: 0,0:10:28.89,0:10:30.77,Default,,0000,0000,0000,,Anh thật xấu xa.
Dialogue: 0,0:10:45.69,0:10:48.17,Default,,0000,0000,0000,,Cô vẫn khít như lần trước.
Dialogue: 0,0:10:48.60,0:10:50.08,Default,,0000,0000,0000,,Đương nhiên rồi
Dialogue: 0,0:10:50.10,0:10:53.23,Default,,0000,0000,0000,,Tôi chưa hề làm chuyện đó kể từ lần bị anh cưỡng bức.
Dialogue: 0,0:10:53.23,0:10:54.01,Default,,0000,0000,0000,,Vì sao thế?
Dialogue: 0,0:10:55.68,0:10:57.59,Default,,0000,0000,0000,,Đó là do lỗi của anh!
Dialogue: 0,0:11:06.59,0:11:09.52,Default,,0000,0000,0000,,Ko, tôi ghét điều đó...
Dialogue: 0,0:11:09.52,0:11:11.52,Default,,0000,0000,0000,,Anh lại làm tôi ra nữa rồi.
Dialogue: 0,0:11:13.99,0:11:18.03,Default,,0000,0000,0000,,Đừng có động vào, hậu môn của tôi... anh thật bệnh hoạn.
Dialogue: 0,0:11:18.71,0:11:20.20,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:11:20.30,0:11:22.15,Default,,0000,0000,0000,,Cô nói gì vậy?
Dialogue: 0,0:11:22.17,0:11:23.30,Default,,0000,0000,0000,,Thư giãn đi nào.
Dialogue: 0,0:11:23.46,0:11:24.31,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:11:39.79,0:11:42.28,Default,,0000,0000,0000,,Tôi ra rồi!
Dialogue: 0,0:12:07.92,0:12:09.09,Default,,0000,0000,0000,,Hey! Toko.
Dialogue: 0,0:12:09.42,0:12:10.45,Default,,0000,0000,0000,,Cô bây giờ thế nào rồi?
Dialogue: 0,0:12:11.61,0:12:15.15,Default,,0000,0000,0000,,Nghe nói cô đã thay thế chức vụ của Kaga.
Dialogue: 0,0:12:15.98,0:12:18.77,Default,,0000,0000,0000,,Việc trước tiên, sao anh ko cho lại nó vào trong nhỉ?
Dialogue: 0,0:12:22.63,0:12:23.87,Default,,0000,0000,0000,,Anh về khi nào thế?
Dialogue: 0,0:12:27.20,0:12:31.03,Default,,0000,0000,0000,,Tôi mới ra tù một tháng trước đây.
Dialogue: 0,0:12:33.49,0:12:35.29,Default,,0000,0000,0000,,Cứu tôi với!
Dialogue: 0,0:12:39.46,0:12:40.90,Default,,0000,0000,0000,,Ai đó làm ơn...?!
Dialogue: 0,0:12:42.15,0:12:47.07,Default,,0000,0000,0000,,Ở đây ko còn chuyện người này giúp đỡ người kia nữa đâu.
Dialogue: 0,0:12:49.46,0:12:51.13,Default,,0000,0000,0000,,Chấp nhận đi.
Dialogue: 0,0:12:51.62,0:12:54.23,Default,,0000,0000,0000,,Làm ơn dừng lại. Tôi là người đã có gia đình.
Dialogue: 0,0:12:54.23,0:12:55.27,Default,,0000,0000,0000,,Tao ko quan tâm.
Dialogue: 0,0:12:56.51,0:12:58.42,Default,,0000,0000,0000,,Ko. Đừng!
Dialogue: 0,0:12:59.73,0:13:02.18,Default,,0000,0000,0000,,Làm ơn dừng lại đi!
Dialogue: 0,0:13:02.35,0:13:04.33,Default,,0000,0000,0000,,Mày là thằng quái nào?
Dialogue: 0,0:13:04.47,0:13:05.91,Default,,0000,0000,0000,,Đừng có phá đám tao.
Dialogue: 0,0:13:07.32,0:13:08.69,Default,,0000,0000,0000,,Cứu tôi với!
Dialogue: 0,0:13:08.69,0:13:09.83,Default,,0000,0000,0000,,Câm ngay!
Dialogue: 0,0:13:11.33,0:13:13.97,Default,,0000,0000,0000,,Tao chưa làm chuyện này cả tháng nay rồi.
Dialogue: 0,0:13:13.97,0:13:15.45,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:13:15.51,0:13:18.21,Default,,0000,0000,0000,,Đừng! Đừng ra vào bên trong!
Dialogue: 0,0:13:18.21,0:13:19.36,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:13:19.59,0:13:20.89,Default,,0000,0000,0000,,Ông thật đáng kinh tởm!
Dialogue: 0,0:13:20.89,0:13:23.53,Default,,0000,0000,0000,,Dừng lại! Ko!
Dialogue: 0,0:13:43.75,0:13:46.64,Default,,0000,0000,0000,,Chúng ta chỉ thừa nhận những ai làm việc cho đất nước.
Dialogue: 0,0:13:47.17,0:13:49.12,Default,,0000,0000,0000,,Ngoài họ ra, những gã đàn ông khác chỉ là đồ bỏ.
Dialogue: 0,0:14:12.38,0:14:13.86,Default,,0000,0000,0000,,Của ngài đây.
Dialogue: 0,0:14:14.09,0:14:15.97,Default,,0000,0000,0000,,Minka-chan, cô luôn rất vui vẻ nhỉ.
Dialogue: 0,0:14:16.14,0:14:18.45,Default,,0000,0000,0000,,Tôi sẽ còn làm việc tại đây lâu hơn nữa.
Dialogue: 0,0:14:18.64,0:14:20.25,Default,,0000,0000,0000,,Tôi sẽ ko bao giờ từ bỏ nơi này.
Dialogue: 0,0:14:20.25,0:14:22.42,Default,,0000,0000,0000,,Minka-chan, cô tới đây được ko?
Dialogue: 0,0:14:22.48,0:14:23.84,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:14:25.02,0:14:27.20,Default,,0000,0000,0000,,Một đĩa gan, tỏi tây chiên, và mì Ramen, phải ko ạ?
Dialogue: 0,0:14:27.39,0:14:28.45,Default,,0000,0000,0000,,Xin chào...
Dialogue: 0,0:14:31.64,0:14:32.19,Default,,0000,0000,0000,,Này
Dialogue: 0,0:14:32.70,0:14:34.01,Default,,0000,0000,0000,,Huh, Akuji...
Dialogue: 0,0:14:34.29,0:14:35.47,Default,,0000,0000,0000,,Akuji!
Dialogue: 0,0:14:36.91,0:14:38.90,Default,,0000,0000,0000,,Em rất mừng vì anh ko sao.
Dialogue: 0,0:14:39.25,0:14:42.65,Default,,0000,0000,0000,,Cuộc chiến đã kết thúc nhưng em ko nhận được tin gì của anh.
Dialogue: 0,0:14:42.65,0:14:44.65,Default,,0000,0000,0000,,Khi đó anh đang trong tù mà.
Dialogue: 0,0:14:44.71,0:14:46.69,Default,,0000,0000,0000,,Anh đã tới đây ngay sau khi rời khỏi nơi đó.
Dialogue: 0,0:14:46.69,0:14:47.62,Default,,0000,0000,0000,,Tha lỗi cho anh nhé.
Dialogue: 0,0:14:49.65,0:14:52.35,Default,,0000,0000,0000,,Vậy, anh sẽ ko quay về nhà nữa phải ko?
Dialogue: 0,0:14:52.67,0:14:54.97,Default,,0000,0000,0000,,Gia đình anh thì sao chứ?
Dialogue: 0,0:14:56.80,0:14:57.95,Default,,0000,0000,0000,,Anh hiểu rồi.
Dialogue: 0,0:14:58.20,0:14:59.14,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:14:59.14,0:15:01.03,Default,,0000,0000,0000,,Đừng tiếp tục công việc đó nữa.
Dialogue: 0,0:15:01.04,0:15:04.03,Default,,0000,0000,0000,,Dù đó là mong muốn của em, anh cũng ko thể làm theo được, Minka
Dialogue: 0,0:15:04.79,0:15:08.73,Default,,0000,0000,0000,,Cha anh đã chết trong cuộc chiến đó còn ông anh thì mất tích.
Dialogue: 0,0:15:09.53,0:15:10.74,Default,,0000,0000,0000,,Ko còn người nào khác cả.
Dialogue: 0,0:15:10.74,0:15:11.56,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:15:17.57,0:15:19.07,Default,,0000,0000,0000,,Vì sao ta muốn vào trong hả?
Dialogue: 0,0:15:19.47,0:15:20.82,Default,,0000,0000,0000,,Đây là nhà ta mà
Dialogue: 0,0:15:21.11,0:15:24.63,Default,,0000,0000,0000,,Vâng, nhưng đây là lệnh của bà chủ.
Dialogue: 0,0:15:24.92,0:15:26.38,Default,,0000,0000,0000,,Bà chủ?
Dialogue: 0,0:15:27.66,0:15:28.86,Default,,0000,0000,0000,,Bà chủ
Dialogue: 0,0:15:29.20,0:15:31.45,Default,,0000,0000,0000,,Đừng nhặng xị lên thế. Hãy ra khỏi đây ngay.
Dialogue: 0,0:15:31.58,0:15:34.06,Default,,0000,0000,0000,,Nơi này ko còn chỗ cho cậu nữa rồi.
Dialogue: 0,0:15:34.21,0:15:38.00,Default,,0000,0000,0000,,Bà là tình nhân thứ 26 của ông tôi phải ko.
Dialogue: 0,0:15:38.00,0:15:40.19,Default,,0000,0000,0000,,Ko phải, hình như là 27 mới đúng?
Dialogue: 0,0:15:40.37,0:15:43.94,Default,,0000,0000,0000,,Tôi là Ran Ichihashi, chủ nhân thứ tư của Wankame-gumi.
Dialogue: 0,0:15:43.94,0:15:47.04,Default,,0000,0000,0000,,Vậy là tôi biết bà chủ ở đây là ai rồi.
Dialogue: 0,0:15:47.04,0:15:48.87,Default,,0000,0000,0000,,Chỉ là mụ tình nhân của ông tôi.
Dialogue: 0,0:15:48.91,0:15:51.80,Default,,0000,0000,0000,,Tôi giỏi hơn cha anh là cái chắc đấy.
Dialogue: 0,0:15:51.80,0:15:55.45,Default,,0000,0000,0000,,Tôi ko cần biết, nhưng đây là nhà tôi.
Dialogue: 0,0:15:58.79,0:16:01.96,Default,,0000,0000,0000,,Ta đoán các người thích đánh nhau hơn là cãi lộn
Dialogue: 0,0:16:01.96,0:16:02.95,Default,,0000,0000,0000,,Hạ hắn đi!
Dialogue: 0,0:16:06.32,0:16:08.44,Default,,0000,0000,0000,,Đừng có nương tay với ta đấy nhé.
Dialogue: 0,0:16:08.76,0:16:10.73,Default,,0000,0000,0000,,Hãy dùng tất cả sức lực các người có!
Dialogue: 0,0:16:16.46,0:16:19.08,Default,,0000,0000,0000,,Cứ nằm đây nhé.
Dialogue: 0,0:16:19.39,0:16:22.04,Default,,0000,0000,0000,,Tại sao cậu lại tin tưởng vào vận may tồi tệ của mình đến thế nhỉ?
Dialogue: 0,0:16:25.57,0:16:28.24,Default,,0000,0000,0000,,Sao mọi chuyện lại thành ra thế này nhỉ.
Dialogue: 0,0:16:33.15,0:16:34.57,Default,,0000,0000,0000,,Anh tỉnh rồi à.
Dialogue: 0,0:16:36.07,0:16:37.32,Default,,0000,0000,0000,,Tôi đang ở đâu thế?
Dialogue: 0,0:16:37.57,0:16:39.43,Default,,0000,0000,0000,,Đây là trụ sở của tổ chức tình nguyện.
Dialogue: 0,0:16:39.56,0:16:42.84,Default,,0000,0000,0000,,Xe cứu thương sẽ ko tới, nếu bệnh nhân là đàn ông.
Dialogue: 0,0:16:43.03,0:16:44.84,Default,,0000,0000,0000,,Tổ chức tình nguyện?
Dialogue: 0,0:16:45.01,0:16:49.32,Default,,0000,0000,0000,,Chúng tôi giúp đỡ những người gặp bất hạnh trong cuộc chiến.
Dialogue: 0,0:16:49.37,0:16:50.76,Default,,0000,0000,0000,,Cô là ai?
Dialogue: 0,0:16:50.76,0:16:53.84,Default,,0000,0000,0000,,Tôi là người đứng đầu nơi này.
Dialogue: 0,0:16:53.93,0:16:55.47,Default,,0000,0000,0000,,Tên tôi là Yoka Aoba.
Dialogue: 0,0:16:56.46,0:16:57.60,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:16:57.98,0:17:01.39,Default,,0000,0000,0000,,Lúc cô bị cưỡng bức, đó cũng là một phần trong công việc tình nguyện hả?
Dialogue: 0,0:17:01.39,0:17:02.96,Default,,0000,0000,0000,,Ko cái đó thì ko!
Dialogue: 0,0:17:03.09,0:17:07.34,Default,,0000,0000,0000,,Lúc đó tôi đang trên đường trở về sau khi đến thăm một bà cụ đã mất con trai trong cuộc chiến.
Dialogue: 0,0:17:07.47,0:17:12.24,Default,,0000,0000,0000,,Để đánh gục tổ chức đó, mình cần phải thành lập một tổ chức của riêng mình.
Dialogue: 0,0:17:13.15,0:17:15.61,Default,,0000,0000,0000,,Cơ thể mình đã yếu đi rất nhiều sau khi ra tù.
Dialogue: 0,0:17:15.71,0:17:17.23,Default,,0000,0000,0000,,Mình cần phải luyện tập thêm.
Dialogue: 0,0:17:29.56,0:17:30.80,Default,,0000,0000,0000,,Làm ơn dừng lại.
Dialogue: 0,0:17:52.25,0:17:54.80,Default,,0000,0000,0000,,Ko. Làm ơn dừng lại.
Dialogue: 0,0:18:07.11,0:18:08.75,Default,,0000,0000,0000,,Cô ướt lắm rồi.
Dialogue: 0,0:18:09.13,0:18:11.74,Default,,0000,0000,0000,,Rõ ràng là cô đang mong chờ chuyện đó.
Dialogue: 0,0:18:18.46,0:18:19.76,Default,,0000,0000,0000,,Cô ướt thật đấy.
Dialogue: 0,0:18:20.06,0:18:21.59,Default,,0000,0000,0000,,Cô đã mong đợi chuyện này rất lâu, đúng ko?
Dialogue: 0,0:18:21.59,0:18:23.23,Default,,0000,0000,0000,,Tôi xấu hổ lắm.
Dialogue: 0,0:18:23.30,0:18:24.47,Default,,0000,0000,0000,,Đừng nói về điều đó nữa.
Dialogue: 0,0:18:24.56,0:18:26.44,Default,,0000,0000,0000,,Vậy thì, ngậm cái này vào miệng cô đi.
Dialogue: 0,0:18:26.66,0:18:28.21,Default,,0000,0000,0000,,Sao cơ? Cái gì vậy?
Dialogue: 0,0:18:31.45,0:18:32.42,Default,,0000,0000,0000,,Cái gì vậy?
Dialogue: 0,0:18:32.42,0:18:32.99,Default,,0000,0000,0000,,Cái này.
Dialogue: 0,0:18:33.38,0:18:34.18,Default,,0000,0000,0000,,Ôi trời ơi!
Dialogue: 0,0:18:39.62,0:18:41.86,Default,,0000,0000,0000,,Cô làm tốt đấy nhỉ.
Dialogue: 0,0:19:12.00,0:19:14.32,Default,,0000,0000,0000,,Xin lỗi, ông xã yêu dấu....
Dialogue: 0,0:19:28.97,0:19:30.72,Default,,0000,0000,0000,,Tôi ko thể chịu đựng thêm nữa.
Dialogue: 0,0:19:42.30,0:19:44.96,Default,,0000,0000,0000,,Tôi ra đây!
Dialogue: 0,0:19:57.28,0:19:58.58,Default,,0000,0000,0000,,Tất cả chuyện này là sao?
Dialogue: 0,0:19:58.72,0:19:59.82,Default,,0000,0000,0000,,Tôi đã trang trí lại văn phòng của cô.
Dialogue: 0,0:20:00.01,0:20:01.13,Default,,0000,0000,0000,,Trông ko ngăn nắp à?
Dialogue: 0,0:20:01.19,0:20:03.29,Default,,0000,0000,0000,,Tôi chưa hề dùng bàn thờ Shinto từ lâu lắm rồi.
Dialogue: 0,0:20:03.29,0:20:05.29,Default,,0000,0000,0000,,Tôi nên để nó ở đâu đây?
Dialogue: 0,0:20:05.29,0:20:09.26,Default,,0000,0000,0000,,Mirai-san, Tomoko-san đang giúp đỡ anh. Anh đã...
Dialogue: 0,0:20:09.40,0:20:12.01,Default,,0000,0000,0000,,Đúng thế, tôi "làm" xong xuôi rồi.
Dialogue: 0,0:20:57.64,0:20:59.78,Default,,0000,0000,0000,,Đừng nhìn. Xấu hổ lắm.
Dialogue: 0,0:20:59.78,0:21:00.73,Default,,0000,0000,0000,,Đây là lần đầu tiên của cô hả?
Dialogue: 0,0:21:01.11,0:21:02.15,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:21:08.46,0:21:10.72,Default,,0000,0000,0000,,Tôi ko nghĩ đây là ý kiến hay.
Dialogue: 0,0:21:10.72,0:21:13.08,Default,,0000,0000,0000,,Yoko sẽ quở mắng tôi vì điều này.
Dialogue: 0,0:21:13.43,0:21:15.08,Default,,0000,0000,0000,,Cô ấy sẽ ko trách cô đâu.
Dialogue: 0,0:21:15.08,0:21:16.93,Default,,0000,0000,0000,,Vì cô ấy cũng làm điều tương tự.
Dialogue: 0,0:21:29.79,0:21:31.95,Default,,0000,0000,0000,,Nhột quá.
Dialogue: 0,0:21:32.95,0:21:35.13,Default,,0000,0000,0000,,Cơ thể tôi mất kiểm soát rồi...
Dialogue: 0,0:21:35.97,0:21:39.11,Default,,0000,0000,0000,,Thư giãn đi. Cái của tôi là thứ tuyệt nhất đó.
Dialogue: 0,0:21:39.11,0:21:41.92,Default,,0000,0000,0000,,Làm sao mà tôi thư giãn được cơ chứ?
Dialogue: 0,0:22:07.47,0:22:10.75,Default,,0000,0000,0000,,Tôi ra rồi. Có thứ gì đó đang...tôi ra rồi.
Dialogue: 0,0:22:21.03,0:22:23.75,Default,,0000,0000,0000,,Ăn uống luôn là việc khiến tôi sung sướng nhất.
Dialogue: 0,0:22:25.14,0:22:27.11,Default,,0000,0000,0000,,Tôi sẽ dạy cho cô một việc cũng sung sướng ko kém.
Dialogue: 0,0:22:38.32,0:22:39.17,Default,,0000,0000,0000,,Thấy thế nào?
Dialogue: 0,0:22:39.40,0:22:42.86,Default,,0000,0000,0000,,Tôi thấy rất sung sướng!
Dialogue: 0,0:22:43.95,0:22:45.15,Default,,0000,0000,0000,,Tất cả là vậy đó.
Dialogue: 0,0:22:45.29,0:22:47.29,Default,,0000,0000,0000,,Uhm, tôi đã có một quãng thời gian khó khăn.
Dialogue: 0,0:22:47.90,0:22:51.49,Default,,0000,0000,0000,,Tôi đã bị đánh gục bởi những gã kia.
Dialogue: 0,0:22:52.24,0:22:53.47,Default,,0000,0000,0000,,Nhưng giờ tôi ko còn giống lúc trước nữa.
Dialogue: 0,0:22:53.47,0:22:55.97,Default,,0000,0000,0000,,Hiện giờ tôi đã hồi phục hoàn toàn.
Dialogue: 0,0:22:56.25,0:23:00.15,Default,,0000,0000,0000,,Ta sẽ đùa giỡn với các người một chút, chuẩn bị đi nào.
Dialogue: 0,0:23:03.31,0:23:05.25,Default,,0000,0000,0000,,Tôi sẽ lấy mạng anh.
Dialogue: 0,0:23:05.35,0:23:06.82,Default,,0000,0000,0000,,Đừng như vậy, Toko.
Dialogue: 0,0:23:07.12,0:23:09.67,Default,,0000,0000,0000,,Cô ko việc gì phải phục vụ cho con mụ dâm đãng đó.
Dialogue: 0,0:23:09.78,0:23:11.51,Default,,0000,0000,0000,,Các người đang làm gì vậy?
Dialogue: 0,0:23:12.11,0:23:13.90,Default,,0000,0000,0000,,Các người ko thể tống khứ hắn đi giống luc trước được hả?
Dialogue: 0,0:23:14.35,0:23:15.54,Default,,0000,0000,0000,,Lũ vô dụng.
Dialogue: 0,0:23:15.71,0:23:17.73,Default,,0000,0000,0000,,Cuối cùng bà đã chịu ra mặt.  Tôi cứ tưởng bà đã trốn mất rồi.
Dialogue: 0,0:23:17.88,0:23:19.60,Default,,0000,0000,0000,,Tôi nghĩ lúc này bà nên trốn đi thì hơn.
Dialogue: 0,0:23:21.45,0:23:22.53,Default,,0000,0000,0000,,Thằng quái nào thế?
Dialogue: 0,0:23:23.32,0:23:25.55,Default,,0000,0000,0000,,Hắn là một dạng Shikoku.
Dialogue: 0,0:23:25.84,0:23:29.44,Default,,0000,0000,0000,,Suteki Ishi, đừng cười nữa. Làm việc của ngươi đi!
Dialogue: 0,0:23:29.85,0:23:33.42,Default,,0000,0000,0000,,Đến lúc rồi!
Dialogue: 0,0:23:41.04,0:23:42.81,Default,,0000,0000,0000,,Một gã quái vật hả...
Dialogue: 0,0:23:57.43,0:23:58.96,Default,,0000,0000,0000,,Chết tiệt. Đáng ra mình phải cẩn thận hơn.
Dialogue: 0,0:24:06.02,0:24:09.69,Default,,0000,0000,0000,,Hạ gục hắn đi! Tao sẽ lo việc kết liễu hắn sau cùng!
Dialogue: 0,0:24:10.06,0:24:11.01,Default,,0000,0000,0000,,Chết tiệt thật.
Dialogue: 0,0:24:15.09,0:24:17.09,Default,,0000,0000,0000,,Motoko, cô làm gì vậy?
Dialogue: 0,0:24:20.62,0:24:22.22,Default,,0000,0000,0000,,Toko, cô..
Dialogue: 0,0:24:22.77,0:24:24.21,Default,,0000,0000,0000,,Chạy đi.
Dialogue: 0,0:24:25.58,0:24:26.65,Default,,0000,0000,0000,,Tôi sẽ còn quay lại.
Dialogue: 0,0:24:29.10,0:24:32.58,Default,,0000,0000,0000,,Hắn chạy rồi. Đuổi theo mau, Dekao!
Dialogue: 0,0:24:32.69,0:24:35.30,Default,,0000,0000,0000,,Ko phải hướng này, ko phải hướng này!
Dialogue: 0,0:24:39.81,0:24:41.34,Default,,0000,0000,0000,,Đi thôi, Akuji-san.
Dialogue: 0,0:24:41.36,0:24:42.38,Default,,0000,0000,0000,,Quay về nhà thôi.
Dialogue: 0,0:24:42.38,0:24:44.38,Default,,0000,0000,0000,,Thả tôi ra! Im ngay!
Dialogue: 0,0:24:45.51,0:24:47.81,Default,,0000,0000,0000,,Đành phải xin lỗi cô hôm nay vậy.
Dialogue: 0,0:24:54.54,0:24:57.17,Default,,0000,0000,0000,,Cô làm ta thật vọng quá, Motoko.
Dialogue: 0,0:24:57.87,0:25:00.37,Default,,0000,0000,0000,,Nếu bà muốn lấy mạng tôi, thì làm luôn bây giờ đi.
Dialogue: 0,0:25:00.37,0:25:03.77,Default,,0000,0000,0000,,Cô thật là ngu ngốc. Như thế chẳng phải dễ dàng cho cô rồi sao.
Dialogue: 0,0:25:06.35,0:25:09.21,Default,,0000,0000,0000,,Bà chủ có chắc là như vậy ko ?
Dialogue: 0,0:25:10.02,0:25:14.25,Default,,0000,0000,0000,,Đúng thế. Ngươi muốn làm đằng trước hay đằng sau cô ta thì tùy.
Dialogue: 0,0:25:14.74,0:25:16.47,Default,,0000,0000,0000,,Các người may mắn đấy.
Dialogue: 0,0:25:16.68,0:25:18.23,Default,,0000,0000,0000,,Cô ta vẫn còn trinh đấy .
Dialogue: 0,0:25:18.23,0:25:19.88,Default,,0000,0000,0000,,Thật sao?!
Dialogue: 0,0:25:20.86,0:25:22.78,Default,,0000,0000,0000,,Tôi cứng lên rồi đây.
Dialogue: 0,0:25:22.78,0:25:25.33,Default,,0000,0000,0000,,Đừng nghĩ đến việc tự sát.
Dialogue: 0,0:25:25.33,0:25:29.41,Default,,0000,0000,0000,,Nếu cô làm chuyện gì ngu ngốc, ta sẽ lấy mạng cha cô.
Dialogue: 0,0:25:45.41,0:25:49.66,Default,,0000,0000,0000,,Ko!
