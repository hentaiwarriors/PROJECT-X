Dialogue: 0,0:00:03.00,0:00:09.45,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:40.02,0:00:42.26,Default,,0000,0000,0000,,Giờ nghỉ đã kết thúc.
Dialogue: 0,0:00:47.60,0:00:49.23,Default,,0000,0000,0000,,Ko, làm ơn
Dialogue: 0,0:00:49.23,0:00:51.23,Default,,0000,0000,0000,,Này, đừng có rời miệng mày ra!
Dialogue: 0,0:00:51.45,0:00:53.01,Default,,0000,0000,0000,,Bú cái này luôn đi.
Dialogue: 0,0:00:54.23,0:00:55.82,Default,,0000,0000,0000,,Những âm thanh tuyệt đấy nhỉ
Dialogue: 0,0:00:56.06,0:01:01.03,Default,,0000,0000,0000,,Phải chăng đó là vinh dự của một người cha khi được ngắm nhìn con gái mình trong hoàn cảnh như vậy?
Dialogue: 0,0:01:01.55,0:01:02.66,Default,,0000,0000,0000,,Motoko!
Dialogue: 0,0:01:03.53,0:01:07.44,Default,,0000,0000,0000,,Cô ta còn là một trinh nữ hai ngày trước đây vậy mà bây giờ cô ta làm chuyện này thật thành thạo.
Dialogue: 0,0:01:07.77,0:01:10.61,Default,,0000,0000,0000,,Chắc hẳn cô ta đã bú thứ đó hàng trăn lần trong vòng ba ngày.
Dialogue: 0,0:01:10.61,0:01:12.03,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:01:16.54,0:01:19.27,Default,,0000,0000,0000,,Đến lúc rồi đây!
Dialogue: 0,0:01:19.27,0:01:21.02,Default,,0000,0000,0000,,Ko, dừng lại
Dialogue: 0,0:01:22.08,0:01:23.20,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:01:30.97,0:01:32.31,Default,,0000,0000,0000,,Ko, ko!
Dialogue: 0,0:01:40.97,0:01:43.17,Default,,0000,0000,0000,,Có vẻ như cô ta ko đợi thêm được nữa!
Dialogue: 0,0:01:48.00,0:01:49.31,Default,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:01:49.50,0:01:53.34,Default,,0000,0000,0000,,Ko! chỗ đó của tôi rách ra mất, nó sắp rách ra rồi!
Dialogue: 0,0:01:55.18,0:01:56.37,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:02:05.54,0:02:09.63,Default,,0000,0000,0000,,Ko, ko! Tôi sắp ra rồi, Tôi ra rồi!
Dialogue: 0,0:02:11.12,0:02:14.58,Default,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:02:28.40,0:02:30.50,Default,,0000,0000,0000,,Ko tệ lắm nhỉ.
Dialogue: 0,0:02:31.22,0:02:31.95,Default,,0000,0000,0000,,Thưa bà chủ.
Dialogue: 0,0:02:33.52,0:02:34.70,Default,,0000,0000,0000,,Akuji?
Dialogue: 0,0:02:35.49,0:02:37.53,Default,,0000,0000,0000,,Tin tức này nghe thú vị đấy.
Dialogue: 0,0:02:37.67,0:02:39.21,Default,,0000,0000,0000,,Chuẩn bị tiến hành đi!
Dialogue: 0,0:02:39.53,0:02:40.13,Default,,0000,0000,0000,,Vâng thưa bà.
Dialogue: 0,0:02:40.69,0:02:45.94,Default,,0000,0000,0000,,Tốt nhất là ông nên thư giãn và chờ xem chuyện gì sẽ xảy ra tiếp theo đi.
Dialogue: 0,0:03:03.47,0:03:09.01,Default,,0000,0000,0000,,DAIAKUJI 2
Dialogue: 0,0:03:25.62,0:03:26.62,Default,,0000,0000,0000,,Chấm dứt kể từ ngày hôm nay.
Dialogue: 0,0:05:35.91,0:05:39.28,Default,,0000,0000,0000,,Đây là việc làm của tổ chức tình nguyện sao?
Dialogue: 0,0:05:42.13,0:05:45.66,Default,,0000,0000,0000,,Đúng thế, công việc của chúng tôi là phục vụ cho cộng đồng.
Dialogue: 0,0:05:45.66,0:05:50.81,Default,,0000,0000,0000,,Cô sẽ được phục vụ cho rất nhiều các quý ông trong cộng đồng đấy.
Dialogue: 0,0:05:52.98,0:05:54.97,Default,,0000,0000,0000,,Với thân thể nhơ bẩn của cô.
Dialogue: 0,0:05:55.52,0:05:57.63,Default,,0000,0000,0000,,Ông nói nhơ bẩn là sao?
Dialogue: 0,0:05:57.84,0:05:59.49,Default,,0000,0000,0000,,Thế này mà ko nhơ bẩn sao?
Dialogue: 0,0:06:02.36,0:06:05.77,Default,,0000,0000,0000,,Tôi mới chỉ chạm nhẹ vào mà nó đã cứng lên rồi.
Dialogue: 0,0:06:05.77,0:06:07.28,Default,,0000,0000,0000,,Chuyện gì thế nhỉ?
Dialogue: 0,0:06:08.97,0:06:11.35,Default,,0000,0000,0000,,Bởi vì, ông...
Dialogue: 0,0:06:12.60,0:06:19.39,Default,,0000,0000,0000,,Cái này đã giúp cô thỏa mãn nhiều lần từ khi cô tới đây.
Dialogue: 0,0:06:21.66,0:06:24.03,Default,,0000,0000,0000,,Cô thích nó đúng ko?
Dialogue: 0,0:06:24.30,0:06:26.03,Default,,0000,0000,0000,,Tôi ko hề.
Dialogue: 0,0:06:26.71,0:06:27.93,Default,,0000,0000,0000,,Nói dối là ko tốt đâu nhé.
Dialogue: 0,0:06:28.34,0:06:34.90,Default,,0000,0000,0000,,Cô muốn cái này vào bên trong cô ngay lập tức, đúng ko?
Dialogue: 0,0:06:35.65,0:06:37.48,Default,,0000,0000,0000,,Ko phải thế.
Dialogue: 0,0:06:37.49,0:06:38.89,Default,,0000,0000,0000,,Tôi hiểu rồi.
Dialogue: 0,0:06:40.14,0:06:41.44,Default,,0000,0000,0000,,Vậy tôi ko dùng nó nữa nhé.
Dialogue: 0,0:06:42.16,0:06:49.51,Default,,0000,0000,0000,,Những ngón tay này đã sờ mó cái mấu nhỏ của cô và làm cô như tan ra vì sung sướng đúng ko.
Dialogue: 0,0:06:52.94,0:06:58.32,Default,,0000,0000,0000,,Hơi thở nóng bỏng, và cái lưỡi này len lỏi khắp nơi trên cơ thể cô.
Dialogue: 0,0:07:03.60,0:07:08.16,Default,,0000,0000,0000,,Cái thứ này sẽ ko bao giờ được dùng để thỏa mãn cô nữa.
Dialogue: 0,0:07:12.93,0:07:15.69,Default,,0000,0000,0000,,Cô ko thể tự mình thỏa mãn được.
Dialogue: 0,0:07:16.29,0:07:19.54,Default,,0000,0000,0000,,Nếu cô muốn như vậy, tôi sẽ dạy cô phải làm gì, được chứ?
Dialogue: 0,0:07:20.98,0:07:22.60,Default,,0000,0000,0000,,Cô câm rồi sao?
Dialogue: 0,0:07:25.16,0:07:26.28,Default,,0000,0000,0000,,Làm ơn...
Dialogue: 0,0:07:27.05,0:07:30.82,Default,,0000,0000,0000,,Khi cầu xin điều gì đó, cô phải nói thật to và rõ ràng.
Dialogue: 0,0:07:31.75,0:07:32.79,Default,,0000,0000,0000,,Làm ơn...
Dialogue: 0,0:07:33.31,0:07:36.16,Default,,0000,0000,0000,,Làm ơn hãy tiếp tục các trò nhơ bẩn đó với tôi!
Dialogue: 0,0:07:36.86,0:07:38.05,Default,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,0:07:39.18,0:07:42.30,Default,,0000,0000,0000,,Vậy cô thích sự nhơ bẩn à?
Dialogue: 0,0:07:44.53,0:07:45.83,Default,,0000,0000,0000,,Vâng
Dialogue: 0,0:07:48.94,0:07:51.04,Default,,0000,0000,0000,,Hôm nay sẽ là bài tốt nghiệp của cô.
Dialogue: 0,0:07:51.56,0:07:52.32,Default,,0000,0000,0000,,Nào
Dialogue: 0,0:07:58.26,0:08:00.85,Default,,0000,0000,0000,,Cô có phải là một phụ nữ dâm đãng ko?
Dialogue: 0,0:08:03.87,0:08:09.56,Default,,0000,0000,0000,,Tôi hỏi lần nữa, cô có phải là một phụ nữ dâm đãng hay ko?
Dialogue: 0,0:08:13.12,0:08:17.65,Default,,0000,0000,0000,,Vâng! Tôi thật nhơ bẩn, tôi là một phụ nữ dâm đãng và nhơ bẩn.
Dialogue: 0,0:08:17.65,0:08:19.07,Default,,0000,0000,0000,,Vậy là xong.
Dialogue: 0,0:08:25.22,0:08:26.30,Default,,0000,0000,0000,,Đúng như tôi đã hình dung.
Dialogue: 0,0:08:26.82,0:08:29.56,Default,,0000,0000,0000,,Shimamoto và những người khác đang làm việc tại nơi đó.
Dialogue: 0,0:08:29.90,0:08:31.45,Default,,0000,0000,0000,,Tiếp tục công việc đi nhé.
Dialogue: 0,0:08:31.99,0:08:33.76,Default,,0000,0000,0000,,Ngài đi đâu thế, Chủ nhân?
Dialogue: 0,0:08:34.72,0:08:35.53,Default,,0000,0000,0000,,Đi ăn chút gì đó.
Dialogue: 0,0:08:44.37,0:08:45.58,Default,,0000,0000,0000,,Anh dạo này thế nào rồi?
Dialogue: 0,0:08:46.23,0:08:51.29,Default,,0000,0000,0000,,Có nhiều lời đồn đại là tổ chức tình nguyện đang kiếm tiền nhờ kinh doanh mãi dâm.
Dialogue: 0,0:08:51.69,0:08:52.45,Default,,0000,0000,0000,,Có thể đúng đấy.
Dialogue: 0,0:08:52.71,0:08:54.74,Default,,0000,0000,0000,,Có thể là sao? Ý anh là thật hả?
Dialogue: 0,0:08:55.12,0:08:57.93,Default,,0000,0000,0000,,Anh ko thể để Wakame-gumi chiếm lấy mọi thứ.
Dialogue: 0,0:08:58.46,0:09:00.28,Default,,0000,0000,0000,,Để làm được điều đó, cần rất nhiều tiền.
Dialogue: 0,0:09:02.20,0:09:03.68,Default,,0000,0000,0000,,Của anh đây, cám ơn vì đã đợi.
Dialogue: 0,0:09:07.67,0:09:08.79,Default,,0000,0000,0000,,Cay thế!
Dialogue: 0,0:09:08.83,0:09:12.90,Default,,0000,0000,0000,,Anh đáng bị như thế, cái này là dành cho những phụ nữ bị ngược đãi.
Dialogue: 0,0:09:12.90,0:09:14.23,Default,,0000,0000,0000,,Minka-san
Dialogue: 0,0:09:14.61,0:09:18.56,Default,,0000,0000,0000,,Anh đã quay về bình yên, nhưng anh lại làm những chuyện như vậy.
Dialogue: 0,0:09:18.78,0:09:19.68,Default,,0000,0000,0000,,Anh hãy lo việc của mình đi!
Dialogue: 0,0:09:21.40,0:09:22.48,Default,,0000,0000,0000,,Minka-san
Dialogue: 0,0:09:22.95,0:09:24.39,Default,,0000,0000,0000,,Chán thật.
Dialogue: 0,0:09:28.17,0:09:30.03,Default,,0000,0000,0000,,Anh để tiền trên bàn nhé.
Dialogue: 0,0:09:35.20,0:09:37.26,Default,,0000,0000,0000,,Akuji, đồ ngốc.
Dialogue: 0,0:09:47.40,0:09:49.21,Default,,0000,0000,0000,,Những người này là ai vậy?
Dialogue: 0,0:09:49.52,0:09:50.75,Default,,0000,0000,0000,,Cô ko biết à?
Dialogue: 0,0:09:51.27,0:09:54.26,Default,,0000,0000,0000,,Họ là những hội viên mới của tổ chức.
Dialogue: 0,0:09:54.49,0:09:55.79,Default,,0000,0000,0000,,Hội viên mới?
Dialogue: 0,0:09:56.14,0:09:57.86,Default,,0000,0000,0000,,Người đeo kính là Shimamoto Jun.
Dialogue: 0,0:09:58.27,0:10:00.58,Default,,0000,0000,0000,,Anh ta là một thành viên cũ của  Wakame-gumi.
Dialogue: 0,0:10:01.29,0:10:04.57,Default,,0000,0000,0000,,Người đàn ông mắt sếch kia là Oasugi Tsuyoshi.
Dialogue: 0,0:10:04.97,0:10:07.54,Default,,0000,0000,0000,,Ông ấy là huấn luyện viên võ thuật của Akuji ở Wakame-gumi.
Dialogue: 0,0:10:07.80,0:10:09.13,Default,,0000,0000,0000,,Ông ấy rất lợi hại.
Dialogue: 0,0:10:09.74,0:10:12.21,Default,,0000,0000,0000,,Ông ấy đã tập luyện cùng Akuji hôm qua.
Dialogue: 0,0:10:12.79,0:10:15.90,Default,,0000,0000,0000,,Và người phụ nữ xinh đẹp ngồi kia là Kanbara Yuko.
Dialogue: 0,0:10:16.57,0:10:21.09,Default,,0000,0000,0000,,Cô ấy từng là huấn luyện viên tình ái của Akuji, và cô ấy cũng cực kì lợi hại.
Dialogue: 0,0:10:21.96,0:10:24.75,Default,,0000,0000,0000,,Cô ấy có lẽ là người phụ nữ đầu tiên của Akuji.
Dialogue: 0,0:10:25.33,0:10:26.82,Default,,0000,0000,0000,,Mối tình đầu ấy hả...
Dialogue: 0,0:10:28.24,0:10:29.50,Default,,0000,0000,0000,,Hey, Tôi về rồi đây.
Dialogue: 0,0:10:29.84,0:10:31.26,Default,,0000,0000,0000,,Chào mừng anh, Aa-kun.
Dialogue: 0,0:10:31.88,0:10:33.04,Default,,0000,0000,0000,,Tình hình thế nào rồi?
Dialogue: 0,0:10:33.45,0:10:35.73,Default,,0000,0000,0000,,Chúng ta cuối cùng đã bán được Ikoma và Heguri.
Dialogue: 0,0:10:36.31,0:10:39.10,Default,,0000,0000,0000,,Chủ nhân, chúng ta sẽ làm gì với con tin đây?
Dialogue: 0,0:10:39.16,0:10:41.89,Default,,0000,0000,0000,,À đúng rồi, chúng ta cần nhanh chóng làm gì đó.
Dialogue: 0,0:10:42.09,0:10:43.89,Default,,0000,0000,0000,,Được thôi, tôi có ý này.
Dialogue: 0,0:10:44.68,0:10:46.01,Default,,0000,0000,0000,,Mang con tin tới đây nào.
Dialogue: 0,0:10:56.54,0:10:58.30,Default,,0000,0000,0000,,Thật đúng như những gì tôi đã hình dung về Aa-kun.
Dialogue: 0,0:10:58.91,0:11:00.48,Default,,0000,0000,0000,,Ý cô là gì khi nói đã hình dung?
Dialogue: 0,0:11:00.48,0:11:01.38,Default,,0000,0000,0000,,Làm cái trò này ấy hả...
Dialogue: 0,0:11:01.38,0:11:03.38,Default,,0000,0000,0000,,Chẳng phải cô cũng từng như thế sao?
Dialogue: 0,0:11:05.75,0:11:11.11,Default,,0000,0000,0000,,Nhưng có hơi hoang đường ko khi anh ấy nói sẽ thành công chỉ sau năm phút, cô ta sẽ đâm anh ấy mất.
Dialogue: 0,0:11:11.32,0:11:13.01,Default,,0000,0000,0000,,Đừng lo. Cứ tiếp tục quan sát đi nào.
Dialogue: 0,0:11:22.64,0:11:25.55,Default,,0000,0000,0000,,Cô có muốn làm việc cho tôi ko?
Dialogue: 0,0:11:26.88,0:11:28.22,Default,,0000,0000,0000,,Ko, tôi ko...
Dialogue: 0,0:11:30.44,0:11:32.32,Default,,0000,0000,0000,,Ôi nó mềm quá...
Dialogue: 0,0:11:40.26,0:11:42.09,Default,,0000,0000,0000,,Hình như có gì đang trào ra dưới đây nhỉ.
Dialogue: 0,0:11:46.99,0:11:51.53,Default,,0000,0000,0000,,Tôi cảm thấy. Dường như đó ko còn là cơ thế tôi nữa.
Dialogue: 0,0:11:54.16,0:11:55.71,Default,,0000,0000,0000,,Cảm thấy thế nào?
Dialogue: 0,0:11:57.36,0:11:59.40,Default,,0000,0000,0000,,Tôi đoán là rất tuyệt.
Dialogue: 0,0:11:59.53,0:12:01.45,Default,,0000,0000,0000,,Nếu anh con giang rộng nó hơn nữa...
Dialogue: 0,0:12:06.51,0:12:08.60,Default,,0000,0000,0000,,Oops, hết năm phú rồi.
Dialogue: 0,0:12:09.12,0:12:09.87,Default,,0000,0000,0000,,Như đã hứa.
Dialogue: 0,0:12:10.03,0:12:11.51,Default,,0000,0000,0000,,Cô có thể đâm tôi với thanh kiếm này nếu muốn.
Dialogue: 0,0:12:12.05,0:12:14.23,Default,,0000,0000,0000,,Hay là cô muốn tôi tiếp tục?
Dialogue: 0,0:12:27.04,0:12:29.08,Default,,0000,0000,0000,,Tôi ko làm được.
Dialogue: 0,0:12:29.54,0:12:30.32,Default,,0000,0000,0000,,Ngoan lắm.
Dialogue: 0,0:12:31.37,0:12:33.15,Default,,0000,0000,0000,,Tốt, Aa-kun đã thắng.
Dialogue: 0,0:12:37.32,0:12:41.30,Default,,0000,0000,0000,,Để thưởng cho cô, tôi sẽ dành cho cô điều cô chưa hề cảm nhận được.
Dialogue: 0,0:12:41.83,0:12:44.96,Default,,0000,0000,0000,,Nó to thế kia thì sao mà nhét vừa được...
Dialogue: 0,0:12:53.33,0:12:57.25,Default,,0000,0000,0000,,Tôi chưa bao giờ cảm thấy thế này trước kia.
Dialogue: 0,0:12:59.00,0:13:00.56,Default,,0000,0000,0000,,Đừng có ra bây giờ đấy nhé.
Dialogue: 0,0:13:05.95,0:13:07.78,Default,,0000,0000,0000,,Sướng quá.
Dialogue: 0,0:13:18.66,0:13:21.04,Default,,0000,0000,0000,,Ko, tôi ko thể chịu đựng hơn được nữa.
Dialogue: 0,0:13:21.28,0:13:24.22,Default,,0000,0000,0000,,Tôi ko giữ thêm được nữa.
Dialogue: 0,0:13:24.22,0:13:25.98,Default,,0000,0000,0000,,Tôi thì có thể giữ lâu hơn nữa đấy.
Dialogue: 0,0:13:25.98,0:13:31.00,Default,,0000,0000,0000,,Ko thể như vậy được, làm ơn ra đi, ko thì tôi phát điên lên mất.
Dialogue: 0,0:13:31.18,0:13:32.40,Default,,0000,0000,0000,,Thế cũng tốt.
Dialogue: 0,0:13:32.58,0:13:34.40,Default,,0000,0000,0000,,Cô cứ phát điên lên đi.
Dialogue: 0,0:13:35.48,0:13:36.84,Default,,0000,0000,0000,,Ko, làm ơn...
Dialogue: 0,0:13:40.95,0:13:43.06,Default,,0000,0000,0000,,Làm ơn tha cho tôi...
Dialogue: 0,0:13:45.01,0:13:46.69,Default,,0000,0000,0000,,Cấp báo!
Dialogue: 0,0:13:47.69,0:13:51.45,Default,,0000,0000,0000,,Vừa có điện thoại từ Wakame-gumi. Chúng đã bắt Minka.
Dialogue: 0,0:13:51.45,0:13:53.28,Default,,0000,0000,0000,,Chúng muốn Akuji tới đó một mình.
Dialogue: 0,0:13:53.38,0:13:54.69,Default,,0000,0000,0000,,Chuyện quái quỉ gì thế?!
Dialogue: 0,0:13:55.73,0:13:57.91,Default,,0000,0000,0000,,Chủ nhân, đó chắn hẳn là một cái bẫy.
Dialogue: 0,0:13:58.17,0:13:59.24,Default,,0000,0000,0000,,Anh chỉ cần cử thuộc hạ đi là được rồi
Dialogue: 0,0:13:59.44,0:14:01.75,Default,,0000,0000,0000,,Ko thể làm thế được.
Dialogue: 0,0:14:01.88,0:14:02.94,Default,,0000,0000,0000,,Đặc biệt là trong trường hợp này.
Dialogue: 0,0:14:03.28,0:14:04.24,Default,,0000,0000,0000,,Chủ nhân...
Dialogue: 0,0:14:08.42,0:14:10.17,Default,,0000,0000,0000,,Tôi vẫn chưa ra mà.
Dialogue: 0,0:14:10.49,0:14:14.53,Default,,0000,0000,0000,,Thế này thì thà giết tôi đi còn hơn!
Dialogue: 0,0:14:21.46,0:14:23.47,Default,,0000,0000,0000,,Thân hình đẹp đấy chứ.
Dialogue: 0,0:14:24.34,0:14:25.45,Default,,0000,0000,0000,,Bà định làm gì...
Dialogue: 0,0:14:26.57,0:14:28.40,Default,,0000,0000,0000,,Một vài điều thú vị.
Dialogue: 0,0:14:32.59,0:14:33.64,Default,,0000,0000,0000,,Cái gì vậy?
Dialogue: 0,0:14:33.99,0:14:35.91,Default,,0000,0000,0000,,Đây là một loại tình dược của Ouime.
Dialogue: 0,0:14:36.52,0:14:38.51,Default,,0000,0000,0000,,Tác dụng của nó khá mạnh đấy.
Dialogue: 0,0:14:41.42,0:14:48.34,Default,,0000,0000,0000,,Ko, ko, ko, ko!
Dialogue: 0,0:14:54.29,0:14:55.88,Default,,0000,0000,0000,,Cái này là bùa may mắn của em.
Dialogue: 0,0:14:55.89,0:14:59.20,Default,,0000,0000,0000,,Nó rất linh nghiệm đấy
Dialogue: 0,0:14:59.38,0:15:01.31,Default,,0000,0000,0000,,Xin lỗi, anh ko thể giữ nó được.
Dialogue: 0,0:15:02.75,0:15:03.81,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:15:03.90,0:15:06.28,Default,,0000,0000,0000,,Chúng ta sống ở hai thế giới hoàn toàn khác nhau.
Dialogue: 0,0:15:06.60,0:15:09.74,Default,,0000,0000,0000,,Anh ko xứng để em quan tâm như vậy đâu.
Dialogue: 0,0:15:10.30,0:15:11.08,Default,,0000,0000,0000,,Tạm biệt.
Dialogue: 0,0:15:13.83,0:15:17.00,Default,,0000,0000,0000,,Làm ơn, làm ơn hãy quay về bình yên nhé.
Dialogue: 0,0:15:26.21,0:15:27.38,Default,,0000,0000,0000,,Là nơi này.
Dialogue: 0,0:15:38.33,0:15:39.19,Default,,0000,0000,0000,,Minka-san!
Dialogue: 0,0:15:42.43,0:15:43.73,Default,,0000,0000,0000,,Đừng có lại gần đây!
Dialogue: 0,0:15:46.38,0:15:47.06,Default,,0000,0000,0000,,Cái gì...
Dialogue: 0,0:15:49.77,0:15:51.37,Default,,0000,0000,0000,,Cậu tới rồi đó à.
Dialogue: 0,0:15:51.43,0:15:54.04,Default,,0000,0000,0000,,Chắc hẳn cậu đang nguyền rủa cô ta.
Dialogue: 0,0:15:55.81,0:15:56.69,Default,,0000,0000,0000,,Con dâm phụ này!
Dialogue: 0,0:15:56.88,0:15:58.29,Default,,0000,0000,0000,,Bà đã làm gì cô ấy vậy?
Dialogue: 0,0:15:58.66,0:16:00.46,Default,,0000,0000,0000,,Làm điều mà anh rất mong chờ.
Dialogue: 0,0:16:00.46,0:16:04.04,Default,,0000,0000,0000,,Ta sẽ giúp anh vui vẻ với cô ta.
Dialogue: 0,0:16:04.04,0:16:05.31,Default,,0000,0000,0000,,Dâm phụ
Dialogue: 0,0:16:06.21,0:16:07.37,Default,,0000,0000,0000,,Bắt đầu đi nào.
Dialogue: 0,0:16:07.64,0:16:08.44,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:16:08.44,0:16:09.41,Default,,0000,0000,0000,,Chẳng phải tôi đã nói từ trước rồi sao?
Dialogue: 0,0:16:09.67,0:16:13.04,Default,,0000,0000,0000,,Nếu cô ko làm điều đó, Akuji sẽ mất mạng.
Dialogue: 0,0:16:13.93,0:16:16.44,Default,,0000,0000,0000,,Minka-san, đừng nghe bà ta...
Dialogue: 0,0:16:32.61,0:16:33.49,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:16:49.15,0:16:53.82,Default,,0000,0000,0000,,Thử nói cô ko muốn làm điều đo xem nào, dưới này nước của cô đang tuôn ra như suối kìa.
Dialogue: 0,0:16:53.82,0:16:55.35,Default,,0000,0000,0000,,Tại vì bà...
Dialogue: 0,0:16:55.97,0:16:58.61,Default,,0000,0000,0000,,Tiếp theo, phục vụ cậu ta bằng âm hộ của cô đi!
Dialogue: 0,0:16:59.14,0:17:00.07,Default,,0000,0000,0000,,Ko thể nào...
Dialogue: 0,0:17:01.64,0:17:02.99,Default,,0000,0000,0000,,Ko, ko thể...
Dialogue: 0,0:17:04.59,0:17:05.41,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:17:26.24,0:17:29.15,Default,,0000,0000,0000,,Akuji, em...
Dialogue: 0,0:17:30.33,0:17:31.43,Default,,0000,0000,0000,,Minka-san
Dialogue: 0,0:17:31.81,0:17:33.85,Default,,0000,0000,0000,,Em thấy thật khác thường.
Dialogue: 0,0:17:34.77,0:17:40.12,Default,,0000,0000,0000,,Điều này thật khác thường, nhưng em ko thể dừng lại được.
Dialogue: 0,0:17:47.01,0:17:52.44,Default,,0000,0000,0000,,Akuji, làm ơn di chuyển đi, ko thì em sẽ...
Dialogue: 0,0:17:52.62,0:17:53.77,Default,,0000,0000,0000,,Minka-san
Dialogue: 0,0:17:54.35,0:17:56.41,Default,,0000,0000,0000,,Akuji, Akuji...
Dialogue: 0,0:17:57.46,0:18:00.84,Default,,0000,0000,0000,,Em sắp ra rồi, làm ơn hãy ra cùng với em.
Dialogue: 0,0:18:02.26,0:18:05.94,Default,,0000,0000,0000,,Ko, em ra rồi, ra rồi!
Dialogue: 0,0:18:07.86,0:18:10.04,Default,,0000,0000,0000,,Huh? Sao thế này...?
Dialogue: 0,0:18:12.39,0:18:14.89,Default,,0000,0000,0000,,Ta sẽ ko để hai người tận hưởng với nhau đâu.
Dialogue: 0,0:18:14.95,0:18:16.98,Default,,0000,0000,0000,,Akuji, nhìn cho kĩ vào nhé...
Dialogue: 0,0:18:17.19,0:18:20.07,Default,,0000,0000,0000,,xem cô ta làm tình với những gã khác thế nào!
Dialogue: 0,0:18:20.18,0:18:21.54,Default,,0000,0000,0000,,Con dâm phụ khốn kiếp
Dialogue: 0,0:18:21.84,0:18:23.87,Default,,0000,0000,0000,,Ko, ko...
Dialogue: 0,0:18:25.17,0:18:27.58,Default,,0000,0000,0000,,Cô em nói ko là sao? Cô ướt lám rồi.
Dialogue: 0,0:18:27.97,0:18:30.38,Default,,0000,0000,0000,,Ko, cái này là vì...
Dialogue: 0,0:18:32.32,0:18:34.45,Default,,0000,0000,0000,,Tiếng kêu của cô em thật cuốn hút.
Dialogue: 0,0:18:34.93,0:18:38.02,Default,,0000,0000,0000,,Ko. đừng làm mạnh như thế...
Dialogue: 0,0:18:40.55,0:18:44.96,Default,,0000,0000,0000,,Tác dụng của thuốc đang tăng lên, cô ta sẽ nứng lên nhanh thôi.
Dialogue: 0,0:18:44.96,0:18:48.41,Default,,0000,0000,0000,,Ko, tôi sắp ko nhịn nổi nữa rồi, làm ơn dừng lại!
Dialogue: 0,0:18:48.41,0:18:51.15,Default,,0000,0000,0000,,Tôi ko nhịn nổi nữa rồi. Tôi mất kiểm soát rồi!
Dialogue: 0,0:18:52.86,0:18:54.62,Default,,0000,0000,0000,,Cô em dễ thương thật đấy.
Dialogue: 0,0:18:54.87,0:18:55.74,Default,,0000,0000,0000,,Ko
Dialogue: 0,0:19:04.12,0:19:05.46,Default,,0000,0000,0000,,Tôi cảm thấy...
Dialogue: 0,0:19:05.78,0:19:09.76,Default,,0000,0000,0000,,Sướng quá, như vậy sướng lắm.
Dialogue: 0,0:19:10.46,0:19:11.41,Default,,0000,0000,0000,,Minka-san!
Dialogue: 0,0:19:11.80,0:19:14.09,Default,,0000,0000,0000,,Nói xem tôi phải làm gì cho cô em nào.
Dialogue: 0,0:19:14.49,0:19:15.68,Default,,0000,0000,0000,,Tôi muốn nó.
Dialogue: 0,0:19:15.97,0:19:17.09,Default,,0000,0000,0000,,Cô em muốn ở đâu?
Dialogue: 0,0:19:17.20,0:19:19.59,Default,,0000,0000,0000,,Trong miệng tôi, hãy nhét cái đó vào miệng tôi.
Dialogue: 0,0:19:19.59,0:19:21.07,Default,,0000,0000,0000,,Cô em thật dâm đãng.
Dialogue: 0,0:19:26.18,0:19:28.10,Default,,0000,0000,0000,,Ngon quá.
Dialogue: 0,0:19:28.23,0:19:31.12,Default,,0000,0000,0000,,Được rồi, giờ tôi sẽ chơi cửa sau của cô em.
Dialogue: 0,0:19:34.74,0:19:36.81,Default,,0000,0000,0000,,Tôi sẽ phá trinh chỗ này của cô em.
Dialogue: 0,0:19:36.81,0:19:39.65,Default,,0000,0000,0000,,Ko, dừng lại, đau quá!
Dialogue: 0,0:19:42.38,0:19:44.16,Default,,0000,0000,0000,,Chỉ đau lúc đầu thôi.
Dialogue: 0,0:19:44.16,0:19:45.50,Default,,0000,0000,0000,,Sau đó sẽ rất tuyệt đấy...
Dialogue: 0,0:19:46.91,0:19:49.76,Default,,0000,0000,0000,,Nó đang vào trong, đang vào trong...
Dialogue: 0,0:19:49.76,0:19:51.80,Default,,0000,0000,0000,,Nó đang vào đâu?
Dialogue: 0,0:19:51.97,0:19:53.62,Default,,0000,0000,0000,,Trong hậu môn tôi.
Dialogue: 0,0:19:54.56,0:19:58.45,Default,,0000,0000,0000,,Sâu quá, sâu trong hậu môn tôi.
Dialogue: 0,0:19:59.18,0:20:02.51,Default,,0000,0000,0000,,Coi nào, hãy ra ngay trước mặt Akuji nào.
Dialogue: 0,0:20:02.84,0:20:04.28,Default,,0000,0000,0000,,Em ko thể, Akuji.
Dialogue: 0,0:20:04.63,0:20:08.55,Default,,0000,0000,0000,,Em xin lỗi, em ko thể chịu đựng nổi nữa.
Dialogue: 0,0:20:11.08,0:20:12.97,Default,,0000,0000,0000,,Thế này, sướng ko hả?
Dialogue: 0,0:20:14.10,0:20:17.13,Default,,0000,0000,0000,,Vâng, sướng lắm, sướng quá đi.
Dialogue: 0,0:20:18.22,0:20:20.55,Default,,0000,0000,0000,,Xin lỗi, em ra mất.
Dialogue: 0,0:20:20.55,0:20:24.58,Default,,0000,0000,0000,,Ko, sướng quá, em sắp ra...
Dialogue: 0,0:20:35.33,0:20:39.08,Default,,0000,0000,0000,,Đó là bài học cho kẻ dám chống đối lại ta.
Dialogue: 0,0:20:41.44,0:20:42.48,Default,,0000,0000,0000,,Dekao!
Dialogue: 0,0:20:58.14,0:20:58.99,Default,,0000,0000,0000,,Chết đi.
Dialogue: 0,0:20:59.01,0:20:59.89,Default,,0000,0000,0000,,Cái gì...?
Dialogue: 0,0:21:01.80,0:21:03.28,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:21:03.28,0:21:05.28,Default,,0000,0000,0000,,Hãy ra khỏi đây mau!
Dialogue: 0,0:21:12.41,0:21:13.42,Default,,0000,0000,0000,,Cô là...?
Dialogue: 0,0:21:13.82,0:21:16.26,Default,,0000,0000,0000,,Đừng quan tâm đến tôi, cô gái kia thế nào rồi?
Dialogue: 0,0:21:16.55,0:21:17.84,Default,,0000,0000,0000,,Đúng rồi.
Dialogue: 0,0:21:22.17,0:21:23.00,Default,,0000,0000,0000,,Đừng lại gần đây.
Dialogue: 0,0:21:23.59,0:21:24.31,Default,,0000,0000,0000,,Minka-san
Dialogue: 0,0:21:27.89,0:21:31.87,Default,,0000,0000,0000,,Tha lỗi cho em, em ko thể ở bên anh được nữa.
Dialogue: 0,0:21:33.28,0:21:35.46,Default,,0000,0000,0000,,Anh đã trông thấy tất cả. Em ko thể...
Dialogue: 0,0:21:35.55,0:21:36.62,Default,,0000,0000,0000,,Minka-san!
Dialogue: 0,0:21:36.72,0:21:37.76,Default,,0000,0000,0000,,Vĩnh biệt.
Dialogue: 0,0:21:39.16,0:21:40.06,Default,,0000,0000,0000,,Minka-san!
Dialogue: 0,0:21:51.97,0:21:53.57,Default,,0000,0000,0000,,Minka!
Dialogue: 0,0:22:00.99,0:22:01.92,Default,,0000,0000,0000,,Cô là dì của chủ nhân?
Dialogue: 0,0:22:02.19,0:22:08.03,Default,,0000,0000,0000,,Vâng, điều này hoàn toàn chính xác, tôi là con gái của ông ngoại Akuji, Ippatsu, và một tình nhân của ông ấy.
Dialogue: 0,0:22:08.56,0:22:10.18,Default,,0000,0000,0000,,Tên tôi là Takeda Satsu.
Dialogue: 0,0:22:10.58,0:22:12.50,Default,,0000,0000,0000,,Cô đã lên chức dì ở tuổi này sao?
Dialogue: 0,0:22:12.94,0:22:15.02,Default,,0000,0000,0000,,Nếu chị ko muốn gọi là dì, thì có thể gọi tôi là Sat-chan.
Dialogue: 0,0:22:22.59,0:22:23.36,Default,,0000,0000,0000,,Cái này là?
Dialogue: 0,0:22:23.57,0:22:24.47,Default,,0000,0000,0000,,Tôi thấy nó trên sàn nhà.
Dialogue: 0,0:22:24.99,0:22:26.54,Default,,0000,0000,0000,,Nó rất quan trọng với anh, phải ko?
Dialogue: 0,0:22:33.41,0:22:34.96,Default,,0000,0000,0000,,Vật này là của cô ấy...
Dialogue: 0,0:22:39.90,0:22:41.00,Default,,0000,0000,0000,,Là ngài Kaga đấy hả.
Dialogue: 0,0:22:41.32,0:22:43.49,Default,,0000,0000,0000,,Mộ thành viên của Wakame-gumi làm gì ở đây vậy?
Dialogue: 0,0:22:43.91,0:22:45.35,Default,,0000,0000,0000,,Tôi có một thỉnh cầu với anh.
Dialogue: 0,0:22:45.84,0:22:46.57,Default,,0000,0000,0000,,Thỉnh cầu?
Dialogue: 0,0:22:47.18,0:22:50.59,Default,,0000,0000,0000,,Tôi mong anh chỉ trừng trị Ran, ko phải toàn bộ Wakame-gumi.
Dialogue: 0,0:22:51.23,0:22:52.52,Default,,0000,0000,0000,,Tôi đã sắp đặt một kế hoạch.
Dialogue: 0,0:22:55.86,0:22:56.60,Default,,0000,0000,0000,,Chết tiệt...
Dialogue: 0,0:22:57.29,0:22:59.79,Default,,0000,0000,0000,,Tại sao ta luôn bị phá đám vào đúng lúc quan trọng nhất chứ?
Dialogue: 0,0:23:00.29,0:23:02.72,Default,,0000,0000,0000,,Bà ca cẩm chuyện đó suốt mấy ngày hôm nay rồi đó.
Dialogue: 0,0:23:02.72,0:23:03.60,Default,,0000,0000,0000,,Sao ngươi dám!
Dialogue: 0,0:23:03.60,0:23:04.90,Default,,0000,0000,0000,,Chúng ta đang bị tấn công!
Dialogue: 0,0:23:04.90,0:23:05.42,Default,,0000,0000,0000,,Cái gì...
Dialogue: 0,0:23:06.38,0:23:07.28,Default,,0000,0000,0000,,Tệ thật!
Dialogue: 0,0:23:07.28,0:23:09.53,Default,,0000,0000,0000,,Có vẻ như Kaga đứng đằng sau chuyện này...
Dialogue: 0,0:23:09.53,0:23:11.78,Default,,0000,0000,0000,,Chúng ta cần phải rời khỏi đây ngay.
Dialogue: 0,0:23:13.68,0:23:16.29,Default,,0000,0000,0000,,Đã có kẻ phản bội trong hàng ngũ chúng ta.
Dialogue: 0,0:23:18.33,0:23:20.07,Default,,0000,0000,0000,,Thêm nữa, chúng mới thu nạp thêm một cô bé cực kì lợi hại.
Dialogue: 0,0:23:20.07,0:23:21.40,Default,,0000,0000,0000,,Chúng ta ko thể ngồi yên ở đây được!
Dialogue: 0,0:23:25.93,0:23:28.76,Default,,0000,0000,0000,,Trước tiên, chúng ta phải xử lí Dekao, nếu ko sẽ ko gì có thể ngăn được hắn.
Dialogue: 0,0:23:29.42,0:23:33.22,Default,,0000,0000,0000,,Để làm được điều này, tôi đã tẩm thuốc mê vào thức ăn của hắn.
Dialogue: 0,0:23:33.49,0:23:35.75,Default,,0000,0000,0000,,Phải kết liễu hắn trong lúc hắn đang ngủ.
Dialogue: 0,0:23:36.40,0:23:37.29,Default,,0000,0000,0000,,Hắn ko có ở đây.
Dialogue: 0,0:23:37.29,0:23:38.40,Default,,0000,0000,0000,,Sao lại như vậy.
Dialogue: 0,0:23:45.02,0:23:46.01,Default,,0000,0000,0000,,Ko thể nào!
Dialogue: 0,0:23:46.24,0:23:47.08,Default,,0000,0000,0000,,Motoko!
Dialogue: 0,0:23:49.05,0:23:50.08,Default,,0000,0000,0000,,Akuji
Dialogue: 0,0:23:52.60,0:23:55.36,Default,,0000,0000,0000,,Kế hoạch của ngươi thật thô thiển.
Dialogue: 0,0:23:56.12,0:23:58.49,Default,,0000,0000,0000,,Ta sẽ ko để ngươi chạy thoát nữa đâu, Akuji.
Dialogue: 0,0:23:58.49,0:24:00.07,Default,,0000,0000,0000,,Dekao, giết hắn đi!
Dialogue: 0,0:24:04.65,0:24:05.72,Default,,0000,0000,0000,,Chậm quá!
Dialogue: 0,0:24:09.44,0:24:11.19,Default,,0000,0000,0000,,Dekao, tấn công tiếp đi!
Dialogue: 0,0:24:17.78,0:24:19.35,Default,,0000,0000,0000,,Cú này là kết liễu đây!
Dialogue: 0,0:24:24.70,0:24:25.46,Default,,0000,0000,0000,,Oasugi!
Dialogue: 0,0:24:28.24,0:24:32.78,Default,,0000,0000,0000,,Chủ nhân, hãy bình tĩnh, ko thể đọ sức mạnh với hắn. Chúng ta cần tấn công vào điểm yếu của hắn!
Dialogue: 0,0:24:33.00,0:24:34.14,Default,,0000,0000,0000,,Dekao!
Dialogue: 0,0:24:35.44,0:24:36.32,Default,,0000,0000,0000,,Được thôi
Dialogue: 0,0:24:39.02,0:24:40.18,Default,,0000,0000,0000,,Đỡ này!
Dialogue: 0,0:24:45.24,0:24:47.77,Default,,0000,0000,0000,,Nào chủ nhân! Hãy xuất chiêu chúng ta đã dày công luyện tập!
Dialogue: 0,0:24:47.88,0:24:48.49,Default,,0000,0000,0000,,Được thôi
Dialogue: 0,0:25:16.57,0:25:18.72,Default,,0000,0000,0000,,Ko thể tin được là mình lại thất bại nhục nhã thế này.
Dialogue: 0,0:25:18.88,0:25:22.16,Default,,0000,0000,0000,,Xong việc này tôi sẽ bắt Akuji làm nốt chuyện đó với tôi.
Dialogue: 0,0:25:22.42,0:25:23.80,Default,,0000,0000,0000,,Chuyện đó sẽ ko bao giờ xảy ra.
Dialogue: 0,0:25:23.99,0:25:29.15,Default,,0000,0000,0000,,Tôi là người cừ nhất, nên tôi sẽ được làm chuyện đó với Akuji.
Dialogue: 0,0:25:29.48,0:25:31.51,Default,,0000,0000,0000,,Còn lâu nhé, người đó sẽ là tôi.
Dialogue: 0,0:25:31.60,0:25:33.14,Default,,0000,0000,0000,,Mơ đi em! Là tôi mới đúng!
Dialogue: 0,0:25:33.14,0:25:34.27,Default,,0000,0000,0000,,Người đó chắc chắn sẽ là tôi!
Dialogue: 0,0:25:34.27,0:25:35.16,Default,,0000,0000,0000,,Là tôi!
Dialogue: 0,0:25:35.16,0:25:35.72,Default,,0000,0000,0000,,Là tôi!
Dialogue: 0,0:25:35.72,0:25:36.29,Default,,0000,0000,0000,,Là tôi!
Dialogue: 0,0:25:36.29,0:25:37.08,Default,,0000,0000,0000,,Chắc chắn sẽ là tôi!
Dialogue: 0,0:25:37.08,0:25:38.08,Default,,0000,0000,0000,,Ko! Là tôi!
Dialogue: 0,0:25:38.34,0:25:38.86,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:25:40.91,0:25:41.35,Default,,0000,0000,0000,,Hai con ấm đầu
Dialogue: 0,0:25:44.61,0:25:45.68,Default,,0000,0000,0000,,Cô ổn chứ?
Dialogue: 0,0:25:47.63,0:25:48.94,Default,,0000,0000,0000,,Tôi ko muốn nói chuyện với anh.
Dialogue: 0,0:25:49.48,0:25:51.40,Default,,0000,0000,0000,,Nếu cô còn có sức để cãi lại tôi thì hẳn là cô vẫn ổn.
Dialogue: 0,0:25:53.43,0:25:54.65,Default,,0000,0000,0000,,Bao giờ thì bọn chúng mới ngừng lại đây?
Dialogue: 0,0:25:55.71,0:25:56.63,Default,,0000,0000,0000,,Ai biết được?
Dialogue: 0,0:25:56.77,0:26:02.33,Default,,0000,0000,0000,,Khi chúng dừng lại, tôi sẽ lại cho chúng uống một liều Viagra đủ để đánh gục một người bình thường.
Dialogue: 0,0:26:02.77,0:26:05.81,Default,,0000,0000,0000,,Chúng chắn chắn sẽ chơi bà cho đến chết.
Dialogue: 0,0:26:07.58,0:26:08.77,Default,,0000,0000,0000,,Ko thể nào...
Dialogue: 0,0:26:08.77,0:26:09.99,Default,,0000,0000,0000,,Tôi ra rồi
Dialogue: 0,0:26:09.99,0:26:13.24,Default,,0000,0000,0000,,Ko, dừng lại. Đừng chơi tôi nữa.
Dialogue: 0,0:26:24.67,0:26:27.01,Default,,0000,0000,0000,,Cuối cùng mình cũng ko tìm thấy cố ấy.
Dialogue: 0,0:26:37.76,0:26:40.35,Default,,0000,0000,0000,,Xin lỗi, nhưng bây giờ tất cả đã kết thúc.
Dialogue: 0,0:26:40.82,0:26:43.45,Default,,0000,0000,0000,,Anh ko xứng để giữ nó.
Dialogue: 0,0:26:48.86,0:26:54.20,Default,,0000,0000,0000,,Vậy, cô sẽ gia nhập Wakame-gumi, ý tôi là Akuji-gumi chứ?
Dialogue: 0,0:26:54.26,0:26:56.21,Default,,0000,0000,0000,,Dự định tiếp theo của anh là gì vậy?
Dialogue: 0,0:26:56.47,0:26:58.45,Default,,0000,0000,0000,,Tôi sẽ kiểm soát toàn bộ Osaka.
Dialogue: 0,0:27:05.59,0:27:06.92,Default,,0000,0000,0000,,Xin lỗi vì đã cắt ngang.
Dialogue: 0,0:27:07.45,0:27:10.56,Default,,0000,0000,0000,,Vùng Shikina có một giáo phái đang phát triển rất mạnh.
Dialogue: 0,0:27:10.94,0:27:11.99,Default,,0000,0000,0000,,Một giáo phái hả?
Dialogue: 0,0:27:12.83,0:27:15.06,Default,,0000,0000,0000,,Họ gọi nó là "Nako-kyo"
