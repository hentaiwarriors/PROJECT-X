Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:15.92,0:00:19.01,Default,,0000,0000,0000,,Taketo! Tôi ko thích những trò như vậy đâu!
Dialogue: 0,0:00:24.29,0:00:26.52,Default,,0000,0000,0000,,Cậu vẫn còn non lắm, cưng à.
Dialogue: 0,0:00:26.59,0:00:30.72,Default,,0000,0000,0000,,Hãy cố chờ đến lúc cậu trở thành một người đàn ông trưởng thành và đáng tin cậy.
Dialogue: 0,0:00:32.70,0:00:34.93,Default,,0000,0000,0000,,Chào buổi sáng, Mr. Nogawa.
Dialogue: 0,0:00:35.47,0:00:38.44,Default,,0000,0000,0000,,Cha cậu đã chấp thuận cho tôi vào làm trong công ty ông ấy.
Dialogue: 0,0:00:38.51,0:00:41.70,Default,,0000,0000,0000,,Tôi sẽ trở thành thư kí riêng của ông.
Dialogue: 0,0:00:42.74,0:00:45.47,Default,,0000,0000,0000,,Chúng ta sẽ bàn chi tiết tại văn phòng nhé, Yumi?
Dialogue: 0,0:00:46.18,0:00:48.24,Default,,0000,0000,0000,,-Sao, à vâng.\N-Cha!
Dialogue: 0,0:00:50.95,0:00:53.44,Default,,0000,0000,0000,,Chuyện gì đây, Taketo?
Dialogue: 0,0:00:55.16,0:00:56.18,Default,,0000,0000,0000,,Yumi....
Dialogue: 0,0:00:56.26,0:00:58.38,Default,,0000,0000,0000,,Thường xuyên cúp học để tới đây ngủ ngày?
Dialogue: 0,0:00:58.46,0:01:02.29,Default,,0000,0000,0000,,-Tôi biết làm gì với cậu đây?\N-Cô muốn gì nào? Hãy để tôi yên.
Dialogue: 0,0:01:02.80,0:01:04.42,Default,,0000,0000,0000,,Tùy cậu thôi.
Dialogue: 0,0:01:04.53,0:01:07.09,Default,,0000,0000,0000,,Cô định ra ngoài với bộ dạng này sao?
Dialogue: 0,0:01:07.17,0:01:09.50,Default,,0000,0000,0000,,Tôi phải đến công ty, đó là công việc. CÔNG VIỆC, cậu hiểu ko.
Dialogue: 0,0:01:09.57,0:01:13.70,Default,,0000,0000,0000,,Cô ăn mặc như vậy mà ko thấy ngượng sao?
Dialogue: 0,0:01:14.44,0:01:17.88,Default,,0000,0000,0000,,Hay là cô luôn sẵn sàng làm theo mọi yêu cầu của ông già tôi?
Dialogue: 0,0:01:22.35,0:01:25.68,Default,,0000,0000,0000,,Cậu còn lâu lắm mới hiểu được chuyện đó.
Dialogue: 0,0:01:37.63,0:01:39.22,Default,,0000,0000,0000,,Xong rồi.
Dialogue: 0,0:01:40.83,0:01:43.80,Default,,0000,0000,0000,,Mình đã giặt kĩ chúng, vậy mà chúng vẫn có mùi khác nhau nhỉ.
Dialogue: 0,0:01:43.94,0:01:44.77,Default,,0000,0000,0000,,Oh?
Dialogue: 0,0:01:45.94,0:01:47.34,Default,,0000,0000,0000,,Ai đó?
Dialogue: 0,0:01:52.75,0:01:55.37,Default,,0000,0000,0000,,Mình sẽ cho ông già thấy ông ta đã sai lầm thế nào khi đánh giá thấp mình.
Dialogue: 0,0:01:55.82,0:01:59.95,Default,,0000,0000,0000,,Với người đàn bà này, mình sẽ có nhiều trò thú vị hơn ông ta nhiều.
Dialogue: 0,0:02:43.16,0:02:45.76,Default,,0000,0000,0000,,Immoral Sisters\N~Đôi quả ngọt chín sớm~
Dialogue: 0,0:02:47.83,0:02:50.17,Default,,0000,0000,0000,,Đêm đầu tiên\N"Trinh nữ sa ngã"
Dialogue: 0,0:02:50.47,0:02:52.17,Default,,0000,0000,0000,,Phần A:\N"Yukie, người vợ dâm đãng"
Dialogue: 0,0:03:04.68,0:03:07.41,Default,,0000,0000,0000,,Xin chào, đây là nhà riêng của Kitazawa.
Dialogue: 0,0:03:08.62,0:03:10.28,Default,,0000,0000,0000,,A chào anh, ông xã.
Dialogue: 0,0:03:11.32,0:03:13.35,Default,,0000,0000,0000,,Vâng, là em đây, Yukie đây.
Dialogue: 0,0:03:18.20,0:03:22.10,Default,,0000,0000,0000,,Ko, ko có gì cả.
Dialogue: 0,0:03:25.61,0:03:28.20,Default,,0000,0000,0000,,Em chỉ hơi....
Dialogue: 0,0:03:29.71,0:03:31.34,Default,,0000,0000,0000,,...khó chịu trong người.
Dialogue: 0,0:03:45.46,0:03:47.05,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:03:49.23,0:03:50.70,Default,,0000,0000,0000,,Thật mà.
Dialogue: 0,0:03:51.30,0:03:53.06,Default,,0000,0000,0000,,Em vẫn ổn
Dialogue: 0,0:04:02.11,0:04:03.84,Default,,0000,0000,0000,,Các con....
Dialogue: 0,0:04:04.48,0:04:06.45,Default,,0000,0000,0000,,Hai chị em nó vẫn khỏe.
Dialogue: 0,0:04:26.43,0:04:27.83,Default,,0000,0000,0000,,Tomoko.
Dialogue: 0,0:04:36.91,0:04:38.78,Default,,0000,0000,0000,,Chơi tốt lắm, Rumi.
Dialogue: 0,0:04:38.95,0:04:40.44,Default,,0000,0000,0000,,Cậu cũng thế.
Dialogue: 0,0:04:40.91,0:04:43.82,Default,,0000,0000,0000,,-Cố lên nhé.\N-Cậu cũng vậy. Hẹn gặp lại sau nhé.
Dialogue: 0,0:04:43.88,0:04:45.85,Default,,0000,0000,0000,,-Tomoko.\N-Rumi.
Dialogue: 0,0:04:46.35,0:04:51.12,Default,,0000,0000,0000,,Tomoko, em ko nên giữ tâm lý rụt rè khi cầm vợt. Đó là một thói quen xấu.
Dialogue: 0,0:04:51.42,0:04:53.55,Default,,0000,0000,0000,,Em xin lỗi.
Dialogue: 0,0:04:54.39,0:04:58.85,Default,,0000,0000,0000,,Chị phải ghé qua câu lạc bộ mĩ thuật một lúc. Em về nhà một mình nhé.
Dialogue: 0,0:05:04.37,0:05:05.56,Default,,0000,0000,0000,,Ouch!
Dialogue: 0,0:05:05.64,0:05:08.07,Default,,0000,0000,0000,,Này cô. Cô ko sao chứ?
Dialogue: 0,0:05:10.51,0:05:12.60,Default,,0000,0000,0000,,Màu hồng hợp với cô lắm đó.
Dialogue: 0,0:05:13.15,0:05:15.44,Default,,0000,0000,0000,,Có đau lắm ko?
Dialogue: 0,0:05:29.53,0:05:32.69,Default,,0000,0000,0000,,Vâng. Anh đừng lo.
Dialogue: 0,0:05:52.85,0:05:54.18,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:06:11.67,0:06:12.93,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:06:30.96,0:06:34.52,Default,,0000,0000,0000,,Ko, ko, anh nghe nhầm đấy. Thật đấy ạ, ko có gì đâu.
Dialogue: 0,0:06:37.30,0:06:39.16,Default,,0000,0000,0000,,Ko có gì đâu. Thật mà.
Dialogue: 0,0:06:42.34,0:06:44.96,Default,,0000,0000,0000,,Vâng, tất nhiên rồi.
Dialogue: 0,0:06:46.64,0:06:50.41,Default,,0000,0000,0000,,Em cũng yêu anh, ông xã.
Dialogue: 0,0:06:58.62,0:07:00.14,Default,,0000,0000,0000,,¥200,000?
Dialogue: 0,0:07:00.35,0:07:03.52,Default,,0000,0000,0000,,Ko thể nào! Đây chỉ là một vụ đụng xe thôi mà!
Dialogue: 0,0:07:05.06,0:07:07.79,Default,,0000,0000,0000,,Cô nên xem kĩ tờ hóa đơn này đi.
Dialogue: 0,0:07:10.30,0:07:15.13,Default,,0000,0000,0000,,Dù cho đó là toàn bộ chi phí,\Nhai người cũng có lỗi trong vụ tai nạn đó mà.
Dialogue: 0,0:07:15.92,0:07:19.03,Default,,0000,0000,0000,,Thỏa thuận thế này nhé.  Cô trả 70%\Nvà chúng tôi trả 30%.
Dialogue: 0,0:07:19.61,0:07:21.90,Default,,0000,0000,0000,,Có nghĩa là cô nợ chúng tôi ¥140,000.
Dialogue: 0,0:07:22.54,0:07:24.87,Default,,0000,0000,0000,,Làm sao tôi có thể trả một khoản tiền lớn như vậy được!
Dialogue: 0,0:07:24.94,0:07:27.88,Default,,0000,0000,0000,,Nếu đưa vụ này ra tòa thì người bất lợi chắc chắn sẽ là cô.
Dialogue: 0,0:07:44.16,0:07:47.29,Default,,0000,0000,0000,,Hãy để tôi về nhà và...
Dialogue: 0,0:07:47.37,0:07:50.06,Default,,0000,0000,0000,,...Tôi sẽ bàn chuyện này với chồng tôi.
Dialogue: 0,0:07:58.75,0:08:01.27,Default,,0000,0000,0000,,Xin lỗi. Tôi cảm thấy hơi chóng mặt.
Dialogue: 0,0:08:02.05,0:08:05.88,Default,,0000,0000,0000,,Trước khi ra về, cảm phiền cô kí vào đây được ko?
Dialogue: 0,0:08:06.32,0:08:07.79,Default,,0000,0000,0000,,Đó là gì?
Dialogue: 0,0:08:07.85,0:08:10.41,Default,,0000,0000,0000,,Chúng tôi cần chữ kí chấp thuận của cô trong bản giao kèo này.
Dialogue: 0,0:08:10.49,0:08:12.89,Default,,0000,0000,0000,,Nhưng chúng ta đâu có thỏa thuận gì về chuyện tiền bạc....
Dialogue: 0,0:08:13.46,0:08:18.40,Default,,0000,0000,0000,,Tôi ko quan tâm, chỉ biết cô đã yêu cầu chúng tôi ko đưa việc này ra tòa.
Dialogue: 0,0:08:20.30,0:08:21.66,Default,,0000,0000,0000,,Tôi xin lỗi.
Dialogue: 0,0:08:22.24,0:08:23.60,Default,,0000,0000,0000,,Cám ơn.
Dialogue: 0,0:08:23.94,0:08:28.00,Default,,0000,0000,0000,,Chồng cô là giám đốc chi nhánh của một công ty thương mại đúng ko?
Dialogue: 0,0:08:28.07,0:08:29.20,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:08:29.31,0:08:32.57,Default,,0000,0000,0000,,¥200,000 có vẻ như hơi quá sức đối với cô trong thời hạn nửa năm.
Dialogue: 0,0:08:33.85,0:08:35.97,Default,,0000,0000,0000,,Cô đang nói về chuyện gì thế?
Dialogue: 0,0:08:36.05,0:08:39.75,Default,,0000,0000,0000,,Với ngoại hình của mình, cô có thể làm ra tiền theo rất nhiều cách.
Dialogue: 0,0:08:40.12,0:08:44.82,Default,,0000,0000,0000,,Cuộc sống của cô hẳn đã thay đổi một cách chóng mặt chỉ với một mảnh giấy nhỉ.
Dialogue: 0,0:08:45.79,0:08:48.35,Default,,0000,0000,0000,,Ý cô là.... Làm ơn hãy chỉ tôi cách đó.
Dialogue: 0,0:08:49.90,0:08:51.89,Default,,0000,0000,0000,,Chuyện gì thế này?
Dialogue: 0,0:08:59.74,0:09:01.40,Default,,0000,0000,0000,,Ngực tôi....
Dialogue: 0,0:09:02.68,0:09:05.51,Default,,0000,0000,0000,,Tôi sẽ cởi nó ra giúp cô nhé, xong ngay đây thôi.
Dialogue: 0,0:09:06.41,0:09:07.88,Default,,0000,0000,0000,,Ko, ko.
Dialogue: 0,0:09:26.93,0:09:29.73,Default,,0000,0000,0000,,Tất cả những điều chúng ta cần làm bây giờ chính là thắt chặt mối quan hệ này.
Dialogue: 0,0:09:30.30,0:09:32.20,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:09:32.51,0:09:33.90,Default,,0000,0000,0000,,Đừng.
Dialogue: 0,0:09:47.19,0:09:49.92,Default,,0000,0000,0000,,Ko. Ai giúp tôi với.
Dialogue: 0,0:09:52.16,0:09:53.52,Default,,0000,0000,0000,,Ông xã.
Dialogue: 0,0:09:55.49,0:09:58.05,Default,,0000,0000,0000,,Ko, ko, ko, ko.
Dialogue: 0,0:09:58.76,0:10:01.16,Default,,0000,0000,0000,,Tha lỗi cho em, ông xã.
Dialogue: 0,0:10:04.00,0:10:05.60,Default,,0000,0000,0000,,Đừng khóc nữa.
Dialogue: 0,0:10:05.67,0:10:08.70,Default,,0000,0000,0000,,Tôi sẽ từ từ thưởng thức cô.\NCô sẽ kiếm đủ ¥200,000 sớm thôi mà.
Dialogue: 0,0:10:12.48,0:10:14.84,Default,,0000,0000,0000,,Ông già, ông cứ chờ xem.
Dialogue: 0,0:10:16.52,0:10:18.11,Default,,0000,0000,0000,,Con về rồi đây.
Dialogue: 0,0:10:18.52,0:10:20.49,Default,,0000,0000,0000,,Hình như mẹ ko có ở nhà?
Dialogue: 0,0:10:21.39,0:10:24.62,Default,,0000,0000,0000,,Mẹ thậm chí ko thèm khóa cửa nữa. Thật là bất cẩn.
Dialogue: 0,0:10:27.73,0:10:31.25,Default,,0000,0000,0000,,Tomoko, em ko nên giữ tâm lý rụt rè khi cầm vợt. Đó là một thói quen xấu.
Dialogue: 0,0:10:32.37,0:10:37.23,Default,,0000,0000,0000,,Mình ko hề muốn như thế. Mình ko xinh đẹp bằng chị ấy.
Dialogue: 0,0:10:37.87,0:10:40.40,Default,,0000,0000,0000,,Chị ấy cũng có một thói quen xấu.
Dialogue: 0,0:11:41.93,0:11:44.16,Default,,0000,0000,0000,,Chắc hẳn chuyện đó rất tuyệt.
Dialogue: 0,0:11:57.65,0:11:59.45,Default,,0000,0000,0000,,Lạ quá.
Dialogue: 0,0:12:33.92,0:12:35.01,Default,,0000,0000,0000,,Gì thế kia?
Dialogue: 0,0:12:37.99,0:12:39.48,Default,,0000,0000,0000,,Mẹ?
Dialogue: 0,0:12:39.89,0:12:42.19,Default,,0000,0000,0000,,Mẹ, mẹ có nhà ko vậy?
Dialogue: 0,0:12:48.33,0:12:49.82,Default,,0000,0000,0000,,Mẹ?
Dialogue: 0,0:12:50.77,0:12:52.17,Default,,0000,0000,0000,,Tomoko....
Dialogue: 0,0:12:52.24,0:12:55.30,Default,,0000,0000,0000,,Sao thế này?\NCó chuyện gì xảy ra vậy mẹ?
Dialogue: 0,0:12:59.75,0:13:02.41,Default,,0000,0000,0000,,-Ko có gì đâu.\N-Thật chứ ạ?
Dialogue: 0,0:13:03.08,0:13:05.88,Default,,0000,0000,0000,,Chuyện này ko liên quan gì tới con đâu.
Dialogue: 0,0:13:06.72,0:13:08.21,Default,,0000,0000,0000,,Mẹ?
Dialogue: 0,0:13:13.23,0:13:14.69,Default,,0000,0000,0000,,Immoral Sisters
Dialogue: 0,0:13:18.80,0:13:19.82,Default,,0000,0000,0000,,Immoral Sisters
Dialogue: 0,0:13:24.77,0:13:25.70,Default,,0000,0000,0000,,Ouch.
Dialogue: 0,0:13:25.77,0:13:27.86,Default,,0000,0000,0000,,Này cô. Cô ko sao chứ?
Dialogue: 0,0:13:29.61,0:13:30.73,Default,,0000,0000,0000,,Tomoko?
Dialogue: 0,0:13:31.71,0:13:34.54,Default,,0000,0000,0000,,Con bé sao thế nhỉ?
Dialogue: 0,0:13:38.82,0:13:40.65,Default,,0000,0000,0000,,Phần B:\NRumi, một trinh nữ sa nga
Dialogue: 0,0:13:45.19,0:13:47.09,Default,,0000,0000,0000,,Sao có thể như vậy?
Dialogue: 0,0:13:51.70,0:13:53.36,Default,,0000,0000,0000,,Chuyện gì thế?
Dialogue: 0,0:13:56.07,0:13:58.16,Default,,0000,0000,0000,,Tôi đang thay đồ! Phiền cô ra ngoài cho!
Dialogue: 0,0:13:59.54,0:14:02.56,Default,,0000,0000,0000,,-Cô là Rumi Kitazawa, phải ko?\N-Sao cơ?
Dialogue: 0,0:14:02.64,0:14:05.04,Default,,0000,0000,0000,,Cô ko nên lớn tiếng như vậy.
Dialogue: 0,0:14:07.25,0:14:12.45,Default,,0000,0000,0000,,Chắc hẳn cô sẽ ko muốn chuyện này bị người khác chú ý đâu.
Dialogue: 0,0:14:20.86,0:14:22.85,Default,,0000,0000,0000,,Tôi sẽ tự giới thiệu.
Dialogue: 0,0:14:22.93,0:14:26.09,Default,,0000,0000,0000,,Tôi là Yumi Okamoto, thư kí riêng của hiệu trưởng học viện Machikawa.
Dialogue: 0,0:14:26.17,0:14:28.16,Default,,0000,0000,0000,,Thứ này là giả!
Dialogue: 0,0:14:28.73,0:14:30.60,Default,,0000,0000,0000,,Thật ko may mắn....
Dialogue: 0,0:14:37.14,0:14:39.48,Default,,0000,0000,0000,,Ông chủ của tôi đang đợi cô đó.
Dialogue: 0,0:14:39.68,0:14:41.81,Default,,0000,0000,0000,,Cô sẽ đi với tôi chứ?
Dialogue: 0,0:14:43.62,0:14:47.95,Default,,0000,0000,0000,,Tốt hơn là cô nên sửa soạn nhanh đi. Ông chủ của tôi là người rất thiếu kiên nhẫn.
Dialogue: 0,0:14:54.36,0:14:56.89,Default,,0000,0000,0000,,Ông ấy thích cô trong bộ dạng này hơn.
Dialogue: 0,0:15:04.50,0:15:07.03,Default,,0000,0000,0000,,Chúng ta phải xin phép trước khi lên tầng thượng.
Dialogue: 0,0:15:07.34,0:15:11.47,Default,,0000,0000,0000,,Vậy hãy nói với mọi người tập trung trên tầng thượng sau nửa giờ nữa.
Dialogue: 0,0:15:11.81,0:15:14.34,Default,,0000,0000,0000,,Nhưng chúng ta đều ko mang đồng phục mà.
Dialogue: 0,0:15:14.75,0:15:17.94,Default,,0000,0000,0000,,Chúng ta có thể thay đồ trên tầng thượng. Ko có ai nhìn chúng ta đâu mà lo.
Dialogue: 0,0:15:18.02,0:15:20.85,Default,,0000,0000,0000,,Mà ngày nào chúng ta chả bị người khác ngắm nhìn.
Dialogue: 0,0:15:21.42,0:15:22.89,Default,,0000,0000,0000,,Thật á?
Dialogue: 0,0:15:24.19,0:15:27.49,Default,,0000,0000,0000,,Khi cô sợ hãi như vậy, cô sẽ rất dễ nứng lên.
Dialogue: 0,0:15:27.86,0:15:29.69,Default,,0000,0000,0000,,Nếu họ trông thấy tôi thế này....
Dialogue: 0,0:15:30.10,0:15:34.00,Default,,0000,0000,0000,,Tôi nghe nói cô rất nổi tiếng trong trường.
Dialogue: 0,0:15:34.33,0:15:38.13,Default,,0000,0000,0000,,Điểm số cao, thành tích thể thao tuyệt vời, luôn hòa nhã với mọi người, cô hẳn là một học sinh rất gương mẫu.
Dialogue: 0,0:15:39.17,0:15:42.61,Default,,0000,0000,0000,,Các nữ sinh ở đây hình như rất thích các vụ Scandal.
Dialogue: 0,0:15:48.98,0:15:52.44,Default,,0000,0000,0000,,Ta đã quá già để đứng đợi ở đây trong thời tiết lạnh thế này.
Dialogue: 0,0:15:54.15,0:15:57.32,Default,,0000,0000,0000,,Cô sẽ sưởi ấm giúp ta chứ.
Dialogue: 0,0:16:00.26,0:16:03.89,Default,,0000,0000,0000,,Cô còn chờ gì nữa hả?\NTa đang nói với cô đấy, Rumi!
Dialogue: 0,0:16:04.56,0:16:08.56,Default,,0000,0000,0000,,Tôi đã bảo cô rồi. Ông chủ của tôi là người thiếu kiên nhẫn.
Dialogue: 0,0:16:08.77,0:16:10.86,Default,,0000,0000,0000,,Tôi chư hề làm điều này bao giờ.
Dialogue: 0,0:16:15.17,0:16:16.40,Default,,0000,0000,0000,,Cô làm gì thế?
Dialogue: 0,0:16:16.48,0:16:19.50,Default,,0000,0000,0000,,Tôi sẽ tải bức ảnh của mẹ cô lên e-mails....
Dialogue: 0,0:16:20.28,0:16:21.47,Default,,0000,0000,0000,,Làm ơn đừng....
Dialogue: 0,0:16:21.55,0:16:24.07,Default,,0000,0000,0000,,Cô hỏi nhầm người rồi đó.
Dialogue: 0,0:16:24.15,0:16:26.98,Default,,0000,0000,0000,,Trong tất cả mọi điều, ta ghét nhất là phải chờ đợi thế này!
Dialogue: 0,0:16:27.05,0:16:29.28,Default,,0000,0000,0000,,Ông muốn tôi làm gì?
Dialogue: 0,0:16:29.36,0:16:34.42,Default,,0000,0000,0000,,Cô ko biết thật sao? Cô đủ lớn để nghe về chuyện đó rồi mà.
Dialogue: 0,0:16:45.00,0:16:46.47,Default,,0000,0000,0000,,Thật ngang bướng.
Dialogue: 0,0:16:50.64,0:16:53.27,Default,,0000,0000,0000,,Đừng đánh tôi! Tôi sẽ làm theo yêu cầu của ông!
Dialogue: 0,0:16:54.45,0:16:58.35,Default,,0000,0000,0000,,-Tôi sẽ làm chuyện đó cho ông.\N-Ông muốn tôi bú nó phải ko?
Dialogue: 0,0:16:58.82,0:17:00.58,Default,,0000,0000,0000,,Tôi sẽ bú nó cho ông.
Dialogue: 0,0:17:03.32,0:17:06.02,Default,,0000,0000,0000,,Lần này, dùng lưỡi của cô cho khéo vào đấy.
Dialogue: 0,0:17:08.93,0:17:11.33,Default,,0000,0000,0000,,Yumi, gửi e-mail đi!
Dialogue: 0,0:17:11.46,0:17:12.79,Default,,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:17:47.03,0:17:51.16,Default,,0000,0000,0000,,Có vẻ như cô ta ko phải học sinh gương mẫu như tôi đã tưởng.
Dialogue: 0,0:17:52.37,0:17:53.90,Default,,0000,0000,0000,,Đúng thế.
Dialogue: 0,0:17:54.07,0:17:57.01,Default,,0000,0000,0000,,Cô ta ko hề ngây thơ như cô trước kia.
Dialogue: 0,0:18:02.38,0:18:04.11,Default,,0000,0000,0000,,Chào buổi sáng, ông chủ.
Dialogue: 0,0:18:05.65,0:18:08.45,Default,,0000,0000,0000,,Cha cậu đã chấp thuận cho tôi vào làm trong công ty ông ấy.
Dialogue: 0,0:18:08.52,0:18:11.61,Default,,0000,0000,0000,,Tôi sẽ trở thành thư kí riêng của ông.
Dialogue: 0,0:18:14.36,0:18:16.59,Default,,0000,0000,0000,,Chúng ta sẽ bàn chi tiết tại văn phòng nhé, Yumi?
Dialogue: 0,0:18:16.66,0:18:18.15,Default,,0000,0000,0000,,Sao, à vâng.
Dialogue: 0,0:18:24.00,0:18:27.37,Default,,0000,0000,0000,,Cũng lâu rồi cháu chưa tới nơi này.
Dialogue: 0,0:18:27.61,0:18:29.63,Default,,0000,0000,0000,,Cháu sẽ nấu thứ gì đó cho bác.
Dialogue: 0,0:18:30.18,0:18:33.30,Default,,0000,0000,0000,,Cháu nấu ăn cừ lắm đấy.
Dialogue: 0,0:18:33.95,0:18:38.94,Default,,0000,0000,0000,,Nhân tiện, Bác này, làm ơn hãy nói với Taketo. Cậu ấy....
Dialogue: 0,0:18:40.45,0:18:44.18,Default,,0000,0000,0000,,Có vẻ như cháu đã trưởng thành hơn rất nhiều so với tuổi của mình.
Dialogue: 0,0:18:44.66,0:18:46.56,Default,,0000,0000,0000,,Bác làm gì thế?
Dialogue: 0,0:18:46.63,0:18:49.56,Default,,0000,0000,0000,,Mặc một chiếc hợp mốt bây giờ chẳng tốt hơn cái này sao?
Dialogue: 0,0:18:50.16,0:18:51.46,Default,,0000,0000,0000,,Làm ơn đừng.
Dialogue: 0,0:18:51.53,0:18:53.52,Default,,0000,0000,0000,,Ta biết cháu ko phải đứa ngốc.
Dialogue: 0,0:18:54.17,0:18:56.36,Default,,0000,0000,0000,,Cha mẹ cháu đã qua đời từ khi cháu còn rất nhỏ...
Dialogue: 0,0:18:56.60,0:19:00.90,Default,,0000,0000,0000,,...và cháu đã ko còn ai để có thể nương tựa.
Dialogue: 0,0:19:01.44,0:19:05.34,Default,,0000,0000,0000,,Cháu thừa biết là ta ko giàu lòng thương người tới mức đó.
Dialogue: 0,0:19:06.48,0:19:09.78,Default,,0000,0000,0000,,Sao bác lại đột ngột nói tới chuyện đó ở đây?
Dialogue: 0,0:19:10.95,0:19:15.22,Default,,0000,0000,0000,,Chắc hẳn bác đang đùa phải ko. Suýt chút nữa, cháu đã tin điều đó là thật đó.
Dialogue: 0,0:19:15.29,0:19:17.69,Default,,0000,0000,0000,,Bác là bác ruột của cháu mà. Sao bác có thể....
Dialogue: 0,0:19:17.76,0:19:20.42,Default,,0000,0000,0000,,Ta nghe đủ rồi đấy!\NCháu bỏ tay ra khỏi chỗ đó ngay đi!
Dialogue: 0,0:19:20.49,0:19:23.12,Default,,0000,0000,0000,,Sao cơ? Cháu chỉ ko muốn bác trông thấy....
Dialogue: 0,0:19:23.33,0:19:25.66,Default,,0000,0000,0000,,Ta nói hãy đễ cho ta coi chỗ đó của cháu!
Dialogue: 0,0:19:25.73,0:19:29.39,Default,,0000,0000,0000,,Đây chính là bài phỏng vấn của cháu!\NGiang chân ra nào!
Dialogue: 0,0:19:33.87,0:19:35.84,Default,,0000,0000,0000,,Nhấc cao lên!
Dialogue: 0,0:19:36.38,0:19:38.64,Default,,0000,0000,0000,,Ko. Ko.
Dialogue: 0,0:19:39.11,0:19:42.24,Default,,0000,0000,0000,,Nhớ lại xem khi còn nhỏ cháu đã ngồi tè như thế nào?
Dialogue: 0,0:19:42.31,0:19:43.41,Default,,0000,0000,0000,,Làm lại cho ta xem nào!
Dialogue: 0,0:19:54.16,0:19:56.56,Default,,0000,0000,0000,,Thằng con ta có vẻ thích điều đó.
Dialogue: 0,0:19:57.70,0:20:00.63,Default,,0000,0000,0000,,Ta cũng thế.
Dialogue: 0,0:20:04.10,0:20:07.54,Default,,0000,0000,0000,,Nếu cháu ko phải trinh nữ thì đơn xin việc của cháu đã bị ta từ chối thẳng thừng rồi.
Dialogue: 0,0:20:12.01,0:20:12.98,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:20:14.28,0:20:15.64,Default,,0000,0000,0000,,Đừng.
Dialogue: 0,0:20:18.95,0:20:20.18,Default,,0000,0000,0000,,Ko!
Dialogue: 0,0:20:24.26,0:20:26.78,Default,,0000,0000,0000,,Làm ơn dừng lại đi.
Dialogue: 0,0:20:29.13,0:20:31.43,Default,,0000,0000,0000,,Ko, ko, ko.
Dialogue: 0,0:20:36.44,0:20:40.34,Default,,0000,0000,0000,,Ko, ko. Dừng lại đi.
Dialogue: 0,0:20:43.08,0:20:44.30,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:20:49.75,0:20:51.15,Default,,0000,0000,0000,,Ko, ko!
Dialogue: 0,0:21:31.62,0:21:33.39,Default,,0000,0000,0000,,Nuốt hết chúng đi.
Dialogue: 0,0:21:37.93,0:21:39.83,Default,,0000,0000,0000,,Khi cô đã xong chuyện đó...
Dialogue: 0,0:21:39.90,0:21:43.63,Default,,0000,0000,0000,,...điều cô cần làm tiếp theo là hãy thủ dâm cho ông ấy xem.
Dialogue: 0,0:21:48.67,0:21:50.73,Default,,0000,0000,0000,,Đừng động vào lưng của tôi.
Dialogue: 0,0:21:50.81,0:21:54.87,Default,,0000,0000,0000,,Cô thật dâm đãng,\NChắc hẳn cô đã ngày ngày làm việc đó.
Dialogue: 0,0:21:55.21,0:21:57.18,Default,,0000,0000,0000,,Làm ơn, hãy tha cho tôi.
Dialogue: 0,0:21:58.35,0:22:00.01,Default,,0000,0000,0000,,Cô đã làm chuyện đó như thế nào?
Dialogue: 0,0:22:00.42,0:22:02.51,Default,,0000,0000,0000,,Tôi làm ở ngay chỗ đó.
Dialogue: 0,0:22:06.49,0:22:09.72,Default,,0000,0000,0000,,Như thế này. Ngay khi tôi còn mặc quần lót.
Dialogue: 0,0:22:10.20,0:22:12.10,Default,,0000,0000,0000,,Cô thích như vậy, phải ko?
Dialogue: 0,0:22:13.37,0:22:15.86,Default,,0000,0000,0000,,Điều đó giống như là tôi đang bị ai đó trông thấy.
Dialogue: 0,0:22:15.93,0:22:20.50,Default,,0000,0000,0000,,Tôi đã nghĩ tới nhức khi họ nhìn tôi ở câu lạc bộ ngoài khóa, và điều đó thật tuyệt.
Dialogue: 0,0:22:22.87,0:22:25.50,Default,,0000,0000,0000,,Ko, đừng làm vậy. Ko.
Dialogue: 0,0:22:29.25,0:22:30.81,Default,,0000,0000,0000,,Cô làm ta ngạc nhiên đấy.
Dialogue: 0,0:22:30.88,0:22:33.48,Default,,0000,0000,0000,,Ta ko ngờ cô lại dâm đến vậy.
Dialogue: 0,0:22:33.65,0:22:36.82,Default,,0000,0000,0000,,Cô ta đã ra một lần, nhưng nhìn xem này.
Dialogue: 0,0:24:12.82,0:24:15.45,Default,,0000,0000,0000,,Quần lót của cô ướt đẫm rồi.
Dialogue: 0,0:24:18.76,0:24:19.78,Default,,0000,0000,0000,,Giờ thì....
Dialogue: 0,0:24:20.13,0:24:22.22,Default,,0000,0000,0000,,Ko, ko phải ở đó....
Dialogue: 0,0:25:01.17,0:25:02.66,Default,,0000,0000,0000,,Đau quá.
Dialogue: 0,0:25:06.51,0:25:07.73,Default,,0000,0000,0000,,Ko.
Dialogue: 0,0:25:08.47,0:25:09.80,Default,,0000,0000,0000,,Dừng lại đi.
Dialogue: 0,0:25:38.04,0:25:40.00,Default,,0000,0000,0000,,Đau quá, đau quá.
Dialogue: 0,0:25:40.64,0:25:44.34,Default,,0000,0000,0000,,Oops. Quần lót của cô vấy máu rồi kìa.
Dialogue: 0,0:25:44.74,0:25:48.54,Default,,0000,0000,0000,,Nghe đây, Rumi. Cô phải mặc nó khi quay về nhà.
Dialogue: 0,0:25:49.71,0:25:52.55,Default,,0000,0000,0000,,Trời. Tầng thượng rộng quá.
Dialogue: 0,0:25:52.85,0:25:54.68,Default,,0000,0000,0000,,Oh, ko. Chúng ta sẽ thay đồ ở đây sao?
Dialogue: 0,0:25:54.75,0:25:57.24,Default,,0000,0000,0000,,Cậu ngốc thế! Chúng ta phải thay đồ trước khi tới chỗ đó.
Dialogue: 0,0:25:57.59,0:26:00.32,Default,,0000,0000,0000,,Nhỡ có ai đó nhìn trộm chúng ta thì sao?
Dialogue: 0,0:26:00.89,0:26:05.16,Default,,0000,0000,0000,,Thôi nào, cậu thay đồ nhanh lên đi!\NChúng ta sắp muộn rồi đó.
Dialogue: 0,0:26:05.23,0:26:09.43,Default,,0000,0000,0000,,Tớ đang nghĩ ko biết nên chọn cặp quần lót nào cho lần tới.
Dialogue: 0,0:26:09.50,0:26:13.63,Default,,0000,0000,0000,,Hôm nay chúng ta sẽ cổ động cho các tuyển thủ quần vợt, thế nên tớ thấy mình giống quả banh quá.
Dialogue: 0,0:26:13.71,0:26:18.17,Default,,0000,0000,0000,,-Quần lót của Nana nhìn hở hang chưa kìa.\N-Đừng lố lăng như thế chứ!
Dialogue: 0,0:26:18.24,0:26:21.04,Default,,0000,0000,0000,,Nana mặc chiếc màu đó là để cổ động cho bạn cô ấy.
Dialogue: 0,0:26:21.11,0:26:22.37,Default,,0000,0000,0000,,Tớ biết rồi!
Dialogue: 0,0:26:22.45,0:26:26.11,Default,,0000,0000,0000,,Nana chỉ mặc quần lót màu đó khi cổ động cho đội tennis.
Dialogue: 0,0:26:26.18,0:26:30.99,Default,,0000,0000,0000,,Tuyển thủ của chúng ta Rumi Kitazawa cũng thích màu đó lắm đấy.
Dialogue: 0,0:26:31.39,0:26:34.83,Default,,0000,0000,0000,,Sao cậu biết được Rumi thích màu đó?
Dialogue: 0,0:26:34.89,0:26:37.79,Default,,0000,0000,0000,,Tớ luôn đứng xem các buổi tập của họ.
Dialogue: 0,0:26:37.84,0:26:40.80,Default,,0000,0000,0000,,Chắc Rumi sẽ chuyển sang mặc màu đỏ ngay thôi, nhỉ?
