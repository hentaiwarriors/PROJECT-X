Dialogue: 0,0:00:00.00,0:00:05.00,Script,,0000,0000,0000,,
Dialogue: 0,0:00:11.57,0:00:13.48,Script,,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:00:13.77,0:00:16.09,Script,,0000,0000,0000,,Quả dưa leo to nóng của anh đang co giật này...
Dialogue: 0,0:00:16.90,0:00:21.88,Script,,0000,0000,0000,,Sướng thật... hãy phóng hết ra ngực em đi.
Dialogue: 0,0:00:36.34,0:00:39.76,Script,,0000,0000,0000,,Ra rồi... nước dưa của anh ra nhiều quá!
Dialogue: 0,0:00:47.52,0:00:50.07,Script,,0000,0000,0000,,Giờ thì làm cho em sướng đi anh.
Dialogue: 0,0:00:53.82,0:00:59.75,Script,,0000,0000,0000,,Nó đang vào, nó đang... củ dưa to lớn của anh đang vào!
Dialogue: 0,0:01:03.32,0:01:10.29,Script,,0000,0000,0000,,Sướng, sướng quá!... Bên trong của em sướng thật!
Dialogue: 0,0:01:14.14,0:01:20.43,Script,,0000,0000,0000,,Ôi! Em ra...!
Dialogue: 0,0:01:23.49,0:01:25.75,Script,,0000,0000,0000,,Let's fall in love, like in ero-manga!
Dialogue: 1,0:01:30.30,0:01:34.04,Ep Title,,0000,0000,0000,,{\fad(791,447)\pos(296,466)}[Let's Fall in Love the Ero-Manga]
Dialogue: 0,0:01:30.30,0:01:34.04,Ep Title,,0000,0000,0000,,{\fad(791,447)\bord5\1a&HFF&\3c&HBDA7EF&\pos(296,466)}[Let's Fall in Love the Ero-Manga]
Dialogue: 0,0:01:35.39,0:01:36.14,Script,,0000,0000,0000,,Ngon!
Dialogue: 0,0:01:36.14,0:01:37.04,Script,,0000,0000,0000,,Ngon cái đầu cậu ấy!
Dialogue: 0,0:01:37.88,0:01:39.54,Script,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:01:39.54,0:01:41.87,Script,,0000,0000,0000,,Mizuki, từ khi nào...
Dialogue: 0,0:01:41.87,0:01:43.86,Script,,0000,0000,0000,,"Từ khi nào" cái con khỉ!
Dialogue: 0,0:01:43.86,0:01:47.10,Script,,0000,0000,0000,,Daisuke, cậu bùng học làm cái trò gì thế hả?
Dialogue: 0,0:01:47.10,0:01:50.97,Script,,0000,0000,0000,,Trò gì là trò gì? Không thấy tớ đang vẽ Truyện "H" à.
Dialogue: 0,0:01:50.97,0:01:52.28,Script,,0000,0000,0000,,Cho tớ xem làm quái gì!
Dialogue: 0,0:01:52.28,0:01:55.23,Script,,0000,0000,0000,,Ế? Tớ đã vẽ chúng bằng cả mồ hôi và xương máu đấy!
Dialogue: 0,0:01:55.23,0:01:58.84,Script,,0000,0000,0000,,Thiệt tình, tớ biết cậu làm việc vất vả.
Dialogue: 0,0:01:58.84,0:02:02.72,Script,,0000,0000,0000,,Cậu cũng biết sắp thi tốt nghiệp rồi đấy,\N thế cậu định đặt việc này trên cả việc học hả?
Dialogue: 0,0:02:02.72,0:02:06.01,Script,,0000,0000,0000,,Thì tháng này bài kiểm tra cả đống ra rồi lại còn mấy việc linh tinh.
Dialogue: 0,0:02:06.01,0:02:08.67,Script,,0000,0000,0000,,khi tớ biết được thì lại đến hạn nộp cái này...
Dialogue: 0,0:02:08.67,0:02:10.94,Script,,0000,0000,0000,,Con trai mà lại than vãn cái nỗi gì!
Dialogue: 0,0:02:10.94,0:02:13.33,Script,,0000,0000,0000,,Đã không lập kế hoạc ngay từ đầu giờ còn kêu ca à?
Dialogue: 0,0:02:13.33,0:02:16.45,Script,,0000,0000,0000,,Đừng có nóng trong người như thế.\N Để tớ lấy chút trà cho cậu giải nhiệt nhé.
Dialogue: 0,0:02:16.45,0:02:19.28,Script,,0000,0000,0000,,Cứ bình tình mà uống- Aaahhh!
Dialogue: 0,0:02:30.58,0:02:32.50,Script,,0000,0000,0000,,Chờ đã! Mizuki, là hiểu lầm thôi mà!
Dialogue: 0,0:02:32.50,0:02:35.28,Script,,0000,0000,0000,,Tớ làm việc suốt cả đêm qua nên chưa ăn uống gì.
Dialogue: 0,0:02:35.28,0:02:36.67,Script,,0000,0000,0000,,Giờ chân tớ nhão cả ra rồi đây...
Dialogue: 0,0:02:36.67,0:02:37.60,Script,,0000,0000,0000,,Đồ ngốc!
Dialogue: 0,0:02:38.10,0:02:43.96,Script,,0000,0000,0000,,{\i1}Chán thật, đã không chịu suy nghĩ gì rồi lại còn làm việc quá sức.{\i0}
Dialogue: 0,0:02:44.45,0:02:45.86,Script,,0000,0000,0000,,Ngồi yên đây, đừng có vác cái mông đi đâu đấy.
Dialogue: 0,0:02:47.97,0:02:49.89,Script,,0000,0000,0000,,Ố, Itadakimasu!
Dialogue: 0,0:02:50.24,0:02:51.74,Script,,0000,0000,0000,,Cảm ơn về bữa ăn!
Dialogue: 0,0:02:51.74,0:02:58.64,Script,,0000,0000,0000,,Đ-Đừng có hiểu lầm! Tớ thấy đói nên làm luôn phần cậu đấy.
Dialogue: 0,0:02:58.89,0:03:03.38,Script,,0000,0000,0000,,Đúng vậy, nấu cà ri nhiều thì món cơm cà ri này mới ngon được.
Dialogue: 0,0:03:03.95,0:03:08.52,Script,,0000,0000,0000,,{\i1}Tên đầu heo đó vẽ Truyện "H" mà không khen nổi 1 câu mình là Tsudere nữa.{\i0}
Dialogue: 0,0:03:08.52,0:03:11.43,Script,,0000,0000,0000,,Vậy, tớ quay lại làm việc tiếp đây.
Dialogue: 0,0:03:11.89,0:03:13.41,Script,,0000,0000,0000,,À, để tớ giúp cho.
Dialogue: 0,0:03:13.87,0:03:14.58,Script,,0000,0000,0000,,Hả, được chứ?
Dialogue: 0,0:03:14.58,0:03:18.30,Script,,0000,0000,0000,,Cậu mà càng xong sớm thì sẽ càng đến trường sớm đúng không?
Dialogue: 0,0:03:18.30,0:03:19.93,Script,,0000,0000,0000,,Vậy, dâng hiến cho cậu hết đấy.
Dialogue: 0,0:03:22.78,0:03:28.00,Script,,0000,0000,0000,,{\i1}Úi, đây là Truyện "H" do Daisuke vẽ...{\i0}
Dialogue: 0,0:03:28.40,0:03:34.44,Script,,0000,0000,0000,,{\i1}Nguyên cái đống này... nhìn kiểu nào đi nữa...\Nthì hình mẫu nhân vật cũng giống mình,{\i0}
Dialogue: 0,0:03:34.44,0:03:37.79,Script,,0000,0000,0000,,{\i1}không hiểu hắn ta nghĩ gì khi vẽ nó nữa?{\i0}
Dialogue: 0,0:03:37.79,0:03:41.82,Script,,0000,0000,0000,,{\i1}Đừng bảo là hắn nghĩ mình giống thế này...{\i0}
Dialogue: 0,0:03:43.12,0:03:45.24,Script,,0000,0000,0000,,D-Daisuke này...
Dialogue: 0,0:03:45.24,0:03:46.80,Script,,0000,0000,0000,,À...
Dialogue: 0,0:03:46.80,0:03:48.26,Script,,0000,0000,0000,,Hmm? Có việc gì?
Dialogue: 0,0:03:48.26,0:03:51.65,Script,,0000,0000,0000,,Nữ nhân vật chính... có phải là...
Dialogue: 0,0:03:51.65,0:03:53.81,Script,,0000,0000,0000,,Ah, vậy chắc cậu cũng đoán ra?
Dialogue: 0,0:03:53.81,0:03:55.75,Script,,0000,0000,0000,,Ừh, tớ vẽ cô ấy theo hình mẫu của cậu.
Dialogue: 0,0:03:56.35,0:03:58.66,Script,,0000,0000,0000,,Bởi tớ thấy cậu hoàn hảo với hình tượng nữ nhân vật chính.
Dialogue: 0,0:03:58.66,0:04:01.46,Script,,0000,0000,0000,,Có người bạn thơ ấu như cậu quả là giúp tớ rất nhiều.
Dialogue: 0,0:04:02.17,0:04:05.32,Script,,0000,0000,0000,,Tớ không cần phải nghĩ về hình mẫu nhân vật nữa.
Dialogue: 0,0:04:05.32,0:04:09.29,Script,,0000,0000,0000,,Khi tớ đã quen với gương mặt cậu, thì quả là dễ vẽ hen!
Dialogue: 0,0:04:12.28,0:04:14.33,Script,,0000,0000,0000,,Hóa ra vậy...
Dialogue: 0,0:04:15.02,0:04:16.74,Script,,0000,0000,0000,,Tớ đã tô xong phần cậu bảo.
Dialogue: 0,0:04:16.74,0:04:17.90,Script,,0000,0000,0000,,Vậy tiếp theo là gì...
Dialogue: 0,0:04:17.90,0:04:20.11,Script,,0000,0000,0000,,Hmm, đợi tớ chút.
Dialogue: 0,0:04:20.64,0:04:21.83,Script,,0000,0000,0000,,Có việc gì sao?
Dialogue: 0,0:04:21.83,0:04:24.54,Script,,0000,0000,0000,,Chà, cái này không biết phải vẽ sao đây?
Dialogue: 0,0:04:25.02,0:04:27.87,Script,,0000,0000,0000,,Mizuki, cậu làm mẫu cho tớ vẽ được chứ?
Dialogue: 0,0:04:28.18,0:04:30.29,Script,,0000,0000,0000,,Cũng được, nhưng 36 kiểu thì cậu muốn kiểu nào?
Dialogue: 0,0:04:30.69,0:04:31.39,Script,,0000,0000,0000,,Kiểu thủ dâm.
Dialogue: 0,0:04:32.96,0:04:35.41,Script,,0000,0000,0000,,Thằng kia... chưa uống thuốc hay sao vậy?
Dialogue: 0,0:04:35.41,0:04:38.62,Script,,0000,0000,0000,,Chọc gan bà thì bà cho máu của mày chảy... ra mà tô nhé.
Dialogue: 0,0:04:38.62,0:04:41.97,Script,,0000,0000,0000,,Nếu cậu không thích thì thôi vậy nhé?
Dialogue: 0,0:04:42.94,0:04:46.46,Script,,0000,0000,0000,,Nếu thế thì để tớ tìm trong cái đống "film tư liệu" kia cũng được.
Dialogue: 0,0:04:47.13,0:04:49.56,Script,,0000,0000,0000,,{\i1}Cái gì vậy nè trời? Thật không tin nổi!{\i0}
Dialogue: 0,0:04:49.82,0:04:52.95,Script,,0000,0000,0000,,{\i1}Bình thường có ai lại đi xem phim con heo ngay trước mũi bạn gái cơ chứ?{\i0}
Dialogue: 0,0:04:50.09,0:04:52.95,Script,,0000,0000,0000,,Chà không biết nên chọn "tư liệu" nào nhỉ?
Dialogue: 0,0:04:53.33,0:04:56.87,Script,,0000,0000,0000,,{\i1}Đã vậy còn trai đơn gái chiếc ở cùng 1 phòng nữa chứ.{\i0}
Dialogue: 0,0:04:56.87,0:05:00.89,Script,,0000,0000,0000,,{\i1}Bộ cậu ta nghĩ mình như một công cụ để thuận tiện cho công việc thôi à?{\i0}
Dialogue: 0,0:05:00.89,0:05:03.00,Script,,0000,0000,0000,,Yup, xem cái này vậy.
Dialogue: 0,0:05:05.60,0:05:06.48,Script,,0000,0000,0000,,Mizuki?
Dialogue: 0,0:05:06.48,0:05:07.45,Script,,0000,0000,0000,,Tui sẽ làm.
Dialogue: 0,0:05:08.42,0:05:14.46,Script,,0000,0000,0000,,Cái tư thế... "lên đồng" ấy... mình sẽ làm mẫu cho.
Dialogue: 0,0:05:15.30,0:05:16.18,Script,,0000,0000,0000,,Thật không đó?
Dialogue: 0,0:05:16.18,0:05:17.20,Script,,0000,0000,0000,,Ừhm.
Dialogue: 0,0:05:20.58,0:05:24.11,Script,,0000,0000,0000,,V-Vậy... cậu lên giường đi.
Dialogue: 0,0:05:29.98,0:05:33.00,Script,,0000,0000,0000,,Vậy được chưa?
Dialogue: 0,0:05:33.00,0:05:35.95,Script,,0000,0000,0000,,À ờ... bước đầu chỉ để lấy ý tưởng thôi.
Dialogue: 0,0:05:49.53,0:05:51.07,Script,,0000,0000,0000,,Này, Mizuki...
Dialogue: 0,0:05:51.23,0:05:52.93,Script,,0000,0000,0000,,S-sao?
Dialogue: 0,0:05:52.93,0:05:56.36,Script,,0000,0000,0000,,Cậu luôn mặc quần áo trong khi thủ dâm sao?
Dialogue: 0,0:06:10.56,0:06:14.29,Script,,0000,0000,0000,,{\i1}Sao mình lại làm việc này chứ... xấu hổ quá!{\i0}
Dialogue: 0,0:06:35.18,0:06:38.09,Script,,0000,0000,0000,,Đ-Đồ ngốc! Đừng có nhìn gần như thế!
Dialogue: 0,0:06:38.09,0:06:41.40,Script,,0000,0000,0000,,Nhưng tớ cần xem cho rõ mấy đường cong của cái hang này...
Dialogue: 0,0:06:42.04,0:06:48.01,Script,,0000,0000,0000,,{\i1}Nếu cậu ấy cứ lại gần như thế...\N mùi của mình... mình vẫn còn chưa tắm nữa!{\i0}
Dialogue: 0,0:06:48.01,0:06:50.74,Script,,0000,0000,0000,,{\i1}Daisuke chắc chắn sẽ nghĩ mình nặng mùi mất.{\i0}
Dialogue: 0,0:06:51.96,0:06:53.96,Script,,0000,0000,0000,,Mizuki... cậu ướt thật đấy.
Dialogue: 0,0:06:54.33,0:06:55.88,Script,,0000,0000,0000,,Đừng có nhìn! Đồ ngốc!
Dialogue: 0,0:06:56.12,0:06:59.47,Script,,0000,0000,0000,,{\i1}Mình có thể cảm thấy hơi thở của Daisuke... người mình nóng ran lên cả rồi...{\i0}
Dialogue: 0,0:06:59.47,0:07:02.69,Script,,0000,0000,0000,,Nếu tớ không nhìn thì cậu làm mẫu để làm gì chứ?
Dialogue: 0,0:07:03.46,0:07:08.07,Script,,0000,0000,0000,,{\i1}Daisuke đang nhìn mình! Ngượng chết mất thôi...{\i0}
Dialogue: 0,0:07:08.35,0:07:12.69,Script,,0000,0000,0000,,Chỗ đó của cậu... tớ thấy xuyên qua cả quần lót này.
Dialogue: 0,0:07:16.38,0:07:17.94,Script,,0000,0000,0000,,Đừng... có chạm vào đó!
Dialogue: 0,0:07:19.07,0:07:22.17,Script,,0000,0000,0000,,Đừng có thọc vào mà! Đừng...!
Dialogue: 0,0:07:22.46,0:07:26.27,Script,,0000,0000,0000,,Nước dưa nhiều như thế này, chắc ngon lắm nhỉ?
Dialogue: 0,0:07:27.20,0:07:27.75,Script,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:07:31.21,0:07:33.18,Script,,0000,0000,0000,,Không, dừng lại đi! Đừng!
Dialogue: 0,0:07:34.30,0:07:38.24,Script,,0000,0000,0000,,Nó dơ lắm... đ-đừng liếm nó mà.
Dialogue: 0,0:07:40.16,0:07:41.42,Script,,0000,0000,0000,,Không!
Dialogue: 0,0:07:42.76,0:07:45.40,Script,,0000,0000,0000,,Nó phát ra âm thanh lạ quá...
Dialogue: 0,0:07:48.27,0:07:52.79,Script,,0000,0000,0000,,Daisuke đang liếm chỗ đó của mình... mình sướng quá!
Dialogue: 0,0:07:54.16,0:07:59.29,Script,,0000,0000,0000,,KHÔNGGGGGGG!!!
Dialogue: 0,0:08:09.01,0:08:11.98,Script,,0000,0000,0000,,Mình ra rồi.
Dialogue: 0,0:08:11.98,0:08:16.06,Script,,0000,0000,0000,,Y như trong manga... mình bắn ra cả rồi.
Dialogue: 0,0:08:16.06,0:08:17.85,Script,,0000,0000,0000,,Mizuki, cậu ra rồi này!
Dialogue: 0,0:08:19.19,0:08:22.30,Script,,0000,0000,0000,,Nè, cảm giác khi cậu bắn nước ra vì cực khoái ra sao vậy?
Dialogue: 0,0:08:24.40,0:08:25.42,Script,,0000,0000,0000,,Mizuki?
Dialogue: 0,0:08:27.95,0:08:32.96,Script,,0000,0000,0000,,Để cho công bằng... tui sẽ khám "chỗ đó" của ông.
Dialogue: 0,0:08:35.62,0:08:40.43,Script,,0000,0000,0000,,{\i1}Wow, vậy quả dưa leo của con trai là thế này... trông cứng quá...{\i0}
Dialogue: 0,0:08:41.22,0:08:45.46,Script,,0000,0000,0000,,{\i1}Nó thật cứng và ấm, lại còn co giật y như trong manga nữa.{\i0}
Dialogue: 0,0:08:46.25,0:08:50.22,Script,,0000,0000,0000,,Mà này, Mizuki-san, tớ phải làm việc tiếp đây...
Dialogue: 0,0:09:08.55,0:09:12.79,Script,,0000,0000,0000,,Bạn thời thơ ấu đang mút cây kẹo của mình. Y như là...
Dialogue: 0,0:09:15.93,0:09:19.70,Script,,0000,0000,0000,,Trong manga người ta làm thế này phải không nhỉ?
Dialogue: 0,0:09:23.80,0:09:28.54,Script,,0000,0000,0000,,Nếu mình không nhầm... thì phải dùng ngực đẩy lên rồi đẩy xuống thế này.
Dialogue: 0,0:09:30.95,0:09:32.77,Script,,0000,0000,0000,,Cậu thấy thế nào, Daisuke?
Dialogue: 0,0:09:32.77,0:09:34.99,Script,,0000,0000,0000,,Cô bạn Tsundere thời thơ ấu của cậu\N đang làm tình với cậu bằng ngực đây này!
Dialogue: 0,0:09:34.99,0:09:38.91,Script,,0000,0000,0000,,Phê lắm, sướng lắm! Ế, Tsudere?
Dialogue: 0,0:09:38.91,0:09:41.55,Script,,0000,0000,0000,,À tớ không có nói gì hết!
Dialogue: 0,0:09:42.18,0:09:44.38,Script,,0000,0000,0000,,A? Có nước gì đó chảy ra nè.
Dialogue: 0,0:09:45.30,0:09:48.97,Script,,0000,0000,0000,,Aaaaa... Mitsuki. Đừng... liếm nó.
Dialogue: 0,0:09:50.04,0:09:51.81,Script,,0000,0000,0000,,Ái... cái gì vậy nè?
Dialogue: 0,0:09:51.81,0:09:54.22,Script,,0000,0000,0000,,Đây là... xuất tinh à?
Dialogue: 0,0:09:55.29,0:09:57.31,Script,,0000,0000,0000,,Vậy là Daisuke cũng xuất ra rồi.
Dialogue: 0,0:09:58.81,0:10:01.00,Script,,0000,0000,0000,,Lại còn bắn cả vào mặt tớ nữa.
Dialogue: 0,0:10:01.71,0:10:03.05,Script,,0000,0000,0000,,Huề cả làng rồi nhé.
Dialogue: 0,0:10:04.51,0:10:05.24,Script,,0000,0000,0000,,Chưa đâu.
Dialogue: 0,0:10:06.62,0:10:10.35,Script,,0000,0000,0000,,Mizuki, tớ muốn... làm luôn với cậu cho trót.
Dialogue: 0,0:10:10.75,0:10:13.52,Script,,0000,0000,0000,,Làm cho trót? Tức là s..e..x hả?
Dialogue: 0,0:10:14.00,0:10:14.53,Script,,0000,0000,0000,,Ừhm.
Dialogue: 0,0:10:15.03,0:10:17.15,Script,,0000,0000,0000,,Cho cuốn manga cậu vẽ sao?
Dialogue: 0,0:10:17.15,0:10:17.86,Script,,0000,0000,0000,,Không.
Dialogue: 0,0:10:19.46,0:10:23.42,Script,,0000,0000,0000,,Tớ... yêu cậu, Mizuki. Thế nên...
Dialogue: 0,0:10:25.42,0:10:26.32,Script,,0000,0000,0000,,Được rồi.
Dialogue: 0,0:10:29.62,0:10:32.07,Script,,0000,0000,0000,,Nếu cậu đã muốn, thì nè... cho cậu hết luôn đó~!
Dialogue: 0,0:10:40.28,0:10:41.84,Script,,0000,0000,0000,,Tớ đút nó vào nhé, Mizuki.
Dialogue: 0,0:10:45.76,0:10:51.46,Script,,0000,0000,0000,,Cái của Daisuke... đang vào... nó đang vào trong mình!
Dialogue: 0,0:10:55.16,0:11:01.72,Script,,0000,0000,0000,,Sướng quá! Tụi mình làm... giống hệt trong manga ấy...
Dialogue: 0,0:11:02.18,0:11:03.94,Script,,0000,0000,0000,,Mizuki, cậu có đau không?
Dialogue: 0,0:11:05.02,0:11:06.63,Script,,0000,0000,0000,,Tớ không sao...!
Dialogue: 0,0:11:06.93,0:11:08.54,Script,,0000,0000,0000,,Vậy tớ sẽ di chuyển nhé!
Dialogue: 0,0:11:08.55,0:11:09.44,Script,,0000,0000,0000,,Uhm!
Dialogue: 0,0:11:28.13,0:11:29.33,Script,,0000,0000,0000,,{\i1}Ái, cảm giác này là sao?{\i0}
Dialogue: 0,0:11:29.33,0:11:31.01,Script,,0000,0000,0000,,{\i1}Dù đây là lần đầu của mình,{\i0}
Dialogue: 0,0:11:31.01,0:11:32.96,Script,,0000,0000,0000,,{\i1}nhưng sướng quá, mình thấy sướng quá!{\i0}
Dialogue: 0,0:11:34.73,0:11:36.97,Script,,0000,0000,0000,,Nó thật ướt và khít...
Dialogue: 0,0:11:36.97,0:11:38.90,Script,,0000,0000,0000,,Bên trong của Mizuki thật tuyệt!
Dialogue: 0,0:11:40.37,0:11:47.45,Script,,0000,0000,0000,,Daisuke... nữa đi, sướng quá. Tớ sướng điên lên mất!
Dialogue: 0,0:11:49.83,0:11:52.67,Script,,0000,0000,0000,,Ngực tớ cũng đang rất tê này...
Dialogue: 0,0:12:07.34,0:12:09.40,Script,,0000,0000,0000,,Daisuke, tớ yêu cậu!
Dialogue: 0,0:12:09.40,0:12:10.75,Script,,0000,0000,0000,,Tớ cũng vậy, Mizuki!
Dialogue: 0,0:12:14.51,0:12:18.47,Script,,0000,0000,0000,,{\i1}Ahhh, thật không ngờ sau khi mình mất đi đời con gái rồi thì mới đến nụ hôn đầu.{\i0}
Dialogue: 0,0:12:22.20,0:12:24.86,Script,,0000,0000,0000,,Tư thế này, xấu hổ lắm!
Dialogue: 0,0:12:29.90,0:12:32.22,Script,,0000,0000,0000,,Mizuki, Mizuki!
Dialogue: 0,0:12:35.01,0:12:39.48,Script,,0000,0000,0000,,Daisuke, tớ đang ra, tớ đang ra!
Dialogue: 0,0:12:40.87,0:12:41.83,Script,,0000,0000,0000,,Tớ... cũng đang ra.
Dialogue: 0,0:12:43.56,0:12:44.85,Script,,0000,0000,0000,,Tớ ra mất thôi!
Dialogue: 0,0:12:49.24,0:12:55.85,Script,,0000,0000,0000,,{\fad(0,700}Nó đang ra! Đang ra! Đang ra! Áaaaá!
Dialogue: 0,0:13:04.69,0:13:05.43,Script,,0000,0000,0000,,Mizuki.
Dialogue: 0,0:13:08.20,0:13:09.49,Script,,0000,0000,0000,,Daisuke.
Dialogue: 0,0:13:11.66,0:13:13.93,Script,,0000,0000,0000,,Mình yêu cậu, yêu cậu nhiều lắm!
Dialogue: 0,0:13:20.28,0:13:23.13,Script,,0000,0000,0000,,Nè, mình thật sự phải làm thế này sao?
Dialogue: 0,0:13:23.46,0:13:27.80,Script,,0000,0000,0000,,Xin cậu đấy, Mizuki. Nốt cái này là xong trang cuối rồi.
Dialogue: 0,0:13:30.23,0:13:33.38,Script,,0000,0000,0000,,Geez, đành vậy đâu còn cách nào khác.
Dialogue: 0,0:13:43.85,0:13:46.32,Script,,0000,0000,0000,,Ra rồi! Ra là vậy sao?
Dialogue: 0,0:13:49.89,0:13:54.92,Script,,0000,0000,0000,,{\i1}Không, Daisuke đang nhìn mình đi tiểu!{\i0}
Dialogue: 0,0:13:57.38,0:13:58.78,Script,,0000,0000,0000,,{\i1}Mình...{\i0}
Dialogue: 0,0:14:08.14,0:14:09.67,Script,,0000,0000,0000,,Cảm ơn cậu nhé, Mizuki.
Dialogue: 0,0:14:09.67,0:14:12.28,Script,,0000,0000,0000,,Giờ tớ có thể hoàn thành nó đúng hạn ngày mai rồi.
Dialogue: 0,0:14:17.36,0:14:20.18,Script,,0000,0000,0000,,Hắn ta chỉ biết mỗi vẽ Ero-manga mà thôi.
Dialogue: 0,0:14:20.91,0:14:23.53,Script,,0000,0000,0000,,Đáng lẽ phải cảm ơn mình mới đúng.
Dialogue: 0,0:14:25.58,0:14:27.96,Script,,0000,0000,0000,,Nè, thế cái này cậu định làm gì?
Dialogue: 0,0:14:27.96,0:14:29.97,Script,,0000,0000,0000,,À, cứ đổ nó vào toilet.
Dialogue: 0,0:14:31.17,0:14:33.94,Script,,0000,0000,0000,,Mình lại phải đi dọn sao?
Dialogue: 0,0:14:33.94,0:14:38.78,Script,,0000,0000,0000,,Thôi, còn hơn là để Daisule ngửi thứ này.
Dialogue: 0,0:14:40.17,0:14:41.10,Script,,0000,0000,0000,,Á!
