Dialogue: 0,0:24:55.86,0:25:02.70,OP vietnamese,,0000,0000,0000,,nước mắt của hạnh phúc\N bay theo cơn gió
Dialogue: 0,0:25:02.70,0:25:10.41,OP vietnamese,,0000,0000,0000,,một hương thơm dịu nhẹ,\N cùng với hơi ấm thân thuộc
Dialogue: 0,0:25:10.41,0:25:13.81,OP vietnamese,,0000,0000,0000,,trong thế giới bị giới hạn này,
Dialogue: 0,0:25:13.81,0:25:16.98,OP vietnamese,,0000,0000,0000,,vượt ra ngoài bầu trời
Dialogue: 0,0:25:17.18,0:25:20.25,OP vietnamese,,0000,0000,0000,,cặp mắt ẩn chứa cảm xúc của bạn
Dialogue: 0,0:25:20.25,0:25:25.95,OP vietnamese,,0000,0000,0000,,đang tìm kiếm điều gì?
Dialogue: 0,0:25:29.29,0:25:32.66,OP vietnamese,,0000,0000,0000,,một ngôi sao vô hình, rụt rè
Dialogue: 0,0:25:32.66,0:25:36.16,OP vietnamese,,0000,0000,0000,,du hành đến thiên đường
Dialogue: 0,0:25:36.16,0:25:39.56,OP vietnamese,,0000,0000,0000,,giống như không nghe,\N không nhận thấy
Dialogue: 0,0:25:39.56,0:25:42.96,OP vietnamese,,0000,0000,0000,,nhẹ nhàng lung linh
Dialogue: 0,0:25:43.03,0:25:49.87,OP vietnamese,,0000,0000,0000,,ngay lúc này bạn hưởng thụ\N quá khứ đầy màu sắc của mình
Dialogue: 0,0:25:49.87,0:25:53.28,OP vietnamese,,0000,0000,0000,,ban mai lung linh,\N màn đêm lạnh lẽo,
Dialogue: 0,0:25:53.28,0:25:55.51,OP vietnamese,,0000,0000,0000,,biến chúng thành đá quý
Dialogue: 0,0:25:55.84,0:26:02.48,OP vietnamese,,0000,0000,0000,,tha thứ cho giọt nước mắt trống rỗng
Dialogue: 0,0:26:02.69,0:26:10.39,OP vietnamese,,0000,0000,0000,,bằng cách ấy,\N tôi muốn được bảo vệ
Dialogue: 0,0:26:10.39,0:26:13.83,OP vietnamese,,0000,0000,0000,,như một mảnh kim
Dialogue: 0,0:26:13.83,0:26:16.99,OP vietnamese,,0000,0000,0000,,xuyên qua thời gian
Dialogue: 0,0:26:17.20,0:26:20.27,OP vietnamese,,0000,0000,0000,,bình minh sẽ sớm đến
Dialogue: 0,0:26:20.27,0:26:25.94,OP vietnamese,,0000,0000,0000,,với tất cả chúng ta
