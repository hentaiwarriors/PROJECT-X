Dialogue: 0,0:00:02.92,0:00:08.44,Default,,0000,0000,0000,,Project này thuộc về website : Baka-sub.net
Dialogue: 0,0:00:17.25,0:00:18.21,Default,,0000,0000,0000,,{\i1}Orifushi{\i0}
Dialogue: 0,0:00:18.92,0:00:20.71,Haruomi,,0000,0000,0000,,Cuối cùng thì cũng tới nơi....
Dialogue: 0,0:00:22.18,0:00:24.70,Default,,0000,0000,0000,,{\i1}Tên tôi là Shiki Haruomi.{\i0}
Dialogue: 0,0:00:25.32,0:00:31.30,Default,,0000,0000,0000,,{\i1}Nhiều chuyện đã xảy đến với tôi và hiện giờ tôi chuyển tới sống cùng với gia đình Orifushi, những người bà con của tôi.{\i0}
Dialogue: 0,0:00:34.24,0:00:37.47,Haruomi,,0000,0000,0000,,Huh, mình đã gọi cho họ trước khi tới đây....
Dialogue: 0,0:00:40.45,0:00:43.41,Haruomi,,0000,0000,0000,,Có lẽ họ đang ở ngoài vườn....?
Dialogue: 0,0:00:51.68,0:00:54.71,Haruomi,,0000,0000,0000,,Xin chào~, anh là Shiki đây!
Dialogue: 0,0:01:00.56,0:01:02.53,Haruomi,,0000,0000,0000,,Có ai ở nhà ko?
Dialogue: 0,0:01:05.47,0:01:08.57,Haruomi,,0000,0000,0000,,Natsumi-san~, chị có ở trong đó ko vậy?
Dialogue: 0,0:01:11.20,0:01:13.83,Akina,,0000,0000,0000,,Xin lỗi~ Mafuyu, hình như chị quên chưa khóa cửa?
Dialogue: 0,0:01:17.25,0:01:18.41,Akina,,0000,0000,0000,,Mafuyu....?
Dialogue: 0,0:01:34.44,0:01:40.05,Title,,0000,0000,0000,,{\b1}Kanojo x Kanojo x Kanojo{\b0}
Dialogue: 0,0:01:41.84,0:01:44.94,Akina,,0000,0000,0000,,Haru-kun, đây là phòng của cậu.
Dialogue: 0,0:01:45.56,0:01:49.24,Haruomi,,0000,0000,0000,,Đây chẳng phải là phòng của bác trai sao? Cậu định nhường nó cho mình thật ư?
Dialogue: 0,0:01:49.24,0:01:53.92,Akina,,0000,0000,0000,,Cha mẹ mình qua đời cũng lâu rồi mà....
Dialogue: 0,0:01:55.18,0:01:56.22,Haruomi,,0000,0000,0000,,Mình hiểu rồi....
Dialogue: 0,0:01:56.22,0:02:00.18,Akina,,0000,0000,0000,,Bây giờ ba chị em mình đang cố hết sức mình để trông nom cửa hiệu của gia đình.
Dialogue: 0,0:02:00.94,0:02:02.45,Haruomi,,0000,0000,0000,,Natsumi-san~
Dialogue: 0,0:02:03.19,0:02:05.93,Akina,,0000,0000,0000,,Haru-kun...., cậu đã trở nên cao lớn thế này rồi sao.
Dialogue: 0,0:02:05.93,0:02:09.53,Haruomi,,0000,0000,0000,,Uhm, nếu so với lần cuối chúng mình gặp nhau trước đây thì đúng là....
Dialogue: 0,0:02:09.53,0:02:10.80,Haruomi,,0000,0000,0000,,Cậu cũng đã....
Dialogue: 0,0:02:12.37,0:02:14.40,Akina,,0000,0000,0000,,Trở nên xinh đẹp hơn phải ko nào?
Dialogue: 0,0:02:14.40,0:02:19.10,Default,,0000,0000,0000,,{\i1}Thành thật mà nói, Akina-chan quả thật trông dễ thương hơn lúc trước rất nhiều....{\i0}
Dialogue: 0,0:02:19.10,0:02:21.38,Akina,,0000,0000,0000,,Cậu đỏ mặt kìa....
Dialogue: 0,0:02:22.30,0:02:25.80,Default,,0000,0000,0000,,{\i1}Lạ quá, sao tự nhiên tim mình lại đập thình thịch như vậy....{\i0}
Dialogue: 0,0:02:27.58,0:02:30.58,Akina,,0000,0000,0000,,Một lần nữa, chào mừng Haru-kun đến sống với bọn mình nào!
Dialogue: 0,0:02:31.03,0:02:33.08,Mafuyu,,0000,0000,0000,,Anh tới trễ đó, Haruomi.
Dialogue: 0,0:02:33.37,0:02:36.21,Natsumi,,0000,0000,0000,,Lâu lắm rồi mới được gặp em đó, Haru-chan.
Dialogue: 0,0:02:36.52,0:02:40.13,Haruomi,,0000,0000,0000,,Uhm~, rất mong nhận được sự chỉ bảo của mọi người.
Dialogue: 0,0:02:40.13,0:02:43.26,Default,,0000,0000,0000,,{\i1}Chúc cả nhà ngon miệng~\NChị biết mà....\NTuyệt quá~{\i0}
Dialogue: 0,0:02:44.69,0:02:49.54,Natsumi,,0000,0000,0000,,Haru-chan, ăn mạnh nữa lên nào.\NĐàn ông con trai phải thế chứ, phải ko nào?
Dialogue: 0,0:02:49.54,0:02:51.43,Haruomi,,0000,0000,0000,,Ah, vâng. Các món ăn chị làm rất ngon.
Dialogue: 0,0:02:52.27,0:02:57.50,Haruomi,,0000,0000,0000,,À này~, Natsumi-san? Về cách chị gọi em ấy, nó khiến em hơi xấu hổ....
Dialogue: 0,0:02:57.51,0:03:00.52,Natsumi,,0000,0000,0000,,Eh, sao thế?
Dialogue: 0,0:03:00.52,0:03:02.67,Haruomi,,0000,0000,0000,,Bây giờ em đã lớn rồi mà~
Dialogue: 0,0:03:02.67,0:03:07.13,Natsumi,,0000,0000,0000,,Nhưng mà~, đó là tại vì Haru-chan trông thật dễ thương.
Dialogue: 0,0:03:07.13,0:03:09.69,Haruomi,,0000,0000,0000,,Chị khen thế làm em thấy hơi ngượng đó....
Dialogue: 0,0:03:09.69,0:03:13.81,Natsumi,,0000,0000,0000,,Nhưng mà~, chị thấy gọi Haru-chan như vậy dễ nghe hơn.
Dialogue: 0,0:03:13.81,0:03:15.61,Haruomi,,0000,0000,0000,,Nhưng mà "chan"  thì hơi....
Dialogue: 0,0:03:15.61,0:03:19.40,Natsumi,,0000,0000,0000,,Nhưng mà~, Haru-chan nghe chẳng phải thân mật hơn sao.
Dialogue: 0,0:03:21.36,0:03:23.47,Natsumi,,0000,0000,0000,,Nhưng mà~~~~
Dialogue: 0,0:03:23.47,0:03:25.40,Haruomi,,0000,0000,0000,,Thôi thôi, em....hiểu rồi.
Dialogue: 0,0:03:25.41,0:03:30.81,Natsumi,,0000,0000,0000,,Em ko phản đối nữa à? Chị hãy còn 12 lí do nữa đấy~
Dialogue: 0,0:03:32.82,0:03:35.31,Natsumi,,0000,0000,0000,,Haru-chan, nói "ah~" đi nào
Dialogue: 0,0:03:35.31,0:03:37.56,Haruomi,,0000,0000,0000,,Ko, Natsumi-san, cái này thì....
Dialogue: 0,0:03:37.56,0:03:41.08,Akina,,0000,0000,0000,,Thôi nào, Onee-chan. Chị đút cho Haru-kun nhiều quá đấy.
Dialogue: 0,0:03:41.08,0:03:45.31,Natsumi,,0000,0000,0000,,Eh, Akina-chan. Em đang ghen tị đấy hả?
Dialogue: 0,0:03:46.22,0:03:50.48,Natsumi,,0000,0000,0000,,Haru-chan, em thích ai nhất trong ba người ở đây?
Dialogue: 0,0:03:50.48,0:03:52.09,Haruomi,,0000,0000,0000,,Eh? Ko, cái này....
Dialogue: 0,0:03:53.74,0:03:56.70,Natsumi,,0000,0000,0000,,Chị hiểu rồi, hãy xem chị biểu diễn thuật tiên tri đây!
Dialogue: 0,0:04:05.31,0:04:09.28,Akina,,0000,0000,0000,,Biết ngay mà, là Akina-chan.
Dialogue: 0,0:04:09.28,0:04:10.89,Akina,,0000,0000,0000,,Eh~, thôi nào chị....
Dialogue: 0,0:04:11.78,0:04:17.72,Natsumi,,0000,0000,0000,,Có thể chị lớn tuổi hơn em, nhưng chị sẽ đối xử với em rất nhẹ nhàng và....
Dialogue: 0,0:04:18.22,0:04:19.05,Akina,,0000,0000,0000,,Khoan đã nào....
Dialogue: 0,0:04:19.25,0:04:22.09,Mafuyu,,0000,0000,0000,,Chị Natsumi chỉ được mỗi cái khoản ngực bự.
Dialogue: 0,0:04:22.59,0:04:24.99,Natsumi,,0000,0000,0000,,Ko phải như thế mà....
Dialogue: 0,0:04:24.99,0:04:25.99,Akina,,0000,0000,0000,,Chị này!
Dialogue: 0,0:04:27.05,0:04:30.73,Default,,0000,0000,0000,,{\i1}Sống chung một mái nhà với cả ba chị em xinh đẹp thế này,{\i0}
Dialogue: 0,0:04:31.09,0:04:33.44,Default,,0000,0000,0000,,{\i1}Mình tốt số thật đấy.{\i0}
Dialogue: 0,0:04:35.50,0:04:40.63,Default,,0000,0000,0000,,{\i1}Sau nhiều biến cố đã trải qua, mình ko nghĩ là sẽ có được cơ may tuyệt vời như vậy.{\i0}
Dialogue: 0,0:04:41.19,0:04:45.46,Default,,0000,0000,0000,,{\i1}Và mình cũng chưa bao giờ hình dung ra cuộc sống của mình hiện giờ....{\i0}
Dialogue: 0,0:04:48.45,0:04:51.43,Haruomi,,0000,0000,0000,,Có lẽ mình ko nên nghĩ mãi về điều này nữa, nhức đầu quá....
Dialogue: 0,0:04:52.87,0:04:53.52,Other,,0000,0000,0000,,Này cậu?
Dialogue: 0,0:04:56.13,0:05:00.83,Other,,0000,0000,0000,,Mình đã nghe chuyện của cậu từ Akina. Mình là Byakudan Midori,
Dialogue: 0,0:05:00.93,0:05:04.18,Other,,0000,0000,0000,,Đây là bạn của mình, Himenohara Suzuraha.
Dialogue: 0,0:05:04.18,0:05:07.02,Other,,0000,0000,0000,,Mình và Suzu là bạn thân của Akina tại câu lạc bộ ngoại khóa.
Dialogue: 0,0:05:08.05,0:05:11.30,Other,,0000,0000,0000,,Nếu cậu chưa rõ điều gì, cứ nói cho bọn mình biết nhé.
Dialogue: 0,0:05:11.50,0:05:13.61,Haruomi,,0000,0000,0000,,Cám ơn....hai bạn.
Dialogue: 0,0:05:13.71,0:05:16.13,Akina,,0000,0000,0000,,Haru-kun, ăn trưa cùng với mình nhé.
Dialogue: 0,0:05:16.13,0:05:20.03,Akina,,0000,0000,0000,,Đây là lần đầu tiên mình chuẩn bị bữa trưa cho cậu đó.
Dialogue: 0,0:05:20.32,0:05:22.80,Other,,0000,0000,0000,,Như vậy là, cậu đã trở nên gần gũi với cô ấy.
Dialogue: 0,0:05:22.80,0:05:27.42,Other,,0000,0000,0000,,Nhưng, cậu cũng phải thật cẩn thận đó. Akina rất nổi tiếng trong trường.
Dialogue: 0,0:05:27.45,0:05:29.85,Other,,0000,0000,0000,,Vì thế cậu phải lưu ý, ko nên tự tạo cho mình nhiều tình địch tại đây.
Dialogue: 0,0:05:40.11,0:05:43.27,Default,,0000,0000,0000,,{\i1}Mình đã hứa sẽ gặp Akina sau giờ hoạt động ngoại khóa....{\i0}
Dialogue: 0,0:05:43.27,0:05:45.25,Default,,0000,0000,0000,,{\i1}Bể bơi ở đâu nhỉ?{\i0}
Dialogue: 0,0:05:46.16,0:05:49.54,Default,,0000,0000,0000,,{\i1}Nhưng mà, Akina sẽ rất tuyệt khi mặc bộ đồ đó~{\i0}
Dialogue: 0,0:05:49.74,0:05:52.80,Default,,0000,0000,0000,,{\i1}Có lẽ kiểu đồ bơi như vậy sẽ ko được sử dụng tại đây....{\i0}
Dialogue: 0,0:06:08.61,0:06:13.24,Default,,0000,0000,0000,,{\i1}Quái lạl, chuyện vừa rồi là sao nhỉ?{\i0}
Dialogue: 0,0:06:17.14,0:06:20.21,Default,,0000,0000,0000,,{\i1}Akina tập luyện thật chăm chỉ.{\i0}
Dialogue: 0,0:06:29.51,0:06:31.56,Default,,0000,0000,0000,,{\i1}Mình phải vào toilet thôi....{\i0}
Dialogue: 0,0:07:36.75,0:07:39.35,Default,,0000,0000,0000,,{\i1}Vậy đây là cái mà mọi người gọi là xe điện ngầm thành phố, huh?{\i0}
Dialogue: 0,0:07:42.66,0:07:46.92,Default,,0000,0000,0000,,{\i1}Nhớ lại chuyện đêm qua, đúng là mình tốt số mới tới được đây....{\i0}
Dialogue: 0,0:07:47.80,0:07:50.65,Akina,,0000,0000,0000,,Thôi nào, Haru-kun. Ở chỗ thế này mà cậu....
Dialogue: 0,0:07:53.22,0:07:55.95,Akina,,0000,0000,0000,,Cái đó lại cọ vào váy mình nữa rồi~
Dialogue: 0,0:07:57.19,0:08:01.15,Akina,,0000,0000,0000,,Haru-kun, chúng ta phải làm gì bây giờ~?
Dialogue: 0,0:08:01.25,0:08:03.62,Haruomi,,0000,0000,0000,,Xin lỗi, mình sẽ lùi lại ngay....
Dialogue: 0,0:08:10.20,0:08:12.53,Haruomi,,0000,0000,0000,,Xin..xin lỗi, mình lại xô vào cậu nữa rồi.
Dialogue: 0,0:08:12.89,0:08:17.51,Akina,,0000,0000,0000,,Haru-kun, làm ơn đừng di chuyển nữa.\NNếu ko thì....
Dialogue: 0,0:08:19.94,0:08:24.12,Akina,,0000,0000,0000,,Nếu thứ này bự lên như vậy, con trai sẽ ra rất nhanh phải ko?
Dialogue: 0,0:08:26.57,0:08:29.58,Akina,,0000,0000,0000,,Mình..mình sẽ giúp cậu ra.
Dialogue: 0,0:08:30.49,0:08:33.20,Akina,,0000,0000,0000,,Shh! Nhỏ tiếng đi nào, họ sẽ nghe thấy mất!
Dialogue: 0,0:08:36.14,0:08:37.99,Akina,,0000,0000,0000,,Thế này phải ko?
Dialogue: 0,0:08:37.99,0:08:41.02,Haruomi,,0000,0000,0000,,Akina, sao cậu học được điều này....
Dialogue: 0,0:08:46.12,0:08:49.09,Akina,,0000,0000,0000,,Ko, ko.... Cái Haru-kun chạm vào chỗ đó rồi.
Dialogue: 0,0:08:49.79,0:08:53.57,Haruomi,,0000,0000,0000,,A..Akina, tuyệt quá quá....
Dialogue: 0,0:08:53.57,0:08:57.83,Akina,,0000,0000,0000,,Haru-kun, ko! Nó đang....
Dialogue: 0,0:08:59.45,0:09:03.79,Akina,,0000,0000,0000,,Cái của Haru-kun, tuyệt quá. Nó thò ra ngoài váy của mình rồi.
Dialogue: 0,0:09:05.09,0:09:06.11,Haruomi,,0000,0000,0000,,Sắp ra rồi....!
Dialogue: 0,0:09:06.34,0:09:08.32,Akina,,0000,0000,0000,,Khoan..Khoan đã! Haru-kun!
Dialogue: 0,0:09:16.59,0:09:17.82,Haruomi,,0000,0000,0000,,Ra rồi....
Dialogue: 0,0:09:18.13,0:09:20.61,Haruomi,,0000,0000,0000,,Xin lỗi..xin lỗi, Akina. Mình chỉ....
Dialogue: 0,0:09:20.91,0:09:22.56,Akina,,0000,0000,0000,,Haru-kun, thứ này....
Dialogue: 0,0:09:23.00,0:09:24.72,Akina,,0000,0000,0000,,Mình uống chúng được ko?
Dialogue: 0,0:09:25.23,0:09:26.09,Haruomi,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:09:34.67,0:09:38.38,Default,,0000,0000,0000,,{\i1}Ko biết Akina có giận mình về chuyện hồi sáng ko nhỉ?{\i0}
Dialogue: 0,0:09:42.34,0:09:46.52,Default,,0000,0000,0000,,{\i1}Sau khi đã làm chuyện như vậy....\Ncó được gọi là tình yêu ko nhỉ?{\i0}
Dialogue: 0,0:09:47.51,0:09:51.69,Default,,0000,0000,0000,,{\i1}Nhưng mà, cái của Haru-kun lại bự lên như vậy~{\i0}
Dialogue: 0,0:09:51.79,0:09:55.54,Default,,0000,0000,0000,,{\i1}Mình đang nghĩ gì vậy nè trời?!{\i0}
Dialogue: 0,0:10:04.31,0:10:07.07,Haruomi,,0000,0000,0000,,A..Akina, cho mình xin lỗi về chuyện sáng nay.
Dialogue: 0,0:10:09.01,0:10:15.17,Akina,,0000,0000,0000,,Đúng..Đúng rồi! Nếu thứ đó mà dây lên đồng phục của cậu, thì sẽ rất tệ đó!
Dialogue: 0,0:10:16.07,0:10:16.68,Haruomi,,0000,0000,0000,,Eh?
Dialogue: 0,0:10:16.88,0:10:18.60,Akina,,0000,0000,0000,,Haru-kun là đồ ngốc!!!!
Dialogue: 0,0:10:18.60,0:10:19.89,Haruomi,,0000,0000,0000,,Này, coi chừng....
Dialogue: 0,0:10:21.70,0:10:22.47,Haruomi,,0000,0000,0000,,Akina!!!!
Dialogue: 0,0:10:27.03,0:10:31.16,Natsumi,,0000,0000,0000,,Huh~, sao Akina-chan về muộn thế nhỉ....
Dialogue: 0,0:10:37.85,0:10:39.01,Haruomi,,0000,0000,0000,,Thật là....
Dialogue: 0,0:10:40.55,0:10:41.99,Haruomi,,0000,0000,0000,,Cậu phải cẩn thận hơn khi sang đường chứ?
Dialogue: 0,0:10:42.66,0:10:46.05,Akina,,0000,0000,0000,,Ha..Haru-kun, cám ơn cậu....
Dialogue: 0,0:10:50.46,0:10:53.53,Default,,0000,0000,0000,,{\i1}Sau nhiều chuyện xảy ra hôm nay, mình thực sự cảm thấy mệt mỏi....{\i0}
Dialogue: 0,0:11:11.83,0:11:16.52,Default,,0000,0000,0000,,{\i1}Mình biết chuyện đó thật tệ,\Nnhưng chỉ thế thôi chắc cũng ko vấn đề gì....{\i0}
Dialogue: 0,0:11:27.21,0:11:28.98,Mafuyu,,0000,0000,0000,,Anh đứng đây từ lúc nãy, phải ko?
Dialogue: 0,0:11:31.02,0:11:33.04,Haruomi,,0000,0000,0000,,Ma..Mafuyu....
Dialogue: 0,0:11:33.34,0:11:34.74,Mafuyu,,0000,0000,0000,,Vào đi nào.
Dialogue: 0,0:11:38.76,0:11:40.96,Haruomi,,0000,0000,0000,,Điều này...., điều này....
Dialogue: 0,0:11:42.07,0:11:44.21,Mafuyu,,0000,0000,0000,,Anh đã đứng nhìn trộm em, thay vì vào phòng tắm phải ko.
Dialogue: 0,0:11:45.02,0:11:48.02,Mafuyu,,0000,0000,0000,,Anh may mắn lắm mới được chứng kiến em thủ dâm đấy nhé?
Dialogue: 0,0:11:49.07,0:11:54.83,Mafuyu,,0000,0000,0000,,Nếu anh ko muốn em nói chuyện này với Akina và những người khác, cho em thấy cậu bé của anh nào.
Dialogue: 0,0:11:54.83,0:11:56.26,Haruomi,,0000,0000,0000,,Này, Mafuyu....
Dialogue: 0,0:11:56.46,0:11:58.78,Haruomi,,0000,0000,0000,,Cậu bé của anh?
Dialogue: 0,0:11:58.96,0:12:00.80,Mafuyu,,0000,0000,0000,,Nó đã bự lên như thế này rồi à....
Dialogue: 0,0:12:03.36,0:12:05.83,Mafuyu,,0000,0000,0000,,Em sẽ giúp anh chuyện này.
Dialogue: 0,0:12:06.85,0:12:09.13,Haruomi,,0000,0000,0000,,Này, mafuyu....
Dialogue: 0,0:12:09.25,0:12:10.51,Mafuyu,,0000,0000,0000,,Yên lặng nào!
Dialogue: 0,0:12:13.78,0:12:14.98,Mafuyu,,0000,0000,0000,,Rất tuyệt, phải ko?
Dialogue: 0,0:12:15.08,0:12:16.42,Mafuyu,,0000,0000,0000,,Thoải mái đi nào.
Dialogue: 0,0:12:17.82,0:12:19.83,Mafuyu,,0000,0000,0000,,Haruomi, anh dâm thật đấy.
Dialogue: 0,0:12:20.31,0:12:22.43,Mafuyu,,0000,0000,0000,,Vẻ mặt anh cho thấy rằng anh đang rất thích thú phải ko.
Dialogue: 0,0:12:23.06,0:12:25.12,Mafuyu,,0000,0000,0000,,Anh muốn mạnh hơn nữa, phải ko?
Dialogue: 0,0:12:30.24,0:12:31.52,Haruomi,,0000,0000,0000,,Ugh, lạnh quá.
Dialogue: 0,0:12:39.33,0:12:41.57,Mafuyu,,0000,0000,0000,,Bàn chân của em thế nào hả?
Dialogue: 0,0:12:41.65,0:12:43.26,Mafuyu,,0000,0000,0000,,Có sướng ko?
Dialogue: 0,0:12:43.26,0:12:46.86,Default,,0000,0000,0000,,{\i1}Tệ quá, mình đâu có biến thái đến mức độ này đâu....{\i0}
Dialogue: 0,0:12:47.06,0:12:48.11,Default,,0000,0000,0000,,{\i1}Sướng quá.{\i0}
Dialogue: 0,0:12:48.65,0:12:54.08,Mafuyu,,0000,0000,0000,,Cái này là thói quen của đàn ông, họ rất dễ thỏa mãn với chân của phụ nữ. Thật khó coi, phải ko.
Dialogue: 0,0:12:54.78,0:12:56.12,Mafuyu,,0000,0000,0000,,Anh muốn ra chưa?
Dialogue: 0,0:12:56.12,0:12:59.50,Mafuyu,,0000,0000,0000,,Anh muốn ra hết những gì anh đang phải kìm nén, phải ko?
Dialogue: 0,0:13:01.69,0:13:05.07,Mafuyu,,0000,0000,0000,,Anh dễ thương lắm, Haruomi.\NChỉ thế thôi mà đã đỏ mặt rồi kìa.
Dialogue: 0,0:13:05.62,0:13:08.38,Mafuyu,,0000,0000,0000,,Cậu bé của anh đã bự tới mức này rồi à,
Dialogue: 0,0:13:08.81,0:13:10.41,Mafuyu,,0000,0000,0000,,Ko sao đâu, cứ ra hết đi nào.
Dialogue: 0,0:13:10.41,0:13:13.25,Mafuyu,,0000,0000,0000,,Hãy ra đi nào, hãy cho em sữa tình yêu của anh.
Dialogue: 0,0:13:13.25,0:13:15.18,Mafuyu,,0000,0000,0000,,Em sẽ chà mạnh hơn nữa.
Dialogue: 0,0:13:15.85,0:13:16.95,Haruomi,,0000,0000,0000,,Ra..Ra rồi....
Dialogue: 0,0:13:24.98,0:13:27.63,Default,,0000,0000,0000,,{\i1}Đây là tinh dịch sao....{\i0}
Dialogue: 0,0:13:40.08,0:13:42.64,Haruomi,,0000,0000,0000,,Chuyện đó với..Mafuyu....
Dialogue: 0,0:13:43.21,0:13:46.02,Haruomi,,0000,0000,0000,,Nhưng, chuyện này tuyệt thật~
Dialogue: 0,0:13:54.95,0:13:56.08,Haruomi,,0000,0000,0000,,Natsumi-san?
Dialogue: 0,0:14:06.24,0:14:11.69,Default,,0000,0000,0000,,{\i1}Mai là ngày nghỉ, mình nên tới cửa hiệu giúp Natsumi-san.{\i0}
Dialogue: 0,0:14:19.60,0:14:24.32,Haruomi,,0000,0000,0000,,Đây..đây là cửa hiệu của Natsumi-san sao?
Dialogue: 0,0:14:29.09,0:14:31.40,Natsumi,,0000,0000,0000,,Haru-chan, cám ơn em rất nhiều~
Dialogue: 0,0:14:31.40,0:14:34.63,Haruomi,,0000,0000,0000,,Ko, chính chị mới là người luôn chăm sóc em trong suốt thời gian qua.
Dialogue: 0,0:14:35.35,0:14:38.23,Haruomi,,0000,0000,0000,,Ủa, sao cửa hiệu vắng khách thế....
Dialogue: 0,0:14:38.23,0:14:44.30,Natsumi,,0000,0000,0000,,Phải đó~! Từ khi chị tiếp quản cửa hiệu đến nay, khách hàng cứ thưa dần~
Dialogue: 0,0:14:44.60,0:14:48.09,Haruomi,,0000,0000,0000,,Na..Natsumi-san, nó tràn ra rồi kìa....
Dialogue: 0,0:14:49.49,0:14:54.48,Natsumi,,0000,0000,0000,,Nhưng mà, nhờ có khoản tiền tiết kiệm của cha và cửa hàng này,
Dialogue: 0,0:14:54.48,0:14:55.78,Natsumi,,0000,0000,0000,,nên mọi chuyện vẫn ổn.
Dialogue: 0,0:14:56.28,0:14:59.41,Natsumi,,0000,0000,0000,,Haru-chan, đừng lo lắng về chuyện này nhé.
Dialogue: 0,0:15:03.95,0:15:07.53,Default,,0000,0000,0000,,{\i1}Natsumi-san chắc hẳn rất vất vả với cửa hiệu này....{\i0}
Dialogue: 0,0:15:09.00,0:15:12.75,Default,,0000,0000,0000,,{\i1}Mặc dù trông vẫn hơi trẻ con, nhưng thân hình của chị ấy thật quyến rũ....{\i0}
Dialogue: 0,0:15:14.42,0:15:17.98,Default,,0000,0000,0000,,{\i1}Vả lại, chị ấy cũng rất đẹp và ngực còn bự nữa~{\i0}
Dialogue: 0,0:15:20.02,0:15:23.65,Haruomi,,0000,0000,0000,,Phải rồi..phải rồi, mềm mại thật....
Dialogue: 0,0:15:23.65,0:15:28.05,Natsumi,,0000,0000,0000,,Haru-chan, nếu em mê ngực như vậy....\NSao em ko thử xem?
Dialogue: 0,0:15:28.15,0:15:29.46,Haruomi,,0000,0000,0000,,Natsumi-san?!
Dialogue: 0,0:15:30.89,0:15:33.03,Natsumi,,0000,0000,0000,,Haru-chan, em thật đáng yêu~
Dialogue: 0,0:15:38.02,0:15:39.85,Haruomi,,0000,0000,0000,,A, Natsumi-san....
Dialogue: 0,0:15:40.05,0:15:40.85,Haruomi,,0000,0000,0000,,Này....
Dialogue: 0,0:15:41.94,0:15:44.81,Natsumi,,0000,0000,0000,,Haru-chan, của em bự thật đấy~
Dialogue: 0,0:15:45.37,0:15:47.30,Natsumi,,0000,0000,0000,,Thôi nào, để chị lôi nó ra nào.
Dialogue: 0,0:15:47.30,0:15:49.98,Natsumi,,0000,0000,0000,,Em ko việc gì phải xấu hổ cả.
Dialogue: 0,0:15:57.61,0:16:03.68,Natsumi,,0000,0000,0000,,Cái của em bự lên thế này là do chị.\NHãy để chị chuộc lỗi với em nào.
Dialogue: 0,0:16:04.29,0:16:09.26,Natsumi,,0000,0000,0000,,Haru-chan, chị sẽ làm cho em sung sướng bằng bộ ngực của chị.
Dialogue: 0,0:16:12.64,0:16:14.95,Haruomi,,0000,0000,0000,,Natsumi-san...., chuyện này ko được.
Dialogue: 0,0:16:16.07,0:16:21.53,Natsumi,,0000,0000,0000,,Em nói gì vậy, coi xem cái của em bự tới mức nào rồi kìa.
Dialogue: 0,0:16:22.13,0:16:26.63,Natsumi,,0000,0000,0000,,Haru-chan rất mê ngực mà, phải ko.
Dialogue: 0,0:16:31.67,0:16:35.55,Haruomi,,0000,0000,0000,,Tuyệt quá, ngực của Natsumi-san thật mềm mại~
Dialogue: 0,0:16:35.75,0:16:42.09,Natsumi,,0000,0000,0000,,Trông thấy thứ to lớn của Haru-chan thế này khiến cho chị vui lắm đó.
Dialogue: 0,0:16:44.12,0:16:46.25,Haruomi,,0000,0000,0000,,Sắp ra rồi! Em sắp ra rồi, Natsumi-san!!
Dialogue: 0,0:16:46.25,0:16:51.35,Natsumi,,0000,0000,0000,,Ko sao đâu, cứ ra hết đi nào.\NThật đó.
Dialogue: 0,0:17:07.21,0:17:09.44,Natsumi,,0000,0000,0000,,Đây là... của Haru-chan
Dialogue: 0,0:17:14.93,0:17:18.81,Natsumi,,0000,0000,0000,,Chỗ này của chị thành ra thế này là lỗi của Haru-chan đấy nhé,
Dialogue: 0,0:17:18.81,0:17:21.62,Natsumi,,0000,0000,0000,,Chị sẽ phạt em.
Dialogue: 0,0:17:21.62,0:17:23.00,Haruomi,,0000,0000,0000,,Chị định phạt em..thế nào?
Dialogue: 0,0:17:23.00,0:17:25.97,Natsumi,,0000,0000,0000,,Chị sẽ bắt em làm tình với chị.
Dialogue: 0,0:17:27.22,0:17:29.98,Haruomi,,0000,0000,0000,,Natsumi-san, chúng ta ko thể làm như thế.
Dialogue: 0,0:17:32.85,0:17:36.60,Natsumi,,0000,0000,0000,,Tuyệt..tuyệt quá, đây chính là... Haru-chan
Dialogue: 0,0:17:38.01,0:17:41.02,Haruomi,,0000,0000,0000,,Natsumi-san, đây là lần đầu tiên của chị à?
Dialogue: 0,0:17:41.22,0:17:44.82,Natsumi,,0000,0000,0000,,Haru-chan, em lại nghĩ lung tung gì hả.
Dialogue: 0,0:17:44.82,0:17:45.65,Haruomi,,0000,0000,0000,,Nhưng mà....
Dialogue: 0,0:17:46.69,0:17:48.64,Natsumi,,0000,0000,0000,,Em thật đáng yêu, Haru-chan.
Dialogue: 0,0:17:49.05,0:17:50.96,Natsumi,,0000,0000,0000,,Chuyện này sẽ ko sao đâu.
Dialogue: 0,0:17:51.55,0:17:52.49,Haruomi,,0000,0000,0000,,Natsumi-san!
Dialogue: 0,0:18:03.60,0:18:08.40,Natsumi,,0000,0000,0000,,Thứ to lớn của Haru-chan, đang vào sâu bên trong chị.
Dialogue: 0,0:18:12.64,0:18:14.89,Haruomi,,0000,0000,0000,,Natsumi-san, chị có muốn ra ko?
Dialogue: 0,0:18:15.49,0:18:21.07,Natsumi,,0000,0000,0000,,Muốn chứ, cái của Haru-chan ko ngừng đâm vào sâu bên trong chị.\NSướng quá.
Dialogue: 0,0:18:26.40,0:18:27.42,Natsumi,,0000,0000,0000,,Tuyệt quá~
Dialogue: 0,0:18:45.26,0:18:46.26,Other,,0000,0000,0000,,Xin lỗi~
Dialogue: 0,0:18:46.36,0:18:48.58,Haruomi,,0000,0000,0000,,Thôi chết, khách hàng kêu chị kìa!
Dialogue: 0,0:18:49.22,0:18:50.23,Haruomi,,0000,0000,0000,,Natsumi-san?
Dialogue: 0,0:18:51.54,0:18:54.19,Haruomi,,0000,0000,0000,,Ukh, Natsumi-san? Sao hơi thở của chị lại có mùi rượu thế này.
Dialogue: 0,0:18:54.29,0:18:56.34,Haruomi,,0000,0000,0000,,Khoan đã, khách hàng đang gọi chị kìa!
Dialogue: 0,0:18:56.34,0:18:57.34,Haruomi,,0000,0000,0000,,Này, Natsumi-san!
Dialogue: 0,0:18:59.19,0:19:01.22,Default,,0000,0000,0000,,{\i1}Vài ngày sau,{\i0}
Dialogue: 0,0:19:01.42,0:19:04.66,Default,,0000,0000,0000,,{\i1}Có vẻ như Natsumi-san vẫn chẳng thay đổi gì.{\i0}
Dialogue: 0,0:19:05.06,0:19:08.32,Default,,0000,0000,0000,,{\i1}Chị ấy thật sự ko nhớ gì cả à?{\i0}
Dialogue: 0,0:19:09.14,0:19:14.55,Mafuyu,,0000,0000,0000,,Haruomi, anh lại tới cửa hiệu của chị Natsumi nữa à.\NAnh thích chị Natsumi đến thế sao?
Dialogue: 0,0:19:16.04,0:19:19.17,Akina,,0000,0000,0000,,Mình thì ko phiền gì đâu,\Nnếu Haru-kun thích thế....
Dialogue: 0,0:19:19.93,0:19:24.23,Haruomi,,0000,0000,0000,,Ko phải như vậy, mình chỉ muốn giúp chị Natsumi đỡ vất vả hơn trong việc trông nom cửa hiệu thôi mà.
Dialogue: 0,0:19:24.43,0:19:29.23,Natsumi,,0000,0000,0000,,Đã hè rồi, vậy mà công việc kinh doanh vẫn ko được tốt lắm.
Dialogue: 0,0:19:30.00,0:19:33.57,Haruomi,,0000,0000,0000,,Đúng rồi, Natsumi-san có một chiếc xe phải ko?
Dialogue: 0,0:19:33.87,0:19:37.08,Haruomi,,0000,0000,0000,,Thế thì, tại sao chúng ta ko mở một cửa hiệu di động nhỉ?
Dialogue: 0,0:19:37.29,0:19:40.93,Haruomi,,0000,0000,0000,,Bằng cách đó, chúng ta có thể bán hàng ngay tại bờ biển.
Dialogue: 0,0:19:40.93,0:19:42.99,Natsumi,,0000,0000,0000,,Đúng là một sáng kiến tuyệt vời.
Dialogue: 0,0:19:42.99,0:19:45.07,Mafuyu,,0000,0000,0000,,Haruomi, đôi lúc anh cũng đáng tin cậy đấy nhỉ.
Dialogue: 0,0:19:45.07,0:19:47.54,Akina,,0000,0000,0000,,Đúng thế! Haru-kun luôn là một cậu bé ngoan mà, phải ko!
Dialogue: 0,0:19:47.54,0:19:50.32,Haruomi,,0000,0000,0000,,Akina, đừng lúc nào cũng coi mình là trẻ con như vậy chứ!
Dialogue: 0,0:19:52.54,0:19:53.90,Akina,,0000,0000,0000,,Mình..xin lỗi....
Dialogue: 0,0:19:59.36,0:20:00.42,Haruomi,,0000,0000,0000,,Em về rồi đây!
Dialogue: 0,0:20:01.02,0:20:01.63,Haruomi,,0000,0000,0000,,Eh?
Dialogue: 0,0:20:02.13,0:20:05.63,Default,,0000,0000,0000,,{\i1}Hôm nay Akina ko tới câu lạc bộ ngoại khóa à?{\i0}
Dialogue: 0,0:20:24.10,0:20:26.39,Default,,0000,0000,0000,,{\i1}Mình phải tới xem cô ấy thế nào mới được.{\i0}
Dialogue: 0,0:20:28.85,0:20:29.52,Haruomi,,0000,0000,0000,,Hả?!
Dialogue: 0,0:20:35.72,0:20:38.40,Haruomi,,0000,0000,0000,,Sao lại nằm ngủ vùi thế kia....
Dialogue: 0,0:20:38.80,0:20:39.86,Haruomi,,0000,0000,0000,,Thật là~
Dialogue: 0,0:20:48.74,0:20:50.74,Haruomi,,0000,0000,0000,,A..Akina....
Dialogue: 0,0:20:55.20,0:20:56.88,Akina,,0000,0000,0000,,Eh, Ha..Haru-kun?!
Dialogue: 0,0:20:58.03,0:20:59.97,Haruomi,,0000,0000,0000,,N..Này....
Dialogue: 0,0:20:59.97,0:21:02.13,Akina,,0000,0000,0000,,Sao Haru-kun lại ở đây?!
Dialogue: 0,0:21:02.14,0:21:04.37,Haruomi,,0000,0000,0000,,Sao cậu lại hỏi thế, chính cậu là người....
Dialogue: 0,0:21:04.37,0:21:05.78,Haruomi,,0000,0000,0000,,Ko, mình xin lỗi....
Dialogue: 0,0:21:05.78,0:21:06.77,Haruomi,,0000,0000,0000,,Mình sẽ ra ngoài ngay.
Dialogue: 0,0:21:06.77,0:21:07.77,Akina,,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:21:07.97,0:21:10.66,Akina,,0000,0000,0000,,Hãy...hãy ở lại đây với mình một lúc....
Dialogue: 0,0:21:10.86,0:21:12.86,Haruomi,,0000,0000,0000,,K..Ko được! Chúng ta ko thể làm vậy được!
Dialogue: 0,0:21:12.86,0:21:14.27,Haruomi,,0000,0000,0000,,Chuyện này ko hay chút nào đâu.
Dialogue: 0,0:21:14.77,0:21:15.56,Akina,,0000,0000,0000,,Ko sao đâu mà.
Dialogue: 0,0:21:19.27,0:21:27.09,Akina,,0000,0000,0000,,Nếu Haru-kun thật sự hiểu được cảm giác của mình, thì ít ra cậu cũng phải ôm lấy mình chứ.
Dialogue: 0,0:21:27.09,0:21:29.43,Akina,,0000,0000,0000,,Giống như một người đàn ông thực thụ.
Dialogue: 0,0:21:30.97,0:21:35.39,Haruomi,,0000,0000,0000,,Nếu cậu còn nói như vậy, mình sẽ ko thể kềm lòng được mất....
Dialogue: 0,0:21:37.23,0:21:44.71,Akina,,0000,0000,0000,,Sao nào, cậu cần gì phải kiềm chế điều đó.Haru-kun và mình đâu còn là trẻ con nữa đâu.
Dialogue: 0,0:21:45.01,0:21:45.79,Haruomi,,0000,0000,0000,,Akina....
Dialogue: 0,0:21:53.61,0:21:54.68,Akina,,0000,0000,0000,,Ha..Haru-kun!
Dialogue: 0,0:22:05.20,0:22:08.59,Akina,,0000,0000,0000,,Haru-kun, bộ động phục này vướng víu quá.
Dialogue: 0,0:22:08.59,0:22:09.69,Haruomi,,0000,0000,0000,,P..Phải rồi....
Dialogue: 0,0:22:12.83,0:22:13.84,Haruomi,,0000,0000,0000,,Akina....
Dialogue: 0,0:22:20.32,0:22:23.53,Haruomi,,0000,0000,0000,,Akina, cậu liếm cái của mình được chứ?
Dialogue: 0,0:22:23.73,0:22:24.53,Akina,,0000,0000,0000,,Được....
Dialogue: 0,0:22:28.46,0:22:33.73,Akina,,0000,0000,0000,,Wo..Wow, nhìn gần thế này.\NTrông nó thật mạnh mẽ nhỉ.
Dialogue: 0,0:22:33.93,0:22:36.32,Haruomi,,0000,0000,0000,,Nếu bạn ko ngại, thì hãy bắt đầu đi.
Dialogue: 0,0:22:36.52,0:22:40.76,Akina,,0000,0000,0000,,Ko phải mình ngại, mà mình thực sự ko biết phải làm thế nào.
Dialogue: 0,0:22:41.49,0:22:44.85,Haruomi,,0000,0000,0000,,Nếu Akina làm chuyện đó, mình sẽ rất sung sướng bất kể nó thế nào.
Dialogue: 0,0:22:50.99,0:22:53.76,Haruomi,,0000,0000,0000,,Phải rồi, như thế đó.
Dialogue: 0,0:22:53.76,0:22:55.67,Haruomi,,0000,0000,0000,,Tuyệt lắm~
Dialogue: 0,0:22:56.94,0:23:00.44,Akina,,0000,0000,0000,,Thật sao? Cậu nói thế làm mình vui lắm.\NVậy, để mình làm thêm nữa nhé.
Dialogue: 0,0:23:06.18,0:23:08.83,Haruomi,,0000,0000,0000,,Akina, nếu cậu cứ làm thế này....
Dialogue: 0,0:23:11.62,0:23:13.42,Haruomi,,0000,0000,0000,,Akina, phải rồi!
Dialogue: 0,0:23:13.42,0:23:14.70,Haruomi,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:23:18.06,0:23:20.50,Haruomi,,0000,0000,0000,,Uh, ko được rồi....\NMình ra mất!
Dialogue: 0,0:23:29.67,0:23:32.50,Haruomi,,0000,0000,0000,,Xin lỗi, mình ra hết vào miệng cậu rồi.
Dialogue: 0,0:23:32.50,0:23:35.35,Akina,,0000,0000,0000,,Eh, ko sao đâu mà.
Dialogue: 0,0:23:35.39,0:23:37.42,Akina,,0000,0000,0000,,Cậu sướng lắm, phải ko.
Dialogue: 0,0:23:37.68,0:23:39.61,Haruomi,,0000,0000,0000,,Phải, chuyện này thật là....
Dialogue: 0,0:23:39.61,0:23:41.15,Haruomi,,0000,0000,0000,,Nào, tới lượt Akina rồi đó.
Dialogue: 0,0:23:43.47,0:23:45.04,Akina,,0000,0000,0000,,Ha..Haru-kun~
Dialogue: 0,0:23:57.93,0:23:59.63,Akina,,0000,0000,0000,,Ko, Haru-kun.
Dialogue: 0,0:24:05.77,0:24:08.13,Haruomi,,0000,0000,0000,,Akina, cậu ướt hết rồi nè.
Dialogue: 0,0:24:08.13,0:24:09.79,Akina,,0000,0000,0000,,Là bởi vì~
Dialogue: 0,0:24:23.05,0:24:26.43,Haruomi,,0000,0000,0000,,Akina, mình muốn nhét nó vào bên trong cậu.
Dialogue: 0,0:24:27.47,0:24:29.51,Akina,,0000,0000,0000,,Đ..Được, ko sao đâu.
Dialogue: 0,0:24:29.71,0:24:31.57,Akina,,0000,0000,0000,,Vào đi, Haru-kun.
Dialogue: 0,0:25:01.72,0:25:02.98,Haruomi,,0000,0000,0000,,Nó vào bên trong rồi....
Dialogue: 0,0:25:04.25,0:25:06.12,Haruomi,,0000,0000,0000,,Đau ko, Akina?
Dialogue: 0,0:25:06.23,0:25:08.77,Akina,,0000,0000,0000,,Ph..Phải, đau lắm....
Dialogue: 0,0:25:08.97,0:25:12.05,Akina,,0000,0000,0000,,Giờ thì đỡ đau rồi....
Dialogue: 0,0:25:12.69,0:25:14.36,Haruomi,,0000,0000,0000,,Mình làm đây, Akina.
Dialogue: 0,0:25:14.56,0:25:15.90,Akina,,0000,0000,0000,,Ph..Phải rồi....
Dialogue: 0,0:25:45.35,0:25:47.24,Haruomi,,0000,0000,0000,,Akina, chuyện này thật sự rất tuyệt!
Dialogue: 0,0:25:47.85,0:25:49.15,Haruomi,,0000,0000,0000,,Mình ko thể ngừng lại được....
Dialogue: 0,0:26:01.13,0:26:05.14,Akina,,0000,0000,0000,,Haru-kun, cái của cậu vẫn còn cứng nè....
Dialogue: 0,0:26:05.80,0:26:08.13,Akina,,0000,0000,0000,,Cậu vẫn còn muốn nữa mà, phải ko.
Dialogue: 0,0:26:08.23,0:26:14.79,Akina,,0000,0000,0000,,Ko sao đâu, mình rất vui vì cậu thích điều đó.
Dialogue: 0,0:26:15.76,0:26:16.72,Haruomi,,0000,0000,0000,,Akina....
Dialogue: 0,0:26:28.34,0:26:32.21,Haruomi,,0000,0000,0000,,A..Akina, Tuyệt quá.
Dialogue: 0,0:26:41.80,0:26:44.18,Akina,,0000,0000,0000,,Haru-kun..Haru-kun~!
Dialogue: 0,0:26:46.95,0:26:48.88,Haruomi,,0000,0000,0000,,Akina..Akina~!
Dialogue: 0,0:26:53.45,0:26:56.96,Akina,,0000,0000,0000,,Haru-kun, mình ko thể kiềm chế được nữa rồi.
Dialogue: 0,0:26:58.56,0:27:00.71,Haruomi,,0000,0000,0000,,Hãy ra cùng hau nào, Akina!
Dialogue: 0,0:27:24.94,0:27:27.46,Mafuyu,,0000,0000,0000,,Akina, chị ngắm nhìn Haruomi hơi nhiều đó.
Dialogue: 0,0:27:27.46,0:27:30.95,Natsumi,,0000,0000,0000,,Haru-chan, có chuyện gì với Akina à?
Dialogue: 0,0:27:30.96,0:27:32.04,Haruomi,,0000,0000,0000,,Ko..Ko có gì đâu ạ....
Dialogue: 0,0:27:32.04,0:27:33.09,Akina,,0000,0000,0000,,Đúng..Đúng như thế,
Dialogue: 0,0:27:33.09,0:27:37.23,Akina,,0000,0000,0000,,Kể từ bây giờ, Haru-kun sẽ phải làm việc chăm chỉ tại cửa hiệu.
Dialogue: 0,0:27:41.14,0:27:43.45,Akina,,0000,0000,0000,,Haru-kun, nhanh lên nào!\NTụi mình sẽ muộn mất đó!
Dialogue: 0,0:27:44.24,0:27:45.75,Default,,0000,0000,0000,,{\i1}Akina dễ thương.{\i0}
Dialogue: 0,0:27:47.08,0:27:49.18,Default,,0000,0000,0000,,{\i1}Natsumi-san  tốt bụng.{\i0}
Dialogue: 0,0:27:51.38,0:27:52.48,Default,,0000,0000,0000,,{\i1}Mafuyu bí ẩn.{\i0}
Dialogue: 0,0:27:54.02,0:27:54.91,Akina,,0000,0000,0000,,Đi thôi nào!
Dialogue: 0,0:27:54.92,0:27:56.32,Haruomi,,0000,0000,0000,,Đ..Được rồi!
Dialogue: 0,0:27:56.52,0:27:59.60,Default,,0000,0000,0000,,{\i1}Kể từ bây giờ, cuộc sống của mình đã thú vị hơn rất nhiều.{\i0}
