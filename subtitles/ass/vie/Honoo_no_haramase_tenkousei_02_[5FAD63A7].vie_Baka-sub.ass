Dialogue: 0,0:03:02.58,0:03:05.75,Default,,0000,0000,0000,,Đã tới lúc chúng ta giải quyết dứt điểm mọi ân oán, Kaede-san.
Dialogue: 0,0:03:06.24,0:03:07.65,Default,,0000,0000,0000,,Đó cũng chính là ý định của tôi.
Dialogue: 0,0:03:09.17,0:03:12.25,Default,,0000,0000,0000,,Quên mình vì Thượng đế là truyền thống của gia tộc Narahashin.
Dialogue: 0,0:03:18.78,0:03:20.46,Default,,0000,0000,0000,,Mãnh Long hạ phàm.
Dialogue: 0,0:03:21.96,0:03:23.80,Default,,0000,0000,0000,,Tránh hay lắm.
Dialogue: 0,0:03:24.09,0:03:28.00,Default,,0000,0000,0000,,Nhưng lần sau liệu có may mắn như vậy không, Tatsuya?
Dialogue: 0,0:03:28.86,0:03:32.37,Default,,0000,0000,0000,,Đừng coi thường tôi.
Dialogue: 0,0:03:34.97,0:03:37.67,Default,,0000,0000,0000,,Cậu không thể bắt kịp thân pháp của tôi đâu!
Dialogue: 0,0:03:37.67,0:03:38.36,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:03:38.36,0:03:40.56,Default,,0000,0000,0000,,Nội công siêu cấp Tatami!
Dialogue: 0,0:03:41.90,0:03:44.62,Default,,0000,0000,0000,,Cái gì!? Chú em ngây thơ quá đấy!
Dialogue: 0,0:03:51.78,0:03:54.89,Default,,0000,0000,0000,,Định xài mấy trò trẻ con đó để qua mặt cao thủ như chị đây à?
Dialogue: 0,0:03:55.03,0:03:57.51,Default,,0000,0000,0000,,Không hẳn vậy.
Dialogue: 0,0:04:01.83,0:04:03.60,Default,,0000,0000,0000,,Thiết luyện!
Dialogue: 0,0:04:09.71,0:04:11.89,Default,,0000,0000,0000,,Tôi thua rồi.
Dialogue: 0,0:04:12.82,0:04:17.21,Default,,0000,0000,0000,,Cậu biết phải làm gì lúc này rồi chứ, Kaede-san?
Dialogue: 0,0:04:17.40,0:04:21.05,Default,,0000,0000,0000,,Phải, dùng lưỡi của cậu đi.
Dialogue: 0,0:04:24.68,0:04:28.00,Default,,0000,0000,0000,,Mùi vị chiếc Naginata của mình thế nào?
Dialogue: 0,0:04:28.10,0:04:30.39,Default,,0000,0000,0000,,Vị ghê quá...
Dialogue: 0,0:04:30.44,0:04:33.04,Default,,0000,0000,0000,,Ghê hả?
Dialogue: 0,0:04:34.30,0:04:37.86,Default,,0000,0000,0000,,Vậy thì cậu hãy dùng miệng liếm sạch nó đi.
Dialogue: 0,0:04:37.89,0:04:39.68,Default,,0000,0000,0000,,Như vậy đó?!
Dialogue: 0,0:04:42.23,0:04:44.38,Default,,0000,0000,0000,,Phải rồi.
Dialogue: 0,0:04:44.46,0:04:47.48,Default,,0000,0000,0000,,Liếm sạch cả bên dưới nữa
Dialogue: 0,0:04:49.02,0:04:50.35,Default,,0000,0000,0000,,Manaka...
Dialogue: 0,0:04:50.40,0:04:53.09,Default,,0000,0000,0000,,Không sao đâu, chỉ cần cô chịu đi theo cậu ấy,
Dialogue: 0,0:04:53.14,0:04:56.99,Default,,0000,0000,0000,,cô sẽ được sống trong niềm hoan lạc bất tận.
Dialogue: 0,0:05:05.08,0:05:07.25,Default,,0000,0000,0000,,Phải, tuyệt lắm.
Dialogue: 0,0:05:07.28,0:05:14.18,Default,,0000,0000,0000,,Chỉ cần cậu làm tôi sung sướng, tôi hứa sẽ đáp lễ cầu đầy đủ.
Dialogue: 0,0:05:22.87,0:05:25.73,Default,,0000,0000,0000,,Cô sẽ cảm thấy thích thú ngay thôi.
Dialogue: 0,0:05:25.80,0:05:28.31,Default,,0000,0000,0000,,Manaka...
Dialogue: 0,0:05:34.61,0:05:38.43,Default,,0000,0000,0000,,Trước tiên hãy cho tôi xem nơi thầm kín của cậu nào.
Dialogue: 0,0:05:47.48,0:05:50.92,Default,,0000,0000,0000,,Kính chào Chủ nhân!
Dialogue: 0,0:05:51.66,0:05:54.88,Default,,0000,0000,0000,,Kính chào Chủ nhân!
Dialogue: 0,0:05:59.20,0:06:02.64,Default,,0000,0000,0000,,"Chủ nhân" quả là một từ ngữ tuyệt vời.
Dialogue: 0,0:06:02.71,0:06:06.06,Default,,0000,0000,0000,,Có vẻ như thời của tôi đã đến rồi.
Dialogue: 0,0:06:07.68,0:06:09.66,Default,,0000,0000,0000,,Honomi,trông cậu dễ thương ghê.
Dialogue: 0,0:06:09.78,0:06:12.19,Default,,0000,0000,0000,,Cám ơn Chủ nhân.
Dialogue: 0,0:06:12.26,0:06:17.68,Default,,0000,0000,0000,,Có vẻ như cô hầu mới kia chưa học được phép chào hỏi lễ độ.
Dialogue: 0,0:06:17.90,0:06:20.38,Default,,0000,0000,0000,,R-Rất vui được gặp ngài.
Dialogue: 0,0:06:22.17,0:06:25.37,Default,,0000,0000,0000,,Này, xem chừng cô ta chưa được dạy dỗ cẩn thận thì phải.
Dialogue: 0,0:06:25.51,0:06:27.45,Default,,0000,0000,0000,,Crocus!
Dialogue: 0,0:06:28.16,0:06:32.03,Default,,0000,0000,0000,,Mày biết phải dạy dỗ cô ta thế nào rồi chứ?
Dialogue: 0,0:06:40.36,0:06:45.81,Default,,0000,0000,0000,,Kaede-san, cậu rên rỉ nghe dâm đãng thật đấy.
Dialogue: 0,0:06:46.05,0:06:50.30,Default,,0000,0000,0000,,Kaede-san này, muốn tôi tăng cường độ lên không?
Dialogue: 0,0:06:50.58,0:06:52.25,Default,,0000,0000,0000,,Kaede-san?
Dialogue: 0,0:06:52.33,0:06:55.81,Default,,0000,0000,0000,,Kính mời Chủ nhân.
Dialogue: 0,0:06:58.87,0:07:02.11,Default,,0000,0000,0000,,Trông cậu kìa, Kaede-san.
Dialogue: 0,0:07:02.28,0:07:03.62,Default,,0000,0000,0000,,Đây là...
Dialogue: 0,0:07:03.71,0:07:06.36,Default,,0000,0000,0000,,Tăng cường độ lên, Crocus.
Dialogue: 0,0:07:09.42,0:07:12.67,Default,,0000,0000,0000,,Nếu tiếp tục giữ thái độ vô lễ như vậy, cậu sẽ bị trừng phạt nặng hơn.
Dialogue: 0,0:07:13.60,0:07:15.46,Default,,0000,0000,0000,,Kaede-san...
Dialogue: 0,0:07:16.37,0:07:17.23,Default,,0000,0000,0000,,Lại đây nào.
Dialogue: 0,0:07:17.30,0:07:18.54,Default,,0000,0000,0000,,Chủ nhân?
Dialogue: 0,0:07:18.54,0:07:21.72,Default,,0000,0000,0000,,Honomi, cậu đã hứa sẽ phục vụ mình chu đáo phải không?
Dialogue: 0,0:07:22.03,0:07:23.70,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:07:23.79,0:07:29.05,Default,,0000,0000,0000,,Không biết sờ nắn bộ ngực cậu sẽ làm mình thích thú tới mức nào.
Dialogue: 0,0:07:29.31,0:07:31.36,Default,,0000,0000,0000,,Xấu hổ chết đi được.
Dialogue: 0,0:07:31.36,0:07:34.08,Default,,0000,0000,0000,,Đúng là tuyệt hảo.
Dialogue: 0,0:07:34.54,0:07:36.71,Default,,0000,0000,0000,,Có khiến cậu vừa lòng không?
Dialogue: 0,0:07:36.71,0:07:37.90,Default,,0000,0000,0000,,Tất nhiên rồi.
Dialogue: 0,0:07:37.90,0:07:39.65,Default,,0000,0000,0000,,Mình thật sự bất ngờ đấy.
Dialogue: 0,0:07:40.34,0:07:44.44,Default,,0000,0000,0000,,Trông nè, thằng nhỏ của mình chào cờ rồi.
Dialogue: 0,0:07:45.69,0:07:48.48,Default,,0000,0000,0000,,Mình vui lắm, Tagami-san.
Dialogue: 0,0:07:51.46,0:07:56.31,Default,,0000,0000,0000,,Chủ nhân, nếu ngài không phiền, hãy cho phép chúng em phục vụ ngài.
Dialogue: 0,0:08:01.18,0:08:06.03,Default,,0000,0000,0000,,Vậy cả ba cậu cùng phục vụ mình một lượt nhé?
Dialogue: 0,0:08:06.05,0:08:07.41,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:08:09.04,0:08:10.42,Default,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:08:12.40,0:08:16.70,Default,,0000,0000,0000,,Crocus, phạt cô ấy thêm một lúc nữa.
Dialogue: 0,0:08:21.21,0:08:23.50,Default,,0000,0000,0000,,Ah, nó bự quá đi mất!
Dialogue: 0,0:08:23.55,0:08:25.84,Default,,0000,0000,0000,,Mềm mại quá...
Dialogue: 0,0:08:26.32,0:08:29.76,Default,,0000,0000,0000,,Chủ nhân, ngài thích chứ?
Dialogue: 0,0:08:31.36,0:08:34.89,Default,,0000,0000,0000,,Phải, tuyệt lắm.
Dialogue: 0,0:08:36.16,0:08:39.67,Default,,0000,0000,0000,,C-Cứ thế này chắc chết quá.
Dialogue: 0,0:08:39.83,0:08:45.76,Default,,0000,0000,0000,,Thằng nhỏ nóng bỏng của cậu đang cọ xát bên trong ngực mình.
Dialogue: 0,0:08:48.84,0:08:50.65,Default,,0000,0000,0000,,Chủ nhân.
Dialogue: 0,0:08:50.77,0:08:52.56,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:08:57.45,0:09:00.32,Default,,0000,0000,0000,,Cậu xấu tính ghê.
Dialogue: 0,0:09:01.23,0:09:03.40,Default,,0000,0000,0000,,Đầu vú các cậu nứng lên rồi kìa...
Dialogue: 0,0:09:03.40,0:09:05.86,Default,,0000,0000,0000,,Cả ba người đều làm rất tuyệt.
Dialogue: 0,0:09:05.98,0:09:10.32,Default,,0000,0000,0000,,Làm mạnh nữa lên, hãy để tinh dịch của mình bắn lên ngực các cậu!
Dialogue: 0,0:09:10.44,0:09:15.00,Default,,0000,0000,0000,,Mình đang rất mong chờ tinh dịch của Chủ nhân! Thật sự đấy!
Dialogue: 0,0:09:17.32,0:09:19.97,Default,,0000,0000,0000,,Honomi, làm vậy...
Dialogue: 0,0:09:26.84,0:09:30.04,Default,,0000,0000,0000,,Cám ơn Chủ nhân.
Dialogue: 0,0:09:36.35,0:09:38.64,Default,,0000,0000,0000,,Thế này hơi sớm.
Dialogue: 0,0:09:39.19,0:09:40.98,Default,,0000,0000,0000,,Lại đây nào, Honomi.
Dialogue: 0,0:09:41.36,0:09:43.60,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:09:49.22,0:09:51.51,Default,,0000,0000,0000,,Chà, mềm thật đấy.
Dialogue: 0,0:09:51.51,0:09:53.82,Default,,0000,0000,0000,,Hai bầu ngực tuyệt hảo.
Dialogue: 0,0:09:54.18,0:09:56.74,Default,,0000,0000,0000,,Chủ nhân...
Dialogue: 0,0:09:59.79,0:10:04.38,Default,,0000,0000,0000,,Chủ nhân, cám ơn cậu, cám ơn cậu vô cùng!
Dialogue: 0,0:10:05.04,0:10:06.98,Default,,0000,0000,0000,,Tụi mình thực sự ghen tị với cậu đó, Honomi!
Dialogue: 0,0:10:06.98,0:10:09.28,Default,,0000,0000,0000,,Chủ nhân ân sủng cậu quá nhiều.
Dialogue: 0,0:10:10.46,0:10:14.05,Default,,0000,0000,0000,,Nếu cậu bú chúng như vậy...
Dialogue: 0,0:10:14.36,0:10:17.60,Default,,0000,0000,0000,,Nó sẽ vọt ra mất... Nó vọt ra mất!
Dialogue: 0,0:10:20.28,0:10:23.43,Default,,0000,0000,0000,,Đợi đã, như vậy thật lãng phí.
Dialogue: 0,0:10:24.00,0:10:26.56,Default,,0000,0000,0000,,Ngon lắm, Honomi.
Dialogue: 0,0:10:26.63,0:10:28.08,Default,,0000,0000,0000,,Ngài nói phải.
Dialogue: 0,0:10:28.16,0:10:30.16,Default,,0000,0000,0000,,Không đúng... Không đúng...
Dialogue: 0,0:10:30.16,0:10:32.31,Default,,0000,0000,0000,,Mình chưa có thai mà!
Dialogue: 0,0:10:32.64,0:10:34.82,Default,,0000,0000,0000,,Chưa có thai hả?
Dialogue: 0,0:10:35.10,0:10:36.92,Default,,0000,0000,0000,,Chủ nhân?
Dialogue: 0,0:10:37.11,0:10:39.14,Default,,0000,0000,0000,,Nếu cậu chưa có thai,
Dialogue: 0,0:10:39.40,0:10:42.27,Default,,0000,0000,0000,,mình sẽ làm cậu mang thai ngay lập tức.
Dialogue: 0,0:10:42.48,0:10:44.58,Default,,0000,0000,0000,,Nào, Sae, Erina.
Dialogue: 0,0:10:44.58,0:10:48.52,Default,,0000,0000,0000,,Tuân lệnh Chủ nhân, mọi công đoạn đều đã chuẩn bị xong.
Dialogue: 0,0:10:50.76,0:10:55.78,Default,,0000,0000,0000,,Chủ nhân, xin cậu hãy lấy đi trinh tiết của mình.
Dialogue: 0,0:10:56.47,0:11:00.08,Default,,0000,0000,0000,,Theo nguyện vọng của cậu, Honomi.
Dialogue: 0,0:11:06.40,0:11:09.89,Default,,0000,0000,0000,,Nó đâm vào bên trong rồi...
Dialogue: 0,0:11:12.52,0:11:15.91,Default,,0000,0000,0000,,Kaede-san, cậu muốn chuyện đó lắm phải không?
Dialogue: 0,0:11:16.02,0:11:18.44,Default,,0000,0000,0000,,Không hẳn vậy.
Dialogue: 0,0:11:21.80,0:11:26.72,Default,,0000,0000,0000,,Chủ nhân, hãy chia sẻ hai bầu vú của Honomi với chúng em.
Dialogue: 0,0:11:26.86,0:11:29.73,Default,,0000,0000,0000,,Được thôi.
Dialogue: 0,0:11:33.86,0:11:38.80,Default,,0000,0000,0000,,Đừng bú nữa, nếu không ngực mình sẽ...
Dialogue: 0,0:11:39.16,0:11:41.43,Default,,0000,0000,0000,,Honomi... Honomi...
Dialogue: 0,0:11:42.14,0:11:45.03,Default,,0000,0000,0000,,Sae,đừng bỏ mặc Erina như vậy chứ.
Dialogue: 0,0:11:44.84,0:11:45.93,Default,,0000,0000,0000,,Vâng!
Dialogue: 0,0:11:48.35,0:11:51.62,Default,,0000,0000,0000,,Erina-chan, nhìn này.
Dialogue: 0,0:11:52.55,0:11:54.99,Default,,0000,0000,0000,,Sae-chan, thứ này tuyệt thật đấy.
Dialogue: 0,0:11:55.66,0:12:00.74,Default,,0000,0000,0000,,Chỉ cần bôi trơn nó một chút, chúng ta sẽ được cùng nhau tận hưởng hoan lạc.
Dialogue: 0,0:12:02.34,0:12:07.69,Default,,0000,0000,0000,,Chủ nhân! Tuyệt quá đi mất! Tuyệt quá đi!
Dialogue: 0,0:12:08.96,0:12:10.75,Default,,0000,0000,0000,,Erina!
Dialogue: 0,0:12:12.32,0:12:15.26,Default,,0000,0000,0000,,Sướng quá đi! Sướng quá đi mất!
Dialogue: 0,0:12:17.29,0:12:19.87,Default,,0000,0000,0000,,Mình cũng vậy!
Dialogue: 0,0:12:26.93,0:12:29.49,Default,,0000,0000,0000,,Mau thú nhận đi.
Dialogue: 0,0:12:29.49,0:12:31.76,Default,,0000,0000,0000,,Ghen tị rồi phải không, Kaede-san?
Dialogue: 0,0:12:31.76,0:12:34.72,Default,,0000,0000,0000,,À thì... Mình chỉ...
Dialogue: 0,0:12:34.72,0:12:40.11,Default,,0000,0000,0000,,Chủ nhân, mình sắp ra... Hãy ra cùng nhau nào...
Dialogue: 0,0:12:43.96,0:12:45.91,Default,,0000,0000,0000,,Được thôi, Honomi.
Dialogue: 0,0:12:46.06,0:12:49.81,Default,,0000,0000,0000,,Hãy sẵn sàng đón nhận tinh dịch của mình bắn vào bên trong tử cung cậu!
Dialogue: 0,0:12:51.19,0:12:54.20,Default,,0000,0000,0000,,Bọn em cũng sắp ra rồi!
Dialogue: 0,0:12:55.94,0:12:57.83,Default,,0000,0000,0000,,Honomi...
Dialogue: 0,0:12:57.92,0:13:00.57,Default,,0000,0000,0000,,Hãy sinh con cho mình!
Dialogue: 0,0:13:17.12,0:13:19.43,Default,,0000,0000,0000,,Xem nào...
Dialogue: 0,0:13:20.96,0:13:23.78,Default,,0000,0000,0000,,Tới lúc rồi.
Dialogue: 0,0:13:25.33,0:13:28.87,Default,,0000,0000,0000,,Kaede-san, nếu chỗ này đã ướt đẫm như vậy, hẳn cậu đang rất thèm muốn phải không?!
Dialogue: 0,0:13:28.94,0:13:31.75,Default,,0000,0000,0000,,Dừng lại! Cậu là đồ tồi...
Dialogue: 0,0:13:34.95,0:13:36.89,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:13:37.36,0:13:39.20,Default,,0000,0000,0000,,Dừng lại đi!
Dialogue: 0,0:13:39.25,0:13:42.33,Default,,0000,0000,0000,,Mình rất khoái nghe cậu van xin đó...
Dialogue: 0,0:13:42.40,0:13:44.46,Default,,0000,0000,0000,,Làm như mình...
Dialogue: 0,0:13:44.96,0:13:47.20,Default,,0000,0000,0000,,Coi nào, mình muốn nghe điều đó.
Dialogue: 0,0:13:48.92,0:13:50.64,Default,,0000,0000,0000,,Làm ơn...
Dialogue: 0,0:13:50.73,0:13:53.27,Default,,0000,0000,0000,,Mình không nghe rõ.
Dialogue: 0,0:13:55.96,0:14:00.69,Default,,0000,0000,0000,,Chủ nhân, làm ơn...
Dialogue: 0,0:14:01.00,0:14:02.77,Default,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,0:14:04.25,0:14:06.49,Default,,0000,0000,0000,,Cậu thật đáng yêu, Kaede.
Dialogue: 0,0:14:06.61,0:14:09.29,Default,,0000,0000,0000,,Này Crocus. Tiến hành đi.
Dialogue: 0,0:14:16.33,0:14:19.10,Default,,0000,0000,0000,,Khoan đã, chuyện này...
Dialogue: 0,0:14:22.44,0:14:26.38,Default,,0000,0000,0000,,Âm hộ của cậu đang thèm khát nó.
Dialogue: 0,0:14:26.40,0:14:27.72,Default,,0000,0000,0000,,Cậu im đi.
Dialogue: 0,0:14:27.72,0:14:30.13,Default,,0000,0000,0000,,Thế này chẳng quá thuận tiện cho mình hành sự sao?
Dialogue: 0,0:14:38.65,0:14:39.80,Default,,0000,0000,0000,,Đau lắm hả?
Dialogue: 0,0:14:39.80,0:14:42.33,Default,,0000,0000,0000,,Cậu muốn nói gì cũng được.
Dialogue: 0,0:14:42.33,0:14:44.39,Default,,0000,0000,0000,,Thật chẳng dễ thương chút nào.
Dialogue: 0,0:14:45.08,0:14:47.86,Default,,0000,0000,0000,,Một câu "Tuyệt quá đi mất" khó nói đến vậy sao!?
Dialogue: 0,0:14:47.92,0:14:50.08,Default,,0000,0000,0000,,Mình thà chết còn hơn nói câu đó!
Dialogue: 0,0:14:50.14,0:14:52.58,Default,,0000,0000,0000,,Miệng lưỡi ghê quá nhỉ.
Dialogue: 0,0:14:52.64,0:14:56.52,Default,,0000,0000,0000,,Cứ thế này thì còn bị phạt dài dài.
Dialogue: 0,0:14:57.25,0:14:58.49,Default,,0000,0000,0000,,Sae?
Dialogue: 0,0:14:58.72,0:15:03.15,Default,,0000,0000,0000,,Mình sẽ dùng nó để khiến cậu tỏ ra ngoan ngoãn hơn.
Dialogue: 0,0:15:03.27,0:15:06.42,Default,,0000,0000,0000,,Không! Đừng đâm vào chỗ đó!
Dialogue: 0,0:15:06.60,0:15:07.93,Default,,0000,0000,0000,,Kaede...
Dialogue: 0,0:15:15.77,0:15:17.66,Default,,0000,0000,0000,,Nó đang khít lại.
Dialogue: 0,0:15:18.15,0:15:22.60,Default,,0000,0000,0000,,Erina, Honomi, lại đây chung vui nào.
Dialogue: 0,0:15:23.09,0:15:24.05,Default,,0000,0000,0000,,Nào!
Dialogue: 0,0:15:24.14,0:15:27.92,Default,,0000,0000,0000,,Vâng, Chủ nhân, em tới ngay đây.
Dialogue: 0,0:15:29.89,0:15:33.12,Default,,0000,0000,0000,,Erina... Honomi...
Dialogue: 0,0:15:36.30,0:15:39.70,Default,,0000,0000,0000,,Nào, chúng ta sẽ cùng nhau vui vẻ.
Dialogue: 0,0:15:39.70,0:15:43.08,Default,,0000,0000,0000,,Niềm vui của các cậu cũng chính là niềm vui của mình.
Dialogue: 0,0:15:43.72,0:15:47.40,Default,,0000,0000,0000,,Này Crocus, họ vẫn chưa thỏa mãn.
Dialogue: 0,0:15:56.64,0:15:58.94,Default,,0000,0000,0000,,Một cảnh tượng tuyệt vời.
Dialogue: 0,0:15:58.94,0:16:00.89,Default,,0000,0000,0000,,Kaede, cả cậu nữa.
Dialogue: 0,0:16:01.00,0:16:02.24,Default,,0000,0000,0000,,Không!
Dialogue: 0,0:16:02.72,0:16:05.53,Default,,0000,0000,0000,,Dù miệng nói không, cậu vẫn rất thèm muốn.
Dialogue: 0,0:16:05.70,0:16:07.19,Default,,0000,0000,0000,,Không! Dừng lại đi!
Dialogue: 0,0:16:07.49,0:16:10.04,Default,,0000,0000,0000,,Cậu muốn mình dừng lại thật sao, Kaede?
Dialogue: 0,0:16:10.73,0:16:14.88,Default,,0000,0000,0000,,Nếu vậy, mình sẽ bắn thật nhiều vào bên trong cậu.
Dialogue: 0,0:16:15.03,0:16:16.75,Default,,0000,0000,0000,,Mình sắp ra rồi đây.
Dialogue: 0,0:16:17.66,0:16:20.51,Default,,0000,0000,0000,,Không! Không được! Đừng ra bên trong!
Dialogue: 0,0:16:20.69,0:16:22.17,Default,,0000,0000,0000,,Thôi nào.
Dialogue: 0,0:16:22.93,0:16:24.83,Default,,0000,0000,0000,,Cậu thực sự muốn mình ra bên trong, phải không
Dialogue: 0,0:16:24.98,0:16:27.18,Default,,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:16:27.41,0:16:29.80,Default,,0000,0000,0000,,Hãy sinh con cho mình, Kaede!
Dialogue: 0,0:16:34.97,0:16:38.65,Default,,0000,0000,0000,,Tất cả các cậu, hãy sinh con cho mình!
Dialogue: 0,0:16:42.80,0:16:44.23,Default,,0000,0000,0000,,Coi nào, mọi người.
Dialogue: 0,0:16:44.35,0:16:47.51,Default,,0000,0000,0000,,Cuộc vui tạm dừng tại đây.
Dialogue: 0,0:16:48.27,0:16:50.87,Default,,0000,0000,0000,,Mình cần sự giúp đỡ của tất cả các cậu.
Dialogue: 0,0:16:52.17,0:16:54.54,Default,,0000,0000,0000,,Chúng ta sẽ chinh phục thế giới!
Dialogue: 0,0:16:54.88,0:16:59.07,Default,,0000,0000,0000,,Chinh phục thế giới! Không gì có thể cản bước chúng ta!
Dialogue: 0,0:16:59.16,0:17:01.99,Default,,0000,0000,0000,,Chỉ cần nỗ lực hết mình, chúng ta sẽ vượt qua mọi thử thách!
Dialogue: 0,0:17:02.32,0:17:07.10,Default,,0000,0000,0000,,{\fnUVN Nguyen Du\fs40\shad0\c&HFCFCFC&\3c&H09140E&\pos(324,241)}Số mục tiêu chưa bị bắn hạ
Dialogue: 0,0:17:19.22,0:17:22.83,Default,,0000,0000,0000,,Này lớp trưởng, cậu làm xong chưa?
Dialogue: 0,0:17:23.22,0:17:25.55,Default,,0000,0000,0000,,Này! Đừng gọi mình là lớp trưởng chứ.
Dialogue: 0,0:17:28.15,0:17:30.02,Default,,0000,0000,0000,,Bọn mình đều xong cả rồi.
Dialogue: 0,0:17:30.21,0:17:32.08,Default,,0000,0000,0000,,Nói mới nhớ, còn cậu thì sao?
Dialogue: 0,0:17:32.12,0:17:33.38,Default,,0000,0000,0000,,Cậu làm xong chưa?
Dialogue: 0,0:17:33.38,0:17:34.19,Default,,0000,0000,0000,,Mình hả?
Dialogue: 0,0:17:34.23,0:17:37.78,Default,,0000,0000,0000,,Tất nhiên rồi, chỉ là một học phần nhỏ thôi mà.
Dialogue: 0,0:17:39.32,0:17:41.00,Default,,0000,0000,0000,,Cậu siêu ghê ha!
Dialogue: 0,0:17:41.09,0:17:43.36,Default,,0000,0000,0000,,Tagami-kun, cậu thật đa tài nhỉ!
Dialogue: 0,0:17:43.40,0:17:45.18,Default,,0000,0000,0000,,Không, đừng nói vậy...
Dialogue: 0,0:17:45.34,0:17:48.57,Default,,0000,0000,0000,,Sayaka, cậu hoàn thành học phần tốt nghiệp chưa?
Dialogue: 0,0:17:49.27,0:17:50.92,Default,,0000,0000,0000,,Đưa mình xem nào! Đưa mình xem nào!
Dialogue: 0,0:17:51.15,0:17:53.11,Default,,0000,0000,0000,,Cậu vẽ Tatsuya-san phải không?
Dialogue: 0,0:17:53.14,0:17:54.60,Default,,0000,0000,0000,,Ờ, nhưng mà...
Dialogue: 0,0:17:55.41,0:17:59.22,Default,,0000,0000,0000,,Mình cũng muốn Tatsuya-san làm mẫu cho mình.
Dialogue: 0,0:17:59.27,0:18:02.64,Default,,0000,0000,0000,,Chà, xui cho cậu là Sayaka đã nhờ cậu ấy làm mẫu trước rồi.
Dialogue: 0,0:18:03.46,0:18:04.79,Default,,0000,0000,0000,,Mình vẫn chưa hoàn thành.
Dialogue: 0,0:18:05.27,0:18:07.66,Default,,0000,0000,0000,,Bức vẽ này vẫn chưa được ưng ý cho lắm.
Dialogue: 0,0:18:07.82,0:18:10.26,Default,,0000,0000,0000,,Khi vẽ xong mình sẽ mời các cậu tới chiêm ngưỡng.
Dialogue: 0,0:18:10.32,0:18:11.32,Default,,0000,0000,0000,,Mình xin phép.
Dialogue: 0,0:18:11.35,0:18:12.28,Default,,0000,0000,0000,,Sayaka?
Dialogue: 0,0:18:12.31,0:18:15.37,Default,,0000,0000,0000,,Mình cá là cô ấy theo trường phái hoàn mĩ.
Dialogue: 0,0:18:15.58,0:18:17.56,Default,,0000,0000,0000,,Tatsuya, mình bắt đầu thấy ghen tị rồi đấy.
Dialogue: 0,0:18:17.61,0:18:18.91,Default,,0000,0000,0000,,Các cậu cứ nói quá.
Dialogue: 0,0:18:22.75,0:18:24.09,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:18:25.94,0:18:27.93,Default,,0000,0000,0000,,Là rắc rối tình yêu hả?
Dialogue: 0,0:18:28.11,0:18:29.50,Default,,0000,0000,0000,,À thì...
Dialogue: 0,0:18:30.66,0:18:32.65,Default,,0000,0000,0000,,Để tôi xem đường tình duyên cho cô nhé.
Dialogue: 0,0:18:32.75,0:18:34.16,Default,,0000,0000,0000,,T-Thật hả?
Dialogue: 0,0:18:34.22,0:18:36.79,Default,,0000,0000,0000,,Đến tìm tôi tại câu lạc bộ sau giờ học.
Dialogue: 0,0:18:36.85,0:18:38.58,Default,,0000,0000,0000,,Cám ơn, Yuko!
Dialogue: 0,0:18:51.34,0:18:52.64,Default,,0000,0000,0000,,Nhìn thấy rồi.
Dialogue: 0,0:18:55.83,0:18:58.51,Default,,0000,0000,0000,,Bạn đời tương lai của cô chính là Tatsuya.
Dialogue: 0,0:18:59.99,0:19:04.00,Default,,0000,0000,0000,,Hãy hành động theo lời mách bảo của trái tim cô...
Dialogue: 0,0:19:04.18,0:19:08.47,Default,,0000,0000,0000,,Đêm nay, trong lớp học...
Dialogue: 0,0:19:10.02,0:19:13.86,Default,,0000,0000,0000,,Có điều...
Dialogue: 0,0:19:14.52,0:19:15.80,Default,,0000,0000,0000,,Điều gì?
Dialogue: 0,0:19:15.94,0:19:19.49,Default,,0000,0000,0000,,Chuyện đó sẽ phá hỏng tình bạn của ba người.
Dialogue: 0,0:19:30.53,0:19:31.73,Default,,0000,0000,0000,,Cám ơn cô.
Dialogue: 0,0:19:31.79,0:19:33.10,Default,,0000,0000,0000,,Chúc may mắn.
Dialogue: 0,0:19:36.69,0:19:40.39,Default,,0000,0000,0000,,Tatsuya, đừng thô bạo như vậy...
Dialogue: 0,0:19:42.01,0:19:44.31,Default,,0000,0000,0000,,Cô đóng kịch khá lắm.
Dialogue: 0,0:19:47.31,0:19:49.44,Default,,0000,0000,0000,,Tôi sẽ thưởng công cho cô thật hậu hĩnh.
Dialogue: 0,0:19:50.30,0:19:51.83,Default,,0000,0000,0000,,Tatsuya!
Dialogue: 0,0:19:52.07,0:19:54.04,Default,,0000,0000,0000,,Cô dâm quá đi thôi.
Dialogue: 0,0:19:54.30,0:19:57.05,Default,,0000,0000,0000,,Để tôi xem cô dâm đãng tới mức nào.
Dialogue: 0,0:19:57.14,0:20:01.50,Default,,0000,0000,0000,,Tatsuya... Anh đang đâm sâu vào bên trong tôi.
Dialogue: 0,0:20:12.63,0:20:13.90,Default,,0000,0000,0000,,Giỏi lắm.
Dialogue: 0,0:20:14.52,0:20:16.08,Default,,0000,0000,0000,,Đúng lúc thật đấy, Yuko.
Dialogue: 0,0:20:16.09,0:20:17.68,Default,,0000,0000,0000,,Tatsuya!
Dialogue: 0,0:20:18.84,0:20:20.42,Default,,0000,0000,0000,,Mạnh nữa lên! Mạnh nữa lên!
Dialogue: 0,0:20:20.42,0:20:22.56,Default,,0000,0000,0000,,Tôi sắp...
Dialogue: 0,0:20:25.20,0:20:26.89,Default,,0000,0000,0000,,Cùng ra nhé, Yuko!
Dialogue: 0,0:20:27.46,0:20:30.27,Default,,0000,0000,0000,,Tatsuya! Tatsuya! Tatsuya!
Dialogue: 0,0:20:32.14,0:20:37.01,Default,,0000,0000,0000,,{\p1\fscx907.5\fscy393.75\pos(538,146)}m 42 -15 l -24 -15 l -24 3 l 42 3
Dialogue: 0,0:20:32.14,0:20:37.01,Default,,0000,0000,0000,,{\fnUVNAi Cap Nang\fs18\pos(318,49)}Tôi có chuyện cần nói với cậu, đợi cậu ở phòng học! Đến hay không thì tùy!
Dialogue: 0,0:20:32.14,0:20:37.01,Default,,0000,0000,0000,,{\fs20\fnUVN Ai Cap Nang\pos(517,73)}-Yumeno Kanae
Dialogue: 0,0:20:32.15,0:20:37.01,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:20:54.77,0:20:56.11,Default,,0000,0000,0000,,Cám ơn cậu!
Dialogue: 0,0:20:56.26,0:20:58.71,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:20:56.62,0:20:58.68,Default,,0000,0000,0000,,Khó thở quá!
Dialogue: 0,0:20:58.87,0:21:00.92,Default,,0000,0000,0000,,Tacchan, Kanae-chan!
Dialogue: 0,0:21:01.10,0:21:04.62,Default,,0000,0000,0000,,Chuyện đó sẽ phá hỏng tình bạn của ba người.
Dialogue: 0,0:21:12.78,0:21:13.93,Default,,0000,0000,0000,,Được rồi.
Dialogue: 0,0:21:27.17,0:21:28.51,Default,,0000,0000,0000,,Cậu đang làm gì vậy?
Dialogue: 0,0:21:29.72,0:21:31.45,Default,,0000,0000,0000,,T-Tập luyện.
Dialogue: 0,0:21:33.36,0:21:36.25,Default,,0000,0000,0000,,Cậu thích Sayaka phải không?
Dialogue: 0,0:21:38.03,0:21:39.19,Default,,0000,0000,0000,,À thì...
Dialogue: 0,0:21:40.37,0:21:43.16,Default,,0000,0000,0000,,Hiểu rồi, tôi nói đúng chứ gì?!
Dialogue: 0,0:21:43.51,0:21:47.12,Default,,0000,0000,0000,,Nhưng mà tôi...
Dialogue: 0,0:21:48.94,0:21:50.51,Default,,0000,0000,0000,,Tôi yêu cậu!
Dialogue: 0,0:21:53.87,0:21:55.92,Default,,0000,0000,0000,,Uhm...
Dialogue: 0,0:21:58.10,0:22:00.06,Default,,0000,0000,0000,,Mình để quên nó ở lớp sao?
Dialogue: 0,0:22:02.13,0:22:04.24,Default,,0000,0000,0000,,K-Kanae...
Dialogue: 0,0:22:07.08,0:22:12.59,Default,,0000,0000,0000,,Dù chỉ được xem như vật thay thế, tôi vẫn muốn được làm chuyện đó cùng cậu.
Dialogue: 0,0:22:12.77,0:22:15.67,Default,,0000,0000,0000,,Chỉ lần này thôi...
Dialogue: 0,0:22:18.04,0:22:20.44,Default,,0000,0000,0000,,Mình không bao giờ coi cậu là thế thân của Minase.
Dialogue: 0,0:22:21.04,0:22:23.76,Default,,0000,0000,0000,,Minase là Minase, còn cậu là cậu.
Dialogue: 0,0:22:23.84,0:22:25.56,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:22:28.65,0:22:34.52,Default,,0000,0000,0000,,Mình muốn được làm chuyện đó với Yumeko Kaede đích thực.
Dialogue: 0,0:22:37.86,0:22:39.22,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:22:42.03,0:22:44.56,Default,,0000,0000,0000,,Tatsuya... Tatsuya...
Dialogue: 0,0:23:03.02,0:23:05.38,Default,,0000,0000,0000,,Mình vừa ra rồi...
Dialogue: 0,0:23:06.23,0:23:11.83,Default,,0000,0000,0000,,Mình biết cậu yêu Sayaka.
Dialogue: 0,0:23:12.00,0:23:18.26,Default,,0000,0000,0000,,Nhưng khi nhìn thấy bức họa đó, mình đã không kiềm chế nổi bản thân.
Dialogue: 0,0:23:21.48,0:23:23.29,Default,,0000,0000,0000,,Vậy nên cậu tìm mình?
Dialogue: 0,0:23:23.32,0:23:26.05,Default,,0000,0000,0000,,X-Xấu xa! Vậy mà cũng nói ra được.
Dialogue: 0,0:23:26.05,0:23:27.10,Default,,0000,0000,0000,,Mau làm điều đó đi.
Dialogue: 0,0:23:27.10,0:23:29.73,Default,,0000,0000,0000,,Được thôi, mình vào đây.
Dialogue: 0,0:23:33.51,0:23:35.46,Default,,0000,0000,0000,,Thư giãn nào, Kanae.
Dialogue: 0,0:23:43.69,0:23:44.98,Default,,0000,0000,0000,,Cậu không sao chứ?
Dialogue: 0,0:23:45.17,0:23:46.80,Default,,0000,0000,0000,,Uhm không sao...
Dialogue: 0,0:23:48.23,0:23:51.69,Default,,0000,0000,0000,,Thằng nhỏ của cậu thật ấm.
Dialogue: 0,0:23:52.58,0:23:54.20,Default,,0000,0000,0000,,Đừng gắng sức quá.
Dialogue: 0,0:23:56.45,0:23:59.35,Default,,0000,0000,0000,,Kanae, bên trong cậu tuyệt quá đi mất.
Dialogue: 0,0:23:59.45,0:24:04.77,Default,,0000,0000,0000,,Tatsuya, thằng nhỏ của cậu vẫn đang bự lên.
Dialogue: 0,0:24:07.67,0:24:09.37,Default,,0000,0000,0000,,Tuyệt quá đi mất.
Dialogue: 0,0:24:12.17,0:24:15.14,Default,,0000,0000,0000,,Tatsuya, thằng nhỏ của cậu thật dũng mãnh!
Dialogue: 0,0:24:23.28,0:24:24.94,Default,,0000,0000,0000,,K-Kanae...
Dialogue: 0,0:24:25.10,0:24:27.45,Default,,0000,0000,0000,,Nữa đi... Nữa đi...
Dialogue: 0,0:24:28.51,0:24:31.05,Default,,0000,0000,0000,,Chúng ta đã hòa làm một.
Dialogue: 0,0:24:32.87,0:24:37.32,Default,,0000,0000,0000,,Mình đã thầm yêu cậu từ rất lâu rồi.
Dialogue: 0,0:24:37.83,0:24:40.68,Default,,0000,0000,0000,,Vì vậy mà mình luôn ghen tị với Sayaka!
Dialogue: 0,0:24:41.62,0:24:44.67,Default,,0000,0000,0000,,Cô ấy luôn ở bên cậu...
Dialogue: 0,0:24:46.95,0:24:48.91,Default,,0000,0000,0000,,Còn mình thì...
Dialogue: 0,0:24:52.60,0:24:55.29,Default,,0000,0000,0000,,Không phải mình đã nói rồi sao? Hãy luôn là chính mình.
Dialogue: 0,0:24:55.41,0:24:56.89,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:25:00.25,0:25:04.04,Default,,0000,0000,0000,,Kanae, mình muốn ngắm kĩ gương mặt cậu.
Dialogue: 0,0:25:12.62,0:25:16.59,Default,,0000,0000,0000,,Tatsuya, mình muốn được ở bên cậu mãi mãi.
Dialogue: 0,0:25:16.90,0:25:20.37,Default,,0000,0000,0000,,Tatsuya! Thằng nhỏ của cậu lai bự lên nữa rồi!
Dialogue: 0,0:25:20.77,0:25:24.01,Default,,0000,0000,0000,,Âm hộ của mình sẽ rách toạc ra mất!
Dialogue: 0,0:25:24.09,0:25:27.76,Default,,0000,0000,0000,,Kanae, mình sẽ bắn vào bên trong cậu.
Dialogue: 0,0:25:28.15,0:25:29.75,Default,,0000,0000,0000,,Được thôi...
Dialogue: 0,0:25:29.75,0:25:32.36,Default,,0000,0000,0000,,Chỉ cần người đó là Tatsuya, mình nguyện chấp nhận tất cả.
Dialogue: 0,0:25:32.47,0:25:36.16,Default,,0000,0000,0000,,Tatsuya! Mình sắp ra rồi! Mình sắp ra rồi!
Dialogue: 0,0:25:36.61,0:25:37.92,Default,,0000,0000,0000,,Mình ra đây, Kanae.
Dialogue: 0,0:25:37.95,0:25:40.16,Default,,0000,0000,0000,,Hãy nhận lấy giọt máu của mình!
Dialogue: 0,0:25:40.22,0:25:43.09,Default,,0000,0000,0000,,Hãy sinh cho mình một đứa con!
Dialogue: 0,0:26:06.02,0:26:09.13,Default,,0000,0000,0000,,Kanae, mình cần sự giúp đỡ của cậu.
Dialogue: 0,0:26:09.22,0:26:12.68,Default,,0000,0000,0000,,Mình sẽ làm mọi thứ vì cậu.
Dialogue: 0,0:26:13.82,0:26:16.22,Default,,0000,0000,0000,,Giờ thì Kanae đã về phe chúng ta.
Dialogue: 0,0:26:16.73,0:26:21.09,Default,,0000,0000,0000,,Lẽ ra cô ấy đã vào phe chúng ta sớm hơn, chỉ cần cậu ta sử dụng phát minh của tôi.
Dialogue: 0,0:26:21.18,0:26:24.03,Default,,0000,0000,0000,,Tình yêu tay ba quả là phiền phức.
Dialogue: 0,0:26:36.42,0:26:38.39,Default,,0000,0000,0000,,Chỉ còn 4 em.
