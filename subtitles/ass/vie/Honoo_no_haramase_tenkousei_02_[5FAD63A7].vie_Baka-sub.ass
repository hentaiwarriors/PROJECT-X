Dialogue: 0,0:00:05.46,0:00:08.72,Default,,0000,0000,0000,,Sao số mình đen thế nhỉ?
Dialogue: 0,0:00:09.15,0:00:11.14,Default,,0000,0000,0000,,Vừa nói cái gì đấy?
Dialogue: 0,0:00:12.25,0:00:13.09,Default,,0000,0000,0000,,Tha cho mình!
Dialogue: 0,0:00:13.13,0:00:14.95,Default,,0000,0000,0000,,Khó thở quá...
Dialogue: 0,0:00:14.98,0:00:19.14,Default,,0000,0000,0000,,Tacchan, đừng vặn vẹo nữa, cứ vậy thì mình vẽ làm sao được.
Dialogue: 0,0:00:19.18,0:00:22.21,Default,,0000,0000,0000,,Ngồi yên nào, Tatsuya.
Dialogue: 0,0:00:23.19,0:00:28.94,Default,,0000,0000,0000,,Kêu gào vô ích, Sayaka đang làm học phần tốt nghiệp cho cả nhóm...
Dialogue: 0,0:00:28.99,0:00:30.68,Default,,0000,0000,0000,,Vậy nên hãy tỏ ra ngoan ngoãn một chút đi.
Dialogue: 0,0:00:30.71,0:00:33.66,Default,,0000,0000,0000,,Bởi vì mình rất thông cảm cho Sayaka khi cậu ấy phải làm người mẫu cho cậu.
Dialogue: 0,0:00:33.72,0:00:36.04,Default,,0000,0000,0000,,Thế kia mà cũng gọi là học phần tốt nghiệp á,
Dialogue: 0,0:00:38.59,0:00:44.61,Default,,0000,0000,0000,,Thà xóa đi vẽ lại còn hơn là ngồi chỉnh sửa cái của nợ kia.
Dialogue: 0,0:00:44.69,0:00:46.31,Default,,0000,0000,0000,,Câm miệng!
Dialogue: 0,0:00:46.37,0:00:49.17,Default,,0000,0000,0000,,Đừng mà... Đừng mà!
Dialogue: 0,0:00:56.38,0:00:58.03,Default,,0000,0000,0000,,Đừng khó tính như vậy, Tacchan.
Dialogue: 0,0:00:58.08,0:01:01.64,Default,,0000,0000,0000,,Bức họa của Kanae có ẩn ý riêng đó.
Dialogue: 0,0:01:02.03,0:01:04.91,Default,,0000,0000,0000,,Cả Sayaka cũng đá xoáy mình sao.
Dialogue: 0,0:01:06.86,0:01:09.02,Default,,0000,0000,0000,,Ẩn ý riêng ấy hả?
Dialogue: 0,0:01:09.07,0:01:12.18,Default,,0000,0000,0000,,Mình nhớ có lần chúng ta tới công viên vẽ phác thảo,
Dialogue: 0,0:01:12.25,0:01:14.58,Default,,0000,0000,0000,,nhưng thay vào đó, một người lại vẽ ra ác quỷ.
Dialogue: 0,0:01:14.70,0:01:16.41,Default,,0000,0000,0000,,Tatsuya...
Dialogue: 0,0:01:16.69,0:01:19.02,Default,,0000,0000,0000,,Còn cậu thì sao hả!?
Dialogue: 0,0:01:19.09,0:01:25.10,Default,,0000,0000,0000,,Nếu cậu dám vẽ chân dung mình không ra cái hồn người, mình sẽ xử đẹp cậu.
Dialogue: 0,0:01:35.03,0:01:37.43,Default,,0000,0000,0000,,L-Là mình đây sao?
Dialogue: 0,0:01:38.59,0:01:41.16,Default,,0000,0000,0000,,Trông dễ thương ghê ha.
Dialogue: 0,0:01:43.20,0:01:49.30,Default,,0000,0000,0000,,Chứng tỏ cậu rất căm ghét mình hoặc có âm mưu gì khác phải không?
Dialogue: 0,0:01:51.78,0:01:55.22,Default,,0000,0000,0000,,Không... Mình vẽ ác quỷ tài không kém gì cậu đâu.
Dialogue: 0,0:01:53.27,0:01:55.24,Default,,0000,0000,0000,,Ác quỷ hả?
Dialogue: 0,0:01:55.29,0:01:56.74,Default,,0000,0000,0000,,Thôi nào, có gì mà phải xấu hổ chứ.
Dialogue: 0,0:01:56.86,0:01:58.68,Default,,0000,0000,0000,,Mình đâu có xấu hổ...
Dialogue: 0,0:01:58.80,0:02:00.11,Default,,0000,0000,0000,,Rõ ràng là có mà.
Dialogue: 0,0:02:00.11,0:02:01.20,Default,,0000,0000,0000,,Đã bảo là không mà.
Dialogue: 0,0:02:01.20,0:02:02.70,Default,,0000,0000,0000,,Chắc chắn là có.
Dialogue: 0,0:02:02.77,0:02:04.79,Default,,0000,0000,0000,,Đã bảo không rồi mà.
Dialogue: 0,0:02:04.86,0:02:08.54,Default,,0000,0000,0000,,Biết ngay mà, Tacchan từ lâu đã thầm yêu...
Dialogue: 0,0:02:19.50,0:02:22.44,Default,,0000,0000,0000,,{\fad(0,1200)\fs40\bord0\shad0\fnUVN Con Thuy\pos(266,108)\c&H396B85&}3 Người Bạn Thân
Dialogue: 0,0:02:22.22,0:02:25.24,Default,,0000,0000,0000,,Tôi đã chuyển tới ngôi trường này được gần một tháng.
Dialogue: 0,0:02:25.31,0:02:28.89,Default,,0000,0000,0000,,Mặc dù tôi chưa có ý định động tới cô bạn thơ ấu Sayaka,
Dialogue: 0,0:02:28.97,0:02:32.79,Default,,0000,0000,0000,,nhưng tham vọng trong tôi vẫn lớn mạnh không ngừng, các em xinh tươi thì chạy theo càng lúc càng nhiều.
Dialogue: 0,0:02:33.88,0:02:37.71,Default,,0000,0000,0000,,Vào lúc này, để thực hiện hoài bão của mình, tôi sẽ làm cho cả lớp dính bầu!
Dialogue: 0,0:02:48.91,0:02:54.87,Default,,0000,0000,0050,,{fscx160\shad0\bord0\fnUVN Nguyen Du\fad(300,800)\3c&HBF9668&\fs40\pos(302,419)\i1\c&HFCF7F8&}Honoo no Haramase Tenkousei{\i0}
