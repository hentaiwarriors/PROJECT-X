Dialogue: 0,0:00:12.35,0:00:13.71,Default,,0000,0000,0000,,Phiền cô vén áo lên.
Dialogue: 0,0:00:15.35,0:00:16.51,Default,,0000,0000,0000,,Vâng ạ.
Dialogue: 0,0:01:04.37,0:01:06.49,Default,,0000,0000,0000,,Sức khỏe cô vẫn ổn.
Dialogue: 0,0:01:06.73,0:01:08.29,Default,,0000,0000,0000,,Bác sĩ...
Dialogue: 0,0:01:09.67,0:01:12.10,Default,,0000,0000,0000,,Xin bác sĩ hãy khám cả nơi này nữa.
Dialogue: 0,0:01:33.33,0:01:38.26,Default,,0000,0000,0000,,Em cần anh, bác sĩ...
Dialogue: 0,0:01:40.13,0:01:42.33,Default,,0000,0000,0000,,Điều này không có trong hợp đồng bảo hiểm của cô.
Dialogue: 0,0:01:43.54,0:01:45.34,Default,,0000,0000,0000,,E-Em biết chứ!
Dialogue: 0,0:01:47.11,0:01:48.80,Default,,0000,0000,0000,,Được thôi, ta bắt đầu luôn nhỉ...
Dialogue: 0,0:02:15.17,0:02:15.90,Default,,0000,0000,0000,,Bác sĩ! Làm ơn đừng nhìn!
Dialogue: 0,0:02:15.90,0:02:17.31,Default,,0000,0000,0000,,Dr. Shameless... Phần 1\NBác sĩ! Làm ơn đừng nhìn em!
Dialogue: 0,0:02:17.31,0:02:17.41,Default,,0000,0000,0000,,Dr. Shameless... Phần 1
Dialogue: 0,0:02:17.41,0:02:19.04,Default,,0000,0000,0000,,Dr. Shameless... Phần 1\NXấu hổ lắm!
Dialogue: 0,0:02:19.04,0:02:19.97,Default,,0000,0000,0000,,Dr. Shameless... Phần 1
Dialogue: 0,0:02:20.58,0:02:24.24,Default,,0000,0000,0000,,Chuyện tiền bạc!
Dialogue: 0,0:02:28.55,0:02:30.28,Default,,0000,0000,0000,,Bệnh viện Nagatsuka
Dialogue: 0,0:02:39.96,0:02:43.45,Default,,0000,0000,0000,,Nơi tiếp nhận hồ sơ
Dialogue: 0,0:02:44.30,0:02:49.74,Default,,0000,0000,0000,,Nếu tiếp tục áp dụng những quy tắc thông thường, bệnh viện sẽ có nguy cơ phá sản.
Dialogue: 0,0:02:50.44,0:02:53.81,Default,,0000,0000,0000,,Đó là lí do tôi mời bác sĩ tới đây.
Dialogue: 0,0:02:53.87,0:02:56.04,Default,,0000,0000,0000,,Chúng tôi đã nghe rất nhiều về tài năng của bác sĩ.
Dialogue: 0,0:03:04.55,0:03:09.79,Default,,0000,0000,0000,,Hình như đã có nhiều nhân viên ở đây nghỉ việc vì ko nhận được lương.
Dialogue: 0,0:03:11.13,0:03:13.49,Default,,0000,0000,0000,,Liệu ngài có đủ khả năng trả lương cho tôi ko?
Dialogue: 0,0:03:14.16,0:03:15.86,Default,,0000,0000,0000,,Bác sĩ ko phải lo về vấn đề đó.
Dialogue: 0,0:03:15.96,0:03:17.95,Default,,0000,0000,0000,,Tôi vẫn còn tiền.
Dialogue: 0,0:03:18.07,0:03:20.06,Default,,0000,0000,0000,,Tôi vừa thế chấp căn hộ của mình...
Dialogue: 0,0:03:23.30,0:03:24.64,Default,,0000,0000,0000,,Cầu xin bác sĩ!
Dialogue: 0,0:03:25.24,0:03:29.27,Default,,0000,0000,0000,,Một tay tôi đã gây dựng nên nơi này, toàn bộ cuộc đời tôi đều dành cho nó!
Dialogue: 0,0:03:29.48,0:03:31.34,Default,,0000,0000,0000,,Bệnh viện là tất cả những gì tôi có.
Dialogue: 0,0:03:31.45,0:03:33.28,Default,,0000,0000,0000,,Tôi cầu xin bác sĩ!
Dialogue: 0,0:03:33.88,0:03:36.28,Default,,0000,0000,0000,,Cậu là người duy nhất có thể cứu được bệnh viện lúc này...
Dialogue: 0,0:03:37.02,0:03:38.25,Default,,0000,0000,0000,,Xin bác sĩ!
Dialogue: 0,0:03:39.39,0:03:40.55,Default,,0000,0000,0000,,Tốt thôi.
Dialogue: 0,0:03:40.66,0:03:42.78,Default,,0000,0000,0000,,Ngài tìm đúng người rồi đấy.
Dialogue: 0,0:03:43.66,0:03:45.02,Default,,0000,0000,0000,,Tôi chấp nhận đề nghị của ngài.
Dialogue: 0,0:03:45.39,0:03:46.45,Default,,0000,0000,0000,,Bác sĩ đồng ý thật sao?
Dialogue: 0,0:03:46.56,0:03:48.03,Default,,0000,0000,0000,,Cám ơn Bác sĩ!
Dialogue: 0,0:03:50.56,0:03:51.53,Default,,0000,0000,0000,,Với một điều kiện...
Dialogue: 0,0:03:51.60,0:03:52.26,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,0:03:52.67,0:03:56.10,Default,,0000,0000,0000,,Tôi phải được quyền điều hành mọi thứ theo ý mình.
Dialogue: 0,0:03:57.54,0:03:58.63,Default,,0000,0000,0000,,Ngài rõ rồi chứ?
Dialogue: 0,0:04:02.31,0:04:03.24,Default,,0000,0000,0000,,Y tá trưởng đâu nhỉ?
Dialogue: 0,0:04:03.68,0:04:07.24,Default,,0000,0000,0000,,Những người hưởng lương cao nhất thì đều đã nghỉ việc từ lâu, Bác sĩ.
Dialogue: 0,0:04:08.12,0:04:08.98,Default,,0000,0000,0000,,Hiểu rồi...
Dialogue: 0,0:04:09.78,0:04:10.48,Default,,0000,0000,0000,,Vậy cô là...
Dialogue: 0,0:04:10.52,0:04:12.85,Default,,0000,0000,0000,,Nagatsuka
Dialogue: 0,0:04:12.95,0:04:14.55,Default,,0000,0000,0000,,Tôi là con gái của giám đốc.
Dialogue: 0,0:04:16.26,0:04:17.69,Default,,0000,0000,0000,,Tên tôi là Honoka!
Dialogue: 0,0:04:17.79,0:04:19.32,Default,,0000,0000,0000,,Rất vui được làm việc cùng Bác sĩ.
Dialogue: 0,0:04:19.79,0:04:20.76,Default,,0000,0000,0000,,"Honoka?"
Dialogue: 0,0:04:21.03,0:04:22.52,Default,,0000,0000,0000,,Viết như thế nào nhỉ?
Dialogue: 0,0:04:23.93,0:04:26.26,Default,,0000,0000,0000,,"Moe-ru", và "Ka" trong "hương thơm."
Dialogue: 0,0:04:27.60,0:04:28.73,Default,,0000,0000,0000,,Nghe quen quá!
Dialogue: 0,0:04:28.80,0:04:30.79,Default,,0000,0000,0000,,Hình như mình đã nghe cái tên này ở đâu đó...
Dialogue: 0,0:04:31.44,0:04:33.50,Default,,0000,0000,0000,,Bác sĩ này!
Dialogue: 0,0:04:33.64,0:04:36.04,Default,,0000,0000,0000,,Bác sĩ định tán tỉnh Honoka ngay khi mới gặp mặt ư.
Dialogue: 0,0:04:36.11,0:04:38.14,Default,,0000,0000,0000,,"Trước đây tôi từng quen một cô gái cũng tên là Honoka."
Dialogue: 0,0:04:38.21,0:04:40.94,Default,,0000,0000,0000,,Bác sĩ sẽ nói vậy để để bắt chuyện với cô ấy phải ko?
Dialogue: 0,0:04:41.02,0:04:44.18,Default,,0000,0000,0000,,Shirakawa, cô làm vậy là thiếu tôn trọng bác sĩ Ishida đó.
Dialogue: 0,0:04:44.85,0:04:46.08,Default,,0000,0000,0000,,Thật thế ư?
Dialogue: 0,0:04:46.45,0:04:48.55,Default,,0000,0000,0000,,Đành vậy,\Nxin lỗi Bác sĩ nhé!
Dialogue: 0,0:04:58.47,0:05:02.73,Default,,0000,0000,0000,,Honoka mà mình quen cũng tầm tuổi cô ấy.
Dialogue: 0,0:05:07.37,0:05:08.71,Default,,0000,0000,0000,,Nghĩ kĩ lại...
Dialogue: 0,0:05:08.91,0:05:11.34,Default,,0000,0000,0000,,...có gì đó ở cô ấy làm mình nhớ tới Honoka...
Dialogue: 0,0:05:12.61,0:05:14.21,Default,,0000,0000,0000,,Mình suy nghĩ quá nhiều rồi.
Dialogue: 0,0:05:14.62,0:05:17.45,Default,,0000,0000,0000,,Cô ấy là con gái độc nhất của Giám đốc mà.
Dialogue: 0,0:05:18.12,0:05:21.75,Default,,0000,0000,0000,,Cả hai không thể là cùng một người được.
Dialogue: 0,0:05:23.06,0:05:25.75,Default,,0000,0000,0000,,Bác sĩ, anh thấy trường hợp này ra sao?
Dialogue: 0,0:05:28.10,0:05:29.65,Default,,0000,0000,0000,,Higashijuujo...
Dialogue: 0,0:05:29.73,0:05:30.86,Default,,0000,0000,0000,,Nghe quen nhỉ...
Dialogue: 0,0:05:30.93,0:05:33.42,Default,,0000,0000,0000,,Vâng, cô ấy là con cháu gia tộc Higashijuujo thuộc liên minhg Công-Thương nghiệp.
Dialogue: 0,0:05:33.50,0:05:34.33,Default,,0000,0000,0000,,Thú vị nhỉ...
Dialogue: 0,0:05:35.30,0:05:38.29,Default,,0000,0000,0000,,Vậy đây là gì?
Dialogue: 0,0:05:38.71,0:05:41.17,Default,,0000,0000,0000,,Trông giống một bản thống kê các chứng bệnh thông thường.
Dialogue: 0,0:05:41.98,0:05:47.88,Default,,0000,0000,0000,,Với những triệu chứng đó, cô ta ko đủ điều kiện để nhập viện điều trị.
Dialogue: 0,0:05:47.98,0:05:49.18,Default,,0000,0000,0000,,Cô ấy thuộc nhóm có nguy cơ mắc bệnh cao.
Dialogue: 0,0:05:49.35,0:05:50.15,Default,,0000,0000,0000,,Nguy cơ mắc bệnh cao?
Dialogue: 0,0:05:50.95,0:05:53.82,Default,,0000,0000,0000,,Cha mẹ cô ấy thường xuyên phải giải quyết công việc ở nước ngoài.
Dialogue: 0,0:05:54.42,0:05:59.29,Default,,0000,0000,0000,,Có lẽ cô ấy giả bệnh để gây sự chú ý với cha mẹ mình.
Dialogue: 0,0:05:59.76,0:06:00.89,Default,,0000,0000,0000,,Oh, ra vậy...
Dialogue: 0,0:06:02.03,0:06:05.00,Default,,0000,0000,0000,,Cô dám cho tôi ăn thứ tồi tệ này sao?
Dialogue: 0,0:06:05.10,0:06:08.43,Default,,0000,0000,0000,,Oh, Miss Higashijuujo, sao cô quá đáng vậy?
Dialogue: 0,0:06:08.50,0:06:09.94,Default,,0000,0000,0000,,Một vừa hai phải thôi chứ!
Dialogue: 0,0:06:10.50,0:06:12.56,Default,,0000,0000,0000,,Tôi muốn ăn cá nóc!
Dialogue: 0,0:06:12.64,0:06:14.44,Default,,0000,0000,0000,,Cả sashimi và gà rán nữa!
Dialogue: 0,0:06:14.71,0:06:16.68,Default,,0000,0000,0000,,Bệnh viện ko phục vụ những thứ đó, cô biết mà!
Dialogue: 0,0:06:17.21,0:06:20.58,Default,,0000,0000,0000,,Thôi nào, tôi đâu có bị bệnh đường ruột.
Dialogue: 0,0:06:20.68,0:06:23.55,Default,,0000,0000,0000,,Sao tôi lại ko được phép ăn thứ mình thích nhỉ? Tôi sẽ thanh toán đầy đủ cho bệnh viện!
Dialogue: 0,0:06:23.95,0:06:25.04,Default,,0000,0000,0000,,Sao hả?
Dialogue: 0,0:06:25.12,0:06:26.48,Default,,0000,0000,0000,,Tôi ko thể chiều ý cô được...
Dialogue: 0,0:06:26.72,0:06:28.75,Default,,0000,0000,0000,,Nội quy bệnh viện ko cho phép điều đó!
Dialogue: 0,0:06:31.19,0:06:32.06,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:06:32.69,0:06:34.39,Default,,0000,0000,0000,,Bác sĩ!
Dialogue: 0,0:06:34.50,0:06:37.19,Default,,0000,0000,0000,,Cô Higashijuujo đây thật ko biết điều!
Dialogue: 0,0:06:37.26,0:06:42.67,Default,,0000,0000,0000,,Cô ấy ko chịu dùng đồ ăn nóng sốt tôi mang đến, đã vậy còn...
Dialogue: 0,0:06:43.27,0:06:45.17,Default,,0000,0000,0000,,Anh là bác sĩ mới phải ko?
Dialogue: 0,0:06:45.97,0:06:47.03,Default,,0000,0000,0000,,Chính là tôi.
Dialogue: 0,0:06:47.54,0:06:48.84,Default,,0000,0000,0000,,Xin được giới thiệu, tên tôi là Ishida.
Dialogue: 0,0:06:48.91,0:06:49.77,Default,,0000,0000,0000,,Rất vui được gặp cô.
Dialogue: 0,0:06:53.65,0:06:55.74,Default,,0000,0000,0000,,Một tiểu thư kiêu kì nữa hả?
Dialogue: 0,0:06:55.82,0:06:57.72,Default,,0000,0000,0000,,Điều trị cho cô ta hẳn sẽ vui lắm đây...
Dialogue: 0,0:06:58.29,0:07:00.98,Default,,0000,0000,0000,,Cho phép tôi kiểm tra tình trạng cơ thể cô nhé?
Dialogue: 0,0:07:01.06,0:07:03.29,Default,,0000,0000,0000,,Phiền hai cô ra ngoài một lúc.
Dialogue: 0,0:07:03.79,0:07:04.59,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:07:05.03,0:07:10.02,Default,,0000,0000,0000,,Trong lần chẩn đoán đầu tiên, tôi muốn được ở một mình cùng bệnh nhân...
Dialogue: 0,0:07:12.10,0:07:13.16,Default,,0000,0000,0000,,Vâng, thưa Bác sĩ.
Dialogue: 0,0:07:19.81,0:07:22.00,Default,,0000,0000,0000,,Được rồi, phiền cô vén áo lên.
Dialogue: 0,0:07:23.58,0:07:25.34,Default,,0000,0000,0000,,Anh sẽ ko gạt tôi như những người khác chứ?
Dialogue: 0,0:07:46.63,0:07:50.00,Default,,0000,0000,0000,,Bệnh nhân luôn giấu kín điểm yếu của mình, vậy nên tôi thường dùng cách đó...
Dialogue: 0,0:07:50.87,0:07:53.43,Default,,0000,0000,0000,,...bên cạnh việc sắp đặt kế hoạch khiến cô ta tin tưởng.
Dialogue: 0,0:07:53.54,0:07:57.48,Default,,0000,0000,0000,,Khi đã sa vào vòng tay của tôi, cô ta sẽ hiểu phụ nữ thật sự cần những gì.
Dialogue: 0,0:08:04.08,0:08:05.18,Default,,0000,0000,0000,,Chuyện gì vậy?
Dialogue: 0,0:08:05.49,0:08:06.68,Default,,0000,0000,0000,,Chuyện gì vậy Bác sĩ!
Dialogue: 0,0:08:08.39,0:08:09.72,Default,,0000,0000,0000,,Tôi e rằng cô đã...
Dialogue: 0,0:08:12.79,0:08:15.39,Default,,0000,0000,0000,,Xin lỗi, phiền cô cởi nốt quần ra.
Dialogue: 0,0:08:15.66,0:08:16.19,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:08:17.10,0:08:18.26,Default,,0000,0000,0000,,Cởi ra nào.
Dialogue: 0,0:08:25.44,0:08:27.93,Default,,0000,0000,0000,,Cần thiết phải khám nơi đó đến vậy sao?
Dialogue: 0,0:08:28.08,0:08:30.17,Default,,0000,0000,0000,,Tôi đâu có bệnh tật gì nơi đó đâu?
Dialogue: 0,0:08:30.24,0:08:31.51,Default,,0000,0000,0000,,Shh, yên lặng nào.
Dialogue: 0,0:08:42.02,0:08:44.25,Default,,0000,0000,0000,,Anh khám bệnh kiểu gì vậy?
Dialogue: 0,0:08:47.46,0:08:48.49,Default,,0000,0000,0000,,Mệt thật đấy.
Dialogue: 0,0:08:50.23,0:08:53.89,Default,,0000,0000,0000,,Mong cô thứ lỗi, e rằng cô phải tự mình làm điều đó thôi.
Dialogue: 0,0:08:54.27,0:08:54.89,Default,,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:08:56.74,0:08:58.30,Default,,0000,0000,0000,,Chuyện đó vô cùng cần thiết.
Dialogue: 0,0:08:58.44,0:09:02.74,Default,,0000,0000,0000,,Nếu cô ko nghe lời, sẽ rất khó khăn cho việc điều trị.
Dialogue: 0,0:09:02.91,0:09:04.90,Default,,0000,0000,0000,,T-Thôi được...
Dialogue: 0,0:09:18.13,0:09:20.39,Default,,0000,0000,0000,,Đừng nhìn chằm chằm vào tôi như vậy!
Dialogue: 0,0:09:21.26,0:09:23.46,Default,,0000,0000,0000,,Chỉ đơn thuần là khám bệnh thôi mà. Đừng quên tôi là bác sĩ của cô!
Dialogue: 0,0:09:23.90,0:09:25.92,Default,,0000,0000,0000,,Nhưng làm vậy xấu hổ lắm...
Dialogue: 0,0:09:26.93,0:09:28.80,Default,,0000,0000,0000,,Đúng vậy, cô làm tốt lắm.
Dialogue: 0,0:09:47.49,0:09:49.46,Default,,0000,0000,0000,,Thú vị thật...
Dialogue: 0,0:10:00.90,0:10:05.03,Default,,0000,0000,0000,,Tôi đã được đào tạo bài bản bên Mĩ.
Dialogue: 0,0:10:05.61,0:10:07.97,Default,,0000,0000,0000,,Vậy nên, việc này là vô cùng cần thiết!
Dialogue: 0,0:10:08.18,0:10:08.83,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:10:09.81,0:10:11.00,Default,,0000,0000,0000,,Mau lên nào!
Dialogue: 0,0:10:26.99,0:10:28.22,Default,,0000,0000,0000,,Thôi nào, ko có gì phải ngại hết!
Dialogue: 0,0:10:28.56,0:10:29.86,Default,,0000,0000,0000,,Vâng.
Dialogue: 0,0:11:08.50,0:11:11.44,Default,,0000,0000,0000,,Nhiệt độ hơi thất thường.
Dialogue: 0,0:11:14.98,0:11:16.84,Default,,0000,0000,0000,,Sức khỏe ổn định.
Dialogue: 0,0:11:18.11,0:11:21.55,Default,,0000,0000,0000,,Tuy nhiên, hình như cô đã nhịn tiểu khá lâu.
Dialogue: 0,0:11:21.72,0:11:23.21,Default,,0000,0000,0000,,Nếu cứ tiếp tục như vậy, cô sẽ đầu độc chính cơ thể mình.
Dialogue: 0,0:11:45.47,0:11:50.21,Default,,0000,0000,0000,,Giờ tôi sẽ kiểm tra ruột già của cô, chỉ là phòng ngừa thôi...
Dialogue: 0,0:11:51.91,0:11:54.28,Default,,0000,0000,0000,,Sẽ ko mất nhiều thời gian đâu.
Dialogue: 0,0:11:54.68,0:11:56.38,Default,,0000,0000,0000,,Tôi rất giỏi trong việc này.
Dialogue: 0,0:12:00.59,0:12:01.85,Default,,0000,0000,0000,,Nhưng nơi đó là...
Dialogue: 0,0:12:01.92,0:12:02.75,Default,,0000,0000,0000,,Xin lỗi...
Dialogue: 0,0:12:03.96,0:12:06.12,Default,,0000,0000,0000,,Nơi đó cũng cần được kiểm tra kĩ lưỡng.
Dialogue: 0,0:12:08.00,0:12:09.22,Default,,0000,0000,0000,,Rát phải ko?
Dialogue: 0,0:12:09.50,0:12:11.73,Default,,0000,0000,0000,,Cô đổ mồ hôi nhiều thật.
Dialogue: 0,0:12:20.77,0:12:21.90,Default,,0000,0000,0000,,Lạnh quá!
Dialogue: 0,0:12:22.21,0:12:25.11,Default,,0000,0000,0000,,Thứ gì đó rất lạnh đang tràn vào bên trong tôi!
Dialogue: 0,0:12:25.41,0:12:27.78,Default,,0000,0000,0000,,Cố chịu đựng một chút nữa thôi.
Dialogue: 0,0:12:35.02,0:12:37.32,Default,,0000,0000,0000,,Bác sĩ, làm ơn hãy quay đi chỗ khác.
Dialogue: 0,0:12:37.56,0:12:39.29,Default,,0000,0000,0000,,Xấu hổ quá!
Dialogue: 0,0:12:39.59,0:12:40.36,Default,,0000,0000,0000,,Ko được.
Dialogue: 0,0:12:41.20,0:12:47.66,Default,,0000,0000,0000,,Tôi sẽ ko thể chẩn đoán chính xác nếu nơi đó ko được làm sạch hoàn toàn.
Dialogue: 0,0:12:48.64,0:12:50.07,Default,,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:12:51.37,0:12:56.14,Default,,0000,0000,0000,,Đừng lo về chuyện ướt giường, dưới chỗ đó đã lót sẵn một tấm nệm.
Dialogue: 0,0:12:59.38,0:13:00.57,Default,,0000,0000,0000,,Dường như nó đang ra...
Dialogue: 0,0:13:05.72,0:13:07.81,Default,,0000,0000,0000,,Đừng cố kìm nén, điều đó ko tốt cho cơ thể cô đâu.
Dialogue: 0,0:13:08.19,0:13:10.45,Default,,0000,0000,0000,,Thư giãn nào!
Dialogue: 0,0:13:10.86,0:13:13.92,Default,,0000,0000,0000,,Thứ đó đang thanh lọc cơ thể cô!
Dialogue: 0,0:13:14.56,0:13:17.43,Default,,0000,0000,0000,,Đừng cố kìm nén, hãy để nó ra đi!
Dialogue: 0,0:13:18.33,0:13:20.30,Default,,0000,0000,0000,,Đừng lo lắng gì hết.
Dialogue: 0,0:13:20.40,0:13:23.93,Default,,0000,0000,0000,,Vẫn còn tôi bên cạnh và chăm sóc cho cô mà!
Dialogue: 0,0:13:36.52,0:13:38.18,Default,,0000,0000,0000,,Mọi chuyện đã rõ như ban ngày.
Dialogue: 0,0:13:38.52,0:13:42.78,Default,,0000,0000,0000,,Tôi sẽ kêu y tá mang tới cho cô một khẩu phần ăn khoa học hơn.
Dialogue: 0,0:13:44.12,0:13:46.96,Default,,0000,0000,0000,,Vậy ư, bác sĩ tài thật đấy.
Dialogue: 0,0:13:47.13,0:13:50.96,Default,,0000,0000,0000,,Tiểu thư Higashijuujo cao quý lại làm bẩn giường mình như vậy sao?
Dialogue: 0,0:13:51.53,0:13:53.73,Default,,0000,0000,0000,,Cô ấy bị tôi bắt gặp khi đang làm chuyện đó.
Dialogue: 0,0:13:53.80,0:13:57.43,Default,,0000,0000,0000,,Tôi đảm bảo từ giờ cô ấy sẽ cư xử lễ độ hơn trước!
Dialogue: 0,0:13:58.24,0:13:59.47,Default,,0000,0000,0000,,Hi vọng anh nói đúng!
Dialogue: 0,0:13:59.84,0:14:04.33,Default,,0000,0000,0000,,Nhân tiện, hình như cô thường lơ là trong việc chăm sóc bệnh nhân.
Dialogue: 0,0:14:04.78,0:14:05.71,Default,,0000,0000,0000,,Điều đó thật khó mà chấp nhận được!
Dialogue: 0,0:14:06.98,0:14:08.41,Default,,0000,0000,0000,,Thật ư?
Dialogue: 0,0:14:08.55,0:14:10.61,Default,,0000,0000,0000,,Là bệnh nhân nào thế?
Dialogue: 0,0:14:14.39,0:14:15.69,Default,,0000,0000,0000,,Oh, bệnh nhân vừa gọi!
Dialogue: 0,0:14:17.39,0:14:18.22,Default,,0000,0000,0000,,Bệnh nhân gọi?
Dialogue: 0,0:14:19.79,0:14:22.76,Default,,0000,0000,0000,,Chà, chà, của anh bự chưa này!
Dialogue: 0,0:14:22.83,0:14:25.20,Default,,0000,0000,0000,,Tôi sẽ chăm sóc nó ngay bây giờ đây...
Dialogue: 0,0:14:28.84,0:14:31.13,Default,,0000,0000,0000,,Anh sẵn sàng chưa?
Dialogue: 0,0:14:40.55,0:14:42.28,Default,,0000,0000,0000,,Sao hả?
Dialogue: 0,0:14:42.68,0:14:43.98,Default,,0000,0000,0000,,Sướng ko nào?
Dialogue: 0,0:14:45.99,0:14:50.18,Default,,0000,0000,0000,,Chắc là sướng lắm phải ko, đến cả tôi còn cảm thấy như vậy mà.
Dialogue: 0,0:14:59.47,0:15:01.30,Default,,0000,0000,0000,,Wow, anh ra nhiều quá.
Dialogue: 0,0:15:01.47,0:15:02.87,Default,,0000,0000,0000,,Sức khỏe anh tốt hơn tôi tưởng đấy!
Dialogue: 0,0:15:07.27,0:15:08.43,Default,,0000,0000,0000,,Vị ghê quá.
Dialogue: 0,0:15:08.98,0:15:13.04,Default,,0000,0000,0000,,Thật khó mà quen được với mùi vị của anh.
Dialogue: 0,0:15:13.48,0:15:16.24,Default,,0000,0000,0000,,Nhưng tôi vẫn thích thứ này...
Dialogue: 0,0:15:20.02,0:15:22.11,Default,,0000,0000,0000,,Nó vẫn còn cứng vậy sao!
Dialogue: 0,0:15:22.19,0:15:24.71,Default,,0000,0000,0000,,Tuyệt quá! Anh vẫn ổn đấy chứ!
Dialogue: 0,0:15:41.01,0:15:43.44,Default,,0000,0000,0000,,Ôi Chúa ơi, tuyệt quá!
Dialogue: 0,0:15:49.65,0:15:50.98,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:15:51.79,0:15:55.72,Default,,0000,0000,0000,,Thằng nhỏ của bệnh nhân nơi đây quá tuyệt vời,\Nchúng thật to và cứng!
Dialogue: 0,0:15:55.92,0:15:58.55,Default,,0000,0000,0000,,Quá tuyệt vời!
Dialogue: 0,0:16:26.79,0:16:28.95,Default,,0000,0000,0000,,Oh, anh có thể đẩy mạnh hơn từ bên dưới được ko?
Dialogue: 0,0:16:37.10,0:16:38.16,Default,,0000,0000,0000,,Xin phép...
Dialogue: 0,0:16:39.60,0:16:41.69,Default,,0000,0000,0000,,Oh, Bác sĩ!
Dialogue: 0,0:16:42.24,0:16:44.43,Default,,0000,0000,0000,,Đến giờ khám bệnh rồi sao?
Dialogue: 0,0:16:44.81,0:16:46.57,Default,,0000,0000,0000,,C-Cô đang làm cái quái gì vậy?
Dialogue: 0,0:16:47.24,0:16:51.18,Default,,0000,0000,0000,,Xin lỗi Bác sĩ, phiền anh đợi cho một lúc...
Dialogue: 0,0:16:52.11,0:16:54.41,Default,,0000,0000,0000,,Cô ko phải RN sao?
Dialogue: 0,0:16:52.11,0:16:55.97,Note,,0000,0000,0000,,{\i1} RN: Real Nurse- y tá thật sự
Dialogue: 0,0:16:54.88,0:16:55.97,Default,,0000,0000,0000,,RN?
Dialogue: 0,0:16:56.75,0:16:58.62,Default,,0000,0000,0000,,Là gì vậy?
Dialogue: 0,0:16:58.89,0:17:00.05,Default,,0000,0000,0000,,Đúng như tôi nghĩ...
Dialogue: 0,0:17:00.49,0:17:05.19,Default,,0000,0000,0000,,Tôi chỉ đóng vai ý tá trong một hộp đêm, vậy nên tôi ko thể trả lời mấy câu hỏi đó của Bác sĩ đâu!
Dialogue: 0,0:17:05.59,0:17:08.22,Default,,0000,0000,0000,,Chính giám đốc đã thuê tôi làm việc này ...
Dialogue: 0,0:17:12.00,0:17:13.36,Default,,0000,0000,0000,,Chuyện quái gì vậy nhỉ?
Dialogue: 0,0:17:13.43,0:17:17.27,Default,,0000,0000,0000,,Vậy là cô ta cũng được thuê để làm công việc giống như mình?
Dialogue: 0,0:17:18.14,0:17:21.51,Default,,0000,0000,0000,,Bác sĩ, anh đang lẩm bẩm gì thế?
Dialogue: 0,0:17:21.58,0:17:24.67,Default,,0000,0000,0000,,Tôi sẽ vượt qua căn bệnh này!
Dialogue: 0,0:17:25.15,0:17:29.45,Default,,0000,0000,0000,,Nếu chết đi, sẽ ko bao giờ tôi có thể cảm nhận được giây phút tuyệt vời này nữa!
Dialogue: 0,0:17:31.18,0:17:34.45,Default,,0000,0000,0000,,Ko thể tin là phương pháp trị bệnh này của cô ta lại có hiệu quả lớn đến vậy...
Dialogue: 0,0:17:34.59,0:17:37.61,Default,,0000,0000,0000,,...nó giúp bệnh nhân lấy lại niềm tin vào cuộc sống, hơn hẳn những cách trị bệnh thông thường.
Dialogue: 0,0:17:39.03,0:17:41.26,Default,,0000,0000,0000,,Nếu phương pháp này thành công với các bệnh nhân khác...
Dialogue: 0,0:17:41.33,0:17:44.13,Default,,0000,0000,0000,,...nó sẽ mang lại cho bệnh viện một khoản thu nhập khổng lồ.
Dialogue: 0,0:17:45.20,0:17:48.10,Default,,0000,0000,0000,,Tuyệt quá đi mất!
Dialogue: 0,0:17:49.90,0:17:51.03,Default,,0000,0000,0000,,Em thực sự là một thiên thần...
Dialogue: 0,0:17:51.30,0:17:54.17,Default,,0000,0000,0000,,Một thiên thần trong bộ đồ y tá!
Dialogue: 0,0:17:54.91,0:17:59.21,Default,,0000,0000,0000,,Tôi chỉ là nhân viên của hộp đêm thôi mà.
Dialogue: 0,0:18:11.99,0:18:16.09,Default,,0000,0000,0000,,Ra ngoài hít thở không khí trong lành sẽ giúp ích cho sức khỏe cô rất nhiều.
Dialogue: 0,0:18:16.96,0:18:19.45,Default,,0000,0000,0000,,Nếu Bác sĩ đã nói vậy...
Dialogue: 0,0:18:21.60,0:18:24.63,Default,,0000,0000,0000,,Cô ko mặc quần lót đấy chứ?
Dialogue: 0,0:18:25.54,0:18:26.34,Default,,0000,0000,0000,,Ko ạ...
Dialogue: 0,0:18:28.07,0:18:31.67,Default,,0000,0000,0000,,Cần phải ngăn chặn tình trạng đổ mồ hôi quá nhiều ở vùng đó.
Dialogue: 0,0:18:32.11,0:18:37.45,Default,,0000,0000,0000,,Hơn nữa, quần lót quá chật cũng là một nguyên nhân gây ức chế thần kinh.
Dialogue: 0,0:18:37.58,0:18:39.42,Default,,0000,0000,0000,,Vậy nên đừng ngại tháo bỏ quần lót vào những lúc ko cần thiết.
Dialogue: 0,0:19:02.08,0:19:04.04,Default,,0000,0000,0000,,Công đoạn này này nằm trong phác đồ điều trị thử nghiệm của cô.
Dialogue: 0,0:19:04.18,0:19:08.61,Default,,0000,0000,0000,,Nếu thiếu nó, quá trình phân tích các triệu chứng bệnh tật sẽ gặp nhiều khó khăn!
Dialogue: 0,0:19:27.23,0:19:30.26,Default,,0000,0000,0000,,Cô ta mất trí rồi sao?
Dialogue: 0,0:19:30.84,0:19:34.07,Default,,0000,0000,0000,,Trông mặt mũi xinh đẹp thế kia, vậy mà...
Dialogue: 0,0:19:34.41,0:19:36.90,Default,,0000,0000,0000,,Mũm mĩm thật...
Dialogue: 0,0:19:37.74,0:19:39.80,Default,,0000,0000,0000,,Nhìn xem, dâm thủy cô ta đang chảy ra kìa!
Dialogue: 0,0:19:59.63,0:20:00.83,Default,,0000,0000,0000,,Có chuyện gì vậy?
Dialogue: 0,0:20:17.02,0:20:18.08,Default,,0000,0000,0000,,Cô sao rồi?
Dialogue: 0,0:20:18.15,0:20:21.68,Default,,0000,0000,0000,,Làm chuyện đó khiến cô có cảm giác được giải thoát, phải ko nào?
Dialogue: 0,0:20:22.52,0:20:26.19,Default,,0000,0000,0000,,L-Làm ơn, nhét thằng nhỏ của bác sĩ vào trong tôi mau lên!
Dialogue: 0,0:20:26.29,0:20:27.82,Default,,0000,0000,0000,,Cô nói gì lạ vậy?
Dialogue: 0,0:20:27.99,0:20:30.19,Default,,0000,0000,0000,,Điều đó đâu nằm trong phác đồ điều trị.
Dialogue: 0,0:20:30.76,0:20:31.86,Default,,0000,0000,0000,,Anh thật nhẫn tâm!
Dialogue: 0,0:20:32.57,0:20:36.19,Default,,0000,0000,0000,,Chính anh đã biến tôi thành như vậy, giờ anh định bỏ mặc tôi thế này sao!
Dialogue: 0,0:20:36.94,0:20:40.57,Default,,0000,0000,0000,,Nhét thứ đó của anh vào bên trong và làm tôi tan chảy đi nào!
Dialogue: 0,0:20:40.81,0:20:44.40,Default,,0000,0000,0000,,Tiểu thư Higashijuujo, thay vì nặng lời như vậy, cô sẽ làm thế nào...
Dialogue: 0,0:20:44.51,0:20:46.38,Default,,0000,0000,0000,,...khi muốn cầu xin người khác ?
Dialogue: 0,0:20:47.91,0:20:49.07,Default,,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:20:49.48,0:20:51.68,Default,,0000,0000,0000,,Ko phải cô vừa yêu cầu tôi điều gì sao?
Dialogue: 0,0:20:54.52,0:21:02.43,Default,,0000,0000,0000,,Làm ơn, làm ơn nhét thằng nhỏ của anh vào bên trong em?
Dialogue: 0,0:21:03.50,0:21:05.93,Default,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,0:21:31.09,0:21:33.36,Default,,0000,0000,0000,,L-Làm ơn, em muốn anh nhét nó vào bên trong!
Dialogue: 0,0:21:35.23,0:21:37.16,Default,,0000,0000,0000,,Cô đúng là một tiểu thư hư hỏng.
Dialogue: 0,0:21:39.63,0:21:40.62,Default,,0000,0000,0000,,Nó đang vào bên trong em!
Dialogue: 0,0:21:40.77,0:21:43.74,Default,,0000,0000,0000,,Bự quá đi mất!
Dialogue: 0,0:21:49.44,0:21:52.97,Default,,0000,0000,0000,,Tuyệt thật đấy, Bác sĩ!
Dialogue: 0,0:21:54.41,0:21:56.54,Default,,0000,0000,0000,,Cô nhạy cảm hơn rồi đấy.
Dialogue: 0,0:21:56.85,0:21:58.68,Default,,0000,0000,0000,,Sao chúng ta ko thử trong tư thế khác nhỉ?
Dialogue: 0,0:22:23.98,0:22:27.41,Default,,0000,0000,0000,,E-Em ko chịu nổi nữa rồi, em chết mất!
Dialogue: 0,0:22:27.48,0:22:28.67,Default,,0000,0000,0000,,Ko dễ như vậy đâu...
Dialogue: 0,0:22:29.22,0:22:30.88,Default,,0000,0000,0000,,Cơ thể em đang rộn lên!
Dialogue: 0,0:22:31.08,0:22:34.32,Default,,0000,0000,0000,,Bên trong em dường như đang tan ra!
Dialogue: 0,0:22:34.39,0:22:35.95,Default,,0000,0000,0000,,Chỉ một chút nữa thôi.
Dialogue: 0,0:22:36.02,0:22:37.85,Default,,0000,0000,0000,,Chúng ta sắp xong rồi...
Dialogue: 0,0:22:51.70,0:22:54.76,Default,,0000,0000,0000,,Quá trình điều trị kết thúc.
Dialogue: 0,0:22:55.01,0:22:59.60,Default,,0000,0000,0000,,Tiểu thư Higashijuujo, cô hoàn toàn ko có bệnh tật gì.
Dialogue: 0,0:23:00.85,0:23:04.61,Default,,0000,0000,0000,,Ngày mai cô sẽ được xuất viện.
Dialogue: 0,0:23:07.82,0:23:10.35,Default,,0000,0000,0000,,Ko, em ko muốn về nhà đâu!
Dialogue: 0,0:23:10.62,0:23:12.28,Default,,0000,0000,0000,,Em ốm thật mà, bác sĩ ko thấy sao?
Dialogue: 0,0:23:12.46,0:23:16.29,Default,,0000,0000,0000,,Em ko thể sống thiếu Bác sĩ được!
Dialogue: 0,0:23:25.44,0:23:26.70,Default,,0000,0000,0000,,Bác sĩ đừng lo!
Dialogue: 0,0:23:26.77,0:23:29.57,Default,,0000,0000,0000,,Chỉ cần em nói với cha một câu,\Nông ấy sẽ cho anh bất cứ thứ gì anh muốn!
Dialogue: 0,0:23:29.71,0:23:32.94,Default,,0000,0000,0000,,Phải rồi, nghe nói tình hình tài chính của bệnh viện đang sa sút.
Dialogue: 0,0:23:33.01,0:23:35.45,Default,,0000,0000,0000,,Em sẽ xin cha tài trợ cho bệnh viện một số tiền lớn...
Dialogue: 0,0:23:35.52,0:23:40.35,Default,,0000,0000,0000,,...đừng bắt em xuất viện, cầu xin Bác sĩ đấy!
Dialogue: 0,0:23:42.02,0:23:43.22,Default,,0000,0000,0000,,Thôi được.
Dialogue: 0,0:23:43.46,0:23:47.59,Default,,0000,0000,0000,,Nếu chuyện đó thực sự khiến cô sốc đến vậy,\Ntôi sẽ xem xét lại chuyện này.
Dialogue: 0,0:23:47.96,0:23:49.43,Default,,0000,0000,0000,,Oh, cám ơn Bác sĩ!
Dialogue: 0,0:23:54.40,0:23:56.33,Default,,0000,0000,0000,,Được rồi, há to ra nào!
Dialogue: 0,0:23:57.00,0:24:01.50,Default,,0000,0000,0000,,Tiểu thư Higashijuujo, đồ ăn ở đây ko vừa miệng cô sao?
Dialogue: 0,0:24:07.88,0:24:09.31,Default,,0000,0000,0000,,N-Nhột quá!
Dialogue: 0,0:24:09.58,0:24:13.85,Default,,0000,0000,0000,,Bác sĩ Ishida sắp tới chưa?
Dialogue: 0,0:24:14.22,0:24:16.31,Default,,0000,0000,0000,,Bác sĩ Ishida đang bận.
Dialogue: 0,0:24:17.02,0:24:19.75,Default,,0000,0000,0000,,Vậy nên cô hãy thôi rên rỉ và cố ăn đi!
Dialogue: 0,0:24:21.36,0:24:24.02,Default,,0000,0000,0000,,Chà! Cô kén cá chọn canh quá đấy.
Dialogue: 0,0:24:24.33,0:24:28.10,Default,,0000,0000,0000,,Cô đã hứa sẽ trở thành bệnh nhân gương mẫu mà, tiểu thư Higashijuujo.
Dialogue: 0,0:24:28.57,0:24:31.33,Default,,0000,0000,0000,,Thôi được, nếu cô nói mà ko giữ lời...
Dialogue: 0,0:24:35.48,0:24:39.17,Default,,0000,0000,0000,,Oh ko, cô lại làm ướt giường rồi sao?
Dialogue: 0,0:24:39.45,0:24:41.41,Default,,0000,0000,0000,,Y như trẻ con vậy.
Dialogue: 0,0:24:41.48,0:24:44.21,Default,,0000,0000,0000,,Để tôi giúp cô thư giãn nhé.
Dialogue: 0,0:24:44.62,0:24:46.35,Default,,0000,0000,0000,,Tuyệt quá đi mất...
Dialogue: 0,0:24:46.82,0:24:51.31,Default,,0000,0000,0000,,Nhờ khoản tài trợ của Higashijuujo, bệnh viện đã thoát khỏi nguy cơ phá sản.
Dialogue: 0,0:24:51.39,0:24:54.69,Default,,0000,0000,0000,,Cô ta đã trở thành nô lệ của tôi.
Dialogue: 0,0:24:54.76,0:24:59.66,Default,,0000,0000,0000,,Nhưng tôi sẽ ko điều trị cho một bệnh nhân duy nhất, chẳng mấy chốc, cô ta sẽ trở nên nhàm chán.
Dialogue: 0,0:25:00.33,0:25:03.33,Default,,0000,0000,0000,,Tôi sẽ nhanh chóng tìm ra nguồn vui mới!
Dialogue: 0,0:25:21.25,0:25:26.06,Default,,0000,0000,0000,,Con người thường bị tác động bởi môi trường sống xung quanh họ.
