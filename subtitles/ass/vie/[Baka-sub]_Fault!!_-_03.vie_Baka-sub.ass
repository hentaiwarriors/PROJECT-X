Dialogue: 0,0:02:12.19,0:02:19.07,Logo,,0000,0000,0000,,{\an8\fad(300,1000)}Baka-sub trân trọng giới thiệu
Dialogue: 0,0:00:07.15,0:00:10.34,Default,Ai,0000,0000,0000,,Tuy câu lạc bộ tennis đã rời trại tập huấn,
Dialogue: 0,0:00:10.34,0:00:13.75,Default,Ai,0000,0000,0000,,nhưng chúng tôi vẫn phải tiếp tục tập luyện trong suốt kì nghỉ hè.
Dialogue: 0,0:00:14.28,0:00:16.23,Default,Reiko,0000,0000,0000,,Chuẩn bị giao bóng nè, Ai.
Dialogue: 0,0:00:19.75,0:00:20.42,Default,Reiko,0000,0000,0000,,Tiếp này!
Dialogue: 0,0:00:23.89,0:00:25.50,Default,Reiko,0000,0000,0000,,Nhanh chân lên nào!
Dialogue: 0,0:00:27.56,0:00:28.28,Default,Reiko,0000,0000,0000,,Mio!
Dialogue: 0,0:00:31.56,0:00:34.20,Default,Reiko,0000,0000,0000,,Coi nào, Mio, phải cố bắt kịp mọi người chứ.
Dialogue: 0,0:00:34.20,0:00:38.00,Default,Mio,0000,0000,0000,,Em chịu thôi! Trời nóng thế này mà cứ chạy hùng hục thì chết khô mất.
Dialogue: 0,0:00:38.00,0:00:43.16,Default,Reiko,0000,0000,0000,,Thì chị đã bảo sau buổi tập luyện hôm nay, chúng ta có thể xài bể bơi còn gì.
Dialogue: 0,0:00:43.16,0:00:45.38,Default,Reiko,0000,0000,0000,,Ráng chịu đựng một chút nữa thôi mà.
Dialogue: 0,0:00:45.77,0:00:47.01,Default,Guy,0000,0000,0000,,Ôi, nóng thật.
Dialogue: 0,0:00:47.01,0:00:52.03,Default,Guy,0000,0000,0000,,Senpai, thật ra thì bây giờ em chỉ muốn nhào ngay xuống bể bơi.
Dialogue: 0,0:00:52.50,0:00:54.17,Default,Mio,0000,0000,0000,,Em cũng vậy!
Dialogue: 0,0:00:54.17,0:00:58.41,Default,Mio,0000,0000,0000,,Em còn mặc sẵn đồ bơi rồi nè.
Dialogue: 0,0:00:58.75,0:01:01.40,Default,Reiko,0000,0000,0000,,Mio, em thật là...
Dialogue: 0,0:01:01.40,0:01:05.09,Default,Mio,0000,0000,0000,,Có sao đâu? Mặc vậy cho đỡ mất công thay đồ.
Dialogue: 0,0:01:05.85,0:01:08.09,Default,Ai,0000,0000,0000,,À này, Senpai...
Dialogue: 0,0:01:08.09,0:01:10.71,Default,Ai,0000,0000,0000,,Em cũng đang có ý định đó.
Dialogue: 0,0:01:11.05,0:01:12.68,Default,Ai,0000,0000,0000,,Xin lỗi chị.
Dialogue: 0,0:01:12.68,0:01:15.23,Default,Ai,0000,0000,0000,,Em đã mặc sẵn đồ bơi trước khi tới đây.
Dialogue: 0,0:01:15.59,0:01:19.85,Default,Reiko,0000,0000,0000,,Hừ. Thôi được rồi...
Dialogue: 0,0:01:19.85,0:01:21.57,Default,Reiko,0000,0000,0000,,Coi như buổi tập kết thúc ở đây.
Dialogue: 0,0:01:21.57,0:01:22.60,Default,Mio,0000,0000,0000,,Hoan hô!
Dialogue: 0,0:01:22.99,0:01:24.29,Default,Guy,0000,0000,0000,,Hơ?
Dialogue: 0,0:01:24.29,0:01:27.75,Default,Guy,0000,0000,0000,,Xem ra còn có một người đang rất mong chờ điều này.
Dialogue: 0,0:01:28.19,0:01:29.33,Default,Teach,0000,0000,0000,,Nè!
Dialogue: 0,0:01:29.33,0:01:30.84,Default,Teach,0000,0000,0000,,Mọi người!
Dialogue: 0,0:01:30.84,0:01:33.88,Default,Teach,0000,0000,0000,,Chúng ta cùng xuống bể bơi nào!
Dialogue: 0,0:01:34.19,0:01:36.05,Default,Shuu,0000,0000,0000,,Sensei?
Dialogue: 0,0:01:36.05,0:01:38.09,Default,Shuu,0000,0000,0000,,Cô đã thay đồ xong rồi cơ à?
Dialogue: 0,0:01:40.39,0:01:42.86,Default,Teach,0000,0000,0000,,Bộ đồ bơi này cô mới mua đó, là mẫu mới nhất.
Dialogue: 0,0:01:42.86,0:01:44.21,Default,Teach,0000,0000,0000,,Em thấy thế nào?
Dialogue: 0,0:01:44.21,0:01:46.11,Default,Shuu,0000,0000,0000,,Thấy thế nào ấy hả?
Dialogue: 0,0:01:46.11,0:01:47.15,Default,Shuu,0000,0000,0000,,À thì...
Dialogue: 0,0:01:47.15,0:01:47.83,Default,Shuu,0000,0000,0000,,Cô thấy đó...
Dialogue: 0,0:01:49.65,0:01:50.70,Default,Shuu,0000,0000,0000,,Em làm trò gì thế, Mio?
Dialogue: 0,0:01:54.96,0:01:57.55,Default,Teach,0000,0000,0000,,Shuu-chan? Em không sao chứ?
Dialogue: 0,0:01:58.08,0:02:00.71,Default,Mio,0000,0000,0000,,Chưa chi đã tớn mắt lên rồi.
Dialogue: 0,0:02:00.71,0:02:01.89,Default,Mio,0000,0000,0000,,Anh giai ngu ngốc.
Dialogue: 0,0:02:01.89,0:02:03.06,Default,Reiko,0000,0000,0000,,Thôi nào, thôi nào.
Dialogue: 0,0:02:03.06,0:02:05.19,Default,Reiko,0000,0000,0000,,Làm vậy sẽ ảnh hướng tới thành tích thi đấu đó.
Dialogue: 0,0:02:06.34,0:02:10.98,Default,Ai,0000,0000,0000,,Có vẻ như vụ đồ bơi đã phát sinh một vài tình huống rắc rối...
Dialogue: 0,0:02:29.74,0:02:33.17,Default,Teach,0000,0000,0000,,A, Shuu-chan, em tỉnh rồi hả?
Dialogue: 0,0:02:33.70,0:02:35.65,Default,Shuu,0000,0000,0000,,Hửm?
Dialogue: 0,0:02:36.55,0:02:39.44,Default,Teach,0000,0000,0000,,Sao hả? Em vẫn còn chóng mặt ư?
Dialogue: 0,0:02:39.44,0:02:41.54,Default,Teach,0000,0000,0000,,Đầu em còn đau không?
Dialogue: 0,0:02:41.54,0:02:43.30,Default,Shuu,0000,0000,0000,,Giờ em thấy khá hơn rồi.
Dialogue: 0,0:02:43.57,0:02:45.54,Default,Teach,0000,0000,0000,,Đừng ngồi dậy lúc này.
Dialogue: 0,0:02:45.54,0:02:47.76,Default,Teach,0000,0000,0000,,Em cứ nằm nghỉ thêm chút nữa đi.
Dialogue: 0,0:02:47.76,0:02:50.30,Default,Shuu,0000,0000,0000,,Không, thật ra thì...
Dialogue: 0,0:02:51.75,0:02:55.70,Default,Teach,0000,0000,0000,,Shuu-chan! Thằng nhỏ của em chào cờ kìa.
Dialogue: 0,0:02:55.70,0:02:57.20,Default,Shuu,0000,0000,0000,,Xin lỗi cô.
Dialogue: 0,0:02:59.78,0:03:01.70,Default,Teach,0000,0000,0000,,Có cần cô kiểm tra nó không?
Dialogue: 0,0:03:03.43,0:03:08.04,Default,Teach,0000,0000,0000,,Ý cô là... quần soọc của em đang bị căng ra và thít chặt thứ ấy.
Dialogue: 0,0:03:08.04,0:03:11.12,Default,Teach,0000,0000,0000,,Nếu cứ để như vậy thì em sẽ đau lắm đó.
Dialogue: 0,0:03:17.08,0:03:20.24,Default,Ai,0000,0000,0000,,Không biết Sugiyama-kun có làm sao không?
Dialogue: 0,0:03:20.24,0:03:23.50,Default,Mio,0000,0000,0000,,Cứ mặc xác anh ấy đi.
Dialogue: 0,0:03:23.85,0:03:28.23,Default,Guy,0000,0000,0000,,Sensei đang chăm sóc cậu ấy, chắc không vấn đề gì đâu.
Dialogue: 0,0:03:28.98,0:03:33.17,Default,Reiko,0000,0000,0000,,Mio, em chạy tới ngó qua cậu ấy một chút dùm chị được không?
Dialogue: 0,0:03:33.17,0:03:34.63,Default,Mio,0000,0000,0000,,Hả?
Dialogue: 0,0:03:34.63,0:03:36.87,Default,Reiko,0000,0000,0000,,Cậu ấy bị chấn thương là do em đấy nhá.
Dialogue: 0,0:03:36.87,0:03:39.90,Default,Reiko,0000,0000,0000,,Cớ sao lại bắt Sensei một mình trông coi cậu ấy hả?!
Dialogue: 0,0:03:41.00,0:03:43.74,Default,Mio,0000,0000,0000,,Hừ, anh giai ngu ngốc.
Dialogue: 0,0:03:43.74,0:03:45.41,Default,Mio,0000,0000,0000,,Em đi là được chứ gì.
Dialogue: 0,0:03:47.35,0:03:48.36,Default,Ai,0000,0000,0000,,Cho mình đi cùng với.
Dialogue: 0,0:03:49.79,0:03:53.39,Default,Reiko,0000,0000,0000,,Rõ ràng là cả hai đều muốn đi mà cứ làm bộ làm tịch mãi.
Dialogue: 0,0:03:58.01,0:04:02.80,Default,Teach,0000,0000,0000,,Cô làm vậy được không, Shuu-chan? Em hết đau chưa?
Dialogue: 0,0:04:02.80,0:04:04.06,Default,Shuu,0000,0000,0000,,Rồi.
Dialogue: 0,0:04:05.46,0:04:08.91,Default,Teach,0000,0000,0000,,Ô. Nó không những bự...
Dialogue: 0,0:04:08.91,0:04:12.33,Default,Teach,0000,0000,0000,,mà lại còn nóng ran và co giật nữa.
Dialogue: 0,0:04:12.67,0:04:16.49,Default,Shuu,0000,0000,0000,,Sensei... Sướng quá đi mất.
Dialogue: 0,0:04:16.49,0:04:17.85,Default,Teach,0000,0000,0000,,Thật ư?
Dialogue: 0,0:04:17.85,0:04:21.86,Default,Teach,0000,0000,0000,,Được cô mân mê sờ nắn như vậy thích lắm hả?
Dialogue: 0,0:04:22.59,0:04:24.13,Default,Teach,0000,0000,0000,,Shuu-chan?
Dialogue: 0,0:04:26.39,0:04:30.42,Default,Shuu,0000,0000,0000,,Chỉ một mình em được hưởng phúc thì hơi bất công.
Dialogue: 0,0:04:32.66,0:04:33.74,Default,Teach,0000,0000,0000,,Đừng làm vậy!
Dialogue: 0,0:04:34.50,0:04:38.39,Default,Teach,0000,0000,0000,,Ai cho phép em sờ vào ngực cô hả!
Dialogue: 0,0:04:38.39,0:04:41.39,Default,Shuu,0000,0000,0000,,Hai đầu vú của cô thật tuyệt vời.
Dialogue: 0,0:04:43.75,0:04:45.54,Default,Teach,0000,0000,0000,,Đừng mân mê hai đầu vú cô!
Dialogue: 0,0:04:45.54,0:04:47.77,Default,Teach,0000,0000,0000,,Chỗ đó của cô nhạy cảm lắm.
Dialogue: 0,0:04:51.07,0:04:53.73,Default,Shuu,0000,0000,0000,,Cô cảm thấy thế nào, sensei?
Dialogue: 0,0:04:53.73,0:04:55.92,Default,Teach,0000,0000,0000,,Thật tuyệt vời.
Dialogue: 0,0:04:55.92,0:05:00.54,Default,Teach,0000,0000,0000,,Nhưng mà... nhưng mà... nếu em tiếp tục, cô sẽ mất tự chủ đó.
Dialogue: 0,0:05:03.79,0:05:05.43,Default,Shuu,0000,0000,0000,,Mát-xa thằng nhỏ của em tiếp đi cô.
Dialogue: 0,0:05:13.76,0:05:18.05,Default,Teach,0000,0000,0000,,Có thứ gì đó sắp trào ra từ dương vật của em.
Dialogue: 0,0:05:18.05,0:05:21.10,Default,Teach,0000,0000,0000,,Tay cô bắt đầu nhớp nháp rồi.
Dialogue: 0,0:05:26.15,0:05:29.09,Default,Shuu,0000,0000,0000,,Cô cảm thấy sao?
Dialogue: 0,0:05:29.85,0:05:32.64,Default,Teach,0000,0000,0000,,Dường như hai đầu vú của cô sắp tan chảy.
Dialogue: 0,0:05:33.47,0:05:35.79,Default,Teach,0000,0000,0000,,Sướng quá đi mất.
Dialogue: 0,0:05:37.61,0:05:40.54,Default,Teach,0000,0000,0000,,Tiếp tục đi mà, Shuu-chan.
Dialogue: 0,0:05:40.54,0:05:42.98,Default,Shuu,0000,0000,0000,,Vậy là cô sắp  ra rồi hả?
Dialogue: 0,0:05:44.11,0:05:48.11,Default,Teach,0000,0000,0000,,Ra ư... Đó là hành động gì nhỉ?
Dialogue: 0,0:05:49.65,0:05:52.46,Default,Teach,0000,0000,0000,,Bên trong cơ thể cô đang nóng ran.
Dialogue: 0,0:05:52.46,0:05:54.75,Default,Teach,0000,0000,0000,,Đầu óc cô dường như trống rỗng.
Dialogue: 0,0:05:57.03,0:06:00.13,Default,Shuu,0000,0000,0000,,Vậy cô trò chúng ta ra cùng nhau nhé.
Dialogue: 0,0:06:00.14,0:06:02.11,Default,Teach,0000,0000,0000,,Sao bỗng nhiên em ngấu nghiến hai đầu vú cô ghê vậy.
Dialogue: 0,0:06:02.09,0:06:07.01,Default,Teach,0000,0000,0000,,Phải, cô hết chịu nổi rồi.  Cô ra đây.
Dialogue: 0,0:06:21.81,0:06:24.20,Default,Teach,0000,0000,0000,,Vậy ra thứ này là tinh dịch hả?
Dialogue: 0,0:06:24.79,0:06:26.36,Default,Teach,0000,0000,0000,,Nóng thật đấy.
Dialogue: 0,0:06:26.76,0:06:29.68,Default,Shuu,0000,0000,0000,,Xin lỗi, em làm dơ hết người cô rồi.
Dialogue: 0,0:06:31.55,0:06:36.37,Default,Teach,0000,0000,0000,,Chắc là em vẫn chưa thoải mãn phải không?
Dialogue: 0,0:06:37.09,0:06:41.83,Default,Teach,0000,0000,0000,,Thấy chưa, thằng nhỏ của em vẫn còn bự lắm.
Dialogue: 0,0:06:41.83,0:06:48.43,Default,Ai,0000,0000,0000,,Ghê quá. Cả Sensei và Sugiyama-kun đều đang hưng phấn.
Dialogue: 0,0:06:48.43,0:06:53.16,Default,Mio,0000,0000,0000,,Nè, có phải anh giai ngu ngốc bị sensei ép làm chuyện đó không nhỉ?
Dialogue: 0,0:06:53.16,0:06:56.35,Default,Ai,0000,0000,0000,,Không. Đâu có giống như vậy.
Dialogue: 0,0:06:59.63,0:07:05.77,Default,Teach,0000,0000,0000,,Lần này cô sẽ để "nó" giúp em được sung sướng trọn vẹn.
Dialogue: 0,0:07:05.77,0:07:07.89,Default,Shuu,0000,0000,0000,,Sensei...
Dialogue: 0,0:07:08.80,0:07:12.86,Default,Teach,0000,0000,0000,,Mau lên nào. Tư thế này xấu hổ lắm.
Dialogue: 0,0:07:12.86,0:07:14.29,Default,Shuu,0000,0000,0000,,Vâng.
Dialogue: 0,0:07:15.84,0:07:17.74,Default,Teach,0000,0000,0000,,Nhẹ nhàng thôi nhé.
Dialogue: 0,0:07:22.79,0:07:26.11,Default,Shuu,0000,0000,0000,,Sensei, đây là lần đầu của cô ư?
Dialogue: 0,0:07:26.97,0:07:32.30,Default,Teach,0000,0000,0000,,Đừng bận tâm về chuyện đó. Đâm nó vào sâu bên trong đi em.
Dialogue: 0,0:07:32.30,0:07:33.68,Default,Shuu,0000,0000,0000,,Nhưng mà...
Dialogue: 0,0:07:33.93,0:07:35.18,Default,Teach,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:07:35.18,0:07:37.84,Default,Teach,0000,0000,0000,,Cô là người lớn cơ mà.
Dialogue: 0,0:07:37.84,0:07:42.19,Default,Teach,0000,0000,0000,,Cô đủ sức "tiếp đón" thằng nhỏ của em.
Dialogue: 0,0:07:43.33,0:07:44.57,Default,Shuu,0000,0000,0000,,Thôi được.
Dialogue: 0,0:07:49.19,0:07:51.44,Default,Teach,0000,0000,0000,,Đau quá... Đau quá!
Dialogue: 0,0:07:53.66,0:07:57.13,Default,Shuu,0000,0000,0000,,Nó vào hết bên trong cô rồi, Sensei.
Dialogue: 0,0:08:01.76,0:08:06.79,Default,Teach,0000,0000,0000,,Bắt đầu di chuyển đi nào, Shuu-chan.
Dialogue: 0,0:08:06.79,0:08:08.68,Default,Shuu,0000,0000,0000,,Nhưng mà cô đang...
Dialogue: 0,0:08:09.80,0:08:10.91,Default,Teach,0000,0000,0000,,Không sao cả.
Dialogue: 0,0:08:10.91,0:08:13.74,Default,Teach,0000,0000,0000,,Em cứ nhịp thoải mái đi.
Dialogue: 0,0:08:13.74,0:08:17.76,Default,Teach,0000,0000,0000,,Không cần lo lắng cho cô đâu.
Dialogue: 0,0:08:18.36,0:08:21.35,Default,Shuu,0000,0000,0000,,Nếu cô cảm thấy không thoải mái thì phải nói ngay cho em nhé.
Dialogue: 0,0:08:24.47,0:08:26.73,Default,Teach,0000,0000,0000,,Shuu-chan, Shuu-chan!
Dialogue: 0,0:08:29.54,0:08:32.41,Default,Shuu,0000,0000,0000,,Sensei, em đang đâm vào tử cung cô.
Dialogue: 0,0:08:34.37,0:08:37.35,Default,Teach,0000,0000,0000,,Em không nên đụng ra đụng vô nơi đó như vậy.
Dialogue: 0,0:08:37.35,0:08:39.96,Default,Teach,0000,0000,0000,,Nơi đó vô cùng đặc biệt.
Dialogue: 0,0:08:39.96,0:08:42.83,Default,Teach,0000,0000,0000,,Chính là nơi em bé xuất hiện đó nha.
Dialogue: 0,0:08:48.10,0:08:49.94,Default,Teach,0000,0000,0000,,Không được, đừng tiếp tục làm vậy nữa.
Dialogue: 0,0:08:49.94,0:08:53.36,Default,Teach,0000,0000,0000,,Nếu không cô sẽ mất tự chủ đó.
Dialogue: 0,0:08:56.00,0:09:00.27,Default,Shuu,0000,0000,0000,,Nhưng mà hạ thể của cô đã ướt đẫm rồi còn gì.
Dialogue: 0,0:09:03.06,0:09:05.02,Default,Teach,0000,0000,0000,,Em xấu lắm.
Dialogue: 0,0:09:05.02,0:09:06.68,Default,Teach,0000,0000,0000,,Phải đó.
Dialogue: 0,0:09:06.68,0:09:10.75,Default,Teach,0000,0000,0000,,Đây là lần đầu... lần đầu tiên của cô, vậy mà...
Dialogue: 0,0:09:10.75,0:09:13.62,Default,Shuu,0000,0000,0000,,Cô sắp ra rồi phải không?
Dialogue: 0,0:09:16.22,0:09:19.11,Default,Teach,0000,0000,0000,,Còn em thì sao?
Dialogue: 0,0:09:19.44,0:09:22.69,Default,Teach,0000,0000,0000,,Em có muốn cùng ra với cô không?
Dialogue: 0,0:09:22.70,0:09:25.05,Default,Shuu,0000,0000,0000,,Có. Ra cùng nhau...
Dialogue: 0,0:09:26.21,0:09:29.13,Default,Teach,0000,0000,0000,,Phải ra cùng cô đấy nhé, Shuu-chan.
Dialogue: 0,0:09:31.07,0:09:34.70,Default,Teach,0000,0000,0000,,Sắp ra rồi, cô sắp ra rồi, cô sắp ra rồi!
Dialogue: 0,0:09:36.18,0:09:40.23,Default,Teach,0000,0000,0000,,Cô ra rồi!
Dialogue: 0,0:09:43.50,0:09:46.17,Default,Ai,0000,0000,0000,,Oa, cậu ấy ra rồi. Thật sự ra rồi.
Dialogue: 0,0:09:46.81,0:09:51.31,Default,Ai,0000,0000,0000,,Cả sensei cũng vậy. Vậy là cô ấy đã cho Sugiyama lần đầu của mình.
Dialogue: 0,0:09:51.31,0:09:53.70,Default,Mio,0000,0000,0000,,Tệ thật đó.
Dialogue: 0,0:09:54.18,0:09:57.82,Default,Mio,0000,0000,0000,,Không ngờ ngay cả sensei cũng là tình địch của chúng ta.
Dialogue: 0,0:09:58.42,0:09:59.72,Default,Ai,0000,0000,0000,,Tình địch ư?
Dialogue: 0,0:09:59.72,0:10:00.70,Default,Mio,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:10:01.20,0:10:06.57,Default,Mio,0000,0000,0000,,Lúc tan học nhớ ghé qua nhà mình nhé. Chúng ta sẽ bàn bạc kĩ hơn.
Dialogue: 0,0:10:07.90,0:10:09.30,Default,Ai,0000,0000,0000,,Ừ.
Dialogue: 0,0:10:11.40,0:10:17.01,Default,Ai,0000,0000,0000,,Sau đó, chúng tôi giả vờ như không trông thấy gì và quay về bể bơi.
Dialogue: 0,0:10:17.34,0:10:21.12,Default,Ai,0000,0000,0000,,Một lúc sau thì Sugiyama và sensei cũng quay lại.
Dialogue: 0,0:10:21.12,0:10:27.44,Default,Ai,0000,0000,0000,,Nhưng có vẻ như cả hai đều không muốn trò chuyện nên tôi cũng chả buồn hỏi han gì bọn họ.
Dialogue: 0,0:10:33.98,0:10:35.79,Default,Ai,0000,0000,0000,,Mio-san?
Dialogue: 0,0:10:36.50,0:10:38.80,Default,Ai,0000,0000,0000,,Cậu đang làm gì thế?
Dialogue: 0,0:10:39.88,0:10:42.17,Default,Mio,0000,0000,0000,,Cậu cũng mau cởi quần áo ra đi.
Dialogue: 0,0:10:42.17,0:10:47.68,Default,Mio,0000,0000,0000,,Chúng ta sẽ cùng nhau tìm cách gia tăng sự quyến rũ của bản thân.
Dialogue: 0,0:10:48.91,0:10:50.95,Default,Mio,0000,0000,0000,,Nhìn này.
Dialogue: 0,0:10:50.95,0:10:54.55,Default,Ai,0000,0000,0000,,Đó là tạp chí khiêu dâm hả?
Dialogue: 0,0:10:55.62,0:10:59.28,Default,Mio,0000,0000,0000,,Phải. Nó được giấu bên trong phòng Shuu nii-chan.
Dialogue: 0,0:10:59.28,0:11:02.55,Default,Mio,0000,0000,0000,,Hình như anh ấy cực kì khoái mấy thứ này.
Dialogue: 0,0:11:03.77,0:11:07.58,Default,Ai,0000,0000,0000,,Cậu muốn chúng ta làm mấy trò trong cuốn tạp chí đó sao?
Dialogue: 0,0:11:07.58,0:11:08.81,Default,Mio,0000,0000,0000,,Chính xác.
Dialogue: 0,0:11:08.81,0:11:12.15,Default,Mio,0000,0000,0000,,Nếu không, chúng ta sẽ thua Sensei mất.
Dialogue: 0,0:11:12.15,0:11:13.56,Default,Mio,0000,0000,0000,,Cậu đồng ý chứ?
Dialogue: 0,0:11:13.85,0:11:17.24,Default,Ai,0000,0000,0000,,Mình không muốn làm chuyện này.
Dialogue: 0,0:11:17.24,0:11:20.57,Default,Ai,0000,0000,0000,,Lỡ Sugiyama-kun quay về...
Dialogue: 0,0:11:20.57,0:11:22.05,Default,Mio,0000,0000,0000,,Đừng lo về vụ đó.
Dialogue: 0,0:11:22.05,0:11:25.72,Default,Mio,0000,0000,0000,,Mình đã nhờ anh ấy đi mua sắm cùng Kei rồi.
Dialogue: 0,0:11:26.32,0:11:28.26,Default,Mio,0000,0000,0000,,Thôi nào, nhanh tay lên.
Dialogue: 0,0:11:29.11,0:11:31.99,Default,Ai,0000,0000,0000,,Thôi được rồi.
Dialogue: 0,0:11:31.99,0:11:33.71,Default,Ai,0000,0000,0000,,Mình sẽ cởi ra ngay...
Dialogue: 0,0:11:36.00,0:11:39.10,Default,Ai,0000,0000,0000,,Ghê quá, thế này thì thiệt là...
Dialogue: 0,0:11:39.10,0:11:42.22,Default,Ai,0000,0000,0000,,Phơi hết cả ra rồi.
Dialogue: 0,0:11:43.09,0:11:46.15,Default,Mio,0000,0000,0000,,Cô ta quấn cả dải băng xung quanh.
Dialogue: 0,0:11:46.15,0:11:48.97,Default,Mio,0000,0000,0000,,Thế này có khi còn mắc cỡ hơn là không mặc gì ấy nhỉ.
Dialogue: 0,0:11:49.97,0:11:54.30,Default,Mio,0000,0000,0000,,Cái gì? Cô ta đang bị nguyên một đám đông đùa giỡn.
Dialogue: 0,0:11:54.30,0:11:56.95,Default,Mio,0000,0000,0000,,Má ơi. Khắp người cô ta đầy tinh dịch.
Dialogue: 0,0:11:57.77,0:12:01.68,Default,Ai,0000,0000,0000,,Cô này chơi với cái dương vật to khiếp.
Dialogue: 0,0:12:02.44,0:12:05.95,Default,Ai,0000,0000,0000,,Mà hình như cô ta có vẻ rất sung sướng.
Dialogue: 0,0:12:06.61,0:12:09.94,Default,Ai,0000,0000,0000,,Họ làm tình ngoài trời này.
Dialogue: 0,0:12:10.35,0:12:16.90,Default,Ai,0000,0000,0000,,Sugiyama-kun thật dâm dê. Sao cậu ấy tàng trữ lắm ảnh đồi trụy thế nhỉ.
Dialogue: 0,0:12:16.91,0:12:19.02,Default,Ai,0000,0000,0000,,Đúng là yêu râu xanh.
Dialogue: 0,0:12:20.11,0:12:24.18,Default,Mio,0000,0000,0000,,Cái này chắc là "đồ chơi" dành cho người lớn.
Dialogue: 0,0:12:24.18,0:12:28.61,Default,Mio,0000,0000,0000,,Chắc Shuu nii-chan muốn xài thử mấy thứ này lắm.
Dialogue: 0,0:12:29.43,0:12:31.35,Default,Mio,0000,0000,0000,,Mắc cỡ chết đi được.
Dialogue: 0,0:12:31.35,0:12:37.71,Default,Ai,0000,0000,0000,,Mio-san, không hiểu sao mình lại bắt đầu cảm thấy khó chịu.
Dialogue: 0,0:12:37.71,0:12:40.52,Default,Mio,0000,0000,0000,,Phải, mình cũng vậy.
Dialogue: 0,0:12:41.59,0:12:46.74,Default,Ai,0000,0000,0000,,Vậy nếu cả hai đều không cưỡng lại được, thì chúng ta làm thử theo tạp chí luôn nhé?
Dialogue: 0,0:12:47.78,0:12:56.42,Default,Ai,0000,0000,0000,,Kể ra thì chỉ lí thuyết suông cũng khôn ổn, phải siêng thực hành mới nhanh lãnh hội được vấn đề.
Dialogue: 0,0:12:56.78,0:12:59.60,Default,Mio,0000,0000,0000,,Phải, cậu nói đúng.
Dialogue: 0,0:12:59.60,0:13:06.84,Default,Mio,0000,0000,0000,,Con gái với nhau cả, có gì mà phải xấu hổ chứ.
Dialogue: 0,0:13:19.20,0:13:23.95,Default,Ai,0000,0000,0000,,Ở trại tập huấn, chúng ta vẫn thường tắm cùng nhau...
Dialogue: 0,0:13:23.95,0:13:28.57,Default,Ai,0000,0000,0000,,nhưng mà làm thế này cũng hơi mắc cỡ.
Dialogue: 0,0:13:29.55,0:13:32.57,Default,Mio,0000,0000,0000,,Nè, mình chạm vào cậu nhé?
Dialogue: 0,0:13:33.93,0:13:36.08,Default,Ai,0000,0000,0000,,Rồi, làm đi.
Dialogue: 0,0:13:37.06,0:13:41.04,Default,Mio,0000,0000,0000,,Oa, da dẻ cậu thật mềm mại.
Dialogue: 0,0:13:41.04,0:13:45.09,Default,Ai,0000,0000,0000,,Cậu xoa nắn chỗ đó thật dịu dàng.
Dialogue: 0,0:13:45.09,0:13:46.79,Default,Ai,0000,0000,0000,,Khác hẳn với tụi con trai.
Dialogue: 0,0:13:48.77,0:13:49.72,Default,Mio,0000,0000,0000,,Nè!
Dialogue: 0,0:13:49.72,0:13:54.91,Default,Ai,0000,0000,0000,,Có phải khi hưng phấn thì hai đầu ti sẽ cứng lên không?
Dialogue: 0,0:13:55.39,0:13:59.50,Default,Ai,0000,0000,0000,,Mình khá nhạy cảm ở nơi này nên cũng hơi mắc cỡ.
Dialogue: 0,0:14:00.31,0:14:02.04,Default,Mio,0000,0000,0000,,Mình cũng vậy.
Dialogue: 0,0:14:02.04,0:14:07.87,Default,Mio,0000,0000,0000,,Khi cậu chạm vô, như có luồng điện chạy qua. Cảm giác rất chân thật.
Dialogue: 0,0:14:07.88,0:14:13.96,Default,Ai,0000,0000,0000,,Mio-san, hay chúng ta lên giường làm tiếp nhé.
Dialogue: 0,0:14:18.49,0:14:22.02,Default,Mio,0000,0000,0000,,Có lẽ chúng ta đều dâm như nhau cả.
Dialogue: 0,0:14:22.02,0:14:25.09,Default,Mio,0000,0000,0000,,Đều là con gái mà cả hai lại ướt đẫm.
Dialogue: 0,0:14:26.50,0:14:32.87,Default,Ai,0000,0000,0000,,Tất cả đều là lỗi của Sugiyama-kun. Cậu ấy đã làm nhiều trò xấu xa với chúng ta.
Dialogue: 0,0:14:34.17,0:14:37.69,Default,Mio,0000,0000,0000,,Mình chọc ngón tay vào nhé, Ai-san.
Dialogue: 0,0:14:40.17,0:14:44.35,Default,Ai,0000,0000,0000,,Làm đi. Mình sẽ bắt chước cậu.
Dialogue: 0,0:14:47.36,0:14:51.25,Default,Mio,0000,0000,0000,,Nè Ai-san, cậu có hay thủ dâm không?
Dialogue: 0,0:14:51.25,0:14:56.16,Default,Ai,0000,0000,0000,,Có chứ. Tầm ba lần một tuần.
Dialogue: 0,0:14:56.75,0:15:03.35,Default,Mio,0000,0000,0000,,Mình thì hay tưởng tượng ra cảnh được Shuu nii-chan đang ôm ấp vuốt ve mỗi khi thủ dâm.
Dialogue: 0,0:15:04.65,0:15:11.18,Default,Ai,0000,0000,0000,,Mình cũng vậy. Mình toàn nhớ lại những lúc cùng Sugiyama-kun làm chuyện đó.
Dialogue: 0,0:15:12.42,0:15:15.67,Default,Mio,0000,0000,0000,,Mình muốn làm chuyện đó với Shuu nii-chan lắm.
Dialogue: 0,0:15:16.43,0:15:19.91,Default,Mio,0000,0000,0000,,Muốn anh ấy đâm thằng nhỏ vào bên trong nơi này.
Dialogue: 0,0:15:21.39,0:15:28.63,Default,Ai,0000,0000,0000,,Phải. Mình muốn cậu ấy đâm thứ đó vào thật sâu, thật sâu bên trong mình.
Dialogue: 0,0:15:28.63,0:15:35.49,Default,Ai,0000,0000,0000,,Nếu Sugiyama-kun muốn, cậu ấy có thể làm những trò trong cuốn tạp chí với mình.
Dialogue: 0,0:15:36.60,0:15:39.96,Default,Mio,0000,0000,0000,,Càng ngày mình lại càng dâm đãng.
Dialogue: 0,0:15:40.74,0:15:44.34,Default,Mio,0000,0000,0000,,Mình thậm chí còn bắt đầu tự làm mấy trò đáng xấu hổ.
Dialogue: 0,0:15:45.80,0:15:48.50,Default,Ai,0000,0000,0000,,Mình hết chịu nổi rồi. Mình sắp ra rồi.
Dialogue: 0,0:15:48.97,0:15:50.54,Default,Ai,0000,0000,0000,,Mạnh nữa lên nào. Mình ra rồi.
Dialogue: 0,0:15:53.08,0:15:58.52,Default,Mio,0000,0000,0000,,Mình cũng sắp ra rồi! Mình ra rồi!
Dialogue: 0,0:15:58.86,0:16:03.87,Default,Both,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:16:06.25,0:16:12.98,Default,Ai,0000,0000,0000,,Mio-san và tôi đã quyết định triển khai kế hoạch sau ba ngày nữa, khi buổi thi đấu kết thúc.
Dialogue: 0,0:16:21.60,0:16:25.13,Default,Shuu,0000,0000,0000,,Hả? Đi đâu hết rồi?
Dialogue: 0,0:16:25.13,0:16:27.81,Default,Shuu,0000,0000,0000,,Bọn họ gọi mình ra đây làm gì vậy nhỉ?
Dialogue: 0,0:16:28.63,0:16:30.66,Default,Mio,0000,0000,0000,,Shuu nii-chan.
Dialogue: 0,0:16:30.66,0:16:33.05,Default,Shuu,0000,0000,0000,,Mio, hóa ra em ở đây à?
Dialogue: 0,0:16:35.78,0:16:40.66,Default,Mio,0000,0000,0000,,Thế nào? Trông em mặc bộ này có hợp không?
Dialogue: 0,0:16:41.70,0:16:45.17,Default,Shuu,0000,0000,0000,,Mio, em đang làm trò gì vậy?
Dialogue: 0,0:16:45.17,0:16:47.15,Default,Shuu,0000,0000,0000,,lại cả Saeki-san nữa?
Dialogue: 0,0:16:47.15,0:16:52.02,Default,Ai,0000,0000,0000,,Mình nghĩ cậu thích trang phục Cosplay.
Dialogue: 0,0:16:52.02,0:16:53.27,Default,Shuu,0000,0000,0000,,Ế?
Dialogue: 0,0:16:53.88,0:17:02.44,Default,Mio,0000,0000,0000,,Bọn em đã quyết định hôm nay sẽ phục vụ anh những trò hay ho mà trước giờ chưa từng có.
Dialogue: 0,0:17:02.44,0:17:08.16,Default,Ai,0000,0000,0000,,Mặc dù rất mắc cỡ nhưng tụi mình sẽ gắng hết sức.
Dialogue: 0,0:17:08.58,0:17:13.16,Default,Shuu,0000,0000,0000,,Ơ, chuyện gì đang xảy ra vậy? {yeah man, I'd be terrified too}
Dialogue: 0,0:17:15.31,0:17:18.43,Default,Shuu,0000,0000,0000,,Khoan đã nào, Saeki-san.
Dialogue: 0,0:17:19.12,0:17:23.37,Default,Ai,0000,0000,0000,,Xin lỗi, nhưng mình không thể chờ lâu hơn nữa.
Dialogue: 0,0:17:23.37,0:17:29.20,Default,Ai,0000,0000,0000,,Và cậu cũng bự lắm rồi nè.
Dialogue: 0,0:17:32.90,0:17:34.63,Default,Shu,0000,0000,0000,,Làm vậy không hay đâu.
Dialogue: 0,0:17:34.63,0:17:38.16,Default,Shu,0000,0000,0000,,Nếu cậu còn tiếp tục, mình sẽ không kiềm chế nổi mất...
Dialogue: 0,0:17:39.09,0:17:43.29,Default,Ai,0000,0000,0000,,Không sao mà. Cậu thích sao mình cũng chiều.
Dialogue: 0,0:17:44.20,0:17:48.67,Default,Ai,0000,0000,0000,,Hôm nay mình muốn thác loạn một phen.
Dialogue: 0,0:17:48.67,0:17:50.31,Default,,0000,0000,0000,,Saeki-san...
Dialogue: 0,0:17:51.30,0:17:54.10,Default,Ai,0000,0000,0000,,Làm ơn đi, Sugiyama-kun.
Dialogue: 0,0:17:54.89,0:17:56.47,Default,Ai,0000,0000,0000,,Đáp lễ đi nào.
Dialogue: 0,0:17:56.47,0:18:00.40,Default,Ai,0000,0000,0000,,Chơi đùa nơi thầm kín đó của mình đi.
Dialogue: 0,0:18:00.40,0:18:02.35,Default,Shuu,0000,0000,0000,,Nếu đã vậy...
Dialogue: 0,0:18:05.39,0:18:07.73,Default,Ai,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,0:18:07.73,0:18:11.15,Default,Ai,0000,0000,0000,,Thô bạo nữa lên nào.
Dialogue: 0,0:18:12.90,0:18:16.21,Default,Shuu,0000,0000,0000,,Cậu ướt đẫm rồi nè.
Dialogue: 0,0:18:17.33,0:18:18.68,Default,Ai,0000,0000,0000,,Không!
Dialogue: 0,0:18:18.68,0:18:23.01,Default,Ai,0000,0000,0000,,Xin lỗi, mình bắt đầu hứng lên rồi.
Dialogue: 0,0:18:23.78,0:18:28.20,Default,Ai,0000,0000,0000,,Chơi đùa với thằng nhỏ của cậu đã khiến mình nổi hứng lên.
Dialogue: 0,0:18:29.87,0:18:33.57,Default,Ai,0000,0000,0000,,Cậu thấy không? Mình sẽ dùng tới bộ ngực.
Dialogue: 0,0:18:33.57,0:18:37.99,Default,Ai,0000,0000,0000,,Chúng đang nóng ran bởi cơn hứng tình của mình.
Dialogue: 0,0:18:38.45,0:18:46.47,Default,Mio,0000,0000,0000,,Oa, dù Ai-san hay mắc cỡ nhưng tới lúc cần cô ấy cũng thật bạo dạn.
Dialogue: 0,0:18:46.47,0:18:49.98,Default,Mio,0000,0000,0000,,Điều đó khiến mình cảm thấy khó chịu.
Dialogue: 0,0:18:56.55,0:19:00.21,Default,Ai,0000,0000,0000,,Không! Đừng liếm nơi ẩm ướt đó.
Dialogue: 0,0:19:00.21,0:19:03.16,Default,Shuu,0000,0000,0000,,Cậu không muốn mình làm vậy hả?
Dialogue: 0,0:19:06.23,0:19:08.12,Default,Ai,0000,0000,0000,,Không, xin lỗi!
Dialogue: 0,0:19:08.92,0:19:14.63,Default,Ai,0000,0000,0000,,Mau liếm chỗ đó nữa đi... liếm âm hộ mình nhiều vào.
Dialogue: 0,0:19:14.99,0:19:19.52,Default,Shuu,0000,0000,0000,,Sao cậu nói toàn dâm ngôn uế ngữ vậy?!!
Dialogue: 0,0:19:19.52,0:19:21.26,Default,Ai,0000,0000,0000,,Ừ thì sao.
Dialogue: 0,0:19:23.64,0:19:28.79,Default,Ai,0000,0000,0000,,Chỉ cần cậu thích nghe là được.
Dialogue: 0,0:19:29.41,0:19:31.60,Default,Shuu,0000,0000,0000,,Vậy cậu mau nói tiếp đi.
Dialogue: 0,0:19:33.31,0:19:42.03,Default,Ai,0000,0000,0000,,Khi chơi đùa với thằng nhỏ... ý mình là, dương vật của cậu, âm hộ...âm hộ mình có cảm giác rất sung sướng.
Dialogue: 0,0:19:43.66,0:19:46.68,Default,Shuu,0000,0000,0000,,Cậu dâm thật đấy, Saeki-san.
Dialogue: 0,0:19:46.68,0:19:53.32,Default,Ai,0000,0000,0000,,Dù đang mắc cỡ nhưng tại sao mình lại cảm thấy sung sướng như vậy nhỉ?
Dialogue: 0,0:19:53.32,0:19:55.44,Default,Shuu,0000,0000,0000,,Mình sắp ra rồi.
Dialogue: 0,0:19:58.12,0:20:01.02,Default,Ai,0000,0000,0000,,Mình cũng vậy, mình sắp ra, sắp ra rồi!
Dialogue: 0,0:20:01.02,0:20:05.07,Default,Ai,0000,0000,0000,,Mau bắn tất cả tinh dịch của cậu lên người mình đi!
Dialogue: 0,0:20:05.07,0:20:08.75,Default,Ai,0000,0000,0000,,Trừng phạt bản tính dâm đãng của mình đi.
Dialogue: 0,0:20:12.41,0:20:13.52,Default,Ai,0000,0000,0000,,Ôi không!
Dialogue: 0,0:20:19.93,0:20:26.12,Default,Ai,0000,0000,0000,,Oa, cậu ra nhiều thật đó.
Dialogue: 0,0:20:26.67,0:20:32.29,Default,Mio,0000,0000,0000,,Nhưng mà thằng nhỏ của Shuu nii-chan vẫn còn đang chào cờ.
Dialogue: 0,0:20:34.45,0:20:39.50,Default,Shuu,0000,0000,0000,,Dừng lại, Mio. Anh vừa ra xong mà.
Dialogue: 0,0:20:39.50,0:20:41.16,Default,Mio,0000,0000,0000,,Còn lâu.
Dialogue: 0,0:20:41.16,0:20:46.43,Default,Mio,0000,0000,0000,,Hôm nay, em sẽ hấp khô anh.
Dialogue: 0,0:20:47.13,0:20:52.04,Default,Mio,0000,0000,0000,,Thấy hai đầu ti em cương cứng lên không hả?!
Dialogue: 0,0:20:52.04,0:20:56.87,Default,Mio,0000,0000,0000,,Dù không so được với Sensei hay Ai-san, nhưng vẫn rất sướng phải không?
Dialogue: 0,0:20:58.43,0:21:02.53,Default,Mio,0000,0000,0000,,Những lúc thế này trông anh dễ thương lắm.
Dialogue: 0,0:21:02.53,0:21:05.07,Default,Mio,0000,0000,0000,,Khiến em chỉ muốn trêu đùa anh thêm thôi.
Dialogue: 0,0:21:06.30,0:21:07.13,Default,Shuu,0000,0000,0000,,Bắt đầu. {missing line}
Dialogue: 0,0:21:18.59,0:21:19.74,Default,Shuu,0000,0000,0000,,Saeki-san?
Dialogue: 0,0:21:22.87,0:21:25.77,Default,Ai,0000,0000,0000,,Mio-san, mình đâm vào nhé.
Dialogue: 0,0:21:25.77,0:21:28.14,Default,Mio,0000,0000,0000,,Ừ, mau làm đi.
Dialogue: 0,0:21:28.44,0:21:32.98,Default,Mio,0000,0000,0000,,Mình ướt đẫm rồi.
Dialogue: 0,0:21:36.47,0:21:41.43,Default,Mio,0000,0000,0000,,Nó đang đâm vào bên trong. Sâu quá đi mất.
Dialogue: 0,0:21:43.87,0:21:46.40,Default,Shuu,0000,0000,0000,,Mio... Saeki-san...
Dialogue: 0,0:21:46.83,0:21:54.41,Default,Mio,0000,0000,0000,,Coi nè, Shuu nii-chan. Thấy tụi em dâm đãng không hả!
Dialogue: 0,0:21:57.54,0:22:02.55,Default,Mio,0000,0000,0000,,Anh có thấy hứng thú với cách...
Dialogue: 0,0:22:02.55,0:22:04.70,Default,Mio,0000,0000,0000,,mà em dùng để hầu hạ...
Dialogue: 0,0:22:04.70,0:22:06.05,Default,Mio,0000,0000,0000,,thằng nhỏ của anh không?
Dialogue: 0,0:22:07.01,0:22:13.38,Default,Ai,0000,0000,0000,,Mio-san, sướng quá, mình không thể ngừng lại được.
Dialogue: 0,0:22:15.12,0:22:18.17,Default,Mio,0000,0000,0000,,Mình cũng vậy, cảm giác thật phấn khích.
Dialogue: 0,0:22:19.00,0:22:24.15,Default,Mio,0000,0000,0000,,phía sau là cậu, còn phía trước là thằng nhỏ của Shuu nii-chan... {this and the previous lines p much made up, she's talking with a dick in her mouth}
Dialogue: 0,0:22:25.95,0:22:27.26,Default,Shuu,0000,0000,0000,,Mio, anh sắp...
Dialogue: 0,0:22:28.82,0:22:36.20,Default,Mio,0000,0000,0000,,Ra đi nào, Shuu nii-chan. Bắn nó vào miệng của em nè.
Dialogue: 0,0:22:37.21,0:22:40.99,Default,Ai,0000,0000,0000,,Ôi không, mình sắp ra nữa rồi.
Dialogue: 0,0:22:41.94,0:22:45.24,Default,Mio,0000,0000,0000,,Không, Ai-san, mình sắp ra rồi.
Dialogue: 0,0:22:45.24,0:22:48.42,Default,Mio,0000,0000,0000,,Nhịp vào, nhịp mạnh nữa vào!
Dialogue: 0,0:22:49.13,0:22:49.90,Default,,0000,0000,0000,,Ra rồi!
Dialogue: 0,0:22:58.69,0:23:04.25,Default,Mio,0000,0000,0000,,Nóng quá. Tinh dịch của anh thật nóng và ẩm ướt.
Dialogue: 0,0:23:04.73,0:23:07.09,Default,Shuu,0000,0000,0000,,Xin lỗi, Mio.
Dialogue: 0,0:23:07.09,0:23:08.61,Default,Mio,0000,0000,0000,,Đừng nói vậy.
Dialogue: 0,0:23:08.61,0:23:15.22,Default,Mio,0000,0000,0000,,Tới lúc anh phải vận động rồi, đâm nó vào bên trong âm hộ em đi.
Dialogue: 0,0:23:21.84,0:23:25.11,Default,Mio,0000,0000,0000,,Với tư thế này thì anh có thể chơi cả hai cùng một lúc.
Dialogue: 0,0:23:25.65,0:23:32.24,Default,Ai,0000,0000,0000,,Nào Sugiyama-kun, tụi mình hết chịu nổi rồi.
Dialogue: 0,0:23:32.24,0:23:35.65,Default,Shuu,0000,0000,0000,,Trông hai người thế kia, mình chỉ muốn ăn ngay lập tức...
Dialogue: 0,0:23:36.56,0:23:45.14,Default,Ai,0000,0000,0000,,Phải. Bọn mình làm vậy là vì cậu mà, vừa ý cậu rồi nhé.
Dialogue: 0,0:23:45.75,0:23:47.37,Default,Shuu,0000,0000,0000,,Mình đâm vào đây.
Dialogue: 0,0:23:51.05,0:23:52.11,Default,Mio,0000,0000,0000,,Shuu nii-chan!
Dialogue: 0,0:23:55.75,0:24:03.15,Default,Ai,0000,0000,0000,,Tuyệt quá! Thằng nhỏ của Sugiyama-kun đang chà xát lên âm hộ hai ta.
Dialogue: 0,0:24:03.15,0:24:07.02,Default,Shuu,0000,0000,0000,,Hạ thể hai người đều nóng ran.
Dialogue: 0,0:24:07.02,0:24:12.22,Default,Mio,0000,0000,0000,,Không, đừng nói thế. Là do anh hết đó.
Dialogue: 0,0:24:12.22,0:24:19.95,Default,Ai,0000,0000,0000,,Từ lâu bọn mình đã thèm muốn thằng nhỏ của cậu rồi.
Dialogue: 0,0:24:21.16,0:24:25.48,Default,Mio,0000,0000,0000,,Tệ quá. Những chỗ kín đáo của chúng ta đều đã ướt nhẹp.
Dialogue: 0,0:24:26.27,0:24:28.32,Default,Mio,0000,0000,0000,,Biến thái quá đi mà!
Dialogue: 0,0:24:28.32,0:24:29.94,Default,,0000,0000,0000,,Mio-san!
Dialogue: 0,0:24:30.82,0:24:36.50,Default,Ai,0000,0000,0000,,Mio-san, hai đầu ti của cậu đang cạ vào mình!
Dialogue: 0,0:24:36.50,0:24:44.05,Default,Mio,0000,0000,0000,,Vậy hả? Ngực cậu cũng nóng hầm hập, cậu dâm y như mình!
Dialogue: 0,0:24:55.76,0:24:59.20,Default,Shuu,0000,0000,0000,,Anh không chịu nổi nữa rồi.
Dialogue: 0,0:24:59.20,0:25:01.40,Default,Mio,0000,0000,0000,,Shuu nii-chan, anh cứ việc... {made up last part, not sure what she says}
Dialogue: 0,0:25:01.40,0:25:07.44,Default,Mio,0000,0000,0000,,Bắn hết tinh dịch vô âm hộ tụi em.
Dialogue: 0,0:25:07.44,0:25:09.37,Default,Ai,0000,0000,0000,,Phải.
Dialogue: 0,0:25:09.37,0:25:14.25,Default,Ai,0000,0000,0000,,Cậu muốn bắn vô đâu cũng được.
Dialogue: 0,0:25:15.70,0:25:19.96,Default,Ai,0000,0000,0000,,Khiến tụi mình chết ngộp trong tinh dịch đi nào. {lololololol}
Dialogue: 0,0:25:20.55,0:25:23.38,Default,Mio,0000,0000,0000,,Sắp ra rồi. Mio sắp ra nữa rồi.
Dialogue: 0,0:25:23.38,0:25:26.76,Default,Mio,0000,0000,0000,,Dâm thủy của Mio đang trào ra như suối.
Dialogue: 0,0:25:26.76,0:25:29.68,Default,Ai,0000,0000,0000,,Mình cũng vậy, mình sắp ra rồi.
Dialogue: 0,0:25:29.68,0:25:32.80,Default,Ai,0000,0000,0000,,Âm hộ mình đang co giật liên hồi...
Dialogue: 0,0:25:32.80,0:25:35.50,Default,Ai,0000,0000,0000,,Cùng nhau... Chúng ta hãy ra cùng nhau nào.
Dialogue: 0,0:25:36.78,0:25:38.57,Default,Mio,0000,0000,0000,,Bắn ra! Bắn ra đi, mau bắn ra đi!
Dialogue: 0,0:25:52.55,0:25:54.22,Default,Ai,0000,0000,0000,,Tuyệt quá...
Dialogue: 0,0:25:55.61,0:25:59.28,Default,Ai,0000,0000,0000,,Cảm giác phía dưới thật nhột nhạt.
Dialogue: 0,0:26:01.60,0:26:05.54,Default,Mio,0000,0000,0000,,Đây là lần đầu tiên mình ra nhiều như vậy.
Dialogue: 0,0:26:07.92,0:26:11.26,Default,Reiko,0000,0000,0000,,Đủ rồi đó ba bạn trẻ.
Dialogue: 0,0:26:13.25,0:26:16.10,Default,Ai,0000,0000,0000,,Sao Senpai lại biết chuyện này?
Dialogue: 0,0:26:16.40,0:26:22.31,Default,Reiko,0000,0000,0000,,Ba người la hét to như vậy mà tôi không nghe thấy thì hơi phí?
Dialogue: 0,0:26:26.90,0:26:31.79,Default,Reiko,0000,0000,0000,,Hai người nghĩ gì trong đầu vậy, mặc mấy thứ đó mà vẫn chơi được sao?
Dialogue: 0,0:26:31.79,0:26:43.03,Default,Ai,0000,0000,0000,,Em xin lỗi. Tại vì trông thấy Sugiyama-kun làm chuyện đó với Sensei, nên chúng em sợ sẽ trở thành kẻ thua cuộc.
Dialogue: 0,0:26:43.03,0:26:47.39,Default,Teach,0000,0000,0000,,Oái, hai em nhìn lén cô à?
Dialogue: 0,0:26:47.39,0:26:49.24,Default,Teach,0000,0000,0000,,Xấu hổ chết đi được.
Dialogue: 0,0:26:49.99,0:26:55.60,Default,Mio,0000,0000,0000,,Đều tại Shuu nii-chan cả, ba phải cho lắm vào.
Dialogue: 0,0:26:55.60,0:26:57.65,Default,Mio,0000,0000,0000,,Anh ấy quá cả nể người khác.
Dialogue: 0,0:26:57.65,0:27:04.06,Default,Reiko,0000,0000,0000,,Vậy hả? Còn chị lại thấy thích điều đó.
Dialogue: 0,0:27:04.39,0:27:07.06,Default,Mio,0000,0000,0000,,Ế? Cái giề?
Dialogue: 0,0:27:08.15,0:27:14.19,Default,Ai,0000,0000,0000,,Sugiyama-kun vốn đâu phải người tự cao tự đại.
Dialogue: 0,0:27:14.20,0:27:18.95,Default,Teach,0000,0000,0000,,Tốt bụng chính là ưu điểm của cậu ấy.
Dialogue: 0,0:27:19.25,0:27:23.94,Default,Mio,0000,0000,0000,,Ơ này mọi người! Sao tự dưng lại đổi sang đề tài này thế?
Dialogue: 0,0:27:24.46,0:27:25.58,Default,Reiko,0000,0000,0000,,Ờ phải đấy.
Dialogue: 0,0:27:26.65,0:27:33.08,Default,Ai,0000,0000,0000,,Cuối cùng, hóa ra tất cả mọi người đều có tình cảm với Sugiyama-kun.
Dialogue: 0,0:27:34.00,0:27:39.14,Default,Ai,0000,0000,0000,,Mình thật sự rất yêu Sugiyama-kun.
