Dialogue: 0,0:00:11.51,0:00:19.68,Fetish,,0000,0000,0000,,{\pos(530,36)}présente
Dialogue: 0,0:00:11.51,0:00:19.68,J-subs,,0000,0000,0000,,{\pos(400,40)}[Johnny-subs]
Dialogue: 0,0:00:19.85,0:00:22.01,ROMAJI,,0000,0000,0000,,Saa hajimemashou
Dialogue: 0,0:00:19.85,0:00:22.01,Fetish,,0000,0000,0000,,{\i1}Allez, commençons !{\i0}
Dialogue: 0,0:00:22.08,0:00:25.27,ROMAJI,,0000,0000,0000,,Atomodori wa sasenai
Dialogue: 0,0:00:22.08,0:00:25.27,Fetish,,0000,0000,0000,,{\i1}Je ne te laisserai pas faire\Nmarche arrière{\i0}
Dialogue: 0,0:00:25.53,0:00:26.92,Fetish,,0000,0000,0000,,{\i1}Are you ready for game?{\i0}
Dialogue: 0,0:00:25.53,0:00:26.92,ROMAJI,,0000,0000,0000,,Are you ready for game?
Dialogue: 0,0:00:27.06,0:00:30.57,ROMAJI,,0000,0000,0000,,Anata no kokoro wo kakimidasu
Dialogue: 0,0:00:27.06,0:00:30.57,Fetish,,0000,0000,0000,,{\i1}Mon petit coup d'œil{\i0}
Dialogue: 0,0:00:30.63,0:00:34.63,ROMAJI,,0000,0000,0000,,Shisen wa yami tsuku Trick
Dialogue: 0,0:00:30.63,0:00:34.63,Fetish,,0000,0000,0000,,{\i1}trouble ton cœur{\i0}
Dialogue: 0,0:00:34.68,0:00:38.11,ROMAJI,,0000,0000,0000,,Tamaranaku ito oshii
Dialogue: 0,0:00:34.68,0:00:38.11,Fetish,,0000,0000,0000,,{\i1}Combien de temps encore{\i0}
Dialogue: 0,0:00:38.15,0:00:41.68,ROMAJI,,0000,0000,0000,,Itsumo no Poker Face
Dialogue: 0,0:00:38.15,0:00:41.68,Fetish,,0000,0000,0000,,{\i1}pourras-tu faire semblant{\i0}
Dialogue: 0,0:00:41.74,0:00:44.98,ROMAJI,,0000,0000,0000,,Heiki na furi dore dake dekiru
Dialogue: 0,0:00:41.74,0:00:44.98,Fetish,,0000,0000,0000,,{\i1}d'être stoïque ?{\i0}
Dialogue: 0,0:00:45.04,0:00:49.86,ROMAJI,,0000,0000,0000,,Konya kowashite ageru wa
Dialogue: 0,0:00:45.04,0:00:49.86,Fetish,,0000,0000,0000,,{\i1}Ce soir, je vais briser\Nce faux-semblant{\i0}
Dialogue: 0,0:00:49.92,0:00:53.18,ROMAJI,,0000,0000,0000,,Tamesareru hayai kodou
Dialogue: 0,0:00:49.92,0:00:53.18,Fetish,,0000,0000,0000,,{\i1}Tu vas expérimenter les palpitations,{\i0}
Dialogue: 0,0:00:53.25,0:00:57.42,ROMAJI,,0000,0000,0000,,Kakehiki wa amai biyaku
Dialogue: 0,0:00:53.25,0:00:57.42,Fetish,,0000,0000,0000,,{\i1}la stratégie est\Nun doux aphrodisiaque{\i0}
Dialogue: 0,0:00:57.46,0:01:00.49,ROMAJI,,0000,0000,0000,,Fuzoroi ni sashidashita shite
Dialogue: 0,0:00:57.46,0:01:00.49,Fetish,,0000,0000,0000,,{\i1}Cette petite irrégularité procure{\i0}
Dialogue: 0,0:01:00.53,0:01:03.05,ROMAJI,,0000,0000,0000,,ZURUI kimochi
Dialogue: 0,0:01:00.53,0:01:03.05,Fetish,,0000,0000,0000,,{\i1}une sensation de malice{\i0}
Dialogue: 0,0:01:03.61,0:01:05.65,ROMAJI,,0000,0000,0000,,Sou Love Game
Dialogue: 0,0:01:03.61,0:01:05.65,Fetish,,0000,0000,0000,,{\i1}Oui, le jeu de l'amour,{\i0}
Dialogue: 0,0:01:05.69,0:01:07.43,ROMAJI,,0000,0000,0000,,Hi wo tsukete yo
Dialogue: 0,0:01:05.69,0:01:07.43,Fetish,,0000,0000,0000,,{\i1}c'est mettre le feu{\i0}
Dialogue: 0,0:01:07.51,0:01:12.41,ROMAJI,,0000,0000,0000,,Binetsu no mama ja tsumaranai kara
Dialogue: 0,0:01:07.51,0:01:12.41,Fetish,,0000,0000,0000,,{\i1}Cette petite fièvre ne vaut rien{\i0}
Dialogue: 0,0:01:12.58,0:01:15.11,ROMAJI,,0000,0000,0000,,Muchuu ni sasete
Dialogue: 0,0:01:12.58,0:01:15.11,Fetish,,0000,0000,0000,,{\i1}Te faire tourner la tête{\i0}
Dialogue: 0,0:01:15.23,0:01:19.25,ROMAJI,,0000,0000,0000,,Crazy motto... ai no wana...
Dialogue: 0,0:01:15.23,0:01:19.25,Fetish,,0000,0000,0000,,{\i1}à t'en rendre encore plus fou...\Nc'est ça, le piège de l'amour{\i0}
Dialogue: 0,0:01:33.91,0:01:37.23,Fetish,,0000,0000,0000,,{\i1}J'aime vraiment la couleur rouge...{\i0}
Dialogue: 0,0:01:39.64,0:01:42.97,Notes,,0000,0000,0000,,{\pos(700,45)}[Riona Kisaragi]
Dialogue: 0,0:01:42.69,0:01:44.72,Fetish,,0000,0000,0000,,{\i1}Rouge à lèvres...{\i0}
Dialogue: 0,0:01:45.50,0:01:47.02,Fetish,,0000,0000,0000,,{\i1}Manucures...{\i0}
Dialogue: 0,0:01:50.32,0:01:52.38,Fetish,,0000,0000,0000,,{\i1}Même les talons aiguilles...{\i0}
Dialogue: 0,0:01:53.55,0:01:55.42,Fetish,,0000,0000,0000,,{\i1}Et bien sûr, le tailleur.{\i0}
Dialogue: 0,0:01:57.47,0:01:59.58,Fetish,,0000,0000,0000,,{\i1}Je me sens diabolique en rouge.{\i0}
Dialogue: 0,0:02:11.69,0:02:14.31,Fetish,,0000,0000,0000,,{\i1}Je n'aimais pas\Nme détacher de la foule.{\i0}
Dialogue: 0,0:02:15.64,0:02:18.93,Fetish,,0000,0000,0000,,{\i1}Mais un jour, je me suis habillée\Nen rouge, et tout à coup...{\i0}
Dialogue: 0,0:02:19.54,0:02:21.01,Fetish,,0000,0000,0000,,{\i1}j'ai complètement changé.{\i0}
Dialogue: 0,0:02:22.20,0:02:24.91,Fetish,,0000,0000,0000,,{\i1}J'ai senti que je voulais\Nque les gens me remarquent,{\i0}
Dialogue: 0,0:02:25.67,0:02:28.46,Fetish,,0000,0000,0000,,moi, la femme vêtue de rouge.
Dialogue: 0,0:02:37.88,0:02:41.92,Fetish,,0000,0000,0000,,{\i1}Cependant, dès que je m'en suis aperçue,\Nj'ai couru vers les sanitaires.{\i0}
Dialogue: 0,0:02:46.79,0:02:50.36,Fetish,,0000,0000,0000,,{\i1}Peut-être que je voulais me débarrasser\Nde cette tension.{\i0}
Dialogue: 0,0:02:52.29,0:02:53.88,Fetish,,0000,0000,0000,,{\i1}Mais en réalité...{\i0}
Dialogue: 0,0:02:57.58,0:02:59.54,Fetish,,0000,0000,0000,,{\i1}Cette main droite vernie en rouge...{\i0}
Dialogue: 0,0:03:00.54,0:03:02.13,Fetish,,0000,0000,0000,,{\i1}... allait se faufiler{\i0}
Dialogue: 0,0:03:02.25,0:03:05.20,Fetish,,0000,0000,0000,,{\i1}dans les endroits gênants\Ndissimulés sous ma jupe.{\i0}
Dialogue: 0,0:03:07.67,0:03:09.74,Fetish,,0000,0000,0000,,{\i1}Et puis, ma main gauche{\i0}
Dialogue: 0,0:03:10.52,0:03:14.56,Fetish,,0000,0000,0000,,{\i1}allait frotter mes parties sensibles\Navec agressivité{\i0}
Dialogue: 0,0:03:15.27,0:03:17.93,Fetish,,0000,0000,0000,,{\i1}sous mon tailleur rouge,\Nsur ce doux tissu.{\i0}
Dialogue: 0,0:03:20.83,0:03:23.68,Fetish,,0000,0000,0000,,{\i1}Même ces jambes que je serrais\Nfermement en face des gens...{\i0}
Dialogue: 0,0:03:24.42,0:03:27.27,Fetish,,0000,0000,0000,,{\i1}Peut-être à cause du pouvoir\Nde mes talons aiguilles,{\i0}
Dialogue: 0,0:03:27.39,0:03:30.22,Fetish,,0000,0000,0000,,{\i1}mes jambes étaient grandes ouvertes.{\i0}
Dialogue: 0,0:03:31.34,0:03:34.58,Fetish,,0000,0000,0000,,{\i1}J'étais dans une situation\Noù je ne pouvais plus me contrôler.{\i0}
Dialogue: 0,0:03:35.83,0:03:36.71,Fetish,,0000,0000,0000,,{\i1}Oui...{\i0}
Dialogue: 0,0:03:37.53,0:03:39.60,Fetish,,0000,0000,0000,,{\i1}Je n'ai pas pu m'empêcher{\i0}
Dialogue: 0,0:03:39.67,0:03:43.11,Fetish,,0000,0000,0000,,{\i1}de me laisser aller à la luxure\Ndans un tel endroit.{\i0}
Dialogue: 0,0:03:44.77,0:03:49.22,Fetish,,0000,0000,0000,,{\i1}Je pense que le rouge a dû\Nrétablir mon esprit lubrique.{\i0}
Dialogue: 0,0:03:51.05,0:03:55.48,Fetish,,0000,0000,0000,,{\i1}Après ça, je me suis vêtue\Nde rouge sans réfléchir.{\i0}
Dialogue: 0,0:03:56.76,0:03:59.85,Fetish,,0000,0000,0000,,{\i1}Cependant, je me suis toujours assurée\Nde porter des sous-vêtements blancs.{\i0}
Dialogue: 0,0:04:01.21,0:04:04.67,Fetish,,0000,0000,0000,,{\i1}Car si jamais je portais aussi\Ndes sous-vêtements rouges,{\i0}
Dialogue: 0,0:04:05.76,0:04:07.90,Fetish,,0000,0000,0000,,{\i1}je ne sais pas ce qui m'arriverait.{\i0}
Dialogue: 0,0:04:09.66,0:04:12.28,Fetish,,0000,0000,0000,,{\i1}Est-ce que je suis bizarre ?{\i0}
Dialogue: 0,0:04:17.11,0:04:18.32,Fetish,,0000,0000,0000,,{\i1}Je m'appelle Riona.{\i0}
Dialogue: 0,0:04:19.10,0:04:22.67,Fetish,,0000,0000,0000,,{\i1}Pourriez-vous rester attentif\Nà mon histoire un peu plus longtemps ?{\i0}
Dialogue: 0,0:04:24.31,0:04:26.30,Fetish,,0000,0000,0000,,{\i1}Une femme qui croise les jambes,{\i0}
Dialogue: 0,0:04:27.18,0:04:29.04,Fetish,,0000,0000,0000,,{\i1}c'est très érotique.{\i0}
Dialogue: 0,0:04:29.97,0:04:34.30,Fetish,,0000,0000,0000,,{\i1}Bon, croiser les jambes\Nalors que je suis en talons aiguilles...{\i0}
Dialogue: 0,0:04:34.67,0:04:36.80,Fetish,,0000,0000,0000,,{\i1}Vous ne voyez pas ça souvent.{\i0}
Dialogue: 0,0:04:38.79,0:04:41.27,Fetish,,0000,0000,0000,,{\i1}Pour la femme adulte\Nque je suis devenue,{\i0}
Dialogue: 0,0:04:41.52,0:04:43.44,Fetish,,0000,0000,0000,,{\i1}plier une jambe sur le côté\Nconvient le mieux.{\i0}
Dialogue: 0,0:04:43.92,0:04:45.44,Fetish,,0000,0000,0000,,{\i1}C'est charmant, n'est-ce pas ?{\i0}
Dialogue: 0,0:04:46.71,0:04:49.91,Fetish,,0000,0000,0000,,{\i1}Ensuite, nous allons lever une jambe\Nen pliant le genou.{\i0}
Dialogue: 0,0:04:51.09,0:04:55.07,Fetish,,0000,0000,0000,,{\i1}Même si l'intéressée\Ndevait juste batifoler,{\i0}
Dialogue: 0,0:04:56.06,0:04:58.59,Fetish,,0000,0000,0000,,{\i1}cette position devrait tout à fait\Nexciter le sujet.{\i0}
Dialogue: 0,0:05:00.07,0:05:02.76,Fetish,,0000,0000,0000,,{\i1}Aussi, pour une pose telle que{\i0}
Dialogue: 0,0:05:02.82,0:05:05.87,Fetish,,0000,0000,0000,,{\i1}tendre les jambes comme ceci...{\i0}
Dialogue: 0,0:05:06.46,0:05:09.88,Fetish,,0000,0000,0000,,{\i1}Est-ce que ça ne suscite pas\Ninévitablement l'intérêt des hommes ?{\i0}
Dialogue: 0,0:05:10.73,0:05:12.74,Fetish,,0000,0000,0000,,{\i1}Cette impuissance peut\Nservir d'appât.{\i0}
Dialogue: 0,0:05:13.86,0:05:17.08,Fetish,,0000,0000,0000,,{\i1}Ensuite, passons à l'excellente\Nposition japonaise traditionnelle.{\i0}
Dialogue: 0,0:05:17.36,0:05:18.99,Fetish,,0000,0000,0000,,{\i1}Voici la position accroupie.{\i0}
Dialogue: 0,0:05:19.61,0:05:24.37,Fetish,,0000,0000,0000,,{\i1}Une femme intellectuelle peut s'asseoir\Ndans cette position sans aucun souci.{\i0}
Dialogue: 0,0:05:25.19,0:05:26.97,Fetish,,0000,0000,0000,,{\i1}Cet interstice est très stimulant.{\i0}
Dialogue: 0,0:05:27.83,0:05:31.93,Fetish,,0000,0000,0000,,{\i1}Cependant, si jamais l'on venait\Nà lever une jambe comme ceci...{\i0}
Dialogue: 0,0:05:32.49,0:05:33.92,Fetish,,0000,0000,0000,,{\i1}Ah, quel mystère...{\i0}
Dialogue: 0,0:05:34.29,0:05:37.67,Fetish,,0000,0000,0000,,{\i1}Soudain, la pose devient\Nextrêmement alléchante.{\i0}
Dialogue: 0,0:05:39.13,0:05:42.56,Fetish,,0000,0000,0000,,{\i1}Enfin, une pose prometteuse\Navec les jambes pliées.{\i0}
Dialogue: 0,0:05:43.63,0:05:45.78,Fetish,,0000,0000,0000,,{\i1}Cependant,\Ncontrairement à une très jeune fille{\i0}
Dialogue: 0,0:05:45.90,0:05:47.96,Fetish,,0000,0000,0000,,{\i1}qui se prépare dans cette position,{\i0}
Dialogue: 0,0:05:48.59,0:05:50.47,Fetish,,0000,0000,0000,,{\i1}c'est une pose pour une femme adulte{\i0}
Dialogue: 0,0:05:50.53,0:05:52.73,Fetish,,0000,0000,0000,,{\i1}qui rentre chez elle,\Népuisée par son travail.{\i0}
Dialogue: 0,0:05:53.03,0:05:56.44,Fetish,,0000,0000,0000,,{\i1}Imaginez-vous assise\Ncomme un canard.{\i0}
Dialogue: 0,0:05:57.99,0:06:01.21,Fetish,,0000,0000,0000,,{\i1}Sans vous préoccuper\Ndu regard des autres,{\i0}
Dialogue: 0,0:06:01.90,0:06:04.43,Fetish,,0000,0000,0000,,{\i1}et ce sentiment de soulagement...{\i0}
Dialogue: 0,0:06:04.90,0:06:09.61,Fetish,,0000,0000,0000,,{\i1}En vérité, c'est un trésor\Nque l'on ne voit pas souvent.{\i0}
Dialogue: 0,0:06:12.31,0:06:15.84,Fetish,,0000,0000,0000,,{\i1}Je vous remercie\Nd'avoir regardé jusqu'à la fin.{\i0}
Dialogue: 0,0:06:17.43,0:06:20.96,Fetish,,0000,0000,0000,,{\i1}C'était la femme qui devient audacieuse\Nquand elle s'habille en rouge,{\i0}
Dialogue: 0,0:06:21.08,0:06:22.50,Fetish,,0000,0000,0000,,{\i1}Riona Kisaragi.{\i0}
Dialogue: 0,0:06:41.44,0:06:43.17,Fetish,,0000,0000,0000,,Bonsoir, chéri.
Dialogue: 0,0:06:47.94,0:06:50.65,Notes,,0000,0000,0000,,{\pos(580,40)}[Yuki Shihôdô]
Dialogue: 0,0:06:52.18,0:06:54.01,Fetish,,0000,0000,0000,,Tu as bien travaillé, aujourd'hui.
Dialogue: 0,0:06:58.75,0:07:02.83,Notes,,0000,0000,0000,,{\pos(440,40)}[Pose le pied sur la chaise]
Dialogue: 0,0:07:03.23,0:07:04.44,Fetish,,0000,0000,0000,,Qu'en penses-tu ?
Dialogue: 0,0:07:26.82,0:07:32.07,Notes,,0000,0000,0000,,{\pos(440,60)}[Écarte les cuisses,\Npuis remonte ta jupe.]
Dialogue: 0,0:08:03.73,0:08:09.15,Notes,,0000,0000,0000,,{\pos(440,40)}[Laisse-moi voir ta culotte.]
Dialogue: 0,0:08:21.81,0:08:25.24,Fetish,,0000,0000,0000,,Chéri... si tu veux mieux voir\Nma culotte,
Dialogue: 0,0:08:26.32,0:08:30.22,Fetish,,0000,0000,0000,,tu peux relever ma jupe\Npar derrière.
Dialogue: 0,0:08:52.56,0:08:54.93,Fetish,,0000,0000,0000,,Oh ! Bonjour, chéri.
Dialogue: 0,0:08:56.33,0:08:58.22,Fetish,,0000,0000,0000,,Le petit déjeuner sera bientôt prêt.
Dialogue: 0,0:09:05.64,0:09:08.47,Fetish,,0000,0000,0000,,Euh... ne me regarde pas\Naussi fixement, s'il te plaît.
Dialogue: 0,0:09:09.44,0:09:10.54,Fetish,,0000,0000,0000,,Ça me gêne...
Dialogue: 0,0:09:13.80,0:09:18.50,Fetish,,0000,0000,0000,,Chéri, tu as dit que tu voulais me voir\Nporter un costume de ce genre.
Dialogue: 0,0:09:22.56,0:09:25.01,Fetish,,0000,0000,0000,,J'ai fait de mon mieux...
Dialogue: 0,0:09:30.12,0:09:31.02,Fetish,,0000,0000,0000,,Et puis...
Dialogue: 0,0:09:32.36,0:09:34.59,Fetish,,0000,0000,0000,,Il y a aussi un bonus...
Dialogue: 0,0:09:40.56,0:09:44.57,Fetish,,0000,0000,0000,,Ça finit par devenir\Nquelque chose de très étrange.
Dialogue: 0,0:09:45.96,0:09:48.79,Fetish,,0000,0000,0000,,Je me suis demandée si je devais\Nte montrer une telle chose.
Dialogue: 0,0:09:49.89,0:09:54.55,Fetish,,0000,0000,0000,,Mais je veux qu'aujourd'hui,\Ntu te donnes à fond au travail.
Dialogue: 0,0:09:56.93,0:09:58.32,Fetish,,0000,0000,0000,,Je te le montre donc.
Dialogue: 0,0:10:21.66,0:10:23.45,Fetish,,0000,0000,0000,,Ne fais pas ça, chéri !
Dialogue: 0,0:10:24.73,0:10:26.76,Fetish,,0000,0000,0000,,Nous n'avons pas le temps !
Dialogue: 0,0:10:27.73,0:10:29.83,Fetish,,0000,0000,0000,,Tu vas être en retard au travail !
Dialogue: 0,0:11:04.91,0:11:08.58,Notes,,0000,0000,0000,,{\pos(140,50)}[Nana Morimura]
Dialogue: 0,0:11:06.88,0:11:11.23,Fetish,,0000,0000,0000,,{\i1}Je travaille en tant que bonne\Npour le propriétaire des lieux.{\i0}
Dialogue: 0,0:11:12.98,0:11:17.50,Fetish,,0000,0000,0000,,{\i1}Mon maître semble très doué\Npour confectionner des tenues de bonne{\i0}
Dialogue: 0,0:11:17.66,0:11:20.71,Fetish,,0000,0000,0000,,{\i1}à l'occidentale, d'où sa notoriété.{\i0}
Dialogue: 0,0:11:22.19,0:11:24.01,Fetish,,0000,0000,0000,,{\i1}C'est pour cette raison{\i0}
Dialogue: 0,0:11:24.27,0:11:28.01,Fetish,,0000,0000,0000,,{\i1}que le costume que je porte\Na été créé par lui.{\i0}
Dialogue: 0,0:11:32.02,0:11:35.40,Fetish,,0000,0000,0000,,{\i1}Il est adorable, c'est pour ça\Nqu'il me plaisait.{\i0}
Dialogue: 0,0:11:37.39,0:11:39.02,Fetish,,0000,0000,0000,,{\i1}Oui...{\i0}
Dialogue: 0,0:11:40.01,0:11:41.32,Fetish,,0000,0000,0000,,{\i1}Jusqu'à ce jour.{\i0}
Dialogue: 0,0:12:09.55,0:12:12.36,Fetish,,0000,0000,0000,,{\i1}Je l'avais remarqué plusieurs fois\Nauparavant, mais...{\i0}
Dialogue: 0,0:12:15.70,0:12:17.67,Fetish,,0000,0000,0000,,{\i1}Par exemple, quand je nettoie\Nles fenêtres,{\i0}
Dialogue: 0,0:12:36.71,0:12:38.96,Fetish,,0000,0000,0000,,{\i1}ou quand je fais les lits,{\i0}
Dialogue: 0,0:12:52.69,0:12:56.05,Fetish,,0000,0000,0000,,{\i1}et quand j'astique le sol,{\i0}
Dialogue: 0,0:12:56.22,0:12:59.73,Fetish,,0000,0000,0000,,{\i1}j'ai l'impression que quelqu'un\Nregarde de derrière.{\i0}
Dialogue: 0,0:13:08.40,0:13:11.40,Fetish,,0000,0000,0000,,{\i1}Cependant, j'ai pensé que ce n'était\Nque mon imagination,{\i0}
Dialogue: 0,0:13:11.44,0:13:13.71,Fetish,,0000,0000,0000,,{\i1}et je n'y ai plus fait attention.{\i0}
Dialogue: 0,0:13:16.14,0:13:17.63,Fetish,,0000,0000,0000,,{\i1}Mais un jour,{\i0}
Dialogue: 0,0:13:21.59,0:13:24.46,Fetish,,0000,0000,0000,,{\i1}alors que je nettoyais\Nle bureau du maître,{\i0}
Dialogue: 0,0:13:24.74,0:13:28.71,Fetish,,0000,0000,0000,,{\i1}plusieurs photos\Nont glissé d'un livre épais.{\i0}
Dialogue: 0,0:13:29.99,0:13:31.76,Fetish,,0000,0000,0000,,{\i1}Lorsque je les ai ramassées...{\i0}
Dialogue: 0,0:13:35.72,0:13:38.90,Fetish,,0000,0000,0000,,{\i1}Ce soupçon de voyeurisme\Nest devenu clair.{\i0}
Dialogue: 0,0:13:39.86,0:13:44.36,Fetish,,0000,0000,0000,,{\i1}J'ai ressenti quelque chose\Nde plus fort que la colère...{\i0}
Dialogue: 0,0:13:46.38,0:13:50.44,Fetish,,0000,0000,0000,,{\i1}Cela venait-il de quand\Nje me suis assoupie sur le canapé ?{\i0}
Dialogue: 0,0:13:51.57,0:13:54.22,Fetish,,0000,0000,0000,,{\i1}J'étais déconcertée à l'idée{\i0}
Dialogue: 0,0:13:54.24,0:13:56.88,Fetish,,0000,0000,0000,,{\i1}que mon maître me voyait\Ndans des moments si embarrassants,{\i0}
Dialogue: 0,0:13:57.13,0:14:01.29,Fetish,,0000,0000,0000,,{\i1}et je me suis sentie envahie\Npar la gêne plutôt que par la colère.{\i0}
Dialogue: 0,0:14:02.91,0:14:06.50,Fetish,,0000,0000,0000,,{\i1}Mais qu'y a-t-il de plus embarrassant{\i0}
Dialogue: 0,0:14:06.56,0:14:10.29,Fetish,,0000,0000,0000,,{\i1}que de révéler des tenues de cuir\Npour les chaleurs estivales ?{\i0}
Dialogue: 0,0:14:11.79,0:14:15.47,Fetish,,0000,0000,0000,,{\i1}Le haut ressemble à un soutien-gorge,\Nne couvrant qu'à moitié la poitrine,{\i0}
Dialogue: 0,0:14:16.51,0:14:21.19,Fetish,,0000,0000,0000,,{\i1}et la jupe est si courte que mon string\Nest clairement visible.{\i0}
Dialogue: 0,0:14:22.79,0:14:26.81,Fetish,,0000,0000,0000,,{\i1}Quant aux moments où je pense\Nque les photos ont été prises...{\i0}
Dialogue: 0,0:14:27.38,0:14:29.48,Fetish,,0000,0000,0000,,{\i1}Je devrais être répugnée\Npar tout ceci.{\i0}
Dialogue: 0,0:14:31.75,0:14:36.47,Fetish,,0000,0000,0000,,{\i1}Quand je pense :\N"le maître me regarde",{\i0}
Dialogue: 0,0:14:37.13,0:14:42.12,Fetish,,0000,0000,0000,,{\i1}en fait, je me sens un peu énervée,\Net c'est bizarre.{\i0}
Dialogue: 0,0:14:50.67,0:14:53.32,Fetish,,0000,0000,0000,,{\i1}Mais malgré tout,{\i0}
Dialogue: 0,0:14:53.40,0:14:57.94,Fetish,,0000,0000,0000,,{\i1}tôt ou tard, je dois voir\Nle visage de mon maître.{\i0}
Dialogue: 0,0:15:10.48,0:15:12.95,Fetish,,0000,0000,0000,,{\i1}Je m'appelle Asuka Senô,{\i0}
Dialogue: 0,0:15:13.03,0:15:15.84,Fetish,,0000,0000,0000,,{\i1}j'ai dix-neuf ans\Net je suis infirmière.{\i0}
Dialogue: 0,0:15:16.70,0:15:20.50,Notes,,0000,0000,0000,,{\pos(185,40)}[Asuka Senô]
Dialogue: 0,0:15:18.98,0:15:20.93,Fetish,,0000,0000,0000,,{\i1}En ce moment...{\i0}
Dialogue: 0,0:15:21.05,0:15:23.86,Fetish,,0000,0000,0000,,{\i1}quelque chose me tracasse.{\i0}
Dialogue: 0,0:15:25.84,0:15:30.39,Fetish,,0000,0000,0000,,{\i1}À l'hôpital où je travaille,\Nil y a un docteur douteux.{\i0}
Dialogue: 0,0:15:31.83,0:15:35.42,Fetish,,0000,0000,0000,,{\i1}En temps normal,\Nc'est un très bon médecin.{\i0}
Dialogue: 0,0:15:36.62,0:15:38.51,Fetish,,0000,0000,0000,,{\i1}Mais cependant...{\i0}
Dialogue: 0,0:15:39.84,0:15:41.81,Fetish,,0000,0000,0000,,{\i1}... quand mon service est fini...{\i0}
Dialogue: 0,0:15:43.48,0:15:48.33,Fetish,,0000,0000,0000,,{\i1}... il me retient et... euh...{\i0}
Dialogue: 0,0:15:49.97,0:15:52.59,Fetish,,0000,0000,0000,,{\i1}Il me fait des choses coquines...{\i0}
Dialogue: 0,0:16:01.54,0:16:05.37,Fetish,,0000,0000,0000,,Allez, passons à un examen\Nde santé physique.
Dialogue: 0,0:16:07.49,0:16:10.54,Fetish,,0000,0000,0000,,Mais nous ne l'avions pas fait hier ?
Dialogue: 0,0:16:23.51,0:16:25.09,Fetish,,0000,0000,0000,,Commence, s'il te plaît.
Dialogue: 0,0:16:27.96,0:16:29.56,Fetish,,0000,0000,0000,,Ou... oui...
Dialogue: 0,0:16:37.41,0:16:41.41,Fetish,,0000,0000,0000,,C'est ça, déboutonne-toi\Net baisse ta fermeture éclair.
Dialogue: 0,0:16:42.86,0:16:44.37,Fetish,,0000,0000,0000,,Détends-toi.
Dialogue: 0,0:16:46.35,0:16:48.12,Fetish,,0000,0000,0000,,En faisant de telles choses,
Dialogue: 0,0:16:49.77,0:16:53.28,Fetish,,0000,0000,0000,,tu peux évacuer\Nla pression du travail.
Dialogue: 0,0:16:56.67,0:16:57.64,Fetish,,0000,0000,0000,,Oui...
Dialogue: 0,0:17:02.31,0:17:05.30,Notes,,0000,0000,0000,,{\pos(445,65)}[Fin des consultations\Npour aujourd'hui]
Dialogue: 0,0:17:18.58,0:17:21.57,Fetish,,0000,0000,0000,,Oh là là... qu'avons-nous ici ?
Dialogue: 0,0:17:22.00,0:17:25.20,Fetish,,0000,0000,0000,,Une infirmière n'a pas le droit\Nde porter des porte-jarretelles.
Dialogue: 0,0:17:26.43,0:17:29.20,Fetish,,0000,0000,0000,,Tu pourrais exciter les patients...
Dialogue: 0,0:17:30.19,0:17:32.21,Fetish,,0000,0000,0000,,Enlevons-les immédiatement.
Dialogue: 0,0:17:32.47,0:17:34.36,Fetish,,0000,0000,0000,,Je... je suis désolée...
Dialogue: 0,0:17:35.47,0:17:36.32,Fetish,,0000,0000,0000,,Euh...
Dialogue: 0,0:17:36.83,0:17:38.71,Fetish,,0000,0000,0000,,Quand je porte\Nde la lingerie habituelle,
Dialogue: 0,0:17:39.14,0:17:41.22,Fetish,,0000,0000,0000,,je transpire et c'est mauvais.
Dialogue: 0,0:17:43.06,0:17:44.74,Fetish,,0000,0000,0000,,C'est pour ça que je les porte.
Dialogue: 0,0:17:45.57,0:17:48.25,Fetish,,0000,0000,0000,,Je ferai attention, alors\Nlaissez-les-moi, s'il vous plaît.
Dialogue: 0,0:17:52.70,0:17:55.27,Fetish,,0000,0000,0000,,Tes seins précieux\Nne peuvent donc pas transpirer,
Dialogue: 0,0:17:55.33,0:17:57.38,Fetish,,0000,0000,0000,,alors enlève ton soutien-gorge.
Dialogue: 0,0:18:02.81,0:18:05.07,Fetish,,0000,0000,0000,,Bien, tu te sens fraîche,\Nmaintenant ?
Dialogue: 0,0:18:05.53,0:18:09.32,Fetish,,0000,0000,0000,,Maintenant, tout ce qui recouvre\Nton corps, c'est ce vêtement...
Dialogue: 0,0:18:10.18,0:18:12.25,Fetish,,0000,0000,0000,,Non... c'est trop embarrassant...
Dialogue: 0,0:18:12.69,0:18:15.00,Fetish,,0000,0000,0000,,Non, ce n'est pas bien.
Dialogue: 0,0:18:16.02,0:18:17.75,Fetish,,0000,0000,0000,,Tu dois ôter le dernier vêtement
Dialogue: 0,0:18:18.32,0:18:20.15,Fetish,,0000,0000,0000,,et être complètement exposée.
Dialogue: 0,0:18:21.64,0:18:23.18,Fetish,,0000,0000,0000,,Mais, docteur,
Dialogue: 0,0:18:23.70,0:18:25.93,Fetish,,0000,0000,0000,,j'ai encore mon bonnet\Nd'infirmière...
Dialogue: 0,0:18:38.87,0:18:41.39,Fetish,,0000,0000,0000,,Magnifique ! Cependant,
Dialogue: 0,0:18:42.34,0:18:44.59,Fetish,,0000,0000,0000,,tu ne dois pas retirer\Ncomplètement ta culotte.
Dialogue: 0,0:18:45.21,0:18:46.88,Fetish,,0000,0000,0000,,Laisse-la ici.
Dialogue: 0,0:18:55.77,0:18:59.69,Fetish,,0000,0000,0000,,C'est le "presque"\Nqui est important, Senô.
Dialogue: 0,0:19:01.31,0:19:02.19,Fetish,,0000,0000,0000,,Oui...
Dialogue: 0,0:19:05.07,0:19:07.86,Fetish,,0000,0000,0000,,M... mais... docteur...
Dialogue: 0,0:19:10.15,0:19:12.12,Fetish,,0000,0000,0000,,Mais quoi ?
Dialogue: 0,0:19:15.18,0:19:15.94,Fetish,,0000,0000,0000,,Euh...
Dialogue: 0,0:19:19.25,0:19:20.77,Fetish,,0000,0000,0000,,Mon bonnet d'infirmière...
Dialogue: 0,0:19:44.01,0:19:47.06,Notes,,0000,0000,0000,,{\pos(180,50)}[Mai Kannazuki]
Dialogue: 0,0:19:56.93,0:19:59.31,Fetish,,0000,0000,0000,,{\i1}Enchantée de vous rencontrer.\NJe m'appelle Mai Kannazuki.{\i0}
Dialogue: 0,0:19:59.95,0:20:03.19,Fetish,,0000,0000,0000,,{\i1}Venons-en au fait.\NQuand une femme s'accroupit{\i0}
Dialogue: 0,0:20:03.53,0:20:04.49,Fetish,,0000,0000,0000,,{\i1}comme ceci,{\i0}
Dialogue: 0,0:20:04.69,0:20:08.18,Fetish,,0000,0000,0000,,{\i1}sentez-vous que vous pourriez\Nvite regarder sa culotte ?{\i0}
Dialogue: 0,0:20:09.43,0:20:12.72,Fetish,,0000,0000,0000,,{\i1}Mais c'est faire preuve\Nd'autant d'intelligence qu'un enfant.{\i0}
Dialogue: 0,0:20:19.23,0:20:21.77,Fetish,,0000,0000,0000,,{\i1}Abstenez-vous de profiter\Ndes femmes qui s'accroupissent.{\i0}
Dialogue: 0,0:20:23.28,0:20:27.34,Fetish,,0000,0000,0000,,{\i1}Voilà pourquoi je vais vous faire\Nune démonstration aujourd'hui.{\i0}
Dialogue: 0,0:20:27.44,0:20:29.80,Fetish,,0000,0000,0000,,{\i1}J'espère\Nque vous en tirerez une leçon.{\i0}
Dialogue: 0,0:20:31.31,0:20:32.92,Fetish,,0000,0000,0000,,{\i1}Sur ce, commençons.{\i0}
Dialogue: 0,0:20:33.93,0:20:35.16,Fetish,,0000,0000,0000,,{\i1}Êtes-vous prêts ?{\i0}
Dialogue: 0,0:20:37.20,0:20:39.62,Fetish,,0000,0000,0000,,{\i1}Quand une femme s'accroupit,\Nle meilleur moment...{\i0}
Dialogue: 0,0:20:42.70,0:20:45.33,Fetish,,0000,0000,0000,,{\i1}... est celui où le mollet\Net la cuisse se rencontrent{\i0}
Dialogue: 0,0:20:45.35,0:20:48.28,Fetish,,0000,0000,0000,,{\i1}et créent une forme anormale.{\i0}
Dialogue: 0,0:20:49.00,0:20:51.31,Fetish,,0000,0000,0000,,{\i1}Pour schématiser,{\i0}
Dialogue: 0,0:20:51.91,0:20:56.28,Fetish,,0000,0000,0000,,{\i1}considérons que la taille de la cuisse\Nest la 2, celle du mollet, la 1.{\i0}
Dialogue: 0,0:20:58.76,0:21:00.12,Fetish,,0000,0000,0000,,{\i1}En position accroupie,{\i0}
Dialogue: 0,0:21:01.43,0:21:04.49,Fetish,,0000,0000,0000,,{\i1}l'épaisseur n'est pas la somme\Nde 1 + 2 = 3.{\i0}
Dialogue: 0,0:21:04.87,0:21:07.45,Fetish,,0000,0000,0000,,{\i1}Elle est toujours en dessous de 3.{\i0}
Dialogue: 0,0:21:08.12,0:21:10.83,Fetish,,0000,0000,0000,,{\i1}C'est parce que les parties du corps\Ns'élargissent.{\i0}
Dialogue: 0,0:21:11.33,0:21:14.25,Fetish,,0000,0000,0000,,{\i1}Oui, une telle perte\Nest due au potentiel{\i0}
Dialogue: 0,0:21:14.29,0:21:16.81,Fetish,,0000,0000,0000,,{\i1}d'une courbure impeccable.{\i0}
Dialogue: 0,0:21:27.03,0:21:28.02,Fetish,,0000,0000,0000,,{\i1}Par exemple,{\i0}
Dialogue: 0,0:21:29.17,0:21:31.26,Fetish,,0000,0000,0000,,{\i1}cette zone de la cuisse\Nqui change de forme{\i0}
Dialogue: 0,0:21:31.30,0:21:33.33,Fetish,,0000,0000,0000,,{\i1}est irrésistible, n'est-ce pas ?{\i0}
Dialogue: 0,0:21:34.46,0:21:36.70,Fetish,,0000,0000,0000,,{\i1}Elle forme en conséquence\Nla compression des deux parties.{\i0}
Dialogue: 0,0:21:37.91,0:21:40.97,Fetish,,0000,0000,0000,,{\i1}Le flux de la ligne\Nde la cuisse est gâché.{\i0}
Dialogue: 0,0:21:49.35,0:21:53.02,Fetish,,0000,0000,0000,,{\i1}Ensuite, si l'on doit soulever\Nnos genoux comme ceci,{\i0}
Dialogue: 0,0:21:53.33,0:21:55.36,Fetish,,0000,0000,0000,,{\i1}d'autres lignes peuvent\Nêtre observées.{\i0}
Dialogue: 0,0:21:56.59,0:21:58.91,Fetish,,0000,0000,0000,,{\i1}Ce mollet élargi exagérément{\i0}
Dialogue: 0,0:21:59.42,0:22:02.31,Fetish,,0000,0000,0000,,{\i1}paraît fort horrible.{\i0}
Dialogue: 0,0:22:02.99,0:22:06.17,Fetish,,0000,0000,0000,,{\i1}et les gens comme moi\Nsont alors mal à l'aise.{\i0}
Dialogue: 0,0:22:13.24,0:22:14.62,Notes,,0000,0000,0000,,[Allez... écartons les jambes\Nde toutes nos forces.]
Dialogue: 0,0:22:21.45,0:22:24.96,Fetish,,0000,0000,0000,,{\i1}Pour une femme telle que moi,\Nl'endroit le plus hideux{\i0}
Dialogue: 0,0:22:26.26,0:22:28.96,Fetish,,0000,0000,0000,,{\i1}se trouve ici,\Nsur la ligne des fesses.{\i0}
Dialogue: 0,0:22:29.83,0:22:32.45,Fetish,,0000,0000,0000,,{\i1}Alors que les jambes s'écartent,\Nla crevasse devient plus excitante.{\i0}
Dialogue: 0,0:22:33.21,0:22:36.96,Fetish,,0000,0000,0000,,{\i1}J'ai des fesses plutôt imposantes,\Nalors elles sont agréables à regarder.{\i0}
Dialogue: 0,0:22:46.15,0:22:48.90,Fetish,,0000,0000,0000,,{\i1}"À ce moment-là,\Nque se passe-t-il à l'avant ?"{\i0}
Dialogue: 0,0:22:52.40,0:22:55.48,Fetish,,0000,0000,0000,,{\i1}Euh... eh bien... en fait...{\i0}
Dialogue: 0,0:22:56.87,0:23:00.95,Fetish,,0000,0000,0000,,{\i1}Si l'on relève les hanches tout en étant\Naccroupie avec les jambes écartées...{\i0}
Dialogue: 0,0:23:04.39,0:23:06.67,Fetish,,0000,0000,0000,,{\i1}Les fesses se serrent\Nl'une contre l'autre,{\i0}
Dialogue: 0,0:23:07.25,0:23:10.04,Fetish,,0000,0000,0000,,{\i1}et les lèvres du bas\Ns'ouvrent en conséquence.{\i0}
Dialogue: 0,0:23:10.46,0:23:12.59,Fetish,,0000,0000,0000,,{\i1}Et l'on est partie\Npour passer à l'action.{\i0}
Dialogue: 0,0:23:14.20,0:23:18.00,Fetish,,0000,0000,0000,,{\i1}Je... je suis désolée...\Nexcusez-moi, s'il vous plaît.{\i0}
Dialogue: 0,0:23:26.61,0:23:30.84,Fetish,,0000,0000,0000,,{\i1}S'il vous plaît ! N'en dites rien\Nau Baron Mascarade...{\i0}
Dialogue: 0,0:23:31.25,0:23:32.53,Fetish,,0000,0000,0000,,{\i1}Gardez le secret...{\i0}
Dialogue: 0,0:23:46.11,0:23:50.91,Fetish,,0000,0000,0000,,{\i1}Que vais-je faire ? Je vous ai montré\Nmon côté lubrique de la sorte...{\i0}
Dialogue: 0,0:24:15.32,0:24:20.65,ROMAJI,,0000,0000,0000,,Muzukashii RUURU de shibarareru no wa iya
Dialogue: 0,0:24:15.32,0:24:20.65,Fetish,,0000,0000,0000,,{\i1}Tu n'aimes pas être lié\Npar des règles contraignantes,{\i0}
Dialogue: 0,0:24:20.72,0:24:22.42,ROMAJI,,0000,0000,0000,,BUT IT DEPENDS ON THE CASE
Dialogue: 0,0:24:20.72,0:24:22.42,Fetish,,0000,0000,0000,,{\i1}BUT IT DEPENDS ON THE CASE{\i0}
Dialogue: 0,0:24:22.54,0:24:26.01,ROMAJI,,0000,0000,0000,,Atashi no waza de shihai shite
Dialogue: 0,0:24:22.54,0:24:26.01,Fetish,,0000,0000,0000,,{\i1}Je te dominerai avec mes techniques,{\i0}
Dialogue: 0,0:24:26.09,0:24:30.23,ROMAJI,,0000,0000,0000,,Ageru kara JUDITH no you ni
Dialogue: 0,0:24:26.09,0:24:30.23,Fetish,,0000,0000,0000,,{\i1}tout comme Judith,{\i0}
Dialogue: 0,0:24:30.30,0:24:33.58,ROMAJI,,0000,0000,0000,,Ira tatta yubisaki
Dialogue: 0,0:24:30.30,0:24:33.58,Fetish,,0000,0000,0000,,{\i1}du bout de mes ongles piquants,{\i0}
Dialogue: 0,0:24:33.65,0:24:37.15,ROMAJI,,0000,0000,0000,,Tanoshimu no TACTICS
Dialogue: 0,0:24:33.65,0:24:37.15,Fetish,,0000,0000,0000,,{\i1}pour m'amuser{\i0}
Dialogue: 0,0:24:37.21,0:24:41.51,ROMAJI,,0000,0000,0000,,Odoketa furi nukedasenai kurai ni
Dialogue: 0,0:24:37.21,0:24:41.51,Fetish,,0000,0000,0000,,{\i1}Arrête de faire l'idiot,{\i0}
Dialogue: 0,0:24:41.58,0:24:45.27,ROMAJI,,0000,0000,0000,,Aishite ageru wa
Dialogue: 0,0:24:41.58,0:24:45.27,Fetish,,0000,0000,0000,,{\i1}je vais commencer à t'aimer{\i0}
Dialogue: 0,0:24:45.35,0:24:48.65,ROMAJI,,0000,0000,0000,,Kosoku na te no uchi sae mo
Dialogue: 0,0:24:45.35,0:24:48.65,Fetish,,0000,0000,0000,,{\i1}Au fond, ce n'est que temporaire,{\i0}
Dialogue: 0,0:24:48.69,0:24:52.81,ROMAJI,,0000,0000,0000,,Kokochi yoku damasareteku
Dialogue: 0,0:24:48.69,0:24:52.81,Fetish,,0000,0000,0000,,{\i1}je sens bien que tu m'as trompée{\i0}
Dialogue: 0,0:24:52.88,0:24:55.34,ROMAJI,,0000,0000,0000,,Jama dato omou hodo
Dialogue: 0,0:24:52.88,0:24:55.34,Fetish,,0000,0000,0000,,{\i1}Si tu comptes t'interposer,{\i0}
Dialogue: 0,0:24:55.48,0:24:59.25,ROMAJI,,0000,0000,0000,,Kirifuda ni kawaru
Dialogue: 0,0:24:55.48,0:24:59.25,Fetish,,0000,0000,0000,,{\i1}je changerai de carte maîtresse{\i0}
Dialogue: 0,0:24:59.37,0:25:02.80,ROMAJI,,0000,0000,0000,,Demo LOVE GAME ki wo tsukete yo
Dialogue: 0,0:24:59.37,0:25:02.80,Fetish,,0000,0000,0000,,{\i1}Mais le jeu de l'amour,\Nc'est de mettre le feu{\i0}
Dialogue: 0,0:25:02.92,0:25:08.16,ROMAJI,,0000,0000,0000,,Suki wo misetara sukasazu koko de
Dialogue: 0,0:25:02.92,0:25:08.16,Fetish,,0000,0000,0000,,{\i1}Si tu as un peu de chance,\Nje te le montrerai sans attendre ici-même{\i0}
Dialogue: 0,0:25:08.28,0:25:10.31,ROMAJI,,0000,0000,0000,,Abaite miseru
Dialogue: 0,0:25:08.28,0:25:10.31,Fetish,,0000,0000,0000,,{\i1}Je vais te montrer de la rage,{\i0}
Dialogue: 0,0:25:10.37,0:25:14.70,Fetish,,0000,0000,0000,,{\i1}SPILL THE BEANS, c'est ça...\Nc'est le piège de l'amour{\i0}
Dialogue: 0,0:25:10.37,0:25:14.70,ROMAJI,,0000,0000,0000,,SPILL THE BEANS sou yo... ai no wana
Dialogue: 0,0:25:14.70,0:25:16.70,Fetish,,0000,0000,0000,,
