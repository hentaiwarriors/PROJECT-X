Dialogue: 0,0:18:22.46,0:18:24.40,Default,NTP,0000,0000,0000,!Effect,Moja siostra i ja
Dialogue: 0,0:18:24.53,0:18:29.06,Default,NTP,0000,0000,0000,!Effect,tułaliśmy się z miejsca na miejsce, zanim trafiliśmy tutaj.
Dialogue: 0,0:18:29.37,0:18:32.83,Default,NTP,0000,0000,0000,!Effect,A gdy już przestałem komukolwiek ufać,
Dialogue: 0,0:18:33.24,0:18:37.08,Default,NTP,0000,0000,0000,!Effect,ty przyjęłaś nas z uśmiechem.
Dialogue: 0,0:18:39.28,0:18:40.91,Default,NTP,0000,0000,0000,!Effect,Wtedy ja...
Dialogue: 0,0:18:42.05,0:18:45.61,Default,NTP,0000,0000,0000,!Effect,Zdecydowałem się dzielić z tobą moje życie.
Dialogue: 0,0:18:55.16,0:18:57.06,Default,NTP,0000,0000,0000,!Effect,Co się stało, Fey?
Dialogue: 0,0:19:00.44,0:19:01.70,Default,NTP,0000,0000,0000,!Effect,Braciszku.
Dialogue: 0,0:19:02.07,0:19:04.13,Default,NTP,0000,0000,0000,!Effect,Kocham cię.
Dialogue: 0,0:19:51.32,0:19:52.68,Default,NTP,0000,0000,0000,!Effect,Braciszku.
Dialogue: 0,0:19:56.32,0:19:57.66,Default,NTP,0000,0000,0000,!Effect,{\i1} Pan!{\i0}
Dialogue: 0,0:19:59.76,0:20:01.59,Default,NTP,0000,0000,0000,!Effect,{\i1} Nie patrz!{\i0}
Dialogue: 0,0:20:10.44,0:20:13.13,Default,NTP,0000,0000,0000,!Effect,{\i1}Nie! Nie patrz!{\i0}
Dialogue: 0,0:20:17.45,0:20:18.97,Default,NTP,0000,0000,0000,!Effect,{\i1} Nie patrz!{\i0}
Dialogue: 0,0:20:20.18,0:20:22.21,Default,NTP,0000,0000,0000,!Effect,{\i1} Panie! Nie patrz!{\i0}
Dialogue: 0,0:20:36.83,0:20:38.13,Default,NTP,0000,0000,0000,!Effect,{\i1} Nie patrz!{\i0}
Dialogue: 0,0:20:38.50,0:20:39.69,Default,NTP,0000,0000,0000,!Effect,{\i1} Nie patrz!{\i0}
Dialogue: 0,0:20:42.24,0:20:45.60,Default,NTP,0000,0000,0000,!Effect,{\i1} Nie patrz na mnie!{\i0}
Dialogue: 0,0:20:45.91,0:20:48.00,Default,NTP,0000,0000,0000,!Effect,{\i1} Jeśli nie przestaniesz, ja...{\i0}
Dialogue: 0,0:20:49.28,0:20:51.94,Default,NTP,0000,0000,0000,!Effect,{\i1} Chce zobaczyć, jak dochodzę!{\i0}
Dialogue: 0,0:20:52.45,0:20:53.68,Default,NTP,0000,0000,0000,!Effect,Dochodzę!
Dialogue: 0,0:20:54.35,0:20:56.11,Default,NTP,0000,0000,0000,!Effect,Dochodzę!
Dialogue: 0,0:21:01.32,0:21:04.72,Default,NTP,0000,0000,0000,!Effect,Dochodzę! Dochodzę! Dochodzę!
Dialogue: 0,0:21:14.87,0:21:16.17,Default,NTP,0000,0000,0000,!Effect,Tak, tak!
Dialogue: 0,0:21:19.44,0:21:20.87,Default,NTP,0000,0000,0000,!Effect,Braciszku!
Dialogue: 0,0:21:23.38,0:21:24.35,Default,NTP,0000,0000,0000,!Effect,Braciszku!
Dialogue: 0,0:21:25.08,0:21:27.01,Default,NTP,0000,0000,0000,!Effect,Dochodzę!
Dialogue: 0,0:21:28.75,0:21:29.48,Default,NTP,0000,0000,0000,!Effect,Dochodzę!
Dialogue: 0,0:21:31.32,0:21:32.15,Default,NTP,0000,0000,0000,!Effect,Fey...
Dialogue: 0,0:21:32.42,0:21:33.25,Default,NTP,0000,0000,0000,!Effect,Dochodzę!
Dialogue: 0,0:21:34.26,0:21:34.88,Default,NTP,0000,0000,0000,!Effect,Dochodzę!
Dialogue: 0,0:21:54.38,0:21:57.50,Default,NTP,0000,0000,0000,!Effect,Sophio, zjedz razem z nami.
Dialogue: 0,0:21:57.68,0:21:58.77,Default,NTP,0000,0000,0000,!Effect,Co?!
Dialogue: 0,0:21:58.95,0:22:01.85,Default,NTP,0000,0000,0000,!Effect,Jestem twoją służącą.
Dialogue: 0,0:22:01.95,0:22:03.11,Default,NTP,0000,0000,0000,!Effect,Ale...
Dialogue: 0,0:22:03.22,0:22:07.92,Default,NTP,0000,0000,0000,!Effect,Proszę, pozwól mi tobie służyć, panienko Fey.
Dialogue: 0,0:22:08.06,0:22:12.93,Default,NTP,0000,0000,0000,!Effect,Fey, dlaczego nie spełnisz prośby Sophii?
Dialogue: 0,0:22:13.09,0:22:14.62,Default,NTP,0000,0000,0000,!Effect,Ale...
Dialogue: 0,0:22:17.17,0:22:20.62,Default,NTP,0000,0000,0000,!Effect,Tak przy okazji, gdzie byliście?
Dialogue: 0,0:22:21.40,0:22:23.03,Default,NTP,0000,0000,0000,!Effect,Ja, um...
Dialogue: 0,0:22:23.14,0:22:25.54,Default,NTP,0000,0000,0000,!Effect,Zwiedzaliście rezydencję?
Dialogue: 0,0:22:26.01,0:22:28.74,Default,NTP,0000,0000,0000,!Effect,Um, tak, właśnie tak...
Dialogue: 0,0:22:28.94,0:22:30.04,Default,NTP,0000,0000,0000,!Effect,Czy było miło?
Dialogue: 0,0:22:35.75,0:22:37.55,Default,NTP,0000,0000,0000,!Effect,Tak, bardzo!
Dialogue: 0,0:22:39.02,0:22:41.39,Default,NTP,0000,0000,0000,!Effect,Rozumiem, cieszę się.
Dialogue: 0,0:22:59.31,0:23:00.54,Default,NTP,0000,0000,0000,!Effect,Panienko Fey.
Dialogue: 0,0:23:03.18,0:23:05.20,Default,NTP,0000,0000,0000,!Effect,Siostro Sophio.
Dialogue: 0,0:23:06.88,0:23:08.44,Default,NTP,0000,0000,0000,!Effect,Jesteś pewna?
Dialogue: 0,0:23:08.82,0:23:10.55,Default,NTP,0000,0000,0000,!Effect,Nie jesteś zmęczona?
Dialogue: 0,0:23:10.69,0:23:11.98,Default,NTP,0000,0000,0000,!Effect,Obiecałam, prawda?
Dialogue: 0,0:23:12.45,0:23:16.45,Default,NTP,0000,0000,0000,!Effect,Moje noce należą do ciebie.
Dialogue: 0,0:23:17.29,0:23:18.12,Default,NTP,0000,0000,0000,!Effect,Panienko Fey!
Dialogue: 0,0:23:18.39,0:23:19.36,Default,NTP,0000,0000,0000,!Effect,Panienko Fey!
Dialogue: 0,0:23:19.46,0:23:21.66,Default,NTP,0000,0000,0000,!Effect,Kocham cię. Tak bardzo cię kocham!
Dialogue: 0,0:23:25.03,0:23:25.93,Default,NTP,0000,0000,0000,!Effect,Fey!
Dialogue: 0,0:23:26.73,0:23:27.67,Default,NTP,0000,0000,0000,!Effect,Fey!
Dialogue: 0,0:23:34.71,0:23:39.11,Default,NTP,0000,0000,0000,!Effect,Sophio, przywitaj się z Fey.
Dialogue: 0,0:23:43.99,0:23:47.58,Default,NTP,0000,0000,0000,!Effect,To Sophia. Od dzisiaj będzie się tobą opiekować.
Dialogue: 0,0:23:48.36,0:23:50.32,Default,NTP,0000,0000,0000,!Effect,Miło mi cię poznać, Sophio.
Dialogue: 0,0:23:50.59,0:23:51.52,Default,NTP,0000,0000,0000,!Effect,Jestem Fey!
Dialogue: 0,0:23:52.06,0:23:53.25,Default,NTP,0000,0000,0000,!Effect,Miło mi cię poznać.
Dialogue: 0,0:23:56.40,0:23:58.63,Default,NTP,0000,0000,0000,!Effect,Miło mi cię poznać, panienko Fey.
Dialogue: 0,0:23:59.40,0:24:04.50,Default,NTP,0000,0000,0000,!Effect,Od tamtej pory moje serce należy do ciebie.
Dialogue: 0,0:24:05.64,0:24:06.94,Default,NTP,0000,0000,0000,!Effect,Siostro...
Dialogue: 0,0:24:08.91,0:24:10.84,Default,NTP,0000,0000,0000,!Effect,Sophio...
Dialogue: 0,0:24:40.81,0:24:46.25,Default,NTP,0000,0000,0000,!Effect,Sophio, chcę dojść razem z tobą.
Dialogue: 0,0:24:46.35,0:24:48.28,Default,NTP,0000,0000,0000,!Effect,Tak, panienko Fey...
Dialogue: 0,0:25:35.10,0:25:36.36,Default,NTP,0000,0000,0000,!Effect,Dochodzę! Dochodzę!
Dialogue: 0,0:25:36.46,0:25:38.19,Default,NTP,0000,0000,0000,!Effect,Ja też...
Dialogue: 0,0:25:57.18,0:25:58.31,Default,NTP,0000,0000,0000,!Effect,Panienko Fey.
Dialogue: 0,0:25:58.45,0:25:59.98,Default,NTP,0000,0000,0000,!Effect,Sophio...
Dialogue: 0,0:26:02.76,0:26:04.92,Default,NTP,0000,0000,0000,!Effect,Sophia...
Dialogue: 0,0:26:26.81,0:26:29.28,Default,NTP,0000,0000,0000,!Effect,Nie powiesz mi?
Dialogue: 0,0:26:31.82,0:26:34.29,Default,NTP,0000,0000,0000,!Effect,O co chodzi, Michael?
Dialogue: 0,0:26:34.92,0:26:37.02,Default,NTP,0000,0000,0000,!Effect,Nie przyjmiesz tego?
Dialogue: 0,0:26:37.19,0:26:38.78,Default,NTP,0000,0000,0000,!Effect,To zaproszenie.
Dialogue: 0,0:26:42.10,0:26:44.72,Default,NTP,0000,0000,0000,!Effect,To prawda, John.
Dialogue: 0,0:26:45.60,0:26:48.66,Default,NTP,0000,0000,0000,!Effect,Jeśli odmówisz, nie będziemy mogli dłużej tutaj mieszkać.
Dialogue: 0,0:26:49.77,0:26:52.10,Default,NTP,0000,0000,0000,!Effect,Nie mamy innego wyjścia, John.
Dialogue: 0,0:27:06.19,0:27:08.75,Default,NTP,0000,0000,0000,!Effect,To miejsce całkowicie różni się od naszego domu.
Dialogue: 0,0:27:08.86,0:27:11.22,Default,NTP,0000,0000,0000,!Effect,Tak, naprawdę.
Dialogue: 0,0:27:11.56,0:27:14.22,Default,NTP,0000,0000,0000,!Effect,Nie mogę się już doczekać.
Dialogue: 0,0:27:15.00,0:27:17.83,Default,NTP,0000,0000,0000,!Effect,Witajcie w moim domu.
Dialogue: 0,0:27:18.40,0:27:22.17,Default,NTP,0000,0000,0000,!Effect,Mam nadzieję, że na tym przyjęciu będziecie się dobrze bawić.
Dialogue: 0,0:27:26.71,0:27:28.27,Default,NTP,0000,0000,0000,!Effect,Wspaniała!
Dialogue: 0,0:27:32.25,0:27:36.55,Default,NTP,0000,0000,0000,!Effect,Nie!
